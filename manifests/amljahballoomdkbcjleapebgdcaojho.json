{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Suddet",
  "version": "1",
  "description": "Tar bort störande designelement från Blocket, för att göra sidan mer lättläst och skön att använda.",
  "manifest_version": 2,
  "browser_action": {
    "name": "Suddet",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_icon": "icon48.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "background.js"
      ],
      "matches": [
        "http://www.blocket.se/*",
        "https://www.blocket.se/*",
        "http://www2.blocket.se/*"
      ]
    }
  ]
}
