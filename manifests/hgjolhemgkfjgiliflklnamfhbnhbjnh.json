{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Miri Regev is watching you",
  "short_name": "Miri",
  "description": "Unofficial extension, satire,humor, stuff...you know.",
  "version": "0.0.3",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "scripts.js"
      ],
      "css": [
        "style.css"
      ],
      "all_frames": false,
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "miri.png",
    "16": "miri.png",
    "48": "miri.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
