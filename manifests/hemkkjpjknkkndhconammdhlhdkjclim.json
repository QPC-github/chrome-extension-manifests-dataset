{
  "name": "Lubuntu Scrollbars",
  "version": "12.10",
  "manifest_version": 2,
  "description": "Makes Chrome/Chromium scrollbars match with Lubuntu.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "scrollbars.css"
      ]
    }
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "offline_enabled": true
}
