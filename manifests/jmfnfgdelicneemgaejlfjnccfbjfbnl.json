{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Калькулятор вкладов",
  "description": "Удобный калькулятор вкладов",
  "version": "1.1",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "code.js"
    ]
  },
  "browser_action": {
    "default_icon": "19x19.png"
  },
  "permissions": [
    "http://mobile-testing.ru/*",
    "http://fonts.googleapis.com/*",
    "tabs"
  ],
  "default_locale": "ru"
}
