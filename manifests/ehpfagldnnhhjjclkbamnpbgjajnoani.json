{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SuperStyler",
  "version": "0.2.7",
  "manifest_version": 2,
  "description": "Rozszerzenie przeglądarki Chrome umożliwiające śledzenie na bieżąco nowych wpisów na blogu SuperStyler.pl.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://www.superstyler.pl/"
  ]
}
