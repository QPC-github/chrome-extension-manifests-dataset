{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search Helper",
  "version": "0.3",
  "description": "Very usefull extension for search input finding. Sorry for my ugly English. Please.",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.11.3.min.js",
        "search_helper.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "loupe.png",
    "48": "loupe.png",
    "128": "loupe.png"
  }
}
