{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Engrade Enhancement Suite",
  "description": "Adds more functionality to the Engrade student webpages",
  "icons": {
    "128": "icon_128.png"
  },
  "version": "1.0.2",
  "author": "Seif Shawkat",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://engradepro.com/student/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-3.2.1.js",
        "optionsPanel.js",
        "studentGrades.js"
      ]
    },
    {
      "matches": [
        "https://engradepro.com/class/gradebook/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-3.2.1.js",
        "optionsPanel.js",
        "classGrades.js"
      ]
    }
  ]
}
