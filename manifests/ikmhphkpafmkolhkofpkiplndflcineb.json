{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "name": "Ghost chat bot",
  "short_name": "Ghost",
  "description": "Chat with simple chat bot, teach him new stuff.",
  "icons": {
    "16": "icon/16.png",
    "19": "icon/19.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png",
    "256": "icon/256.png"
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://*/",
    "https://*/"
  ],
  "version": "1.0.211"
}
