{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iFramatic",
  "description": "Inspects responsive iframe contents",
  "version": "0.8.1",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Inspect this contnet!",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
