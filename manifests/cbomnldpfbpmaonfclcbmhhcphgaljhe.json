{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bitsell X",
  "short_name": "Bitsell X",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Trade 100+ cryptocurrencies FAST! Non-Custodial, Lax verification, No account, BEST market rates. Even EUR/USD credit card accepted!",
  "author": "XcSN - ABP Interactive Â© 2020",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
