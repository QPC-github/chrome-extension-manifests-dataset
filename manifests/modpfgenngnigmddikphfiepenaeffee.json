{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zentail.com",
  "version": "1.2",
  "permissions": [
    "https://*.amazon.com/",
    "http://*.amazon.com/",
    "https://amazon.com/",
    "http://amazon.com/",
    "https://*.newegg.com/",
    "http://*.newegg.com/",
    "https://newegg.com/",
    "http://newegg.com/",
    "https://*.walmart.com/",
    "http://*.walmart.com/",
    "https://walmart.com/",
    "http://walmart.com/",
    "https://*.wayfair.com/",
    "http://*.wayfair.com/",
    "https://wayfair.com/",
    "http://wayfair.com/",
    "https://*.target.com/",
    "http://*.target.com/",
    "https://target.com/",
    "http://target.com/",
    "https://*.ebay.com/",
    "http://*.ebay.com/",
    "https://ebay.com/",
    "http://ebay.com/",
    "https://*.staples.com/",
    "http://*.staples.com/",
    "https://*.sears.com/",
    "http://*.sears.com/",
    "https://www.zentail.com/"
  ],
  "icons": {
    "16": "zentail_logo.png",
    "48": "zentail_logo.png",
    "128": "zentail_logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.amazon.com/dp/*",
        "http://www.amazon.com/*dp/*",
        "https://www.amazon.com/*dp/*",
        "http://amazon.com/*dp/*",
        "https://amazon.com/*dp/*",
        "http://www.amazon.com/*gp/*",
        "https://www.amazon.com/*gp/*",
        "http://amazon.com/*gp/*",
        "https://amazon.com/*gp/*"
      ],
      "css": [
        "zentail.css"
      ],
      "js": [
        "amazon.js"
      ]
    },
    {
      "matches": [
        "https://*.zentail.com/*",
        "http://*.zentail.com/*",
        "https://zentail.com/*",
        "http://zentail.com/*"
      ],
      "js": [
        "zentail.js"
      ]
    }
  ]
}
