{
  "background": {
    "scripts": [
      "scripts/back.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_title": "Ciantel"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://www.ciantel.com/favicon.ico",
      "image_url": "https://www.ciantel.com/web?q={searchTerms}",
      "image_url_post_params": "",
      "instant_url": "https://www.ciantel.com/web?q={searchTerms}",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "https://www.ciantel.com/",
      "name": "SafeSearch",
      "search_url": "https://www.ciantel.com/web?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "https://www.ciantel.com/web?q={searchTerms}",
      "suggest_url_post_params": ""
    },
    "startup_pages": [
      "https://www.ciantel.com/"
    ]
  },
  "description": "Encuentra películas, series y canciones cambiando tu motor de búsqueda",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.mysearch.fun/*"
    ]
  },
  "version": "1.2.1",
  "manifest_version": 2,
  "name": "Buscar canciones y películas",
  "permissions": [
    "storage",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
