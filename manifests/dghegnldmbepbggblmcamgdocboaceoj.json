{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Facebook Popover",
  "version": "0.2.2",
  "description": "Removes the pesky facebook pop over when you are not logged in.",
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "https://www.facebook.com/*"
      ]
    }
  ]
}
