{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "МирТесен",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "web_accessible_resources": [],
  "description": "Рекомендательная социальная медиаплатформа",
