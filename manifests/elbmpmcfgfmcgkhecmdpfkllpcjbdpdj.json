{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Universal keyboard layout online",
  "description": "This extension allows you to easly type accents and other characters of any country without the specific keyboard.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  }
}
