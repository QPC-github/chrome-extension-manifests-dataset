{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Golang syntax highlighter",
  "version": "1.2",
  "description": "Highlight syntax in golang official docs and blog",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://golang.org/doc/*",
        "https://blog.golang.org/*"
      ]
    }
  ]
}
