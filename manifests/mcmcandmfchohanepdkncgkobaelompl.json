{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No More Holidays",
  "description": "Removes all the holiday scripts from IC.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "http://catseekerproject.activeboard.com",
  "content_scripts": [
    {
      "matches": [
        "http://www.camsathome.com/*",
        "http://*.amsterdamlivexxx.com/*"
      ],
      "all_frames": true,
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "version": "1.0.1.0"
}
