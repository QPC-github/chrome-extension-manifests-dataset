{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com https://ssl.google-analytics.com; object-src 'self'",
  "description": "Acorta las URL de forma fácil y descargare el código QR",
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "manifest_version": 2,
  "name": "Acortador URL",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "contextMenus"
  ],
  "short_name": "Acortador URL",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
