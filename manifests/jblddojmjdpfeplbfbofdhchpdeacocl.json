{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kokoni",
  "short_name": "Kokoni",
  "version": "1.4.0",
  "default_locale": "en",
  "description": "__MSG_description__",
  "permissions": [
    "alarms",
    "contextMenus",
    "downloads",
    "downloads.shelf",
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon/icon016.png",
    "48": "icon/icon048.png",
    "128": "icon/icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
