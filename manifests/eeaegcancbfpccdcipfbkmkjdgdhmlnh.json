{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VeganSearch Extension",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "128": "vegansearch128.png",
    "46": "vegansearch48.png",
    "16": "vegansearch16.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.vegansearch.org/",
    "search_provider": {
      "name": "vegansearch",
      "keyword": "vegansearch",
      "search_url": "http://www.vegansearch.org/results.html?q={searchTerms}",
      "favicon_url": "http://www.vegansearch.org/favicon.ico",
      "suggest_url": "http://www.vegansearch.org/suggest?q={searchTerms}",
      "instant_url": "http://www.vegansearch.org/instant?q={searchTerms}",
      "image_url": "http://www.vegansearch.org/image?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://www.vegansearch.org/"
    ]
  }
}
