{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SEIP Automation",
  "description": "Developed by: Engr. Syed Rowshan Ali",
  "version": "1.9.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "page_action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "BTMA-SEIP Automation",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://localhost:8285/*",
    "http://erp.btmaseip.org.bd/*",
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://tms.seip-fd.gov.bd/traineeInfo/index*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "trainee.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "*://tms.seip-fd.gov.bd/trainingInstitutes/view*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "institute.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "*://tms.seip-fd.gov.bd/courseInfos/index*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "course.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "*://tms.seip-fd.gov.bd/batchInfos/index*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "batch.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "*://tms.seip-fd.gov.bd/trainingCertificates/certifiedTrainees*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "certificate.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "*://tms.seip-fd.gov.bd/trainerInfos/index*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "trainer.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "result.html",
    "result.js",
    "icon.png",
    "popup.js"
  ]
}
