{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PDFSci",
  "version": "13.12.5",
  "description": "Affiche les PDF des revues scientifique sans les iframes",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {},
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "rewrite.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "web_accessible_resources": []
}
