{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Thumbs Way Up",
  "short_name": "A New Tab Extension",
  "description": "Open a new tab and see the cool shit I made!",
  "version": "1.972",
  "browser_action": {
    "default_icon": "./assets/good.png"
  },
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
