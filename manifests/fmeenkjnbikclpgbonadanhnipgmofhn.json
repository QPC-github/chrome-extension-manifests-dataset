{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Bicom Systems ltd",
  "background": {
    "scripts": [
      "call_action.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "google_phone_lib.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "highlight_numbers.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Use gloCOM to call numbers in Chrome.",
  "icons": {
    "128": "app128.png",
    "16": "app16.png",
    "32": "app32.png",
    "48": "app48.png"
  },
  "manifest_version": 2,
  "name": "gloCOM Call",
  "permissions": [
    "contextMenus"
  ],
  "version": "5.1.0.0",
  "web_accessible_resources": [
    "glocom_icon.jpg",
    "detectAjax.js"
  ]
}
