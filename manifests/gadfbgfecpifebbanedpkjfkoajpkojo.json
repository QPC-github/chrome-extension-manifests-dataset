{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.0.8",
  "icons": {
    "16": "res/img/logo(16px).png",
    "48": "res/img/logo(48px).png",
    "128": "res/img/logo(128px).png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "bootstrap.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "res/img/label-tag-example.png"
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "name": "GitHub (Enterprise) Projects Plus",
  "description": "[Deprecated] Adds advanced labels management and other functionality to GitHub."
}
