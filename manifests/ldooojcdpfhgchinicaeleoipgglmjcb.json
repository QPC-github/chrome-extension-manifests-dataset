{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FirstQuarto",
  "description": "A social original-content sharing tool based on quality-rating by readers.",
  "version": "1.2",
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "38": "38.png",
    "19": "19.png",
    "16": "16.png"
  },
  "browser_action": {
    "default_icon": "19.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://firstquarto.com/"
  ]
}
