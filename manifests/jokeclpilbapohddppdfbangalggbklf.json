{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ITSGOOD!",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Сервис для сохранения изображений",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://itsgood.ru/*"
  ],
  "author": "ITSGOOD",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "jquery.js",
      "itsgood.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "itsgood",
    "default_popup": "popup.html"
  }
}
