{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Preloaded JSON viewer",
  "version": "1.2",
  "description": "View all JSON information loaded in a webpage inside a script tag with type text/json",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "JSON viewer",
    "default_icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/64.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/64.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
