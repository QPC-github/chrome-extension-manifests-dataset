{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Darkipedia",
  "description": "This extension creates a dark theme for Wikipedia",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://en.wikipedia.org/wiki/*"
      ],
      "css": [
        "dark_style.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
