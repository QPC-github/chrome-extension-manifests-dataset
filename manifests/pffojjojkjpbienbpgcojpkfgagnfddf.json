{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notes",
  "description": "Offline secure storage of personal notes",
  "version": "0.4",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "activeTab"
  ]
}
