{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Swisscows",
  "description": "__MSG_description__",
  "version": "1.3",
  "author": "Roman Savchenko",
  "browser_action": {
    "default_icon": {
      "16": "favicon.png",
      "32": "favicon-32.png",
      "48": "favicon-48.png"
    },
    "default_title": "Swisscows"
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon-32.png",
    "48": "favicon-48.png"
  },
  "default_locale": "en",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Swisscows",
      "keyword": "Swisscows",
      "search_url": "https://swisscows.ch/web?query={searchTerms}",
      "favicon_url": "https://swisscows.ch/favicon-32.png",
      "suggest_url": "https://swisscows.ch/api/suggest?query={searchTerms}",
      "image_url": "https://swisscows.ch/image?query={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "short_name": "swisscows"
}
