{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GDPR Webcache Redirect",
  "short_name": "gdpr-redir",
  "minimum_chrome_version": "43",
  "version": "1.0.2",
  "description": "Automatically redirects '451 Unavailable for legal reasons' to Google Cache",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Automatically redirects '451 Unavailable for legal reasons' to Google Cache",
    "default_popup": "browseraction/index.html"
  },
  "permissions": [
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
