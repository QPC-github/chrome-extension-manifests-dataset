{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vidble",
  "version": "1.0.3",
  "description": "Save pictures to vidble.com!",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "vidble-16.png",
    "48": "vidble-48.png",
    "128": "vidble-128.png"
  },
  "manifest_version": 2
}
