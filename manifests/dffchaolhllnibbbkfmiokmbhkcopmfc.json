{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Udacity Playback Rate",
  "version": "0.8",
  "icons": {
    "48": "speed48.png",
    "128": "speed128.png"
  },
  "manifest_version": 2,
  "description": "Control playback rate of Udacity videos.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.youtube.com/embed/*"
  ],
  "background": {
    "scripts": [
      "channel.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/embed/*"
      ],
      "js": [
        "playback.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://www.udacity.com/course/viewer*"
      ],
      "js": [
        "button.js"
      ],
      "css": [
        "button.css"
      ]
    }
  ]
}
