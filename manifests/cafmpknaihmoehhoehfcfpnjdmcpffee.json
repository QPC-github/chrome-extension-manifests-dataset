{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pokedex Linker",
  "description": "Converts pokemon names to links that open up ThePoke.de/x/ with easy access info when you hover over the link.",
  "version": "3.6",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png",
    "512": "icon_512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "popper.min.js",
        "tooltip.min.js",
        "db.js",
        "content.js"
      ]
    }
  ]
}
