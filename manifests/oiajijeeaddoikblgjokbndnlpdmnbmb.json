{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistence": "true",
    "scripts": [
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "19-icon.png",
    "default_popup": "popup.html"
  },
  "description": "Savrock.com offers you a lightweight browser extention that provides you easy access to our WHOIS lookup service.",
  "icons": {
    "128": "128-icon.png",
    "16": "16-icon.png",
    "48": "48-icon.png"
  },
  "manifest_version": 2,
  "name": "Savrock.com WHOIS Search",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "version": "1.0.0.1"
}
