{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "We|Share It!",
  "version": "1.3",
  "description": "Create a JAM-Task from a SnapShot!",
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline' 'sha256-+XKhTGLYEbU1iqpbo3XowlBp+Sx3AKo68vzKCbTdrrY=' 'sha256-Xmj6xrQfWgztl4kR5wmBjDqMDrKPI061bSUQcGGCU54=' 'nonce-sapui5lib' 'unsafe eval' https://openui5.hana.ondemand.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "desktopCapture",
    "pageCapture",
    "<all_url>",
    "https://*.sapjam.com/*"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/solvia_16.png",
      "32": "images/solvia_32.png",
      "48": "images/solvia_48.png",
      "128": "images/solvia_128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/solvia_16.png",
    "32": "images/solvia_32.png",
    "48": "images/solvia_48.png",
    "128": "images/solvia_128.png"
  },
  "manifest_version": 2
}
