{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LMUR",
  "version": "2.27",
  "description": "Overlays UR and problem markers onto Livemap",
  "content_scripts": [
    {
      "matches": [
        "https://*.waze.com/*livemap*",
        "http://*.waze.com/*livemap*"
      ],
      "js": [
        "bootstrap.user.js"
      ]
    }
  ],
  "permissions": [
    "https://*.waze.com/*livemap*",
    "http://*.waze.com/*livemap*"
  ],
  "web_accessible_resources": [
    "LMUR.2.27.user.js"
  ]
}
