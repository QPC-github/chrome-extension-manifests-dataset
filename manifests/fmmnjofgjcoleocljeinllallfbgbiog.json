{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tidio Mobile RWD",
  "description": "Tidio RWD is a free programming GUI â€“ designed to allow you to quickly convert your website to a mobile-friendly version.",
  "version": "2.0.3",
  "browser_action": {
    "default_icon": "./img/i24.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./img/i16.png",
    "32": "./img/i32.png",
    "48": "./img/i48.png",
    "64": "./img/i64.png",
    "128": "./img/i128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/background-exec.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "public-files/*"
  ],
  "author": [
    {
      "name": "Tidio Ltd.",
      "email": "contact@tidio.net"
    }
  ]
}
