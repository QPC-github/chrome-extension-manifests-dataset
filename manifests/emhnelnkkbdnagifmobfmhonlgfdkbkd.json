{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Xage.ru",
  "description": "Молодежный портал для адекватной молодежи Xage.ru  (Гаджеты, Мода, Фильмы, Книги, Игры, Фитнес, Креатив).",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_title": "Xage.ru",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "date.js",
      "lib.js",
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://feeds.feedburner.com/*",
    "*://xage.ru/*",
    "alarms",
    "notifications"
  ],
  "web_accessible_resources": [
    "128.png"
  ]
}
