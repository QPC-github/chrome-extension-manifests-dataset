{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.1",
  "manifest_version": 2,
  "name": "Elite Download Manager",
  "short_name": "download-manager",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "http://mybrowseraddon.com/download-manager.html",
  "description": "Easily manage all your downloads from toolbar popup!",
  "permissions": [
    "storage",
    "downloads",
    "notifications",
    "downloads.open",
    "downloads.shelf"
  ],
  "browser_action": {
    "default_popup": "data/popup/popup.html",
    "default_title": "Elite Download Manager",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
