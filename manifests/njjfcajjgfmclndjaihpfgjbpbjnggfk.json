{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "画像映像色反転",
  "version": "1.0",
  "manifest_version": 2,
  "description": "ページ内の画像や動画の色を反転します。",
  "homepage_url": "http://tys11.net",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "run_at": "document_start"
    }
  ]
}
