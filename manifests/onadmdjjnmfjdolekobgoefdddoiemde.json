{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Awakens.me Emojis",
  "description": "Replaces common ascii faces with the emoji code equivalent in your input.",
  "version": "1.1",
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.awakens.me/*",
        "https://*.awakens.me/*"
      ],
      "js": [
        "awks.js"
      ]
    }
  ]
}
