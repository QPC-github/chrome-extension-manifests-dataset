{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HDNewPics",
  "version": "2.0.5",
  "description": "Remove Kpop news's useless query string to get best quality photos",
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.topstarnews.net/news/*",
        "*://cds.topstarnews.net/news/*",
        "*://ssl.pstatic.net/*",
        "*://post-phinf.pstatic.net/*",
        "*://img1.daumcdn.net/thumb/*",
        "*://t1.daumcdn.net/cfile/*",
        "*://*.uf.tistory.com/image/*",
        "*://mimgnews.pstatic.net/*",
        "*://vfan-phinf.pstatic.net/*",
        "*://www.thebigdata.co.kr/*",
        "*://static.kstyle.com/*",
        "*://pbs.twimg.com/media/*",
        "*://cdnimg.melon.co.kr/*",
        "*://img-mdpr.freetls.fastly.net/*",
        "*://kfakescom.files.wordpress.com/*",
        "*://cfapfakes.com/wp-content/uploads/*",
        "*://www.kfapfakes.com/wp-content/uploads/*",
        "*://mpost.tv/wp-content/uploads/*",
        "*://newsimg.sedaily.com/*",
        "*://tenasia.hankyung.com/webwp_kr/*",
        "*://img2.sbs.co.kr/img/*",
        "*://static.wixstatic.com/media/*",
        "*://www.koreanfakes.com/wp-content/uploads/*",
        "*://yt3.ggpht.com/*",
        "*://popwave.jp/wp-content/uploads/*",
        "*://shoutfakes.weebly.com/uploads/*",
        "*://imgnews.pstatic.net/image/*",
        "*://image.fnnews.com/resource/*",
        "*://pbs.twimg.com/ad_img/*"
      ],
      "js": [
        "pics.js"
      ]
    }
  ]
}
