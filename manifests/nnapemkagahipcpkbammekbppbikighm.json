{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wave Selector",
  "version": "2.0",
  "description": "Build an Extension!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/einsteinlogo.png",
      "32": "images/einsteinlogo.png",
      "48": "images/einsteinlogo.png",
      "128": "images/einsteinlogo.png"
    }
  },
  "icons": {
    "16": "images/einsteinlogo.png",
    "32": "images/einsteinlogo.png",
    "48": "images/einsteinlogo.png",
    "128": "images/einsteinlogo.png"
  },
  "manifest_version": 2
}
