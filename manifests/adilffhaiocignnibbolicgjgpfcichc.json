{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "intento› Email Tracking",
  "author": "intento›",
  "version": "2.1.1",
  "description": "Avec intento› suivez facilement les échanges entre votre entreprise et chacun de vos contacts commerciaux, sur tous les supports.",
  "manifest_version": 2,
  "icons": {
    "128": "intento_128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "intento_128x128.png"
    },
    "default_title": "Intento Gmail Tracking"
  },
  "chrome_url_overrides": {},
  "permissions": [
    "storage",
    "https://mail.google.com/*",
    "http://mail.google.com/*",
    "http://*.google.com/*",
    "https://*.googleusercontent.com/*",
    "https://*.google.com/*",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://mail.google.com/*",
        "https://mail.google.com/*"
      ],
      "js": [
        "src/content.js"
      ],
      "css": [
        "src/content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "dist/extension.js"
  ]
}
