{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "InstagramChronologicalOrder",
  "short_name": "Instagram Chronological Order",
  "version": "1.0.1",
  "description": "Sort Instagram Home page by chronological order",
  "browser_action": {
    "default_icon": "assets/icon128.png",
    "default_title": "Sort Instagram"
  },
  "permissions": [
    "activeTab",
    "https://www.instagram.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "web_accessible_resources": [
    "sort.js"
  ]
}
