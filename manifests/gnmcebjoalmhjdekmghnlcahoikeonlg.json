{
  "author": "wallpapertab",
  "name": "San Francisco HD Wallpapers New Tab",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "default_locale": "en",
  "version": "1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "128.png"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://wallpapertab.com/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "Search Web...",
      "name": "Search Web...",
      "search_url": "https://wallpapertab.com/web/search?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "https://wallpapertab.com/web/search?q={searchTerms}",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/js/background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "identity",
    "tabs",
    "bookmarks",
    "topSites",
    "geolocation"
  ],
  "icons": {
    "128": "128.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
