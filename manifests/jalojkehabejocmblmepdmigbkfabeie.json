{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Autum",
  "short_name": "Autum",
  "description": "Autum helps you discover entertainment.",
  "author": "Autum",
  "version": "2.0.0",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "permissions": [
    "https://*.netflix.com/*",
    "https://*.imdb.com/*",
    "tabs"
  ],
  "web_accessible_resources": [
    "assets/*",
    "injected/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://autum.com/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://ext.autum.com; object-src 'self';"
}
