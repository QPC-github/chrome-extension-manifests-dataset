{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AmiBriefer",
  "description": "Get notified whenever the placement portal is updated.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "version": "1.4",
  "permissions": [
    "http://www.amity.edu/placement/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "AmiBriefer",
    "default_popup": "popup.html"
  }
}
