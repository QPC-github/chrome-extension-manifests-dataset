{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Metal Archives Exporter",
  "description": "Exports MA Data to a spreadsheet.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Export this page",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
