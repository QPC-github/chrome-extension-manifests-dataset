{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "17Track Search",
  "description": "A simple context menu that adds a 17Track Search function for selected text/tracking numbers",
  "version": "1.0.0.1",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "menu.js"
    ]
  },
  "icons": {
    "16": "icon.png"
  }
}
