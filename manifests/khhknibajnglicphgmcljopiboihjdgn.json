{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icons/plane-64.png",
    "default_title": "__MSG_name__",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/popup.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "icons/plane-128.png",
    "64": "icons/plane-64.png"
  },
  "name": "__MSG_name__",
  "version": "1.0.4",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*.travelpayouts.com/*"
  ]
}
