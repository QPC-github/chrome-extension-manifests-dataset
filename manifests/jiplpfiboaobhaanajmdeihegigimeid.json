{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabber Bookmarks",
  "version": "1.1.3",
  "description": "Adds bookmarks to your Tabber bookmark list",
  "homepage_url": "https://tabber.tech",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
