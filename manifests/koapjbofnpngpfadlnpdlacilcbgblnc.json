{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Series.ly points hack",
  "version": "1",
  "manifest_version": 2,
  "description": "Use series.ly without the points restriction",
  "browser_action": {
    "name": "Series.ly points hack",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_icon": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "hack.js"
      ],
      "matches": [
        "http://series.ly/*",
        "https://series.ly/*"
      ]
    }
  ]
}
