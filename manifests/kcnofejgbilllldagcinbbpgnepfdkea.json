{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PHP Search",
  "version": "0.2.2",
  "manifest_version": 2,
  "description": "Search PHP documentation in the Omnibox.",
  "icons": {
    "16": "php-logo-16.png",
    "48": "php-logo-48.png",
    "128": "php-logo-128.png"
  },
  "background": {
    "scripts": [
      "config.js",
      "indexer.js",
      "renderer.js",
      "index-data.js",
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "php"
  },
  "permissions": [
    "tabs",
    "http://php.net/"
  ],
  "minimum_chrome_version": "18"
}
