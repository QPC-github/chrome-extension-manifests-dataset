{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chale Keyboard",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "This extension lets you type your local language's special characters. Ghanaian special characters currently available for now.",
  "homepage_url": "https://play.google.com/store/apps/details?id=com.chale.android.softkeyboard",
  "icons": {
    "16": "icons/chale_16.png",
    "48": "icons/chale_48.png",
    "128": "icons/chale_128.png"
  },
  "browser_action": {
    "default_icon": "icons/chale_128.png",
    "default_popup": "chale_popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
