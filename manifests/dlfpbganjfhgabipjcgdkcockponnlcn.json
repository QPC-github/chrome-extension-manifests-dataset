{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Delete History Extension",
  "short_name": "clearhistory",
  "description": "Deletes chrome history when last tab is closed",
  "version": "1.1",
  "background": {
    "scripts": [
      "deletehistory.js"
    ]
  },
  "browser_action": {
    "default_title": "DeleteHistory on close"
  },
  "permissions": [
    "tabs",
    "history"
  ]
}
