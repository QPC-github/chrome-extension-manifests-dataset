{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mindful Click",
  "description": "Get personalize ethical rankings on athletic shoes!  Don't just buy a shoe that feels good; buy a shoe that does good.",
  "version": "0.2.4",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/logo38.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "16": "icons/logo16.png",
    "19": "icons/logo19.png",
    "32": "icons/logo32.png",
    "38": "icons/logo38.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  }
}
