{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Embrace Screensharing Utility",
  "description": "Embrace Screensharing Utility",
  "version": "1.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}
