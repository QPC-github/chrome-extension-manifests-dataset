{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VBox Live TV Extension",
  "short_name": "VBox Live TV",
  "version": "1.0.4",
  "description": "VBox Home TV Gateway",
  "icons": {
    "48": "img/vbox-48x48.png",
    "128": "img/vbox-128x128.png",
    "16": "img/vbox-16x16.png"
  },
  "browser_action": {
    "default_icon": "img/vbox-19x19.png",
    "default_title": "Connect to VBox Home TV Gateway",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
