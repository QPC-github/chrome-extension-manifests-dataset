{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Adetpia View All Clicker",
  "version": "1.1",
  "description": "Automatically clicks the View All button on Adeptia when viewing a project.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://dataexchange.iscorp.com:8443/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "icons": {
    "16": "images/ico.png",
    "32": "images/ico.png",
    "48": "images/ico.png",
    "128": "images/ico.png"
  },
  "manifest_version": 2
}
