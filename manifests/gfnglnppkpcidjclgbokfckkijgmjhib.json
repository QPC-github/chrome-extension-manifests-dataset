{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wallcat",
  "description": "Beautiful daily wallpapers for your new tabs.",
  "icons": {
    "16": "/public/img/icon_16x16.png",
    "32": "/public/img/icon_32x32.png",
    "256": "/public/img/icon_256x256.png",
    "512": "/public/img/icon_512x512.png"
  },
  "version": "1.1.2",
  "minimum_chrome_version": "22.0.0.0",
  "chrome_url_overrides": {
    "newtab": "index.htm"
  },
  "web_accessible_resources": [
    "font/*.woff2"
  ],
  "permissions": [
    "https://beta.wall.cat/",
    "storage",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "topSites",
    "chrome://favicon/"
  ]
}
