{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ClickFix for Ontraport",
  "version": "1.0.2",
  "description": "Helps you find input/select field ID and Name easily",
  "browser_action": {
    "default_title": "Start",
    "default_popup": "popup/index.html",
    "default_icon": {
      "32": "images/icon32.png",
      "128": "images/icon.jpg"
    }
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "page/page.css"
      ],
      "js": [
        "page/page.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ]
}
