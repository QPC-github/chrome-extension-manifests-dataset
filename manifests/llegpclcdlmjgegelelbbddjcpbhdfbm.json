{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "description": "Backtrace images like a sir. Get all the information easily from the image context menu",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "Image Backtrace!",
  "permissions": [
    "contextMenus",
    "http://*/*"
  ],
  "version": "1.1.2"
}
