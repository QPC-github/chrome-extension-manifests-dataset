{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "execute.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Nicer Inverter"
  },
  "description": "Darkens bright pages for easier night time reading.  Unofficial improvement of Inverter.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "name": "Nicer Inverter",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.8",
  "manifest_version": 2
}
