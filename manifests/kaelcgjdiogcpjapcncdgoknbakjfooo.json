{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Photo To Cartoon Editor",
  "description": "Convert any photo or image that you have into a cartoon with a single click using our APP!",
  "version": "0.0.0.2",
  "offline_enabled": false,
  "app": {
    "urls": [
      "http://photoparody.com/"
    ],
    "launch": {
      "web_url": "http://photoparody.com/"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
