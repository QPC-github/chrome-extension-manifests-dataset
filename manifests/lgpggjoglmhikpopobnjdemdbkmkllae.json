{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PassPressPlus",
  "description": "Bypass PressPlus popup commonly used on news sites.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.jpg",
    "48": "icon48.jpg"
  },
  "content_scripts": [
    {
      "css": [
        "nopressplus.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
