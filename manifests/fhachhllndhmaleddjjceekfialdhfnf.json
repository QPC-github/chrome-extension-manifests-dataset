{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "View All Offers",
  "description": "This extension will skip you straight to the Offers page on amazon",
  "version": "1.4.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
