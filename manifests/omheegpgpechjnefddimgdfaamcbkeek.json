{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.1",
  "name": "TsawqNet",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "start",
    "default_popup": "pop.html"
  },
  "icons": {
    "32": "tsawq.32.png",
    "48": "tsawq.48.png",
    "128": "tsawq.128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://api.tsawq.net/",
    "storage",
    "notifications",
    "background"
  ]
}
