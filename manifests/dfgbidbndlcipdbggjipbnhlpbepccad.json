{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery.js",
      "deflate.js",
      "base64.js",
      "md5.js",
      "animator.js",
      "lang.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/content_script.css"
      ],
      "js": [
        "jquery.js",
        "content_script.js"
      ],
      "matches": [
        "<all_urls>",
        "https://www.google.com/*"
      ]
    }
  ],
  "description": "JoiBiz Chrome Extension for 702 Helicopters.",
  "externally_connectable": {
    "matches": [
      "*://*.google.com/*"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "homepage_url": "http://702Helicopters.com",
  "name": "702 Helicopters JoiBiz,Elavon and Reviews",
  "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "notifications",
    "tabs",
    "unlimitedStorage",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "storage",
    "http://702helicopters.com/",
    "http://www.702helicopters.com/",
    "http://striptoursoverlasvegas.com/",
    "http://www.striptoursoverlasvegas.com/"
  ],
  "version": "1.0.25"
}
