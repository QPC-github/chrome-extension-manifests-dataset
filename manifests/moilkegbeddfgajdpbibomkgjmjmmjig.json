{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Increment++",
  "description": "Increments number in the end of address bar string. Convenient for navigating web pages with id.",
  "version": "1.0",
  "icons": {
    "96": "IDInc_96.png",
    "64": "IDInc_64.png",
    "48": "IDInc_48.png",
    "32": "IDInc_32.png",
    "16": "IDInc_16.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Increment++",
    "default_icon": "IDInc_128.png"
  },
  "author": "Freakytools",
  "short_name": "Increment++"
}
