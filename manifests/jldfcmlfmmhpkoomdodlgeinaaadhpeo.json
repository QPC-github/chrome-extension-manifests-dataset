{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "PriceInt helps you make good decisions and save money.",
  "icons": {
    "64": "icon64.png"
  },
  "manifest_version": 2,
  "name": "PriceInt",
  "permissions": [
    "tabs",
    "http://*/*",
    "webNavigation",
    "history",
    "storage"
  ],
  "version": "1.0.0"
}
