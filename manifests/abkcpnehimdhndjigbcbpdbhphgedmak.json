{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RapidPrint On-Demand Clothing Exporter",
  "short_name": "RapidPrint",
  "version": "0.0.63",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "48": "assets/shirt-small.png",
    "128": "assets/shirt-small.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "RapidPrint Exporter",
    "default_icon": "assets/shirt-small.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "assets/jquery.min.js",
        "assets/sweetalert.min.js",
        "content.js"
      ],
      "css": [
        "assets/swal.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/assets/jquery.min.js*",
    "*.local",
    "/settings.json"
  ],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "file://*/viewerbeta/*",
    "management",
    "contextMenus",
    "storage",
    "webRequestBlocking",
    "activeTab",
    "tabs"
  ]
}
