{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "brainstormer",
  "version": "1.0",
  "description": "random pictures, words and colours to help you brainstorm cool ideas",
  "manifest_version": 2,
  "author": "fejlesztő lány",
  "chrome_url_overrides": {
    "newtab": "brainstormer.html"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "brainstormer"
  },
  "permissions": [
    "*://colormind.io/*",
    "*://picsum.photos/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  }
}
