{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RegExp Incognito",
  "version": "1.1.2",
  "author": "Matthias Berla",
  "description": "Automatically switch to Incognito mode when a regex matches.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "history",
    "storage",
    "tabs"
  ]
}
