{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Prolitiken",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tillader dig at bruge politiken.dk uden abbonoment.. hvis du er for prol til at give 10kr om m√•neden",
  "icons": {
    "128": "prolitiken.png"
  },
  "permissions": [
    "tabs",
    "*://*.politikken.dk/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.politiken.dk/*"
      ],
      "css": [
        "hide.css"
      ],
      "js": [
        "extra.js"
      ],
      "run_at": "document_start"
    }
  ]
}
