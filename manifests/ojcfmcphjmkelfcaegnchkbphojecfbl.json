{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RollerDerp",
  "version": "1.0.1",
  "description": "Roller Derp gif image hide/show.",
  "permissions": [
    "tabs",
    "http://rollerderp.tumblr.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://rollerderp.tumblr.com/",
        "http://rollerderp.tumblr.com/page/*"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "content.js"
      ]
    }
  ]
}
