{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clicking Speed Test",
  "short_name": "Clicking Speed Test",
  "version": "3.1.1",
  "icons": {
    "48": "img/cst-icon-48.png",
    "128": "img/cst-icon-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "sandbox": {
    "pages": [
      "index.html"
    ]
  },
  "permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "offline_enabled": true
}
