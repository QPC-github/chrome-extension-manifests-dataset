{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TabelasNutricionais.net",
  "description": "Pesquise e compare calorias e valores nutricionais de alimentos comercializados no Brasil.",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "http://www.tabelasnutricionais.net",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
