{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SkyDome Forever",
  "description": "SkyDome Forever: A Chrome extension that replaces all instances of Rogers Center with SkyDome",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
