{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Click Limiter",
  "version": "1.0.1",
  "description": "Limits the number of clicks to one per second for accessibility assistance.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "app.min.js"
      ],
      "css": [
        "app.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
