{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "EFH",
  "version": "1.5.2",
  "manifest_version": 2,
  "description": "Revisa tus horas imputadas en el EFH.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Horas EFH",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "cookies",
    "http://efh.imit.cl/"
  ],
  "web_accessible_resources": [
    "js/background.js",
    "js/funcionesComunes.js",
    "js/popup.js"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
