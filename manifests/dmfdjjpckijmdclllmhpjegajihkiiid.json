{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CSP Disabler for developers",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Devtool - disable csp and x-frame",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
