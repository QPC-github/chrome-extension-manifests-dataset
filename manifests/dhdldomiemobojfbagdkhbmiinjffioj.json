{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rick Astley Bomb",
  "description": "Make every web page you visit turn into a rick astley video. Enjoy.",
  "version": "1.0.0",
  "icons": {
    "128": "icon.PNG"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
