{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Upside Down Politics",
  "description": "This extension changes certain political phrases and names to Stranger Things references",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "http://*/*"
  ]
}
