{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mindfactory Price Analyzer",
  "version": "1.3",
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "https://www.mindfactory.de/product_info.php*"
      ],
      "js": [
        "jquery-latest.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon128.png",
    "32": "icon.png"
  },
  "description": "Shows you the price history of products listed on mindfactory"
}
