{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LoR Un-Kitten",
  "version": "0.0.5",
  "description": "A quick workaround to keep the kittens busy and get LoR working again.",
  "homepage_url": "https://tunez.com",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://listenonrepeat.com/*",
        "https://listenonrepeat.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://listenonrepeat.com/*",
    "https://listenonrepeat.com/*",
    "https://www.youtube.com/*",
    "http://www.youtube.com/*",
    "https://www.youtube.com/iframe_api",
    "tabs"
  ]
}
