{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BugHerd: Visual Feedback Tool for Websites",
  "version": "1.25.33",
  "description": "BugHerd is a visual feedback tool for websites",
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "BugHerd"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "bugsnag.js",
      "main.js"
    ]
  },
  "homepage_url": "https://bugherd.com",
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js",
        "disabled_project.js",
        "found_project.js",
        "unauthenticated.js",
        "disabled_organization.js",
        "error.js",
        "loading.js"
      ]
    }
  ],
  "manifest_version": 2
}
