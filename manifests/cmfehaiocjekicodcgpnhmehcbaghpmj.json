{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Amazon Zocalo Web Clipper",
  "version": "1.0.62.1",
  "description": "Imports a web page into Amazon Zocalo.",
  "icons": {
    "16": "data/icons/icon16_hex.png",
    "48": "data/icons/icon48_full.png",
    "128": "data/icons/icon128_full.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "lib/utils.js",
      "lib/messaging.js",
      "lib/browser.js",
      "lib/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "data/icons/icon19_hex_gray.png",
      "38": "data/icons/icon38_hex_gray.png"
    },
    "default_popup": "data/popup.html",
    "default_title": "Amazon Zocalo Web Clipper"
  },
  "permissions": [
    "alarms",
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
