{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Browser privacy clean-up assistant",
  "version": "3.8",
  "description": "Help you clean up your Google Chrome browsing history and all kinds of private information",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "Clear browsing data"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "webRequest"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
