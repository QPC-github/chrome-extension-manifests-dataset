{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "you-to->Bit",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "A simple extension.",
  "icons": {
    "128": "./images/you-to-bit-icon-128x128.png"
  },
  "background": {
    "scripts": [
      "./dependencies/jquery-3.4.1.min.js",
      "./dependencies/notes.js",
      "background.js",
      "./dependencies/rxjs.umd.min.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "https://www.youtube.com/*",
    "http://www.youtube.com/*",
    "https://www.bitchute.com/*"
  ]
}
