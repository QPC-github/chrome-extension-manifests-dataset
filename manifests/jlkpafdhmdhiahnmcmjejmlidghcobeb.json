{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Close Others",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Close Other Tabs.",
  "icons": {
    "16": "close_others.png",
    "48": "close_others_24.png",
    "128": "close_others_128.png"
  },
  "browser_action": {
    "default_icon": "close_others.png",
    "default_title": "Close Others"
  },
  "background": {
    "scripts": [
      "close_others.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
