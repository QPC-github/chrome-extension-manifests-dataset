{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ely Games Day",
  "version": "3.0",
  "description": "Support EGD through Amazon",
  "icons": {
    "48": "EGD-48.png",
    "128": "EGD-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.amazon.co.uk/*",
        "https://www.amazon.co.uk/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "manifest_version": 2
}
