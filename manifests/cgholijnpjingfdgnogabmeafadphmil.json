{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Comment-It",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "https://mk-development.com/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Comment-It!",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "activeTab",
    "tabs"
  ],
  "web_accessible_resources": [
    "src/inject/inject.html",
    "src/inject/images/*"
  ]
}
