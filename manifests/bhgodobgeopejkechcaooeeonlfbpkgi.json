{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Inform ARIA Newsletter",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Sends the address of the active tab to ARIA Newsletter, in order for it to be published in ARIA Bulletin.",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icons/aria_nl_16.png",
    "48": "icons/aria_nl_48.png",
    "128": "icons/aria_nl_128.png"
  },
  "browser_action": {
    "default_title": "Send this interesting AI article to ARIA NewsLetter",
    "default_icon": "icons/send_page_19.png"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
