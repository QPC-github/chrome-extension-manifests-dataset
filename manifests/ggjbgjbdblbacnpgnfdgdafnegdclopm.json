{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HUDTube",
  "version": "1.2",
  "description": "HUDTube extension for opening videos in HUDTube.app",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "toolbar.png",
    "default_title": "HUDTube Player"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "events.js"
    ],
    "persistent": false
  },
  "incognito": "split",
  "homepage_url": "http://hudtubeapp.com"
}
