{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Auto Tab Switch"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://s3.amazonaws.com; object-src 'self'",
  "description": "Auto Tab Switch is the best extension to switch tab refresh tab and scroll tab on a timer ",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Auto Tab Switch",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
