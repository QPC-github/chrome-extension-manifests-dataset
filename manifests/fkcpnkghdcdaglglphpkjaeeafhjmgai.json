{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BD OpenVPN-2",
  "description": "OpenVPN session manager",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "https://accounts.google.com/*",
    "https://www.google.com/a/google.com/*",
    "https://insipid.mtv.corp.google.com/*",
    "clipboardWrite",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": {
      "19": "ovpn-disconnected-19x19.png",
      "38": "ovpn-disconnected-38x38.png"
    }
  },
  "icons": {
    "16": "ovpn-16x16.png",
    "48": "ovpn-48x48.png",
    "128": "ovpn-128x128.png"
  }
}
