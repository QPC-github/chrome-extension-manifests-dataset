{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "exaMeta - Quick Save",
  "version": "0.1.1",
  "description": "Quickly save & share useful links directly from the broweser using this Chrome extension.",
  "browser_action": {
    "default_icon": "images/em_logo_38x38.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://exameta.dev/",
    "http://localhost/",
    "http://exameta.com/",
    "tabs",
    "contextMenus"
  ]
}
