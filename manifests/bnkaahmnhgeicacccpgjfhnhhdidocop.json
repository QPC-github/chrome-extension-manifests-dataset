{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "meet NetworkingLine",
  "description": "Extension simple para hacer videoconferencias",
  "version": "0.1",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "netline-logo-16x16.png",
    "48": "netline-logo-48x48.png",
    "128": "netline-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.networkingline.com/*"
    ]
  }
}
