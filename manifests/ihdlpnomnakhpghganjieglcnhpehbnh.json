{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "DBox Chrome Extension",
  "description": "Send download tasks to DBox",
  "version": "1.1.1",
  "manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "default-src 'self'",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  }
}
