{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Grid Preview For Google Reader",
  "version": "1.6",
  "manifest_version": 2,
  "description": "Grid Preview For Google Reader by Kadir 'Akkara' Yardımcı",
  "permissions": [
    "http://www.google.com/reader/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/reader/*",
        "https://www.google.com/reader/*",
        "http://*/reader/view/*",
        "https://*/reader/view/*"
      ],
      "css": [
        "gridview.css"
      ],
      "js": [
        "gridview.js"
      ]
    }
  ]
}
