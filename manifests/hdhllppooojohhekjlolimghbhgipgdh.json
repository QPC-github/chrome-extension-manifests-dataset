{
  "manifest_version": 2,
  "name": "League of Legends (LoL) Event Schedule",
  "version": "2.5.0",
  "background": {
    "page": "index.html#/background",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Watch events!",
    "default_popup": "index.html#/popup"
  },
  "icons": {
    "19": "assets/icon.png",
    "128": "assets/icon.png",
    "16": "assets/icon.png",
    "24": "assets/icon.png",
    "256": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "512": "assets/icon.png",
    "64": "assets/icon.png"
  },
  "short_name": "LES",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' "
}
