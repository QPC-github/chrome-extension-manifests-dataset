{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Turathi",
  "version": "1.1",
  "description": "البحث عن اي مقطع مظلل في موقع تراثي بالضغط على زر الفأرة الأيمن",
  "permissions": [
    "contextMenus"
  ],
  "browser_action": {
    "default_title": "Turathi",
    "default_icon": "images/32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "manifest_version": 2
}
