{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jargon.ai Screen Sharing",
  "author": "Jargon <support@jargon.ai>",
  "version": "0.0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Jargon users can now share their screens during a video call. More at https://www.jargon.ai/",
  "homepage_url": "https://www.jargon.ai/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.thinkloot.com/*",
      "https://*.jargon.ai/*",
      "http://localhost:8080/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "logo128transparent.png"
  ]
}
