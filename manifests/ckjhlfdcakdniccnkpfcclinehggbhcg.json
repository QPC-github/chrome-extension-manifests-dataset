{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Drawer",
  "description": "easy to store the tabs that on the window or open it",
  "version": "0.2",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "Drawer",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
