{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jumboed",
  "short_name": "Jumboed",
  "description": "This extension jumboes your URL.",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "http://www.jumboed.com/",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
