{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "inject.js"
      ],
      "matches": [
        "http://*.forbes.com/*",
        "https://*.forbes.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Skips annoying intersitials on certain sites.",
  "key": "",
  "manifest_version": 2,
  "name": "Skipper",
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.4"
}
