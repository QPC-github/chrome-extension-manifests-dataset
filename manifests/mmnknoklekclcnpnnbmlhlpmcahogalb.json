{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "__MSG_appName__",
  "version": "1.2.0",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "icons/logo-128.png",
    "48": "icons/logo-48.png",
    "16": "icons/logo-16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "ch.js"
      ]
    }
  ]
}
