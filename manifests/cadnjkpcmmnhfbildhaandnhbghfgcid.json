{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jira Copy Paste",
  "version": "1.0",
  "description": "Select multiple JIRA issues to copy their details.",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  }
}
