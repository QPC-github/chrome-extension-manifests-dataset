{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wi.to Image Uploader",
  "default_locale": "en",
  "version": "1.0",
  "description": "__MSG_app_description__",
  "icons": {
    "180": "assets/img/180_icon.png",
    "192": "assets/img/192_icon.png"
  },
  "background": {
    "scripts": [
      "assets/scripts/jquery.min.js",
      "assets/scripts/app.js",
      "assets/scripts/upload.js"
    ]
  },
  "browser_action": {
    "default_title": "Wi.to Image Uploader",
    "default_icon": "assets/img/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "notifications",
    "*://*/*",
    "clipboardWrite"
  ]
}
