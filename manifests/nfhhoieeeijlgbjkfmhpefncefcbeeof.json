{
  "manifest_version": 2,
  "name": "SearchInfo",
  "version": "1.0.1",
  "description": "Search for up-to-date information",
  "web_accessible_resources": [
    "index.html"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "http://searchinfo.pro/32.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "si",
      "name": "SearchInfo",
      "search_url": "http://searchinfo.pro/go/?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "",
      "suggest_url_post_params": ""
    }
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png"
  },
  "omnibox": {
    "keyword": "i"
  },
  "permissions": [
    "http://*/*",
    "tabs",
    "storage",
    "cookies"
  ],
  "short_name": "Search Info",
  "update_url": "https://clients2.google.com/service/update2/crx"
}
