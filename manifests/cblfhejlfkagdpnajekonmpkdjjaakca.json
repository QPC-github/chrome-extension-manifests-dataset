{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SFRef Utility",
  "description": "This extension will help you extract matches from Arbiter and pensra.org(Ridgestar). Gameofficials support is in Beta",
  "version": "1.5",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "SFRef Extract Matches"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "tabs"
  ]
}
