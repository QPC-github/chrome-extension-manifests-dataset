{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Recruiterflow",
  "description": "Recruiterflow Chrome Extension",
  "version": "4.8.3",
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "activeTab",
    "https://recruiterflow.com/*",
    "downloads"
  ],
  "optional_permissions": [
    "file://*/*"
  ],
  "homepage_url": "https://recruiterflow.com",
  "background": {
    "scripts": [
      "js/browser-polyfill.min.js",
      "js/background.js",
      "js/jquery.min.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "views/*",
    "fonts/*",
    "img/*",
    "js/jquery.min.js",
    "js/load-gmail.js",
    "js/browser-polyfill.min.js"
  ],
  "key": "",
  "icons": {
    "48": "img/favicon48.png",
    "128": "img/favicon128.png"
  }
}
