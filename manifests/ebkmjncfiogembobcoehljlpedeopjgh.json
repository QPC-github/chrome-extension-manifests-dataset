{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "These Are The Nights That Never Die!",
  "name": "Avicii Theme",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Avicii"
  },
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "icons": {
    "64": "smallIcon.png",
    "128": "icon.png"
  },
  "incognito": "split"
}
