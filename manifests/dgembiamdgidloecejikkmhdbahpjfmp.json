{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tweet Newline",
  "version": "0.2.4",
  "manifest_version": 2,
  "description": "Twitter Webでの改行の表示を有効にします。",
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "default-icon": [
        "icon48.png"
      ],
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "newline.css"
      ]
    }
  ]
}
