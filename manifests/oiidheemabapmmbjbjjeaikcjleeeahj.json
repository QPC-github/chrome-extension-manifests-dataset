{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "asdphone.com",
  "description": "Much more than the yellow pages the most complete results of the information of the companies and professionals",
  "version": "0.1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "favicon_16.png",
    "default_title": "asdphone - Much more than the yellow pages"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "MacCtrl+Shift+A"
      },
      "description": "Opens index.html"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "favicon_48.png"
  ],
  "icons": {
    "16": "favicon_16.png",
    "48": "favicon_48.png",
    "128": "favicon_128.png"
  },
  "content_security_policy": "script-src 'self' https://www.asdphone.com; object-src 'self'"
}
