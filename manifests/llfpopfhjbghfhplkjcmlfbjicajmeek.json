{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Memoria Bookmark App Extension",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "https://hidden-bastion-43962.herokuapp.com/*/"
  ],
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
