{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "A simple extension that allows you to stream your desktop into meetings with PrimeVOX Video.",
  "externally_connectable": {
    "matches": [
      "*://video.primevox.net/*"
    ]
  },
  "icons": {
    "128": "pv-logo-128x128.png",
    "16": "pv-logo-16x16.png",
    "48": "pv-logo-48x48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "PrimeVOX Video Streamer",
  "permissions": [
    "desktopCapture"
  ],
  "version": "0.1.6"
}
