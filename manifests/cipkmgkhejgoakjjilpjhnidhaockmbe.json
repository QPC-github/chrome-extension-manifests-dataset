{
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon-16.png",
    "default_popup": "popup.html",
    "default_title": "Hoot multi tools"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://ssl.google-analytics.com https://ajax.googleapis.com https://cdnjs.cloudflare.com; object-src 'self'",
  "default_locale": "vi",
  "description": "Hoot multi tools",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png",
    "48": "images/icon-48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Hoot",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "cookies",
    "storage",
    "webRequestBlocking",
    "webRequest"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.9"
}
