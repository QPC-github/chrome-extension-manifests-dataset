{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "净化淘宝链接",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "净化淘宝链接，支持：淘宝、天猫、1688。",
  "homepage_url": "https://eallion.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Clean up",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "bg.html"
  },
  "permissions": [
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "tabs",
    "https://*.taobao.com/*",
    "https://*.tmall.com/*",
    "https://*.1688.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.taobao.com/*",
        "https://*.tmall.com/*",
        "https://*.1688.com/*"
      ],
      "js": [
        "jquery-1.7.2.js",
        "main.js"
      ],
      "run_at": "document_start"
    }
  ]
}
