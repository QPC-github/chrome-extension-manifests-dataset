{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Asana Auto Filter",
  "description": "Automatically filter Asana boards to show only your tasks.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://app.asana.com/*/board*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
