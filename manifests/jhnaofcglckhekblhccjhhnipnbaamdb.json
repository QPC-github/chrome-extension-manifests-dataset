{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Buffstream Extension",
  "version": "1.1",
  "description": "Removes everything but the video from buffstream streams.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://buffstreamz.com/*",
        "http://bfst.to/*",
        "http://buffstream.to/*"
      ],
      "js": [
        "./index.js"
      ]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
