{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My Cloud9",
  "Short_Name": "Cloud9",
  "description": "__MSG_description__",
  "minimum_chrome_version": "29",
  "version": "2.0.0.0",
  "offline_enabled": true,
  "default_locale": "en",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "64": "icon_64.png",
    "128": "icon_128.png",
    "256": "icon_256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "webview",
    "storage"
  ]
}
