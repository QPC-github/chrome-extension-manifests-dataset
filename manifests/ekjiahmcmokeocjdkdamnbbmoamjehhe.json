{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NewsPaperComicsStrips",
  "description": "Daily comics strips of Calvin&hobbes,Lio,Peanuts and Garfields",
  "short_name": "NPCS",
  "manifest_version": 2,
  "version": "1.1.0",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "16": "img/icon.png",
    "64": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "storage",
    "alwaysOnTopWindows",
    "webview",
    "<all_urls>",
    "https://www.google-analytics.com/*"
  ]
}
