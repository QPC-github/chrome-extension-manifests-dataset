{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CisionPoint",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Quick access to Cision's data module",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "name": "Contact Search Extension",
    "default_icon": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
