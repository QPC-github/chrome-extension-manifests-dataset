{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "2 Second Tumblr",
  "description": "Post directly to Tumblr from Chrome.",
  "version": "1.2.1",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://*.tumblr.com/",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  }
}
