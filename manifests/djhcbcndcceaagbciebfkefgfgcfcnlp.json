{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "New Tab",
  "description": "Shopping New Tab",
  "version": "6.1.4",
  "manifest_version": 2,
  "permissions": [
    "history",
    "topSites",
    "cookies",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "*://*.shopperstab.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Open Shopping New Tab"
  },
  "icons": {
    "128": "img/logo_128x.png",
    "16": "img/logo_16x.png",
    "48": "img/logo_48x.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
