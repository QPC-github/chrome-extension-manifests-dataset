{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Christmas Word Search Puzzles",
  "short_name": "Christmas Word Search",
  "description": "The best Christmas word search puzzles! Choose a size for each randomly generated, themed puzzle.",
  "version": "3.0",
  "manifest_version": 2,
  "offline_enabled": true,
  "permissions": [
    "storage"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  }
}
