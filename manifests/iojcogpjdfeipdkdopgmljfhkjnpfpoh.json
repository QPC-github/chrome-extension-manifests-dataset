{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Standard deviation for metacritic",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "description": "Includes the standard deviation next to review scores.",
  "content_scripts": [
    {
      "matches": [
        "http://www.metacritic.com/*"
      ],
      "js": [
        "std.js"
      ],
      "run_at": "document_end"
    }
  ]
}
