{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trematoda",
  "short_name": "Trema",
  "description": "They are internal parasites of molluscs and vertebrates. ",
  "version": "1.8",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "Trematoda.js"
      ]
    }
  ],
  "icons": {
    "24": "icon_24.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimited_storage",
    "http://stumpyinc.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
