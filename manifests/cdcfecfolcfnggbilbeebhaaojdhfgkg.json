{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self';style-src 'self' 'unsafe-inline' *;media-src *;",
  "description": "Change Your New Tab Background to Bitcoin Images",
  "manifest_version": 2,
  "name": "Bitcoin Custom Backgrounds",
  "permissions": [
    "storage"
  ],
  "version": "8",
  "icons": {
    "128": "images/icon.png"
  }
}
