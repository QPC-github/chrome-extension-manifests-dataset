{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WoW Token Tracker",
  "description": "Tracks WoW Token price in the action house market",
  "version": "1.0",
  "author": "Jaime Hidalgo Garc√≠a - JaimehRubiks",
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/functions.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "https://wowtoken.info/snapshot.json",
    "tabs"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
