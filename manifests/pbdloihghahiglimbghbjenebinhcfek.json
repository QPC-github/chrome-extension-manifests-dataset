{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Public Sessions Extensions Whitelist Test",
  "description": "This extension verifies Whitelisting of Extensions in Public session. Has Notifications,tabs(optional) and Bluetooth.",
  "version": "1.0.0.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "bluetooth": {
    "uuids": [
      "180D",
      "1809",
      "180F"
    ],
    "low_energy": true
  },
  "permissions": [
    "notifications",
    "enterprise.deviceAttributes",
    "browser",
    "certificateProvider"
  ],
  "optional_permissions": [
    "tabs",
    "http://www.google.com/",
    "bookmarks"
  ],
  "display_in_launcher": true
}
