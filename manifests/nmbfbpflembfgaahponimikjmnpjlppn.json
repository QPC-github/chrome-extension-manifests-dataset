{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Prettytab",
    "default_icon": "img/icon32.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://my.prettytab.com/favicon.ico",
      "is_default": true,
      "keyword": "EZ",
      "name": "Search",
      "search_url": "https://my.prettytab.com/search/?q={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "256": "img/icon256.png",
    "128": "img/icon128.png",
    "96": "img/icon96.png",
    "48": "img/icon48.png",
    "32": "img/icon32.png",
    "16": "img/icon16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "19.0.0.0",
  "name": "__MSG_name__",
  "permissions": [
    "tabs",
    "management",
    "https://*.prettytab.com/*",
    "http://*.prettytab.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.7"
}
