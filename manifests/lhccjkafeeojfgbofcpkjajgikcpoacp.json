{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZyngaTools",
  "version": "2.5",
  "manifest_version": 2,
  "description": "ZyngaTools disabled popup's and enabled Multiple Client for comfortable gaming.",
  "background": {
    "scripts": [
      "jquery-1.9.0.min.js",
      "background.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "contextMenus",
    "notifications",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "document_idle.js"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "include_globs": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
