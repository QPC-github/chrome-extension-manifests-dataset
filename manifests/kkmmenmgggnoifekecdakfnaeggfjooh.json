{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cota Proxy Segplan",
  "description": "Essa extensão apresenta o tempo restante da cota no proxy através de um cronômetro.",
  "version": "1.2",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "cronometro.js"
    ]
  },
  "permissions": [
    "https://secure.flickr.com/",
    "notifications",
    "tabs",
    "webRequest",
    "webNavigation",
    "management",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "icon.png",
    "icon-on.png",
    "util_max.js"
  ]
}
