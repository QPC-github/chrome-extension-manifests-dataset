{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube Everywhere",
  "description": "Lets YouTube videos play on any page with a YouTube Link",
  "version": "1.3.3",
  "icons": {
    "16": "img/ytlogo128.png",
    "48": "img/ytlogo128.png",
    "128": "img/ytlogo128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "img/youtube.png"
  ],
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://www.youtube.com/*",
        "*://www.reddit.com/",
        "*://www.reddit.com/?*"
      ],
      "run_at": "document_end",
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ]
    }
  ]
}
