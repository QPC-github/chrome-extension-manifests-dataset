{
  "background": {
    "scripts": [
      "js/searchUtil.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "html/NewTab.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "is_default": true,
      "encoding": "UTF-8",
      "favicon_url": "http://www.ultimatecloud.me/images/favicon.ico",
      "keyword": "trovi.com",
      "name": "Trovi",
      "search_url": "http://www.ultimatecloud.me/default?gd=EX1000004&ISID=ISID_ID&SearchSource=58&CUI=SB_CUI&UM=6&q={searchTerms}&PCSF=SU_DEFAULT",
      "suggest_url": "http://www.ultimatecloud.me/suggest/CSuggestJson.ashx?prefix={searchTerms}&PCSF=SU_SUGGEST"
    }
  },
  "description": "Find your favorite cloud sites from your Chromeâ„¢ New Tab.",
  "icons": {
    "128": "images/icons/128.png",
    "16": "images/icons/16.png",
    "48": "images/icons/48.png"
  },
  "manifest_version": 2,
  "name": "Ultimate Cloud",
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "Ultimate Cloud",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0.3",
  "web_accessible_resources": [
    "js/generalSearchPage.js",
    "html/NewTab.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*index.html?extid*"
      ],
      "js": [
        "js/injectExtID.js"
      ]
    }
  ]
}
