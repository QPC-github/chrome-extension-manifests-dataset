{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BitStamp EUR",
  "description": "This extension retrieves the bitcoin EUR price",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "javascript/bitStampChecker.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://www.bitstamp.net/",
    "alarms"
  ]
}
