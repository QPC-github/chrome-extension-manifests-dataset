{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "QUOTE.fm",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "The official QUOTE.fm extension.",
  "background": {
    "scripts": [
      "chrome/background.js"
    ]
  },
  "browser_action": {
    "title": "Click to open QUOTE.fm menu",
    "default_icon": "lib/chromeicon16px.png"
  },
  "icons": {
    "16": "lib/chromeicon16px.png",
    "48": "lib/chromeicon48px.png",
    "128": "lib/chromeicon128px.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>"
  ]
}
