{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Greenjob Schedule Live Meeting Extension",
  "description": "This extension allows to schedule live meetings for Greenjob platform.",
  "version": "1.3.0",
  "browser_action": {
    "default_icon": "schedule-live.png",
    "default_title": "Greenjob Live",
    "default_popup": "schedule-live.html"
  },
  "permissions": [
    "storage",
    "https://meet.greenjobinterview.net/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://meet.greenjobinterview.net/ https://ajax.aspnetcdn.com https://cdnjs.cloudflare.com; object-src 'self'"
}
