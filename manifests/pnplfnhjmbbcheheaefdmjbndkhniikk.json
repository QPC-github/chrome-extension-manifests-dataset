{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Matt Becker",
  "content_scripts": [
    {
      "js": [
        "js/backgrounds.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "https://*/_/chrome/newtab*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "js/contextmenu.js"
    ]
  },
  "web_accessible_resources": [
    "assets/icons/dl.png",
    "assets/icons/window.png"
  ],
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/Logo-No-Bleed-16.png",
      "24": "assets/icons/Logo-No-Bleed-24.png",
      "32": "assets/icons/Logo-No-Bleed-32.png"
    }
  },
  "icons": {
    "16": "assets/icons/Logo-No-Bleed-16.png",
    "48": "assets/icons/Logo-No-Bleed-48.png",
    "128": "assets/icons/Logo-No-Bleed-128.png"
  },
  "options_page": "/options.html",
  "description": "A lightweight extension to add high resolution backgrounds to your new tab page.",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "name": "Spruce",
  "short_name": "Spruce",
  "version": "1.1.0"
}
