{
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "assets/icon_48.png",
    "default_popup": "popup.html"
  },
  "description": "Играйте в игру \"Война и Мир\"",
  "icons": {
    "128": "assets/icon_128.png",
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "59",
  "name": "Война и Мир",
  "permissions": [
    "contentSettings",
    "background",
    "*://*.facebook.com/*",
    "*://*.vk.com/*",
    "*://*.ok.ru/*",
    "*://*.odnoklassniki.ru/*",
    "*://*.my.com/*",
    "*://*.mail.ru/*",
    "*://*.agium.com/*"
  ],
  "incognito": "spanning",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.2"
}
