{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cloud To Someone Else's Computer",
  "version": "1.0",
  "description": "Replaces the buzzword 'cloud' with 'someone else's computer'.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "replace.js"
      ],
      "run_at": "document_end"
    }
  ]
}
