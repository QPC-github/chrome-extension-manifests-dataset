{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Queue",
  "short_name": "Queue",
  "version": "0.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
