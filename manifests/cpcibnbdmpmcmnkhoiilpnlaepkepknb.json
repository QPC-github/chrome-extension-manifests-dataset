{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab",
  "description": "âš¡ A high performance new tab page that gets you where you need to go faster.",
  "version": "0.12.4",
  "version_name": "v0.12.4",
  "homepage_url": "https://github.com/MaxMilton/new-tab",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "history",
    "storage",
    "tabs",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "n.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "s.html"
  },
  "background": {
    "scripts": [
      "b.js"
    ],
    "persistent": true
  },
  "offline_enabled": true,
  "incognito": "not_allowed",
  "content_security_policy": "default-src 'none'; script-src 'self'; style-src 'unsafe-inline'; img-src data: chrome: *;"
}
