{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MOT",
  "description": "Make the current page red",
  "version": "2.0",
  "browser_action": {
    "default_icon": "icon-01.png",
    "default_popup": "mypopup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "alarms",
    "https://ajax.googleapis.com/",
    "*://*/*",
    "tabs",
    "webNavigation",
    "notifications"
  ],
  "manifest_version": 2
}
