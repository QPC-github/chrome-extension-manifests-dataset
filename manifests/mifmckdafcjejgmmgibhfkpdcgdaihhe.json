{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "exclude_globs": [],
      "exclude_matches": [],
      "include_globs": [],
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/mod/quiz/*",
        "http://*/moodle/mod/quiz/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "converted_from_user_script": true,
  "description": "Свободное программное обеспечение для проверки грамматики, пунктуации, орфографии и стиля",
  "manifest_version": 2,
  "name": "Проверка грамматики, орфографии и стиля",
  "version": "7.7",
  "permissions": [
    "management"
  ],
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Toggle feature foo"
    }
  }
}
