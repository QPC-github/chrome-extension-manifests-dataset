{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitLab ToDo Plus",
  "description": "GitLab ToDo Plus",
  "author": "Yohei",
  "version": "1.1.0",
  "manifest_version": 2,
  "default_locale": "ja",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_128.png",
    "default_title": "GitLab ToDo Plus",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "permissions": [
    "alarms",
    "notifications",
    "tabs",
    "storage"
  ]
}
