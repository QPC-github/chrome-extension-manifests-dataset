{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "js/WebExtension.js",
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "minimum_chrome_version": "29",
  "name": "__MSG_app_name__",
  "default_locale": "uk",
  "author": "АТ \"ІІТ\"",
  "icons": {
    "128": "icons/TabBar.128.png",
    "32": "icons/TabBar.32.png",
    "64": "icons/TabBar.64.png",
    "16": "icons/TabBar.16.png"
  },
  "background": {
    "scripts": [
      "js/WebExtension.js",
      "js/background.js"
    ]
  },
  "options_ui": {
    "page": "views/settings/settings.html",
    "open_in_tab": true
  },
  "version": "1.3.1.9",
  "options_page": "views/settings/settings.html",
  "manifest_version": 2,
  "permissions": [
    "nativeMessaging",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "128": "icons/TabBar.128.png",
      "32": "icons/TabBar.32.png",
      "64": "icons/TabBar.64.png",
      "16": "icons/TabBar.16.png"
    },
    "default_popup": "views/popup/popup.html"
  },
  "description": "__MSG_app_description__"
}
