{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Divesh Panwar",
  "version": "1.1",
  "description": "MEAN/MERN Stack Developer",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo_large16.png",
      "32": "images/logo_large32.png",
      "48": "images/logo_large48.png",
      "128": "images/logo_large.png"
    }
  },
  "icons": {
    "16": "images/logo_large16.png",
    "32": "images/logo_large32.png",
    "48": "images/logo_large48.png",
    "128": "images/logo_large.png"
  },
  "content_security_policy": "script-src 'self' https://use.fontawesome.com; object-src 'self'",
  "manifest_version": 2
}
