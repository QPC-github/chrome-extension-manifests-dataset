{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WikiClean",
  "description": "Helps you copy paste Wiki easily without links and citations!",
  "version": "0.1.8",
  "author": "Smankusors (Antony Kurniawan)",
  "homepage_url": "http://smankusors.16mb.com/webproj/03_WikiClean",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png",
    "256": "img/icon_256.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "64": "img/icon_64.png",
      "128": "img/icon_128.png",
      "256": "img/icon_256.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
