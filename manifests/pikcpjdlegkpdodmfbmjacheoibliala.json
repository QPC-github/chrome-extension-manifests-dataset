{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WhatTheRage",
  "version": "1.1",
  "manifest_version": 2,
  "description": "WhatTheRage - All the best GIFs to make it through the day!",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "WhatTheRage",
    "default_popup": "index.html"
