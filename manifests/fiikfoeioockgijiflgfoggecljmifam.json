{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/ico128.png",
    "default_title": "Password show"
  },
  "content_scripts": [
    {
      "js": [
        "libs/jquery-3.2.1.min.js",
        "password.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Password show",
  "icons": {
    "128": "img/ico128.png",
    "16": "img/ico16.png",
    "32": "img/ico32.png",
    "48": "img/ico48.png"
  },
  "manifest_version": 2,
  "name": "Password show",
  "short_name": "Password show",
  "version": "1.0"
}
