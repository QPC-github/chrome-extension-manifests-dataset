{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "healthconsult.com Screen Sharing",
  "author": "ehumanlife, Inc.",
  "version": "0.0.11",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Share the screen from your computer in a medical video visit or in a medical committee with up to 16 people.",
  "homepage_url": "https://www.healthconsult.com",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.healthconsult.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
