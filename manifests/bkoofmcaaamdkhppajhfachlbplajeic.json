{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MemeBoard",
  "version": "1.4.6",
  "description": "Some lit clean memes in your browser!",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/mem16.png",
      "32": "images/mem32.png",
      "48": "images/mem48.png",
      "128": "images/mem128.png"
    }
  },
  "icons": {
    "16": "images/mem16.png",
    "32": "images/mem32.png",
    "48": "images/mem48.png",
    "128": "images/mem128.png"
  },
  "manifest_version": 2
}
