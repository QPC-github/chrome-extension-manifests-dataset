{
  "background": {
    "scripts": [
      "tb.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "google.js",
        "notification.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Nie przegap żadnej okazji! Wyszukiwarka kodów CentrumKuponowe będzie Cię informowała, czy w sklepach online znajdują się kupony.",
  "icons": {
    "128": "skin/logo_128.png",
    "16": "skin/logo_16.png",
    "48": "skin/logo_48.png"
  },
  "manifest_version": 2,
  "name": "Kupony, kody rabatowe - CentrumKuponowe",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.53"
}
