{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SEO Audit Tool",
  "description": "Comprehensive SEO Audit with Actionable Insights in just one click",
  "short_name": "RoboAuditor",
  "version": "10.5",
  "icons": {
    "16": "robo16.png",
    "24": "robo32.png",
    "32": "robo48.png",
    "128": "roboauditor.png"
  },
  "browser_action": {
    "default_icon": "roboauditor.png",
    "default_title": "RoboAuditor | SEO Audit Tool"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
