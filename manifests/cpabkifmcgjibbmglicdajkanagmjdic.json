{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NewsZeit 提示器",
  "version": "1.4",
  "description": "显示你在 NewsZeit 上未读文章的数量。",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "https://www.newszeit.com/",
    "https://www.newszeit.com/",
    "tabs"
  ],
  "options_page": "options.html"
}
