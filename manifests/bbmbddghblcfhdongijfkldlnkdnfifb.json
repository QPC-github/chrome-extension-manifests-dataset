{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "name": "[Slizer] adblock for YouTube player",
  "description": "Blocks ads on YouTube â„¢ video player, etc",
  "version": "1.3.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "default-src 'self'; style-src 'self' 'unsafe-inline' 'unsafe-eval'; connect-src *; frame-src 'self' https://money.yandex.ru",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Block ADV",
    "default_popup": "popup.html"
  }
}
