{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extractor de entidades",
  "version": "1.1",
  "description": "Mejora tu SEO incluyendo las entidades de las que tratan tus competidores",
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "/images/extractor-entidades16.png",
    "32": "/images/extractor-entidades32.png",
    "48": "/images/extractor-entidades48.png",
    "128": "/images/extractor-entidades128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "/images/extractor-entidades16.png",
      "32": "/images/extractor-entidades32.png",
      "48": "/images/extractor-entidades48.png",
      "128": "/images/extractor-entidades128.png"
    },
    "default_title": "Extraer entidades"
  },
  "background": {
    "scripts": [
      "/js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "/images/*"
  ]
}
