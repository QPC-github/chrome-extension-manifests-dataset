{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Partstat Search",
  "version": "1.0.0",
  "description": "Allows an electronic component part number highlighted on any webpage to be searched in http://www.partstat.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "http://www.partstat.com/*"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "imageinfo-16.png",
    "48": "imageinfo-48.png",
    "128": "imageinfo-128.png"
  },
  "manifest_version": 2
}
