{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "StartForFans",
  "description": "Customize your homepage and newtab page with startforfans.com",
  "version": "2.8",
  "chrome_settings_overrides": {
    "homepage": "http://startforfans.com/",
    "startup_pages": [
      "http://startforfans.com/"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "start.html"
  },
  "permissions": [
    "webRequest",
    "cookies",
    "webRequestBlocking",
    "*://*.startforfans.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  }
}
