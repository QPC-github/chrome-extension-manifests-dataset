{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CurEx",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "The simpliest way to get currency rates from CBRF.",
  "options_page": "options.html",
