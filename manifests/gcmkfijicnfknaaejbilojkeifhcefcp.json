{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dark ESEA",
  "version": "1.6",
  "description": "Dark theme for ESEA's website.",
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/*.png",
    "images/*.jpg"
  ],
  "content_scripts": [
    {
      "css": [
        "darkesea.css"
      ],
      "matches": [
        "http://*.esea.net/*",
        "https://*.esea.net/*"
      ]
    }
  ]
}
