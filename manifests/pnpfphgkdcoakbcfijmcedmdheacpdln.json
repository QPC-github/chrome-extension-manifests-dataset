{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "logo_48.png",
    "default_popup": "popup/ext_popup.html",
    "default_title": "__MSG_extName__"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.7.1.min.js",
        "jquery.flot.min.js",
        "func.js",
        "startup.js",
        "w6m_latest.js"
      ],
      "matches": [
        "http://*/*"
      ]
    },
    {
      "js": [
        "jquery-1.7.1.min.js",
        "jquery.flot.min.js",
        "func.js",
        "startup.js",
        "w6m_latest.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.wishment.com; object-src 'self'",
  "default_locale": "zh_CN",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "logo_128.png",
    "48": "logo_48.png",
    "16": "logo_16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "__MSG_extName__",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "unlimitedStorage"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.6.2.5",
  "web_accessible_resources": [
    "logo_16.png",
    "logo_48.png",
    "logo_128.png",
    "popup/logo.png",
    "popup/popup_title_05.png",
    "popup/popup_title_10.png",
    "onepng/sitelogos.png",
    "w6m_latest.js"
  ]
}
