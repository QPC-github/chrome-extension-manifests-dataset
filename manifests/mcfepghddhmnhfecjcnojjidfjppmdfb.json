{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube streaming search button",
  "version": "1.0.0",
  "description": "Add streaming search button to youtube.com",
  "icons": {
    "16": "icon/16.png",
    "24": "icon/24.png",
    "32": "icon/32.png",
    "128": "icon/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
