{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Prevent Duplicated Tabs",
  "version": "1.3",
  "description": "Prevents having duplicated tabs, instead it focuses and reloads the initial one",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon-128.png"
  }
}
