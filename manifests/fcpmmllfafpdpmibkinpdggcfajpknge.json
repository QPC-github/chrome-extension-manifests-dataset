{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Saganworks",
  "description": "This extension allows you to quickly add image files and URLs to your Saganworks account.",
  "version": "2.1.8",
  "icons": {
    "16": "images/png/SW_ICON-RGB-4C.png",
    "48": "images/png/SW_ICON-RGB-4C.png",
    "128": "images/png/SW_ICON-RGB-4C.png"
  },
  "background": {
    "scripts": [
      "lib/jquery.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "popup.html",
    "settings.html",
    "login.html"
  ],
  "browser_action": {
    "default_icon": "images/png/smlogo.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "notifications"
  ]
}
