{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "https://abiesys.ru/@azot"
      ]
    },
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://abiesys.ru/testing/t*/q*"
      ]
    },
    {
      "js": [
        "content2.js"
      ],
      "matches": [
        "https://abiesys.ru/testing"
      ]
    }
  ],
  "icons": {},
  "web_accessible_resources": [
    "icons/*.png"
  ],
  "manifest_version": 2,
  "name": "a-a-test",
  "options_page": "options.html",
  "page_action": {
    "default_icon": "icons/helmet.png",
    "default_popup": "popup.html",
    "default_title": "A-a-test"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://abiesys.ru/*",
    "http://127.0.0.1/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.9"
}
