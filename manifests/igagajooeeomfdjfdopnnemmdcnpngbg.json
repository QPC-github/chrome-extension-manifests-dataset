{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "DeKi: Delete Kindle Items",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Batch delete Items from your kindle library @ amazon.com",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "19.png",
    "default_popup": "deki.html"
  },
  "permissions": [
    "tabs",
    "https://www.amazon.com/gp/digital/fiona/manage*"
  ]
}
