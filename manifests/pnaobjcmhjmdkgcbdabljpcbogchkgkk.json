{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "xkcd 1288",
  "version": "1.0",
  "description": "Plaintext substitutions that make reading news more fun :)",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "string_replacer.js"
      ],
      "run_at": "document_end"
    }
  ]
}
