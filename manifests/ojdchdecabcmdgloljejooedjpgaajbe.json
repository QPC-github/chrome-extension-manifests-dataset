{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GifsOnTumblr",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Brings GIFs back to Tumblr blogs/dashboard instead of WEBPs",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "<all_urls>"
  ]
}
