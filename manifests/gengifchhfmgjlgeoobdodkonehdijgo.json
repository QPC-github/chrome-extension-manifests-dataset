{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PolyMarks - Reinvent the Bookmark",
  "version": "1.5",
  "description": "Collect your tabs as 1 PolyMark. Relaunch them in 1 click.",
  "manifest_version": 2,
  "permissions": [
    "cookies",
    "notifications",
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "Images/icon48.png"
  ],
  "icons": {
    "16": "Images/icon16.png",
    "48": "Images/icon48.png",
    "128": "Images/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "PolyMarks",
    "default_icon": "Images/icon.png",
    "default_popup": "popup.html"
  }
}
