{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "App Store QR Coder",
  "version": "1.0",
  "description": "Add QR code to app page of App Store.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://itunes.apple.com/*/app/*/id*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
