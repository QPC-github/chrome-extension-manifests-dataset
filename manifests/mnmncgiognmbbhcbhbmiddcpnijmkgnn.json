{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Just Angular Search",
  "description": "Type 'ng' plus your keywords into the Omnibox to search.",
  "short_name": "Just Angular",
  "author": "Aaron Hood",
  "version": "0.0.2",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "omnibox": {
    "keyword": "ng"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "how-to.html"
  }
}
