{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "谷歌翻译",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "contentScript.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
