{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Skoopsi for Chrome",
  "version": "1.0.3.3",
  "description": "Skoop the web and ditch bookmarks for good.",
  "icons": {
    "128": "/assets/images/icons/anubis128.png",
    "48": "/assets/images/icons/anubis48.png",
    "16": "/assets/images/icons/anubis16.png"
  },
  "browser_action": {
    "default_icon": "/assets/images/icons/anubis16.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
