{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VASCO Smart Card Reader Extension",
  "version": "2.3.3.0",
  "manifest_version": 2,
  "description": "VASCO Smart Card Reader Chrome Extension",
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:8080/*",
      "http://*.vasco.com/*",
      "https://*.mydigipass.com/*"
    ]
  },
  "permissions": [
    "nativeMessaging"
  ]
}
