{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "#Sunnudagur á RÚV.is",
  "description": "Viðbót sem birtir tvít merkt #sunnudagur við hliðina á beinu útsendingunni á RÚV.is. Birtist eingöngu á milli 10 og 14 á sunnudögum.",
  "version": "1.0.2",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://ruv.is/ruv*",
        "http://www.ruv.is/ruv*"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ]
}
