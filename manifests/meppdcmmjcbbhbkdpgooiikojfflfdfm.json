{
  "content_scripts": [
    {
      "js": [
        "theia.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "bookmarks for web! use ctrl + m to create new bookmarks!",
  "icons": {
    "128": "bookmarks_128x128.png",
    "16": "bookmarks_16x16.png",
    "48": "bookmarks_48x48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "web bookmarks",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "ftp://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
