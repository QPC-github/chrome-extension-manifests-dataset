{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon_16.png",
