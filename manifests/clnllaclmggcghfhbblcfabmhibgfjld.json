{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK auto iframe",
  "version": "1.0.0",
  "description": "Расширение для подгрузки ссылок через iframe",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "js": [
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/vkTestIframe.js"
  ]
}
