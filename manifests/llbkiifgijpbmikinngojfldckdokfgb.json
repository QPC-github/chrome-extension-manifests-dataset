{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Notificaciones YouTube (DEMO)",
  "manifest_version": 2,
  "version": "0.0.0.9",
  "short_name": "YouTube Notificacion",
  "icons": {
    "16": "ima/icon16.png",
    "48": "ima/icon48.png",
    "128": "ima/icon128.png"
  },
  "description": "Probando notificaciones, para un canal de youtube",
  "browser_action": {
    "default_icon": {
      "48": "ima/icon48.png",
      "128": "ima/icon128.png"
    },
    "default_title": "Notificaciones",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery-2.1.3.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "notifications",
    "*://*.youtube.com/*",
    "*://*.ytimg.com/*",
    "tabs"
  ]
}
