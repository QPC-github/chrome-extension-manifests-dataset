{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "遨游收藏夹",
  "version": "1.0",
  "manifest_version": 2,
  "description": "在chrome中可快速访问遨游在线收藏夹",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://fav.maxthon.cn/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
