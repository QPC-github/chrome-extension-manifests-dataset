{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MojeVyhody.cz",
  "version": "1.2",
  "manifest_version": 2,
  "homepage_url": "http://mojevyhody.cz",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "/js/background.js"
    ]
  },
  "default_locale": "cs",
  "description": "Peníze zpět za každý nákup! Vrátíme Vám až 30 % z Vašich on-line nákupů!",
  "icons": {
    "256": "images/icon_256.png",
    "128": "images/icon_128.png",
    "16": "images/icon_16.png",
    "48": "images/icon_48.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "notifications",
    "storage",
    "webNavigation",
    "cookies",
    "privacy"
  ],
  "web_accessible_resources": [
    "*"
  ]
}
