{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Monster Hunter Meowstress Image Replacer",
  "version": "0.1",
  "web_accessible_resources": [
    "nekojo/*.jpg",
    "nekojo/*.png",
    "nekojo/*.PNG"
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "nekojo.js"
      ]
    }
  ]
}
