{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TopLink",
  "version": "1.3.0",
  "manifest_version": 2,
  "description": "Reorders your Chrome Bookmarks in usage frequency order.",
  "minimum_chrome_version": "13.0.782.215",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "background",
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html"
}
