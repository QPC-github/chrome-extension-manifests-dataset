{
  "chrome_url_overrides": {
    "newtab": "pop.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/ https://accounts.google.com ; object-src 'self'",
  "description": "Simple New Tab Shows Apps, Most Visited Sites, Recently Closed, Bookmarks and Search Box at one place.",
  "icons": {
    "128": "icons/icon-128.png",
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png"
  },
  "manifest_version": 2,
  "name": "Simple New Tab",
  "options_page": "pop.html#options",
  "permissions": [
    "bookmarks",
    "unlimitedStorage",
    "<all_urls>",
    "chrome://favicon/",
    "fontSettings",
    "management",
    "http://*/*",
    "topSites",
    "https://*/*",
    "tabs",
    "sessions"
  ],
  "short_name": "Simple New Tab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
