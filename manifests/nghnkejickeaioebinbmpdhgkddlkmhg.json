{
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/lib/jquery.js",
      "scripts/lib/md5.min.js",
      "scripts/util.js",
      "scripts/lib/localforage.min.js",
      "scripts/config.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon/icon19.png",
      "38": "images/icon/icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "LianLian trade authenticity tool"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [],
      "js": [
        "scripts/lib/jquery.js",
        "scripts/lib/jquery.cookie.js",
        "scripts/lib/md5.min.js",
        "scripts/config.js",
        "scripts/util.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "为您采集贸易真实性信息",
  "options_page": "options.html",
  "icons": {
    "128": "images/icon/icon128.png",
    "16": "images/icon/icon16.png",
    "48": "images/icon/icon48.png"
  },
  "manifest_version": 2,
  "name": "连连贸易真实性信息采集工具",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab"
  ],
  "short_name": "LianLian TAT",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6",
  "web_accessible_resources": []
}
