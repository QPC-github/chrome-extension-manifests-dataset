{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GetList",
  "description": "Categorise saved purchases on Ebay.",
  "version": "1.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://cart.payments.ebay.com.au/*"
      ],
      "css": [
        "inject.css"
      ],
      "js": [
        "jquery.min.js",
        "inject.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
