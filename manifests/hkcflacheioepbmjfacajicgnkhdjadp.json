{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tocoda - Aide à la recherche immobilière",
  "short_name": "Tocoda",
  "description": "Amélioration des résultats de recherche sur les sites immobiliers",
  "version": "1.26",
  "icons": {
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "init.js"
      ],
      "matches": [
        "https://www.leboncoin.fr/*",
        "https://www.seloger.com/*",
        "https://www.logic-immo.com/*",
        "https://www.paruvendu.fr/*",
        "https://www.pap.fr/*",
        "https://www.bienici.com/*",
        "https://www.bellespierres.com/*",
        "https://www.avendrealouer.fr/*",
        "https://www.immoweb.be/fr/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_title": "Tocoda",
    "default_icon": {
      "128": "logo-128.png"
    }
  },
  "permissions": [
    "https://*/*"
  ],
  "web_accessible_resources": [
    "icon-home.png",
    "icon-alert.png"
  ],
  "manifest_version": 2
}
