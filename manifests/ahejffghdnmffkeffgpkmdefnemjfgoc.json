{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "scripts/fsXBrowserSupport.js",
      "scripts/fsUtils.js",
      "scripts/fsCommands.js",
      "scripts/fsBackground.js",
      "scripts/fsSSS.js",
      "scripts/fsPDF.js",
      "scripts/fsNativeAPISupport.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "fsPopup.html",
    "default_title": "超星截图"
  },
  "commands": {
    "last-used-action": {
      "description": "截图快捷键",
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      }
    }
  },
  "default_locale": "zh_CN",
  "description": "超星截图",
  "icons": {
    "128": "images/icon.png",
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png"
  },
  "manifest_version": 2,
  "name": "__MSG_application_title__",
  "offline_enabled": true,
  "optional_permissions": [
    "tabs",
    "<all_urls>",
    "downloads"
  ],
  "options_page": "fsOptions.html",
  "permissions": [
    "activeTab",
    "contextMenus",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "chaoxingScreenshot",
  "version": "0.00.02",
  "web_accessible_resources": [
    "images/*.gif"
  ]
}
