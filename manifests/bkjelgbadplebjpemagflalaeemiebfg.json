{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extension Hey!",
  "description": "Extensión necesaria para poder utilizar la aplicación Hey! U App",
  "version": "1.3.9",
  "background": {
    "scripts": [
      "jquery-1.7.2.min.js",
      "background.js"
    ]
  },
  "incognito": "split",
  "icons": {
    "48": "assets/icons/heyExtIcon_48.png",
    "96": "assets/icons/heyExtIcon_96.png"
  },
  "permissions": [
    "browsingData",
    "management",
    "tabs",
    "activeTab",
    "chrome://favicon/",
    "http://*/"
  ]
}
