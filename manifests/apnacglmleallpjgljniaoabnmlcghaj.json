{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EMI Calculator",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Calculate Monthly Installment (EMI) for Home Loan, Car Loan & Personal Loan",
  "homepage_url": "http://www.abhishekumar.com/",
  "icons": {
    "16": "icons/apps-icon-16-x16.png",
    "48": "icons/apps-icon-48-x48.png",
    "128": "icons/apps-icon-128-x128.png"
  },
  "browser_action": {
    "default_icon": "icons/apps-icon-16-x16.png",
    "default_title": "EMI Calculator",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "notifications",
    "cookies",
    "<all_urls>"
  ]
}
