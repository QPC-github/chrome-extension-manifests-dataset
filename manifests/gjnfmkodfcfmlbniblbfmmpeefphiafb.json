{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "elearn",
  "manifest_version": 2,
  "version": "1.3",
  "description": "Support elearing lecture for non-IE browsers",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://elearning.jejunu.ac.kr/Lesson.do*",
        "https://elearning.jejunu.ac.kr/MLesson.do*"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
