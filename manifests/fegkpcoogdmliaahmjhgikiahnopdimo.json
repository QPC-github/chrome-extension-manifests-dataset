{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Care to Search by Care to Click",
  "version": "1.48",
  "description": "Find product deals within search results!",
  "browser_action": {
    "default_icon": "resources/icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "background",
    "storage",
    "tabs",
    "*://*/*"
  ],
  "options_page": "resources/options.html",
  "manifest_version": 2,
  "web_accessible_resources": [
    "resources/click-here.png",
    "resources/compare-arrow.png"
  ]
}
