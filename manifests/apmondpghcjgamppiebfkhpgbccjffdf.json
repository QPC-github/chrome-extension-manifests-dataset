{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Element VoIP Screen Sharing",
  "description": "Screensharing utility",
  "version": "0.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
    "64": "logo64.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://*.elementvoip.com/*"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*.elementvoip.com/*"
      ]
    }
  ]
}
