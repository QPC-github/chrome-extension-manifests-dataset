{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WaxTabs",
  "description": "Save current tab for later.",
  "version": "0.55",
  "background": {
    "scripts": [
      "tabs.js"
    ]
  },
  "browser_action": {
    "default_icon": "plus2.png",
    "default_title": "Cache that tab!"
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "icons": {
    "16": "plus2.png",
    "48": "logo128.png",
    "128": "logo128.png"
  },
  "commands": {
    "cache_Tab": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Toggle cache url"
    },
    "popUp": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q"
      },
      "description": "Toggle popUp"
    }
  }
}
