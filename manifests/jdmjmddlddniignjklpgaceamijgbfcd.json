{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ChromeType",
  "description": "Adds typewriter noises to textareas when enabled.",
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "sound/*"
  ],
  "background": {
    "scripts": [
      "typebackground.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enable typewriter"
  },
  "manifest_version": 2
}
