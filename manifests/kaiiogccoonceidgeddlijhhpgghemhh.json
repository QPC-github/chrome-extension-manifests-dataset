{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "St. Mary Immaculate Parish Help Desk",
  "description": "This Chrome Extension is built for St. Mary Immaculate Parish. You can use this extension to quickly access their HelpDesk Portal.",
  "version": "0.0.2.7",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "desktopCapture"
  ],
  "options_page": "options.html",
  "icons": {
    "200": "img/icon.png"
  }
}
