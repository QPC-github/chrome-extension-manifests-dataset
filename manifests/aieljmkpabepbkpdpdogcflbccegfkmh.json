{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ittru Fusion",
  "version": "1.0.5",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://fusion.ittru.com/*",
        "https://ittrufusion.appspot.com/*",
        "https://ittrusign.appspot.com/*",
        "http://localhost:8888/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "nativeMessaging",
    "tabs"
  ],
  "externally_connectable": {
    "matches": [
      "https://fusion.ittru.com/*",
      "https://ittrufusion.appspot.com/*",
      "https://ittrusign.appspot.com/*",
      "http://localhost:8888/*"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "96": "icon96.png"
    },
    "default_title": "Ittru Fusion"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "96": "icon96.png"
  },
  "manifest_version": 2
}
