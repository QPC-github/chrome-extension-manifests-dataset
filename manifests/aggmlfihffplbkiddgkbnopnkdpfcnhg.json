{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Select first Button",
  "description": "Click this Button and first radio buttons on the current website will be selected",
  "version": "0.1.1",
  "browser_action": {
    "default_icon": "one.jpg",
    "default_title": "Radio one"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  }
}
