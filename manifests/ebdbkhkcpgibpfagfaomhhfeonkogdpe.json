{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DK Rake Colorator",
  "version": "1.2",
  "description": "Shows calculated rake percentage and color coding.",
  "web_accessible_resources": [
    "style.css"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "DK Rake Colorator "
  },
  "permissions": [
    "declarativeContent",
    "<all_urls>"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
