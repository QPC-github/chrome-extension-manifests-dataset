{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JStream",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Jewish Music Streaming for Google Chrome",
  "browser_action": {
    "default_icon": "48.png",
    "default_title": "JStream",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://jstream.brochos.com/"
  ]
}
