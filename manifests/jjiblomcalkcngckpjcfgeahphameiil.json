{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rocktober",
  "author": "Brandon Putre <brandon.putre@gmail.com>",
  "description": "Welcome to Rocktober!",
  "version": "0.7",
  "browser_action": {
    "default_title": "Rocktober"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
