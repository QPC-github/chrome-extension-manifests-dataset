{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "browser_action": {
    "default_icon": "img/engager-icon-16.png"
  },
  "description": " Increase your level of work engagement and productivity.",
  "icons": {
    "128": "img/engager-icon-128.png",
    "16": "img/engager-icon-16.png"
  },
  "manifest_version": 2,
  "name": "Engager",
  "permissions": [
    "tabs"
  ],
  "version": "1.3"
}
