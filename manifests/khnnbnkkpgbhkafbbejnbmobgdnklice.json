{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sans Style",
  "description": "Apply custom style to websites",
  "version": "1.0",
  "short_name": "CSSadder",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "sansstyle.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "SONJASAPPS_30x30.png",
    "default_popup": "popup.html",
    "default_title": "Sans Style"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
