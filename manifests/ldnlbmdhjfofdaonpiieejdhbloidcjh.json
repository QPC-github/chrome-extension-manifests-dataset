{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_title": "Add a url to Instapaper",
    "default_popup": "popup.html"
  },
  "description": "Adding URLs to Instapaper service",
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png"
  },
  "name": "InstaFiller",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://*.instapaper.com/*"
  ],
  "version": "1.1.3"
}
