{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SU Jobs Lisitng by Rahul",
  "description": "This extension shows a List of Recent SU Jobs in just a click ",
  "version": "2.0",
  "browser_action": {
    "default_icon": "icod.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icod3.png",
    "32": "icod1.png",
    "128": "icod.png"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Trigger_Me"
    }
  }
}
