{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Syracuse Orange",
  "version": "0.2",
  "description": "Make Facebook Orange",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icon.png"
    }
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "manifest_version": 2
}
