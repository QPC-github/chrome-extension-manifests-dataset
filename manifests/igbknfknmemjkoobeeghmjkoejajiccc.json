{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BP Top Tracker",
  "description": "",
  "default_locale": "en",
  "version": "1.0",
  "homepage_url": "http://www.bptoptracker.com/",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "BP Top Tracker"
  },
  "permissions": [
    "tabs",
    "http://www.bptoptracker.com/"
  ],
  "background": {
    "scripts": [
      "jquery-1.11.1.min.js",
      "redirect.js"
    ],
    "persistent": false
  }
}
