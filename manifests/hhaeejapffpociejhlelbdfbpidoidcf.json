{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My Todos",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "The easiest todo list in world! It sits right in your browser and reminds you each time you open a new tab!",
  "homepage_url": "http://partechsystems.com",
  "icons": {
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg.html",
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/todo.html"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
