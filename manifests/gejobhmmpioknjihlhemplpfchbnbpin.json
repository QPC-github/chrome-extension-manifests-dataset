{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SBAHJifier",
  "description": "Make any page look like SWEET BRO AND HELLA JEFF.",
  "version": "1.0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "SBAHJify!",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
