{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What Day Is Today",
  "description": "What Day Is Today, is a simple extension allow you to check specialities of each day",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html",
    "default_title": "What Day Is Today"
  },
  "permissions": [
    "activeTab"
  ]
}
