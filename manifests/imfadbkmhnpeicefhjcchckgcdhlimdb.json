{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SalesRep.ai Widget",
  "description": "Extension help representatives communicate with customers.",
  "version": "0.6.3.4",
  "permissions": [
    "notifications",
    "alarms",
    "webNavigation",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "socket.io.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "SalesRep.ai Widget",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
