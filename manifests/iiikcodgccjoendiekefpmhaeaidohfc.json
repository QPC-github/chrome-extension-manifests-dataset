{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Streaming Calculator Pro",
  "short_name": "StreamCalcPro",
  "description": "Streaming media calculator for internet radio or tv stations.",
  "manifest_version": 2,
  "version": "1.4",
  "permissions": [
    "webview",
    "system.network",
    "http://*/*",
    "https://*/*",
    "http://streamcalcpro.casterclub.com/*",
    "https://streamcalcpro.casterclub.com/*",
    "https://*.google.com/*",
    "http://*.google.com/*",
    "https://*.googleapis.com/*",
    "http://*.googleapis.com/*"
  ],
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "images/blue_calculator-16x16.png",
    "32": "images/blue_calculator-32x32.png",
    "64": "images/blue_calculator-64x64.png",
    "128": "images/streamcalcpro.png"
  }
}
