{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yemeksepeti Ürün Resimleri",
  "description": "Ürün listelerindeki resimleri, her ürünün altına iliştirir. Böylece resimlere bakarak seçim yapmanızı sağlar.",
  "author": "Oguz Gelal",
  "version": "1.0.6",
  "content_scripts": [
    {
      "matches": [
        "*://yemeksepeti.com/*",
        "*://*.yemeksepeti.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "jquery.min.js",
        "ysinject.js"
      ],
      "css": [
        "ysstyle.css"
      ]
    }
  ]
}
