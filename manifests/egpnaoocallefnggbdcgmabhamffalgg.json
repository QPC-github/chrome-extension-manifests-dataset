{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_popup": "popup.html"
  },
  "description": "inKurdish Translation",
  "icons": {
    "32": "32.png",
    "16": "16.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "name": "inKurdish.com",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+I",
        "mac": "Command+I",
        "chromeos": "Ctrl+I",
        "linux": "Ctrl+I"
      }
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "3.1"
}
