{
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Replace your classic white tab with dark background tab",
  "manifest_version": 2,
  "version": "1.2",
  "name": "Night Mode Tab",
  "icons": {
    "128": "images/nightmode128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
