{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Privacy Free",
  "description": "You are in a privacy free area and you are under 24 Hr surveillance",
  "version": "0.1.0",
  "icons": {
    "16": "favicon.ico",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "policeline.gif"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "css.css"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
