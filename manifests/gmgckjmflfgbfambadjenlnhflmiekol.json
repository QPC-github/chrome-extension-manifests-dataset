{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "bg.min.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "cs.min.1.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' data: chrome: filesystem:; connect-src 'self' http: https: chrome-extension:; frame-src http: https:",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://chrome.google.com/webstore/detail/foxyblocker/gmgckjmflfgbfambadjenlnhflmiekol",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "48": "img/icon_48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "unlimitedStorage",
    "notifications"
  ],
  "version": "0.3.2"
}
