{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OOT",
  "description": "Merge event with Pos",
  "icons": {
    "128": "img/icon-128.png"
  },
  "version": "4.3",
  "background": {
    "scripts": [
      "jquery-1.10.2.js",
      "popup.js",
      "jquery-ui.js"
    ]
  },
  "browser_action": {
    "default_title": "OOT",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>"
  ]
}
