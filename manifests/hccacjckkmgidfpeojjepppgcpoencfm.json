{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NABUU",
  "description": "This extension adds youtube videos and articles to your NABUU account",
  "version": "1.1.0",
  "browser_action": {
    "default_icon": "favicon_inverse.png",
    "default_popup": "popup.html",
    "default_title": "NABUU"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery-3.2.1.min.js"
      ]
    }
  ],
  "permissions": [
    "http://nabuuapp.com/",
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus"
  ]
}
