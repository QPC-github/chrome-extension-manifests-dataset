{
  "content_scripts": [
    {
      "js": [
        "index.js"
      ],
      "matches": [
        "*://smarthand.pro/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "name": "SmartHand Helper",
  "permissions": [
    "tabs",
    "storage",
    "clipboardRead",
    "*://smarthand.pro/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1"
}
