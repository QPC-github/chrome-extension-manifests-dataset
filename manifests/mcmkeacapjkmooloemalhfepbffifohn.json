{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy Open Favs",
  "description": "One-click to open all of your favorite pages.",
  "version": "1.2",
  "options_page": "options.html",
  "icons": {
    "19": "images/icon.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "background.html",
    "default_title": "Easy Open Favs - Setup in Options"
  }
}
