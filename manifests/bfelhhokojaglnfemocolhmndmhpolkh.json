{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stamp A Spot",
  "description": "Stamp any place you enjoy going and connect with others who like the same spot.",
  "version": "0.1.3.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "StampASpot-16.png",
    "128": "StampASpot-128.png"
  },
  "permissions": [
    "webview",
    "geolocation"
  ]
}
