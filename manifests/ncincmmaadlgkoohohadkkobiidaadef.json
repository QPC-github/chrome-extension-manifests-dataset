{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Petfinder Fixer",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Fix Petfinder.com Search Result Links",
  "content_scripts": [
    {
      "matches": [
        "http://www.petfinder.com/*"
      ],
      "js": [
        "jquery.js",
        "app.js"
      ],
      "run_at": "document_start"
    }
  ]
}
