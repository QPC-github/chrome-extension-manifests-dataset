{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Geevv",
  "manifest_version": 2,
  "description": "Geevv Search Engine",
  "version": "1.1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Geevv"
  },
  "chrome_url_overrides": {
    "newtab": "master.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
