{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URLBlock",
  "description": "data:text Ad locker, perfect for use with an adblocker to filter the extra unwanted popups",
  "version": "1.1",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "on.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  }
}
