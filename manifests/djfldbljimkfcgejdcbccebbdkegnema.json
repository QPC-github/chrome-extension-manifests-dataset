{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ain't",
  "short_name": "ain't",
  "description": "Rich Text Editor (RTE). Made by a writer... for writers. Made on a Chromebook... for Chromebooks.",
  "version": "2.4",
  "icons": {
    "16": "images/a16.png",
    "32": "images/a32.png",
    "64": "images/a64.png",
    "128": "images/a128.png",
    "256": "images/a256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "alwaysOnTopWindows",
    "storage",
    "tts",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ]
}
