{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jira Issue Opener",
  "description": "This extension lets you enter a JIRA issue key and opens a new tab to that issue",
  "version": "1.0.1",
  "short_name": "Jira Issue Opener",
  "homepage_url": "http://witty.blog",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "jira.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
