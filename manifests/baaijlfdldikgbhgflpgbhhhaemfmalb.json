{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/jelliphy-button.png",
    "default_title": "Jelliphy",
    "default_popup": "popup.html"
  },
  "description": "Jelliphy Loader allows to keep the Jelliphy GUI always active and to automatically apply Jelliphy customizations while navigating.",
  "icons": {
    "128": "img/jelliphy_128.png",
    "48": "img/jelliphy_48.png"
  },
  "name": "Jelliphy Loader",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "bookmarks"
  ],
  "version": "0.1001"
}
