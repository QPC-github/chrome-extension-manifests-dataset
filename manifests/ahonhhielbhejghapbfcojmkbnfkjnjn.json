{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Joe Biden to Joe Budden",
  "description": "Changes all instances of the phrase 'Joe Biden' on a webpage to 'Joe Budden.'",
  "version": "0.2",
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
