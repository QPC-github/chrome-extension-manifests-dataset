{
  "author": "Chartered Information Systems Pvt. Ltd",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Signer.Digital - Returns Signing, PDF Signing and Web User Auth, RSA Encryption/Decryption.",
  "manifest_version": 2,
  "name": "Signer.Digital",
  "page_action": {
    "default_icon": "icon32.png"
  },
  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.6.3"
}
