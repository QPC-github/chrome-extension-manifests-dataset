{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SetGov - Beta",
  "description": "Find out what presidential & local candidates actually support! Pick a race, learn about candidate's platforms & go vote!",
  "version": "0.0.8",
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.min.js; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "logo_no_circle.png"
    },
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
