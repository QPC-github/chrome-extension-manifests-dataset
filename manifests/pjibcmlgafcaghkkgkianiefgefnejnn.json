{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Real Duque",
  "version": "1.2",
  "description": "OINK OINK OINK!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "styles/twitter.css"
      ],
      "js": [
        "scripts/twitter.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "fonts/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
