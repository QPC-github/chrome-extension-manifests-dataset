{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "QuickMeme Tumblr button",
  "version": "2",
  "manifest_version": 2,
  "description": "Adds a Tumblr button to QuickMeme pages",
  "content_scripts": [
    {
      "matches": [
        "http://www.quickmeme.com/meme/*"
      ],
      "js": [
        "page.js"
      ]
    }
  ]
}
