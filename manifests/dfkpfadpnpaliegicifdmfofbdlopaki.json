{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Link Checker",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get total internal and external links on page",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
