{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Apps",
  "version": "1.1.1.0",
  "manifest_version": 2,
  "homepage_url": "https://singleclickapps.com/apps-button",
  "description": "Toolbar shortcut to the Apps page",
  "icons": {
    "16": "icon.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "offline_enabled": true,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Apps"
  },
  "incognito": "spanning",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html"
}
