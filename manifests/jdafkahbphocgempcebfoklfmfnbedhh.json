{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sideplayer",
  "description": "play video files and audio files in a local machine.",
  "default_locale": "en",
  "version": "0.0.0.1",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "play on the side",
    "default_icon": {
      "19": "icon19.png"
    }
  },
  "manifest_version": 2
}
