{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AppleJuiceForChrome",
  "version": "0.1.3",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "description": "Use this plugin to ensure michael gets his daily apple juice exposure",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "basic.css"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.jpg"
  ]
}
