{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Delete History on tab close and start",
  "description": "Delete History on tab close  and start",
  "version": "1.2",
  "permissions": [
    "history",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "history16.png",
    "32": "history32.png",
    "48": "history48.png",
    "128": "history128.png"
  }
}
