{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "lib/lodash.min.js",
        "lib/jquery.min.js",
        "script.js"
      ],
      "matches": [
        "*://twitter.com/*"
      ]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "description": "Tweet hider by MightyPork",
  "manifest_version": 2,
  "name": "Hide Any Tweet",
  "version": "1.0.1"
}
