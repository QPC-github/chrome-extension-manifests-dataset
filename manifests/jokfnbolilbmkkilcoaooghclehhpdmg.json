{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Assist",
  "version": "1.0",
  "manifest_version": 2,
  "description": "an assistant that locates certain shop websites or diplays google maps",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "MobiAssist",
    "default_icon": "assistant.ico",
    "default_popup": "assist.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com;object-src 'self'",
  "options_page": "options.html",
  "permissions": [
    "background",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
