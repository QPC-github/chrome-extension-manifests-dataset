{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My Reminder Chrome Plugin",
  "description": "This extension allows My Reminder users to add reminders without having to visit the website.",
  "short_name": "My Reminder",
  "version": "1.2",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
