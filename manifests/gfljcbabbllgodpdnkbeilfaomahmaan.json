{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Мульти поиск в Яндекс и Google в один клик",
  "icons": {
    "128": "images/128x128.png",
    "16": "images/16x16.png",
    "48": "images/48x48.png"
  },
  "manifest_version": 2,
  "name": "Мультипоиск. Multi Search",
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "storage",
    "http://multi-search.ru/"
  ],
  "version": "1.1.4"
}
