{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Juno.hu as default opener page",
  "description": "This extension open Juno.hu page as default new page.",
  "version": "1.0",
  "homepage_url": "http://juno.hu",
  "chrome_url_overrides": {
    "newtab": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "background",
    "http://*.google.com/"
  ]
}
