{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Things To Get Me",
  "version": "2.0",
  "description": "The no-fuss gift list maker. Add items to your gift list from any website.",
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "*://*.thingstogetme.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 2
}
