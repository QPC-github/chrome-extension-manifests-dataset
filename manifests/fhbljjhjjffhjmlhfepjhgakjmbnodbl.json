{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Barebones Google Play Music UI",
  "short_name": "Bare Bones",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "http://play.google.com/*",
        "https://play.google.com/*"
      ],
      "css": [
        "barebones.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
