{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0.51",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Click here"
  },
  "background": {
    "scripts": [
      "backgroundPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "identity.email",
    "background",
    "storage",
    "https://shopswain.auth0.com/*",
    "https://app.amzrefundfinder.com/*"
  ],
  "web_accessible_resources": [
    "refund-max.html"
  ]
}
