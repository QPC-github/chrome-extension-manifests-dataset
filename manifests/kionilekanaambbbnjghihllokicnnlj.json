{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Animation Generator",
  "version": "1.1.2",
  "description": "Generate CSS Animation code for your page",
  "background": {
    "page": "devtools/background.html"
  },
  "devtools_page": "devtools/devtools.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "sandbox": {
    "pages": [
      "devtools/sandbox.html"
    ]
  }
}
