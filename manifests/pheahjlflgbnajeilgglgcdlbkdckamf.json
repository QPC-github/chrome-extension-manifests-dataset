{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Checkvist Web Clipper",
  "description": "Capture and organize content from websites, issue trackers, Gmail, ZenDesk, Jira into sharable lists.",
  "version": "2.3.1",
  "background": {
    "scripts": [
      "frame/storage.js",
      "background_script.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "https://checkvist.com/",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon_bookmarklet_128.png",
    "default_title": "Save to Checkvist"
  },
  "commands": {
    "run-bookmarklet": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      },
      "description": "Open Checkvist Web Clipper"
    }
  },
  "web_accessible_resources": [
    "frame/layoutSimple.css",
    "frame/chosen-sprite.png",
    "frame/chosen-sprite@2x.png",
    "frame/times-square-light.svg",
    "frame/icon_192192_1.png",
    "frame/icons_sprite.svg",
    "frame/blue_circle.svg",
    "frame/lib/prototype.js",
    "frame/lib/maxkir/common/core.js",
    "frame/lib/maxkir/base.js",
    "frame/lib/maxkir/progress.js",
    "frame/lib/maxkir/ajax.js",
    "frame/lib/maxkir/ui/message.js",
    "frame/lib/maxkir/ui/kbd.js",
    "frame/lib/chosen.proto.js",
    "frame/lib/remarkable.js",
    "frame/lib/purify.js",
    "frame/lib/integrations.js",
    "frame/lib/render_util.js",
    "frame/add_popup.html",
    "frame/add_popup.css",
    "frame/add_popup.js",
    "frame/task_selector.js",
    "frame/completion_support.js",
    "frame/data_cache.js",
    "frame/storage.js",
    "frame/tribute/tribute.js",
    "frame/tribute/tribute.css",
    "options.html",
    "options.js",
    "options.css",
    "icon_bookmarklet_128.png"
  ],
  "icons": {
    "128": "icon_bookmarklet_128.png",
    "48": "icon_bookmarklet_48.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2
}
