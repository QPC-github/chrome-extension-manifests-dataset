{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "XYZurl.me {free url shortening}",
  "description": "This extension will allow users to shorten the URL of the page they are currently browsing.",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://www.xyzurl.me/"
  ]
}
