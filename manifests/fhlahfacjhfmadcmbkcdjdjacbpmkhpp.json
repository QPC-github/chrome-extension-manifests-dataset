{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Completed Listings on Ebay",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Completed Listings on Ebay ",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
