{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter :orig",
  "description": "Redirects Twitter pictures and profile banners to their full-size URLs.",
  "version": "0.0.6",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://twitter.com/*",
    "*://mobile.twitter.com/*",
    "*://pbs.twimg.com/media/",
    "*://pbs.twimg.com/profile_banners/",
    "*://pbs.twimg.com/profile_images/"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
