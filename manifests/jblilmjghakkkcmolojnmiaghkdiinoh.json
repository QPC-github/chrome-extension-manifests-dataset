{
  "name": "Agonia Poetry",
  "description": "quick access to a huge library of poems in 10 languages",
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_title": "Poetry by Agonia",
    "default_popup": "agonia.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
