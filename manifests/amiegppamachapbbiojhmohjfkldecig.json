{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hermit-xtension",
  "version": "1.4",
  "description": "Get up to date with the hermits!",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Hermit Craft"
  },
  "permissions": [
    "http://hermitcraft.com/api/hermit"
  ],
