{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TnP IIT Delhi Resume Builder",
  "version": "2.0",
  "manifest_version": 2,
  "browser_action": {
    "name": "TnP Reordering Data",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
