{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "bufBizButcher",
  "description": "Don't worry about it.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.bizjournals.com/*"
      ],
      "run_at": "document_start",
      "css": [
        "bbb.css"
      ]
    }
  ],
  "permissions": [
    "*://*.bizjournals.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
