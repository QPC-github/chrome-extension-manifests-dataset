{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WayMonkey Chrome Extension",
  "version": "1.0.7",
  "homepage_url": "http://www.waymonkey.com",
  "short_name": "WayMonkey ",
  "manifest_version": 2,
  "description": "WayMonkey",
  "content_scripts": [
    {
      "js": [
        "wmscript.js"
      ],
      "matches": [
        "http://www.waymarking.com/*"
      ]
    }
  ],
  "icons": {
    "16": "waymonkey_icon.png",
    "48": "waymonkey_icon_48.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "waymonkey_icon.png",
    "default_title": "WayMonkey",
    "default_popup": "popup.html"
  }
}
