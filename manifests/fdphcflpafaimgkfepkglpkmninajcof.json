{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Been there done that",
  "author": "Satish Patruni",
  "version": "1.2.2",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage",
    "identity"
  ],
  "short_name": "BTDT",
  "description": "Extension to remember what you did over a period of time",
  "homepage_url": "http://btdt.genxon.io/",
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "BTDT-16.png",
    "48": "BTDT-48.png",
    "128": "BTDT-128.png"
  },
  "browser_action": {},
  "oauth2": {
    "client_id": "534229919563-78cl6m80otp1aqr8mile0s54b87smo7f.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/user.emails.read"
    ]
  },
  "commands": {
    "toggle-pin": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      },
      "description": "Toggle tab pin"
    }
  }
}
