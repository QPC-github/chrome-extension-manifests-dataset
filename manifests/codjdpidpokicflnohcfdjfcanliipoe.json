{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Encrypto-Blog Reader",
  "description": "Decryption extension for Google Chrome.",
  "version": "1.4.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://heuuuth.blogspot.ie/*"
      ],
      "js": [
        "myHeuuuthScript.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "key.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "unlimitedStorage",
    "http://heuuuth.blogspot.ie/",
    "tabs",
    "notifications"
  ]
}
