{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CB Hide Ads",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Hide CB Ads",
  "permissions": [
    "https://chaturbate.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chaturbate.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "all_frames": true
    }
  ]
}
