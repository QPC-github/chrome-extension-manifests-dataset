{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "name": "Интернет-банк Faktura.ru",
  "short_name": "Faktura.ru",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Расширение для работы Интернет-банка Faktura.ru в браузере Google Chrome.",
  "icons": {
    "128": "sksj-plugin-logo.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.authority.ru/*",
      "*://*.besafe.ru/*",
      "*://*.faktura.ru/*",
      "*://*.bankplus.ru/*",
      "*://dbo.europlanbank.ru/*",
      "*://ibank.ziraatbank.ru/*",
      "*://*.cft.ru/*",
      "*://*.win.ftc.ru/*",
      "*://localhost/*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
