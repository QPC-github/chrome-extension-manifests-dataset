{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Overbuff Stats Tracker",
  "description": "Tracks stats between session to see improvements on overall ratings",
  "version": "1.3.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.overbuff.com/players/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "overbuff.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
