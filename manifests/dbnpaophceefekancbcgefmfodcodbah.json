{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Selflikes",
  "description": "A great way to add new Selflikes from any websites to your library!!",
  "version": "1.4",
  "browser_action": {
    "default_icon": "images/icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "http://www.selflikes.com/",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/jquery-2.1.4.min.js",
      "js/background.js"
    ]
  },
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  }
}
