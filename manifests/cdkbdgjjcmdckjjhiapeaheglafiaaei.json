{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Japanese Word of the Day",
  "version": "1.0.0",
  "description": "Learn new Japanese word every day. In the extension you'll find sample sentences with the word and native pronunciation.",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "16": "img/Icon_16.png",
    "19": "img/Icon_19.png",
    "48": "img/Icon_48.png",
    "128": "img/Icon_128.png"
  },
  "permissions": []
}
