{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Timetab",
  "description": "Replace the New Tab page to the page of your choice depending on the current time.",
  "version": "1.8",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "utils.js",
      "dataManager.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Timetab Settings",
    "default_popup": "popup.html"
  }
}
