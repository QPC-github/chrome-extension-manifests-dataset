{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "アメブロ記事検索",
  "version": "1.0.0",
  "description": "アメブロ内の記事を検索する",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "star/icon16.png",
    "32": "star/icon32.png",
    "64": "star/icon64.png",
    "128": "star/icon128.png"
  }
}
