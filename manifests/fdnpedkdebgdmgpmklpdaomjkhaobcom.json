{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Delicious.com"
  },
  "name": "Delicious.com",
  "description": "Unofficial Chrome extension for http://delicious.com. Based on official bookmarklet code.",
  "homepage_url": "http://aktagon.com/projects/chrome-extension-for-delicious",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.5",
  "manifest_version": 2
}
