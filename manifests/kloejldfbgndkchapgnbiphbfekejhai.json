{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tony's AutoFill",
  "version": "1.1.1",
  "description": "Automatically Fill in those DNS Settings",
  "browser_action": {
    "default_icon": "tonykidicon.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>"
  ]
}
