{
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "trangmall.com"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "onload.js"
      ],
      "matches": [
        "http://*.1688.com/*",
        "https://*.1688.com/*",
        "https://*.1688.com/*",
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://*.vip.com/*",
        "https://*.vip.com/*",
        "http://*.vipshop.com/*",
        "https://*.vipshop.com/*"
      ],
      "run_at": "document_end",
      "css": [
        "css/style.css"
      ]
    }
  ],
  "description": "Công cụ đặt(order) hàng trên hệ thống trangmall.com",
  "icons": {
    "512": "icons/icon.png"
  },
  "manifest_version": 2,
  "name": "trangmallcom",
  "short_name": "orderQC",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*.1688.com/*",
    "https://*.1688.com/*",
    "https://*.1688.com/*",
    "http://*.taobao.com/*",
    "https://*.taobao.com/*",
    "http://*.tmall.com/*",
    "https://*.tmall.com/*",
    "http://*.vip.com/*",
    "https://*.vip.com/*",
    "http://*.vipshop.com/*",
    "https://*.vipshop.com/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.6"
}
