{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabDecor",
  "version": "1.0.3",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "short_name": "Tab Decorator",
  "description": "Tab Decorator - Minimal Tab Decorator with Unsplash Images based on your Interest",
  "browser_action": {
    "default_icon": {
      "19": "icons/unicorn16x16.png",
      "48": "icons/unicorn48X48.png",
      "128": "icons/unicorn128X128.png"
    }
  },
  "icons": {
    "19": "icons/unicorn16x16.png",
    "48": "icons/unicorn48X48.png",
    "128": "icons/unicorn128X128.png"
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'"
}
