{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Sign up, shop online, make a difference!",
  "homepage_url": "http://www.whaleslide.com/",
  "manifest_version": 2,
  "name": "Liveries",
  "version": "2.3.9",
  "icons": {
    "128": "assets/images/icons/icon128.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "assets/images/icons/icon128.png"
    },
    "default_popup": "assets/html/popup.html"
  },
  "background": {
    "scripts": [
      "assets/js/global/config.js",
      "assets/js/global/settings.js",
      "assets/js/background/helpers.js",
      "assets/js/background/auth.js",
      "assets/js/background/postOffice.js",
      "assets/js/background/events.js",
      "assets/js/background/filter.js",
      "assets/js/background/options.js",
      "assets/js/background/init.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "assets/js/global/config.js",
        "assets/vendor/jquery.js",
        "assets/vendor/mutation-summary.js",
        "assets/js/popup/messages.js",
        "assets/js/popup/setContent.js",
        "assets/js/popup/helpers.js",
        "assets/js/content/templates.js",
        "assets/js/content/helpers.js",
        "assets/js/content/popup.js",
        "assets/js/content/icons.js",
        "assets/js/content/init.js"
      ],
      "css": [
        "assets/css/main.css"
      ]
    }
  ],
  "permissions": [
    "https://www.whaleslide.com/",
    "http://www.whaleslide.com/",
    "http://static.whaleslide.com/",
    "http://ext.whaleslide.com/",
    "http://devpeak.whaleslide.com/",
    "http://dev.whaleslide.com/",
    "*://aotb.mysearch.co.uk/",
    "notifications",
    "tabs",
    "cookies",
    "contextMenus",
    "activeTab",
    "webRequestBlocking",
    "webRequest",
    "storage",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "assets/images/*.gif",
    "assets/images/*.jpg",
    "assets/images/*.png",
    "assets/images/*.svg",
    "assets/css/fonts/*.ttf",
    "assets/css/fonts/*.woff2"
  ]
}
