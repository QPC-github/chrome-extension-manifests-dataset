{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RealComment",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Comment any website with the RealComment extension!",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
