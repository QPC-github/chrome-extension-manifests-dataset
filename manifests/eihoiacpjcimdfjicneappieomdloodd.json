{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "百度随心听",
  "version": "1.2",
  "description": "百度随心听是百度mp3推出的一个在线音乐服务。界面清爽简洁，操作简便，清新的音乐分类，不用添加任何播放列表，更不用费神想听什么，随时随地畅听好音乐。",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications",
    "webRequestBlocking",
    "webRequest",
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_title": "百度随心听",
    "default_icon": "icon.png"
  }
}
