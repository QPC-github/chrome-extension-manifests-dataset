{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RBSoft Tools",
  "version": "1.20",
  "description": "Tool-Bibliothek f√ºr Ansteuerung und Fokussierung von Browser-Tabs und Steuerung von iFrames",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "tabs",
    "webNavigation",
    "https://*.infocar.ch/"
  ],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "allFrames": true
    }
  ]
}
