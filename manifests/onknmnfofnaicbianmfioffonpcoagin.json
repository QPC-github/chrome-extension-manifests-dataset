{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PL News",
  "description": "This extension sends updates for different programming language libraries",
  "version": "1.0.3",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "pl_news.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "author": "Piyush Agarwal - piyush4793@gmail.com"
}
