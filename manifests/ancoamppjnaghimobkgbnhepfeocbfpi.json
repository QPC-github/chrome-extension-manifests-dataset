{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YAKjs context menu",
  "description": "This extension provides a context menu to run local YAKjs commands.",
  "version": "1.1.1.2924",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "yakjs-icon-16.png",
    "48": "yakjs-icon-48.png",
    "128": "yakjs-icon-128.png"
  },
  "background": {
    "scripts": [
      "yakjs-chrome-extension.js"
    ]
  }
}
