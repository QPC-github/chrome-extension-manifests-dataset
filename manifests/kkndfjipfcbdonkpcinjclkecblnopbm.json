{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "_escaped_fragment_= URL Switcher",
  "version": "1",
  "description": "Adds ?_escaped_fragment_= to URL and refreshes page. For testing crawling JavaScript web applications. #PhantomJS",
  "background": {
    "scripts": [
      "core.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "phantom16.png",
    "48": "phantom.png",
    "96": "phantom96.png",
    "128": "phantom128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "phantom16.png",
      "19": "phantom19.png",
      "38": "phantom38.png",
      "48": "phantom.png",
      "96": "phantom96.png",
      "128": "phantom128.png"
    },
    "default_title": "add _escaped_fragment_="
  }
}
