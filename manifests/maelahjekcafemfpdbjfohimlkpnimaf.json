{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Just Mute",
  "description": "This extension mutes tabs",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/mute.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "background"
  ]
}
