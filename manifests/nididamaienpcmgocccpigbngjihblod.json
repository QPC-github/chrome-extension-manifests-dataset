{
  "content_scripts": [
    {
      "css": [
        "css/emojiplus.css"
      ],
      "js": [
        "js/injection.js"
      ],
      "matches": [
        "*://vk.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "incognito": "split",
  "description": "Популярные стикеры бесплатно",
  "icons": {
    "128": "images/icons/icon128.png",
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Мои стикеры",
  "short_name": "Стикеры ВК",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [
    "js/emojiplus.js",
    "js/guruvk_im.js",
    "fonts/seguiemj.ttf",
    "images/emoji_sprite.png"
  ]
}
