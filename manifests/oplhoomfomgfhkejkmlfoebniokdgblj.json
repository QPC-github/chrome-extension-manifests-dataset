{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OGame Auto Lobby",
  "version": "1.7",
  "description": "Auto joins a server when game backs to lobby.",
  "permissions": [
    "https://*.ogame.gameforge.com/",
    "webNavigation",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "presistent": false
  },
  "browser_action": {
    "default_popup": "",
    "default_icon": {
      "32": "icon-off.png"
    }
  },
  "icons": {
    "16": "icon-off.png",
    "48": "icon-off.png",
    "128": "icon-off.png"
  },
  "manifest_version": 2
}
