{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unfuddle Jumper",
  "version": "1.4",
  "description": "Allows you to jump quickly to specific ticket in your logged in Unfuddle project(s).",
  "short_name": "FudJump",
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "ricon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "ricon16.png",
    "32": "ricon32.png",
    "48": "ricon48.png",
    "128": "icon128.png"
  },
  "author": "arallsopp",
  "manifest_version": 2
}
