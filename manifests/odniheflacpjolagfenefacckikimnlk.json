{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Well-Defined",
  "description": "Finds definitions for highlighted text in browsers.",
  "version": "0.2.5",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "definitionPopup.html",
    "default_title": "Click to open Well-Defined menu."
  }
}
