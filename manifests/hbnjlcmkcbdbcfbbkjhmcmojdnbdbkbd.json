{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Comment Disliker",
  "version": "0.0.0.1",
  "description": "It dislikes annoying Youtube comments.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "32": "icon.png"
  }
}
