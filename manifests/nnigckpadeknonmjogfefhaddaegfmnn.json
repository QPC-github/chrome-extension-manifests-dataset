{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto Refresh PowerBI Report",
  "description": "Automatically clicks Refresh button on Power BI report. Compatible with Power BI new look.",
  "version": "1.0.0.5",
  "browser_action": {
    "default_icon": {
      "19": "Assets/img/19.png",
      "38": "Assets/img/38.png"
    },
    "default_title": "Auto Refresh PowerBI Report",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "Assets/img/16.png",
    "19": "Assets/img/19.png",
    "38": "Assets/img/38.png",
    "64": "Assets/img/64.png",
    "128": "Assets/img/128.png"
  },
  "permissions": [
    "tabs",
    "https://*.powerbi.com/*"
  ]
}
