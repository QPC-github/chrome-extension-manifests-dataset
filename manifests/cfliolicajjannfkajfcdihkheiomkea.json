{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Calculadora de Apostas Múltiplas",
  "description": "Calcule de forma fácil os valores das suas apostas múltiplas.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "offline_enabled": true,
  "permissions": []
}
