{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "facebook-plus",
  "description": "Make facebook more productive",
  "version": "1.2",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "facebook-plus.js"
      ]
    }
  ]
}
