{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "myTips Visual Builder",
  "description": "Create on-screen tooltips and product tutorials without coding.",
  "version": "2.9",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "widget.js",
    "events.js"
  ],
  "browser_action": {
    "default_icon": {
      "38": "images/icon48.png"
    }
  },
  "icons": {
    "16": "images/icon48.png",
    "38": "images/icon48.png",
    "128": "images/icon128-active.png"
  }
}
