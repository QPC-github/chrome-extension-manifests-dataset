{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabstash",
  "description": "Store opened tabs for later use.",
  "version": "0.1",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "resources/icon.png",
    "default_popup": "index.html"
  }
}
