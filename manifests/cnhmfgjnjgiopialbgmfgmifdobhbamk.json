{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "This extension maps comments on metafilter",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "GraphFi extension",
  "short_name": "GraphFi",
  "content_scripts": [
    {
      "matches": [
        "https://*.metafilter.com/*"
      ],
      "js": [
        "jquery-3.2.1.slim.min.js",
        "app.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "version": "1.0.4"
}
