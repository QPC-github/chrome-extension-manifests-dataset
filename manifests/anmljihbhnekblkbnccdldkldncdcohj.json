{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "logoFree",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Watch movies online from your favourite websites with out logos on screen.",
  "icons": {
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.hotstar.com/movies/*"
      ],
      "js": [
        "hotstar.js"
      ]
    },
    {
      "matches": [
        "https://www.sunnxt.com/movie/*"
      ],
      "js": [
        "sun.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "logoFree"
  }
}
