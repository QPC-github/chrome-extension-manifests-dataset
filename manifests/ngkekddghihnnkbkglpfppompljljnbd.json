{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fizikist",
  "version": "2.0",
  "manifest_version": 2,
  "description": "https://www.fizikist.com",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "load.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js",
      "javascript.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "https://*/"
  ],
  "web_accessible_resources": [
    "48.png"
  ]
}
