{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Patter Notify",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Notifications for Patter Rooms and Private Messages in Chrome. Requires an app.net account.",
  "icons": {
    "48": "patter-48.png",
    "128": "patter-128.png"
  },
  "incognito": "split",
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-1.8.2.min.js",
      "patter-notify.js"
    ]
  },
  "permissions": [
    "notifications",
    "alarms",
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    "patter-small.png"
  ]
}
