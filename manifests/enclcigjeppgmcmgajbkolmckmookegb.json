{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Whois ?",
  "description": "Cliquez sur le bouton pour afficher, en 1 seconde, le Whois du nom de domaine de l'onglet actif. Simple et rapide !",
  "version": "1.1",
  "icons": {
    "128": "img/icon.png",
    "32": "img/icon_32.png",
    "16": "img/icon_16.png"
  },
  "web_accessible_resources": [
    "js/whois.js",
    "css/whois.css"
  ],
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
