{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ad and Stuff Blocker",
  "description": "Blocks ads and stuff",
  "version": "1.25",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_title": "Blocks ads and stuff"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
