{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Email link for Trello",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Email Trello card",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "lib/jquery-2.0.3.min.js",
        "trelloemaillink.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "lib/jquery-2.0.3.min.map"
  ]
}
