{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Horaires d’ouverture",
  "description": "Horaires d’ouverture",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/offen32x32.png",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "img/offen32x32.png",
    "48": "img/icon-trans.png",
    "128": "img/icon-trans.png"
  },
  "permissions": [
    "geolocation",
    "http://www.horairesdouverture24.fr/*"
  ]
}
