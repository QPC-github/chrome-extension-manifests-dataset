{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shopsta",
  "version": "1.0.1",
  "description": "Find important shopping information to popular online retailers and service providers with just one click!",
  "homepage_url": "http://couponlawn.com/",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation",
    "contextMenus",
    "webRequest",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "jquery-1.9.1.min.js",
        "ci.content.pack.js",
        "libs/jquery-no-conflict.js",
        "libs/handlebars.runtime.min.js",
        "libs/underscore-min.js",
        "libs/backbone-min.js",
        "libs/backbone-no-conflict.js",
        "libs/underscore-no-conflict.js",
        "shared/constants.js",
        "shared/utils.js",
        "content/style.js",
        "content/templates.js",
        "content/slider.js",
        "content/serp.js",
        "content/serp-handler.js",
        "content/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "ci.browser.helper.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/18x18-grey.png",
    "default_title": "Shopsta"
  },
  "icons": {
    "24": "images/24x24.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  }
}
