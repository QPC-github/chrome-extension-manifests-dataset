{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Awesome Second Home Button",
  "version": "1.2.0.0",
  "description": "Awesome button just like the home button, but set to a completely different location than the first home button.",
  "icons": {
    "16": "secondHome_16.png",
    "48": "secondHome_48.png",
    "128": "secondHome_128.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "secondHome_19.png",
    "default_title": "Awesome Second Home Button"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "manifest_version": 2
}
