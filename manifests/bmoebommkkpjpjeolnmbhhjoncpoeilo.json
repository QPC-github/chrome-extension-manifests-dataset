{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Meowify Extension",
  "description": "Meowify turns your everyday web browsing into a cat celebration!",
  "version": "1.1",
  "icons": {
    "16": "icon16.gif",
    "48": "icon48.gif",
    "128": "icon128.gif"
  },
  "permissions": [
    "https://www.google-analytics.com/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "meowify.js",
        "catwalk.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "meow1.wav",
    "src/meow1.wav",
    "src/meow1.wav",
    "src/meow2.wav",
    "src/meow3.wav",
    "src/meow4.wav",
    "src/meow5.wav",
    "src/meow6.wav",
    "img/catface.png",
    "/img/catface.png",
    "catface.png"
  ]
}
