{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PageText SearchLight",
  "description": "This extension highlights all words on the page of the word you have selected.",
  "version": "0.1",
  "permissions": [],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "background.css"
      ],
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
