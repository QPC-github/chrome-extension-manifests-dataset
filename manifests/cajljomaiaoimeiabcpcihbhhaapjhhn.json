{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube WPlayer",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "PluginPanel.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "Plugin.js"
      ]
    }
  ]
}
