{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Imgur expand shortcut",
  "description": "Expands all images on keypress on Imgur.",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*.imgur.com/gallery/*"
      ],
      "js": [
        "expand.js"
      ]
    }
  ],
  "manifest_version": 2
}
