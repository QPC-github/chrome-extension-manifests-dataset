{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.1.0",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/browserAPI.js",
        "/list.js",
        "/script.js"
      ],
      "css": [
        "/inject.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "/options.html"
  ],
  "browser_action": {
    "default_title": "__MSG_appName__",
    "default_icon": "icon128.png"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
