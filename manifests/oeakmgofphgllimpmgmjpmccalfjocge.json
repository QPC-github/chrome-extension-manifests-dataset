{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Automatic redirect to subscriptions",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "https://youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ]
    }
  ]
}
