{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Submit by The Edge 1.15",
  "version": "1.15",
  "manifest_version": 2,
  "description": "The fastest way to save articles to The Edge.",
  "icons": {
    "16": "img/16x16.png",
    "48": "img/48x48.png",
    "128": "img/128x128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "img/19x19.png",
      "38": "img/38x38.png"
    },
    "default_title": "TheEdge",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ]
}
