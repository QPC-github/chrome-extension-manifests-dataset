{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Drexel Student Helper",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "Provides a few life supporting features for Drexel University students.",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "permissions": [
    "http://*.drexel.edu/",
    "https://*.drexel.edu/",
    "notifications",
    "tabs",
    "background",
    "tts"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.8.2.min.js",
      "js/background.js"
    ]
  },
  "options_page": "html/options.html",
  "chrome_url_overrides": {
    "newtab": "html/newtab.html"
  }
}
