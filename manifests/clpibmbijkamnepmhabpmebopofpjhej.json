{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "FSL"
  },
  "content_scripts": [
    {
      "js": [
        "myscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "default-src 'none'; script-src 'self'",
  "description": "Replaces RSL-related words & phrases with preferred ones for Colorado Rapids fans. ",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "19": "icon19.png",
    "16": "icon16.png"
  },
  "manifest_version": 2,
  "name": "FSL",
  "short_name": "FSL",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.1"
}
