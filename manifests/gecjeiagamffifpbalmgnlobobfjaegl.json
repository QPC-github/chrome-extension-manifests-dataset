{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "includes/content.js"
      ]
    }
  ],
  "name": "UIDextractor V7.4 2015 Super Engine >> Mac/PC",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "web_accessible_resources": [
    "res/*"
  ],
  "description": "UIDextractor V7.52 2015 Super Engine >> Mac/PC",
  "background": {
    "page": "background.html"
  },
  "version": "1.9",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/button.png",
    "default_title": "UIDextractor V7.52 2015 Super Engine >> Mac/PC"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}
