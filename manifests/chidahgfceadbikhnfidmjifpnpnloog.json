{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "molink網路行銷",
  "manifest_version": 2,
  "short_name": "molink",
  "description": "網路行銷培訓、企業顧問輔導、網路創業",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "default16.png"
  },
  "icons": {
    "128": "128px_1316-1507252013.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
