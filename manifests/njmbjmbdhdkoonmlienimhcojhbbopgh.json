{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SanGadget",
  "version": "0.0.5",
  "description": "Hides the political articles from Engadget.com",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "matches": [
        "*://www.engadget.com/*"
      ]
    }
  ]
}
