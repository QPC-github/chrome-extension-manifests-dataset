{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "author": "StarTeam Media",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
