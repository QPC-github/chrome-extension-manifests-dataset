{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Add to Calendar",
  "version": "1.0.0",
  "description": "Create appointment in google calendar, copies the title of the page in subject and url in body automatically",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "calendar.png",
    "48": "calendar.png",
    "128": "calendar.png"
  },
  "browser_action": {
    "default_icon": "calendar.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
