{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "click-and-omni.js"
    ]
  },
  "omnibox": {
    "keyword": "playasia"
  },
  "name": "Search Play Asia",
  "description": "Useful tools for quickly searching Play Asia.",
  "version": "1.0.2",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "pasearch16.png",
    "48": "pasearch48.png",
    "128": "pasearch128.png"
  },
  "browser_action": {
    "default_icon": "pasearch48.png",
    "default_title": "Search Play Asia",
    "default_popup": "search.html"
  }
}
