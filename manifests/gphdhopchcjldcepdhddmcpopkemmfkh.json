{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Edit Any Webpage",
  "description": "Edit anywebpage For Fun",
  "version": "1.1",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Edit This Page!"
  },
  "icons": {
    "256": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}
