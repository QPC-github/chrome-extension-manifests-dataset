{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ManExt",
  "short_name": "M.E.",
  "description": "Manage - and group - your installed extensions",
  "version": "2.0.1",
  "author": "Mads Cordes",
  "icons": {
    "128": "assets/img/icon_128.png"
  },
  "permissions": [
    "management",
    "storage"
  ],
  "browser_action": {
    "default_icon": "assets/img/icon.png",
    "default_popup": "index.html"
  }
}
