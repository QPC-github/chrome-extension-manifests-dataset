{
  "name": "Body Mask",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "It keeps overlay on top of your browser",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
