{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KnO",
  "description": "Select text on web page to google.",
  "version": "1",
  "author": "Ajeet Kumar",
  "short_name": "KNO",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Select & Google"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ]
}
