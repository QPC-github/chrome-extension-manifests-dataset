{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alore Email Finder",
  "description": "Find verified business email addresses of millions people on LinkedIn, Angel.co or Twitter via our powerful email finder.",
  "version": "2.0.1.4",
  "browser_action": {
    "default_icon": "img/logo_Alore_Blue_16x16.png",
    "default_popup": "index.html",
    "default_title": "Alore Email Finder"
  },
  "icons": {
    "16": "img/logo_Alore_Blue_16x16.png",
    "32": "img/logo_Alore_Blue_32x32.png",
    "128": "img/logo_Alore_Blue_128x128.png"
  },
  "permissions": [
    "https://crm.alore.io/",
    "notifications",
    "tabs",
    "activeTab",
    "webNavigation",
    "storage",
    "background",
    "https://www.linkedin.com/*",
    "http://*/",
    "https://*/",
    "*://*/*"
  ],
  "web_accessible_resources": [
    "img/logo_Alore_Blue_128x128.png",
    "img/spinner.gif"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://use.typekit.net/qst7fcw.js; object-src 'self'"
}
