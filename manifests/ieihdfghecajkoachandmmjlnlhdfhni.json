{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Testlink projects sorting",
  "description": "This extension sort dropdown menu with projects.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "contentscript.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html"
}
