{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Webcam Favourite Model",
  "version": "0.1.3",
  "description": "Monitor your favourite models in Chartubate, MyFreeCams, Bongocams",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "Webcam Favourite Model",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
