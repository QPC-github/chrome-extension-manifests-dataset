{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/Globe.png"
  },
  "manifest_version": 2,
  "version": "1.0",
  "name": "Global with Murphy",
  "description": "One stop shop for all things Global History related.",
  "icons": {
    "128": "icons/Globe.png",
    "16": "icons/Globe.png",
    "48": "icons/Globe.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
