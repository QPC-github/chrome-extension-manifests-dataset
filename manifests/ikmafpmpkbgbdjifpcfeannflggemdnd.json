{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tally is ReDi",
  "short_name": "Redi",
  "description": "Tally is ReDi sync your Tally.",
  "version": "1.4",
  "icons": {
    "16": "assets/redi_16.png",
    "128": "assets/redi_512.png"
  },
  "browser_action": {
    "default_icon": "assets/redi_512.png"
  },
  "background": {
    "scripts": [
      "thirdParty/jquery-min.js",
      "script/background-min.js"
    ]
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'sha256-TaZSOqiQIbZADBblJxtY2YGItvMpb9mJsq5FhjhKu3U='; object-src 'self'"
}
