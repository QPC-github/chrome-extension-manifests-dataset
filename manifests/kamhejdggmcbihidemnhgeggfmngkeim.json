{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Goleando Broker Tool",
  "description": "Check the odds of players to change their prices.",
  "version": "1.2",
  "permissions": [
    "tabs",
    "http://golean.do/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://golean.do/stats/players*"
      ],
      "js": [
        "checker.js"
      ]
    }
  ],
  "icons": {
    "64": "favicon.ico"
  }
}
