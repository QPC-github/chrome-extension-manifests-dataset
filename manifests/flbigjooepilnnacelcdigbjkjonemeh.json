{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Subreddit",
  "version": "1.0",
  "description": "Allows user to type r/ <string> into their address bar to navigate to a subreddit!",
  "omnibox": {
    "keyword": "r/"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
