{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Plumb - Read Smarter",
  "short_name": "Plumb",
  "description": "Transform articles via natural language processing into a revolutionary reading format that allows you to read smarter and faster.",
  "version": "0.1.4",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "title": "Test"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/jquery.min.js",
      "js/parse.min.js",
      "background.js"
    ]
  }
}
