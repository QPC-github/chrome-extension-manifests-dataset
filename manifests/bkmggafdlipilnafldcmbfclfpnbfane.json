{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "landmax.pro launcher",
  "description": "This extension will help you to access your landmax.pro account with a single click",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon38.png"
    }
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
