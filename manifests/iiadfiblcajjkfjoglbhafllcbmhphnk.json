{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_popup": "html/adviceIndex.html",
    "default_title": ""
  },
  "permissions": [
    "<all_urls>",
    "webRequestBlocking",
    "tabs",
    "webRequest",
    "cookies",
    "storage",
    "*://*.cloudapif.com/*"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2,
  "name": "Advice from Artists",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "short_name": "Artists Advice",
  "version": "3.6.3",
  "background": {
    "scripts": [
      "js/axios.js",
      "js/adviceReq.js",
      "js/jquery-2.2.9.js"
    ]
  }
}
