{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Free Translator UTS Translations",
  "version": "1.0",
  "description": "Free Translator by UTS",
  "manifest_version": 2,
  "incognito": "split",
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*.googleapis.com/*"
  ]
}
