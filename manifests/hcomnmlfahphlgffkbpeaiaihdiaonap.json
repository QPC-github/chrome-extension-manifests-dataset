{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CustomTab",
  "manifest_version": 2,
  "description": "Lets you create a custom NewTab Page",
  "version": "0.2",
  "permissions": [
    "management",
    "topSites",
    "geolocation",
    "tabs",
    "chrome://favicon/",
    "http://api.openweathermap.org/data/2.5/forecast/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
