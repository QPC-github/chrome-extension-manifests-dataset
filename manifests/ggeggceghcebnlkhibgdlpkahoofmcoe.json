{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Filterr",
  "description": "This extension save your web page to your Filterr Account.",
  "version": "1.1",
  "background": {
    "page": "background/index.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "38": "img/icon38.png"
    },
    "default_popup": "popup.html",
    "default_text": "Save to Filterr"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
