{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1",
  "name": "Alt-Titler",
  "description": "Fill blank image TITLE attributes with what's in their ALT attribute, if found.",
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2
}
