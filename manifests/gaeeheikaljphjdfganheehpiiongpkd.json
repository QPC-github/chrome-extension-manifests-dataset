{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "fontbundles-128.png",
    "default_popup": "index.html",
    "default_title": "FontBundles.net Search"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Search fonts on FontBundles.net and open the results in a new tab.",
  "icons": {
    "128": "fontbundles-128.png",
    "64": "fontbundles-64.png",
    "48": "fontbundles-48.png",
    "32": "fontbundles-32.png",
    "16": "fontbundles-16.png"
  },
  "manifest_version": 2,
  "name": "FontBundles.net Search",
  "permissions": [
    "contextMenus",
    "tabs",
    "https://fontbundles.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
