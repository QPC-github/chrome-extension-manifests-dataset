{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SUGUCHIZU",
  "description": "行きたい場所へのルートがすぐ分かる！",
  "version": "0.9",
  "options_page": "options.html",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "contextMenus"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon48.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "SUGUCHIZU",
    "default_popup": "options.html"
  }
}
