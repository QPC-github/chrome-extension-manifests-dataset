{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "any2qr",
  "description": "Convert url/link/image url/select text to QR image",
  "version": "1.0.0",
  "homepage_url": "http://www.easybots.cn/",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "menu.js"
    ]
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "manifest_version": 2
}
