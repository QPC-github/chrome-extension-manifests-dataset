{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Redmine Issue Counter",
  "version": "0.1",
  "description": "Redmine Issue Counter",
  "permissions": [
    "storage",
    "tabs"
  ],
  "optional_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/script-execution.js",
      "js/common.min.js",
      "js/background.min.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/marketing8.png",
      "38": "images/marketing8.png"
    },
    "default_title": "Redmine Issue Counter"
  },
  "icons": {
    "48": "images/marketing8.png",
    "128": "images/marketing8.png"
  }
}
