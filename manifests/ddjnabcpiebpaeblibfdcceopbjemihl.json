{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tellmeword",
  "description": "Create and publish a blog post in a seconds with tellmeword.com",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "https://tellmeword.com/"
  ],
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "19": "img/icon19.png",
    "16": "img/icon16.png"
  },
  "browser_action": {
    "default_title": "Create new post",
    "default_icon": "img/icon48.png",
    "default_popup": "popup.html"
  }
}
