    "tabs",
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "np.css"
      ],
      "js": [
        "np_cs.js"
      ]
    }
  ]
