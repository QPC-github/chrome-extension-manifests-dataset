{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CotG+",
  "author": "Samuel Colbran (samuco.net)",
  "description": "Adds new features to Crown Of The Gods",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "essentials.user.js"
      ],
      "matches": [
        "https://*.crownofthegods.com/World00.php"
      ]
    }
  ],
  "permissions": [
    "cookies",
    "https://*.crownofthegods.com/"
  ],
  "web_accessible_resources": [
    "inject.js"
  ],
  "icons": {
    "16": "castle-16.png",
    "128": "castle-128.png"
  }
}
