{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Цитадель: Лига Героев",
  "short_name": "Цитадель: Лига Героев",
  "description": "Плагин для расширения функциональности онлайн игры Лига Героев",
  "version": "3.0.41",
  "homepage_url": "https://www.citadel-liga.info/liga/plugin",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://fantasyland.ru/*",
    "http://*.fantasyland.ru/*",
    "https://fantasyland.ru/*",
    "https://*.fantasyland.ru/*"
  ],
  "manifest_version": 2,
  "converted_from_user_script": true,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "http://fantasyland.ru/*",
        "http://*.fantasyland.ru/*",
        "https://fantasyland.ru/*",
        "https://*.fantasyland.ru/*"
      ],
      "js": [
        "LigaH.user.js"
      ],
      "all_frames": true,
      "matches": [
        "http://*.fantasyland.ru/*",
        "https://*.fantasyland.ru/*"
      ],
      "run_at": "document_end"
    }
  ]
}
