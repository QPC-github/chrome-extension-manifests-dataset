{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BOTS4 Reverse order of messages",
  "description": "This extension reverse the order of the messages given in a postoffice conversation.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://bots4.net/post-office/read/*"
      ],
      "js": [
        "Reverse_order_of_messages.user.js"
      ]
    }
  ],
  "permissions": [
    "http://bots4.net/"
  ]
}
