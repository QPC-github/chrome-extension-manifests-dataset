{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "自動延長判定",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Change Back Ground Color Specific Auctions",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://page.auctions.yahoo.co.jp/jp/auction/*"
      ],
      "js": [
        "backGround.js"
      ]
    }
  ]
}
