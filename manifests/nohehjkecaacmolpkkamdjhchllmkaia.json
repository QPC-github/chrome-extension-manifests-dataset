{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screen Time",
  "version": "0.0.0.1",
  "description": "Keep track of your time on the internet.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
