{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Invite all Friends [2019]",
  "description": "Invite All Friends To Like a Page or Join Event. With this addon you can invite all you friends instantly.",
  "version": "1.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "web_accessible_resources": [
    "script.js"
  ]
}
