{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RXWS Tester",
  "description": "An application for sending test requests to RXWS",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "1.0.0",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "clipboardWrite",
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  }
}
