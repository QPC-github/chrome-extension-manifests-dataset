{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Up ImageGrab",
  "description": "Grab images off the web, and manipulate them using up.feel.nu tools.",
  "version": "1.0",
  "background": {
    "scripts": [
      "imageGrab.js"
    ]
  },
  "icons": {
    "16": "up_logo_yellow.png",
    "48": "up_logo_yellow48.png",
    "128": "up_logo_yellow128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
