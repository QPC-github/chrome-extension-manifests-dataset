{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Eyelashes",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Search some text..",
  "homepage_url": "http://massinflux.com",
  "options_page": "src/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "contextMenus",
    "storage",
    "https://*/*",
    "http://*/*"
  ]
}
