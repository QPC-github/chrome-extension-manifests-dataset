{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Live Hero",
  "version": "2.6",
  "description": "Rhythm game that attaches to videos.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persist": true
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
