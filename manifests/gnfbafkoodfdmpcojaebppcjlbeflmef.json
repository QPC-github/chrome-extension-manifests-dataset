  "manifest_version": 2,
  "name": "Unicorn Speak 2.0.2000",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
