{
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Unicorn Speak 2.0.2000"
  },
  "icons": {
    "256": "icon.png",
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
