{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Find Korea from Dropdowns",
  "description": "Automatically finds Korea from html select tags (dropdowns) with right-click",
  "version": "1.2",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "manifest_version": 2
}
