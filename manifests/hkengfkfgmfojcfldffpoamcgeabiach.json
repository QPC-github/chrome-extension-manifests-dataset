{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Startup Button",
  "description": "One button, 20 different startups",
  "version": "10.7.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "The Startup Button",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
