{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "/content/external/underscore-min.js",
      "/content/external/string.min.js",
      "/content/external/jquery-2.1.1.min.js",
      "/content/search/search-engines.js",
      "/content/chrome/settings.js",
      "/content/common/prefs-sys.js",
      "/content/common/prefs.js",
      "/content/common/conf-sys.js",
      "/content/common/conf.js",
      "/content/common/settings-dev.js",
      "/content/external/md5.min.js",
      "/content/chrome/utils.js",
      "/content/chrome/common.js",
      "/content/common/abtest.js",
      "/content/common/udata.js",
      "/content/chrome/setup.js",
      "/content/common/nt_ptr.js",
      "/content/chrome/lifecycle.js",
      "/content/search/search-redirect.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/skin/icons/128.png",
      "16": "/skin/icons/16.png",
      "48": "/skin/icons/48.png"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://mychrome.cn/search/fav.png",
      "image_url": "",
      "instant_url": "",
      "is_default": true,
      "keyword": "SM",
      "name": "Search Assistant",
      "params": "__PARAM__",
      "search_url": "http://mychrome.cn/search/index.aspx?q={searchTerms}",
      "suggest_url": "http://mychrome.cn/search/index.aspx?s={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "/content/newtab.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "This extensions lets you to easily switch between search engines directly from the new tab page",
  "icons": {
    "128": "/skin/icons/128.png",
    "16": "/skin/icons/16.png",
    "48": "/skin/icons/48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "32",
  "name": "Search Assistant",
  "permissions": [
    "<all_urls>",
    "management",
    "storage",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "contextMenus",
    "bookmarks",
    "topSites"
  ],
  "sandbox": {
    "pages": []
  },
  "version": "1.0.0.0",
  "web_accessible_resources": [
    "/skin/icons/16.png"
  ]
}
