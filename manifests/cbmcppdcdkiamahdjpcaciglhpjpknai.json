{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LazyFox",
  "version": "0.0.1",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "yeah"
  },
  "browser_action": {
    "default_title": "That's the tool tip",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "mystyle.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
