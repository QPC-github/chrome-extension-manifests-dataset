{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "xinsheng tool",
  "version": "1.0.0",
  "description": "transfer '!@#$%^&*()' to '1234567890'",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "xinsheng tool"
  },
  "content_scripts": [
    {
      "matches": [
        "http://xinsheng.huawei.com/*"
      ],
      "css": [
        "bubble.css"
      ],
      "js": [
        "xinsheng.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "http://xinsheng.huawei.com/*"
  ],
  "homepage_url": "http://xinsheng.huawei.com/"
}
