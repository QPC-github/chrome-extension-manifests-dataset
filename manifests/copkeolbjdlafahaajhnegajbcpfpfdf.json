{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "Cosmic Shovel, Inc.",
  "background": {
    "scripts": [
      "scripts/common.js",
      "scripts/background.js"
    ]
  },
  "name": "배송대행신청서 자동등록",
  "description": "해외쇼핑으로 구매한 제품의 배송대행신청서를 자동등록해줍니다.",
  "version": "1.1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ]
}
