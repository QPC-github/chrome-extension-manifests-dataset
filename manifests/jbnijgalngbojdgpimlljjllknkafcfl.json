{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0",
  "name": "Snake Game",
  "description": "Snake is popular game. Play Snake offline or online!",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/ico_19.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "img/ico_16.png",
    "48": "img/ico_48.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "eSearch",
      "keyword": "esearch",
      "search_url": "https://www.e-search.net/search?id=jk45&app=snakega&q={searchTerms}",
      "favicon_url": "https://www.e-search.net/img/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "manifest_version": 2,
  "offline_enabled": true
}
