{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Check'em All",
  "version": "1.0.2",
  "description": "Recursively checks a webpage for broken links.",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "lib-jq/jquery.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
