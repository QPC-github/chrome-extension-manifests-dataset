{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fiat2LTC",
  "version": "0.2.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-latest.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "frameGBP.html",
    "frameGBPlites.html",
    "frameUSD.html",
    "frameUSDlites.html",
    "frameEUR.html",
    "frameEURlites.html",
    "frameAUD.html",
    "frameAUDlites.html",
    "frameBRL.html",
    "frameBRLlites.html",
    "frameCAD.html",
    "frameCADlites.html",
    "frameCZK.html",
    "frameCZKlites.html",
    "frameKRW.html",
    "frameKRWlites.html",
    "frameMXN.html",
    "frameMXNlites.html",
    "framePLN.html",
    "framePLNlites.html",
    "frameRON.html",
    "frameRONlites.html",
    "frameTRY.html",
    "frameTRYlites.html"
  ],
  "browser_action": {
    "default_icon": "f2lLogo1-small.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
