{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hsn Plugin",
  "description": "This plugin will analyze anypage using Hsn plugin",
  "version": "5.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "hsn.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery-2.1.3.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "jquery-2.1.3.min.js",
        "content.js"
      ]
    }
  ]
}
