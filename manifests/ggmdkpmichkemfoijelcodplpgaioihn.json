{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Agantty SortByDue",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Sorts Agantty Projects By Due Date.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Sort"
  },
  "permissions": [
    "*://*.agantty.com/*",
    "tabs"
  ]
}
