{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.cloudhq.net; object-src 'self' https://www.cloudhq.net",
  "web_accessible_resources": [
    "javascripts/*",
    "icons/*"
  ],
  "name": "Schedule Email by cloudHQ",
  "short_name": "Schedule Email by cloudHQ",
  "icons": {
    "128": "icons/icon-128x128.png",
    "48": "icons/icon-48x48.png"
  },
  "browser_action": {
    "default_icon": "icons/gmail_schedule_email.png"
  },
  "converted_from_user_script": true,
  "background": {
    "persistent": true,
    "scripts": [
      "javascripts/jquery-3.3.1.min.js",
      "javascripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "exclude_globs": [],
      "css": [
        "stylesheets/jquery.dropdown.css",
        "stylesheets/popover.css",
        "stylesheets/main_gmail_schedule_email.css"
      ],
      "include_globs": [],
      "js": [
        "javascripts/inboxsdk.js",
        "javascripts/main_gmail_schedule_email_loader.js"
      ],
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ]
    }
  ],
  "version": "1.0.0.7",
  "description": "Schedule your email to send at a later date and time",
  "permissions": [
    "storage",
    "background",
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "https://www.cloudhq.net/"
  ],
  "manifest_version": 2
}
