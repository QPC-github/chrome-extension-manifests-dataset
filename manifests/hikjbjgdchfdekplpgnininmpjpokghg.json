{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Automatic Downloads Remover",
  "description": "This extension automatically removes downloaded files that are older than 24 hours.",
  "version": "1.0.4",
  "permissions": [
    "downloads",
    "idle"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
