{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "short_name": "__MSG_short_name__",
  "description": "__MSG_ext_desc__",
  "default_locale": "en",
  "version": "2.0.0",
  "minimum_chrome_version": "48",
  "author": "Adtraction <info@adtraction.com>",
