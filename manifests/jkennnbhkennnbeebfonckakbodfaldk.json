{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Duden-Mentor-Textprüfung",
  "short_name": "Duden-Mentor",
  "description": "Fehler vermeiden, souverän formulieren!",
  "icons": {
    "16": "assets/icons/mentor16.png",
    "32": "assets/icons/mentor32.png",
    "48": "assets/icons/mentor48.png",
    "128": "assets/icons/mentor128.png"
  },
  "version": "0.8.4",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "/static/js/backgroundScript.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "/page/popup.html"
  },
  "web_accessible_resources": [
    "page/iFrame.html",
    "static/js/iframe.*.bundle.js",
    "static/css/iframe.*.css",
    "static/css/popup.*.css",
    "static/css/contentScript.*.css",
    "assets/icons/user.svg",
    "assets/icons/mentor.svg"
  ]
}
