{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save To Arazoo",
  "version": "0.6.6",
  "author": "Arazoo",
  "description": "Save product and material information directly to your Arazoo libraries and projects.",
  "icons": {
    "16": "assets/icons/arazoo_logo_16.png",
    "24": "assets/icons/arazoo_logo_24.png",
    "48": "assets/icons/arazoo_logo_48.png",
    "96": "assets/icons/arazoo_logo_96.png",
    "128": "assets/icons/arazoo_logo_128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "activeTab",
    "https://*.arazoo.com/",
    "https://arazoo.com/"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/arazoo_logo_16.png",
      "24": "assets/icons/arazoo_logo_24.png",
      "48": "assets/icons/arazoo_logo_48.png",
      "96": "assets/icons/arazoo_logo_96.png",
      "128": "assets/icons/arazoo_logo_128.png"
    },
    "default_title": "Save To Arazoo"
  },
  "background": {
    "page": "html/background.html"
  },
  "web_accessible_resources": [
    "html/iframe.html"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
