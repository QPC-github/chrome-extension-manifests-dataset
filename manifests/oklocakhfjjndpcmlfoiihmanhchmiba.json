{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ShortOut",
  "version": "0.6.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com https://ssl.google-analytics.com; object-src 'self'",
  "description": "One click URL shortening.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "bicon.png",
    "default_popup": "index.html",
    "default_title": " "
  },
  "permissions": [
    "tabs"
  ]
}
