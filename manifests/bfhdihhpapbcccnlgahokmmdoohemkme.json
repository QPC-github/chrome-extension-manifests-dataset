{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sparqling",
  "description": "An RDF store in your browser",
  "version": "0.5.1",
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "permissions": [
    "<all_urls>"
  ]
}
