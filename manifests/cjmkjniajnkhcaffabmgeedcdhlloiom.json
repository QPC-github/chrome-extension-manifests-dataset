{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Bookmarks",
  "description": "Add a minified bookmarks toolbar to the context menu.",
  "version": "1.2",
  "homepage_url": "https://github.com/7blink/Quick-Bookmarks",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "bookmarks"
  ]
}
