{
  "author": "LikeMeMusic.com",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "LikeMeMusic.com Extension"
  },
  "description": "Opens LikeMeMusic.com for adding current music link",
  "homepage_url": "https://www.likememusic.com",
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://www.likememusic.com/*"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "web_accessible_resources": [
    "testinstalled.js"
  ],
  "key": "",
  "manifest_version": 2,
  "name": "LikeMeMusic.com Extension",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6"
}
