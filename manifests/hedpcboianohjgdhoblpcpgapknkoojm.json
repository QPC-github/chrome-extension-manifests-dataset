{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Treasure Arena",
  "description": "Treasure Arena is an online battle-arena for up to 4 players.",
  "version": "0.2.3.5",
  "author": "Vennril",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "sockets": {
    "tcp": {
      "connect": [
        "*.treasurearena.com:8082"
      ]
    }
  },
  "oauth2": {
    "client_id": "675666169345-jis4agifnma1qq7fdegoeqsdrd33tcdl.apps.googleusercontent.com",
    "scopes": []
  },
  "permissions": [
    "storage",
    "fullscreen",
    "webview",
    "identity",
    "identity.email",
    "*://*.treasurearena.com/",
    "*://*.cpmstar.com/",
    "*://*.paypal.com/",
    "*://*.paypalobjects.com/"
  ]
}
