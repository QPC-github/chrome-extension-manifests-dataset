{
  "author": "hda",
  "background": {
    "scripts": [
      "core.js"
    ]
  },
  "description": "Quickly search for highlighted text on TPB and KAT sites via right-click",
  "icons": {
    "16": "tpb-icon.png",
    "48": "tpb-icon-big.png"
  },
  "manifest_version": 2,
  "name": "Right-Click Search TPB and KAT sites",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "TPBKAT Chrome",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1"
}
