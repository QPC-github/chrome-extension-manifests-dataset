{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ocultar Facebook",
  "manifest_version": 2,
  "version": "1.0.0",
  "short_name": "Ocultar Facebook",
  "icons": {
    "48": "img/facebookoculto.png",
    "128": "img/facebookoculto128.png"
  },
  "description": "Ocultar Facebook",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "jquery/jquery-2.0.3.min.js",
        "inject.js"
      ],
      "css": [
        "inject.css"
      ],
      "imagen": [
        "fondoMovistar.png"
      ]
    }
  ],
  "web_accessible_resources": []
}
