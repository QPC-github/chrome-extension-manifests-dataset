{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ええやん！",
  "version": "0.2",
  "manifest_version": 2,
  "description": "フェイブック上の全ての「いいね！」を「ええやん！」にする拡張機能です。",
  "default_icon": "eeyan.png",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.facebook.com/"
  ]
}
