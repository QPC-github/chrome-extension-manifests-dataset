{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QR Linker",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Opens QR Code Links",
  "browser_action": {
    "default_icon": "QRIcon19.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "QRIcon16.png",
    "64": "QRIcon64.png",
    "128": "QRIcon128.png"
  },
  "options_ui": {
    "page": "popup.html",
    "chrome_style": true,
    "open_in_tab": true
  }
}
