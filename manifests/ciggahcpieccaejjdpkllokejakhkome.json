{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AppsScript Color",
  "description": "This extension change the color style of the google apps script editor. It also allow to use folders with folder/subFolder/file",
  "version": "1.1.1",
  "icons": {
    "16": "AppScriptColor-16.png",
    "48": "AppScriptColor-48.png",
    "128": "AppScriptColor-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://script.google.com/*"
      ],
      "js": [
        "injectColor.js"
      ]
    }
  ]
}
