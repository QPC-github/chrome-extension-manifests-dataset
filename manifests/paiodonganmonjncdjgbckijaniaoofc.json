{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NSELMeet Screen Share",
  "description": "Screensharing, Audio and Video utility sample for NSELMeet",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {},
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://nselmeet.nucleussoftware.com/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://nselmeet.nucleussoftware.com/*"
    ]
  }
}
