{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "App for Skp Web",
  "permissions": [
    "storage"
  ],
  "short_name": "appforskype",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "An easy-to-use and lite App for Skype web",
  "homepage_url": "http://mybrowseraddon.com/skype-app.html",
  "content_scripts": [
    {
      "js": [
        "data/inject.js"
      ],
      "css": [
        "data/inject.css"
      ],
      "matches": [
        "*://*.skype.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "App for Skypeâ„¢",
    "default_icon": {
      "16": "data/icon16.png",
      "32": "data/icon32.png",
      "48": "data/icon48.png",
      "64": "data/icon64.png"
    }
  },
  "icons": {
    "16": "data/icon16.png",
    "32": "data/icon32.png",
    "48": "data/icon48.png",
    "64": "data/icon64.png",
    "128": "data/icon128.png"
  }
}
