{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ScreamBlocker",
  "description": "Korkunçlu Karı'ya Son !",
  "version": "2.0.3",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://localhost/",
    "http://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "sample.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "rounded-icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "rounded-16x16.png"
  }
}
