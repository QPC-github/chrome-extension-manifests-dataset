{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Font Bolder",
  "description": "Make the page font bolder",
  "version": "1.1",
  "permissions": [
    "webNavigation",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "bolder.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make font bolder",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
