{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FindZebra search",
  "description": "Search FindZebra from the context menu",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "clickHandler.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "fzscript.js"
      ]
    }
  ]
}
