{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.2",
  "description": "A theme solely devoted to the man with incredible martial arts skills and most powerful fist, IRON-FIST! Enjoy :-)",
  "name": "Iron-Fist Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_title": "IRON-FIST!"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Iron Fist.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
