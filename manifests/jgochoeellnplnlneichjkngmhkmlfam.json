{
  "background": {
    "page": "tg/background.html"
  },
  "content_scripts": [
    {
      "js": [
        "tg/script.js",
        "tg/common.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "tg/images/48.png",
    "default_popup": "tg/tecnogaming.html",
    "default_title": "TecnoGaming Menu"
  },
  "description": "Facil Navegacion y Busqueda en los Sitios de TecnoGaming",
  "icons": {
    "128": "tg/images/128.png",
    "16": "tg/images/16.png",
    "32": "tg/images/32.png",
    "48": "tg/images/48.png"
  },
  "manifest_version": 2,
  "name": "TecnoGaming Menu",
  "options_page": "about/options.html",
  "permissions": [
    "http://*/*",
    "http://suggestqueries.google.com/*",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.3.5"
}
