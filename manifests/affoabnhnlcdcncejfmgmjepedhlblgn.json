{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Match",
  "description": "Match at least 3 tile to bring down the wall without reaching the top.",
  "version": "1.0",
  "manifest_version": 2,
  "homepage_url": "http://www.zalmos.com/",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ],
      "container": "panel",
      "width": 600,
      "height": 600
    }
  },
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "offline_enabled": true
}
