{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iNetBits",
  "description": "Personalize the Internet",
  "version": "1.0",
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "favicon-48x48.png",
    "96": "favicon-96x96.png",
    "128": "favicon-128x128.png",
    "196": "favicon-196x196.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png",
      "48": "favicon-48x48.png",
      "96": "favicon-96x96.png",
      "128": "favicon-128x128.png",
      "196": "favicon-196x196.png"
    },
    "default_popup": "popup.html",
    "default_title": "Open iNetBits"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://www.inetbits.com/"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "iNetBits",
      "keyword": "inb",
      "favicon_url": "http://www.inetbits.com/favicons/favicon-32x32.png",
      "search_url": "http://www.inetbits.com/search.aspx?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
