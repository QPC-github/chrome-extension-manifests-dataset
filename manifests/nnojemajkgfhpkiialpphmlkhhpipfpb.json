{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Envato desktop sales notifications",
  "version": "1.0.2",
  "description": "Shows off a notification when you get a new sale on an envato marketplace.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
