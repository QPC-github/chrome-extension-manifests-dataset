{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Brandshop",
  "description": "Последние поступления товар в магазин Brandshop",
  "version": "1.1",
  "permissions": [
    "http://brandshop.ru/"
  ],
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "browser_action": {
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  }
}
