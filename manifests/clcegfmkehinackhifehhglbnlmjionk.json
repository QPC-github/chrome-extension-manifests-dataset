{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ubiq Price",
  "description": "Ubiq Price Monitor and other utilities around Ubiq",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "author": "Jose Antonio Lazaro <neokensou@gmail.com>, based on Alex Werner's code <alex@werner.fr>",
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "permissions": [
    "background",
    "notifications",
    "storage",
    "https://api.coinmarketcap.com/",
    "http://www.ecb.europa.eu/"
  ]
}
