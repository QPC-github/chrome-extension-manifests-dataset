{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebFonter",
  "default_locale": "en",
  "description": "__MSG_description__",
  "version": "0.1.18",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "cjs/bootstrap.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "js/*",
    "cjs/*",
    "css/*"
  ],
  "browser_action": {
    "default_icon": "images/chrome-icon-19x19.png",
    "default_title": "WebFonter"
  },
  "icons": {
    "128": "images/chrome-icon-128x128.png"
  },
  "homepage_url": "http://webfonter.fontshop.com"
}
