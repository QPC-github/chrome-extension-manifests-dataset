{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Informes Nosis",
  "version": "2.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Buscar informes en Nosis.",
  "icons": {
    "128": "icono128.png",
    "16": "icono16.png"
  },
  "browser_action": {
    "default_icon": "icono16.png",
    "default_popup": "barra.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+N",
        "windows": "Alt+N",
        "chromeos": "Alt+N",
        "mac": "Alt+N"
      }
    }
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/",
    "https://*/",
    "http://informes.nosis.com/*"
  ],
  "web_accessible_resources": [
    "icono13.png"
  ]
}
