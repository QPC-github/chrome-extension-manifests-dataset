{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bozoneitor",
  "description": "Troca 'Bolsonaro' por seu melhores apelidos no browser.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "permissions": [
        "activeTab"
      ]
    }
  ]
}
