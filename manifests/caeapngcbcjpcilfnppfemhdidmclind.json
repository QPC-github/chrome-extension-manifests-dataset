{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ShopOnSet WishList Extension",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Add items to your ShopOnSet character wishlists from supported shopping sites!",
  "homepage_url": "https://shop.synconset.com",
  "icons": {
    "16": "icons/Star16x16.png",
    "48": "icons/Star48x48.png",
    "128": "icons/Star128x128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/Star16x16.png",
    "default_title": "ShopOnSet Wishlists"
  },
  "permissions": [
    "cookies",
    "storage",
    "http://www.google.com/",
    "https://shop.synconset.com/",
    "https://www.shopstyle.com/",
    "http://localhost:3010/",
    "http://localhost/",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "styles/*.css",
    "src/browser_action/*",
    "src/popup/*",
    "src/js/*"
  ]
}
