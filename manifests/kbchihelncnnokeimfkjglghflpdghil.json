{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "instafollowers.co News",
  "description": "The extension for Google Chrome lets you know about latest news of instafollowers.co before anyone else.",
  "background": {
    "page": "background.html"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "*://www.instafollowers.co/*",
    "*://instafollowers.co/*",
    "*://www.instafollowers.co*",
    "*://instafollowers.co*"
  ],
  "version": "1.0"
}
