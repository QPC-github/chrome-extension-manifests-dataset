{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery.js",
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": "icone-19.png",
    "default_popup": "popup.html",
    "default_title": "Renner Push Store"
  },
  "manifest_version": 2,
  "name": "Renner Push Store",
  "description": "Renner Push Store",
  "version": "2.0.2",
  "icons": {
    "48": "icon.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://campanhas.lojasrenner.com.br/*"
  ],
  "content_security_policy": "script-src 'self' https://campanhas.lojasrenner.com.br; object-src 'self'"
}
