{
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Tasks doesn't have do be dull and boring! Put color into it! Add tasks, notes, and just about anything! Save it locally on your pc.!",
  "name": "todoo",
  "permissions": [
    "tabs",
    "bookmarks",
    "http://www.google-analytics.com/ga.js",
    "http://*.google.com/",
    "unlimitedStorage"
  ],
  "options_page": "options/options.html",
  "offline_enabled": true,
  "icons": {
    "128": "128.png",
    "16": "16.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "1.9.0.2"
}
