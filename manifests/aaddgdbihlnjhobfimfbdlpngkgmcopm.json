{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DNSBil",
  "version": "1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "favicon.ico"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery.js",
        "popup.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
