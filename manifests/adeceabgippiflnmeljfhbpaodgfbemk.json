{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KenticoCloudEnhancements",
  "version": "1.14",
  "manifest_version": 2,
  "description": "An extension for Kentico Cloud.",
  "icons": {
    "16": "on.png",
    "48": "on.png",
    "128": "on.png"
  },
  "browser_action": {
    "default_icon": "on.png",
    "default_title": "Enable/disable Kentico Cloud Enhancements"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "on.png",
    "off.png",
    "injected.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://app.kontent.ai/*",
        "https://login.kontent.ai/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "https://app.kontent.ai/*",
    "https://login.kontent.ai/*"
  ]
}
