{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wiki-Ads",
  "description": "Donate to Wikipedia by viewing advertisements when you visit it.",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*.wikipedia.org/*",
        "https://*.wikipedia.org/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
