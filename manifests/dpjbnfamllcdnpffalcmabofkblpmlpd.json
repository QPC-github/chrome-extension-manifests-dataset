{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Calendar AutoHide Toolbars",
  "short_name": "GCalAutoHide",
  "version": "0.2.5",
  "description": "More events. Less toolbar.",
  "icons": {
    "128": "GCal_AutoHide_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/calendar/*"
      ],
      "css": [
        "gcal_autohide.css"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
