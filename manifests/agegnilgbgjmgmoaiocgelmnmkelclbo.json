{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MTab",
  "description": "Just another newtab extension",
  "version": "0.0.6",
  "manifest_version": 2,
  "permissions": [
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "./js/jquery-2.1.1.min.js",
      "./js/functions.js"
    ]
  },
  "offline_enabled": true
}
