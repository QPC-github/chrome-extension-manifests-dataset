{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SurfsUp Email",
  "description": "Implements a button to send page URL via email to planesurfers.",
  "version": "1.4",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "mail_128x128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://www.google.com/flights"
  ],
  "browser_action": {
    "default_title": "SurfsUp Email",
    "default_icon": "email_16x16.png"
  },
  "manifest_version": 2
}
