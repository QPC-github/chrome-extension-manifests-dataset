{
  "background": {
    "scripts": [
      "request.js"
    ]
  },
  "icons": {
    "48": "icon.png"
  },
  "description": "Block and redirect from biased propaganda israeli websites.",
  "manifest_version": 2,
  "name": "YBlock",
  "web_accessible_resources": [
    "page.js",
    "swfobject2.js",
    "pacman.swf",
    "images/*.png",
    "images/*.gif",
    "images/*.jpg",
    "redirect.htm"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
