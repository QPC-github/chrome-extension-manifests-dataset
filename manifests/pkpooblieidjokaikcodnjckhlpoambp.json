{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "2016 Political Candidates",
  "version": "0.1",
  "icons": {
    "128": "FlagIcon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
