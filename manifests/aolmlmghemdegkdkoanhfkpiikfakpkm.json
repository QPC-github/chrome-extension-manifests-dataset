{
  "name": "coco@tabs",
  "version": "1.2",
  "manifest_version": 2,
  "description": "coco@tabs add tabs in your Gmail interface for quick and easy navigation. You can even add custom labels!",
  "options_page": "option.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "email.png"
  },
  "icons": {
    "128": "email.png",
    "48": "email.png",
    "19": "email.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "http://mail.google.com/*",
        "https://mail.google.com/*"
      ],
      "js": [
        "js/jquery.js",
        "js/GmailTabs.js"
      ],
      "css": [
        "css/mailChrome.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "GmailImages/inbox.png",
    "GmailImages/sent.png",
    "GmailImages/all.png",
    "GmailImages/drafts.png",
    "GmailImages/spam.png",
    "GmailImages/starred.png",
    "GmailImages/trash.png",
    "js/jquery.js",
    "js/GmailTabs.js",
    "css/mailChrome.css"
  ],
  "permissions": [
    "tabs",
    "*://mail.google.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
