{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LIU Grading",
  "version": "1.9",
  "manifest_version": 2,
  "description": "Automatically upload grades to UMS",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon1.png",
    "default_popup": "popup.html"
  }
}
