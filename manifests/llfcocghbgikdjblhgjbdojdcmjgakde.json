{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Incognito Messenger Theme (Dark)",
  "version": "1.0.1",
  "description": "Make Messenger nicer on your eyes.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.messenger.com/*",
        "https://*.messenger.com/*"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
