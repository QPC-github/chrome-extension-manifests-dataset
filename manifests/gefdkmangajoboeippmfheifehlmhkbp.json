{
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/suzzy_off.png",
    "default_popup": "popup.html",
    "default_title": "Suzzysaur Streams"
  },
  "icons": {
    "128": "img/suzzy_icon.png"
  },
  "description": "Be notified when Suzzysaur goes online!",
  "manifest_version": 2,
  "name": "Suzzysaur Streams",
  "permissions": [
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "https://api.twitch.tv/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
