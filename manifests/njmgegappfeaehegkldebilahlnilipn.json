{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "highlyreco",
  "description": "find book recomendations by domain experts",
  "version": "1.3",
  "icons": {
    "128": "assets/icon_big.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "*://*.highlyreco.com/*",
    "https://platform.twitter.com/*",
    "cookies",
    "storage"
  ]
}
