{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "PixelPal helps validate a website's NextRoll pixel implementation by providing ordered pixel request and response details.",
  "version": "1.0.38",
  "name": "PixelPal (by NextRoll)",
  "short_name": "PixelPal",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webNavigation",
    "debugger",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "popup.html",
    "embed.js",
    "embed.css",
    "interact.min.js",
    "icon-34.png"
  ]
}
