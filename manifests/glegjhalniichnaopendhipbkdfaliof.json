{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Bookmarks",
  "version": "1.0.2",
  "description": "Save your favorite URLs to the 1ww.com cloud service",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "bookmark.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "*://*.1ww.com/",
    "*://*.kodama.com/"
  ],
  "icons": {
    "128": "bookmark.png"
  }
}
