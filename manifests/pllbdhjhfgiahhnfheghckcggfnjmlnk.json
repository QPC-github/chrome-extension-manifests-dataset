{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kpi2day+",
  "description": "Розширення Kpi2day+ дозволить тобі та твоїм друзям зручно отримувати потрібну інформацію твого універу!",
  "version": "0.0.7",
  "browser_action": {
    "default_icon": "static/img/logo.png",
    "default_popup": "popup.html",
    "default_title": "kpi2day.com"
  },
  "icons": {
    "64": "static/img/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "static/js/jquery.min.js"
      ]
    }
  ]
}
