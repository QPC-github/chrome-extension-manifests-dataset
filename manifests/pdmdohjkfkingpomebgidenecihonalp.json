{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CssAn Free",
  "description": "Visual Animations with CSS of any element at your web",
  "version": "0.4",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "icono48.png",
    "128": "icono128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery-ui.js",
      "background.js",
      "animador.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icono48.png",
    "default_title": "Animate a web"
  },
  "manifest_version": 2
}
