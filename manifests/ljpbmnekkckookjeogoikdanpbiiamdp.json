{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Verspiration",
  "“description”": "Receive a new bible verse a day and a welcome when opening a new tab!",
  "version": "1.0.4.1",
  "author": "Samuel Lee",
  "browser_action": {
    "default_icon": "bible.png",
    "default_title": "Verspiration"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
