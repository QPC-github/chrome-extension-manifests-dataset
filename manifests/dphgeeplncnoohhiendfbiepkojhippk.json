{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/32x32.png",
    "icons": {
      "128": "images/128x128.png",
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "contentStyle.css"
      ],
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "description cua tui",
  "homepage_url": "https://www.matbao.net",
  "icons": {
    "128": "images/128x128.png",
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png"
  },
  "manifest_version": 2,
  "name": "MinhBao",
  "version": "1.0",
  "web_accessible_resources": [
    "frame.html"
  ]
}
