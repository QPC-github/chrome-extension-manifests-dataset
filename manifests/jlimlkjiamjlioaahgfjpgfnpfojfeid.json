{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VK gif autoplay",
  "version": "1.0.7",
  "description": "Automatically starts playing back all gifs on the IM page of VK.com",
  "content_scripts": [
    {
      "matches": [
        "http://vk.com/*",
        "https://vk.com/*"
      ],
      "js": [
        "gifautoplay.js"
      ]
    }
  ]
}
