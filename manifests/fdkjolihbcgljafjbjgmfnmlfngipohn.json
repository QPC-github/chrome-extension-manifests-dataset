{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DoppelBangers Search",
  "description": "Right click reverse image search for DoppelBangers",
  "version": "0.0.2",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
