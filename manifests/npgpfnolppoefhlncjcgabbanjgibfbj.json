{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab-stash",
  "version": "1.0.3",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "description": "Expansion to quickly save a set of browser tabs.",
  "icons": {
    "48": "pic/icon.png"
  },
  "browser_action": {
    "default_icon": "pic/icon.png",
    "default_title": "stash",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "manifest_version": 2
}
