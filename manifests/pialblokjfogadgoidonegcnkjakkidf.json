{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rutracker.org магнет-ссылка",
  "description": "Приложение создаёт магнет-ссылку из хэш-суммы на сайте rutracker.org.",
  "version": "0.0.0.2",
  "content_scripts": [
    {
      "matches": [
        "http://rutracker.org/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
