{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.2",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "name": "Browser Internals",
  "short_name": "under-the-hood",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "http://mybrowseraddon.com/under-the-hood.html",
  "description": "Access all about:about pages from a stylish toolbar popup!",
  "browser_action": {
    "default_title": "Browser Internals",
    "default_popup": "data/popup/popup.html",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
