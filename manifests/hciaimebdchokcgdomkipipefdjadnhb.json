{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ashish's Hello World App 2",
  "description": "My first extension - Say Hi to me always",
  "version": "2",
  "author": "Ashish Poddar",
  "browser_action": {
    "default_icon": "tab-icon.png",
