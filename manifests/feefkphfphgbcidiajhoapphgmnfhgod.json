{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "animachine (beta)",
  "short_name": "animachine",
  "version": "0.3.1",
  "manifest_version": 2,
  "description": "GUI for GSAP",
  "homepage_url": "https://github.com/animachine/animachine",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "564": "icons/icon564.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "open animachine"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
