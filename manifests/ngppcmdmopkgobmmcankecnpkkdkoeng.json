{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "FO"
  },
  "content_security_policy": "script-src 'self' http://127.0.0.1; object-src 'self';",
  "description": "自动选择合适代理上国内和国外网站。基于佛代理客户端开发。",
  "icons": {
    "128": "img/logo.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "20.0.0",
  "homepage_url": "http://www.xiaozhekou.com/",
  "name": "佛代理",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.3.1"
}
