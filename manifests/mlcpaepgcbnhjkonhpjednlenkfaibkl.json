{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "6M桌面分享",
  "description": "6M桌面分享插件.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "6m-logo-16x16.png",
    "48": "6m-logo-48x48.png",
    "128": "6m-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://m.v6plus.com/*"
    ]
  }
}
