{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shortcut2Url",
  "description": "Shortcut2Url gives you 4 keyboard shortcuts for your favorite sites",
  "version": "1.1.2",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "commands": {
    "1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Ctrl+Shift+1",
      "global": true
    },
    "2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "Ctrl+Shift+2",
      "global": true
    },
    "3": {
      "suggested_key": {
        "default": "Ctrl+Shift+3",
        "mac": "Command+Shift+3"
      },
      "description": "Ctrl+Shift+3",
      "global": true
    },
    "4": {
      "suggested_key": {
        "default": "Ctrl+Shift+4",
        "mac": "Command+Shift+4"
      },
      "description": "Ctrl+Shift+4",
      "global": true
    }
  }
}
