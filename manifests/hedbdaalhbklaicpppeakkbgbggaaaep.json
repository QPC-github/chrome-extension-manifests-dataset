{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "storage"
  ],
  "manifest_version": 2,
  "name": "OpenExternalUrlInNewTab",
  "options_page": "options.html",
  "version": "0.0.2.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-3.1.0.min.js",
        "OpenExternalUrlInNewTab.js",
        "options.js"
      ]
    }
  ]
}
