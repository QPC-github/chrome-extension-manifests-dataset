{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Udacity™ Video FastForward",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "description": "Keybindings for altering the speed of Udacity™ YouTube™ video lectures",
  "content_scripts": [
    {
      "matches": [
        "http://*.udacity.com/*"
      ],
      "js": [
        "ff.js"
      ]
    }
  ]
}
