{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Drakemoon Helper",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Don't get tricked.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "matches": [
        "https://www.drakemoon.com/*"
      ]
    }
  ]
}
