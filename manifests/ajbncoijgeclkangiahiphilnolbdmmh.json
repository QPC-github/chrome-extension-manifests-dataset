      "css": [
        "css/bootstrap.min.css",
        "css/quill.css",
        "css/jquery.toast.min.css",
        "css/spin.css",
        "css/progress.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "/js/injectScript.js",
    "/js/bulk-import.js",
    "json/config.json"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
