{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "0.7",
  "default_locale": "en",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/wooshark1_128x128.png",
    "default_popup": "popup.html",
    "default_title": ""
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "images/wooshark1_128x128.png",
    "128": "images/wooshark1_128x128.png"
  },
  "background": {
    "scripts": [
      "js/background.js",
      "js/openOption.js",
      "js/loading.js",
      "js/editImage.js",
      "js/jquery-3.2.1.min.js"
    ],
    "persistence": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.aliexpress.com/*",
        "https://*.lunapic.com/*",
        "https://htmledit.squarefree.com/*",
        "https://aeproductsourcesite.alicdn.com/*",
        "https://www.amazon.com/*",
        "https://www.amazon.in/*",
        "https://www.amazon.co.jp/*",
        "https://www.amazon.fr/*",
        "https://www.amazon.de/*",
        "https://www.amazon.it/*",
        "https://www.amazon.nl/*",
        "https://www.amazon.es/*",
        "https://www.amazon.ae/*",
        "https://www.amazon.com.tr/*",
        "https://www.amazon.co.uk/*",
        "https://www.amazon.ca/*",
        "https://www.amazon.ca/*",
        "https://www.amazon.com/*",
        "https://www.amazon.com.au/*",
        "https://www.amazon.com.br/*",
        "https://www.ebay.de/*",
        "https://www.ebay.com/*",
        "https://www.ebay.co.uk/*",
        "https://www.ebay.com.de/*",
        "https://www.ebay.com.au/*",
        "https://www.ebay.ca/*",
        "https://www.ebay.fr/*",
        "https://www.ebay.it/*",
        "https://www.ebay.es/*",
        "https://www.ebay.at/*",
        "https://www.ebay.ch/*",
        "https://www.ebay.com.hk/*",
        "https://www.ebay.com.sg/*",
        "https://www.ebay.com.my/*",
        "https://www.ebay.in/*",
        "https://www.ebay.ph/*",
        "https://www.ebay.ie/*",
        "https://www.ebay.pl/*",
        "https://www.ebay.be/*",
        "https://www.benl.ebay.be/*",
        "https://www.ebay.nl/*",
        "https://www.ebay.cn/*",
        "https://*.etsy.fr/*",
        "https://*.etsy.com/*",
        "https://www.amazon.com.mx/*",
        "https://shoppingcart.aliexpress/*"
      ],
      "all_frames": false,
      "js": [
        "js/editor.js",
        "js/jquery-3.2.1.min.js",
        "js/lodash.js",
        "js/ebay.js",
        "js/etsy.js",
        "js/amazon.js",
        "js/bootstrap.min.js",
        "js/ordersAndProducts.js",
        "js/lunaPicEdit.js",
        "js/editDescription.js",
        "js/selectReview.js",
        "js/selectReviewAmazon.js",
        "js/math.js",
        "js/purchaseProduct.js",
        "js/quill.js",
        "js/sync.js",
        "js/vendor/fontawesome.js",
        "js/vendor/jquery.toast.min.js",
        "js/full-import.js",
        "js/vendor/spin.js",
        "js/vendor/progress.js",
        "js/aliexpress-v2.js",
        "js/aliexpress_order_v2.js",
        "js/aliexpress_order_v3.js",
        "js/bulk-import_v2.js",
        "js/purchase_v2.js",
        "js/purchase_v3.js",
        "js/bulk-import-ebay.js",
        "js/import-from-seller.js"
