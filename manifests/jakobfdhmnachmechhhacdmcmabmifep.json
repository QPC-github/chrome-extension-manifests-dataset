{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Find CF",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Test Extension",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "lib/C.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
