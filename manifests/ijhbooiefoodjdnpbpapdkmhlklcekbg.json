{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "autobleep",
  "version": "0.0.2.0",
  "manifest_version": 2,
  "description": "Automatically restarts playback for bleep.com audio previews.",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.bleep.com/*",
        "*://bleep.com/*"
      ],
      "js": [
        "autobleep.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
