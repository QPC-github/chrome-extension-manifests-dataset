{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Online Copyright Registration",
  "manifest_version": 2,
  "short_name": "OnlineCopyrightRegistration",
  "description": "Copyright registration in a fairly simple and painless process with an easy to use platform that protects your hard work",
  "version": "5.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_136-1503169289.png"
  },
  "icons": {
    "128": "128px_136-1503169284.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
