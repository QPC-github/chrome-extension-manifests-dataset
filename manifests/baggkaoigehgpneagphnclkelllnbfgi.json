{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "US Address Validation",
  "short_name": "US Address Validation",
  "description": "This extension helps you verifying and validating US addresses",
  "version": "1.1",
  "author": "Victor",
  "homepage_url": "https://www.validstreets.com/",
  "browser_action": {
    "default_icon": "images/logo19.png",
    "default_popup": "popup.html",
    "default_title": "US Address Validation"
  },
  "icons": {
    "128": "images/logo128.png"
  },
  "permissions": [
    "activeTab",
    "http://localhost:5000/",
    "https://api.validstreets.com/",
    "storage"
  ],
  "background": {
    "scripts": [
      "event-page.js"
    ],
    "persistent": false
  }
}
