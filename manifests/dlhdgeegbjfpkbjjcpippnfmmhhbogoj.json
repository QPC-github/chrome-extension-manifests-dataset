{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MrHighTech",
  "description": "Extension MrHighTech",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webNavigation"
  ],
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "24": "icon24.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  }
}
