{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Price Bookmark"
  },
  "default_locale": "en",
  "description": "Description for the Chrome extension",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "Price Bookmark",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage",
    "activeTab"
  ],
  "version": "2.0"
}
