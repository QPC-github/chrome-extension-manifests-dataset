{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IP",
  "manifest_version": 2,
  "version": "0.2.6",
  "minimum_chrome_version": "32",
  "description": "Get's your public/private IP address",
  "permissions": [
    "notifications",
    "storage"
  ],
  "icons": {
    "64": "ip.png",
    "128": "ip_128.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "ip.png",
      "128": "ip_128.png"
    }
  }
}
