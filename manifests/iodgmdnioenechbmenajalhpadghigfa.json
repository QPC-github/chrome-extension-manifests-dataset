{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Turbo Outliner",
  "version": "0.0.5",
  "description": "Most accurate web article table of contents generator!",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/icon-32.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+O",
        "mac": "MacCtrl+Shift+O"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "app.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ]
}
