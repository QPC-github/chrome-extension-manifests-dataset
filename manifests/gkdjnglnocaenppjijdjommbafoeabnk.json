{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hey Bob - Your Bike Taxi",
  "short_name": "Your Bike Taxi",
  "description": "Book a Bob for a Quick Ride!",
  "version": "1.0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "homepage_url": "http://heybobrides.com",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
