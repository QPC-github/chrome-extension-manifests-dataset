{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "URL Alias Rewrite",
  "version": "0.8.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "description": "Rewrites user-definied aliases to URLs (e.g. m/ -> maps.google.com)",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "http://matthiaslinder.com/rewrite+alias",
  "omnibox": {
    "keyword": "go"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
