{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Codeforces participants remover",
  "version": "1.0",
  "description": "Removes all participants from the Codeforces mashup.",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://codeforces.com/*",
    "http://codeforces.com/*"
  ]
}
