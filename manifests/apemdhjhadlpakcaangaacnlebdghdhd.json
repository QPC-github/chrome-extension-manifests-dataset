{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clipboard Notes",
  "version": "1.2",
  "description": "Creating notes from the selected portion of the text or a selected image to revise it later.",
  "icons": {
    "16": "clipboardx16.jpg",
    "48": "clipboardx48.jpg",
    "128": "clipboardx128.jpg"
  },
  "browser_action": {
    "default_icon": "clipboard-01.jpg",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ],
  "options_page": "popup.html"
}
