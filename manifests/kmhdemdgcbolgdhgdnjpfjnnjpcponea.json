{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Toap It!",
  "description": "Select a picture, add some text, and share it with your friends.",
  "version": "0.0.0.2",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "main.js"
    ]
  },
  "icons": {
    "16": "icon-bitty.png",
    "48": "icon-small.png",
    "128": "icon-large.png"
  }
}
