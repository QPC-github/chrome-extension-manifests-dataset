{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Price Sea - твоя корзина для всех покупок",
  "short_name": "Price Sea",
  "description": "Это расширение даёт возможность быстро добавить любой товар в вашу корзину на PriceSea.ru",
  "version": "1.4.1",
  "icons": {
    "32": "src/images/logo_32.png",
    "128": "src/images/logo_128.png",
    "196": "src/images/logo_196.png",
    "256": "src/images/logo_256.png"
  },
  "browser_action": {
    "default_icon": "src/images/logo_32.png",
    "default_popup": "src/popup.html",
    "default_title": "Price Sea"
  },
  "permissions": [
    "activeTab"
  ]
}
