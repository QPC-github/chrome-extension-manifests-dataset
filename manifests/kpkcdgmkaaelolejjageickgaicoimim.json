{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Accuen Universal Tag Simulator",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "Use this tool to simulate in-situ screengrabs using only the JavaScript tag and dimensions.",
  "homepage_url": "http://www.accuenmedia.com",
  "short_name": "AUTS",
  "browser_action": {
    "name": "Click to start AUTS",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "32": "icon32.png",
    "16": "icon32.png",
    "64": "icon32.png",
    "128": "icon128.png"
  }
}
