{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true
    }
  ],
  "description": "Alcatel Lucent Enterprise Screen sharing",
  "icons": {
    "128": "logo128.png",
    "48": "logo48.png",
    "16": "logo16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Alcatel Lucent Enterprise Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "version": "1.0.0"
}
