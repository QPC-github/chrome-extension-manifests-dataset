{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "19": "img/icon.png",
      "38": "img/icon.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Boost Your Marketing campaigns",
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "manifest_version": 2,
  "name": "SINFILTRATOR",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "contextMenus",
    "storage",
    "http://sinfiltrator.com/"
  ],
  "version": "1.0.0.9",
  "web_accessible_resources": [
    "img/icon128.png"
  ]
}
