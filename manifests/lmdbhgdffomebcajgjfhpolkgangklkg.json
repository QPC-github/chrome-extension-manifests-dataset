{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabTidy",
  "description": "This extension provides a button to delete half your tabs at random.",
  "homepage_url": "http://incoherency.co.uk/tabtidy/",
  "version": "1.2",
  "icons": {
    "16": "tabtidy16.png",
    "48": "tabtidy48.png",
    "128": "tabtidy128.png"
  },
  "browser_action": {
    "default_icon": "tabtidy.png"
  },
  "background": {
    "scripts": [
      "tabtidy.js"
    ]
  }
}
