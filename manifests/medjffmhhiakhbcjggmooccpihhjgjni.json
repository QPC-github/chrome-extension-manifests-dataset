{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Time Machine",
  "description": "Obliterates the last fifteen minutes of your browsing history",
  "version": "0.3.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "The cake is a lie"
  },
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "permissions": [
    "browsingData",
    "activeTab"
  ]
}
