{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SpiegelOnline-UnPlus",
  "description": "SpiegelOnline Plus lesen, ohne zu bezahlen!",
  "version": "9.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.spiegel.de/*"
      ],
      "js": [
        "/removal.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
