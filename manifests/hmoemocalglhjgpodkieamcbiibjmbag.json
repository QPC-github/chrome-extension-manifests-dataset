{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "count the number of characters(文章の文字数を調べる)",
  "manifest_version": 2,
  "description": "文章をドラッグ選択すると文字数を表示します。",
  "icons": {
    "128": "count-the-number-of-characters128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "search.js"
      ]
    }
  ],
  "version": "1.0"
}
