{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabSearch",
  "description": "A simple, clean extension to search through your open Chrome tabs.",
  "version": "1.4",
  "permissions": [
    "tabs",
    "http://tabsearch.react.technology/"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon@2x.png",
      "48": "logo_n48.png",
      "128": "logo_n128.png"
    },
    "default_popup": "search.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  },
  "content_security_policy": "script-src 'self' https://connect.facebook.net; object-src 'self'"
}
