{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Chrome Verge",
  "description": "Markup any webpage with a pen and higlighter utensil, and be able to erase these marks with the eraser.",
  "version": "1.0",
  "icons": {
    "48": "Images/Logos/icon48 version 2.png",
    "128": "Images/Logos/icon128 version 2.png"
  },
  "browser_action": {
    "default_icon": "Images/Logos/icon16 version 2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ]
}
