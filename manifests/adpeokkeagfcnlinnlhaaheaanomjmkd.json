{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yes im over 18!",
  "description": "Automatically selects Yes to the question \"Are over 18 years old?\". Saves your time.",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "short_name": "Yes im over 18!",
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*",
        "https://www.reddit.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
