{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "list me! for Salesforce",
  "short_name": "listme! for SFDC",
  "version": "2.5",
  "description": "Go from a Salesforce record to it's corresponding list view at the push of a button. Plus lots more great features!",
  "manifest_version": 2,
  "author": "snow monkey lab",
  "background": {
    "persistent": false,
    "scripts": [
      "listme_decision.js"
    ]
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    },
    "default_title": "list me!"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "https://*.salesforce.com/*",
    "https://*.force.com/*",
    "https://*.lightning.force.com/*",
    "notifications"
  ],
  "web_accessible_resources": [
    "images/listme.png",
    "images/thanks.png",
    "images/32.png",
    "setuppage/setup.js",
    "setuppage/setup_user_classic.js",
    "setuppage/setup_user_lightning.js",
    "listmescripts/listme_classic.js",
    "listmescripts/listme_lightning.js"
  ]
}
