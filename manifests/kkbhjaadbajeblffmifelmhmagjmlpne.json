{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IE8 User-Agent",
  "version": "0.1.6",
  "description": "Alternate User-Agent to IE8",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "icon128.png"
    },
    "default_title": "IE8 User-Agent"
  },
  "author": "Nikolay Kim",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/"
  ]
}
