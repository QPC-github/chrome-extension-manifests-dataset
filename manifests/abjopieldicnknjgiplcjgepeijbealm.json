{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Micro Focus",
  "background": {
    "scripts": [
      "lib/loglevel.min.js",
      "sl_NativeAccess.min.js",
      "basicSSOAccess.min.js",
      "chromeconstants.js",
      "Sandbox.min.js",
      "Background.min.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/application_16.png",
      "32": "images/application_32.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "wizard.css"
      ],
      "js": [
        "lib/jquery.min.js",
        "lib/loglevel.min.js",
        "lib/css-selector-generator.min.js",
        "constants.js",
        "lib/xml-writer.js",
        "chromeconstants.js",
        "Content.min.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://addons.mozilla.org/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_shortDescription__",
  "homepage_url": "https://www.netiq.com/solutions/identity-access-management/single-sign-on.html",
  "icons": {
    "128": "images/application_128.png",
    "16": "images/application_16.png",
    "256": "images/application_256.png",
    "32": "images/application_32.png",
    "48": "images/application_48.png",
    "64": "images/application_64.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "55.0",
  "name": "__MSG_extensionName__",
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "3.3.5",
  "version_name": "3.3.5",
  "web_accessible_resources": [
    "Keyboard.min.js",
    "manifest.json",
    "images/application_128.png",
    "images/generic.png",
    "ux/*"
  ]
}
