{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Music Albums",
  "version": "0.0.3",
  "description": "Shows album bookmarks from some music sites",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
