{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "faraday25x25.png",
    "default_title": "Faraday Chrome Extension",
    "icons": [
      "faraday25x25.png"
    ],
    "name": "Faraday Chrome Extension"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "WebAgenda.js"
      ],
      "matches": [
        "http://*.faraday.com.tw/WebAgenda/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Faraday Chrome Extension",
  "icons": {
    "128": "faraday25x25.png"
  },
  "name": "Faraday Chrome Extension",
  "permissions": [
    "tabs"
  ],
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
