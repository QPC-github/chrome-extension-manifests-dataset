{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Quick Auto Refresh"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://s3.amazonaws.com; object-src 'self'",
  "description": "Quick Auto Refresh is the best extension to auto reload a website every a given time",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Quick Auto Refresh",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>",
    "*://*.google.com/*",
    "*://*.youtube.com/*",
    "*://*.facebook.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.1"
}
