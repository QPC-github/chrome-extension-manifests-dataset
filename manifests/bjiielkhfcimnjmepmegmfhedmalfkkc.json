{
  "content_scripts": [
    {
      "css": [
        "css/gmmHelpMenu.css"
      ],
      "js": [
        "jquery-latest.min.js",
        "gmm.ac.user.js",
        "gmm.fl.user.js",
        "gmm.nbd.user.js",
        "gmm.rz.user.js",
        "gmm.sl.user.js",
        "gmmMapSatHotkey.js",
        "gmmHelpMenu.js",
        "gmmEnterHotkey.js",
        "gmmDeletePointHotkey.js"
      ],
      "matches": [
        "*://www.google.com/mapmaker*",
        "*://www.google.com.ua/mapmaker*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://testapi.eu5.org; object-src 'self'",
  "converted_from_user_script": true,
  "description": "Спрощує життя пересічного картографа",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png"
  },
  "manifest_version": 2,
  "name": "GMM Scripts",
  "permissions": [
    "*://www.google.com/mapmaker*",
    "*://www.google.com.ua/mapmaker*",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.4",
  "web_accessible_resources": [
    "*.js",
    "css/*.css",
    "icons/*.*",
    "img/*.*"
  ]
}
