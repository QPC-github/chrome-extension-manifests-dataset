{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "",
  "icons": {
    "128": "icon/128.png",
    "16": "icon/16.png",
    "24": "icon/24.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "64": "icon/64.png"
  },
  "manifest_version": 2,
  "name": "Scrollton",
  "permissions": [
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.2"
}
