{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Confirmation Bias Search Inverter",
  "short_name": "Cure confirmation bias with this search inverter",
  "description": "Cure confirmation bias with this search inverter that challenges you to consider disconfirming evidence.",
  "version": "0.1.4",
  "permissions": [
    "tabs"
  ],
  "omnibox": {
    "keyword": "CB"
  },
  "background": {
    "scripts": [
      "scripts/compromise.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_popup": "pages/popup/popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
