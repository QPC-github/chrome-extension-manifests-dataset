{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "image backupper",
  "version": "1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "persistent": false
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Image Backupper"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon/16.png",
    "18": "icon/18.png",
    "24": "icon/24.png",
    "32": "icon/32.png",
    "36": "icon/36.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}
