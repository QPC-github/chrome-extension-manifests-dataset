{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UKIP Goggles",
  "description": "Turn the Word Wide Web into little England.",
  "icons": {
    "128": "icon_128.png"
  },
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "ukip-goggles.css"
      ],
      "js": [
        "jquery-2-1-1.js",
        "jquery.replacetext.js",
        "burn.js",
        "ukip-goggles.js"
      ],
      "all_frames": true
    }
  ]
}
