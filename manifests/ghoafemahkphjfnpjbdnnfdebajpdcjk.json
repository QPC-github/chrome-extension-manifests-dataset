{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TruthFinder Background Check",
  "description": "Run a Background Check on Anyone",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "TruthFinder Background Check"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
