{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "This extension blocks those annoying sponsor threads on Fark.",
  "version": "0.2",
  "name": "BlockFarkPlugs",
  "content_scripts": [
    {
      "matches": [
        "http://*.fark.com/*"
      ],
      "css": [
        "mystyles.css"
      ]
    }
  ]
}
