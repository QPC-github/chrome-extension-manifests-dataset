{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/Oberlo.css",
        "css/iziModal.css"
      ],
      "js": [
        "js/oberlo.js"
      ],
      "matches": [
        "https://app.oberlo.com/products*",
        "https://app.oberlo.com/my-products*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/Oberlo.css",
        "css/iziModal.css"
      ],
      "js": [
        "js/oberlo_import.js"
      ],
      "matches": [
        "https://app.oberlo.com/import*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/importify.css",
        "css/iziModal.css"
      ],
      "js": [
        "js/importify.js"
      ],
      "matches": [
        "https://app.importify.net/dashboard/products/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/dropified.css",
        "css/iziModal.css"
      ],
      "exclude_matches": [
        "https://app.dropified.com/product?store=n*",
        "https://app.dropified.com/product?page=*&store=n"
      ],
      "js": [
        "js/dropified.js"
      ],
      "matches": [
        "https://app.dropified.com/product?store=*",
        "https://app.dropified.com/product?page=*&store=*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/modalyst.css",
        "css/iziModal.css"
      ],
      "js": [
        "js/modalyst.js"
      ],
      "matches": [
        "https://modalyst.co/item_manager/sync_list*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/expressfy.css",
        "css/iziModal.css"
      ],
      "js": [
        "js/expressfy.js"
      ],
      "matches": [
        "https://app.expressfy.com/dashboard/products/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/shopmaster.css",
        "css/iziModal.css"
      ],
      "js": [
        "js/shopmaster.js"
      ],
      "matches": [
        "https://www.shopmaster.com/shopify/product/index.html?curSelect=*&state=*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "js/aliexpress.js",
        "js_helpers/bootstrap-notify.min.js",
        "js_helpers/notifications.js"
      ],
      "matches": [
        "*://areviewsapp.com/insert-reviews",
        "*://areviewsapp.com/comments/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This app enables you to Import reviews directly from Oberlo,Importify and dropified and other apps",
  "differential_fingerprint": "1.f741f56cd95c66d0d7d12e0ba95411e51301f8c5b41928f0272ea9ee5dddcf3f",
  "externally_connectable": {
    "matches": [
      "https://*.areviewsapp.com/*"
    ]
  },
  "icons": {
    "128": "images/icons/areviews_icon128.png",
    "16": "images/icons/areviews_icon16.png",
    "19": "images/icons/areviews_icon19.png",
    "38": "images/icons/areviews_icon38.png",
    "48": "images/icons/areviews_icon48.png"
  },
  "manifest_version": 2,
  "name": "Areviews - get reviews from aliexpress,amazon",
  "permissions": [
    "*://*.areviewsapp.com/*",
    "*://areviewsapp.com/*",
    "background",
    "https://ar.aliexpress.com/*"
  ],
  "version": "4.5.3",
  "web_accessible_resources": [
    "*.gif"
  ]
}
