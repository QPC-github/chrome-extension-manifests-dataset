{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Speaking-Clock",
  "version": "1.3",
  "description": "Indian Patriot Alarm Clock",
  "permissions": [
    "background",
    "tts"
  ],
  "background": {
    "scripts": [
      "com.js",
      "back.js"
    ]
  },
  "browser_action": {
    "default_icon": "indo.png",
    "default_title": "Speaking-Clock",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "clock-16.png"
  },
  "manifest_version": 2
}
