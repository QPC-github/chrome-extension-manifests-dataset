{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Website History",
  "description": "Show history of current website in easy way",
  "version": "1.0",
  "manifest_version": 2,
  "offline_enabled": true,
  "permissions": [
    "history",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "main.html"
  },
  "icons": {
    "16": "icons/history_16_16.png",
    "32": "icons/history_32_32.png",
    "48": "icons/history_48_48.png",
    "64": "icons/history_64_64.png",
    "128": "icons/history_128_128.png",
    "256": "icons/history_256_256.png",
    "512": "icons/history_512_512.png"
  }
}
