{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alaska Airlines Mileage Plan™ Shopping button",
  "version": "1.1.35.1",
  "description": "Activate Alaska Airlines Mileage Plan™ miles directly from 850+ store sites with one click and never miss out on miles again.",
  "homepage_url": "https://www.mileageplanshopping.com",
  "author": "Cartera",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "vendors/bundle.js",
        "content/bundle.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "vendors/bundle.js",
      "bg/bundle.js"
    ],
    "persistent": true
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon-48.png",
      "32": "img/icon-48.png"
    },
    "default_title": "Mileage Plan™ Shopping button"
  },
  "icons": {
    "16": "img/icon-48.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "*.png",
    "*.svg",
    "*.css",
    "*.woff",
    "*.woff2",
    "*.ttf",
    "*.otf"
  ]
}
