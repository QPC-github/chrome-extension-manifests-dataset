{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Download Manager",
  "description": "Show recent downloads in the extension pane.",
  "version": "1.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/download19.png",
    "default_popup": "popup/popup.html",
    "default_title": "View active and completed downloads."
  },
  "icons": {
    "16": "images/download16.png",
    "32": "images/download32.png",
    "48": "images/download48.png",
    "64": "images/download64.png",
    "128": "images/download128.png"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "downloads",
    "downloads.shelf",
    "downloads.open",
    "storage",
    "tabs"
  ],
  "optional_permissions": [
    "notifications"
  ]
}
