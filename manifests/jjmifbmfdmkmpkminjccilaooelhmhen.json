{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Responsive Design Tester",
  "description": "Used to view your project on mobile resolutions.",
  "version": "1.7",
  "manifest_version": 2,
  "permissions": [
    "file:///*",
    "tabs",
    "activeTab",
    "background",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}
