{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "vocabulary.js"
    ]
  },
  "description": "Looks up a highlighted word in Vocabulary.com and shows its description in a pop up box",
  "icons": {
    "16": "vocab-medium.png"
  },
  "manifest_version": 2,
  "name": "Vocabulary",
  "page_action": {
    "default_icon": {
      "16": "vocab-medium.png"
    }
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "version": "0.1"
}
