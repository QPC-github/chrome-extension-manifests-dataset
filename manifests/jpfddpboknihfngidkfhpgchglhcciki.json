{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "da_extension.css"
      ],
      "js": [
        "jquery-1.11.3.min.js",
        "DigiavantajExtension.js"
      ]
    }
  ],
  "description": "Online alışveriş sitelerini ziyaret ettiğinizde “Digiavantaj” sayesinde kazanabileceğiniz Digiturk fatura indirimini hatırlatır.",
  "name": "Digiavantaj Hatırlatıcı",
  "version": "2.22",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
