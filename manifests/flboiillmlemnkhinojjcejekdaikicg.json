{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "好好市集 HohoMarket",
  "description": "這裡齊集香港最優質的獨立品牌，給你滿滿的生活靈感！",
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_title": "好好市集 HohoMarket",
    "default_icon": {
      "19": "images/logo.png",
      "38": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "background": {
    "scripts": [
      "javascript/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "permissions": [],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://www.hohomarket.hk/*",
      "*://hohomarket.hk/*"
    ]
  }
}
