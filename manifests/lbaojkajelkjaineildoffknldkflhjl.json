{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "moments.js"
    ]
  },
  "browser_action": {
    "default_title": "TabLife",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "dash.html"
  },
  "description": "Replace new tab page with a personal dashboard, best pictures, inspirational quotes.",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png"
  },
  "manifest_version": 2,
  "name": "TabLife",
  "options_page": "settings.html",
  "offline_enabled": true,
  "permissions": [
    "geolocation",
    "unlimitedStorage",
    "*://*.yahooapis.com/*",
    "*://*.solevl.com/*",
    "*://*.accuweather.com/*",
    "topSites",
    "storage"
  ],
  "short_name": "TabLife",
  "version": "0.4.0"
}
