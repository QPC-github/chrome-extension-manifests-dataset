{
  "name": "ITGStore.ro Extension",
  "description": "Extensie ITG",
  "icons": {
    "16": "16.png"
  },
  "browser_action": {
    "default_icon": "48.png",
    "default_title": "Extensie ITG",
    "default_popup": "itgmod.html"
  },
  "permissions": [
    "cookies"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "itg"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "0.1"
}
