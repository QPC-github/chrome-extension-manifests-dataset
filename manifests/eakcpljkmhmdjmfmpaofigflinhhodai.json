{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Custom New Tab",
  "description": "Your most sentimental pictures in your default tab, uploaded by you and randomly shown. ",
  "version": "3",
  "author": "Justin Wong",
  "icons": {
    "48": "photos/CustomNewTab48.png",
    "128": "photos/CustomNewTab128.png"
  },
  "browser_action": {
    "default_title": "Have a good day"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "topSites",
    "unlimitedStorage",
    "<all_urls>"
  ]
}
