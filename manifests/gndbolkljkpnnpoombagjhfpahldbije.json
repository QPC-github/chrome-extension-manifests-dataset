{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Hides all comments for YouTube™ until you mouseover them",
  "name": "Comment Hider for YouTube™",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch"
      ],
      "css": [
        "comment_hider.css"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.2.3"
}
