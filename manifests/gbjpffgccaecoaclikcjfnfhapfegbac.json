{
  "content_scripts": [
    {
      "css": [
        "css/estilo.css"
      ],
      "js": [
        "js/jquery-2.1.4.min.js",
        "js/vercomment.js"
      ],
      "matches": [
        "https://www.taringa.net/*"
      ],
      "include_globs": [
        "https://www.taringa.net/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_title": "Comment T!",
    "default_popup": "ver.html"
  },
  "description": "Ver los posts y comentarios de Taringa! sin salir de la home. by emilandi",
  "icons": {
    "128": "img/comments-512.png",
    "16": "img/comments-512.png",
    "19": "img/comments-512.png",
    "24": "img/comments-512.png",
    "32": "img/comments-512.png",
    "38": "img/comments-512.png",
    "48": "img/comments-512.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Comment T!",
  "permissions": [
    "https://www.taringa.net/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.18",
  "web_accessible_resources": [
    "css/post.css",
    "css/story.css"
  ]
}
