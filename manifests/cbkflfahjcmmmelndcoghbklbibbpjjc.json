{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "screen Cleaner",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Add to chrome to elminate popup on wral",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.wral.com/*",
    "https://www.wral.com/*/*",
    "https://www.wral.com/*/*/*",
    "https://futuristicfifth.com/*",
    "storage"
  ],
  "icons": {
    "16": "extLogo16.jpg",
    "48": "extLogo48.jpg",
    "128": "extLogo128.jpg"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {}
}
