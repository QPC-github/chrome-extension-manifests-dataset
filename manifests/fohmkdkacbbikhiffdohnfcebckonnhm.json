{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "kreativfont-128.png",
    "default_popup": "index.html",
    "default_title": "Kreativ Font Search"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Search new fonts on KreativFont.com and open the results in a new tab.",
  "icons": {
    "128": "kreativfont-128.png",
    "64": "kreativfont-64.png",
    "48": "kreativfont-48.png",
    "32": "kreativfont-32.png",
    "16": "kreativfont-16.png"
  },
  "manifest_version": 2,
  "name": "Kreativ Font Search",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://www.kreativfont.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2.0"
}
