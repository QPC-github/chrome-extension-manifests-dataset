{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OhMyRadio",
  "description": "Online Radio Player!",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "js/libs/soundmanager2.js",
      "js/OhMyRadio.js",
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "http://vishaltelangre.com/"
  ]
}
