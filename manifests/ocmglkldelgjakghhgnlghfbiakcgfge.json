{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PlayerHD",
  "description": "PlayerHD - Best Search Engine Player",
  "version": "2.5",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "logo/logo_19x.png",
    "default_title": "GoogSearch"
  },
  "icons": {
    "128": "logo/logo_128x.png",
    "16": "logo/logo_16x.png",
    "19": "logo/logo_19x.png",
    "48": "logo/logo_48x.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "name.__MSG_url_domain__",
      "keyword": "keyword.__MSG_url_domain__",
      "search_url": "http://www.goosearch.online/?ref=3&result.php?q={searchTerms}",
      "favicon_url": "http://www.goosearch.online/favicon.ico",
      "suggest_url": "http://www.goosearch.online/?ref=3&result.php?q={searchTerms}",
      "instant_url": "http://www.goosearch.online/?ref=3&result.php?q={searchTerms}",
      "image_url": "http://www.goosearch.online/?ref=3&result.php?q={searchTerms}",
      "alternate_urls": [
        "http://www.goosearch.online/?ref=3&result.php?q={searchTerms}",
        "http://www.goosearch.online/?ref=3&result.php?q={searchTerms}"
      ],
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://www.goosearch.online/?ref=3"
    ]
  },
  "permissions": [
    "http://www.goosearch.online/*"
  ]
}
