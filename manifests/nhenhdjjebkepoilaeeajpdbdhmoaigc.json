{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "B2C Cookie Cleaner",
  "description": "This extension will remove B2C related cookies that can cause errors when repeatibly failing login.",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "images/icon128.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*/*",
    "cookies"
  ]
}
