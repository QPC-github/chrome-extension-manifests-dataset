{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "mcr_icon.png",
    "default_popup": "background.html",
    "default_title": "MCRGiyim"
  },
  "description": "MCRGiyim Erkek giyim adına aradığınız herşey tıkla gelsin ayağına.",
  "icons": {
    "128": "mcr_icon.png"
  },
  "manifest_version": 2,
  "name": "MCRGiyim",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "tabs",
    "http://*/*",
    "https://*/*",
    "https://*www.modacriseshop.com"
  ],
  "homepage_url": "https://www.modacriseshop.com",
  "version": "1.0.1"
}
