{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Marklet Extension",
  "version": "1.3.0",
  "manifest_version": 2,
  "description": "Instantly add any website to your marklet account.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_name": "Add to Marklet",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://marklet.com/"
  ],
  "homepage_url": "http://marklet.com",
  "offline_enabled": false
}
