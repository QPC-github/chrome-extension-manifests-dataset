{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "assets/image/efficiently.png"
    },
    "default_popup": "index.html"
  },
  "description": "Time is precious! Be Efficient and stop yourself wasting time on websites with Efficently!",
  "icons": {
    "128": "assets/image/efficiently.png"
  },
  "manifest_version": 2,
  "name": "Efficiently",
  "permissions": [
    "storage",
    "tabs",
    "notifications"
  ],
  "short_name": "Efficiently",
  "version": "1.4"
}
