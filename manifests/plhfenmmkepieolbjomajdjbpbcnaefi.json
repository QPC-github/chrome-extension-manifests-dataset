{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RootDesk",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Integrated work tracking platform.Customer Support,BugTracker,Agile Software,Ticketing,Helpdesk,Automation,Collaboration,Timesheet",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "notifications",
    "bookmarks",
    "cookies",
    "background",
    "geolocation",
    "history",
    "sessions",
    "unlimitedStorage",
    "webRequest"
  ],
  "homepage_url": "http://rootdesk.datamoulds.com/BugHopper/deligateLogin.do",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "rootdesk",
    "default_popup": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "icon.png"
  }
}
