{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Roblox GameSort",
  "version": "2.41",
  "manifest_version": 2,
  "description": "Sorts and displays all of the highest rated games based on their thumb scores.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.roblox.com/*",
        "https://www.roblox.com/*"
      ],
      "js": [
        "jQuery.js",
        "page.js"
      ]
    }
  ]
}
