{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PioneerRx Support Center",
  "short_name": "Support Center",
  "description": "This extension provides quick access to commonly used employee actions.",
  "version": "2018.04.10.2",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "alarms",
    "clipboardWrite",
    "contentSettings",
    "background",
    "http://*.pioneerrx.com/*",
    "https://*.pioneerrx.com/*"
  ],
  "omnibox": {
    "keyword": "prx"
  },
  "icons": {
    "16": "assets/img/icon.png",
    "48": "assets/img/PioneerRx-Icon.png",
    "128": "assets/img/PioneerRx-Icon-128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "assets/js/jquery.min.js",
      "assets/js/events.js",
      "assets/js/misc.js",
      "assets/js/settings.js",
      "assets/js/omnibox.js",
      "assets/js/notifications.js",
      "assets/js/jquery.signalR-2.4.1.min.js",
      "assets/js/signalr.js",
      "assets/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/img/icon.png",
    "default_popup": "login.html"
  },
  "web_accessible_resources": [
    "assets/img/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.pioneerrx.com/*"
    ]
  }
}
