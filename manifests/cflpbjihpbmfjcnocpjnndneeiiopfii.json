{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rotate Google Hangouts",
  "description": "This extension will rotate your google hangouts screen.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon1.png",
    "default_popup": "popup.html"
  }
}
