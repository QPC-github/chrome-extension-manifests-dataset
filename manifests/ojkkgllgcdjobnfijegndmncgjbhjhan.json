{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AdWords Style Tweaks",
  "description": "CSS based adjustments for the AdWords interface",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "adwfails.css"
      ],
      "matches": [
        "https://adwords.google.com/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
