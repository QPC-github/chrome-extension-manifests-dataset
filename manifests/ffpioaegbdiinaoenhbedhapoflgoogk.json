{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon/128.png",
    "default_popup": "popup.html"
  },
  "author": "小黑君",
  "name": "小黑二维码",
  "homepage_url": "https://www.baidu.com",
  "description": "自带搜索引擎广告屏蔽技能的二维码工具，技能升级中。。。",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "1.0"
}
