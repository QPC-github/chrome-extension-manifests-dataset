{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Viola's Stamp",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Welcome to Viola's Stamp, a guide to where Viola might've gone to support Black businesses in Toronto.",
  "icons": {
    "16": "images/viola.png",
    "48": "images/viola.png",
    "128": "images/viola.png"
  },
  "browser_action": {
    "default_icon": "images/viola.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
