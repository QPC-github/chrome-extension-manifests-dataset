{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Live Data",
  "short_name": "Live Data",
  "version": "1.0.1",
  "version_name": "1.0.1",
  "description": "Identify prospects who understand what you're selling and why.",
  "icons": {
    "128": "/img/logo.png"
  },
  "browser_action": {
    "default_icon": "/img/logo.png"
  },
  "background": {
    "scripts": [
      "/lib/jquery.min.js",
      "/lib/webx.js",
      "/js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "web_accessible_resources": [
    "/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
