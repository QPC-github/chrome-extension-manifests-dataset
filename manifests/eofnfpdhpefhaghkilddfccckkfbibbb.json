{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "xkcd: Substitutions",
  "description": "Makes reading the news more fun!",
  "version": "0.0.1",
  "icons": {
    "256": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "inject.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "jquery.js",
    "inject.js"
  ]
}
