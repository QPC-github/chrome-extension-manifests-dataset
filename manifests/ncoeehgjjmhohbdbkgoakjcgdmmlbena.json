{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "form.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.8.2.min.js",
        "popup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "description": "Obtenha os melhores cupons e descontos das lojas online que voce esta navegando e economize em suas compras!",
  "manifest_version": 2,
  "name": "Mais Barato",
  "permissions": [
    "tabs",
    "bookmarks",
    "*://*/*",
    "http://kadmus-informatica.com.br/*",
    "https://braip.com.br/*",
    "cookies",
    "http://compre.vc/*",
    "https://www.lomadee.com/*",
    "http://kadmus.ofertou.com/*",
    "https://www.lomadee.com/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.2"
}
