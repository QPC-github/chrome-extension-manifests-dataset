{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DeepL translator",
  "description": "iFrame of deepl.com/translator",
  "version": "2.0",
  "browser_action": {
    "default_popup": "popup.html",
    "defaut_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.11.3.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
