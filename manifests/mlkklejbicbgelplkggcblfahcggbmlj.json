{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Batuush",
  "description": "Batuush",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "mazaalai-bootstrap.css"
      ],
      "js": [
        "bg.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "http://i.imgur.com/"
  ]
}
