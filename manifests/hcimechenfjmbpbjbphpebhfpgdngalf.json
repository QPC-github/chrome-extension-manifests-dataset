{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MLS Condo Listing Estimator",
  "short_name": "MLS Est",
  "description": "This extension provides an estimate for the actual sold price if the condo was to be sold today on MLS Toronto listings.",
  "version": "4.5",
  "browser_action": {
    "default_popup": "app.html"
  },
  "icons": {
    "16": "img/house_16_x_16.png",
    "48": "img/house_48_x_48.png",
    "128": "img/house_128_x_128.png"
  },
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
