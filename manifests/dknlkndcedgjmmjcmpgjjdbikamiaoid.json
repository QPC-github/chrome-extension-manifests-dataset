{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Usable Learn",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Makes D2L [uw] usable again. #UWaterlo #hashtags",
  "minimum_chrome_version": "16",
  "content_scripts": [
    {
      "matches": [
        "http://learn.uwaterloo.ca/*",
        "https://learn.uwaterloo.ca/*"
      ],
      "css": [
        "fixer.css"
      ]
    }
  ],
  "permissions": [
    "http://learn.uwaterloo.ca/*",
    "https://learn.uwaterloo.ca/*"
  ],
  "icons": {
    "16": "images/context menu icon.png",
    "19": "images/browser action icon.png",
    "48": "images/small app icon.png",
    "128": "images/big app icon.png"
  }
}
