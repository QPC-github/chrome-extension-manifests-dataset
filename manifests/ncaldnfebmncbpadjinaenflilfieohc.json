{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Klumpinatoren",
  "description": "This Chrome extension helps you forget that Rasmus Paludan exists.",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*.dk/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
