{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TDC Bitbucket helper",
  "version": "1.0.6",
  "description": "Insights into your PR.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://bitbucket.tdc.dk/projects/*/pull-requests/*"
      ],
      "all_frames": true,
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/tooltip.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/Logo.png",
    "48": "images/Logo.png",
    "128": "images/Logo.png"
  },
  "web_accessible_resources": [
    "images/React.png",
    "images/sass.png",
    "images/dotNet.png",
    "images/oneApprovalBadge.png",
    "images/twoApprovalBadge.png",
    "images/linesAddedIcon.png",
    "images/linesRemovedIcon.png",
    "css/tooltip.css"
  ]
}
