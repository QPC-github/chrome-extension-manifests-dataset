{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "2048",
  "default_locale": "en",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png"
  },
  "web_accessible_resources": [
    "res/*"
  ],
  "description": "packaged version of 2048, make you load game faster and play it offline",
  "background": {
    "page": "background.html"
  },
  "version": "1.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Play 2048"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "management",
    "cookies"
  ]
}
