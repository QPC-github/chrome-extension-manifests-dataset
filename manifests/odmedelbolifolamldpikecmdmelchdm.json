{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Poem A Day",
  "description": "This extension will provide a poem a day",
  "version": "0.2",
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "permissions": [
    "activeTab",
    "http://poetrydb.org/*",
    "storage"
  ]
}
