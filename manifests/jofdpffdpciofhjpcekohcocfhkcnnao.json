{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AndroTurk Radyo - Radyo Dinle",
  "version": "1.0",
  "short_name": "AndroTurk",
  "description": "Yüzlerce radyo istasyonunu ücretsiz dinleyeceğiniz pratik radyo uygulaması",
  "permissions": [
    "contentSettings",
    "http://*/",
    "https://*/",
    "http://*/*",
    "background",
    "storage"
  ],
  "background": {
    "persistent": true,
    "page": "b.html"
  },
  "browser_action": {
    "default_icon": "list_logo.png",
    "default_popup": "popup.html"
  },
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "radio.otf"
  ],
  "content_security_policy": "script-src 'self' https://s3.amazonaws.com/rekmobads/js/rekmob-test.js  https://ads.rekmob.com; object-src 'self'  ",
  "manifest_version": 2
}
