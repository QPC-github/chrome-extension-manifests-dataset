{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Translate English to Italian",
  "version": "1.0",
  "description": "Traduce una Pagina da Inglese a Italiano",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_title": "Translate English to Italian"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "manifest_version": 2
}
