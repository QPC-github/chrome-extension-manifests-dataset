{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GolakoMusic Import",
  "version": "2.5.2",
  "manifest_version": 2,
  "description": "Importieren zu GolakoMusic - Hinweis: Du musst dich auf der Videoseite (z.b. Youtube) befinden, um die Buttons zu verwenden.",
  "icons": {
    "16": "icon/icon.png",
    "32": "icon/icon.png",
    "128": "icon/icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "GolakoMusic Import",
    "default_icon": "icon/icon.png",
    "default_popup": "popup.html"
  }
}
