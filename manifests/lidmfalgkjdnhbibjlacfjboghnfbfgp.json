{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iMarkker",
  "description": "Fast Create Marker",
  "version": "1.0.3",
  "page_action": {
    "default_icon": {
      "19": "icons_19.png",
      "38": "icons_38.png"
    }
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "icons_16.png",
    "32": "icons_32.png",
    "48": "icons_48.png",
    "128": "icons_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
