{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Touremind",
  "description": "Never miss eSports events again.",
  "version": "1.1.1",
  "options_page": "options/chromeoptions.html",
  "permissions": [
    "http://www.teamliquid.net/*",
    "http://www.touremind.net/*",
    "*://*.dd27438.kasserver.com/*",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "chromemain.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "chromeevents.html",
    "default_title": "show tournaments"
  }
}
