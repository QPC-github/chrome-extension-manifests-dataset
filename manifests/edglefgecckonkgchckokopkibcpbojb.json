{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "content/style.css"
      ],
      "js": [
        "content/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "default_locale": "en_US",
  "description": "",
  "devtools_page": "devtools.html",
  "icons": {
    "128": "icons/logo_128.png",
    "16": "icons/logo_16.png",
    "32": "icons/logo_32.png",
    "48": "icons/logo_48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "xPath Finder",
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "enable.js"
  ],
  "version": "1.0.2"
}
