{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RatticChromium",
  "description": "An extension to interface with the RatticWeb API and automatically type in your passwords.",
  "version": "1.0.1",
  "options_page": "options.html",
  "icons": {
    "64": "image/rattic64.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "image/rattic19.png",
      "38": "image/rattic38.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery.js",
        "js/uri.js",
        "js/cscript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/uri.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "storage"
  ]
}
