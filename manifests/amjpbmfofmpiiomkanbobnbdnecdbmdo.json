{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EPS Screen Sharing",
  "author": "Enterprise productivity software",
  "version": "0.0.0.8",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for https://web.epsfamily.com It capture content of your screen.",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://web.epsfamily.com/*",
      "*://web-test.epsfamily.ca/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
