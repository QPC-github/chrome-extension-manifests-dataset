{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Collect pictures",
  "description": "一个简单的图片收集插件",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "version": "0.0.1",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery-1.8.3.js",
      "background.js"
    ]
  },
  "browser_action": {},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "fonts/*"
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "https://nfeng.net.cn"
}
