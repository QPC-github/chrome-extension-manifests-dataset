{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Individual-1",
  "description": "Changes 'Donald Trump' to 'Individual-1' on Chrome.",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png"
  },
