{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RateMyProf UofT",
  "description": "Look up professor's rating right on Acorn's course selection page",
  "version": "3",
  "icons": {
    "128": "Professor_courtesy-of-RateMyProfessor-1.png"
  },
  "browser_action": {
    "default_icon": "Professor_courtesy-of-RateMyProfessor-1.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventpage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "http://www.ratemyprofessors.com/*"
  ]
}
