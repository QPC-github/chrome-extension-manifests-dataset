{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rocket timer",
  "version": "1.0",
  "description": "Extension that shows the time to the shuttle launch. With Rocket timer, you can follow the launch of the shuttle online.",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html"
  },
  "offline_enabled": true,
  "permissions": [
    "notifications",
    "storage"
  ],
  "web_accessible_resources": [
    "images/icon48.png"
  ]
}
