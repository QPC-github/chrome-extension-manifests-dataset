{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "16": "icon16.png",
    "48": "icon16.png",
    "128": "icon16.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.js",
        "script.js",
        "background.js"
      ],
      "matches": [
        "https://www.roblox.com/*",
        "http://www.roblox.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Ro-Comments [ROBLOX Comments]",
  "permissions": [
    "https://www.roblox.com/",
    "http://www.roblox.com/"
  ],
  "version": "3.5",
  "background": {
    "scripts": [
      "background.js",
      "jquery.js"
    ]
  }
}
