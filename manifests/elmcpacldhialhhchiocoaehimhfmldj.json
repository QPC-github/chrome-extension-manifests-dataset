{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "lib/jquery-2.1.4.min.js",
        "lib/jquery-ui.js",
        "js/popup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Easy Shopping este o extensie usor de instalat si utilizat. <br /><br/>Contact: office.dinca[at]gmail.com",
  "icons": {
    "128": "images/icon.png",
    "32": "images/icon.png",
    "64": "images/icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Easy Shopping",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "tabs",
    "notifications",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.4.7"
}
