{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LinLin Coffee Equipment",
  "version": "1.0",
  "description": "Coffee beans search engine. Over 50 types of exotic fresh roasted beans in Bangkok stock,",
  "icons": {
    "48": "img/48-48-linlin-coffee-icon.png",
    "16": "img/16-16-linlin-coffee-icon.png",
    "128": "img/128-128-linlin-coffee-icon.png"
  },
  "browser_action": {
    "default_icon": "img/128-128-linlin-coffee-icon.png",
    "default_title": "Press the button to see more actions",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
