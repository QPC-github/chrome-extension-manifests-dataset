{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Autofill for Chrome by Fillr",
  "short_name": "Autofill",
  "description": "The worldâ€™s most intelligent autofill. Fillr is so good, it will change the way you shop and fill forms online forever.",
  "version": "1.7.34",
  "key": "",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "sjcl.js",
      "rncryptor.js",
      "background-bundled.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "identity",
    "identity.email",
    "privacy",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "MobileWidget.js",
        "content-script-bundled.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.fillr.com/*",
      "*://*.fillrdev.com/*",
      "*://localhost/*"
    ]
  },
  "web_accessible_resources": [
    "abandonment.js",
    "icon.png",
    "chip.html",
    "popup.html"
  ],
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js; object-src 'self'"
}
