{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kayler01 Alert",
  "description": "Status kana≈Çu Kayler01 na Twitch.tv",
  "version": "1.0.4",
  "author": "Marton (rafal.martonik@gmail.com)",
  "icons": {
    "16": "images/kayler01_ico_16_16.png",
    "32": "images/kayler01_ico_32_32.png",
    "128": "images/kayler01_ico_128_128.png"
  },
  "permissions": [
    "notifications",
    "https://api.twitch.tv/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/settings.js",
      "js/twitch.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/kayler01_ico_16_16.png",
      "32": "images/kayler01_ico_32_32.png",
      "128": "images/kayler01_ico_128_128.png"
    }
  },
  "content_security_policy": "script-src 'self' https://api.twitch.tv; object-src 'self'"
}
