{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Personal Browser",
  "description": "This extension sets your New Tab different themes with photos, links, updates and more",
  "version": "1.2",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "personal-browser.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "cookies",
    "http://*.personal-browser.com/"
  ],
  "optional_permissions": [
    "management"
  ],
  "browser_action": {
    "default_title": "Personal Browser",
    "default_icon": "img/icon16.png"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
