{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GetMedia! Trilulilu Downloader",
  "version": "1.0",
  "manifest_version": 2,
  "description": "GetMedia.info - Now it's very easy to download media files from Trilulilu.",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "http://*.trilulilu.ro/stream.php*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
