{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Follower for Instagram",
  "description": "Instagram Follower.Mass Follows, UnFollows, Likes and UnLikes.This bot uses variable time intervals to imitate a person.",
  "version": "0.0.35",
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "tabs",
    "https://www.instagram.com/*",
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Click to action",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "web_accessible_resources": [
    "options.html"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
