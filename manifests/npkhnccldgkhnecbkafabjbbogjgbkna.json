{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "tphp.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "unlimitedStorage"
  ],
  "name": "BuzzTab",
  "version": "0.1.5",
  "icons": {
    "128": "icons/logo128.png",
    "16": "icons/logo16.png",
    "48": "icons/logo48.png"
  },
  "description": "BuzzTab ",
  "browser_action": {
    "default_icon": "icons/logo128.png"
  },
  "optional_permissions": [
    "bookmarks",
    "chrome://favicon/"
  ]
}
