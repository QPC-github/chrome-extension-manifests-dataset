{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tracker",
  "version": "1.6.0",
  "manifest_version": 2,
  "description": "Make tracking time easy and painless with automated tracking, reminders, and reports.",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "charge.js",
      "Notify.js",
      "jquery1.7.2.min.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "notifications",
    "http://chargetracker.appspot.com/*"
  ]
}
