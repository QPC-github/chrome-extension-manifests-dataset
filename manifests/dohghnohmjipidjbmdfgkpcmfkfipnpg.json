{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tail",
  "version": "1.1",
  "description": "This app works like the 'tail-f' command.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_title": "Tail",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
