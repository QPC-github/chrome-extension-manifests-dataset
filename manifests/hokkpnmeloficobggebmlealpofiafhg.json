{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sally Twitch",
  "description": "Sally's Twitch Extension",
  "version": "1.5.4",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "ext.html",
    "default_icon": "icons/icon_off_128.png"
  },
  "icons": {
    "64": "icons/icon_on_64.png",
    "128": "icons/icon_on_128.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "tabs"
  ]
}
