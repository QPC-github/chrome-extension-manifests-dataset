{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Just One Tab",
  "description": "Just One Tab is a free Google Chrome extension that forces you to single-task.",
  "version": "0.1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "Tab Limit"
  },
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2
}
