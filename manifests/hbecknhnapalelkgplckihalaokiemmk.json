{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RedditHider",
  "version": "2.1.0",
  "description": "Hide Reddit links as you vote or click on them.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.reddit.com/*"
      ],
      "js": [
        "./dist/content_script.js"
      ]
    }
  ],
  "icons": {
    "16": "images/reddit-hide.16.png",
    "32": "images/reddit-hide.32.png",
    "48": "images/reddit-hide.48.png",
    "128": "images/reddit-hide.128.png"
  }
}
