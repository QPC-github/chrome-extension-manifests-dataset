{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cage for Chrome",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "34": "icon-34.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "notifications",
    "storage",
    "tabs",
    "https://*.cageapp.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.cageapp.com/*"
    ]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Capture pages with a click, download files, and seamlessly add screenshots to Cage.",
  "version": "0.0.6.2",
  "version_name": "0.0.6 beta.2"
}
