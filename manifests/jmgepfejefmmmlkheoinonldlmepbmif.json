{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "MaHua",
  "version": "1.0",
  "description": "一个在线编辑markdown文档的编辑器!",
  "browser_action": {
    "default_icon": "images/19.png",
    "default_title": "MaHua编辑器"
  },
  "icons": {
    "16": "images/16.png",
    "19": "images/19.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "background_page": "global.html",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
