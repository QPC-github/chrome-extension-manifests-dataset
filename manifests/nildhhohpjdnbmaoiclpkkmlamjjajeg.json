{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter follow by keywords",
  "description": "An awesome extension to search a keyword & follow or unfollow on Twitter.",
  "version": "1.2",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Easy search 0n twitter - Follow & Unfollow",
    "default_icon": "et_128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
