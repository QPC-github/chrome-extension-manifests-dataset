{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mic Note -Voice Recorder & Notepad",
  "version": "1.1.25",
  "minimum_chrome_version": "37",
  "manifest_version": 2,
  "description": "Record voice while taking note synced, for lectures,meetings,conferences,interviews,memos...",
  "offline_enabled": true,
  "default_locale": "en",
  "icons": {
    "16": "MN_16.png",
    "48": "MN_48.png",
    "128": "MN_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "oauth2": {
    "client_id": "720314066041-0njqjfjboidhbbuu890inrn976av3rja.apps.googleusercontent.com",
    "scopes": []
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "https://www.googleapis.com/",
    "identity",
    "https://accounts.google.com/",
    "<all_urls>",
    "audioCapture",
    "videoCapture",
    "power",
    {
      "fileSystem": [
        "write"
      ]
    },
    "notifications",
    "identity.email"
  ],
  "file_handlers": {
    "micnote": {
      "types": [
        "application/micnote"
      ],
      "extensions": [
        "micnote"
      ],
      "title": "Mic Note"
    }
  },
  "url_handlers": {
    "micnote": {
      "matches": [
        "http://micnote.treebeardbros.com/chrome-open/*"
      ],
      "title": "Mic Note"
    }
  },
  "platforms": [
    {
      "nacl_arch": "x86-64",
      "sub_package_path": "_platform_specific/x86-64/"
    },
    {
      "nacl_arch": "x86-32",
      "sub_package_path": "_platform_specific/x86-32/"
    },
    {
      "nacl_arch": "arm",
      "sub_package_path": "_platform_specific/arm/"
    },
    {
      "sub_package_path": "_platform_specific/all/"
    }
  ],
  "short_name": "Mic Note"
}
