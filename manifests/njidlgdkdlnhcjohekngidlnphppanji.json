{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "main/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/icons/19.png",
    "default_popup": "main/popup.html"
  },
  "description": "Perform Reverse Image Search with just one click.",
  "homepage_url": "https://www.smalltoolstation.com/reverse-image-search",
  "icons": {
    "128": "assets/icons/128.png",
    "16": "assets/icons/16.png",
    "48": "assets/icons/48.png"
  },
  "manifest_version": 2,
  "name": "Reverse Image Search Tool for Chrome",
  "permissions": [
    "https://ajax.googleapis.com/",
    "activeTab",
    "contextMenus"
  ],
  "version": "1.0"
}
