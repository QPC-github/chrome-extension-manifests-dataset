{
  "browser_action": {
    "default_icon": "DocSight.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Visualize visits to various documentation resources.",
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "background": {
    "page": "background.html"
  },
  "name": "DocSight",
  "permissions": [
    "history",
    "https://api.stackexchange.com/",
    "tabs",
    "unlimitedStorage",
    "storage",
    "chrome://favicon/"
  ],
  "options_page": "options.html",
  "manifest_version": 2,
  "version": "1.0.8"
}
