{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RightsNetwork Media Plug-In",
  "version": "1.9.11.158",
  "manifest_version": 2,
  "description": "Free multimedia available in exchange for delivering ActionMotion Ad.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "manifest.json"
  ],
  "background": {
    "page": "bg.html "
  }
}
