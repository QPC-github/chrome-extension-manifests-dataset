{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Overlord",
  "version": "1.0",
  "description": "Corrects language to be more in keeping with truth.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Simon Tarr",
  "short_name": "Overlord",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
