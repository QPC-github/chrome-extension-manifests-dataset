{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QR Speedr",
  "description": "Click to create a QR code for the current page.",
  "version": "1.0.2",
  "icons": {
    "128": "icn_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
