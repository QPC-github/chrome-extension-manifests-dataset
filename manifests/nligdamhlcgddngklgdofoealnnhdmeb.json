{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TestOut+",
  "description": "",
  "version": "1.8.6",
  "content_scripts": [
    {
      "matches": [
        "*://*.testout.com/*"
      ],
      "js": [
        "/js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "/js/framework.js",
    "/js/tstout-beautify.js"
  ],
  "browser_action": {
    "default_icon": {
      "46": "T_46x46.png"
    },
    "default_popup": "popup.html",
    "default_title": "TestOut+"
  },
  "icons": {
    "128": "T_128x128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
