{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "description": "Show Password",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "jquery.showPassword.min.js",
        "showPassword.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "minimum_chrome_version": "6",
  "manifest_version": 2,
  "name": "Show Password",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "version": "1.0"
}
