{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HistoryStats",
  "version": "1.4",
  "description": "Provides a statistical and graphical display of web page visits stored in your browser's history",
  "browser_action": {
    "default_icon": "icon24.png"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "http://*/*",
    "https://*/*",
    "history"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
