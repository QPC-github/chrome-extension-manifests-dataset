{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "You're Fired",
  "version": "1.0",
  "description": "Let me tell you about getting fired. I probably know more about getting fired than anyone. Make firing great again.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "author": "Joshua Hatfield"
}
