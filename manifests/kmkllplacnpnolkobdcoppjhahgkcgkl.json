{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Font Picker",
  "description": "Find fonts by hovering over text.",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {}
}
