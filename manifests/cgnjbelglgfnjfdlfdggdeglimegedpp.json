{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Title with URL",
  "description": "Get the active web page's title and URL. ",
  "version": "0.2",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "name": "Retrieve",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "twu-icon-19.png",
      "38": "twu-icon-38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.com/"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "content_script.js"
      ]
    }
  ],
  "icons": {
    "16": "twu-icon-16.png",
    "128": "twu-icon-128.png"
  },
  "permissions": [
    "clipboardWrite",
    "tabs",
    "activeTab"
  ]
}
