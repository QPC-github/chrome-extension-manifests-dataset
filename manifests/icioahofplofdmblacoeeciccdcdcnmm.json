{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "Wow_js.js"
    ]
  },
  "browser_action": {
    "default_icon": "image/19.png",
    "default_popup": "Wow_Web.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "Wow_js.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ]
    }
  ],
  "description": "一键应用_(张益斌制作)",
  "icons": {
    "100": "image/100.png",
    "128": "image/128.png",
    "19": "image/19.png",
    "48": "image/48.png"
  },
  "manifest_version": 2,
  "name": "一键应用",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "1.2.0"
}
