{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Space Launch Schedule",
  "description": "Space Launch Schedule App.",
  "manifest_version": 2,
  "version": "0.2",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "https://launchlibrary.net/",
    "https://maps.googleapis.com/",
    "https://s3.amazonaws.com/launchlibrary/RocketImages/",
    "storage"
  ],
  "offline_enabled": false,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
