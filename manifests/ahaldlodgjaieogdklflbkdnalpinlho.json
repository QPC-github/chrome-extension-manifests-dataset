{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JCT - Moodle++",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "*://moodle.jct.ac.il/",
        "*://moodle.jct.ac.il/?*"
      ],
      "js": [
        "js/jquery.min.js",
        "content-scripts/hide-courses.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "96": "icon.png"
  },
  "permissions": [
    "storage"
  ]
}
