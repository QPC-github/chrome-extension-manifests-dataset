{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "e-bonsplans",
  "description": "e-bonsplans",
  "version": "1.0",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-2.0.2.min.js",
      "background.js",
      "ext.js"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "web_accessible_resources": [
    "icon16.png"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "notifications",
    "http://www.e-bonsplans.fr/"
  ]
}
