{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spotify Sound Mixer: Relax & Meditation",
  "version": "1.1",
  "description": "Mix sounds by yourself to avoid distraction",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
