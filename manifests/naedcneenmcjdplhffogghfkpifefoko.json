{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello | Custom Fields First",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Pushes the custom fields above the description",
  "homepage_url": "https://codebrauer.com",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Trello | Custom Fields First"
  },
  "permissions": [
    "activeTab",
    "https://trello.com/*",
    "tabs"
  ]
}
