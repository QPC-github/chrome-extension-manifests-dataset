{
  "author": "Aqualight",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Aqualight",
    "default_popup": "popup.html"
  },
  "description": "Aqualight marketing",
  "icons": {
    "16": "aqualight/img/hexa.png",
    "32": "aqualight/img/hexa.png",
    "48": "aqualight/img/hexa.png",
    "128": "aqualight/img/hexa.png"
  },
  "name": "Aqualight",
  "permissions": [
    "tabs",
