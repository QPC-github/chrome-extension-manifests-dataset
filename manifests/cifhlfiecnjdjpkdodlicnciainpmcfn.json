{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TryLikes Player",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Additional tooling for the TryLikes Player",
  "icons": {
    "16": "trylikes_16.png",
    "48": "trylikes_48.png",
    "128": "trylikes_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "background",
    "tabs",
    "alarms",
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}
