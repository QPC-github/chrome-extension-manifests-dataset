{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-assets/icon.png",
    "default_title": "QR Code Generator",
    "name": "QR Code Generator"
  },
  "icons": {
    "128": "icon-assets/icon_128.png",
    "16": "icon-assets/icon_16.png",
    "48": "icon-assets/icon_48.png"
  },
  "manifest_version": 2,
  "name": "QR Code Generator",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "version": "0.1"
}
