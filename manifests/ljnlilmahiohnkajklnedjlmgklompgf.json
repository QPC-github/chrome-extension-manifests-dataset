{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Помощник Совести: рассрочка на Яндекс Маркете",
  "description": "Покажет месячный платеж на товары в Яндекс Маркете, подсветит магазины где принимают карту Совесть и выдачу в Google и Яндекс Поиске",
  "version": "1.1",
  "permissions": [
    "webNavigation",
    "activeTab",
    "tabs",
    "notifications",
    "https://market.yandex.ru/product/*",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "*.svg",
    "*.jpg",
    "*.JPG",
    "*.png",
    "*.PNG",
    "*.json"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
