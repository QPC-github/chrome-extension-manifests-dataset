{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pontos VTS",
  "version": "1.2",
  "description": "Vessel Track Monitoring System.",
  "icons": {
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "permissions": [
    "http://pontos.solutions/",
    "http://my-pontos.com/"
  ],
  "browser_action": {
    "default_title": "Pontos Solutions",
    "default_icon": {
      "19": "images/icon_19.png",
      "48": "images/icon_48.png"
    },
    "default_popup": "popup.html"
  }
}
