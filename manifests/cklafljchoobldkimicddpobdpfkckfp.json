{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_chrome_app_name__",
  "short_name": "__MSG_chrome_shortname__",
  "version": "1.1.4",
  "description": "__MSG_chrome_app_description__",
  "permissions": [
    "management",
    "bookmarks",
    "contextMenus",
    "notifications",
    "topSites",
    "unlimitedStorage"
  ],
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "icons": {
    "128": "image/icon_128.png",
    "16": "image/icon_16.png"
  },
  "manifest_version": 2
}
