{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HubSpot Service Hub - Jira Issues",
  "short_name": "HubSpot Service Hub - Jira Issues",
  "description": "The HubSpot Service Hub - Provides you with the easy access to view and create Jira issues for HubSpot tickets.",
  "version": "1.0.6",
  "manifest_version": 2,
  "author": "MPULL",
  "permissions": [
    "tabs",
    "https://jira-hubspot.mpull.com/api/"
  ],
  "icons": {
    "16": "assets/images/mpull-logo-final.png",
    "48": "assets/images/mpull-logo-final.png",
    "128": "assets/images/mpull-logo-final.png"
  },
  "browser_action": {
    "default_icon": "assets/images/mpull-logo-final.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "assets/js/background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
