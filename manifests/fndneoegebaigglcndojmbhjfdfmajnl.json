{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TwitterCleaner",
  "version": "1.0.1",
  "description": "One click followers cleaner",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/followers"
      ],
      "css": [
        "report-spam.css"
      ],
      "js": [
        "report-spam.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "report-spam-icon.png",
    "report-spam-avatar-block.png"
  ],
  "manifest_version": 2
}
