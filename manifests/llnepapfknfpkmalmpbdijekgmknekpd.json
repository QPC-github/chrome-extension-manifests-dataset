{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mr. Trump Fart Swap",
  "description": "Replace all occurrences of \"Trump\" with \"Fart\" and other easter eggs",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "sprintf.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
