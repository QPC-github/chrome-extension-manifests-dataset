{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Artistaday.com Daily Contemporary Art",
  "description": "Loads the featured artist from Artistaday.com as your background in tabs and gives you quick access to search across 2500+ artists.",
  "version": "2.0.6",
  "short_name": "Artist A Day",
  "icons": {
    "16": "logo_16.jpg",
    "48": "logo_48.jpg",
    "128": "logo_128.jpg",
    "256": "logo_256.jpg"
  },
  "permissions": [
    "activeTab",
    "https://www.artistaday.com/",
    "https://www.google-analytics.com/",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "aad.html"
  },
  "browser_action": {
    "default_icon": "logo_16.jpg",
    "default_title": "Search Artistaday.com",
    "default_popup": "search.html"
  }
}
