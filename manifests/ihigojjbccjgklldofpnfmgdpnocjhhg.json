{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "When Did I Wake Up",
  "description": "An extension that attempts to guess the time you woke up by analyzing dates in the history.",
  "version": "2.0",
  "permissions": [
    "history",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "38": "icon128.png"
    },
    "default_title": "When Did I Wake Up"
  },
  "icons": {
    "128": "icon128.png"
  },
  "options_page": "options.html"
}
