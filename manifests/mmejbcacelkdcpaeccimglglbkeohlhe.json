{
  "manifest_version": 2,
  "name": "udata",
  "short_name": "udata",
  "author": "guangcai.jgc@alibaba-inc.com",
  "description": "udata数据插件，多终端可视化用户行为分析的好产品",
  "icons": {
    "128": "image/icon_128.png",
    "16": "image/icon_16.png",
    "48": "image/icon_48.png",
    "64": "image/icon_64.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "background/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "image/icon_48.png",
    "default_title": "udata 6.11.4"
  },
  "content_security_policy": "script-src 'self'  https://s.tbcdn.cn http://127.0.0.1 http://localhost; object-src 'self'",
  "optional_permissions": [
    "downloads"
  ],
  "permissions": [
    "browsingData",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "*://*.alibaba-inc.com/*",
    "*://*.taobao.com/*",
    "*://*.tmall.com/*",
    "*://*.1688.com/*",
    "*://*.youku.com/*",
    "*://*.daraz.lk/*",
    "*://*.lazada.sg/*",
    "*://*.fliggy.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "6.11.4",
  "web_accessible_resources": [
    "image/icon/icon_48.png",
    "js/inject.js",
    "js/spmclick.js"
  ]
}
