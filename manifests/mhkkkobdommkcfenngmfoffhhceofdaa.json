{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit USL Check",
  "description": "Commissioned by MrAahz and built by ThunderPowa",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*",
        "https://www.reddit.com/*",
        "http://old.reddit.com/*",
        "https://old.reddit.com/*"
      ],
      "js": [
        "jquery.min.js",
        "USL.js"
      ]
    }
  ]
}
