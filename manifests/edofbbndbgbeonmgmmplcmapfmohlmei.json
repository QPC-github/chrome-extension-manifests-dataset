{
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'unsafe-eval'; object-src 'unsafe-eval'",
  "description": "NoteIt is a tools that helps you to take notes.It is Simple, Quick and Autosave",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "manifest_version": 2,
  "name": "NoteIt",
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.0.9"
}
