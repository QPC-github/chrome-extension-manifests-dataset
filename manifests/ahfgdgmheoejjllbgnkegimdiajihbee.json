{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Heavybit Podcastsのページを英語学習用に使いやすくします",
  "version": "1.2.0",
  "name": "Heavybit extension for English learners",
  "short_name": "Heavybit extension",
  "manifest_version": 2,
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://www.heavybit.com/library/podcasts/*/ep-*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
