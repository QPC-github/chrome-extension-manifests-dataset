{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What's on Beat",
  "description": "This simple app will show which song is playing on radio Beat right now",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [],
  "background": {
    "scripts": [
      "jquery.js"
    ]
  }
}
