{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JavaScript Tracker",
  "short_name": "JS-Tracker",
  "description": "Track JavaScript that uses DOM/jQuery APIs on web page at runtime",
  "version": "3.0.1",
  "icons": {
    "16": "assets/appicon-16.png",
    "48": "assets/appicon-48.png",
    "128": "assets/appicon-128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": false
  },
  "devtools_page": "dist/devtool.html",
  "web_accessible_resources": [
    "dist/sidebar.html",
    "dist/sidebar.css"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/",
    "https://*/",
    "file://*/"
  ]
}
