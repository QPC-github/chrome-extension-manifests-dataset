{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "napOleon",
  "manifest_version": 2,
  "version": "1.3",
  "permissions": [
    "storage"
  ],
  "description": "Keep track of your sleeping and naps and subtract napping time from a bank.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "bg.html"
  },
  "icons": {
    "16": "bed.png",
    "48": "bed-64.png",
    "128": "bed-128.png"
  }
}
