{
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "jquery-ui.js",
        "gcvote.js"
      ],
      "matches": [
        "http://www.geocaching.com/seek/cache_details.aspx?*",
        "http://www.geocaching.com/geocache/GC*",
        "http://www.geocaching.com/my/*",
        "http://www.geocaching.com/map*",
        "http://www.geocaching.com/seek/nearest.aspx*",
        "http://www.geocaching.com/seek/gmnearest.aspx*",
        "http://www.geocaching.com/seek/log.aspx?*",
        "http://www.geocaching.com/seek/cdpf.aspx?*",
        "http://www.geocaching.com/bookmarks/view.aspx?*",
        "http://*.gcvote.de/*",
        "https://www.geocaching.com/seek/cache_details.aspx?*",
        "https://www.geocaching.com/my/*",
        "https://www.geocaching.com/map*",
        "https://www.geocaching.com/seek/nearest.aspx*",
        "https://www.geocaching.com/seek/gmnearest.aspx*",
        "https://www.geocaching.com/seek/log.aspx?*",
        "https://www.geocaching.com/seek/cdpf.aspx?*",
        "https://www.geocaching.com/bookmarks/view.aspx?*",
        "https://www.geocaching.com/*",
        "https://*.gcvote.com/*",
        "https://*.dosensuche.de/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "displays average votes and lets users vote for the quality of caches on geocaching.com",
  "homepage_url": "http://gcvote.com",
  "icons": {
    "128": "GCVote128x128.png",
    "32": "GCVote32x32.png",
    "48": "GCVote48x48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "name": "GCVote",
  "permissions": [
    "http://gcvote.com/",
    "http://dl.dropbox.com/",
    "http://dl.dropboxusercontent.com/",
    "https://ssl.webpack.de/gcvote.com/",
    "storage"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "3.3.3"
}
