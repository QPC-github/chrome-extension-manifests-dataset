{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Estimato",
  "description": "Estimate better for the work you do.",
  "version": "1.0.1",
  "browser_action": {
    "default_title": "Estimato",
    "default_icon": "images/logo.png",
    "default_popup": "chrome-extension.html"
  },
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
