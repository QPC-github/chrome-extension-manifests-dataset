{
  "background": {
    "page": "background.html"
  },
  "description": "百度短网址，点击图标生成当前页的短网址",
  "browser_action": {
    "default_icon": "16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "128.png",
    "16": "16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "百度短网址",
  "permissions": [
    "http://*/*",
    "notifications",
    "tabs",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "*://*/*",
    "unlimitedStorage",
    "management",
    "cookies"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
