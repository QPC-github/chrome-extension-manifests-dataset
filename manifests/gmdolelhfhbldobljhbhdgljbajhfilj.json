{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "img/icon22.png",
    "default_popup": "popup.html",
    "default_title": "Search Stackoverflow"
  },
  "default_locale": "en",
  "description": "Stackoverflow Search Extension",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png"
  },
  "minimum_chrome_version": "6.0",
  "name": "search stackoverflow",
  "permissions": [
    "contextMenus"
  ]
}
