{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wallpaper Warehouse",
  "version": "1.4",
  "description": "The blog features articles on home wallpapers to decorate your walls. Read to find more information.",
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Wallpaper Warehouse"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.js"
    ]
  }
}
