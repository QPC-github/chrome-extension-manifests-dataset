{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Beautify Wikipedia",
  "description": "Make Wikipedia Beautiful",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
