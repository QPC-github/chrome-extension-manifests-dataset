{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuickQuakSearch",
  "description": "This extension to QuickQuaksearch in newtab.",
  "Description": "QuickQuak",
  "version": "1.1.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "Quickquack.html"
  },
  "web_accessible_resources": [
    "Test.png"
  ],
  "chrome_settings_overrides": {
    "homepage": "http://quickquacksearch.com",
    "search_provider": {
      "name": "name.__MSG_url_domain__",
      "keyword": "keyword.__MSG_url_domain__",
      "search_url": "http://quickquacksearch.com/Search.aspx?q={searchTerms}",
      "favicon_url": "http://quickquacksearch.com/favicon.ico",
      "suggest_url": "http://quickquacksearch.com/suggest?q={searchTerms}",
      "instant_url": "http://quickquacksearch.com/instant?q={searchTerms}",
      "image_url": "http://quickquacksearch.com/image?q={searchTerms}",
      "search_url_post_params": "search_lang=__MSG_url_domain__",
      "suggest_url_post_params": "suggest_lang=__MSG_url_domain__",
      "instant_url_post_params": "instant_lang=__MSG_url_domain__",
      "image_url_post_params": "image_lang=__MSG_url_domain__",
      "alternate_urls": [
        "http://quickquacksearch.com/Search.aspx?q={searchTerms}",
        "http://quickquacksearch.com/Search.aspx?q={searchTerms}"
      ],
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
