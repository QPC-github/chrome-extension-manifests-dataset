{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TUT.Фильтр",
  "short_name": "TUT.Фильтр",
  "description": "TUT.Фильтр отфильтровывает навязчивые новости на портале TUT.BY, оставляя ваш разум ясным и чистым!",
  "version": "1.1.0.3",
  "options_page": "options.html",
  "icons": {
    "16": "tut.by.logo-16x16.png",
    "48": "tut.by.logo-48x48.png",
    "128": "tut.by.logo-128x128.png"
  },
  "browser_action": {
    "default_icon": "tut.by.logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*.tut.by/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.tut.by/*"
      ],
      "css": [
        "mystyles.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://*.tut.by/*"
      ],
      "js": [
        "jquery-1.12.0.min.js",
        "filter-storage.js",
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
