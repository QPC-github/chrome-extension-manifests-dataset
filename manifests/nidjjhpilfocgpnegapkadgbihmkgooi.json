{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UptimeRobot Extension",
  "version": "1.1.4",
  "manifest_version": 2,
  "description": "Check the status of your web sites on UptimeRobot",
  "homepage_url": "http://www.TravelingTechGuy.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "UptimeRobot Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "http://api.uptimerobot.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
