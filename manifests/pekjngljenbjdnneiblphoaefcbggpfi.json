{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab Rigby & Quote Extension",
  "description": "Open a new tab to see a picture of Rigby & a random quote.",
  "version": "1",
  "author": "Kelly Sample",
  "browser_action": {
    "default_icon": "images/sharpei.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
