{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Daily Star - Lebanon",
  "version": "1.2",
  "description": "The Daily Star - Lebanon official Chrome extension",
  "icons": {
    "48": "img/TDS-48.png",
    "128": "img/TDS-128.png",
    "16": "img/TDS-16.png"
  },
  "browser_action": {
    "default_icon": "img/TDS-19.png",
    "default_title": "The Daily Star - Lebanon",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ]
}
