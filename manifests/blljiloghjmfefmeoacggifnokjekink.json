{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js",
      "scripts/jquery-3.2.1.min.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/PlayPause48.png",
    "default_title": "Play/Pause"
  },
  "commands": {
    "play-pause-music": {
      "suggested_key": {
        "default": "MediaPlayPause",
        "mac": "MediaPlayPause"
      },
      "description": "Play pause music",
      "global": true
    }
  },
  "description": "Play/Pause on tab.",
  "icons": {
    "16": "img/PlayPause16.png",
    "48": "img/PlayPause48.png",
    "128": "img/PlayPause128.png"
  },
  "manifest_version": 2,
  "name": "Tab Play Pause",
  "permissions": [
    "tabs",
    "storage",
    "https://www.udemy.com/",
    "https://open.spotify.com/",
    "https://www.deezer.com/",
    "https://play.google.com/",
    "https://www.youtube.com/",
    "http://www.fabricahost.com.br/",
    "https://cursos.alura.com.br/"
  ],
  "version": "1.0.8"
}
