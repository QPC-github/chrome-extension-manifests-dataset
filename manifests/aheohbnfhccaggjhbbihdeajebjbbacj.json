{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "1.1.0.11",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "16": "logos/16.png",
    "19": "logos/19.png",
    "128": "logos/128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "lib/jquery-2.0.2.min.js",
        "constants.js",
        "lib/underscore.js",
        "embedded_ga_host.js",
        "common/analytics.js",
        "common/storage.js",
        "background.js"
      ],
      "persistent": true
    }
  },
  "permissions": [
    "notifications",
    "alarms",
    "*://*/*",
    "chrome://favicon/",
    "storage",
    "unlimitedStorage"
  ],
  "sandbox": {
    "pages": [
      "embedded_ga_host.html"
    ]
  },
  "manifest_version": 2
}
