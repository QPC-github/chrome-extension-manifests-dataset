{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MeasureSEO â€“ Keyword Density",
  "short_name": "Keyword Density",
  "version": "1.0",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "description": "Easy to use seo keyword density tool that shows the most frequently used words on the current page.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/lib/jquery-3.3.1.slim.min.js",
        "js/content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/lib/jquery-3.3.1.slim.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/128x128.png",
    "default_popup": "popup.html",
    "default_title": "Keyword Density"
  }
}
