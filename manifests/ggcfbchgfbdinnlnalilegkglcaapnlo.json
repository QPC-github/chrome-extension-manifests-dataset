{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Combinatorics Calculator",
  "version": "1.0",
  "description": "A specialized calculator to help with Combinatorics calculations",
  "content_scripts": [
    {
      "matches": [
        "http://www.google.co.in/"
      ],
      "js": [
        "javascript/jquery-2.1.1.min.js",
        "javascript/extension-support.js",
        "javascript/math.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "default-icon.png",
    "default_title": "Combinatorics Calculator",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "offline_enabled": true,
  "short_name": "Combo Calc"
}
