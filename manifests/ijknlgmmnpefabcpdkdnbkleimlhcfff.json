{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TouchActive Addit",
  "version": "1.6",
  "manifest_version": 2,
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "icons": {
    "16": "ta_icon_16x16.png",
    "19": "ta_icon_19x19.png",
    "48": "ta_icon_48x48.png",
    "128": "ta_icon_128x128.png"
  },
  "background": {
    "scripts": [
      "listener.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "__MSG_defaultTitle__",
    "default_icon": "ta_icon_19x19.png"
  }
}
