{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Ads from Tumblr",
  "version": "1.0.9",
  "manifest_version": 2,
  "description": "Remove ads from tumblr.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.tumblr.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
