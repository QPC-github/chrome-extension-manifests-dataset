{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kuttipencil",
  "description": "A tiny utility to type Malayalam.",
  "version": "6.0",
  "browser_action": {
    "default_icon": "img/icon192.png",
    "default_popup": "popup.html",
    "default_title": "Kuttipencil"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "16": "img/icon192.png",
    "48": "img/icon192.png",
    "128": "img/icon192.png"
  }
}
