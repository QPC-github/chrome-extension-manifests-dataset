{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "50",
  "name": "robots.txt viewer",
  "short_name": "robots.txt",
  "description": "check the robots.txt content on websites with one click.",
  "homepage_url": "https://www.robots-viewer.com/",
  "author": "Nico Hemkes",
  "version": "1.5.7",
  "options_ui": {
    "page": "about.html"
  },
  "browser_action": {
    "default_title": "robots.txt viewer",
    "default_icon": "img/icon_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "img/icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://www.robots-viewer.com/"
  ]
}
