{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Klick Chrome Extension",
  "short_name": "Klick",
  "version": "0.0.0.6",
  "description": "Klick Chrome Extension",
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "128": "logo-128.png"
  },
  "author": "D'Arcy Rittich",
  "content_scripts": [
    {
      "matches": [
        "https://*.mail.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "offline_enabled": false,
  "web_accessible_resources": [
    "jquery-2.1.1.min.js",
    "gmail.js",
    "uuid.js",
    "main.js"
  ]
}
