{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Genius Design Quotes",
  "version": "0.9.2",
  "description": "Shows you a beautifully crafted quote from a design genius every time you open a new tab.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://seriousness.nl/"
  ],
  "chrome_url_overrides": {
    "newtab": "quotes.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js; object-src 'self'",
  "manifest_version": 2
}
