{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide Input",
  "description": "Hides the data inside all input fields, eg email, name, address, etc. Great for streamers, or if you're working in public.",
  "version": "1.0.0.3",
  "icons": {
    "16": "hide-input-16.png",
    "48": "hide-input-48.png",
    "128": "hide-input-128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "hide.css"
      ]
    }
  ]
}
