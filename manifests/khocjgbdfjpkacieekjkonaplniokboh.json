{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Qogeo",
  "description": "Informations géographiques et démographiques des communes, départements et régions de France.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*.qogeo.com/"
  ],
  "homepage_url": "http://www.qogeo.com",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
