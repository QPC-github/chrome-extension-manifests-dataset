{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Link Icon",
  "version": "4.2",
  "description": "Know where you're going to before clicking on a link.",
  "icons": {
    "16": "images/extension/16.png",
    "48": "images/extension/48.png",
    "128": "images/extension/128.png"
  },
  "author": "pb_ee1",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "opentip/opentip.css",
        "opentip/opentip-custom.css"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "opentip/opentip-jquery.min.js",
        "link_icon.js",
        "tld.js",
        "engine.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "options_page": "options.html",
  "short_name": "Link Icon"
}
