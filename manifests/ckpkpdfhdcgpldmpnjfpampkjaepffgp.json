{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "screenshot and find pageTag",
  "description": "make work easily",
  "version": "1.3",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "pageTag",
    "default_icon": "48.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jQuery.js",
        "myscript.js"
      ]
    }
  ],
  "manifest_version": 2
}
