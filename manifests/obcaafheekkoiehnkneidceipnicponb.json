{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "YapiHayat.com",
  "version": "1.0",
  "manifest_version": 2,
  "description": "En güncel sıfır projeler ve yapı haberleri",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "popup.js"
      ]
    }
  ],
  "permissions": [
    "http://www.yapihayat.com/"
  ]
}
