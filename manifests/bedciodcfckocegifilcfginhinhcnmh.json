{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "crawl.png",
    "default_popup": "views/view.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/main.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Se connecte automatiquement à un conseiller dès que celui-ci est disponible",
  "icons": {
    "128": "crawl.png",
    "16": "crawl.png",
    "48": "crawl.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/pipe.js"
    ]
  },
  "manifest_version": 2,
  "name": "Bouygue Automatic Contact",
  "author": "Dave Hill",
  "homepage_url": "https://dyrk.org",
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "version": "1.0"
}
