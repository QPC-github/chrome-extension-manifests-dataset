{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BuzzNearby",
  "description": "This extension displays nearby weekend events to a user",
  "version": "1.0",
  "browser_action": {
    "default_icon": "assets/images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Find Events nearby with EventBrite"
  },
  "permissions": [
    "<all_urls>"
  ]
}
