{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Control your browser's tabs from command line",
  "manifest_version": 2,
  "name": "BroTab",
  "version": "1.2.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "brotab-icon-128x128.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "short_name": "BroTab"
}
