{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.0",
  "name": "CB Insights",
  "manifest_version": 2,
  "description": "Pull CB Insightsâ€™ VC Data Right Into Your Browser",
  "key": "",
  "browser_action": {
    "default_title": "Look up on CB Insights",
    "default_icon": "img/icons/icon-small.png"
  },
  "icons": {
    "16": "img/icons/icon-bitty.png",
    "48": "img/icons/icon-small.png",
    "128": "img/icons/icon-large.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "js/gmail.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inject.html",
    "js/libs/jquery.min.js",
    "js/libs/gmail.js",
    "js/gmailRunner.bundle.js"
  ],
  "omnibox": {
    "keyword": "cbi"
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "object-src 'self' ;script-src 'self' 'unsafe-eval'; style-src * 'unsafe-inline'; img-src *;"
}
