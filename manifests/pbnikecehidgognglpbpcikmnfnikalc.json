{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "F5 Access Guard",
  "version": "1.0.0.6",
  "description": "F5 Access Guard enables seamless, secure access to enterprise applications",
  "icons": {
    "32": "32x32.png",
    "64": "64x64.png",
    "128": "128x128.png"
  },
  "permissions": [
    "idle",
    "webRequest",
    "nativeMessaging",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "common-utilities.js",
      "globals.js",
      "clientinfo-status-tracker.js",
      "webrequest.js",
      "nmapi-common.js",
      "nmapi.js",
      "background.js"
    ]
  },
  "manifest_version": 2
}
