{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://www.amazon.co.uk/*",
        "https://www.amazon.co.uk/*",
        "http://amazon.co.uk/*",
        "https://amazon.co.uk/*"
      ]
    }
  ],
  "description": "Convierte los precios de Amazon.co.uk de libras a euros y traduce detalles sobre costes del envio.",
  "icons": {
    "128": "images/logo-128.png",
    "16": "images/logo-16.png",
    "48": "images/logo-48.png"
  },
  "manifest_version": 2,
  "name": "Amazon.co.uk en Euros cambio actual",
  "permissions": [
    "http://download.finance.yahoo.com/*",
    "tabs",
    "cookies",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "clipboardWrite",
    "idle",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "3.3.0"
}
