{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smooth Scroll Sucks",
  "version": "1.0",
  "description": "Take back your scrollbar from those who insist on holding it hostage",
  "page_action": {
    "default_title": "SMOOTH SCROLL SUCKS!"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "declarativeContent",
    "*://*/*scroll.js*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
