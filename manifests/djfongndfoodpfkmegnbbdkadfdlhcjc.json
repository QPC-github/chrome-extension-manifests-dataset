{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tested in Browser",
  "description": "This extension allows you to indicate what browsers you tested your pull request code in.",
  "version": "1.1",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://github.com/",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_title": "Tested In Browser"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
