{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Calendar Plus",
  "short_name": "GCal Plus",
  "version": "1.7.1",
  "manifest_version": 2,
  "description": "Extra Options for Google Calendar",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "key": "",
  "icons": {
    "128": "iconnew.png"
  },
  "browser_action": {
    "name": "GCalPlus",
    "icons": [
      "iconnew.png"
    ],
    "default_icon": "iconnew.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/calendar/u/*"
      ],
      "run_at": "document_start",
      "js": [
        "jquery-3.3.1.min.js",
        "jquery-ui.min.js",
        "jquery-ui.multidatespicker.js",
        "Moment.js"
      ],
      "css": [
        "jquery-ui.min.css",
        "jquery-ui.multidatespicker.css",
        "datepicker.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://calendar.google.com/calendar/",
    "identity",
    "storage",
    "webNavigation",
    "https://accounts.google.com/o/oauth2/v2/auth"
  ],
  "oauth2": {
    "client_id": "782490446709-6sshm5rboluibdvr0oi60d9eo6711fn1.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "https://www.googleapis.com/auth/calendar.events",
      "https://www.googleapis.com/auth/calendar.settings.readonly",
      "email",
      "profile",
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "web_accessible_resources": [
    "https://accounts.google.com/o/oauth2/v2/auth",
    "extras.css",
    "hide.css",
    "resizeMonth.css",
    "jquery-3.3.1.min.js"
  ]
}
