{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Code Tree Updates",
  "version": "1.1",
  "description": "An extension that adds functionality to CodeTree",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://codetree.com/projects/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
