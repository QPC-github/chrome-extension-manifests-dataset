{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Video Game Quotes",
  "short_name": "New Tab",
  "version": "0.2.4",
  "description": "An extension that will give you randomly selected motivational quotes referenced from video games and RPGs!",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "permissions": [
    "activeTab"
  ]
}
