{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Say Time for Chrome"
  },
  "description": "This extension speaks the time every hour.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Say Time for Chrome",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "alarms",
    "storage",
    "tts"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.3.0"
}
