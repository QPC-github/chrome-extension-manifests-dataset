{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Resourcify",
  "description": "Free access to 100's of teacher-created resources for IM 6-8 Math.",
  "version": "0.0.35",
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "cookies",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "assets/logo-lit-16.png",
    "48": "assets/logo-lit-48.png",
    "128": "assets/logo-lit-128.png"
  },
  "browser_action": {
    "default_icon": "./assets/logo-dark-48.png"
  },
  "web_accessible_resources": [
    "assets/logo-full.png",
    "assets/background.jpg"
  ]
}
