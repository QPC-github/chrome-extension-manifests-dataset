{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://faces.bronto.com/*"
      ]
    }
  ],
  "description": "A tool for Bronto employees only.  Adds subordinates info on a boss' faces page; perform advanced faces searches from the url bar.",
  "icons": {
    "128": "brontoFaces128Transp.png",
    "16": "iconBlacking2.png"
  },
  "manifest_version": 2,
  "name": "faces",
  "omnibox": {
    "keyword": "faces"
  },
  "permissions": [
    "http://faces.bronto.com/"
  ],
  "scrreminder": "make next version something like 0.1.0.0.0.0.1",
  "short_name": "faces",
  "version": "0.1.1.0"
}
