{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SaveTabs",
  "description": "Save open tabs",
  "version": "1.0",
  "browser_action": {
    "default_icon": "newIcon.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "sessions"
  ]
}
