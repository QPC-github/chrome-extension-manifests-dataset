{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "dnscheck.js"
    ]
  },
  "name": "Whois One-Click DNS Lookup",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Easily perform DNS Lookups from the navigation bar.",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
