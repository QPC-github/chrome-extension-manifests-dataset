{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Astronomy Everyday",
  "description": "Displays the astronomy picture of the day with a brief description.",
  "version": "1.0.1",
  "icons": {
    "128": "images/earth.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
