{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "iReview",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Right-click selected product names to quickly search for reviews.",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "ireview.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
