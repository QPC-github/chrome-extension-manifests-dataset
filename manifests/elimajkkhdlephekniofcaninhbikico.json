{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trab",
  "version": "1.3",
  "description": "Extend default Trab functionality.",
  "homepage_url": "http://trab.io",
  "icons": {
    "48": "trab-48.png",
    "128": "trab-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "alarms",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "trab-19.png",
      "38": "trab-38.png"
    },
    "default_title": "Trab"
  },
  "manifest_version": 2
}
