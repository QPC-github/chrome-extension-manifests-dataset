{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Data Generator",
  "description": "Create random name, job title or text data.",
  "version": "1.0.3",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery-3.1.0.min.js",
        "js/popup.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
