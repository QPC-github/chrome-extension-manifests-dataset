{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Diit readed comments",
  "version": "1.2",
  "description": "Marks read comments on diit.cz, cdr.cz",
  "icons": {
    "48": "diit-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://diit.cz/*",
        "https://cdr.cz/*"
      ],
      "js": [
        "comments.js"
      ]
    }
  ],
  "applications": {
    "gecko": {
      "id": "diitreaded@jman.cz"
    }
  }
}
