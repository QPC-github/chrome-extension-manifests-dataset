{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Totallicks",
  "version": "0.5.1",
  "manifest_version": 2,
  "description": "Find Tabs",
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://totallicks.com/*",
    "http://totallicks.com/",
    "http://*.totallicks.com/"
  ]
}
