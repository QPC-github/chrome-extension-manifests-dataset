{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Хрономер",
  "icons": {
    "48": "img/pr48.png",
    "64": "img/pr64.png",
    "128": "img/pr128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "description": "Автоматический подсчет хронометража и статистики текста. Хрономер - учитывает особенности дикторской подачи и произношение цифр.",
  "browser_action": {
    "default_icon": {
      "64": "img/pr64.png"
    },
    "default_title": "Хрономер - определение хронометража текста",
    "default_popup": "popur.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "homepage_url": "https://diktorov.net",
  "version": "1.0.2"
}
