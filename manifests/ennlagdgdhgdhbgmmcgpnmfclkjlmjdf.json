{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "D3.js Sidekick",
  "description": "Provides d3.js help docs as an extension.",
  "version": "1.1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://github.com/mbostock/d3/wiki/*",
    "storage"
  ]
}
