{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Uolla Booking Price Tracker",
  "version": "0.0.9",
  "description": "Uolla Booking Price Tracker",
  "manifest_version": 2,
  "page_action": {
    "default_title": "Uolla Booking Price Tracker",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "identity",
    "storage",
    "identity.email",
    "tabs"
  ],
  "oauth2": {
    "client_id": "34841990441-4tgsroausvs6utqn0ckf6n741mk84kfm.apps.googleusercontent.com",
    "scopes": [
      "profile"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "short_name": "Uolla Price Tracker"
}
