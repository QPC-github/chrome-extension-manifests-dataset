{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Fill Oralce SSO Credential",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Auto Fill Oralce SSO Credential",
  "permissions": [
    "tabs",
    "https://login.oracle.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://login.oracle.com/*"
      ],
      "js": [
        "sjcl.js",
        "jquery-1.7.1.min_cn.ziscloud.js",
        "auto_fill_oracle_sso.js"
      ],
      "run_at": "document_end"
    }
  ]
}
