{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19-offline.png",
      "38": "icons/38-offline.png"
    },
    "default_popup": "popup.html"
  },
  "description": "Powiadamienia o strumieniach LiveGamers.pl.",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "LiveGamers.pl",
  "permissions": [
    "background",
    "identity",
    "notifications",
    "*://*.livegamers.pl/*"
  ],
  "version": "4.0.4"
}
