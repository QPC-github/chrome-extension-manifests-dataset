{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/agreement.css"
      ],
      "js": [
        "jquery.js"
      ],
      "matches": [
        "*://grozber.com/*",
        "*://www.grozber.com/*"
      ]
    }
  ],
  "description": "Grozber - запчасти для сельхозтехники",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/128.png"
  },
  "manifest_version": 2,
  "name": "grozber",
  "page_action": {
    "default_icon": "img/48.png",
    "default_popup": "popup.html",
    "default_title": "Grozber"
  },
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "*://*/*",
    "tabs",
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.38"
}
