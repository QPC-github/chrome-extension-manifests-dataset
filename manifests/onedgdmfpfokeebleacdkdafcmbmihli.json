{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Статистика advse.ru",
  "description": "Статистика advse.ru прямо в браузере",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Статистика advse.ru",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://advse.ru/*",
    "https://advse.ru/*"
  ]
}
