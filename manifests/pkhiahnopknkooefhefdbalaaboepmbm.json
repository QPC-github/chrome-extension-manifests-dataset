{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lightweight Notepad",
  "version": "4",
  "manifest_version": 2,
  "description": "Very very lightweight notepad.",
  "browser_action": {
    "default_icon": "notepad.png",
    "default_popup": "notepad.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.3.1.min.js",
        "function.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "offline_enabled": true
}
