{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "www.luluwebmaster.fr",
  "author": "@luluwebmaster",
  "description": "Extension du blog www.luluwebmaster.fr.",
  "version": "1.0.0",
  "homepage_url": "https://www.luluwebmaster.fr/",
  "browser_action": {
    "default_icon": "imgs/favicon.png",
    "default_title": "www.luluwebmaster.fr",
    "default_popup": "main.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "https://www.luluwebmaster.fr/*",
    "https://i.ytimg.com/*",
    "https://i.luwe.fr/*",
    "notifications",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "imgs/logo.png",
    "48": "imgs/logo.png",
    "128": "imgs/logo.png"
  }
}
