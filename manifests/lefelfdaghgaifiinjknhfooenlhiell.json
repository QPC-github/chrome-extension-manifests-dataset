{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Rocky Mountain USO SpiralFunds ",
  "version": "2.0.1.0",
  "description": "Shopping with Purpose",
  "homepage_url": "http://support.spiralfunds.com/",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "jquery-1.6.2.min.js",
        "ci.content.pack.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "ci.browser.helper.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "logo-18.png",
    "default_title": "spiralfunds.com"
  },
  "icons": {
    "48": "logo-48.png"
  }
}
