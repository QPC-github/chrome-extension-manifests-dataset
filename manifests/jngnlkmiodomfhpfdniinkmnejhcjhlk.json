{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Brexit Means Breakfast",
  "version": "1.0",
  "description": "Replaces all references to Brexit to Breakfast...because Brexit means Breakfast",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
