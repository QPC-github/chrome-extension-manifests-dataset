{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Christmas Countdown and Radio",
  "short_name": "Christmas",
  "version": "0.3.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Christmas Countdown and Radio",
    "default_icon": "icon_128.png"
  },
  "description": "A beautiful Christmas Countdown. Also, a list of Christmas radio stations that you can listen to all day.",
  "offline_enabled": false,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs"
  ]
}
