{
  "name": "Cutlass: The Ad Reporter",
  "short_name": "Cutlass",
  "version": "1.0.0",
  "description": "Reports Facebook ads as spam, because that's what ads are.",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "index.js"
      ],
      "matches": [
        "https://www.facebook.com/*"
      ]
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
