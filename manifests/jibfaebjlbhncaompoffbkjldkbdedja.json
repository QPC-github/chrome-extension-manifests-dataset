{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CV from Linkedin",
  "description": "Download PDF CV from Linkedin",
  "version": "2.2",
  "page_action": {
    "default_icon": {
      "16": "images/CV.png",
      "128": "images/CVB.png"
    }
  },
  "author": "FAS",
  "icons": {
    "48": "images/CVB.png",
    "128": "images/CVB.png"
  },
  "background": {
    "scripts": [
      "pdfmake.min.js",
      "vfsfonts.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "https://linkedin.com/in/*"
  ]
}
