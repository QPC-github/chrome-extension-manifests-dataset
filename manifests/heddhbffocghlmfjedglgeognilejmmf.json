{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "link to QR"
  },
  "description": "Get the QR code of the open page to scan and open on your mobile device!",
  "developer": {
    "name": "kliraks"
  },
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "24": "img/24.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "64": "img/64.png"
  },
  "manifest_version": 2,
  "name": "link to QR",
  "permissions": [
    "tabs"
  ],
  "version": "1.2"
}
