{
  "background": {
    "page": "background.html"
  },
  "description": "Skyguard Endpoint Chrome Extension",
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*",
        "ftp://*/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "SkyGuard Endpoint Browser Helper",
  "permissions": [
    "tabs",
    "downloads",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "file:///*",
    "ftp://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.7.4"
}
