{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kids Safe Web",
  "description": "Block websites, that could harm your child.",
  "icons": {
    "16": "kidsSafeWeb16.png",
    "32": "kidsSafeWeb32.png",
    "48": "kidsSafeWeb48.png",
    "128": "kidsSafeWeb128.png"
  },
  "version": "1.0.7",
  "author": "George Zotos",
  "incognito": "split",
  "browser_action": {
    "default_icon": "kidsSafeWeb48.png",
    "default_title": "Kids Safe Web",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "categories.js"
    ],
    "persistent": false
  },
  "permissions": [
    "management",
    "storage",
    "tabs",
    "activeTab",
    "webNavigation",
    "*://www.extendbrowser.com/*"
  ]
}
