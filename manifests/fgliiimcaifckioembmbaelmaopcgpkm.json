{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GD Clock",
  "description": "Get current date time in Chrome.",
  "version": "0.2",
  "permissions": [
    "alarms"
  ],
  "background": {
    "scripts": [
      "jquery-2.0.3.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  }
}
