{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fast note",
  "description": "save a quick image or a text note",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
