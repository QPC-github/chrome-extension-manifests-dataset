{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Cache Hit or Miss",
  "version": "0.4",
  "manifest_version": 2,
  "description": "See if the loaded page was a cache HIT or MISS.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "webRequest",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
