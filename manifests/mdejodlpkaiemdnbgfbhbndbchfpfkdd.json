{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The World News",
  "version": "1.1",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "constants.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "notifications",
    "https://theworldnews.net/*",
    "https://api.2ip.ua/geo.json?ip="
  ],
  "web_accessible_resources": [
    "icon_128.png"
  ],
  "browser_action": {
    "default_title": "The World News",
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  }
}
