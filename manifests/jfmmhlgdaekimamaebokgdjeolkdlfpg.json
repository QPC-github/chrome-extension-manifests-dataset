{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ad-skipper",
  "short_name": "Ad-skipper",
  "manifest_version": 2,
  "version": "2.0.1",
  "author": "Nikhil Singh",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "skip.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  }
}
