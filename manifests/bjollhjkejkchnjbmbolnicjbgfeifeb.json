{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickLike",
  "version": "1.3",
  "description": "Easy to like,tweet and search bookmarks in your browser.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "QuickLike",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "popup.html"
  ],
  "permissions": [
    "bookmarks",
    "tabs",
    "geolocation",
    "contextMenus",
    "https://twitter.com/share",
    "http://www.facebook.com/",
    "http://connect.facebook.net/",
    "http://maps.googleapis.com/maps/api/geocode/*",
    "http://maps.google.com/maps/api/*"
  ]
}
