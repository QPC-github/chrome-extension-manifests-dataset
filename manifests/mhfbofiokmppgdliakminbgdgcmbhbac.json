{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "icon.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "tool.html"
  ],
  "name": "Web Clipper",
  "version": "1.27.0",
  "description": "Universal open source web clipper for Notion,OneNote,Joplin,Yuque,Bear,Github and more notes.",
  "permissions": [
    "activeTab",
    "storage",
    "https://api.clipper.website/*",
    "https://resource.clipper.website/*"
  ],
  "optional_permissions": [
    "cookies",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}
