{
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+R"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Resize Page by dragging to test responsive design",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Simple RWD Resizer",
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
