{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Crumble",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Auto-accepts cookie permission dialogues from various websites to bypass rules imposed by the EU Cookie Law.",
  "content_scripts": [
    {
      "matches": [
        "http://www.oracle.com/*",
        "https://www.oracle.com/*"
      ],
      "js": [
        "oracle.js"
      ],
      "run_at": "document_start"
    }
  ]
}
