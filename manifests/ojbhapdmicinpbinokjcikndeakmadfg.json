{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "extension.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Wonderwerk Screen Recording"
  },
  "description": "This extension allows to record the screen in Wonderwerk when given the permission.",
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.herokuapp.co/*",
      "*://app.wonderwerk.co/*",
      "*://wonderwerk-staging.herokuapp.com/*",
      "*://ziggeo.io/*",
      "*://ziggeo.com/*",
      "*://www-dev.ziggeo.com/*",
      "*://dev.ziggeo.io/*",
      "*://eu-west-1.ziggeo.com/*",
      "*://eu-west-1.ziggeo.io/*"
    ]
  },
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png"
  },
  "manifest_version": 2,
  "name": "Wonderwerk Screen Recording",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Wonderwerk",
  "version": "0.0.4"
}
