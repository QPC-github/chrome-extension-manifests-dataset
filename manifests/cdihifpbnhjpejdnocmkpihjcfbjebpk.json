{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "liqui",
  "description": "The fastest and simplest way to search on liquipedia",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "liqui",
    "default_icon": "icons/128.png",
    "default_popup": "popup/popup.html"
  },
  "options_page": "about/about.html",
  "omnibox": {
    "keyword": "liqui"
  },
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2
}
