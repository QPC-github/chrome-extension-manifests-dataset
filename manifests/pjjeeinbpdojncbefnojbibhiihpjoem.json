{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Neue Face",
  "description": "Makes Facebook Look Nicer",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/",
        "https://www.facebook.com/"
      ],
      "css": [
        "fbnv1.css"
      ],
      "run_at": "document_start"
    }
  ]
}
