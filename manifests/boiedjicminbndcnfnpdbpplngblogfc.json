{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "nukleus",
  "version": "1.18",
  "description": "Use nukleus to sell more stuffâ€¦ Post your listings to craigslist, faster.",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "page_action": {
    "default_icon": "images/nukleus-cl-48.png",
    "default_title": "nukleus ==>"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/contentscript.js"
      ],
      "css": [
        "css/nukleus-ext.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "icons": {
    "48": "images/nukleus-cl-48.png",
    "128": "images/nukleus-cl-128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/nukleus-cl-19.png",
    "images/nukleus-cl-48.png",
    "images/nukleus-cl-128.png",
    "images/nukleus-cl-48-hr.png",
    "images/nukleus-cl-logo.png",
    "images/nukleus-cl-logo-hr.png",
    "js/config.json",
    "html/sidebar.html",
    "html/login.html",
    "html/listings.html",
    "html/toolbar.html"
  ]
}
