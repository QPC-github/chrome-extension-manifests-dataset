{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube home to subscriptions",
  "description": "This extension changes the home page of youtube to your subscription page",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
