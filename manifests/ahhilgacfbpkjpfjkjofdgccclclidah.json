{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "propscrub",
  "description": "This extension allows developers to modify properties of elements in the DOM by scrubbing with the cursor.",
  "version": "1.0",
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "css": [
        "core.css"
      ],
      "js": [
        "core.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "proximanova-regular.ttf",
    "proximanova-light.ttf",
    "proximanova-bold.ttf",
    "save@2x.png",
    "revert@2x.png",
    "cancel@2x.png",
    "arrows@2x.png",
    "dot@2x.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "propscrub.js"
    ]
  },
  "manifest_version": 2
}
