{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide My Email",
  "version": "2",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "128": "./img/128.png",
    "48": "./img/48.png",
    "32": "./img/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "./img/128.png",
      "48": "./img/48.png",
      "32": "./img/icon.png"
    },
    "default_title": "Hide My Email",
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html"
}
