{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NoteStamp",
  "version": "1.2.1",
  "description": "Annotate Your Video Lectures!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "icons": {
    "16": "assets/logo16.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+1",
        "windows": "Alt+Shift+X"
      },
      "description": "Open"
    }
  }
}
