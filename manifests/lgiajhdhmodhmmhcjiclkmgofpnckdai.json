{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "DVhab.ru - Новости Хабаровска",
  "manifest_version": 2,
  "version": "2",
  "content_scripts": [
    {
      "matches": [
        "http://www.dvhab.ru/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "injected.js"
  ],
  "description": "DVhab.ru",
  "browser_action": {
    "default_icon": "1.png",
    "default_popup": "popup.html"
  }
}
