{
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://google.com/*",
        "http://www.google.com/*",
        "http://google.co.in/*",
        "http://www.google.co.in/*",
        "https://google.com/*",
        "https://www.google.com/*",
        "https://google.co.in/*",
        "https://www.google.co.in/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Number google search results!",
  "icons": {
    "128": "images/icon.png"
  },
  "name": "Number google search results",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1"
}
