{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CSS Bricks",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Bringing you the most recent shots from CSS Bricks.",
  "homepage_url": "http://cssbricks.com",
  "icons": {
    "16": "icons/icon16.jpg",
    "48": "icons/icon48.png",
    "128": "icons/icon128.jpg"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
