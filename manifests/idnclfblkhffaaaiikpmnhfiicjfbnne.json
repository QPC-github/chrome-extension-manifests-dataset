{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Craigslist Garage Sale Helper",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension allows you to quickly find the addresses within the garage sale section of Craigslist.",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "ys16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "ys16.png",
    "48": "ys48.png",
    "128": "ys128.png"
  }
}
