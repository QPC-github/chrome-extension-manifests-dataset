{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmarks Checker",
  "version": "1.3",
  "description": "With this extension you can batch check your bookmarks and delete the bookmarks that are no longer valid.",
  "author": "Matthias Ainsworth",
  "icons": {
    "128": "images/bookmarkschecker.png"
  },
  "browser_action": {
    "default_icon": "images/bookmarkschecker.png",
    "default_popup": "popup.html",
    "default_title": "Bookmarks Checker"
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "bookmarks"
  ]
}
