{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SearchABAPSources",
  "description": "Search a selected text in the ABAP sources.",
  "version": "1.16.10",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "searchABAP.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
