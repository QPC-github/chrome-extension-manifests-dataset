{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL Manager",
  "description": "__MSG_appdesc__",
  "version": "1.0.1",
  "default_locale": "en",
  "options_page": "options.html",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "64": "img/64.png",
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/16.png",
    "default_title": "URL Manager Chrome Extension"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "<all_urls>"
  ],
  "manifest_version": 2
}
