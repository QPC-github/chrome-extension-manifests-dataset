{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "zh_CN",
  "author": "Meng Zhuo",
  "name": "34NM",
  "description": "__MSG_mail_forwarder_description__",
  "version": "2.0.1",
  "icons": {
    "128": "icon_large.png",
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon_disable.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://34nm.com/api/",
    "alarms",
    "storage"
  ],
  "background": {
    "scripts": [
      "/js/events.js"
    ],
    "persistent": false
  }
}
