{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dribbble Demetricator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Protect your ego. Removes like, comment and view counts from Dribbble.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://dribbble.com/*",
        "https://dribbble.com/*",
        "https://www.dribbble.com/*"
      ],
      "css": [
        "dribbbledemetricator.css"
      ]
    }
  ]
}
