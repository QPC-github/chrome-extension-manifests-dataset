{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "2Day Desktop Screen Share",
  "description": "2DAY Desktop Screen Share extension for Audio and Video Conferencing.",
  "version": "0.1.6.3",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "2day-logo-16x16.png",
    "48": "2day-logo-48x48.png",
    "128": "2day-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.2day.sg/*",
      "*://2day.sg/*",
      "*://meet.learntoday.co/*"
    ]
  }
}
