{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SEO Tags",
  "description": "Finds and displays web pages' tags such as title, meta description, h1, h2, Google Analytics, and more.",
  "version": "1.1.2",
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Show Tags",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
