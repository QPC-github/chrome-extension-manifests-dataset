{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extname__",
  "description": "__MSG_extdesc__",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "storage",
    "https://*.extfans.com/",
    "http://cdn.content.prod.cms.msn.com/",
    "https://www.google.com/",
    "webRequest",
    "webRequestBlocking"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.extfans.com/*"
    ]
  },
  "icons": {
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "icons/32.png",
      "48": "icons/48.png",
      "96": "icons/96.png",
      "128": "icons/128.png"
    }
  },
  "background": {
    "page": "pages/background/bg.html"
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab/index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "20.0.7"
}
