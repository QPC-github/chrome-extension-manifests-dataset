{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ShowdMe-showdmeqa",
  "description": "This extension improves screensharing UX and security for ShowdMe.",
  "version": "0.3.1",
  "manifest_version": 2,
  "minimum_chrome_version": "50",
  "icons": {},
  "permissions": [
    "desktopCapture",
    "tabs",
    "activeTab",
    "http://localhost/*",
    "https://localhost/*",
    "https://monimus.org/*",
    "https://monimus.net/*",
    "https://monimus.me/*",
    "https://monimus.com/*",
    "https://showdme.net/*",
    "https://showdme.me/*",
    "https://*.monimus.org/*",
    "https://*.monimus.net/*",
    "https://*.monimus.me/*",
    "https://*.monimus.com/*",
    "https://*.showdme.net/*",
    "https://*.showdme.me/*",
    "http://*.mondev.com/*",
    "https://*.mondev.com/*",
    "https://monimus.local/*",
    "https://*.monimus.local/*",
    "https://showdmeqa.net/*",
    "https://showdmeqa.me/*",
    "https://*.showdmeqa.me/*",
    "https://*.showdmeqa.net/*",
    "https://showdmedev.net/*",
    "https://*.showdmedev.net/*",
    "https://showdmestag.net/*",
    "https://*.showdmestag.net/*",
    "https://showdmetest.net/*",
    "https://*.showdmetest.net/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "https://localhost/*",
      "https://monimus.org/*",
      "https://monimus.net/*",
      "https://monimus.me/*",
      "https://monimus.com/*",
      "https://showdme.net/*",
      "https://*.showdme.net/*",
      "https://showdme.me/*",
      "https://*.monimus.org/*",
      "https://*.monimus.net/*",
      "https://*.monimus.me/*",
      "https://*.monimus.com/*",
      "https://*.showdme.net/*",
      "https://*.showdme.me/*",
      "http://*.mondev.com/*",
      "https://*.mondev.com/*",
      "https://*.monimus.local/*",
      "https://monimus.local/*",
      "https://*.showdmestag.net/*",
      "https://showdmeqa.net/*",
      "https://showdmeqa.me/*",
      "https://*.showdmeqa.me/*",
      "https://*.showdmeqa.net/*",
      "https://showdmetest.net/*",
      "https://*.showdmetest.net/*"
    ]
  }
}
