{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Sites",
  "description": "Quick Launch Shortcuts for your Sites",
  "author": "Pramod Devireddy",
  "version": "1.0.1",
  "icons": {
    "128": "icon_128x128.png"
  },
  "browser_action": {
    "default_icon": "icon_128x128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
