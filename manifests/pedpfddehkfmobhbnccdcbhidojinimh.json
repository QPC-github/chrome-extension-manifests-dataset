{
  "background": {
    "scripts": [
      "adguard/adguard-api.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_128.png",
    "default_popup": "popup.html",
    "default_title": "Video Adblocker - Block Pre Roll Ad"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "Video AdBlocker - the Master Adblock for video ads. Block Pre-Roll ads on Youtube, FB, Vimeo and other",
  "icons": {
    "256": "img/icon_256.png"
  },
  "manifest_version": 2,
  "name": "Video Adblocker - Block Pre Roll Ad",
  "permissions": [
    "webRequest",
    "<all_urls>",
    "storage",
    "tabs",
    "activeTab",
    "webRequestBlocking",
    "webNavigation",
    "management"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
