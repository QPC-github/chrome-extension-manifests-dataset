{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Managify Add Bookmark",
  "description": "Quickly bookmark websites using this extension.",
  "short_name": "add-Bookmark",
  "version": "1.3",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/create-bookmark-app.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "tabs"
  ],
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
