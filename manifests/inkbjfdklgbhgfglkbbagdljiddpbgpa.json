{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Powiadamiacz Yuuhiego",
  "version": "0.1.0",
  "description": "Powiadamianie o streamie Yuuhiego",
  "homepage_url": "https://bitbucket.org/kallosz/yuuhi-chrome-ext/issues",
  "manifest_version": 2,
  "minimum_chrome_version": "46",
  "icons": {
    "128": "images/icon-128.png"
  },
  "permissions": [
    "alarms",
    "notifications",
    "https://api.hitbox.tv/*",
    "https://yuuhi.kinteractive.pl/*"
  ],
  "optional_permissions": [
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "config.js",
      "api.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "images/icon-38.png"
    }
  }
}
