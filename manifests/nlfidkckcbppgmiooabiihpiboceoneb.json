{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0.2",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "minimum_chrome_version": "35",
  "default_locale": "ru",
  "icons": {
    "19": "icons/rambler19.png",
    "48": "icons/rambler48.png",
    "128": "icons/rambler128.png"
  },
  "homepage_url": "http://r0.ru/?_openstat=ZXh0X3NlYXJjaF9jaHJvbWU7Ozs",
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "rambler.ru",
      "name": "__MSG_searchName__",
      "is_default": true,
      "encoding": "UTF-8",
      "favicon_url": "http://nova.rambler.ru/static/search/images/favicon.ico",
      "image_url": "http://images.rambler.ru/search?query={searchTerms}&_openstat=ZXh0X3NlYXJjaF9jaHJvbWU7Ozs",
      "search_url": "http://nova.rambler.ru/search?query={searchTerms}&_openstat=ZXh0X3NlYXJjaF9jaHJvbWU7Ozs",
      "suggest_url": "http://nova.rambler.ru/suggest?v=3&query={searchTerms}&_openstat=ZXh0X3NlYXJjaF9jaHJvbWU7Ozs"
    }
  }
}
