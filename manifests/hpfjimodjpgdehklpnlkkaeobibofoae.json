{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Copalive",
  "description": "Partidos en tiempo real",
  "version": "1.0",
  "icons": {
    "16": "icon_128.png",
    "32": "icon_128.png",
    "48": "icon_128.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.copalive.com/"
  ],
  "content_scripts": [
    {
      "js": [
        "parse-1.2.18.min.js",
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Partidos en tiempo real",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
