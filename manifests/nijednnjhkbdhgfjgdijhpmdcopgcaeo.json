{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ken M Paster",
  "version": "1.0.2",
  "description": "Copies Ken M quotes to your clipboard",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Ken M"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "clipboardWrite"
  ],
  "commands": {
    "jaden": {
      "suggested_key": {
        "default": "Ctrl+Shift+2"
      },
      "description": "Ken M Paster Keyboard Shortcut",
      "global": true
    }
  }
}
