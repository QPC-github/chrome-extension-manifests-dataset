{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Longdesc",
  "description": "Highlights and provides right-click (or Ctrl-click on Mac) access to image long descriptions, where provided",
  "version": "0.0.8",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "prep.js"
    ]
  },
  "manifest_version": 2
}
