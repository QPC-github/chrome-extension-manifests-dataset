{
  "author": "WebGameAPP",
  "name": "Final Fantasy HD Wallpapers Tab Theme",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "default_locale": "en",
  "version": "4.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Final Fantasy HD Wallpapers Tab Theme",
    "default_icon": "128.png",
    "default_popup": "gametab.html"
  },
  "permissions": [
    "gcm",
    "storage",
    "tabs"
  ],
  "icons": {
    "128": "128.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
