{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Atlassian UN-WAT",
  "description": "UN-WAT Atlassian external links",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.atlassian.net/*"
      ],
      "all_frames": true,
      "js": [
        "unwat.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {},
  "permissions": [
    "activeTab"
  ]
}
