{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BenoSupport Quick Support",
  "description": "Get Tech Support fast and easy right within Google Chrome.",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "/img/icons/bs-app_16x16.png",
    "128": "/img/icons/bs-app_128x128.png"
  },
  "browser_action": {
    "default_icon": "/img/icons/bs-app_16x16.png",
    "default_popup": "window.html",
    "default_title": "Launch BenoSupport App"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*"
  ]
}
