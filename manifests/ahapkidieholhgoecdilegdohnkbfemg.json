{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IDK Shuffle",
  "version": "1.0",
  "description": "Generate alternate phrases for 'I don't know'!",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/idk16.png",
      "32": "images/idk32.png",
      "48": "images/idk48.png",
      "128": "images/idk128.png"
    }
  },
  "icons": {
    "16": "images/idk16.png",
    "32": "images/idk32.png",
    "48": "images/idk48.png",
    "128": "images/idk128.png"
  }
}
