{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AirFind DT Extension",
  "short_name": "AirFind DT",
  "description": "Replaces new tab with personal dashboard with a beautiful background, the current date, news articles and more",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com/; object-src 'self'",
  "version": "0.0.2",
  "author": "AirFind",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "/assets/images/Chrome_Extension.png",
    "default_title": "New tab"
  },
  "permissions": [
    "activeTab"
  ]
}
