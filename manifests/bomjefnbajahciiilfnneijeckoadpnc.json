{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "TEL WebRTC Desktop Sharing Extension",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Desktop sharing for TEL for WebRTC",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow desktop sharing in TEL for WebRTC applications.",
  "externally_connectable": {
    "matches": [
      "*://192.168.76.105/*",
      "*://59.160.207.105/*",
      "*://104.198.107.50/*",
      "*://54.215.220.152/*"
    ]
  },
  "icons": {
    "16": "woogeen_icon_16.png"
  },
  "browser_action": {
    "default_icon": "woogeen_icon_16.png"
  }
}
