{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Begone, Chikattsu!"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "Un-chikattsu-ifier",
  "permissions": [
    "http://*.google.com/",
    "http://*.com/",
    "tabs"
  ],
  "version": "1.0"
}
