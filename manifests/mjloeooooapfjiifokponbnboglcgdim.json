{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "deblurr",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Disables anti-aliasing.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "cs.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
