{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KDV HESAPLAMA",
  "version": "1.1",
  "manifest_version": 2,
  "description": "KDV Hesaplama %18 Türkiye vergi hesaplamaları. KDV dahil veya KDVsiz tutarı kolayca hesaplayın.",
