{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Flow Of Time",
  "description": "Allows user to customize their New Tab Page",
  "version": "1.2",
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {
    "16": "Hourglass-icon.png",
    "48": "Hourglass-icon.png",
    "128": "icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
