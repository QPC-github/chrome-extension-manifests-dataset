{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Smart Pause",
  "description": "Smart Pause pauses your video on YouTube, Vimeo and Netflix.",
  "version": "1.0.3",
  "icons": {
    "48": "stop.png"
  },
  "browser_action": {
    "default_icon": "stop.png"
  },
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://fouad.co",
  "permissions": [
    "*://*/*",
    "tabs",
    "videoCapture"
  ]
}
