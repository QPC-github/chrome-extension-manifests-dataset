{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "El Comercio",
  "short_name": "GEC",
  "signature": "GEC",
  "author": "Pablo Reyes",
  "version": "1.2",
  "description": "El Comercio - Noticias del Ecuador y del mundo.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
