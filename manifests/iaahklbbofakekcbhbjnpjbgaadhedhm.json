{
  "browser_action": {
    "default_icon": {
      "16": "src/icon/icon16.png",
      "19": "src/icon/icon19.png",
      "32": "src/icon/icon32.png",
      "38": "src/icon/icon38.png",
      "48": "src/icon/icon48.png"
    },
    "default_popup": "popup.html",
    "default_title": "Settings"
  },
  "content_scripts": [
    {
      "js": [
        "content.js",
        "jquery-3.4.1.js"
      ],
      "matches": [
        "https://beta.tanktrouble.com/*",
        "https://test-cdn-beta.tanktrouble.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "author": "CommanderAnime",
  "description": "TankTroubleAddons - the go-to extension for all your mayhem needs!",
  "icons": {
    "128": "src/icon/icon128.png",
    "16": "src/icon/icon16.png",
    "32": "src/icon/icon32.png",
    "48": "src/icon/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "TankTroubleAddons",
  "permissions": [
    "management",
    "activeTab",
    "storage"
  ],
  "short_name": "TankTroubleAddons",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.4.7",
  "web_accessible_resources": [
    "styles.css",
    "misc_styles.css",
    "/images/*"
  ]
}
