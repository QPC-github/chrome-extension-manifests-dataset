{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "New Tab Plus",
  "description": "add a new specific tab",
  "version": "1.0.1",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "New Specific Tab",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2
}
