{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "manifest_version": 2,
  "version": "2",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/16.png",
    "default_popup": "popup.html"
  }
}
