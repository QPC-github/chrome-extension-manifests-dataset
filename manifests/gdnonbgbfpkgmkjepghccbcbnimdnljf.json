{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "64": "images/icon2.png"
  },
  "browser_action": {
    "default_icon": "images/icon2.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "activeTab",
    "http://scrapy-spider-reviews-dev2.us-west-2.elasticbeanstalk.com/*"
  ],
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "images/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "name": "Word Cloud Amazon Review",
  "version": "0.4.7",
  "description": "The fastest way to see reviews of Amazon products. Improve your productivity and save your time."
}
