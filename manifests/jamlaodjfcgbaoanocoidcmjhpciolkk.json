{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One2Lead - LinkedIn Automation Tool",
  "short_name": "One2Lead",
  "description": "The easiest way for lead generation using LinkedIn. Find, reach and engage with thousands of potential customers in one click.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "image/64.png"
  },
  "icons": {
    "16": "image/16.png",
    "32": "image/32.png",
    "48": "image/48.png",
    "64": "image/64.png",
    "128": "image/128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://linkedin.com/*",
        "https://www.linkedin.com/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "identity",
    "https://linkedin.com/*",
    "https://www.linkedin.com/*"
  ],
  "incognito": "not_allowed",
  "externally_connectable": {
    "matches": [
      "*://linkedin.com/*",
      "*://www.linkedin.com/*"
    ]
  },
  "minimum_chrome_version": "71.0",
  "version": "1.6.72",
  "key": ""
}
