{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "Plugin Data Cleaner",
  "description": "Delete all installed plugin data",
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Delete all installed plugin data"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "browsingData",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
    "PluginData": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "Plugin Data Cleaner"
    }
  },
  "version": "0.1",
  "manifest_version": 2
}
