{
  "name": "Jobsterz Search Extension",
  "options_page": "popup.html",
  "description": "Complete Recruitment Solutions| Consultancy services in Delhi, Noida, Mumbai, Pune, Bangalore| Jobsterz",
  "manifest_version": 2,
  "author": "Jobsterz",
  "version": "1",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": "background.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://www.jobsterz.com/#s=1",
  "incognito": "split",
  "input_components": "chrome.storage.sync",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "Jobsterz"
  },
  "permissions": [
    "notifications",
    "webRequest",
    "*://*.jobsterz.com/"
  ]
}
