{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "bg.html"
  },
  "browser_action": {
    "default_icon": "48.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "bg.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "It's now even easier to upload images to Fractyl! Simply right click and image or video and click 'Upload to Fractyl'",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "19": "19.png",
    "192": "192.png",
    "24": "24.png",
    "32": "32.png",
    "38": "38.png",
    "48": "48.png",
    "72": "72.png"
  },
  "manifest_version": 2,
  "name": "Fractyl extension",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "short_name": "Fractyl",
  "version": "1.5.1"
}
