{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sucuri Sitecheck",
  "description": "Scan any website for security issues, blacklisting, and malware with Sucuri SiteCheck",
  "version": "1.0.11",
  "background": {
    "scripts": [
      "js/background.js",
      "js/unserialize.js"
    ]
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": "images/s_working.png"
  },
  "icons": {
    "16": "images/s_working.png",
    "48": "images/s_48.png",
    "128": "images/s_128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://sitecheck.sucuri.net/",
    "*://restdns.net/"
  ],
  "manifest_version": 2
}
