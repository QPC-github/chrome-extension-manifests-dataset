{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iChat Intelizign",
  "short_name": "iChat",
  "description": "A simple extension that allows you to share your desktop with iChat Videoconferencing - Intelizign",
  "version": "0.0.11",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "iChat_16x16.png",
    "48": "iChat_48X48.png",
    "128": "iChat_128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://ichat-vcp.intelizi.com/*",
      "*://ichat-vc.intelizi.com/*",
      "*://10.0.0.112/*",
      "*://10.0.0.179/*",
      "*://192.168.6.195/*",
      "*://talkexdvc.intelizi.com/*"
    ]
  }
}
