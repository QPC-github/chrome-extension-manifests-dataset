{
  "content_scripts": [
    {
      "js": [
        "lib/jquery-1.9.1.min.js",
        "main.js"
      ],
      "matches": [
        "https://mail.google.com/*",
        "http://mail.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Play attached Audio in Gmail™ and Google Apps™. for Voicemail",
  "homepage_url": "http://minmax.at",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "Voicemail Audioplayer for Gmail™",
  "permissions": [
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://www.emoods.de/log/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.22",
  "web_accessible_resources": [
    "lib/jquery-1.9.1.min.js",
    "main.js"
  ]
}
