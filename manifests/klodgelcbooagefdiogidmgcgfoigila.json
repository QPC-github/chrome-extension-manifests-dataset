{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "IPIM Extension",
  "version": "1.14",
  "description": "IPIM Extension (from IPIM.me http://ipim.me) helps you collect, manage information, take screenshots, bookmarking websites, etc.",
  "icons": {
    "16": "ipim_chrome_16.png",
    "48": "ipim_chrome_48.png",
    "128": "ipim_chrome_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ipim_chrome_24.png",
    "default_title": "Take a screen shot!"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "unlimitedStorage",
    "cookies",
    "webRequest",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
