{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Electra",
  "description": "This extension will give you details about Electra Currency",
  "version": "1.7",
  "browser_action": {
    "default_icon": "/img/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
