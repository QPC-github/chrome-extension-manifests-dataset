{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "closekiosk.js"
    ]
  },
  "description": "A Google Chrome Extension to automatically exit Kiosk mode when any window navigates to *exitkiosk*.",
  "icons": {
    "128": "closekiosk_128.png",
    "16": "closekiosk_16.png",
    "48": "closekiosk_48.png"
  },
  "manifest_version": 2,
  "name": "Exit Kiosk",
  "permissions": [
    "tabs"
  ],
  "version": "1.0.0"
}
