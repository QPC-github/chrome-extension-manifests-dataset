{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VoatSuite",
  "version": "1.1",
  "description": "Makes voat a little bit easier.",
  "author": "wernersbacher",
  "homepage_url": "https://m.wernersbacher.de/",
  "content_scripts": [
    {
      "matches": [
        "*://*.voat.co/v/*/comments/*"
      ],
      "js": [
        "jquery.js",
        "inject.js"
      ],
      "css": [
        "inject.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
