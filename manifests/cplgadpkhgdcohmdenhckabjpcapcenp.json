{
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "js": [
        "checkinstall.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "html/popup.html",
    "default_title": "You are Unique!"
  },
  "description": "always motivates you with inspiring words",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "Smiley Motivation",
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.4"
}
