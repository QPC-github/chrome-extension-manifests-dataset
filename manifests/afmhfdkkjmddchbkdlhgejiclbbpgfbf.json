{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "vs_content.js"
      ],
      "matches": [
        "*://vsco.co/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Opens highest possible resolution VSCO images in a new tab!",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "name": "VSCO Viewer",
  "permissions": [
    "tabs",
    "*://vsco.co/*"
  ],
  "version": "1.2.1"
}
