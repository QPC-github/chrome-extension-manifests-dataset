{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Make The Internet Great Again",
  "description": "2016 Election Twitter Filter Chrome Extension.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "twitter_filter.js"
      ],
      "run_at": "document_start"
    }
  ]
}
