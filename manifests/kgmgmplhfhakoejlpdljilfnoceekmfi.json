{
  "browser_action": {
    "default_popup": "pages/options.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-2.1.4.min.js",
        "js.js"
      ],
      "matches": [
        "https://hilanet.co.il/Hilannetv2/Attendance/calendarpage.aspx*",
        "https://*.net.hilan.co.il/Hilannetv2/Attendance/calendarpage.aspx*"
      ]
    }
  ],
  "description": "Extras for Hilanet",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Hilanet Helper",
  "options_page": "pages/options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
