{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "News Mess Reader",
  "description": "Cleans up styles, making it easier to read text.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.stwnewspress.com/*",
        "http://www.stwnewspress.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "About this messy reader."
  }
}
