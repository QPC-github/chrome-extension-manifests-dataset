{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My Dark Web",
  "version": "0.1",
  "description": "Darkens light pages I like.",
  "content_scripts": [
    {
      "css": [
        "goog.css"
      ],
      "matches": [
        "https://*.google.com/search"
      ]
    },
    {
      "css": [
        "goog-android.css"
      ],
      "matches": [
        "https://developer.android.com/reference/*",
        "https://developer.android.com/guide/*"
      ]
    },
    {
      "css": [
        "goog-docs.css"
      ],
      "matches": [
        "https://docs.google.com/document/d/*/edit"
      ]
    },
    {
      "css": [
        "stackoverflow.css"
      ],
      "matches": [
        "https://stackoverflow.com/questions/*"
      ]
    },
    {
      "css": [
        "javadoc.css"
      ],
      "matches": [
        "*://*/javadoc/*"
      ]
    }
  ]
}
