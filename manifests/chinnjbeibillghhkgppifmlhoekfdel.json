{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Newtab",
  "version": "1.3",
  "author": "Eray Chumak",
  "description": "Provides a clean new tab interface with no distractions.",
  "short_name": "nt",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "./page/index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Provides a clean new tab interface with no distractions.",
    "default_popup": "./popup/index.html",
    "default_icon": {
      "128": "./assets/logo_128.png"
    }
  },
  "icons": {
    "128": "./assets/logo_128.png"
  }
}
