{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Calcify",
  "description": "Page friendly Calculator",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "Calcify",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  }
}
