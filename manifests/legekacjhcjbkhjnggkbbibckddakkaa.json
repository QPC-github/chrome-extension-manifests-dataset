    "default_title": "__MSG_ext_name__",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "notifications",
    "tabs",
    "storage"
  ]
}
