{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "T-speak",
  "description": "Simple extension that allows you to stream your desktop into meetings with Jitsi Meet and Jitsi Videobridge (T-speak).",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo-teleco-16x16.png",
    "48": "logo-teleco-48x48.png",
    "128": "logo-teleco-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://t-speak.teleco.it/*"
    ]
  }
}
