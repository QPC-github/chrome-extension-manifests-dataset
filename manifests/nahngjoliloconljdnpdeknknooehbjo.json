{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Callisto space simulator",
  "short_name": "Callisto",
  "manifest_version": 2,
  "description": "Newtonian non-relativistic current technology real-time space simulator with visual and instrumental flight",
  "version": "1.0.83",
  "offline_enabled": true,
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "chromebook_background.js"
      ]
    }
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "https://ghost.sk/"
  ]
}
