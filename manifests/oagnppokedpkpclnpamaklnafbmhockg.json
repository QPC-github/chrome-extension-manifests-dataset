{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quote notifier",
  "version": "2.1.9",
  "manifest_version": 2,
  "description": "Regularly shows inspiring quotes in a small popup. Very easy and configurable.",
  "icons": {
    "16": "open_book.png",
    "48": "open_book.png",
    "128": "open_book.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://quotes4all.net/*"
  ],
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "open_book.png",
    "notification.htm"
  ]
}
