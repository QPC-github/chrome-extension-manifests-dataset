{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DelMe",
  "description": "This extension will clear your history!",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "bk.png"
  },
  "permissions": [
    "notifications",
    "tabs",
    "storage",
    "browsingData"
  ],
  "manifest_version": 2
}
