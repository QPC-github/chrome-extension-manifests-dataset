{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FB - No questions, please.",
  "description": "Removes the 'Complete Your Profile' box from facebook.",
  "version": "0.2.4",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "https://www.facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "domready.js",
        "main.js"
      ]
    }
  ]
}
