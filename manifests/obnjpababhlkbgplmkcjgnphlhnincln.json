{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/browser_action_16x16.png",
    "default_title": "Maneesh Tab Productivity",
    "default_popup": "options.html"
  },
  "icons": {
    "16": "images/browser_action_16x16.png",
    "48": "images/browser_action_48x48.png",
    "128": "images/logo_128x128.png"
  },
  "description": "Stay Focused. Eliminate Distraction.",
  "name": "Maneesh Chrome Extension",
  "permissions": [
    "tabs",
    "idle",
    "activeTab",
    "https://pavlok.herokuapp.com/",
    "http://localhost:3000/",
    "http://*.facebook.com/*",
    "cookies",
    "identity"
  ],
  "version": "0.3.8",
  "manifest_version": 2
}
