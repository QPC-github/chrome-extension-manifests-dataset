{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HTML Parser",
  "description": "Will convert your HTML code into XML coding compatible with the Blogger templates",
  "version": "1.1",
  "icons": {
    "128": "128.png"
  },
  "app": {
    "urls": [
      "http://htmlparse.com/"
    ],
    "launch": {
      "web_url": "http://www.htmlparse.com/?ref=cws"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
