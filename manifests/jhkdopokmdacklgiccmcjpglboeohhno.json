{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wish Wrapped",
  "description": "This extension allows you to add products to Wish Wrapped",
  "version": "3.2.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Wish Wrapped"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  }
}
