{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gmail Refresh No Border",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Removes the border from the GMail refresh button",
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/mail/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
