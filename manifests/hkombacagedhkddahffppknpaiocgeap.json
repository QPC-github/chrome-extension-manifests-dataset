{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cryonics",
  "description": "This extension saves all your opened tabs and resuscitates them later on any computer.",
  "version": "1.1.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
