{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gerador de CPF e CNPJ",
  "version": "1.5.1",
  "manifest_version": 2,
  "description": "Gerar CPF/CNPJ para auxiliar os desenvolvedores em testes de sistemas. O usuário e responsável pela uso das informações.",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "offline_enabled": true,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-context-menu.png",
    "128": "icon-large.png"
  },
  "permissions": [
    "background",
    "contextMenus",
    "tabs",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Gerador de CPF e CNPJ",
    "default_popup": "popup.html"
  }
}
