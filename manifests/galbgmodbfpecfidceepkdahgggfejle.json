{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/lib/jquery-3.0.0.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/icons/16_messenger.png",
    "default_title": "Solaborate Scheduler",
    "name": "Solaborate Scheduler"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "This extension allows you to reach your own Solaborate virtual call room quickly â€“ making productive meetings closer than ever.",
  "icons": {
    "128": "/icons/128_messenger.png",
    "16": "/icons/16_messenger.png",
    "48": "/icons/48_messenger.png"
  },
  "manifest_version": 2,
  "name": "Solaborate Scheduler",
  "permissions": [
    "cookies",
    "activeTab",
    "https://*.solaborate.com/*",
    "*://*.google.com/calendar/*"
  ],
  "version": "2.0.4",
  "web_accessible_resources": [
    "/html/sol_conference_buttons_v1.html",
    "/html/sol_conference_buttons_v2.html",
    "/js/conference.js"
  ]
}
