{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SpeedUp",
  "description": "Share easily",
  "version": "2.1",
  "browser_action": {
    "default_icon": "icon.ico",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "storage",
    "activeTab"
  ]
}
