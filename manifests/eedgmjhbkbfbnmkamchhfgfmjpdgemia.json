{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "description": "Unlimited QR Code/Bar Code Reader.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "QR Code And Bar Code generator",
  "permissions": [
    "cookies",
    "*://*.ankit.snvdev.website/*",
    "https://*.snvdev.website//*",
    "storage",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "version": "1.1"
}
