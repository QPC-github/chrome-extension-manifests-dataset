{
  "background": {
    "scripts": [
      "vendor/assets/javascripts/jquery-2.1.0.min.js",
      "assets/javascripts/wildfireConsole.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/images/FirePHP_19.png",
    "default_title": ""
  },
  "content_scripts": [
    {
      "js": [
        "vendor/assets/javascripts/jquery-2.1.0.min.js",
        "vendor/assets/javascripts/jquery.bpopup.min.js",
        "assets/javascripts/logging.js"
      ],
      "css": [
        "assets/styles/content.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "FirePHP extension for Google Chrome",
  "icons": {
    "128": "assets/images/FirePHP_128.png",
    "16": "assets/images/FirePHP_16.png",
    "24": "assets/images/FirePHP_24.png",
    "32": "assets/images/FirePHP_32.png",
    "48": "assets/images/FirePHP_48.png"
  },
  "web_accessible_resources": [
    "assets/images/*"
  ],
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "17",
  "name": "FirePHPc",
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.6"
}
