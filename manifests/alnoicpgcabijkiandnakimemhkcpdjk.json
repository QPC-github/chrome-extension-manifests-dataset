{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lattice",
  "version": "2.0.4",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "http://localhost:5100/",
    "http://localhost:5000/"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "myapp.js",
        "jquery.min.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "128": "icons/128x128.png",
    "144": "icons/144x144.png",
    "152": "icons/152x152.png"
  },
  "web_accessible_resources": [
    "icons/*.png"
  ],
  "browser_action": {
    "default_icon": "icons/32x32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
