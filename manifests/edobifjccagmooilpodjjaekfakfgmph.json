{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DEVIL FAST BOOKMARK",
  "description": "This extension is intended to manuplate bookmarks quickly with a keyboard only.",
  "version": "3.1",
  "background": {
    "scripts": [
      "src/js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon/icon_16.png"
    },
    "default_title": "DEVIL FAST BOOKMARK",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "https://www.google.com/*"
  ]
}
