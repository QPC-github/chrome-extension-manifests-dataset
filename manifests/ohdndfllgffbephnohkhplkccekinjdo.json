{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LogicAppToSVG",
  "manifest_version": 2,
  "version": "0.1.1",
  "description": "Downloads a SVG version of Logic App workflow.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons_icon128.png"
  },
  "icons": {
    "38": "icons_icon38.png",
    "128": "icons_icon128.png",
    "256": "icons_icon256.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
