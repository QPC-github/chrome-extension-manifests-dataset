{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Happy Day New Tab",
  "version": "0.3",
  "description": "Happy New Tab with RAM Usage Action Button",
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "browser_action": {
    "default_title": "Current RAM Usage",
    "default_icon": "icon.png"
  },
  "permissions": [
    "system.memory"
  ],
  "short_name": "Happy Tab",
  "version_name": "Initial",
  "background": {
    "scripts": [
      "happyTab.js"
    ],
    "persistent": false
  }
}
