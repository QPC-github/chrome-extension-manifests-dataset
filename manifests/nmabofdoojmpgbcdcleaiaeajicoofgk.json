{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LivePhish No Spoilers",
  "description": "Hide Setlist and Spoilers from LivePhish app",
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Hide Spoilers",
    "default_icon": "phish_16x16.png"
  }
}
