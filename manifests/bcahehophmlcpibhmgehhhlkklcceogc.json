{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Olddit",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Make reddit usable again",
  "homepage_url": "http://raghavsood.com/olddit",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "1024": "icons/icon1024.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://www.reddit.com/"
  ]
}
