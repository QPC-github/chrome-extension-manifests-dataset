{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MicroHealth Meetings Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with MicroHealth Meetings.",
  "version": "1.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "watermark.png",
    "48": "watermark.png",
    "128": "watermark.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.microhealthllc.com/*"
    ]
  }
}
