{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "zoom.js"
      ],
      "css": [
        "css/zoom.css"
      ],
      "matches": [
        "*://twitter.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Allows for zooming of images on twitter®",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Zoom for Twitter®",
  "short_name": "Zoom",
  "version": "2.0.0.1"
}
