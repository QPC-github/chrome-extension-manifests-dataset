{
  "author": "Vukadin Njegos",
  "content_scripts": [
    {
      "css": [
        "fancybox/source/jquery.fancybox.css",
        "css/style.css"
      ],
      "js": [
        "js/jquery.js",
        "fancybox/source/jquery.fancybox.js",
        "js/script.js"
      ],
      "matches": [
        "http://rs.titlovi.com/*",
        "http://titlovi.com/*",
        "http://ba.titlovi.com/*",
        "http://mk.titlovi.com/*",
        "http://si.titlovi.com/*",
        "http://en.titlovi.com/*"
      ]
    }
  ],
  "description": "Extends titlovi.com functionality, adding direct download link and IMDB link and rating on listing.",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "71": "images/icon71.png"
  },
  "manifest_version": 2,
  "name": "Titlovi.com Plus",
  "permissions": [
    "http://www.imdb.com/",
    "https://www.youtube.com/",
    "http://titlovi.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.4",
  "web_accessible_resources": [
    "fancybox/source/fancybox_overlay.png",
    "fancybox/source/fancybox_sprite.png",
    "fancybox/source/fancybox_loading.gif",
    "fancybox/source/fancybox_sprite@2x.png",
    "fancybox/source/fancybox_loading@2x.gif"
  ]
}
