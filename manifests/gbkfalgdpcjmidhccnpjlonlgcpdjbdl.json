{
  "author": "OÄŸuzhan Atabek",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "GScholar19.png",
      "38": "GScholar38.png"
    },
    "default_title": "Right-Click Search Google Scholar"
  },
  "icons": {
    "16": "GScholar16.png",
    "32": "GScholar32.png",
    "48": "GScholar48.png",
    "128": "GScholar128.png"
  },
  "manifest_version": 2,
  "name": "Right-Click Search Google Scholar",
  "short_name": "RCS Google Scholar",
  "description": "This extension searches for selected text on Google Scholar via right-click and shows the result on a new 'passive' tab",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
