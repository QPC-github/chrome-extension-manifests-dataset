{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Aeons",
  "version": "0.0.13",
  "description": "3D organisms made from your data. Find out what the web knows about you. From your mood to your personality to who you sleep with.",
  "permissions": [
    "unlimitedStorage",
    "activeTab",
    "tabs",
    "storage",
    "geolocation",
    "notifications"
  ],
  "background": {
    "scripts": [
      "analyse_text.js",
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/aeon_logo16.png",
    "32": "icons/aeon_logo32.png",
    "48": "icons/aeon_logo48.png",
    "128": "icons/aeon_logo128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
