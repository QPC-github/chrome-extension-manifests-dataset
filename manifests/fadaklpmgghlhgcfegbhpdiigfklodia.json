{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "toolbar.ico",
    "name": "Zynga Tools"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "include_globs": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "features.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "extension.jpg"
  },
  "manifest_version": 2,
  "name": "ZTools V1.1",
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "contextMenus",
    "notifications",
    "storage"
  ],
  "short_name": "ZT 1.1",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
