{
  "name": "Boomajoom",
  "version": "0.1.1",
  "description": "Save links to Boomajoom.",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "homepage_url": "https://www.boomajoom.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "cookies",
    "*://*.boomajoom.com/"
  ],
  "browser_action": {},
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  }
}
