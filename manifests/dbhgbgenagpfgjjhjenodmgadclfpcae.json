{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "gist.io",
  "version": "0.0.1",
  "description": "Toggle between github.com and gist.io when viewing gists",
  "icons": {
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "img/19.png",
      "38": "img/38.png"
    },
    "default_title": "View this on gist.io"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
