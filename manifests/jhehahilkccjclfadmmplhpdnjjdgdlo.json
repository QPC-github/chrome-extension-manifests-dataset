{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/football-128.png",
    "default_title": "StudyCity (Play Time)"
  },
  "description": "Blocks sites until progress is made studying",
  "icons": {
    "16": "icons/graduate-16.png",
    "48": "icons/graduate-48.png",
    "128": "icons/graduate-128.png"
  },
  "manifest_version": 2,
  "name": "StudyCity",
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "tabs",
    "<all_urls>",
    "identity",
    "identity.email",
    "storage",
    "unlimitedStorage",
    "idle"
  ],
  "version": "1.8.7",
  "web_accessible_resources": [
    "icons/graduate-full.png",
    "icons/break_full.png"
  ]
}
