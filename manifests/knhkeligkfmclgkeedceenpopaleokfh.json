{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tippin Extension (tippin.me)",
  "short_name": "Tippin",
  "key": "",
  "version": "1.0.3",
  "description": "Send and receive Bitcoin tips to Twitter users!",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "64": "icon.png",
      "128": "icon-128.png"
    }
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://tippin.me/*"
      ],
      "js": [
        "jquery.js",
        "content-tippin.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": [
        "jquery.js",
        "content-tippin.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "jquery.js",
        "content-twitter.js"
      ],
      "css": [
        "style001.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://mobile.twitter.com/*"
      ],
      "js": [
        "jquery.js",
        "content-twitter.js"
      ],
      "css": [
        "style001.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "js": [
        "jquery.js",
        "content-tweetdeck.js"
      ],
      "css": [
        "style001.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://twitter.com/*",
    "https://tweetdeck.twitter.com/*",
    "https://api.tippin.me/*",
    "storage"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ]
}
