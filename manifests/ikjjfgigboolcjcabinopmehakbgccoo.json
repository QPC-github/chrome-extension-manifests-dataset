{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "String Calculator",
  "description": "Type 'sc' plus your expression to open String Calculator in new tab.",
  "version": "2.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "omnibox": {
    "keyword": "sc"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "newtab_search16.png",
      "32": "newtab_search32.png"
    }
  },
  "icons": {
    "16": "newtab_search16.png",
    "32": "newtab_search32.png",
    "48": "newtab_search48.png",
    "128": "newtab_search128.png"
  }
}
