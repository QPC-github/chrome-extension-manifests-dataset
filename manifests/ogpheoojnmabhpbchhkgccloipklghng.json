{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Muted Posts",
  "description": "Hide muted posts.",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": [
        "*://plus.google.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
