{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SFChat Fixer",
  "version": "1.0.0",
  "description": "Fixes some common issues with the SF chat system. With love, ProfessorPasserine",
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.sofurry.com/chat"
      ],
      "js": [
        "fixer.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "highlight_override.js",
    "cache_buster.js"
  ]
}
