{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NewsCracker",
  "description": "Fake News Checker",
  "version": "3.0.1",
  "short_name": "newscracker",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Rate article."
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "<all_urls>",
    "webRequest",
    "http://newscracker-mac-1.us-west-2.elasticbeanstalk.com/"
  ]
}
