{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PrintWhatYouLike",
  "version": "1.5",
  "manifest_version": 2,
  "description": "PrintWhatYouLike lets you print the good parts of any web page while skipping ads and other junk.",
  "icons": {
    "16": "icon16.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "pwyl_bookmarklet_10.js"
  ],
  "browser_action": {
    "default_title": "PrintWhatYouLike",
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://www.printwhatyoulike.com 'unsafe-eval'; img-src 'self' https://www.printwhatyoulike.com; object-src 'self';"
}
