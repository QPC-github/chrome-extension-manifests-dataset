{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "A2Zapps Notifications",
  "description": "This extension shows Notification in A2Zapps.com.",
  "version": "1.2.0",
  "permissions": [
    "http://na1.a2zapps.com/",
    "http://na2.a2zapps.com:8080/",
    "tabs",
    "location"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "activity.js"
    ]
  }
}
