{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search Funnel",
  "short_name": "searchfunnel",
  "manifest_version": 2,
  "version": "1.0",
  "description": "The fastest way to get search results",
  "browser_action": {
    "default_popup": "searchFunnel.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons": {
    "16": "search-funnel-icon-16.png",
    "48": "search-funnel-icon-48.png",
    "128": "search-funnel-icon-128.png"
  }
}
