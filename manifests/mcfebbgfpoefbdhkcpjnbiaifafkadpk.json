{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Speed Up Scroll",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Speed up rendering on page scroll",
  "author": "Ruslan Login",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "user.js"
      ],
      "css": [
        "user.css"
      ]
    }
  ]
}
