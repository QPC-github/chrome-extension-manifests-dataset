{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "I'm Kampf. Donald Kampf"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Add this app to your Chrome browser and replace all instances of \"Trump\" with \"Kampf.\"",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "I'm Kampf. Donald Kampf",
  "permissions": [
    "http://*.google.com/",
    "http://*.com/",
    "tabs"
  ],
  "version": "1.0"
}
