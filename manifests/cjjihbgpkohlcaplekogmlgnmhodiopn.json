{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SMOOK",
  "version": "3.0.6",
  "description": "Send message on one key",
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": [
        "jquery.js",
        "content-script.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons": {
    "256": "iconUP.png"
  },
  "web_accessible_resources": [
    "jquery.js",
    "scontent-script.js"
  ]
}
