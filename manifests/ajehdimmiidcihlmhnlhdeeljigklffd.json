{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AWS SAML Page Usability",
  "version": "1.0.3",
  "description": "Simplify UX for AWS SAML Page",
  "permissions": [
    "https://signin.aws.amazon.com/saml"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://signin.aws.amazon.com/saml"
      ],
      "js": [
        "jquery.min.js",
        "contentScript.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
