{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Swift Orchidist",
  "description": "This extension provides the user with a fuzzy predictive search function for the name of orchid species.",
  "version": "1.0.4",
  "browser_action": {
    "default_icon": "icons/icon_16.png",
    "default_popup": "index.html",
    "default_title": "Search orchids!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://orchid.botanick.info/*",
    "https://fonts.gstatic.com/*"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "options_page": "options.html"
}
