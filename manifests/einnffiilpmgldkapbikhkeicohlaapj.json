{
  "background": {
    "scripts": [
      "js/localStorage.js",
      "js/CheckAuth.js",
      "js/Background.js",
      "js/jquery.min.js",
      "js/Common.js",
      "js/limitationLI.js",
      "js/background/task.js",
      "js/background/newsModule.js",
      "js/background/autoSearch.js",
      "js/background/parserRecruiter.js",
      "js/background/parserSN.js",
      "js/background/parserDefault.js",
      "js/background/autosearchEngine.js",
      "js/background/newVersionChecker.js"
    ]
  },
  "browser_action": {
    "default_popup": "html/Popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/Content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "matches": [
        "https://app.snov.io/*"
      ],
      "js": [
        "js/Snovio.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://snov.io/"
      ],
      "js": [
        "js/jquery.min.js",
        "js/presence.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://app.snov.io/prospects/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/presence.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "img/128.png",
    "19": "img/19.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "cookies",
    "notifications",
    "storage",
    "webRequest",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "short_name": "__MSG_appShortName__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.11.4"
}
