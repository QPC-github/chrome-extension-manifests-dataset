{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DailyDota2 Twitch Notifier",
  "version": "1.50",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.twitch.tv/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "contentscript.js"
  ]
}
