{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "gcphelper",
  "version": "1.1",
  "description": " Productivity enhancement tool for users of Google Cloud Platform !",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "GCP Helper",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "manifest_version": 2
}
