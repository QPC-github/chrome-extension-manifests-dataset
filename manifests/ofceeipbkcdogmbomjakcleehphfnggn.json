{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Naver Webtoon Comment Hider",
  "version": "0.2.0",
  "description": "This extension hides comments in Naver webtoon",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://comic.naver.com/ncomment/ncomment.nhn*"
      ],
      "js": [
        "hider.js"
      ],
      "all_frames": true
    }
  ]
}
