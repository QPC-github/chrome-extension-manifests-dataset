{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "World Cup",
  "description": "Keep up with world cup events",
  "version": "2.3",
  "permissions": [
    "https://*.unipaltec.com/*"
  ],
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "browser_action": {
    "default_title": "Watch World Cup Online",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
