{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "MatevzFa",
  "homepage_url": "https://github.com/MatevzFa/osrs-polls-extension",
  "name": "Enhanced Old School RuneScape Poll Results",
  "version": "0.0.5",
  "description": "It makes official poll result pages useful.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "matches": [
        "http://services.runescape.com/*/oldschool/results.ws?id=*"
      ]
    }
  ]
}
