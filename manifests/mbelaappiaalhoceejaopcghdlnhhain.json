{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "XML-RPC Client",
  "description": "XML-RPC Client",
  "version": "1.1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/ico32.png"
  },
  "background": {
    "scripts": [
      "js/event.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "img/ico128.png",
    "32": "img/ico32.png",
    "16": "img/ico16.png"
  },
  "permissions": [
    "clipboardWrite",
    "storage",
    "<all_urls>"
  ]
}
