{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitReacts",
  "description": "Add reactions to GitHub repos!!! Because why not?",
  "version": "1.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://github.com/*",
        "https://github.com/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "velocity.js",
        "velocity.ui.min.js",
        "insert.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "c505d7a.png"
  ]
}
