{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "description": "iPrivate Search is a safe, easy to use Private Search  does not track your search history. ",
  "icons": {
    "128": "icon.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "private search",
  "permissions": [
    "tabs"
  ],
  "version": "1.2"
}
