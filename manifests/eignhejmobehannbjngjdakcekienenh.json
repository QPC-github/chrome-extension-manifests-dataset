{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FB Cookie",
  "short_name": "Facebook Cookie",
  "description": "Facebook Cookie - `c_user` + `xs`",
  "version": "1.0.3",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "FB Cookie",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*.facebookcorewwwi.onion/*",
    "*://*.facebook.com/*",
    "activeTab",
    "clipboardWrite",
    "cookies",
    "declarativeContent"
  ]
}
