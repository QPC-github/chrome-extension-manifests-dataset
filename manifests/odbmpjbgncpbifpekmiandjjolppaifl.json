{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screen Sharing for Live Conference",
  "author": "Niche Video Media,llc",
  "version": "1.21",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Niche Video Media Screen Sharing For Live Conference 1.0",
  "homepage_url": "https://nichevid.com",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
