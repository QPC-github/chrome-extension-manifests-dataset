{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick-Bookmarks",
  "description": "You can quickly find the bookmark. From a favicon.",
  "author": "pro_bi@naver.com",
  "version": "1.2",
  "permissions": [
    "bookmarks",
    "alarms",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "Quick-Bookmarks",
    "default_icon": "icon.png",
    "default_popup": "app.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
