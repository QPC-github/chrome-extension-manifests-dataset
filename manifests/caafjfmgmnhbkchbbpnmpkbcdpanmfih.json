{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "52pojie",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "吾爱破解 Chrome 插件。现有功能：左侧目录增加滚动，折叠帖子内容，过滤水帖，自动签到。",
  "permissions": [
    "https://www.52pojie.cn/*"
  ],
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.52pojie.cn/*"
      ],
      "js": [
        "bundle.js"
      ]
    }
  ]
}
