{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "description": "Post to composer.io directly from your browser.",
  "icons": {
    "128": "icon_128.png",
    "16": "icon.png",
    "48": "icon_48.png"
  },
  "name": "ComposerChrome",
  "permissions": [
    "tabs",
    "http://composer.io/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "manifest_version": 2
}
