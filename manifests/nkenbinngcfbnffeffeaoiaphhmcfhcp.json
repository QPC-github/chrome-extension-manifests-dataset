{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Daily CBT",
  "version": "1.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Extension for writing 'Anxiety risk assesment' and 'Thought records' Copyright by Andras Papp",
  "icons": {
    "16": "face-plain_16.png",
    "48": "face-plain_48.png",
    "128": "face-plain_128.png"
  },
  "browser_action": {
    "default_icon": "face-plain_16.png",
    "default_title": "Daily CBT",
    "default_popup": "cbt.html"
  }
}
