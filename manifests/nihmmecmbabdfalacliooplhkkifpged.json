{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ColorKarma",
  "description": "ColorKarma is a simple palette tool, just generate an awesome color palette, and you're good to go!",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
