{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Resistance to Change",
  "version": "1.0.0",
  "description": "Keeps github repos on the right like the good old days.",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "main.css"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
