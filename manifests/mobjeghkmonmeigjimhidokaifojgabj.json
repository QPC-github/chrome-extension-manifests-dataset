{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ferramenta PR",
  "version": "1.0",
  "description": "Ferramenta para identificar as colas de derivacao",
  "browser_action": {
    "default_icon": "icon24.png",
    "default_popup": "PR.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
