{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My search",
  "description": "Customized search",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://*.baidu.com/",
    "http://*.google.com/",
    "https://*.google.com/",
    "http://cn.bing.com/"
  ]
}
