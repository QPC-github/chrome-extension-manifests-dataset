{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search Confluence",
  "description": "To use, type 'cf' plus a search term into the Omnibox.",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://confluence.oraclecorp.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "omnibox": {
    "keyword": "cf"
  },
  "manifest_version": 2
}
