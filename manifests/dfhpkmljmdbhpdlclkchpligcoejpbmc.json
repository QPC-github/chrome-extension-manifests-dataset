{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MobilMarks",
  "version": "0.9",
  "icons": {
    "16": "icons/mmico16.png",
    "46": "icons/mmico46.png",
    "128": "icons/mmico128.png"
  },
  "description": "A low memory usage way to \"keep open\" tabs",
  "author": "Dillard Robertson",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "manage.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "icons/mmicog19.png",
      "38": "icons/mmicog38.png"
    },
    "default_title": "Unknown State",
    "default_popup": ""
  }
}
