{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "44",
  "version": "0.97.0",
  "author": "Tyto Robotics Inc.",
  "name": "RCbenchmark.com - GUI",
  "short_name": "rcbenchmarkGUI",
  "description": "Crossplatform control software for the RCbenchmark.com motor test tool",
  "offline_enabled": true,
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "eventPage.js"
      ],
      "persistent": false
    }
  },
  "permissions": [
    "https://www.google-analytics.com/",
    "http://*.rcbenchmark.com/",
    "serial",
    "storage",
    "fileSystem",
    "fileSystem.write",
    "fileSystem.retainEntries",
    "notifications",
    "https://docs.google.com/forms/d/*",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ],
  "sandbox": {
    "pages": [
      "tabs/autocontrol/sandbox.html"
    ]
  },
  "icons": {
    "128": "images/icon_128.png"
  }
}
