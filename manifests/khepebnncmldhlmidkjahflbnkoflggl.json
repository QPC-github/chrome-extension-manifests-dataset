{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "name": "CA Service Desk Screenshot By Ness",
  "short_name": "Ness Screenshot",
  "description": "מאפשר לצרף תצלום מסך למערכת הפניות, לאחר הורדת התוסף יש ללחוץ על האייקון שמתווסף ולהתקין את הקובץ במסך שנפתח",
  "version": "1.6",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "icons": {
    "16": "icon.png",
    "128": "icon-128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.ness-os.com/*",
      "*://*.ness-tech.co.il/*"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "downloads"
  ]
}
