{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KeyForge Cards",
  "version": "1.0",
  "description": "Keyforge cards viewer",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "ext.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "32": "icon.png"
  }
}
