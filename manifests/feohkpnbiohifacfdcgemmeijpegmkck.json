{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BillionSearch",
  "version": "0.0.3",
  "manifest_version": 2,
  "short_name": "BSearch",
  "description": "Bookmark Lovers, this is for you! The best new tab theme with the option to add Quick bookmarks.",
  "homepage_url": "https://billionsearch.com",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "overide.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
