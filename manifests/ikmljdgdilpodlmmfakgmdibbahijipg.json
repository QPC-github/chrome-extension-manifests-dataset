{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Try It Later!",
  "description": "Save all the Apps or Web Services you want to try at a later time!",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "http://www.tryitlater.com/",
    "tabs"
  ]
}
