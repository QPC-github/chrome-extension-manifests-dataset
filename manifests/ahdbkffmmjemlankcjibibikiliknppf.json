{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Censor",
  "description": "Censors reddit usernames for easily anonymized screenshots",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Click to hide usernames"
  },
  "manifest_version": 2
}
