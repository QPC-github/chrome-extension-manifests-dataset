{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Плагин для сайта www.nnm.me v2.0",
  "author": "Sigala",
  "version": "2.1.2",
  "description": "Плагин для http://nnm.me для Chrom'иумов v 2.0",
  "icons": {
    "16": "icons/16.png",
    "38": "icons/64.png",
    "48": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "js": [
        "chrom.user.js"
      ],
      "matches": [
        "http://nnm.me/*",
        "http://nnm.me/",
        "http://*.nnm.me/*",
        "http://*.nnm.me/",
        "http://*/nnm.me/*"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "19": "icons/24.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "38": "icons/64.png",
      "76": "icons/64.png",
      "128": "icons/128.png"
    },
    "default_title": "Плагин NoNaMe v 2.0",
    "default_popup": "popup.html"
  }
}
