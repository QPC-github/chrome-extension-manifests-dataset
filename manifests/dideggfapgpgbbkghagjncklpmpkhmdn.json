{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Roundero",
  "description": "This extension will round all prices on the page",
  "version": "2.2",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Round prices",
    "default_icon": "icon.png",
    "default_popup": "roundero.html"
  },
  "manifest_version": 2
}
