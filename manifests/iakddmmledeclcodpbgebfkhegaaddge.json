{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BIFIT Signer",
  "version": "1.1.1",
  "description": "__MSG_appDesc__",
  "default_locale": "ru",
  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "Icon16.png",
    "48": "Icon48.png",
    "128": "Icon128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "manifest.json"
  ]
}
