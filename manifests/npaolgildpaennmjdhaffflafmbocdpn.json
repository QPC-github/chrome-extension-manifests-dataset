{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noticias y Actualidad",
  "description": "Noticias para Chrome",
  "version": "1.0",
  "author": "@sysmaya",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": true
  },
  "permissions": [
    "http://noticiados.net/"
  ],
  "browser_action": {
    "default_icon": "ico.png",
    "default_popup": "popup.html"
  }
}
