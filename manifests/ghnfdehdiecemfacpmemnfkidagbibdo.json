{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unslant",
  "version": "2.0",
  "description": "Break your bubble.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage"
  ],
  "web_accessible_resources": [
    "logo.png"
  ]
}
