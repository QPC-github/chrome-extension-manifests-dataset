{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "localPathToLocalhost",
  "description": "Changes docroot path to docroot url",
  "version": "1.2.1",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Changes docroot path to docroot url"
  },
  "icons": {
    "16": "icon19.png",
    "36": "icon36.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
