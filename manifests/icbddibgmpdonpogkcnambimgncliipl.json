{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "downloads",
    "https://savetogdrive.pdfzorro.com/*",
    "<all_urls>",
    "https://www.pdfzorro.com/*"
  ],
  "name": "PDF Editor Extension - PDFzorro",
  "version": "0.0.0.15",
  "short_name": "Use Google Chrome as PDF Editor",
  "description": "Open PDF files with Chrome and edit them with PDFzorro Extension",
  "icons": {
    "16": "logo16.png",
    "128": "logo.png"
  },
  "container": [
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "904120569284",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
