{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video Speed Shift",
  "description": "Video Speed Shift",
  "version": "3.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*",
    "http://youtube.com/*",
    "https://youtube.com/*",
    "http://youtu.be/*",
    "https://youtu.be/*",
    "http://www.youtu.be/*",
    "https://www.youtu.be/*"
  ],
  "browser_action": {
    "default_title": "Video Speed Shift"
  },
  "options_page": "index.html",
  "manifest_version": 2
}
