{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GW Warp Bookmarks",
  "description": "Adds Warp Bookmarks into Galaxy Warfare.",
  "version": "1.1",
  "author": "HBTech",
  "version_name": "Active Developmental Beta",
  "homepage_url": "http://game.galaxywarfare.com",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://game.galaxywarfare.com/*"
      ],
      "js": [
        "jquery.min.js",
        "default.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
