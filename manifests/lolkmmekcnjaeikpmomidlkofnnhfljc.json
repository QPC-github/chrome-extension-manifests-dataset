{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "extensionly",
  "version": "1.1",
  "description": "(自动)管理插件!Automatic Extension Manager.",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "management"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/棒冰.png",
      "32": "images/棒冰.png",
      "48": "images/棒冰.png",
      "128": "images/棒冰_128.png"
    }
  },
  "icons": {
    "16": "images/棒冰.png",
    "32": "images/棒冰.png",
    "48": "images/棒冰.png",
    "128": "images/棒冰_128.png"
  },
  "manifest_version": 2
}
