{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timer Pro",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "This simple timer will not let you forget important things.",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "img/clock.png",
    "default_popup": "popup.html"
  },
  "offline_enabled": true,
  "permissions": [
    "storage",
    "notifications"
  ],
  "web_accessible_resources": [
    "img/48.png"
  ]
}
