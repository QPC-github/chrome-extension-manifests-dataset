{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mobbed Glob",
  "version": "0.0.1.0",
  "author": "whs2k and shashwatblack",
  "description": "Keeps tab on your web usage",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "downloads",
    "storage"
  ],
  "background": {
    "scripts": [
      "idb.js",
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2,
  "icons": {
    "512": "icon.png"
  }
}
