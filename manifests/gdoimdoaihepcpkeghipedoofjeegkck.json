{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UniqTab",
  "description": "Simply make each tab url unique with a refresh option as you navigate the web.",
  "version": "0.0.0.1",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "Control UniqTab",
    "default_popup": "MainIndex.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
