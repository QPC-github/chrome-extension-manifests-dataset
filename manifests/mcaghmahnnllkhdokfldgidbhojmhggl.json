{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Mit Ihrem Einkauf spenden Sie automatisch einen gewissen Beitrag ohne weiteren Aufwand.",
  "manifest_version": 2,
  "name": "Andrea&Falk Charity Foundation",
  "version": "1.5",
  "author": "Andrea&Falk",
  "homepage_url": "http://andrea-und-falk.com/charity24/",
  "icons": {
    "48": "icon/newLogo128.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' https://codepen.io/charity24/pen/wQZYYL.js 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon/Logo48.png",
    "default_title": "Andrea&Falk Charity Foundation"
  },
  "permissions": [
    "tabs",
    "webRequestBlocking",
    "webNavigation",
    "webRequest",
    "browsingData",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "css/*.css",
    "fonts/*.woff",
    "fonts/*.woff2",
    "fonts/*.ttf",
    "fonts/*.eot",
    "images/*"
  ]
}
