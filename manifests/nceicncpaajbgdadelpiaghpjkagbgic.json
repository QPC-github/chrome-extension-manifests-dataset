{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UrbanLookup",
  "description": "Stop wasting your time searching manually",
  "version": "0.1",
  "browser_action": {
    "default_icon": "ub.png"
  },
  "icons": {
    "16": "ub.png",
    "48": "ub.png",
    "128": "ub.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "scripts.js"
    ]
  },
  "manifest_version": 2
}
