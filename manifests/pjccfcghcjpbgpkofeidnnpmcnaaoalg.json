{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DebugMode",
  "description": "This extension adds Debug class to the current page body tag",
  "version": "1.0",
  "browser_action": {
    "default_title": "Go to debug mode"
  },
  "background": {
    "scripts": [
      "blq-debug.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
