{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Giphy for GitHub",
  "description": "",
  "version": "1.0.4",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "244": "icons/icon244.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
