{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Простой конвертер валют",
  "permissions": [
    "*://*.iliz.net/*",
    "*://*.cryptocompare.com/*"
  ],
  "version": "1.10",
  "description": "Калькулятор курсов для валют: доллары, рубли, гривны, белорусские рубли, евро, польские злотые, BitCoin, Ethereum, Zcash, Litecoin",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Простой конвертер валют",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "19": "icon19.png",
    "48": "icon48.png"
  }
}
