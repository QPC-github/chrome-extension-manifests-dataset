{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Urbacolors Tab",
  "description": "Mettez du Street Art dans votre navigateur et découvrez de nouvelles oeuvres à chaque nouvel onglet.",
  "version": "1.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "http://fonts.googleapis.com/",
    "https://streetart.photo/",
    "https://www.urbacolors.com/"
  ]
}
