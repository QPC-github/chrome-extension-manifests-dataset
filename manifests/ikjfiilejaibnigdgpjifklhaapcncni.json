{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Not Kevin Hart",
  "manifest_version": 2,
  "name": "Who is Kevin Hart",
  "description": "Google will pretend to not know about Kevin Hart",
  "version": "3.0.18",
  "browser_action": {
    "default_title": "Remove Kevin Hart",
    "default_icon": {
      "19": "images/icon19_inactive.png",
      "38": "images/icon38_inactive.png"
    }
  },
  "permissions": [
    "tabs",
    "https://*.google.com/"
  ],
  "background": {
    "scripts": [
      "scripts/bg.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  }
}
