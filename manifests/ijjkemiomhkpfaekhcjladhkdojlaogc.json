{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "goormIDE: Powerful Code Editor with Container",
  "author": "goorm inc",
  "description": "The Powerful Cloud-based IDE Service",
  "version": "3.0.0.3",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
