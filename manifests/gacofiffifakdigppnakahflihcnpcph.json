{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Ynet Popup",
  "description": "Remove the annoying Ynet-Anti-Ad-Blocker popup message",
  "manifest_version": 2,
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "http://www.ynet.co.il/*",
        "https://www.ynet.co.il/*"
      ]
    }
  ]
}
