{
  "author": "Roman Gryadunov",
  "content_scripts": [
    {
      "js": [
        "app.js"
      ],
      "matches": [
        "https://*.youtube.com/*",
        "https://*.habrahabr.ru/*",
        "https://*.adme.ru/*",
        "https://*.lifehacker.ru/*",
        "http://*.the-village.ru/*",
        "https://*.9gag.com/*",
        "http://*.thechive.com/*",
        "http://*.collegehumor.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "16x16.png",
      "19": "19x19.png",
      "38": "38x38.png",
      "48": "48x48.png",
      "128": "128x128.png"
    },
    "default_title": "Attaboy! Stop ads!"
  },
  "description": "Hide ads at youtube.com, 9gag.com, collegehumor.com, thechive.com, habrahabr.ru, adme.ru, lifehacker.ru, the-village.ru",
  "icons": {
    "128": "app-icon.png",
    "16": "16x16.png",
    "48": "48x48.png"
  },
  "manifest_version": 2,
  "name": "Attaboy! Stop ads!",
  "short_name": "Attaboy!",
  "permissions": [],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
