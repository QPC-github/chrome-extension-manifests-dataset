{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SeoStack Keyword Tool",
  "description": "FREE tool to find long tail keywords quickly and easily. Brought to you by SeoStack",
  "version": "1.1.7",
  "icons": {
    "128": "icon.png"
  },
  "short_name": "SeoStack",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*.google.com/*",
    "*://*.duckduckgo.com/*",
    "*://*.bing.com/*",
    "*://*.pinterest.com/*",
    "*://*.yahoo.com/*",
    "*://*.amazon.com/*",
    "*://*.amazon.co.uk/*",
    "*://*.amazon.co.jp/*",
    "*://*.amazon.cn/*",
    "*://*.ebaystatic.com/*"
  ]
}
