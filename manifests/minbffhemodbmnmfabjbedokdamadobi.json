{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Redirect Ad block",
  "version": "2.0",
  "description": "Ad blocker to prevent unwanted redirection...",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "blocked_domains.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
