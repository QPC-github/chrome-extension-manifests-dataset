{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Style Cleanser",
  "description": "Make the internet More Readable",
  "version": "1",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "content.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this Text more Readable",
    "default_icon": "SCicon.png"
  },
  "manifest_version": 2
}
