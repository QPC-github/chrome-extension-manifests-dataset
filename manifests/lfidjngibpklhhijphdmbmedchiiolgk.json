{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VMware Horizon URL Content Redirection Helper",
  "description": "Chrome Extension to enable URL redirection in VMware Horizon.",
  "version": "8.0.0",
  "icons": {
    "16": "horizon-16.png",
    "48": "horizon-48.png",
    "128": "horizon-128.png"
  },
  "background": {
    "scripts": [
      "filterUrl.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ]
}
