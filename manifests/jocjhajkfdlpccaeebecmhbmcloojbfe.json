{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TornScout Backpack",
  "version": "0.5.7",
  "description": "TornScout Backpack is a user script, designed to improve user experience of the Torn City.",
  "permissions": [
    "activeTab",
    "http://www.torn.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.torn.com/*",
        "*://torn.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "backpack.user.js"
      ]
    }
  ],
  "manifest_version": 2
}
