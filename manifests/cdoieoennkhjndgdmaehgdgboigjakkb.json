{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hitmarkers",
  "manifest_version": 2,
  "version": "4.20",
  "description": "An MLG extension that puts hitmarkers in your browser.",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
