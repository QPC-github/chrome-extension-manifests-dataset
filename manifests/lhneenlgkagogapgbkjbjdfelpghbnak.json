{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Search the selected text in any search engines through a context menu.",
  "icons": {
    "128": "icon_large.png",
    "16": "icon_small.png",
    "48": "icon-normal.png"
  },
  "manifest_version": 2,
  "name": "Context Menu Search",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "version": "1.1.0.1"
}
