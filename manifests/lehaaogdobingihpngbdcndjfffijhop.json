{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Miny.co | URL Shortener",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Quickly shorten your URLs",
  "browser_action": {
    "default_icon": "icon_toolbar.png",
    "default_popup": "result.html"
  },
  "permissions": [
    "<all_urls>",
    "http://*.facebook.com/",
    "tabs"
  ]
}
