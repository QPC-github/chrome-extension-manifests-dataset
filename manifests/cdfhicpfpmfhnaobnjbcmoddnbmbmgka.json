{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Xerox Standard Accounting Utility Extension",
  "description": "Support for Xerox MFP's CIS Portal",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "version": "0.74",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
