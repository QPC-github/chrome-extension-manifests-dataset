{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AgileForce Screen Sharing",
  "description": "An extension to allow screen sharing.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "https://*.agileforce.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
