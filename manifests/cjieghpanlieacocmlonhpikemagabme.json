{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ETools",
  "version": "1.0.2",
  "description": "This extension can zoom in/out the content.",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png"
  },
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_title": "ETools",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
