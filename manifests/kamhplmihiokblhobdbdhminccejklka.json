{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "name": "PirateProxy",
  "version": "1.1",
  "description": "PirateProxy",
  "icons": {
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_title": "PirateProxy",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://dl.slethen.io/",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
