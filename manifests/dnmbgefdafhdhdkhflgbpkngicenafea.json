{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Official SAT Question of the Day",
  "description": "College Board SAT Question of the Day RSS",
  "version": "0.5",
  "icons": {
    "128": "collegeboard.png"
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "collegeboard-small.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery.xml2json.js",
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "http://sat.collegeboard.org/practice/sat-question-of-the-day-rss",
    "http://sat.collegeboard.org/practice/sat-question-of-the-day"
  ]
}
