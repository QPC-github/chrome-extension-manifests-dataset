{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Link Saver",
  "description": "Save links for use later.",
  "version": "1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/custom.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
