{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VeloCV",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "1 click convert your LinkedIn profile into a beautiful website!",
  "homepage_url": "https://velocv.com",
  "author": "veloCV",
  "offline_enabled": false,
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "veloCV",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "https://*.linkedin.com/*",
    "https://velocv.com/*"
  ]
}
