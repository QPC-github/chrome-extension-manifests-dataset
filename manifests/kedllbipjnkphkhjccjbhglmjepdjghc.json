{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Right Tabs By Misuta",
  "description": "右側のタブを閉じる拡張機能です。",
  "version": "1.1",
  "icons": {
    "128": "icon.png",
    "16": "icon-r.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-r.png",
    "default_title": "右側のタブを閉じる拡張機能です。"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
