{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DollarVision",
  "description": "Replace mentions of seemingly human companies with the publicly traded stock abbreviation.",
  "version": "0.1.1",
  "icons": {
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
