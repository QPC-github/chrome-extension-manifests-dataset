{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bica",
  "version": "0.0.1",
  "description": "The Question and Answer Platform for Teams",
  "homepage_url": "https://bica.io",
  "permissions": [
    "storage",
    "identity",
    "notifications",
    "https://*.auth0.com/*",
    "https://api.bica.io/*"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icons/128.png"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "manifest_version": 2
}
