{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Steroids for Qualiportal",
  "short_name": "Steroids for QP",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Adds the functionality to filter by category. See the dropdown beneath the side navigation.",
  "content_scripts": [
    {
      "matches": [
        "https://qualiportal.swisscom.com/*"
      ],
      "js": [
        "jquery.min.js",
        "ext.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "info.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
