{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  },
  "name": "Khmer font enlarger",
  "description": "This extension chooses the Khmer OS Content font and makes the text larger for Khmer content",
  "version": "1.3",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-2.2.3.min.js",
        "mutation-summary.js",
        "tweakfont.js"
      ]
    }
  ]
}
