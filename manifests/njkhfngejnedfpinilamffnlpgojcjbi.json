{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Resizer Unofficial",
  "version": "1.0.0",
  "description": "Simple extension for the new Google Resizer.",
  "homepage_url": "https://section214.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/icon-128.png",
    "default_title": "Resizer"
  },
  "permissions": [
    "tabs"
  ]
}
