{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qrcoder",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "convert the current active tab url to qrcode image",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
