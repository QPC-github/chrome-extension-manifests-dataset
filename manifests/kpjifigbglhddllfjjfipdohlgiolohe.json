{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Service-Now AutoFrames",
  "version": "0.1",
  "description": "Adds frames to new Service-Now tabs",
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.service-now.com/*",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
