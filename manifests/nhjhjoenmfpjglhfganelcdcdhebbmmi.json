{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouAd Skipper",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Sample Chrome Extension",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "youtube_auto_skip.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "youtube_auto_skip.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
