{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tabs URLs in current window (Wayl Assured)",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "View all tabs URLs in current window (Wayl Assured)",
  "homepage_url": "https://www.wayl.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}
