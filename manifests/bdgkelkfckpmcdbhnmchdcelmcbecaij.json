{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "reverse reddit",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This extension checks if the current webpage was posted on reddit",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "reverse reddit",
    "default_popup": "reverse_reddit.html"
  },
  "permissions": [
    "tabs"
  ]
}
