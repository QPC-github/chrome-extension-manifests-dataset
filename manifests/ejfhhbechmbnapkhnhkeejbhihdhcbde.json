{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tumblpass",
  "description": "This extension provides you advanced user experience with Tumblr blogs.",
  "version": "2.4.46",
  "permissions": [],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "blockremover.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
