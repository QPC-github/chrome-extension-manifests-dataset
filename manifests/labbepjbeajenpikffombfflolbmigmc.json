{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to CurationPod",
  "description": "Pinterest style bookmark system. Save your link, text, youtube video, picture in Category",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "extension.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "128": "icon_128.png",
    "32": "icon.png"
  }
}
