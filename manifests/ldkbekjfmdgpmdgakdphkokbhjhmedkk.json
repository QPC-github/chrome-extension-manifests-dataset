{
  "author": "Peoplefone AG",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://127.0.0.1:3000/*",
        "https://meet.peoplefone.at/*"
      ]
    }
  ],
  "description": "This Google Chrome extension brings screen sharing to peoplefone MEET",
  "homepage_url": "https://www.peoplefone.at",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "MEET peoplefone.at",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.3",
  "web_accessible_resources": [
    "icon.png"
  ]
}
