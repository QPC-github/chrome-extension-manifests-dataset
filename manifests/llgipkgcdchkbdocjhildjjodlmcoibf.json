{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Master Splinter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "split and save tabs for later",
  "browser_action": {
    "default_icon": "ms.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "256": "ms256.png"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}
