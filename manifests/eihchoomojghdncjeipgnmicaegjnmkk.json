{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Employed Limited",
  "version": "0.0.0.1",
  "description": "Employed is an employment automation extension for multiple online hiring services, most famously indeed.com and linked.com.",
  "author": "Nesta Saint. Clever Parchment",
  "homepage_url": "https://www.linkedin.com/jobs/search/",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon_128.png",
    "default_title": "Employed",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.linkedin.com/jobs/search/",
    "tabs"
  ],
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  }
}
