{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dark theme by VladZavr",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Read web pages in dark places with comfort.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Dark theme"
  }
}
