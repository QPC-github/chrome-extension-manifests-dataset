{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BiblioScope",
  "author": "First Domino, LLC",
  "description": "Learn about possible bibliophiliac trivia behind the Wikipedia links of your surfed web pages.",
  "version": "0.12",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
