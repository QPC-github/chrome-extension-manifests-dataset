{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Philly Train Status Extension",
  "description": "Septa Train Status",
  "version": "2.0.0",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://www3.septa.org/hackathon/NextToArrive/"
  ],
  "browser_action": {
    "default_icon": "image/Train.PNG",
    "default_popup": "ext_popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "ext_background.html"
  }
}
