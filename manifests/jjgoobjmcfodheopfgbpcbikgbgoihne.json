{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "eCommerce Outtakes",
  "description": "IMPROVING USABILITY AND THE ECOMMERCE EXPERIENCE: ONE BLOG POST AT A TIME.",
  "icons": {
    "48": "lion48-2.png",
    "128": "lion128-2.png"
  },
  "version": "1.1",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery.min.js",
      "eco.js"
    ]
  },
  "permissions": [
    "tabs",
    "pageCapture",
    "http://*/"
  ],
  "browser_action": {
    "default_icon": "lion2.png",
    "default_popup": "eco.html"
  }
}
