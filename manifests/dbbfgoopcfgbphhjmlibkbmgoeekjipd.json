{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/run19.png"
    },
    "default_title": "runTimers"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Run multiple timers in a single browser tab",
  "icons": {
    "16": "img/run16.png",
    "48": "img/run48.png",
    "112": "img/run112.png",
    "128": "img/run128.png"
  },
  "manifest_version": 2,
  "name": "runTimers",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "notifications"
  ],
  "version": "1.0.0"
}
