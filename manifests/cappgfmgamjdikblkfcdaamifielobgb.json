{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "网址导航",
  "version": "1.24",
  "manifest_version": 2,
  "description": "上网导航.让您上网更方便。收录最热门最常用网址。安全绿色纯净。",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://api.flickr.com/"
  ]
}
