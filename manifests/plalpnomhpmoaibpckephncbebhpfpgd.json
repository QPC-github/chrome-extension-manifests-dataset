{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Striker Manager stadium calculator",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Calculate maximum possible income from the next home game",
  "content_scripts": [
    {
      "matches": [
        "http://*.strikermanager.com/estadio.php"
      ],
      "all_frames": true,
      "js": [
        "code.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://en3.strikermanager.com/",
    "tabs",
    "http://en2.strikermanager.com/",
    "tabs",
    "http://en1.strikermanager.com/",
    "tabs",
    "http://en.strikermanager.com/"
  ]
}
