{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Total Mobility Kiosk v1.99.22",
  "description": "Mobility Kiosk.  Learn about wheelchair vans and view complete wheelchair van inventory.",
  "version": "1.99.22",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "power",
    "fullscreen",
    "<all_urls>",
    "fileSystem",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "kiosk-icon-16.png",
    "128": "kiosk-icon-128.png"
  },
  "kiosk_enabled": true
}
