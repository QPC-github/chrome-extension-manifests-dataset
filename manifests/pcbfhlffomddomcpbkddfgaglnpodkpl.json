{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Lord of the Tabs",
  "short_name": "Tab Lord",
  "description": "Open tab management",
  "version": "0.0.11.9",
  "browser_action": {
    "default_title": "theLordOfTheTabs"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage",
    "chrome://favicon/"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "lot16.png",
    "32": "lot32.png",
    "42": "lot32.png",
    "48": "lot48.png",
    "64": "lot48.png",
    "128": "lot128.png"
  }
}
