{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "yt_ffc",
  "version": "1.0",
  "description": "Warns users if the content is deemed Family Friendly",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watc*"
      ],
      "js": [
        "detect.js"
      ]
    }
  ]
}
