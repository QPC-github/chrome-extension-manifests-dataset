{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Red Green Text!",
  "version": "1.0",
  "description": "This makes many main words on the screen alternate between color red and color green.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "code.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
