{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Demandware Buisiness Manager UI Classic",
  "description": "This extension changes the UI of the Demandware Buisiness Manager menu to the classic design",
  "version": "1.12",
  "manifest_version": 2,
  "icons": {
    "19": "icon_19.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    "resource/*"
  ],
  "browser_action": {
    "default_title": "Demandware Buisiness Manager UI Classic",
    "default_icon": "icon_48.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js",
        "menuswap.js"
      ],
      "css": [
        "dwuic.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ]
}
