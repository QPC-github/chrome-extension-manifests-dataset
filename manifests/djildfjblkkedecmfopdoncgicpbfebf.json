{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BeDesigner",
  "description": "An extension to allow you to screen sharing.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "img/128.png",
    "19": "img/19.png",
    "48": "img/48.png"
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://localhost:*/*",
      "*://*.bedesigner.io/*"
    ]
  },
  "key": "",
  "permissions": [
    "desktopCapture"
  ]
}
