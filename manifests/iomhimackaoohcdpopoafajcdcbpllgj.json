{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sanekat",
  "description": "A meerkat video resizer.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://meerkatapp.co/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://meerkatapp.co/*"
  ]
}
