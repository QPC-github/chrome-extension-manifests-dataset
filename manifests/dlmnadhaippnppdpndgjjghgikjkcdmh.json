{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js",
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/off_19.png",
    "default_popup": "popup.html",
    "default_title": "Mobile Dev"
  },
  "description": "모바일 개발자를 위한 도구",
  "icons": {
    "128": "icons/on_128.png",
    "19": "icons/on_19.png",
    "38": "icons/on_38.png"
  },
  "manifest_version": 2,
  "name": "MobileDev",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "browsingData",
    "tabs",
    "cookies",
    "proxy",
    "<all_urls>"
  ],
  "version": "1.1.0",
  "web_accessible_resources": [
    "icons/on_19.png",
    "icons/off_19.png"
  ]
}
