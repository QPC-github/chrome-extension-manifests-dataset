{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DataZen - LinkedIn prospects into Hubspot",
  "version": "0.0.0.3",
  "description": "Get LinkedIn prospects into Hubspot in one click",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
