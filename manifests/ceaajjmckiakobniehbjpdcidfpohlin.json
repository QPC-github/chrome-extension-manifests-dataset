{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sample IME for IME extension API",
  "version": "1.0.4",
  "permissions": [
    "input"
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "description": "Sample IME for IME extension API",
  "background": {
    "scripts": [
      "ime.js",
      "init.js"
    ]
  },
  "input_components": [
    {
      "name": "Sample IME for IME extension API",
      "type": "ime",
      "id": "sample_ime_for_ime_extension_api",
      "description": "Sample IME for IME extension API",
      "language": "en-US",
      "layouts": [
        "us::eng"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
