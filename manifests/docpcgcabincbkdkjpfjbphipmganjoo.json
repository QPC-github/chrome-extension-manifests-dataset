{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HidePlay",
  "author": "St√•le Raknes",
  "description": "Hides the huge playbutton from the native video player.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "play.png",
    "default_title": "Play"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "noplay.css"
      ]
    }
  ]
}
