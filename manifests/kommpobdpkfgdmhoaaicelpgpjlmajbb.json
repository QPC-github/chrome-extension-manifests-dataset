{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BeProdigy Bookmark",
  "version": "1.1",
  "description": "A Basic extension to manage bookmarks in chrome",
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "128": "beprodigy.png"
  },
  "browser_action": {
    "default_title": "BeProdigy Bookmark Manager",
    "default_icon": "beprodigy.png",
    "default_popup": "gui.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
