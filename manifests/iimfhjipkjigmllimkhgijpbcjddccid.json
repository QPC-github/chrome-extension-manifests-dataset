{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tìm hàng Quảng Châu",
  "description": "tim hang quang chau, cong cu dat hang taobao, tmall, 1688, mua hang trung quoc, timhangquangchau, đặt hàng nhanh chóng",
  "version": "1.1",
  "page_action": {
    "default_icon": "images/128x128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "64": "images/64x64.png",
    "128": "images/128x128.png"
  },
  "permissions": [
    "storage",
    "https://timhangquangchau.com/",
    "http://timhangquangchau.com/",
    "https://2.taobao.com/item.htm"
  ],
  "web_accessible_resources": [
    "images/*"
  ],
  "homepage_url": "http://timhangquangchau.com",
  "short_name": "timhangquangchau.com",
  "input_components": {
    "rate": 3470
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.timhangquangchau.com/*",
        "http://*.timhangquangchau.com/*",
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "https://*.tmall.hk/*",
        "http://detail.1688.com/*",
        "https://detail.1688.com/*",
        "http://wwww.amazon.com/*",
        "https://wwww.amazon.com/*"
      ],
      "css": [
        "/css/all.min.css"
      ],
      "js": [
        "/dist/all.min.js"
      ]
    }
  ]
}
