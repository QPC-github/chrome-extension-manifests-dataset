{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hyperlink",
  "description": "This extension will link context to the source system",
  "version": "1.0.0.1",
  "icons": {
    "16": "img/link1_16.png",
    "32": "img/link1_32.png",
    "48": "img/link1_48.png",
    "128": "img/link1_128.png"
  },
  "browser_action": {
    "default_icon": "img/link1_19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "context.js"
    ]
  },
  "web_accessible_resources": [
    "img/link1_48.png"
  ]
}
