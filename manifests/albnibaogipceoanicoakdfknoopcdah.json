{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jobaffinity",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "1.3.7",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "js": [
        "LinkedInParser.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "identity",
    "cookies",
    "activeTab",
    "tabs",
    "https://www.jobaffinity.fr/",
    "https://jobaffinity.fr/"
  ]
}
