{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Examine Offline Data (Cookies, HTML5 Storage)",
  "version": "1.2.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Examine Offline Data",
    "default_icon": "icon48.png"
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "page_script.js"
  ]
}
