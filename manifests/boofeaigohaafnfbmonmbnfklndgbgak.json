{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tiny Notepad: Minimalist & Quick Notes",
  "version": "0.0.3",
  "description": "A beautiful, simple way to write down notes",
  "icons": {
    "16": "icons/16px.png",
    "32": "icons/32px.png",
    "48": "icons/48px.png",
    "128": "icons/128px.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "author": "Theodore Hu",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "storage"
  ]
}
