{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "hacktweet",
  "version": "1.24",
  "manifest_version": 2,
  "description": "Extension that converts tweets to images then uploads them to pintrest",
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "addButton.js",
        "such_ajax.js",
        "html2canvas.js",
        "twimgListener.js"
      ]
    }
  ],
  "permissions": [
    "http://twitter.com/*",
    "https://twitter.com/*"
  ]
}
