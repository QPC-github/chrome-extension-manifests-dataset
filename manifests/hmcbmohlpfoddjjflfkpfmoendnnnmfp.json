{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Link Expander",
  "description": "Expand your links yo",
  "version": "1.1",
  "author": "DannyAziz",
  "content_scripts": [
    {
      "js": [
        "index.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
