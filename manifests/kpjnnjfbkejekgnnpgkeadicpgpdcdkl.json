{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Invite All Friends the Easy Way",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Inviter",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Inviter"
  },
  "author": "Francesco Biagini",
  "permissions": [
    "*://*.facebook.com/*"
  ],
  "version": "2.10.15"
}
