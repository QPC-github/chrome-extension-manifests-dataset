{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Modify Salesforce Appearance",
  "version": "0.1.1",
  "description": "Salesforce Colors extension",
  "icons": {
    "48": "48x48Icon.png",
    "128": "128x128Icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "accountStyles.css"
      ],
      "matches": [
        "https://*.salesforce.com/001*"
      ]
    },
    {
      "css": [
        "opportunityStyles.css"
      ],
      "matches": [
        "https://*.salesforce.com/006*"
      ]
    },
    {
      "css": [
        "contactstyles.css"
      ],
      "matches": [
        "https://*.salesforce.com/003*"
      ]
    }
  ]
}
