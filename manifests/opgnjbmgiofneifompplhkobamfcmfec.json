{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Messaggero Unlimited",
  "version": "0.1",
  "description": "Leggi Il Messaggero senza limiti",
  "short_name": "Messaggero Unlimited",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ilmessaggero.it/*",
        "http://www.ilmessaggero.it/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ]
}
