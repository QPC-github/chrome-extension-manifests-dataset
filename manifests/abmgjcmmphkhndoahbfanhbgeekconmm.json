{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/store.js",
      "js/settings.js",
      "js/menu.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "gfx/icon_16.png",
    "default_title": "Insert Text Options"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'sha256-zwtN93GVKZb8qIreVN19Uh+aYkquDFCRrC0dx28dQkk='; object-src 'self';",
  "description": "Inserts text into any text field at the click of a button.",
  "icons": {
    "128": "gfx/icon_128.png",
    "16": "gfx/icon_16.png",
    "48": "gfx/icon_48.png"
  },
  "manifest_version": 2,
  "name": "Insert Text",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "version": "2.0.5"
}
