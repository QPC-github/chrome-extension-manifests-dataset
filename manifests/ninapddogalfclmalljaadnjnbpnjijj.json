{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "leaf_transp.png",
    "default_popup": "popup.html",
    "default_title": "BioKnol"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "BioKnol",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "BioKnol",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2.7"
}
