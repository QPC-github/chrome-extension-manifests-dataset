{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Qgle",
  "description": "This extension allows the user to highlight text and use ctrl+q to Google it.",
  "version": "0.0.0.3",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.min.js"
      ]
    }
  ],
  "browser_action": {}
}
