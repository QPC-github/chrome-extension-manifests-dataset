{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Finlandsite Nieuws",
  "version": "2.7.0",
  "description": "Laatste 5 nieuwsberichten via Finlandsite",
  "icons": {
    "128": "news_icon.png"
  },
  "browser_action": {
    "default_title": "Finlandsite Nieuws",
    "default_icon": "news_action.png",
    "default_popup": "feed.html"
  },
  "permissions": [
    "tabs",
    "http://finlandsite.nl/*"
  ]
}
