{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "抓取api数据返回JSON",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Wiki Parser",
  "version": "0.6.6",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Wiki Parser"
  },
  "content_scripts": [
    {
      "matches": [
        "https://wiki.mail.netease.com/*",
        "http://doc.hz.netease.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "jquery.js",
    "crawle_inject.js",
    "jsonToSchema.js"
  ]
}
