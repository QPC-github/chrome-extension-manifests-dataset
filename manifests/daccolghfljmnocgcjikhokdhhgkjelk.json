{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "InSalesCRM Contact Fetcher",
  "version": "1.0.1",
  "description": "InSalesCRM Contact Fetcher for Google Chrome browser ",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Sync"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "64": "icon.png"
  },
  "manifest_version": 2
}
