{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Go to Alerts"
  },
  "description": "Get Bukkit Alerts",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "BukkitAlert",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [
    "img/48.png"
  ]
}
