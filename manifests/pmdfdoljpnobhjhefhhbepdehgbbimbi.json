{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QRCode This",
  "description": "This generates QR code for the URL you are browsing. From version 2.0, you can choose different size and share it through email.",
  "version": "2.5",
  "browser_action": {
    "default_title": "QRCode This",
    "default_icon": "qrcode-this.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
