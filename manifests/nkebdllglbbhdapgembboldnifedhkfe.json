{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ekonnekt Desktop Sharing",
  "description": "Extension that allows share desktop into meetings",
  "version": "0.1.7",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "ekonnekt-logo-16x16.png",
    "48": "ekonnekt-logo-48x48.png",
    "128": "ekonnekt-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.ekonnekt.com/*"
    ]
  }
}
