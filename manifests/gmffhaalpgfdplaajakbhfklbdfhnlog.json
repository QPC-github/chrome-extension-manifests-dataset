{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tideways Profiler",
  "short_name": "tideways",
  "description": "This extension allows you to control tracing applications with Tideways Profiler.",
  "version": "1.3.8",
  "author": "Tideways GmbH",
  "homepage_url": "https://tideways.io",
  "icons": {
    "128": "img/tideways_128.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "<all_urls>",
    "https://app.tideways.io/browser/settings",
    "https://app.tideways.local/browser/settings"
  ],
  "browser_action": {
    "default_icon": "img/tideways_16.png",
    "default_popup": "profiler.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
