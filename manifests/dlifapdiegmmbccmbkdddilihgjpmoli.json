{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "TwitchNoFlash.js"
      ],
      "matches": [
        "http://*.twitch.tv/*",
        "https://*.twitch.tv/*",
        "http://twitch.tv/*",
        "https://twitch.tv/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Forces Twitch to use HTML5 player instead of the Flash player.",
  "manifest_version": 2,
  "version": "1.2",
  "author": "Bayram1995",
  "homepage_url": "http://www.bayram1995.org",
  "name": "Twitch No Flash",
  "short_name": "Twitch No Flash",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
