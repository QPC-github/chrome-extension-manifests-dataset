{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Switcher",
  "description": "With this extension you can browse Reddit trough memes and news faster than ever !",
  "version": "0.2.36",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
