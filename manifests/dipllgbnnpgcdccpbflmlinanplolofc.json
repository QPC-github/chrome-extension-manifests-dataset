{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "wikin128.png"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Wikipedia Search Plugin",
  "icons": {
    "128": "wikin128.png",
    "16": "wikin16.png",
    "48": "wikin48.png"
  },
  "manifest_version": 2,
  "name": "Wikin",
  "version": "2.0.1"
}
