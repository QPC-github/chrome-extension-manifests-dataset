{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "The Travelst",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Save any piece or entire webpage in your collections to plan your perfect trip.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "*://*.getr.me",
    "*://*labs.gurron.me",
    "tabs",
    "http://www.getr.me/",
    "http://labs.gurron.com/",
    "bookmarks"
  ]
}
