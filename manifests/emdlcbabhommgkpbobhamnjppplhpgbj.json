{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IMSLP impatience",
  "version": "0.0.0.2",
  "description": "Skip the 15 second wait time on imslp with this simple extension!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://imslp.org/wiki/Special:ImagefromIndex/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
