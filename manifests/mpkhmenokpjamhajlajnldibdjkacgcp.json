{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flair Player",
  "short_name": "Flair",
  "description": "Online Radio Player for Google Chromeâ„¢, Chromebook, and Chromecast",
  "version": "0.10.28",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": [
        "resources/data/min.js",
        "scripts/obj/min.js",
        "scripts/min.js",
        "min.js"
      ]
    }
  },
  "file_handlers": {
    "audio": {
      "extensions": [
        "m3u",
        "pls"
      ],
      "title": "Flair Player"
    }
  },
  "optional_permissions": [
    "notifications"
  ],
  "permissions": [
    "fileSystem",
    "storage",
    "http://*/"
  ],
  "icons": {
    "16": "iconsmall.png",
    "128": "icon96.png"
  },
  "sockets": {
    "tcp": {
      "connect": ""
    },
    "tcpServer": {
      "listen": "127.0.0.1:*"
    }
  }
}
