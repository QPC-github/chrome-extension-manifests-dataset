{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Icon",
  "version": "1.0.3",
  "permissions": [
    "contextMenus"
  ],
  "description": "View the current time from the chrome extension bar. Right click on the time icon for options.",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
