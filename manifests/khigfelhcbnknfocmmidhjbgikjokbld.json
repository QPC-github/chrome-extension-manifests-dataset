{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YDS Online Eğitim Videoları: Hız & Tekrar",
  "description": "YDS Online Eğitim sistemindeki videolarında bu eklenti ile hız ve tekrarlama özelliğini aktif edebilirsiniz.",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "vimeo.js"
      ],
      "matches": [
        "*://vimeo.com/*",
        "*://player.vimeo.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/vimeo128.png",
    "48": "images/vimeo48.png"
  },
  "version": "1.9",
  "manifest_version": 2
}
