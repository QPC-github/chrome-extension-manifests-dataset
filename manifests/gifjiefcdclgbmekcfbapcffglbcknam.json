{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My-Musix",
  "description": "This extension adds YouTube or Vimeo links to your personal My-Musix library",
  "version": "1.0.7",
  "permissions": [
    "alarms",
    "tabs",
    "activeTab",
    "webNavigation",
    "declarativeContent",
    "https://www.my-musix.com/*"
  ],
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    },
    "default_title": "Click to link it!"
  },
  "icons": {
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "web_accessible_resources": [
    "js/jquery-1.11.0.min.js",
    "js/background.js"
  ]
}
