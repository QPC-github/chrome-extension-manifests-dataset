{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No Stress N1",
  "description": "Removes negative votes in comments from rs.n1info.com",
  "version": "1",
  "author": "Ervin Serfozo",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://rs.n1info.com/*",
        "http://rs.n1info.com/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
