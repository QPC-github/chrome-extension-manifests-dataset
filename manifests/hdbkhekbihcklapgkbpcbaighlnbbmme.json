{
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/config.js",
        "js/script.js",
        "js/bootstrap.min.js"
      ],
      "css": [
        "css/addon_tool.css",
        "css/main.css",
        "css/boostrap4.css",
        "css/fontawesome.min.css"
      ],
      "matches": [
        "https://*.amazon.co.jp/*",
        "https://item.rakuten.co.jp/*",
        "https://store.shopping.yahoo.co.jp/*",
        "https://www.uniqlo.com/jp/store/goods/*",
        "https://www.locondo.jp/shop/commodity/*",
        "https://item.fril.jp/*",
        "https://www.mapcamera.com/item/*",
        "https://page.auctions.yahoo.co.jp/jp/auction/*",
        "https://contact.auctions.yahoo.co.jp/buyer/*"
      ]
    }
  ],
  "description": "Tomoniglobal - オーダーツール",
  "manifest_version": 2,
  "name": "Công Cụ Đặt Hàng Tomoniglobal.com",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.3"
}
