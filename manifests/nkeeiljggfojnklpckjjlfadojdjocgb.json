{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Georgian Font Fixer",
  "short_name": "GFF",
  "version": "0.2",
  "description": "Fixes Georgian Uppercase problem in Chrome 69",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "fix.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "sylfaen.ttf"
  ]
}
