{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Absolute date time for pixiv Sketch",
  "short_name": "psdate",
  "description": "pixiv Sketch の投稿日付を年月日表示へ変更します。",
  "version": "1.6",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/main.js"
      ],
      "matches": [
        "https://sketch.pixiv.net/*"
      ]
    }
  ],
  "manifest_version": 2
}
