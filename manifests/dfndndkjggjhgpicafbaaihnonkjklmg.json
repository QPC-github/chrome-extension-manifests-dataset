{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MGSearch",
  "version": "1.2",
  "description": "Quick search on mgautomotive.com.ua site",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>",
    "clipboardRead"
  ],
  "background": {
    "scripts": [
      "mgSearch-background.js"
    ]
  },
  "browser_action": {
    "default_popup": "mgSeach.html",
    "default_icon": "mgsearch.png"
  },
  "manifest_version": 2
}
