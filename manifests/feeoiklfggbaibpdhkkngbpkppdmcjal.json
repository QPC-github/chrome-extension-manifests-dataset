{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Counter",
  "version": "1.1",
  "description": "Displays the current number of open tabs, open windows or both",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "incognito": "split",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Tab Counter"
  },
  "manifest_version": 2
}
