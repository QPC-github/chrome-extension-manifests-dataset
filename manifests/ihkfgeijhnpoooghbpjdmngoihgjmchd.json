{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "THON Countdown",
  "description": "Get Hype for THON",
  "author": "DJHerls",
  "version": "1.0.2",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "1700": "icons/icon1700.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/_/chrome/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ]
}
