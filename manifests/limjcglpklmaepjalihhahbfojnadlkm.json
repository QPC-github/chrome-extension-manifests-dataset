{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PrettyRadical",
  "description": "Get information about a news article or blog in seconds, including the title, url, author, and author's email!",
  "version": "0.0.18",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "identity",
    "identity.email",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com https://*.google.com 'unsafe-eval'; object-src 'self'",
  "oauth2": {
    "client_id": "894766299825-8qmdr5n64fhc2b5nbrm6e8kmp9jepl7j.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  }
}
