{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SPOOKED!",
  "description": "Do not use this extension unless you wish to be SPOOKED!",
  "version": "1",
  "browser_action": {
    "default_icon": "spookedPumpkin.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "SPOOKED!.js"
      ],
      "run_at": "document_end"
    }
  ]
}
