{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HTML/JSON to JavaScript String Convertor",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "HTML/JSON to JavaScript String Convertor",
  "browser_action": {
    "default_icon": "img/sample-19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "img/sample-48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
