{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "img/19.png",
      "38": "img/38.png"
    },
    "default_title": "Subscribe",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/sniff_common.js",
        "js/doc_start.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/sniff_common.js",
        "js/feed_finder.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Easily Subscribe RSS and Atom Feeds to Feedly.",
  "homepage_url": "https://browsernative.com/feedly-chrome-extension/",
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "Feedly Subscribe Button",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ]
}
