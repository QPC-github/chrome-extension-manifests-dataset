{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LightShot opener",
  "description": "Open LightShot pages by typing only their codes.",
  "version": "1.3",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open LightShot pages",
    "default_popup": "lso.html"
  }
}
