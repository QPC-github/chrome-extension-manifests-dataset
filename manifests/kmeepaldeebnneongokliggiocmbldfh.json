{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Assinatura Digital - FP2 Tecnologia",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Assinatura Digital utilizada pela FP2 Tecnologia",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "page.js"
  ],
  "permissions": [
    "nativeMessaging"
  ]
}
