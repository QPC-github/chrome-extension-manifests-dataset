{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.1.7",
  "name": "clipdown",
  "description": "Markdown editor/viewer for Google Chrome and Firefox.",
  "homepage_url": "https://github.com/saltyshiomix/clipdown",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-32.png"
  },
  "icons": {
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "/index.html"
  ]
}
