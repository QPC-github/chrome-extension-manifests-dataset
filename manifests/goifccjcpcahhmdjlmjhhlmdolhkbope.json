{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_application_title__",
  "version": "1.0.3.18",
  "description": "__MSG_application_description__",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "author": "Assima PLC",
  "background": {
    "scripts": [
      "background/main.js",
      "background/browser.js",
      "background/constants.js",
      "background/debug.js",
      "background/events.js",
      "background/frameMapper.js",
      "background/icon.js",
      "background/ipc.js",
      "background/logging.js",
      "background/tabs.js",
      "background/utils.js",
      "background/webSockets.js",
      "background/windows.js",
      "background/runner.js"
    ],
    "persistent": true
  },
  "incognito": "spanning",
  "permissions": [
    "<all_urls>",
    "nativeMessaging",
    "tabs",
    "webNavigation"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.disabled.png",
      "48": "images/icon48.disabled.png",
      "128": "images/icon128.disabled.png"
    }
  },
  "default_locale": "en"
}
