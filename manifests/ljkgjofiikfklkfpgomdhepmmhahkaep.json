{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "うた",
  "description": "Japanese Song Lyrics Search",
  "version": "1.0.8",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "うた"
  },
  "permissions": [
    "http://*/",
    "tabs"
  ]
}
