{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vinocount",
  "version": "1.0.1",
  "description": "View Vinocount inventory while browsing supplier's websites.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://app.vinocount.com/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/logo.png"
    }
  },
  "icons": {
    "128": "images/logo.png"
  },
  "manifest_version": 2
}
