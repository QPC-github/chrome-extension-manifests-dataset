{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "e14 plugin",
  "short_name": "Search",
  "description": "Searches for the selected text in the NaverDictionary",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "windows": "Ctrl+Shift+D",
        "mac": "Ctrl+Shift+D",
        "linux": "Ctrl+Shift+D"
      },
      "description": "Trigger",
      "global": true
    }
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  }
}
