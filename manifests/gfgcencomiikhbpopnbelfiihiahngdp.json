{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ImageScratch",
  "version": "1.3.4",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "img/19.png"
    },
    "default_title": "ImageScratch",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "img/*.png"
  ],
  "permissions": [
    "storage",
    "*://*/*"
  ]
}
