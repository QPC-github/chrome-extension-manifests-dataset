{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZbigZ - Official Extension",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Add torrents to ZBIGZ in one click",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://zbigz.com/*",
    "https://zbigz.com/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "base64.js",
      "zbigz.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/16.png"
  },
  "web_accessible_resources": [
    "icons/16.png",
    "icons/48.png",
    "icons/128.png"
  ]
}
