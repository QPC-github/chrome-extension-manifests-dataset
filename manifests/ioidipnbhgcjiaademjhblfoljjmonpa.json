{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ColorMixture",
  "version": "0.5.2",
  "description": "Show grey, red, green, blue, yellow, magenta, cyan scales of an image",
  "manifest_version": 2,
  "icons": {
    "48": "color48.png",
    "128": "color128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*.jpg",
        "http://*/*.gif",
        "http://*/*.png",
        "https://*/*.jpg",
        "https://*/*.gif",
        "https://*/*.png"
      ],
      "js": [
        "edit.js"
      ]
    }
  ]
}
