{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FC New Tab Bookmarks",
  "description": "The Most Beautiful New Tab Bookmaks Manager. (This is an alpha version, moltre features will be added soon)",
  "version": "0.5.0",
  "manifest_version": 2,
  "icons": {
    "128": "img/icon_128.png",
    "256": "img/icon_256.png",
    "512": "img/icon_512.png"
  },
  "permissions": [
    "bookmarks",
    "identity",
    "identity.email",
    "storage",
    "management",
    "unlimitedStorage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
