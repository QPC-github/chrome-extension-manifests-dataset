{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "Nic Mulvaney",
  "name": "Quick Wikipedia",
  "short_name": "Wikipedia search",
  "description": "Quick Wikipedia",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": {
      "16": "icons/Wikipedia-icon16.png",
      "48": "icons/Wikipedia-icon48.png",
      "128": "icons/Wikipedia-icon128.png"
    },
    "default_popup": "index.html",
    "default_title": "Quick Wikipedia"
  },
  "icons": {
    "16": "icons/Wikipedia-icon16.png",
    "48": "icons/Wikipedia-icon48.png",
    "128": "icons/Wikipedia-icon128.png"
  },
  "permissions": [
    "https://en.wikipedia.org/"
  ],
  "content_security_policy": "script-src 'self' https://en.wikipedia.org; object-src 'self'"
}
