{
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "background": "background.html",
  "browser_action": {
    "default_icon": "logofox.png",
    "default_title": "rpidity"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-1.7.1.min.js",
        "js/tool.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "rpidity",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "name": "rpidity",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1"
}
