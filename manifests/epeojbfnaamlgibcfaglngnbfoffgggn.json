{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PushD PopD",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "16": "save16.png",
    "48": "save48.png",
    "128": "save.png"
  },
  "permissions": [
    "activeTab",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "pushd": {
      "description": "Push URL",
      "suggest_key": {
        "default": "Ctrl+Shift+Period"
      }
    },
    "popd": {
      "description": "Pop URL",
      "suggest_key": {
        "default": "Ctrl+Shift+Comma"
      }
    }
  }
}
