{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Eat Paper",
  "version": "1.1.8",
  "manifest_version": 2,
  "description": "A tool to save Google Scholar results graphically",
  "browser_action": {
    "default_icon": "ep_icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
