{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ニココメ",
  "version": "1.0.1",
  "description": "ニコニコ動画、コメント抽出＆タグ抽出付き検索",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
