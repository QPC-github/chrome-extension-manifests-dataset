{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabViewer",
  "version": "0.1",
  "description": "This extension will display all Window and all Tab.",
  "browser_action": {
    "default_title": "Tab Viewer"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
