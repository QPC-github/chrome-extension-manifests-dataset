{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello",
  "version": "0.0.49",
  "manifest_version": 2,
  "description": "Quickly create Trello cards and access your Trello boards from the Omnibox.",
  "permissions": [
    "tabs",
    "storage",
    "https://trello.com/"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "omnibox": {
    "keyword": "t"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "default_title": "Trello",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "login.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
