{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Checkfront",
  "description": "Smart Simplifed Online Bookings",
  "version": "3.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "urls": [
      "https://*.checkfront.com/",
      "https://*.checkfront.co.uk/"
    ],
    "launch": {
      "web_url": "https://www.checkfront.com/login/?app=chrome"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
