{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Car Value",
  "version": "1.4",
  "description": "Car Value for used cars from Kelly Blue Book (kbb) on sites like craigslist, autotrader ...",
  "permissions": [
    "activeTab",
    "unlimitedStorage"
  ],
  "icons": {
    "24": "assets/img/24x24.png",
    "128": "assets/img/128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.craigslist.org/*",
        "*://*/*"
      ],
      "js": [
        "assets/js/jquery-2.1.1.min.js",
        "assets/js/panel.js",
        "assets/craigslist/craigslist-kbb.js"
      ],
      "css": [
        "assets/js/panel.css",
        "assets/craigslist/craigslist-kbb.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "assets/js/jquery-2.1.1.min.js",
    "assets/craigslist/kbb.html"
  ],
  "browser_action": {
    "default_icon": {
      "24": "assets/img/24x24.png"
    },
    "default_title": "Car Value",
    "default_popup": "assets/craigslist/popup.html"
  },
  "background": {
    "scripts": [
      "assets/js/localforage.min.js",
      "assets/js/rx.lite.js",
      "assets/js/json5.js",
      "assets/js/string_score.js",
      "assets/craigslist/craigslist-kbb-bg.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
