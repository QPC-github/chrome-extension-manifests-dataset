{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Amazon.at + Suchvorschläge von hoo",
  "version": "5.1",
  "manifest_version": 2,
  "description": "Amazon-Suche mit Auto-Vervollständigung Ihrer Suchanfrage und weiteren Optionen.",
  "icons": {
    "16": "amazon16.png",
    "48": "amazon48.png",
    "128": "amazon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://completion.amazon.co.uk/",
    "http://completion.amazon.co.jp/",
    "http://completion.amazon.com/"
  ]
}
