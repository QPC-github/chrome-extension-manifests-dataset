{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bingle for Windows 10",
  "description": "Redirect bing to google! Change your default Web Browser to Google Chrome and use with Windows 10 Taskbar Web Search",
  "version": "1.0.1",
  "icons": {
    "128": "icon/128.png",
    "16": "icon/16.png",
    "48": "icon/48.png"
  },
  "background": {
    "scripts": [
      "backgroundscripts.js"
    ]
  },
  "manifest_version": 2,
  "options_page": "option.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.bing.com/*"
  ]
}
