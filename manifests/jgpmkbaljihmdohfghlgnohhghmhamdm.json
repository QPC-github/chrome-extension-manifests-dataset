{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Al-Mared Arabic Editor verb conjugator",
  "version": "0.0.0.6",
  "description": "Arabic Text Editor, Arabic verb conjugator, quran search محرّر نصوص عربية/تصريف الأفعال العربية/ باحث قرآني",
  "offline_enabled": true,
  "icons": {
    "128": "./html/image/i128.png",
    "96": "./html/image/i96.png",
    "72": "./html/image/i72.png",
    "64": "./html/image/i64.png",
    "48": "./html/image/i48.png",
    "32": "./html/image/i32.png",
    "16": "./html/image/i16.png"
  },
  "browser_action": {
    "default_title": "Al-Mared Arabic Editor verb conjugator",
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "*://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
