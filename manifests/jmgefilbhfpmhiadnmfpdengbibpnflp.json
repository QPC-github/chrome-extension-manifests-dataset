{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simon Burns-Cox",
  "version": "1.6",
  "description": "This is the blog and news section of Simon Burns-Cox, Sculptor and Letter Carver.",
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Simon Burns-Cox"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.js"
    ]
  }
}
