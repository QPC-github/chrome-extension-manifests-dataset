{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UpBeat",
  "version": "1.2",
  "description": "Listen to UpBeat with ease.",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png",
    "361": "icon-361.png"
  },
  "permissions": [
    "webRequest",
    "storage",
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "48": "icon-48.png",
      "128": "icon-128.png",
      "361": "icon-361.png"
    },
    "default_title": "UpBeat",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "popup/Background.js"
    ]
  }
}
