{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Givanza is a search engine like Google. We use our ad revenue to plant trees where nature and people need them most.",
  "manifest_version": 2,
  "icons": {
    "256": "img/logo.png"
  },
  "name": "Givanza - Our search engine is planting trees",
  "background": {
    "scripts": [
      "ext-background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "./img/logo.png",
    "default_title": "Givanza - Our search engine is planting trees"
  },
  "permissions": [],
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://givanza.org/fontdesign/imgs/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "Givanza Search",
      "name": "Givanza Search",
      "search_url": "https://givanza.org/?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "",
      "suggest_url_post_params": ""
    }
  },
  "version": "1.2"
}
