{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GeorgeNet OS Helper",
  "version": "0.1.0",
  "description": "Loads needed Scratch extensions when you start the OS",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://*.scratch.mit.edu/*"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "Settings-icon.png"
  }
}
