{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Honorable Mentions",
  "version": "0.1",
  "description": "Put the notifications button on twitter where god intended it to be",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*",
        "*://twitter.com/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "257": "icon257.png"
  }
}
