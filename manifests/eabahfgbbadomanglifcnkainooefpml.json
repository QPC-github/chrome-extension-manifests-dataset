{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noflix",
  "description": "This extension will hide netflix movies you dont want to watch",
  "version": "1.2",
  "browser_action": {
    "default_icon": "butt.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "permissions": [
    "https://www.netflix.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.netflix.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
