{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL Screener",
  "permissions": [
    "tabs"
  ],
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-64.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "A URL screener to help filter a list of urls.",
  "version": "0.0.2"
}
