{
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/jquery-timing.min.js",
        "js/script.js"
      ],
      "matches": [
        "*://*.pathofexile.com/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Path of Exile stash searcher. Search your stash by name and mod!",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "PoE Stash Searcher++",
  "permissions": [
    "tabs",
    "*://*.pathofexile.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.42"
}
