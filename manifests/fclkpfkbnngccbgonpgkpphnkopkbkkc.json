{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UserCents",
  "description": "Get paid for your two cents.",
  "version": "0.0.0.7",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "css/*.css",
    "img/*.*",
    "fonts/*.*"
  ],
  "permissions": [
    "tabs",
    "cookies",
    "https://www.usercents.com/",
    "http://www.usercents.dev:3000/*",
    "http://www.usercents.dev/*",
    "https://ajax.googleapis.com/"
  ]
}
