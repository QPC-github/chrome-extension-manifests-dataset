{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabSession",
  "version": "1.0",
  "description": "Serves different cookies for diff tabs",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "target_32x32.png",
      "38": "target_32x32.png"
    }
  },
  "manifest_version": 2
}
