{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/logo25.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/reset.css",
        "css/photo2.css",
        "css/rss.css",
        "css/sendWrap.css",
        "css/topic.css",
        "css/comb_PRF_feed.css",
        "css/page.css"
      ],
      "js": [
        "js/SysConstant.js",
        "js/lib/STK.js",
        "js/lib/jquery.js",
        "js/lib/jfeed.js",
        "js/lib/STKTools.js",
        "js/apps/at.js",
        "js/CwsirCore.js",
        "js/CwsirAjax.js",
        "js/apps/rss.js",
        "js/apps/filter_weibo.js",
        "js/apps/pageStart.js",
        "js/apps/module_mouseselection.js",
        "js/apps/module_searchurl.js",
        "js/apps/module_topicSearch.js",
        "js/apps/module_entry.js",
        "js/apps/module_photo.js",
        "js/apps/module_sendweibo.js",
        "js/apps/module_drag.js",
        "js/apps/module_transfurl.js",
        "js/apps/module_page.js",
        "js/apps/module_setPhotoInitial.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "i微链",
  "icons": {
    "16": "images/logo16.png",
    "25": "images/logo25.png"
  },
  "manifest_version": 2,
  "name": "i微链",
  "options_page": "options/options.html",
  "permissions": [
    "cookies",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/*",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "4.1",
  "web_accessible_resources": [
    "images/send_weibo.png",
    "images/sendBtn.png",
    "images/dragger.png",
    "images/nav_logo114.png",
    "authorization_callback.html",
    "options/options.html"
  ]
}
