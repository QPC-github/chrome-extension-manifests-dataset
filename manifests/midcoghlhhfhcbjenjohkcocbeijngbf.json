{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Burninate for Burnside's Litecoin Mining Pool",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Burninate",
  "browser_action": {
    "default_icon": "images/burninate.png",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "popup": "popup.html",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://ltc.kattare.com/",
    "https://vircurex.com/api/",
    "tabs"
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "images/loading.gif"
  ]
}
