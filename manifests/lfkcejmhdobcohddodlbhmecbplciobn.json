{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tiny Proxy",
  "version": "0.6.3",
  "description": "A small footprint proxy extension. Allows simple and complex settings. Quick and easy to switch between proxies.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png",
    "19": "icon-19.png",
    "16": "icon-16.png"
  },
  "permissions": [
    "proxy",
    "contextMenus",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "ico.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "manifest_version": 2
}
