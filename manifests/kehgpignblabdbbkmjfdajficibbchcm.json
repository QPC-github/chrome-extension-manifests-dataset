{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Password Revealer",
  "short_name": "Password Revealer",
  "version": "0.2",
  "description": "Reveals passwords hidden by asterisks.",
  "permissions": [
    "declarativeContent",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "revealer.js",
      "reveal.js"
    ]
  },
  "icons": {
    "16": "reveal16.png",
    "48": "reveal48.png",
    "128": "reveal128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "reveal19.png",
      "38": "reveal38.png",
      "128": "reveal128.png"
    },
    "default_title": "Reveal Password!"
  },
  "manifest_version": 2
}
