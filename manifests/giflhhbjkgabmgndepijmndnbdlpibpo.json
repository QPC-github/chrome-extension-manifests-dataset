{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "乐视海外翻墙软件",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "unblock-letv 乐视海外翻墙软件",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "unblock-letv",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "tool.js"
    ]
  }
}
