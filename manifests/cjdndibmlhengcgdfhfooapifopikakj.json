{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remember It",
  "short_name": "Remember It",
  "version": "0.0.0.7",
  "description": "Replace Chrome new tab with list of things to remember",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "management"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' ; object-src 'self'"
}
