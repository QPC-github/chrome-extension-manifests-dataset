{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Video Volume Sync",
  "version": "0.0.6",
  "description": "Remember and synchronise the volume for videos on Facebook.",
  "icons": {
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "manifest.js",
        "app.js"
      ]
    }
  ]
}
