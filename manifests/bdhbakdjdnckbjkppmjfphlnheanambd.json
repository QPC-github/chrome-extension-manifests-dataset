{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Netword App",
  "short_name": "Search. Save. Simple! Discover local businesses and save.",
  "version": "1.14.8",
  "manifest_version": 2,
  "description": "Netword App",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://www.netword.com/*"
  ]
}
