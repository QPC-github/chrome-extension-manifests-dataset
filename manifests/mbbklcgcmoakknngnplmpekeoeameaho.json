{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social Security Form Filler",
  "externally_connectable": {
    "matches": [
      "*://*.socialsecurityapp.eu/*"
    ]
  },
  "version": "0.0.3.2",
  "description": "Help with filling out A1 social security form on virk. This extension is exclusively made for Relocares Social Security App.",
  "permissions": [
    "webRequest",
    "*://*.socialsecurityapp.eu/",
    "activeTab",
    "declarativeContent",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/SSAicon16.png",
      "32": "icons/SSAicon32.png",
      "48": "icons/SSAicon48.png",
      "128": "icons/SSAicon128.png"
    }
  },
  "icons": {
    "16": "icons/SSAicon16.png",
    "32": "icons/SSAicon32.png",
    "48": "icons/SSAicon48.png",
    "128": "icons/SSAicon128.png"
  },
  "manifest_version": 2
}
