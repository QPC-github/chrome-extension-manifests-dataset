{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ekşi Sözlük Arama",
  "description": "Bu uzantı(extension) ile hızlı bir şekilde eksisozluk.com üzerinde arama yapabilirsiniz.",
  "omnibox": {
    "keyword": "es"
  },
  "icons": {
    "16": "hb_icon.png"
  },
  "version": "1.1",
  "browser_action": {
    "default_icon": "hb_icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/popup.js",
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
