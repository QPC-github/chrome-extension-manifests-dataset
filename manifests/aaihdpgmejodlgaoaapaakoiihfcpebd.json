{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Notify Last Modified Date & time of web page.",
  "manifest_version": 2,
  "name": "LastModified",
  "version": "1.0",
  "icons": {
    "32": "icons/update.png",
    "64": "icons/update64.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icons/update.png"
    }
  }
}
