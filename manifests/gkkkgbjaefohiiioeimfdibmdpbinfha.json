{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "core/proxy.js",
      "core/apps.js"
    ]
  },
  "browser_action": {
    "default_title": "Siwel.info Agence Kabyle d'Information"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Permet d'accéder au site de l'agence Siwel, bloqué en Algérie par les autorités algériennes.",
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "name": "Siwel.info Agence Kabyle d'Information",
  "offline_enabled": false,
  "permissions": [
    "*://*/*",
    "<all_urls>",
    "proxy",
    "activeTab",
    "alarms",
    "browsingData",
    "contextMenus",
    "cookies",
    "declarativeContent",
    "fontSettings",
    "gcm",
    "management",
    "nativeMessaging",
    "notifications",
    "sessions",
    "storage",
    "tabCapture",
    "tabs",
    "topSites",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.2"
}
