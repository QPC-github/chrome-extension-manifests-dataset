{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LessonPaths",
  "version": "1.3",
  "minimum_chrome_version": "17",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "16": "images/LessonPathsLogo16x16.png",
    "48": "images/LessonPathsLogo48x48.png",
    "128": "images/LessonPathsLogo128x128.png"
  },
  "description": "The official LessonPaths extension for Chrome. Add steps to your Playlists with just the click of a button.",
  "browser_action": {
    "default_icon": "images/LessonPathsLogo16x16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "*://localhost/*",
    "*://www.lessonpaths.com/*"
  ]
}
