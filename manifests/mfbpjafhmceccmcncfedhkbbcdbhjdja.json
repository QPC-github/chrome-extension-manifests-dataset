{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Temporary Media",
  "description": "Share Images and other Files with an Expiry Date",
  "version": "1.1",
  "icons": {
    "16": "logo16x16.png",
    "48": "logo48x48.png",
    "128": "logo128x128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Temporary Media",
    "default_icon": "logo16x16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
