{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarklet URL",
  "short_name": "BMlet URL",
  "description": "Hint execution of your bookmarklets",
  "version": "1.0.6",
  "permissions": [
    "activeTab",
    "bookmarks",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "bookmark.png"
  },
  "manifest_version": 2
}
