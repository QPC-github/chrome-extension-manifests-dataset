{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.2.1",
  "name": "Keyword Tool Dominator",
  "description": "Keyword Tool Dominator Extension",
  "short_name": "KTD Extension",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "background": {
    "page": "core.html"
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://*.keywordtooldominator.com/v1/*",
        "https://*.keywordtooldominator.com/v1/*",
        "http://*.keywordtooldominator.com/account/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Keyword Tool Dominator Extension"
  },
  "permissions": [
    "http://*.keywordtooldominator.com/",
    "https://*.keywordtooldominator.com/",
    "http://*.amazon.com/",
    "http://*.amazon.com/",
    "http://*.amazon.co.uk/",
    "https://*.amazon.co.uk/",
    "http://*.amazon.co.jp/",
    "https://*.amazon.co.jp/",
    "http://*.amazon.cn/",
    "https://*.amazon.cn/",
    "http://*.bing.com/",
    "https://*.bing.com/",
    "http://*.ebay.com/",
    "https://*.ebay.com/",
    "http://*.ebaystatic.com/",
    "https://*.ebaystatic.com/",
    "http://*.etsy.com/",
    "https://*.etsy.com/",
    "http://*.google.com/",
    "https://*.google.com/",
    "http://*.google.co.uk/",
    "https://*.google.co.uk/",
    "https://*.fbatoolkit.com/",
    "https://*.walmart.com/"
  ]
}
