{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Callproof Extension",
  "version": "1.0",
  "description": "Callproof Actions",
  "icons": {
    "48": "img/cp_logo.png",
    "128": "img/cp_logo.png",
    "16": "img/cp_logo.png"
  },
  "browser_action": {
    "default_icon": "img/cp_logo.png",
    "default_title": "Callproof",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
