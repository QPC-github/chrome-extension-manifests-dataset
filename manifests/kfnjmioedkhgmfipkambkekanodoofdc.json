{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Hunt",
  "version": "2.0.1",
  "manifest_version": 2,
  "description": "Page Hunt: best pages for startupers, every day",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "scripts/app.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
