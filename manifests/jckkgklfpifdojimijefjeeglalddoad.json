{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cred",
  "description": "Short URLs that pay you. Quick access: Option+4; Alt+4",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "logo_small.png",
      "38": "logo_large.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+4"
      }
    }
  },
  "permissions": [
    "https://cred.is/",
    "activeTab"
  ]
}
