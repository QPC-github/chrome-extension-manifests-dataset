{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Filter People",
  "short_name": "FilterPPL",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "version": "1.0",
  "description": "Easily filter out people from a webpage.",
  "icons": {
    "256": "images/256.png",
    "128": "images/128.png",
    "48": "images/48.png"
  },
  "browser_action": {
    "default_icon": "images/128.png",
    "default_popup": "pages/popup.html",
    "default_title": "Filter People"
  }
}
