{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Strona Startowa ShareInfo.pl",
  "description": "Set homepage on site www.shareinfo.pl",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "image.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://shareinfo.pl/",
    "startup_pages": [
      "http://shareinfo.pl/"
    ]
  },
  "icons": {
    "128": "image.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://www.shareinfo.pl/",
    "http://shareinfo.pl/"
  ],
  "externally_connectable": {
    "matches": [
      "*://shareinfo.pl/*"
    ]
  }
}
