{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Funky Karts",
  "description": "A Nonstop Platform Roller",
  "version": "1.6.4",
  "minimum_chrome_version": "49",
  "icons": {
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "fullscreen",
    "overrideEscFullscreen",
    "storage",
    "unlimitedStorage"
  ],
  "requirements": {
    "3D": {
      "features": [
        "webgl"
      ]
    }
  }
}
