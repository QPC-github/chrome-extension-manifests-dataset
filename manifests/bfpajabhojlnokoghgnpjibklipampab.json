{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hubstaff for Chromebook",
  "version": "0.9.16.90",
  "version_name": "0.9.16-52b57a69d",
  "manifest_version": 2,
  "description": "Chrome time tracker app with screenshots, online timesheets, task management & employee payroll",
  "icons": {
    "16": "img/icon16c.png",
    "32": "img/icon32c.png",
    "48": "img/icon48c.png",
    "128": "img/icon128c.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/app.js"
      ]
    }
  },
  "permissions": [
    "notifications",
    "storage",
    "idle",
    "desktopCapture",
    "https://*.hubstaff.com/api/*",
    "https://hubstaff-production.s3.amazonaws.com/*",
    "https://hubstaff-account.s3.amazonaws.com/*",
    "https://*.cloudfront.net/*"
  ],
  "optional_permissions": [
    "https://*/*"
  ]
}
