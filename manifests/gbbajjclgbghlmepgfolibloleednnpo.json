{
  "background": {
    "page": "background.html"
  },
  "description": "Yarly for Google Chrome",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "web_accessible_resources": [
    "icon_48.png"
  ],
  "manifest_version": 2,
  "name": "Yarly",
  "browser_action": {
    "default_popup": "pasteImage.html",
    "default_title": "Paste Content",
    "default_icon": "icon_48.png"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "gcm",
    "notifications",
    "tabs",
    "contextMenus",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "*://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.5.0"
}
