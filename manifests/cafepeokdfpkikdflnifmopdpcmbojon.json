{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kodcu.com Haberler",
  "manifest_version": 2,
  "version": "2.2.10",
  "description": "Kodcu.com Haberlerine Daha Yakınsınız",
  "icons": {
    "128": "images/d_icon.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "*://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Kodcu.com | Değer Katan Bilgiler",
    "default_icon": "images/icon.png",
    "default_popup": "listofarticles.html"
  },
  "omnibox": {
    "keyword": "kodcu"
  }
}
