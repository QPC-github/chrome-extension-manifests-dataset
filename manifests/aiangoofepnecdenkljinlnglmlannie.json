{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Allows you to hide deleted files from pull request review",
  "manifest_version": 2,
  "name": "Bit Bucket Helper Plugin",
  "version": "1.2.2",
  "icons": {
    "48": "icons/hide-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.bitbucket.org/*"
      ],
      "js": [
        "jquery.js",
        "bit-bucket-helper-plugin.js"
      ]
    }
  ]
}
