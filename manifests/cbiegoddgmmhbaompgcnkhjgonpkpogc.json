{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Navi Live CSS Editor",
  "description": "Live in-browser CSS Editor with syntax highlighting and gutter hints.",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
