{
  "name": "Gadget Buzz",
  "short_name": "Extension",
  "description": "Tips, information and insights of technology and gadgets that will make your life easier.",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Gadget Buzz",
    "default_popup": "gadget.html"
