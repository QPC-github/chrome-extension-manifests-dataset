{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Permanent tab renamer плагин",
  "version": "0.2",
  "description": "Rename tab untill close!",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_title": "PTabRenamer",
    "default_popup": "popup.html",
    "default_icon": "icon/48.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab"
  ],
  "background": {
    "page": "background.html"
  }
}
