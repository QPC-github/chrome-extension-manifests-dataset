{
  "background": {
    "scripts": [
      "jquery.js",
      "right.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon38.png",
    "default_popup": "button/popup.html",
    "default_title": "Copy and paste multiple items, get more Clipboards"
  },
  "default_locale": "en",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "__MSG_description__",
  "commands": {
    "copyme": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z"
      },
      "description": "copy 1"
    },
    "copymee": {
      "suggested_key": {
        "default": "Ctrl+Shift+A"
      },
      "description": "copy 2"
    },
    "pasteme": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "Paste 1"
    },
    "pastemee": {
      "suggested_key": {
        "default": "Ctrl+Shift+2"
      },
      "description": "Paste 2"
    }
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "16": "img/icon16.png",
    "128": "img/icon128.png",
    "48": "img/icon48.png"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.97"
}
