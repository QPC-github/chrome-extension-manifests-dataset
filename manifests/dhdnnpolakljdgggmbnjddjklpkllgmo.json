{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SeoSevens - SEO &  Website Analysis",
  "short_name": "SeoSevens",
  "description": "SeoSevens to improve your site and identify opportunities to get anead of the competition",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "app.js",
      "background.js"
    ]
  }
}
