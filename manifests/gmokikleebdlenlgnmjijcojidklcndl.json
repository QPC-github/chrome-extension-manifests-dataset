{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zoopage",
  "version": "0.1.1",
  "description": "Save current page to zootool",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "zootool.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
