{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trump-Blocker",
  "version": "0.0.1",
  "author": "Michele Riva,",
  "description": "Trump's face hurts my eyes",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Trump-Blocker",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "dist/trumpblocker.umd.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
