{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "STAR: Support tool for accessing resources",
  "version": "0.050",
  "description": "Support tool for accessing resources",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ncbi.nlm.nih.gov/*",
        "https://www.sciencedirect.com/*",
        "https://www.nature.com/*",
        "https://aip.scitation.org/*",
        "https://journals.aps.org/*",
        "https://link.springer.com/*",
        "https://*.onlinelibrary.wiley.com/*",
        "https://onlinelibrary.wiley.com/*",
        "https://iopscience.iop.org/article/*",
        "https://science.sciencemag.org/*",
        "https://journals.sagepub.com/*",
        "https://academic.oup.com/*/article-abstract/*",
        "https://www.cell.com/*",
        "https://pubs.rsc.org/*",
        "https://www.tandfonline.com/*",
        "http://apps.webofknowledge.com/*",
        "https://pubs.acs.org/*",
        "https://ci.nii.ac.jp/naid/*",
        "https://iss.ndl.go.jp/*"
      ],
      "css": [
        "css/alertify.css",
        "css/option.css"
      ],
      "js": [
        "js/lib/jquery.min.js",
        "js/lib/alertify.js",
        "js/contentscript.js",
        "js/option.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "option.html",
  "permissions": [
    "storage"
  ]
}
