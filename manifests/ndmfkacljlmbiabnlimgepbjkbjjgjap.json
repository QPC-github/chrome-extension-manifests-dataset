{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GRurl",
  "short_name": "GRurl",
  "version": "1",
  "description": "URL shortener. Powered by http://grurl.xyz",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click to shorten URL with grurl.xyz"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
