{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "E-biblioteka",
  "description": "Liepājas Universitātes pieeja E-bibliotēkas resursiem.",
  "version": "0.16.1",
  "permissions": [
    "proxy"
  ],
  "background": {
    "scripts": [
      "bibl/app.js"
    ]
  },
  "browser_action": {
    "default_icon": "bibl/logo38.png"
  },
  "icons": {
    "128": "bibl/logo128.png",
    "38": "bibl/logo38.png"
  }
}
