{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TinyToolbarClock",
  "description": "This extension shows the time and date in the toolbar",
  "version": "1.2",
  "icons": {
    "16": "myclock16.png",
    "32": "myclock32.png",
    "48": "myclock48.png",
    "128": "myclock128.png"
  },
  "browser_action": {
    "default_icon": "myclock48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
