{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Web Page Details",
  "short_name": "wpd v.04",
  "version": "0.4",
  "manifest_version": 2,
  "author": "Harish U Warrier",
  "description": "This app Show details of webpage",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "*://*/",
    "activeTab",
    "contextMenus",
    "nativeMessaging",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "Web Page Details",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "scr.js"
  ],
  "content_security_policy": "script-src 'self' http://127.0.0.1:8000; object-src 'self'"
}
