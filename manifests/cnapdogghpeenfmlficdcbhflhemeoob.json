{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Classroom Redirector",
  "description": "Redirects me from my home account to my school account in Google Classroom",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "https://classroom.google.com/u/0/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
