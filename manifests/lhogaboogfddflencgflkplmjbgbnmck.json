{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BookmarksInTab",
  "version": "1.0",
  "description": "Show bookmarks as your homepage(new tab),you can manage bookmarks;在新标签页显示书签，并且可以管理书签。",
  "permissions": [
    "bookmarks",
    "contextMenus",
    "chrome://favicon/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "options.html",
    "default_title": "BookmarksInTab"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
