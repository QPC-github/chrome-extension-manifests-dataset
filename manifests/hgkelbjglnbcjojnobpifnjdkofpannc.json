{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Here to say",
  "description": "Automatically wraps all reddit comments in a thread with the greatest rap verse of all time.",
  "version": "0.2.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/r/*/comments/*",
        "https://www.reddit.com/r/*/comments/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
