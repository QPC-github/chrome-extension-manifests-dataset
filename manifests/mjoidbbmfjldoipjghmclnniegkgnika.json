{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kokous24 for Gmail and Calendar",
  "description": "Quickly send Kokous24 meeting invite using Google Mail and Google Calendar",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png"
  },
  "browser_action": {
    "default_icon": "img/icon_32.png",
    "default_popup": "index.html",
    "default_title": "Send Kokous24 Meeting Invite"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "omnibox": {
    "keyword": "invite"
  },
  "homepage_url": "https://www.kokous24.com",
  "permissions": [
    "cookies",
    "tabs",
    "https://*.google.com/*",
    "https://*.kokous24.fi/*"
  ],
  "optional_permissions": [],
  "version": "2.4.5",
  "web_accessible_resources": [
    "css/bootstrap.min.css",
    "css/font-awesome.min.css",
    "js/bootstrap.min.js",
    "js/jquery.min.js",
    "js/angular.min.js",
    "js/angular-route.min.js",
    "js/spin.min.js",
    "js/app.js",
    "fonts/*",
    "pages/login.html",
    "pages/logout.html",
    "pages/about.html",
    "pages/invite.html",
    "pages/join.html",
    "pages/selectvmr.html",
    "index.html",
    "img/*.png"
  ]
}
