{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Why Are Pronouns So Hard",
  "version": "0.7.5",
  "description": "For those people who just don't get pronouns",
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_title": "Pronouns",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.0.min.js",
        "content.js",
        "jquery-ui.min.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
