{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FreeDropship - Dropship the winning products",
  "short_name": "FreeDropship",
  "description": "FreeDropship - Find and dropship the Aliexpress winning products easily.",
  "version": "1.1.1",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/logo.png",
    "default_title": "FreeDropship",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "assets/jquery-2.2.3.min.js",
      "background.bundle.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.amazon.com/*",
    "*://*.aliexpress.com/*",
    "https://*.freedropship.org/*",
    "https://freedropship.org/*",
    "https://gj.mmstat.com/*",
    "https://freedropship.wegox.cn/*",
    "http://localhost:17999/*",
    "*://*.wegox.cn/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.amazon.com/*",
        "*://*.aliexpress.com/*",
        "*://*.freedropship.org/*",
        "*://freedropship.org/*",
        "http://localhost:17999/*",
        "*://*.wegox.cn/*"
      ],
      "js": [
        "setup.bundle.js"
      ],
      "css": [
        "setup.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "inject.bundle.js"
  ]
}
