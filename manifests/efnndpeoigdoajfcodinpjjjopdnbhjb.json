{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Helpful New Tab",
  "version": "1.1",
  "description": "New tab extension",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://search-brand.com/tabs-fav.ico",
      "is_default": true,
      "keyword": "helpful",
      "name": "Helpful New Tab",
      "search_url": "https://search-brand.com/search.php?q={searchTerms}",
      "suggest_url": "https://search-brand.com/suggest.php?q={searchTerms}"
    }
  },
  "icons": {
    "128": "icons/tabs-128.png",
    "16": "icons/tabs-16.png",
    "48": "icons/tabs-48.png"
  },
  "short_name": "helpful"
}
