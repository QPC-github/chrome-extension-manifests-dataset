{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Top Deals by Icelolly",
  "description": "Discover amazing holiday deals with every new tab!",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "version": "1.0.1",
  "chrome_url_overrides": {
    "newtab": "deals.html"
  },
  "background": {
    "scripts": [
      "js/jquery-3.1.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "storage"
  ]
}
