{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "description": "Web Traffic Analysis and Visitors Details SEO plugin. See visitors on icon. See trends. Similar sites and Similarweb info.",
  "name": "VStat - visit statistics and website traffic",
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "activeTab",
    "identity",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.92",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "VStat",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
