{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "愛しのジョルダン音声検索",
  "description": "ジョルダン乗換案内を音声入力で検索する拡張機能です。",
  "version": "1.1.0",
  "manifest_version": 2,
  "icons": {
    "128": "images/icon-128.png"
  },
  "options_ui": {
    "page": "pages/options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
