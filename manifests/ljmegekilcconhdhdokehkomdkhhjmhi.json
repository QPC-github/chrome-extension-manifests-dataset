{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fidget Tab",
  "description": "Fidget around when you open a new tab!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "fidgettabpng.png"
  },
  "chrome_url_overrides": {
    "newtab": "expand.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
