{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto Submit Form via Shortcut",
  "short_name": "FormShortcut",
  "description": "Automatically submit a form via CMD/CTRL+S",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
