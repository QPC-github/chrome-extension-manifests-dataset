{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "contentscript.js"
      ],
      "matches": [
        "http://pinterest.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "description": "Label purchasable items on Pinterest!",
  "name": "PinShop",
  "version": "1",
  "permissions": [
    "http://*/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
