{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MKT Digital 360",
  "description": "Aplicação com dicas práticas sobre Marketing Digital. Mais info em http://www.marketingdigital360.net/blog",
  "version": "0.3.33",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://marketingdigital360.net/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
