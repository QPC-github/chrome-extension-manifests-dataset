{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Whooma",
  "description": "The core of the powerful streaming search engine, directly on your computer!",
  "icons": {
    "128": "logo_128.png",
    "48": "logo_48.png"
  },
  "version": "1.2.0",
  "manifest_version": 2,
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://whooma.net/*",
        "http://www.whooma.net/*"
      ],
      "css": [],
      "js": [
        "url_load.js"
      ]
    }
  ]
}
