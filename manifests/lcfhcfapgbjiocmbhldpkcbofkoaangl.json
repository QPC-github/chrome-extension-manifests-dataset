{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BlueLink",
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "blue.js"
      ]
    }
  ],
  "icons": {
    "16": "blue16.png",
    "48": "blue48.png",
    "128": "blue128.png"
  },
  "browser_action": {
    "default_title": "Every page is an unvisited page",
    "default_icon": "blue16.png"
  },
  "manifest_version": 2
}
