{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Color Code App",
  "version": "1.0",
  "description": "Displays the color codes for the current tab",
  "browser_action": {
    "default_icon": "icon19-small.png",
    "default_title": "Color Code App"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>",
    "contextMenus"
  ],
  "icons": {
    "16": "icon19-small.png",
    "48": "icon19-large.png"
  }
}
