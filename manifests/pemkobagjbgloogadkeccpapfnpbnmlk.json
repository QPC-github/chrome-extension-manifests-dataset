{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "set the design url and parames ,help web developer  find the difference between html and the design",
  "icons": {
    "16": "images/css-16.png",
    "128": "images/css-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "debugger",
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "match_about_blank": true,
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/css-16.png",
      "38": "images/css-48.png"
    },
    "default_title": "the design setting",
    "default_popup": "options.html"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "images/css-48.png"
  ]
}
