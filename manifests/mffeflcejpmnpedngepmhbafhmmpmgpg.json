{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "My Global Ponies",
  "version": "6.5",
  "description": "Ponies. Everywhere.",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "script.js"
      ]
    }
  ]
}
