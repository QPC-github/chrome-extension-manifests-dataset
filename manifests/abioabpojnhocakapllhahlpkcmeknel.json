{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Send to xNode",
  "description": "Lets you pass Domains, IPs and E-Mail addresses straight to xNode for analysis.",
  "manifest_version": 2,
  "version": "1.1",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "getset.js",
      "stxn.js"
    ]
  },
  "minimum_chrome_version": "16",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}
