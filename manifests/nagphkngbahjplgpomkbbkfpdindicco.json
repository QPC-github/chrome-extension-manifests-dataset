{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ubiAttendance",
  "description": "Track Attendance, Time & location of Employees -24*7",
  "version": "1.1.4",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "geolocation"
  ]
}
