{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide youtube",
  "description": "Hide video stream on youtube and listen only audio",
  "version": "0.0.2",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "youtube16.png",
    "32": "youtube32.png",
    "128": "youtube128.png"
  },
  "browser_action": {
    "default_title": "Hide video"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
