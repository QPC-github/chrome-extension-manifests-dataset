{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "imgur Uploader",
  "description": "Right-click uploading of images and screenshots anonymously or to your imgur account.",
  "version": "2.3.1",
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "contextMenus",
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/common.js",
      "js/main.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "img/logo_48.png",
    "options.html"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
