{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SNULife Reply",
  "version": "0.24",
  "manifest_version": 2,
  "description": "댓글 남긴 글 목록 저장",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://snulife.com/*"
      ],
      "js": [
        "reply_capture.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://www.snulife.com/",
    "storage",
    "tabs"
  ]
}
