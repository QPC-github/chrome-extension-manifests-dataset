{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Know My Classmates",
  "description": "This extension shows you the faces of your friendly classmates from NUS School of Computing",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://ivle.nus.edu.sg/*"
      ],
      "js": [
        "photos.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
