{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "lookup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "支持网易有道词典在浏览器中实现鼠标取词",
  "name": "网易有道词典鼠标取词插件",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "nativeMessaging"
  ],
  "version": "2.5.2",
  "minimum_chrome_version": "37",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/19.png",
    "default_title": "网易有道词典鼠标取词插件"
  }
}
