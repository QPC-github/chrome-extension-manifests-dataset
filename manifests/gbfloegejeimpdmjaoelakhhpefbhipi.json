{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Manuel Zoho SIV",
  "version": "1.0",
  "description": "Pour Manuel, prototype de form-filling (cible Zoho/SIV)",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "http://42morrow.fr/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/42m_16.png",
      "32": "images/42m_32.png",
      "48": "images/42m_48.png",
      "128": "images/42m_128.png"
    }
  },
  "icons": {
    "16": "images/42m_16.png",
    "32": "images/42m_32.png",
    "48": "images/42m_48.png",
    "128": "images/42m_128.png"
  },
  "manifest_version": 2
}
