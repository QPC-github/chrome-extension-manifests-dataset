{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wiimmfi NUTMEG",
  "short_name": "NUTMEG",
  "version": "1.2",
  "description": "Can someone check the NUTMEG of this room again to see if it's all fixed?",
  "content_scripts": [
    {
      "css": [
        "base.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://wiimmfi.de/mkw/room/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon32.png"
  }
}
