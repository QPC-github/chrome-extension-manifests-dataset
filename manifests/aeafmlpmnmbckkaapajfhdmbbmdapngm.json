{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Delete Element",
  "description": "Adds option to context menu to quickly delete a page element",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
