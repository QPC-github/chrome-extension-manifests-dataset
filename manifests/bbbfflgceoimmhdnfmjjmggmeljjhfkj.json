{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Amazon Associate Tag Redirector",
  "homepage_url": "http://www.dustinhawkins.com/atr/",
  "short_name": "ATR",
  "icons": {
    "16": "icons/icon_016.png",
    "32": "icons/icon_032.png",
    "48": "icons/icon_048.png",
    "128": "icons/icon_128.png"
  },
  "manifest_version": 2,
  "name": "Amazon Tag Redirector",
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "16": "icons/icon_016.png"
    },
    "default_title": "ATR"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.0.1",
  "web_accessible_resources": [
    "icons/*"
  ]
}
