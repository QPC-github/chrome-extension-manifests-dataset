{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Popout for Google Chrome™",
  "description": "Popout for Google Chrome™ is simple extension that allows Google Chrome™ tabs and web apps to be opened in a separate window.",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Popout to external window.",
    "default_icon": "icon48.png"
  }
}
