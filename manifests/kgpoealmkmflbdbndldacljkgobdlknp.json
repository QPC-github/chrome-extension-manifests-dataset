{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-3.1.0.min.js",
      "background.js"
    ]
  },
  "name": "Gradcafe Auto Checker",
  "description": "This extension checks the results from http://www.thegradcafe.com/ every 3 minutes",
  "version": "0.0.3",
  "icons": {
    "436": "icons/pray.png"
  },
  "browser_action": {
    "default_icon": "popup/pray.png",
    "default_popup": "popup/popup.html",
    "default_title": "Gradcafe Auto Checker"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://thegradcafe.com/*"
  ],
  "author": "Stool",
  "short_name": "gradcafeAutoChecker"
}
