{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Colored Tab Counter",
  "version": "1.0.2",
  "description": "Counts open tabs and colored signals how many tabs you got",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
