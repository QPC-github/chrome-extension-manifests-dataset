{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keyboard Enhancements",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Adds keyboard navigation and other keyboard shortcuts for popular web sites.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.tumblr.com/dashboard"
      ],
      "js": [
        "jquery.min.js",
        "tumblr.js"
      ]
    }
  ]
}
