{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DB1 Ponto",
  "version": "1.1.15",
  "manifest_version": 2,
  "description": "DB1 Ponto",
  "homepage_url": "http://github.com/heidiks",
  "content_scripts": [
    {
      "matches": [
        "https://prd.meuholerite.com.br/*",
        "http://prd.meuholerite.com.br/*"
      ],
      "css": [
        "lib/css/bootstrap.min.css"
      ],
      "js": [
        "lib/js/jquery.js",
        "lib/js/moment.min.js",
        "src/inject/classes.js",
        "src/inject/calculator.js"
      ]
    }
  ],
  "icons": {
    "16": "icon/logo16.png",
    "48": "icon/logo48.png",
    "128": "icon/logo128.png"
  },
  "browser_action": {
    "default_title": "DB1 Ponto Extension"
  },
  "permissions": [
    "https://*.meuholerite.com.br/*",
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
