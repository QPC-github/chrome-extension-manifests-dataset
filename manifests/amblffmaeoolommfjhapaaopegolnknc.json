{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Embeds visualizations from LearnGLASS within a Moodle website.",
  "background": {
    "page": "background.html"
  },
  "name": "LearnGLASS",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications"
  ],
  "homepage_url": "http://beethoven.gast.it.uc3m.es/",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "16": "icons/button.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "includes/content_kango.js",
        "includes/invoke_async_module.js",
        "includes/message_target_module.js",
        "includes/userscript_client.js",
        "includes/content_init.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "version": "1.3"
}
