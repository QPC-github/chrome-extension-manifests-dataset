{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wooshark for AliExpress & woocommerce",
  "short_name": "Wooshark for AliExpress & woocommerce",
  "description": "Import products from aliexpress to woocommerce",
  "version": "1.8.8",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/aliexpress128.png",
    "default_popup": "popup.html",
    "default_title": ""
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "images/aliexpress128.png",
    "128": "images/aliexpress128.png"
  },
  "background": {
    "scripts": [
      "js/background.js",
      "js/loading.js",
      "js/openOption.js",
      "js/editImage.js",
      "js/vendor/jquery-3.2.1.min.js"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://*.aliexpress.com/*",
        "https://*.lunapic.com/*",
        "https://aeproductsourcesite.alicdn.com/*",
        "https://shoppingcart.aliexpress/*"
      ],
      "all_frames": true,
      "js": [
        "js/editor.js",
        "js/vendor/jquery-3.2.1.min.js",
        "js/vendor/lodash.js",
        "js/vendor/bootstrap.min.js",
        "js/lunaPicEdit.js",
        "js/selectReview.js",
        "js/vendor/math.js",
        "js/vendor/quill.js",
        "js/productUrl.js",
        "js/vendor/fontawesome.js",
        "js/vendor/jquery.toast.min.js",
        "js/vendor/spin.js",
        "js/vendor/progress.js",
        "js/aliexpress-v2.js",
        "js/bulk-import_v2.js",
        "js/aliexpress_order_v2.js",
        "js/aliexpress_order_v3.js",
        "js/import-from-seller.js"
      ],
      "css": [
        "css/bootstrap.min.css",
        "css/quill.css",
        "css/jquery.toast.min.css",
        "css/spin.css",
        "css/progress.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "/js/injectScript.js",
    "/js/bulk-import.js",
    "json/config.json"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
