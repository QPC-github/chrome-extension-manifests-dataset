{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "rBritto",
  "description": "Replaces all images on all pages to Romero Britto.",
  "manifest_version": 2,
  "version": "1",
  "icons": {
    "128": "icon_128x128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "js": [
        "rBritto.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ]
}
