{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fusemarks",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "fusemarks-16.png",
      "24": "fusemarks-24.png",
      "32": "fusemarks-32.png"
    },
    "default_title": "Fusemarks"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ]
}
