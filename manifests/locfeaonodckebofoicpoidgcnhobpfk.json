{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Guardian Science Tab",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Replaces the Fashion tab with Science on TheGuardian.com",
  "content_scripts": [
    {
      "matches": [
        "http://www.theguardian.com/*"
      ],
      "js": [
        "ReplaceFashionWithScience.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.theguardian.com/*"
  ]
}
