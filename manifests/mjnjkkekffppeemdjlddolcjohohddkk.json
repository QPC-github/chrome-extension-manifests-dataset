{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Liss Is More",
  "description": "Replace all instances of Less with Liss. *shrug* It made me laugh.",
  "version": "0.5",
  "icons": {
    "128": "casey_liss.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
