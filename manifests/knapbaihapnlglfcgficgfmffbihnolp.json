{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VK24Online",
  "description": "Приложение, кторое позволяет находиться онлайн 24 часа в сутки",
  "version": "1.0",
  "background": {
    "scripts": [
      "bck.js"
    ]
  },
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://vk.com/",
    "storage"
  ]
}
