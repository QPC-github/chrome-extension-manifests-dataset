{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facebook This Name",
  "description": "Select name, open context menu, and look up the name on Facebook people search",
  "version": "2.0.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "popup.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "fbnamelogo.png"
  },
  "manifest_version": 2
}
