{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HANDe",
  "description": "My first Chrome extension.",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "SEGP.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "64": "SEGP.png",
    "32": "SEGP.png",
    "16": "SEGP.png",
    "128": "SEGP.png"
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
