{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Removed Sponsored Content",
  "short_name": "R S C",
  "description": "This extension removes sponsored Content from web pages",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "css": [
        "mycss.css"
      ],
      "run_at": "document_idle",
      "js": [
        "jquery.js",
        "jquery-ui.js",
        "myscript.js"
      ]
    }
  ],
  "icons": {
    "16": "RSC.png",
    "48": "RSC.png",
    "128": "RSC.png"
  },
  "permissions": [
    "tabs"
  ]
}
