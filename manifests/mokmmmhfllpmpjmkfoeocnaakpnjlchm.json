{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Globish Space Screen Sharing",
  "version": "1.2",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.globish.co.th/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
