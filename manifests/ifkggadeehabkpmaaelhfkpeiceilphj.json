{
  "background": {
    "scripts": [
      "/js/common.js",
      "/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/img/64.png",
    "default_popup": "/popup.html",
    "default_title": "__MSG_extName__"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+C",
        "mac": "Alt+C"
      }
    }
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "/js/jquery.min.js",
        "/js/common.js",
        "/js/content.js",
        "/js/numeral.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "128": "/img/128.png",
    "32": "/img/32.png",
    "64": "/img/64.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "permissions": [
    "notifications",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "web_accessible_resources": [
    "/img/64.png"
  ]
}
