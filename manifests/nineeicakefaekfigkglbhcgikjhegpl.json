{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "20 Minuten",
  "version": "2.5",
  "description": "Zeigt die aktuellen Nachrichten von 20 Minuten an.",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png"
    },
    "default_title": "20 Minuten",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://*.20min.ch/"
  ],
  "manifest_version": 2
}
