{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Favourite Bookmarks",
  "version": "1.0.4",
  "description": "This extension groups the Bookmarks with respect to the domain and can add more frequetly used as favourite bookmarks.",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Favourite Bookmarks",
    "default_popup": "main.htm",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "storage",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
