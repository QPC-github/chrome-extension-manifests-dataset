{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Agora Web Screensharing | DynEd",
  "permissions": [
    "desktopCapture",
    "management"
  ],
  "short_name": "Screen sharing for Agora",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.0.3",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow screen sharing in Agora Web Application (DynEd Live Sessions).",
  "externally_connectable": {
    "matches": [
      "*://www.dyned.com/*",
      "*://localhost/*",
      "*://52.77.200.151/liveb2itest/*",
      "*://www.liveb2itest.id.dyned.com/*",
      "*://live.dyned.com/*"
    ]
  },
  "icons": {
    "128": "128w.png",
    "16": "16w.png",
    "48": "48w.png"
  },
  "browser_action": {
    "default_icon": "16w.png"
  }
}
