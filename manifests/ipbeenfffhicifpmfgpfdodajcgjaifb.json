{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-latest.js",
        "blockjohnson.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Say goodbye to the Johnson. Forever.",
  "manifest_version": 2,
  "name": "BlockJohnson",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1.4"
}
