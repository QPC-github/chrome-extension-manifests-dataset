{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Netherlands Traffic Information",
  "description": "Visual representation of the traffic jams in Netherlands.",
  "version": "0.2",
  "permissions": [
    "http://*/",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "grey.png",
    "default_popup": "map.html"
  },
  "background": {
    "persistent": true,
    "page": "bg.html"
  }
}
