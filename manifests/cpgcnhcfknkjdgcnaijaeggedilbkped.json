{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Everyday Movies",
  "description": "Introduce for you all interesting movies.",
  "version": "3",
  "author": "TechVSI",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}
