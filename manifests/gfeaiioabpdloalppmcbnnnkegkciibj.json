{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "3o2.co - The URL shortener",
  "description": "This extension makes easier to shorten any URL from browser.",
  "version": "1.1.5",
  "icons": {
    "16": "abtz-favicon.png",
    "48": "abtz-favicon.png",
    "128": "abtz-favicon-128.png"
  },
  "browser_action": {
    "default_icon": "abtz-favicon.png",
    "default_popup": "popup.html",
    "default_title": "3o2.co URL Shortener"
  },
  "permissions": [
    "activeTab",
    "https://3o2.co/"
  ]
}
