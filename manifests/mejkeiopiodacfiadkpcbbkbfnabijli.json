{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "2.6.3",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "homepage_url": "http://www.sappack.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "lib/lodash-2.4.1.js",
      "lib/jquery-2.1.0.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "ftp://*/",
    "nativeMessaging",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.sappack.com/*",
        "http://www.sappack.com/*"
      ],
      "js": [
        "src/check.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "src/popup.html"
  },
  "web_accessible_resources": [
    "icons/*",
    "lib/css/*",
    "src/field_menu.html"
  ],
  "key": ""
}
