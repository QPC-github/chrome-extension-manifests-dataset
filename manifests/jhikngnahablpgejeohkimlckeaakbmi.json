{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "sample browser",
  "manifest_version": 2,
  "description": "Buscador de google con una capa de personalización,para fácil acceso a recursos de Manel",
  "default_locale": "es",
  "version": "3.01",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "Z/index.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://manel-benlloch.esy.es/"
  ]
}
