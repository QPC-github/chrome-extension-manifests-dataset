{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pint Button",
  "description": "Pin anything, anywhere. Armenian Social Network",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Pin this page",
    "default_icon": "img/logo19x19.png"
  },
  "icons": {
    "128": "img/logo_128.png",
    "16": "img/logo16x16.png",
    "48": "img/logo_48.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://www.lovelymms.com/",
    "contextMenus"
  ],
  "version": "1.0"
}
