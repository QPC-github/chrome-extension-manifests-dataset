{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IQDB Search",
  "description": "A lightweight reverse image search, allowing for reliable lookup of many anime/manga style pictures regardless of site.",
  "version": "0.1",
  "background": {
    "scripts": [
      "iqdb.js"
    ]
  },
  "icons": {
    "20": "icon.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
