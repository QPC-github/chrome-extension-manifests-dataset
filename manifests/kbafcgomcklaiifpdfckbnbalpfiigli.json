{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "webextension-polyfill/browser-polyfill.js",
      "whatwg-fetch/fetch.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "LE.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://ec2-3-122-33-34.eu-central-1.compute.amazonaws.com/*"
      ],
      "js": [
        "webextension-polyfill/browser-polyfill.js",
        "contentScript.js"
      ]
    }
  ],
  "server_name": "http://ec2-3-122-33-34.eu-central-1.compute.amazonaws.com",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens medication.html"
    }
  },
  "name": "Léková encyclopedie - medikace",
  "manifest_version": 2,
  "permissions": [
    "alarms",
    "notifications",
    "storage",
    "background",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "version": "2.3"
}
