{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Minimalist New Tab Page",
  "author": "Levi Olson",
  "version": "1.0.8",
  "description": "A minimalist new tab page customizable by you (including weather provided by Dark Sky)",
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newPage.html"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "geolocation"
  ]
}
