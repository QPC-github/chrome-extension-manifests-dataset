{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Inspiration Wall",
  "version": "1.125",
  "description": "Your own personalized wall of website inspiration",
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ],
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  }
}
