{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TIP TV",
  "version": "0.3",
  "description": "Nossa extensão permite assistir varios canais de tv sem anuncios e sem travas",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "TV AO VIVO"
  },
  "icons": {
    "16": "20.png",
    "48": "40.png",
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "vamos ultilizar seu naveador para transmissão dos canais"
  ],
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
