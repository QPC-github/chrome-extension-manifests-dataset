{
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "css": [
        "src/inject/inject.css"
      ],
      "matches": [
        "https://outlook.office.com/*",
        "https://outlook.office365.com/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "Make the new KSUmail Outlook web client look better",
  "homepage_url": "http://ksumail.kennesaw.edu",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "KSUmail Beautifier",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.61"
}
