{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "InLine coeff",
  "description": "Информация о коэфах и основных ставках по линии",
  "version": "1.0.5",
  "browser_action": {
    "default_icon": "img/main.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://*/"
  ],
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  }
}
