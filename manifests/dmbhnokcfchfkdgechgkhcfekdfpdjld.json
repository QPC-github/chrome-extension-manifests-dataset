{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tpay.com",
  "version": "1.4.3",
  "minimum_chrome_version": "18",
  "description": "Automatycznie uzupe≈Çnia dane przelewu bankowego.",
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "idle",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "img/padlock.png",
    "img/navi_ext.gif",
    "img/close.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/core.js",
        "js/const.js",
        "js/msg.js",
        "js/dom.js",
        "js/UI.js",
        "js/transferData.js",
        "js/bankDom.js",
        "js/checker.js",
        "js/logic.js",
        "js/init.js"
      ]
    }
  ]
}
