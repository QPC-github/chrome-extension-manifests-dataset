{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZUNI",
  "version": "1.9",
  "description": "The ZUNI extension makes it easy to keep all your favorites in one place.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "_comment": "default_popup : popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "popup.js"
    ],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "notifications"
  ]
}
