{
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "js/page.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_app_description__",
  "icons": {
    "128": "assets/icon128.png",
    "48": "assets/icon48.png",
    "16": "assets/icon16.png"
  },
  "commands": {
    "add-accents": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Add accents for selected text"
    }
  },
  "manifest_version": 2,
  "name": "__MSG_app_name__",
  "permissions": [
    "contextMenus",
    "<all_urls>",
    "activeTab",
    "chrome://favicon/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2017.3.18.1"
}
