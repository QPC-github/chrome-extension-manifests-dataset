{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WordPress Codex Cheatsheet",
  "description": "Quick access to commonly used WordPress functions",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon-16.png",
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
