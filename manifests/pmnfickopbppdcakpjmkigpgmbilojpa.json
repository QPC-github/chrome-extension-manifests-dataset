{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wikipedia Bağlantı Sağlayıcısı",
  "background": {
    "persistent": false,
    "scripts": [
      "Arkaplan.js"
    ]
  },
  "version": "1.0",
  "manifest_version": 2,
  "description": "Bu Chrome uzantısı Türkiye'de erişimi kısıtlı olan Wikipedia platformuna Wikizero üzerinden erişmenizi sağlar.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
