{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sales Engagement for Pipedrive",
  "description": "Share sales materials by email, get notification when itâ€™s read, take action based on buyer interest",
  "version": "2.1.13",
  "version_name": "2.1.13",
  "manifest_version": 2,
  "minimum_chrome_version": "28.0.0",
  "icons": {
    "16": "icons/icon32.png",
    "48": "icons/icon128.png",
    "128": "icons/icon256.png"
  },
  "browser_action": {
    "default_title": "Yooba Slides for Pipedrive",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png",
      "36": "icons/icon36.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "72": "icons/icon72.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png",
      "512": "icons/icon512.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.pipedrive.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "https://*.pipedrive.com/*",
    "https://*.yooba.com/*",
    "https://*.cloudfront.net/*"
  ],
  "web_accessible_resources": [
    "element-icons.ttf",
    "element-icons.woff"
  ]
}
