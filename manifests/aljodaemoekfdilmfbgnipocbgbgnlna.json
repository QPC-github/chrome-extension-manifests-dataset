{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Workshift",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "A better workshift website interface",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "icon_popup.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "content.js",
        "select2.js"
      ],
      "matches": [
        "http://workshift.bsc.coop/*/online_signoff.php",
        "http://workshift.bsc.coop/",
        "http://workshift.bsc.coop/*"
      ],
      "css": [
        "select2.css",
        "content.css"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://workshift.bsc.coop/stb/online_signoff.php",
    "notifications",
    "http://172.16.1.50:9091/transmission/rpc"
  ],
  "web_accessible_resources": [
    "light.png",
    "squares.png",
    "select2x2.png",
    "jquery.js",
    "48.png"
  ]
}
