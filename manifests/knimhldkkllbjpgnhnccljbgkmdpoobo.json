{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Esports",
  "description": "Helpfully forces the proper spelling of esports in all contexts around the internet.",
  "version": "0.2",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "esports.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  }
}
