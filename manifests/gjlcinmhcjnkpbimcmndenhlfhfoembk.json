{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "manifest_version": 2,
  "version": "1.3.27",
  "homepage_url": "http://www.lockplus.org/welcome.htm",
  "background": {
    "page": "background.htm"
  },
  "icons": {
    "16": "appicon16.png",
    "48": "appicon48.png",
    "128": "appicon128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "idle",
    "cookies",
    "contextMenus",
    "background",
    "browsingData",
    "clipboardWrite",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://secure.lockplus.org; object-src 'self'"
}
