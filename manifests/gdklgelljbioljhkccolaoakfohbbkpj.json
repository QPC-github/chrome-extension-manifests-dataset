{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "engagis.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Block new tabs if they are different than localhost.",
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "name": "Engagis Block New Tabs Extension",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "incognito": "split",
  "browser_action": {
    "default_icon": "engagisfavicon.png"
  },
  "icons": {
    "16": "engagisfavicon.png",
    "48": "engagisfavicon.png",
    "128": "engagisfavicon.png"
  }
}
