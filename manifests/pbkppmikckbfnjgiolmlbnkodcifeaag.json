{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "9gag Enable Video Fullscreen",
  "description": "This extension will force the youtube video to enable fullscreen button.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://9gag.tv/*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "init.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://9gag.tv/*"
  ]
}
