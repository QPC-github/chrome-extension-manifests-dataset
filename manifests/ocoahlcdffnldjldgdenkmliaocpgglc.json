{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Maximize Salesforce Data Rows",
  "description": "Displays maximum number of Salesforce data rows when 'more' link is clicked.",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "permissions": [
    "*://*.salesforce.com/",
    "webNavigation",
    "tabs"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/jq.js",
      "scripts/onload.js"
    ]
  }
}
