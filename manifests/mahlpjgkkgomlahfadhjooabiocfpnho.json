{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PolaroPrint",
  "description": "Create a PDF ready to print with your pictures",
  "version": "1.2.2",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "img/polaroprint16.png",
    "48": "img/polaroprint48.png",
    "128": "img/polaroprint.png"
  },
  "minimum_chrome_version": "11",
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ]
}
