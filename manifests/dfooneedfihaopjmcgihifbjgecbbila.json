{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SMARTRPA",
  "description": "SMARTRPA extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "core.css"
      ],
      "js": [
        "popup.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
