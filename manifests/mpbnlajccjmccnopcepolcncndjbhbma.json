{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Email Signatures by NEWOLDSTAMP",
  "description": "Free Email Signatures Generator for Gmail, GSuite, Outllook, Apple Mail.",
  "version": "1.1",
  "homepage_url": "https://newoldstamp.com",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "manifest.json"
  ],
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "cookies",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webNavigation",
    "http://newoldstamp.com/editor",
    "https://newoldstamp.com/editor/"
  ],
  "browser_action": {
    "default_icon": "images/icon16.png"
  }
}
