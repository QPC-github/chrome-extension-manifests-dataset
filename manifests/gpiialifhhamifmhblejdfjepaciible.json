{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HillarySays",
  "description": "This extension scans a page for text and returns a candidates position on a certain issue",
  "version": "0.0.4",
  "browser_action": {
    "default_icon": {
      "19": "icon0.png"
    },
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "getKeyWordsFromPage.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
