{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "myUVU",
  "version": "0.5.0",
  "description": "Utah Valley University students & faculty access to the myUVU web portal.Not an official version and is not affiliated with UVU.",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "permissions": [
    "webview"
  ]
}
