{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AEA Scout",
  "description": "AEA Scout extension",
  "version": "1.0",
  "icons": {
    "128": "img/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/index.js"
      ],
      "css": [
        "css/index.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "AEA Scout",
    "default_icon": "img/icon.png"
  },
  "web_accessible_resources": [
    "img/*"
  ]
}
