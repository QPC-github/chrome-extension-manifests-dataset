{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Review dropdown replacer",
  "version": "1.8",
  "manifest_version": 2,
  "permissions": [
    "http://*.stackoverflow.com/*",
    "http://*.stackexchange.com/*",
    "http://*.askubuntu.com/*",
    "http://*.superuser.com/*",
    "http://*.serverfault.com/*",
    "http://*.mathoverflow.com/*",
    "http://stackoverflow.com/*",
    "http://askubuntu.com/*",
    "http://superuser.com/*",
    "http://serverfault.com/*",
    "http://mathoverflow.com/*",
    "http://stackapps.com/*",
    "https://*.stackoverflow.com/*",
    "https://*.stackexchange.com/*",
    "https://*.askubuntu.com/*",
    "https://*.superuser.com/*",
    "https://*.serverfault.com/*",
    "https://*.mathoverflow.com/*",
    "https://stackoverflow.com/*",
    "https://askubuntu.com/*",
    "https://superuser.com/*",
    "https://serverfault.com/*",
    "https://mathoverflow.com/*",
    "https://stackapps.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery-1.11.0.min.js",
        "dropdown.user.js"
      ],
      "matches": [
        "http://*.stackoverflow.com/*",
        "http://*.stackexchange.com/*",
        "http://*.askubuntu.com/*",
        "http://*.superuser.com/*",
        "http://*.serverfault.com/*",
        "http://*.mathoverflow.com/*",
        "http://stackoverflow.com/*",
        "http://askubuntu.com/*",
        "http://superuser.com/*",
        "http://serverfault.com/*",
        "http://mathoverflow.com/*",
        "http://stackapps.com/*",
        "https://*.stackoverflow.com/*",
        "https://*.stackexchange.com/*",
        "https://*.askubuntu.com/*",
        "https://*.superuser.com/*",
        "https://*.serverfault.com/*",
        "https://*.mathoverflow.com/*",
        "https://stackoverflow.com/*",
        "https://askubuntu.com/*",
        "https://superuser.com/*",
        "https://serverfault.com/*",
        "https://mathoverflow.com/*",
        "https://stackapps.com/*"
      ],
      "css": [
        "dropdown.css"
      ],
      "run_at": "document_end"
    }
  ]
}
