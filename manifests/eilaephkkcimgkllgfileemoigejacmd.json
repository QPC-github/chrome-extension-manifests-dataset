{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seattle Restaurant Week",
  "description": "This extension will show you Yelp's rating for restaurants",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click me!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://srw.seattletimes.com/*"
      ],
      "css": [
        "core.css"
      ],
      "js": [
        "jquery-2.2.3.min.js",
        "oauth.js",
        "core.js",
        "authen.js",
        "jquery-ui.min.js",
        "sha1.js",
        "prettyprint.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
