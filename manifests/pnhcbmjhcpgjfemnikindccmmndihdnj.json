{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "autoStart",
  "description": "This extension starts new pages from 'autoStart folder in bookmarks",
  "version": "1.0",
  "icons": {
    "128": "action_page.png"
  },
  "browser_action": {
    "default_icon": "action_page_38.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  }
}
