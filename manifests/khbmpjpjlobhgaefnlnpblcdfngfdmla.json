{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Todo list planner",
  "version": "1.0",
  "description": "Opens a new tab with a persistent todo list",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
