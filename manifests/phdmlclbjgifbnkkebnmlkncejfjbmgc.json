{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoAway!",
  "description": "Alt+Shift+Click to remove element from page, Alt+Shift+RightArrow to FastForward playing video.",
  "version": "0.6",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "inject.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2
}
