{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Bookmarks Tab"
  },
  "description": "Replace new tab page with an overview of your bookmarks",
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "manifest_version": 2,
  "name": "Bookmarks Tab",
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "version": "0.0.5"
}
