{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Not So Grey",
  "version": "1.0.8",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "browser_action": {
    "default_icon": "dist/images/nsg-icon-popup.png"
  },
  "icons": {
    "128": "dist/images/app_icon_chrome_128.png"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "background",
    "clipboardWrite",
    "clipboardRead"
  ]
}
