{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarkreader chrome plugin",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A browser action to export your bookmarks to txt file. Then it can be imported from https://bookmarkreader.info",
  "browser_action": {
    "default_title": "BookmarkReader",
    "default_icon": "icon.png"
  },
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
