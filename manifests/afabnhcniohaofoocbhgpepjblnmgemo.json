{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ynet BoolshitThunder",
  "description": "This super-light extension will block ynet's annoying popup",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.ynet.co.il/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
