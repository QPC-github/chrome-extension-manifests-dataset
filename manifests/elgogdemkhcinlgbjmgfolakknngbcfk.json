{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Overplace",
  "version": "2.1",
  "background": {
    "scripts": [
      "facebook_check_box.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "name": "Seleziona tutti i fan Facebook",
    "default_icon": "icona_16x16.png"
  },
  "icons": {
    "16": "icona_16x16.png",
    "48": "icona_48_48.png",
    "128": "icona_128_128.png"
  },
  "manifest_version": 2
}
