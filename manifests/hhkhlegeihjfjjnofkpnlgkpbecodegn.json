{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GW2 Wiki Search",
  "description": "To use, type 'gw2' plus a search term into the Omnibox.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "gw2"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://wiki.guildwars2.com/*"
  ]
}
