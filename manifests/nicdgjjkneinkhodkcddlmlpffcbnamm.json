{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pray to Fart",
  "version": "1.0",
  "description": "Fun for the whole family",
  "icons": {
    "16": "images/iconsmall.png",
    "48": "images/iconmed.png",
    "128": "images/iconbig.png"
