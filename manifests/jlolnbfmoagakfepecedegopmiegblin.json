{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UK Parliment Petition Statistics",
  "description": "Shows a graph of current page's petitions.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "https://petition.parliament.uk/*"
  ],
  "browser_action": {
    "default_icon": {
      "24": "icons/24.png",
      "32": "icons/32.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  }
}
