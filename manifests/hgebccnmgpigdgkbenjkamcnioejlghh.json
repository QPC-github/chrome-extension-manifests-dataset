{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Email All Tabs",
  "description": "This extension lets you email all tabs in your current window.",
  "version": "0.80",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon32.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Email All Tabs"
  }
}
