{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PlusNarrative Development Tools",
  "description": "This PlusNarrative extension adds various web developer tools to your browser",
  "version": "1.0",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ]
}
