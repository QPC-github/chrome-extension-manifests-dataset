{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Veronym Holding GmbH",
  "version": "1.1",
  "description": "Veronym security extension",
  "permissions": [
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery-3.3.1.js",
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "favicon.png"
  },
  "web_accessible_resources": [
    "powerful-prevention.svg"
  ],
  "manifest_version": 2
}
