{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aliexpress Tracking",
  "description": "Расширение позволяет отслеживать статус посылки прямо на странице заказа.",
  "version": "3.2.40",
  "short_name": "AliTrack",
  "permissions": [
    "*://www.edost.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://aliexpress.com/*",
        "*://aliexpress.ru/*",
        "*://*.aliexpress.com/*",
        "*://*.aliexpress.ru/*",
        "*://trade.alibaba.com/*",
        "*://trade.aliexpress.ru/*",
        "*://best.aliexpress.ru/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-1.9.1.js",
        "contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "loader.gif",
    "btnx.png",
    "jquery.min.map",
    "sign.png",
    "dev_label.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
