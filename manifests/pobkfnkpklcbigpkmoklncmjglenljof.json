{
  "background": {
    "scripts": [
      "config.js",
      "js/jquery-3.3.1.min.js",
      "js/superagent.min.js",
      "js/aes.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/content.css"
      ],
      "js": [
        "config.js",
        "js/jquery-3.3.1.min.js",
        "js/aes.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This extension allows a user to select an IOC in a chrome browser and search or submit a highlighted text to an enclave in TruSTAR.",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "TruSTAR",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1.1",
  "web_accessible_resources": [
    "partials/*",
    "images/*"
  ]
}
