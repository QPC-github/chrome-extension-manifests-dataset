{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Eat-together",
  "description": "Integration des notes tripadvisor sur foodora et deliveroo",
  "version": "4.0",
  "permissions": [
    "activeTab",
    "https://eatogether-wagon.herokuapp.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://deliveroo.fr/*",
        "*://*.foodora.fr/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "eat-together.js"
      ],
      "css": [
        "chrome-extension.css"
      ]
    }
  ]
}
