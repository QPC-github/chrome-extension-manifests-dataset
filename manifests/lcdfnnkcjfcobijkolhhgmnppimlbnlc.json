{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "B",
  "short_name": "🅱️",
  "description": "The 🅱️est extension.",
  "version": "1.1",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "*://*/*.css",
        "*://*/*.js",
        "*://*/*.json"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
