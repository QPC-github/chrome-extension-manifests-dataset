{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "io oyunları",
  "description": "Ücretsiz Online IO Oyunları oynayın. IO Oyunları Meb de oyna.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "io"
  }
}
