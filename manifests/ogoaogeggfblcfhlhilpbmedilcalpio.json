{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Password Revealer",
  "description": "Make the masked password readable",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "revealPassword.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make the masked password readable",
    "default_icon": "p.png"
  },
  "manifest_version": 2
}
