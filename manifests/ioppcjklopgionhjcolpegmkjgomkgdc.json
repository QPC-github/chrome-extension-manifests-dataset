{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Geek Beat Live",
  "description": "The Official Geek Beat Live Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "gblive16x16.ico"
  },
  "icons": {
    "128": "gblive128x128.ico",
    "16": "gblive16x16.ico"
  }
}
