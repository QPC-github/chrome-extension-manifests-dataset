  "homepage_url": "http://bestradio.fm/",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/button.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Радио онлайн BestRadio.FM",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications",
    "cookies",
    "webRequest"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
