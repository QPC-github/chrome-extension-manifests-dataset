{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Magic Pop",
  "manifest_version": 2,
  "version": "1.0.7",
  "description": "让你每次打开标签都有一个好心情和提升效率：获取最新最酷的好产品，获取最新 iOS 限免消息",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "magic_pop.js"
    ]
  },
  "browser_action": {
    "default_icon": "./image/MagicPopLOGO.png"
  },
  "chrome_url_overrides": {
    "newtab": "magic_pop.html"
  },
  "permissions": [
    "https://mergeek.com/",
    "activeTab",
    "https://magic-pop.oss-cn-beijing.aliyuncs.com/"
  ]
}
