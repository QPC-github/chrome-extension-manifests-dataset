{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.7",
  "name": "Open And Closes",
  "options_page": "popup.html",
  "description": "This extension will Open and Close tabs by config",
  "browser_action": {
    "default_icon": "images/RedIcon.png",
    "default_title": "Open and Close: Not Running!"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "js": [
        "js/jquery-3.2.1.js",
        "js/jquery.datetimepicker.full.js",
        "js/jquery.weekLine.js",
        "popup.js"
      ],
      "css": [
        "css/jquery.datetimepicker.css",
        "css/cleanslate.css",
        "css/jquery.weekLine.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
