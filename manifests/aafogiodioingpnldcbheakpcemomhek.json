{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SecuhapEx",
  "description": "This extension connect the local resource.",
  "version": "2.0.1",
  "author": "secuhap",
  "content_scripts": [
    {
      "js": [
        "js/contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "nativeMessaging"
  ]
}
