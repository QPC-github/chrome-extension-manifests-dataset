{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Readbble",
  "version": "0.3",
  "description": "Readbble",
  "permissions": [
    "background",
    "identity",
    "tabs",
    "declarativeContent",
    "https://api.readbble.com/*",
    "https://fonts.googleapis.com/*"
  ],
  "browser_action": {
    "default_title": "Open readbble info",
    "default_icon": {
      "16": "icons/logo_extension_16.png",
      "32": "icons/logo_extension_32.png",
      "48": "icons/logo_extension_48.png",
      "128": "icons/logo_extension_128.png"
    },
    "default_popup": "pages/popup.html"
  },
  "icons": {
    "16": "icons/logo_extension_16.png",
    "32": "icons/logo_extension_32.png",
    "48": "icons/logo_extension_48.png",
    "128": "icons/logo_extension_128.png"
  },
  "background": {
    "persistent": false,
    "page": "pages/background.html"
  },
  "manifest_version": 2,
  "key": "",
  "oauth2": {
    "client_id": "76947812450-6q7obgf28np4e414k2k9r2i0lngt3vfu.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
