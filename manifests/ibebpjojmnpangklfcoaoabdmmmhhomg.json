{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tailor cookies",
  "version": "1.2",
  "description": "Tailor - analyze all your cookies",
  "short_name": "Tailor cookies",
  "permissions": [
    "cookies",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "templates/*.html",
    "assets/js/*.js",
    "assets/images/*.png"
  ],
  "browser_action": {
    "default_title": "Tailor cookies",
    "default_icon": "assets/images/icon.png",
    "default_popup": "templates/cookies.html"
  },
  "icons": {
    "128": "assets/images/icon.png"
  }
}
