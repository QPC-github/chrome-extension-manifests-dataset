{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FanatikBet Giriş",
  "description": "FanatikBet in güncel adresine erişim yasağı olmadan bağlanın !",
  "manifest_version": 2,
  "version": "1.0.1",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "script.js"
    ]
  },
  "browser_action": {
    "default_title": "FanatikBet Giriş",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "icons": {
    "128": "icon_128x128.png",
    "256": "icon_256x256.png"
  }
}
