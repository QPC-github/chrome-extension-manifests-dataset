{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitch Moroccan League Streamers",
  "version": "1.1",
  "description": "Pour Savoir si nos Streamers sont enligne !",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "notifications",
    "management"
  ]
}
