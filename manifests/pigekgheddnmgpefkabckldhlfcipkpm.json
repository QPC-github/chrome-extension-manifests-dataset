{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "no caps",
  "version": "1.1.0",
  "author": "9999yea.rs",
  "description": "makes all letters lowercase",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "css": [
        "main.css"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "offline_enabled": true
}
