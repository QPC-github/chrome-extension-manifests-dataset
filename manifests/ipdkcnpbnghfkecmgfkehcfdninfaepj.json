{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Todo Cloud New Tab Page",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Replaces your new tab page with the Todo Cloud application.",
  "icons": {
    "16": "img/icon16.png",
    "19": "img/icon19.png",
    "32": "img/icon32.png",
    "38": "img/icon48.png",
    "48": "img/icon48.png",
    "64": "img/icon48.png",
    "96": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs"
  ]
}
