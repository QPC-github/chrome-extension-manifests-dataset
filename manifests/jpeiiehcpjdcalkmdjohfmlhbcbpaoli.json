{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL Changer",
  "version": "1.3",
  "description": "Paul's URL Changer",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "images/16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
