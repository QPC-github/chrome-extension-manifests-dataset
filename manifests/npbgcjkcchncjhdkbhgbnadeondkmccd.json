{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EasyProjects profiler",
  "description": "Allows to see details of each request to Easy Projects site",
  "version": "0.11",
  "devtools_page": "devtools.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "*://*.easyprojects.net/*",
    "*://*.easyprojects.local/*",
    "*://*.dev.logicsoftware.net/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "imgs/ep_16x16.png",
    "48": "imgs/ep_48x48.png",
    "128": "imgs/ep_128x128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
