{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Gifs",
  "description": "Browse r/gifs in style",
  "version": "1.0",
  "permissions": [
    "contentSettings",
    "http://www.reddit.com/r/gifs/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/r/gifs/*"
      ],
      "js": [
        "lazy.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "lazy.png"
  }
}
