{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Craigslist Hide Utility",
  "description": "Optionally hide listings on Craigslist with this utility",
  "version": "1.0",
  "permissions": [
    "*://*.craigslist.org/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.craigslist.org/*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "src/data_manager.js",
        "src/ui_list.js",
        "src/ui_single.js",
        "craigslist_hide.js"
      ]
    }
  ]
}
