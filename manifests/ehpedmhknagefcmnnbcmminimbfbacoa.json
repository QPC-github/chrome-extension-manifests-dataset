{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aecius",
  "description": "Website Recommendation Engine",
  "version": "1.11",
  "background": {
    "scripts": [
      "history.js"
    ]
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "history",
    "storage",
    "tabs",
    "https://www.aecius.net/*",
    "cookies"
  ]
}
