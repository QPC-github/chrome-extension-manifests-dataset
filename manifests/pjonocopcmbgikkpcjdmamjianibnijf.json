{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DomainStats.co.uk - Seo and Domain analysis",
  "short_name": "DomainStats",
  "description": "Use DomainStats to see your SEO score and improve your site and even see your site income estimation",
  "version": "0.01",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "app.js",
      "background.js"
    ]
  }
}
