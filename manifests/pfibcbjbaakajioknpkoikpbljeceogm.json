{
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "write.js"
      ],
      "matches": [
        "https://www.playbuzz.com/*",
        "http://www.playbuzz.com/*"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "check_page.js"
  ],
  "icons": {
    "128": "icon.png"
  },
  "name": "Тестер",
  "description": "Ответы на тесты в playbuzz.com",
  "permissions": [
    "storage",
    "tabs",
    "*://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.77"
}
