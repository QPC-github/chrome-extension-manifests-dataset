{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Whale Watcher",
  "description": "Get notified when tagged on whaleshares.",
  "version": "0.0.0.1",
  "author": "jepu",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/bootstrap.min.js",
      "js/popup.js",
      "js/@whaleshares/wlsjs/dist/steem.min.js",
      "background.js"
    ]
  }
}
