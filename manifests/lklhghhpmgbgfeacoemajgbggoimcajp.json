{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ad Extractor",
  "author": "Digibit",
  "description": "Load & Export Facebook Ads",
  "version": "2.6.9",
  "permissions": [
    "tabs",
    "downloads",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/adextractor/icon-16.png"
  },
  "icons": {
    "16": "icons/adextractor/icon-16.png",
    "48": "icons/adextractor/icon-48.png",
    "128": "icons/adextractor/icon-128.png"
  },
  "background": {
    "scripts": [
      "base.js",
      "countries.js",
      "posts.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "export.css",
    "overlay.css",
    "export.js",
    "icons/gear.gif",
    "icons/adextractor/logo-128.png"
  ]
}
