{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ACM Paper Formatted Download",
  "description": "Download file name",
  "version": "1.4",
  "permissions": [
    "http://dl.acm.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://dl.acm.org/*"
      ],
      "js": [
        "jquery-1.11.0.min.js",
        "moment.js",
        "myscript.js"
      ]
    }
  ]
}
