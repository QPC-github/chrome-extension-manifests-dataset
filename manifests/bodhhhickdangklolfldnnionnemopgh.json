{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Caesar shift",
  "version": "0.0.1",
  "description": "Changing the text on a page to be encoded with a simple caesar shift. Should be fun when co-workers are not locking their machine",
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/caesar16.png",
    "32": "images/caesar32.png",
    "48": "images/caesar48.png",
    "128": "images/caesar128.png"
  },
  "manifest_version": 2
}
