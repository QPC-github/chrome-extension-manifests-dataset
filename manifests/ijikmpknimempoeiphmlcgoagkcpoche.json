{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Wiki Note",
  "description": "hide wiki note",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://wiki.sankuai.com/*"
      ],
      "js": [
        "hide.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "hide wiki note"
  },
  "manifest_version": 2
}
