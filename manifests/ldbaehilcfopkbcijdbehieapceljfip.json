{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LittleLittlenglish",
  "short_name ": "Littlelittlenglish",
  "description": "Littlelittlenglish her yeni sekmede size farklı bir ingilizce kelimenin anlamını,cümle içinde kullanımını ve okunuşunu gösterir.",
  "version": "1.2",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "http://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "logo2.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "thirdParty/img/icon128.png"
  ],
  "icons": {
    "16": "thirdParty/img/icon16.png",
    "64": "thirdParty/img/icon64.png",
    "128": "thirdParty/img/icon128.png"
  }
}
