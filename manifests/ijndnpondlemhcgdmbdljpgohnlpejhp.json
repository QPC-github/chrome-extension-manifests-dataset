{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Effin A/B Test Calculator",
  "description": "This extension allows you to calculate the statistical significance & conversion rate of your A/B tests.",
  "version": "1.2.3",
  "icons": {
    "16": "images/16icon.png",
    "48": "images/48icon.png",
    "128": "images/128icon.png",
    "512": "images/512icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://*.segment.com https://*.mxpnl.com; object-src 'self'"
}
