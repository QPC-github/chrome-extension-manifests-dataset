{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VPN PROXY MASTER",
  "description": "VPN Proxy Master, 100% free, totally unlimited VPN, secured and easy to use",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon@2x.png",
    "128": "icons/icon@2x.png"
  },
  "permissions": [
    "proxy",
    "unlimitedStorage",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com; object-src 'self'",
  "browser_action": {
    "default_title": "VPN PROXY MASTER",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
