{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Chatroom",
  "description": "Add additional function to dimbo.tv chatroom.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*.memehk.com/*",
        "http://*.dimbo.tv/*",
        "http://*.ppstation.hk/*",
        "http://*.edmondpoon.com/*"
      ],
      "css": [
        "data/app.css",
        "data/emojify.css",
        "data/emojify.nature.css",
        "data/emojify.objects.css",
        "data/emojify.people.css",
        "data/emojify.places.css",
        "data/emojify.symbols.css"
      ],
      "js": [
        "data/mutation-summary.js",
        "data/emojify.js",
        "data/app.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}
