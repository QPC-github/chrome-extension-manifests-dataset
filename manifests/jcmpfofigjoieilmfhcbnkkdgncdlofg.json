{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "25Live Google Calendar Extension",
  "description": "This extension allows the user to interact with Google Calendar via 25Live data.",
  "version": "0.0.0.14",
  "browser_action": {
    "default_icon": "icon_25live.png",
    "default_popup": "google_gadget.html",
    "default_title": "Click here to see available spaces from 25Live!"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "oauth2": {
    "client_id": "566877628774-rc0quc03k6ss5scc6u2bd9fd6f91evc0.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "*://*/",
    "identity",
    "tabs"
  ]
}
