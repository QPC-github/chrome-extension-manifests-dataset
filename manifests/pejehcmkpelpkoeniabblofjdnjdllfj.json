{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Productive StartPage",
  "description": "__MSG_description__",
  "version": "1.0",
  "default_locale": "en",
  "incognito": "split",
  "permissions": [
    "tabs",
    "topSites",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": "19.png"
  },
  "icons": {
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "short_name": "productive-page",
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
