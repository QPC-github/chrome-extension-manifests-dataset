{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/GoRes@16x16.png",
    "default_popup": "popup.html",
    "default_title": "GoRes"
  },
  "description": "An URL redirect tool. Learn from ReRes and Fiddler. Thanks for their great work, especially ReRes.",
  "icons": {
    "128": "img/GoRes@128x128.png",
    "16": "img/GoRes@16x16.png",
    "32": "img/GoRes@32x32.png",
    "48": "img/GoRes@48x48.png"
  },
  "manifest_version": 2,
  "name": "GoRes",
  "options_page": "options.html",
  "permissions": [
    "debugger",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2.0"
}
