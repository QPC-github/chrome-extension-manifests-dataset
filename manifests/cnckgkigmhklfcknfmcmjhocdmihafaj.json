{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bib Bypasser",
  "author": "Kindust.Com",
  "homepage_url": "https://kindust.com",
  "version": "1.0",
  "description": "Bypass some annoying popups that beg you to turn your adblocker off!",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "network.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Bib Bypasser",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
