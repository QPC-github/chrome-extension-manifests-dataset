{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What-If Random Button",
  "description": "Adds a random button to all xkcd what-if pages.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://what-if.xkcd.com/*",
        "http://what-if.xkcd.com/*"
      ],
      "css": [
        "whatifrand.css"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "whatifrand.js"
      ]
    }
  ]
}
