{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Show Apps in new tab",
  "short_name": "Show Apps",
  "description": "This extension shows the apps overview when opening a new tab in Chrome.",
  "version": "2.1.0",
  "chrome_url_overrides": {
    "newtab": "go.html"
  },
  "icons": {
    "128": "icon-128.png",
    "64": "icon-64.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "permissions": [
    "tabs"
  ]
}
