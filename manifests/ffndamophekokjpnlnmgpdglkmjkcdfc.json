{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "16.7.7.2",
  "default_locale": "sk",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "images/dobromat128p.png",
    "32": "images/dobromat_32.png",
    "48": "images/dobromat_48.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/dobromat_19.png",
      "38": "images/dobromat_38.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_popupTitle__"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "box/*"
  ]
}
