{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TapStock Reader",
  "short_name": "TapStock Reader",
  "description": "Read TapStock asset tags using TappyUSB NFC readers",
  "version": "1.2.0",
  "minimum_chrome_version": "41",
  "permissions": [
    "serial"
  ],
  "icons": {
    "16": "res/img/png/icon_16.png",
    "128": "res/img/png/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
