{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pokeitout.",
  "description": "Remove Pokemon and some of it affliated keywords from any website you visit",
  "short_name": "pokeitout",
  "version": "0.0.1",
  "icons": {
    "16": "icon_016.png",
    "32": "icon_032.png",
    "48": "icon_048.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_end"
    }
  ]
}
