{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mav Extension",
  "description": "This extension a test made by Christo Mavrick.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "m.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "browsingData",
    "desktopCapture",
    "tabs",
    "https://localhost:9999/*"
  ]
}
