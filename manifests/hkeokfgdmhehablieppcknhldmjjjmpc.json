{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Telugu News Channels Live",
  "short_name": "Telugu News",
  "description": "Telugu News Channels Live",
  "version": "2.0",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://www.googleapis.com/; object-src 'self'"
}
