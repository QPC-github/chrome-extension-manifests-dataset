{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Name2Email: Find email by name for free",
  "author": "Reply",
  "homepage_url": "https://reply.io",
  "version": "2.2.19",
  "description": "Find anyone's email based on their name right from Gmail for free, quickly and easily",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "transliteration.min.js",
      "jquery-3.4.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mail.google.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    },
    {
      "matches": [
        "*://*.mail.google.com/*"
      ],
      "js": [
        "editor.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "webRequest",
    "tabs",
    "storage",
    "management",
    "*://name2email.com/*",
    "*://mail.google.com/*",
    "*://contacts.google.com/*",
    "*://run.replyapp-stage.net/*",
    "*://api.reply.io/*"
  ]
}
