{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Complemento para descargar XML directamente del SAT. ",
  "manifest_version": 2,
  "name": "CFDX Descarga",
  "version": "3.4.2",
  "homepage_url": "https://natanvilchis.org",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "browser_style": false,
    "default_popup": "popup/choose_page.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "permissions": [
    "*://portalcfdi.facturaelectronica.sat.gob.mx/*",
    "tabs",
    "downloads",
    "activeTab"
  ]
}
