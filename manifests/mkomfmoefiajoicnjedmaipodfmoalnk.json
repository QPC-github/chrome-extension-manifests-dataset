{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "eVanik Online GST Tally",
  "version": "4.7",
  "author": "Harsh Ranjan Das",
  "short_name": "Online GST Tally",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.evanik.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "eVanik Online GST Tally"
  },
  "permissions": [
    "<all_urls>"
  ]
}
