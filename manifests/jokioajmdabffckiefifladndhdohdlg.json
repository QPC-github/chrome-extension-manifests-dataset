{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinkBasket",
  "description": "Save web pages you like to the cloud with LinkBasket",
  "version": "0.0.0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "linkbasketchromeextension.js"
    ]
  },
  "manifest_version": 2,
  "minimum_chrome_version": "6",
  "icons": {
    "19": "images/icon19.png",
    "32": "images/icon32.png"
  }
}
