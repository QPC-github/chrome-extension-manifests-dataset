{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clear GV Inbox",
  "description": "This extension allows you to clear all your google voice messages in one click",
  "version": "0.1",
  "permissions": [
    "tabs",
    "http://www.google.com/voice/*",
    "https://www.google.com/voice/*"
  ],
  "browser_action": {
    "default_title": "Clear GV Inbox",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
