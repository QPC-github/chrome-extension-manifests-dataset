{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rebound",
  "version": "0.0.75",
  "manifest_version": 2,
  "description": "Extension for Rebound -- The web is a big place, we help you remember it.",
  "homepage_url": "http://www.grabRebound.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "contentSettings",
    "cookies",
    "storage",
    "tabs",
    "geolocation",
    "http://*/*",
    "*://use.typekit.net/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery/jquery.min.js",
      "js/moment.min.js",
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://use.typekit.net; object-src 'self'"
}
