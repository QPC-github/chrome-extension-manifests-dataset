{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Cold Search"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://www.coldsearch.com/favicon.png",
      "is_default": true,
      "keyword": "Cold Search",
      "name": "Cold",
      "search_url": "https://www.coldsearch.com/webs?&type=ds&pp=srcupdips&q={searchTerms}",
      "search_url_post_params": ""
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Cold Search setting your browser default search",
  "homepage_url": "https://www.coldsearch.com/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "Cold Search",
  "permissions": [
    "tabs",
    "storage",
    "http://*.df8c.online/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2.4",
  "web_accessible_resources": [
    "check.png"
  ]
}
