{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "nCena",
  "version": "1.01",
  "manifest_version": 2,
  "description": "Replaces 1% of web images with John Cena",
  "icons": {
    "16": "img/16.jpg",
    "48": "img/48.jpg",
    "128": "img/128.jpg"
  },
  "offline_enabled": false,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/jquery-3.2.1.min.js",
        "src/ncena.js"
      ]
    }
  ]
}
