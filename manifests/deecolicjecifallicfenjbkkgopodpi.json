{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fake Routes",
  "description": "Creates Fake express routes for your website, for functional testing.",
  "version": "0.1",
  "permissions": [
    "debugger",
    "downloads"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Fake Roues"
  },
  "manifest_version": 2
}
