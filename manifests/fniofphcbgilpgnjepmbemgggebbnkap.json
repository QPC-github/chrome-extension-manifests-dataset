{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Blocks all photos on Facebook, while still allowing normal navigation (allows Facebook icons, stylesheets, scripts, etc).",
  "background": {
    "scripts": [
      "background.js",
      "chrome.js"
    ]
  },
  "name": "Facebook without photos",
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*.facebook.com/*",
    "https://*.facebook.com/*",
    "http://*.fbcdn.net/*",
    "https://*.fbcdn.net/*"
  ],
  "minimum_chrome_version": "19",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "version": "0.9.2"
}
