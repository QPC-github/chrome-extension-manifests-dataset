{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweet280",
  "description": "This extension lets you to tweet in 280 characters on TweetDeck.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://tweetdeck.twitter.com/*",
        "https://tweetdeck.twitter.com/*"
      ],
      "js": [
        "tw.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
