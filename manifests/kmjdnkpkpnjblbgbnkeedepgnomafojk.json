{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TweetDeck Launcher",
  "manifest_version": 2,
  "version": "2.0.2",
  "background": {
    "scripts": [
      "launcher.js"
    ]
  },
  "permissions": [
    "tabs",
    "management"
  ],
  "browser_action": {
    "default_icon": {
      "19": "assets/24.png",
      "38": "assets/48.png"
    }
  },
  "icons": {
    "16": "assets/16.png",
    "24": "assets/24.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "64": "assets/64.png",
    "128": "assets/128.png"
  }
}
