{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ZocialSky",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Zocial Sky",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_title": "ZocialSky",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://www.zocialsky.com/",
    "http://ajax.googleapis.com/",
    "tabs"
  ],
  "homepage_url": "http://www.zocialsky.com",
  "chrome_url_overrides": {
    "newtab": "zocialsky.html"
  }
}
