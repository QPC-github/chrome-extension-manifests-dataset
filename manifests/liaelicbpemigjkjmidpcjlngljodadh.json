{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "4chan URL",
  "description": "Upload images from URL.",
  "version": "1.0.2",
  "permissions": [
    "*://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.4channel.org/*",
        "*://*.4chan.org/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
