{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pinvents.com - Event Checker",
  "homepage_url": "http://pinvents.com/",
  "version": "1.0.1",
  "minimum_chrome_version": "17",
  "default_locale": "en",
  "icons": {
    "16": "icon-16.jpg",
    "19": "icon-19.png",
    "48": "icon-48.jpg",
    "64": "icon-64.png",
    "128": "icon-128.jpg"
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "Pinvents.com"
  },
  "permissions": [
    "background",
    "alarms",
    "notifications",
    "cookies",
    "geolocation",
    "http://*.pinvents.com/*",
    "https://*.pinvents.com/*",
    "http://pinvents.com/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  }
}
