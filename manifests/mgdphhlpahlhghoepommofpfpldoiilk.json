{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sensible New Tab Landing Page",
  "short_name": "SeNTaLP",
  "description": "Lightweight override of browser's new tab behavior.",
  "author": "RocketCityElectromagnetics",
  "version": "1.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "SensibleNewTab.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "chrome://favicon/"
  ],
  "optional_permissions": [
    "tabs",
    "topSites",
    "management",
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "images/icon82.png"
  },
  "icons": {
    "82": "images/icon82.png"
  },
  "incognito": "split"
}
