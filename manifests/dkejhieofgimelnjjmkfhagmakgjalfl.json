{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RawGIT Viewer",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "RawGIT Viewer puts Github repository HTML assets into the browser view, when available.",
  "browser_action": {
    "default_title": "RawGIT Viewer",
    "default_icon": "img/rawgit_default_19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/rawgit_16.png",
    "19": "img/rawgit_19.png",
    "24": "img/rawgit_24.png",
    "32": "img/rawgit_32.png",
    "48": "img/rawgit_48.png",
    "64": "img/rawgit_64.png",
    "96": "img/rawgit_96.png",
    "128": "img/rawgit_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
