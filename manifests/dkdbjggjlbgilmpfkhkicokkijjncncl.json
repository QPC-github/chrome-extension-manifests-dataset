{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Daniel Franklin",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://xkcd.com; object-src 'self'",
  "description": "A simple New Tab page, which shows you the latest XKCD webcomic.",
  "icons": {
    "128": "images/icon.128.png",
    "16": "images/icon.16.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "name": "New tab page with the latest XKCD",
  "permissions": [
    "activeTab",
    "bookmarks",
    "topSites",
    "storage",
    "http://explainxkcd.com/",
    "https://xkcd.com/"
  ],
  "short_name": "XKCD New Tab",
  "version": "1.2.0"
}
