{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KK学习助手",
  "short_name": "KK学习助手",
  "description": "追踪最新公开课资源, 聪明你的公开课学习, 做你的公开课学习向导！",
  "manifest_version": 2,
  "version": "0.13.2",
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.min.js"
    ]
  },
  "permissions": [
    "tabs",
    "background",
    "notifications",
