{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WakeUp Me",
  "description": "Reminds you to wake up.Autosuggestion begins with creating a positive message to affirm to yourself.",
  "version": "1.0",
  "author": "Andrejs Aleksejevs",
  "homepage_url": "http://prohoney.com",
  "browser_action": {
    "default_icon": "img/sakura96.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery-2.1.4.min.js",
      "js/bootstrap-toggle.min.js",
      "js/run.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "images/*",
    "js/*",
    "css/*"
  ],
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "alarms"
  ],
  "icons": {
    "16": "img/sakura16.png",
    "32": "img/sakura32.png",
    "48": "img/sakura48.png",
    "96": "img/sakura96.png"
  }
}
