{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Carnaval 2013",
  "version": "2",
  "manifest_version": 2,
  "description": "A chrome extension to remind you when certain live streams are available for Youtube Carnaval 2013 coverage.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "http://yt-carnaval2013.appspot.com/",
    "background",
    "notifications",
    "tabs"
  ],
  "web_accessible_resources": [
    "images/add.png",
    "images/bg.jpg",
    "images/carnaval-logo.png",
    "images/carnaval-logo-graphic.png",
    "images/default.png",
    "images/favicon.png",
    "images/icon.png",
    "images/icon16.png",
    "images/icon48.png",
    "images/icon128.png",
    "images/motorola-logo.png",
    "images/remove.png",
    "images/reset.png",
    "images/tick.png"
  ],
  "background": {
    "scripts": [
      "js/Epoch.js",
      "js/Local.js",
      "js/Model.js",
      "js/Ext.js"
    ],
    "persistent": true
  }
}
