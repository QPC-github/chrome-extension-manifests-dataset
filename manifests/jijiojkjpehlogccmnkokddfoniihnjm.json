{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.9",
  "manifest_version": 2,
  "short_name": "privacy-plus",
  "name": "Privacy Protector Plus",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "http://mybrowseraddon.com/privacy-plus.html",
  "options_ui": {
    "chrome_style": true,
    "page": "data/options/options.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "description": "removes all forms of tracking including web-bugs, tracking-scripts and information-collectors, and protects your personal data.",
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
