{
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/libraries/jquery-1.11.3.min.js",
      "scripts/contextMenu.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "views/popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "nooQ Chrome extension which can copy URLs, web page content and highlighted text directly into your nooQ application.",
  "icons": {
    "128": "assets/logo128.png",
    "16": "assets/logo16.png",
    "48": "assets/logo48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "nooQ Web Clipper",
  "options_page": "views/settings.html",
  "permissions": [
    "http://nooq.local/*",
    "http://*.nooq.it/*",
    "https://*.nooq.it/*",
    "storage",
    "contextMenus",
    "notifications",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3"
}
