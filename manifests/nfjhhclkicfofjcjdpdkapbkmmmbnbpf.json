{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open Discord"
  },
  "description": "Adds a button to the toolbar that allows you to open a popup Discord window outside browser tabs.",
  "manifest_version": 2,
  "name": "Discord Popup-Window",
  "version": "1.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
