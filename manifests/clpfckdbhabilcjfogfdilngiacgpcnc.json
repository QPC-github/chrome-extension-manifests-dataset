{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_title": "360Services tools",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "/css/style.css"
      ],
      "js": [
        "build.js"
      ],
      "matches": [
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://*.tmall.hk/*",
        "https://*.tmall.hk/*",
        "http://*.1688.com/*",
        "https://*.1688.com/*"
      ]
    }
  ],
  "description": "Hỗ trợ đặt hàng trên TAOBAO, TMALL, 1688",
  "icons": {
    "128": "images/icon_128.png",
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "64": "images/icon_64.png"
  },
  "manifest_version": 2,
  "name": "360Services Tools",
  "permissions": [
    "activeTab",
    "storage",
    "https://360services.vn/",
    "http://360services.vn/"
  ],
  "version": "1.2.0",
  "web_accessible_resources": [
    "images/*"
  ]
}
