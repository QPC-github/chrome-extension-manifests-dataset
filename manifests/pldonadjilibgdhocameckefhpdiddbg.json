{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unused CSS Tracker",
  "version": "1.4",
  "description": "Tracks the unused CSS selectors in your application",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/track_128.png"
    }
  },
  "icons": {
    "128": "images/track_128.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "manifest_version": 2
}
