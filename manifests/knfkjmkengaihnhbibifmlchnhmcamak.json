{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Switcher",
  "short_name": "TabSwitch",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Application switches two last highlighted tabs by pressing Alt+Space keys combination.",
  "permissions": [],
  "icons": {
    "128": "images/logo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "toggle-tab": {
      "suggested_key": {
        "windows": "Alt+Space",
        "mac": "Alt+Space",
        "linux": "Alt+Space"
      },
      "description": "Tab switch run command"
    }
  }
}
