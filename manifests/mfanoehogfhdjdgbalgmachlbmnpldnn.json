{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Randian hotspot extension",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "This plugin allows users to click on objects within the video to learn more, Example: https://www.youtube.com/watch?v=XbKCyQn5oTc",
  "homepage_url": "http://randian.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "bootstrap/js/bootstrap.min.js",
        "assets/js/spin.js",
        "assets/js/jquery.spin.js",
        "assets/owl-carousel/owl.carousel.js",
        "assets/js/jquery.cycle2.js",
        "assets/js/jquery.cycle2.carousel.js"
      ],
      "css": [
        "assets/css/blocks.css",
        "assets/css/app.css",
        "bootstrap/css/bootstrap.min.css",
        "assets/owl-carousel/owl.carousel.css",
        "custom.css",
        "assets/css/custom_rand.css"
      ],
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Randian"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "https://*.randian.com/*",
    "http://*.randian.com/*",
    "https://qa.randian.com/Catalog/get_item_data/17",
    "tabs"
  ],
  "web_accessible_resources": [
    "assets/css/custom_rand.css"
  ]
}
