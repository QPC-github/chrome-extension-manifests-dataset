{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Isee.IO Desktop Streamer",
  "description": "A extension that allows you to stream your desktop into meetings with Isee.Io Meet.",
  "version": "1.1.0",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "isee-logo-16x16.png",
    "48": "isee-logo-48x48.png",
    "128": "isee-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.isee.io/*"
    ]
  }
}
