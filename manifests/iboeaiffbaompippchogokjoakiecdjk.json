{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Frequency Of Irish Lotto Numbers",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "This plugin/Code gets the past 50 results from the Irish lotto results and orders then most likely to show up to least.",
  "browser_action": {
    "default_icon": "img/logo/lotto.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "img/logo/lotto.png"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "css/bootstrap/*.css",
    "css/myStyles.css",
    "css/fonts/*",
    "js/*.js",
    "dist/css/bootstrap/*.css",
    "dist/css/myStyles.css",
    "dist/css/fonts/*",
    "dist/js/*.js"
  ]
}
