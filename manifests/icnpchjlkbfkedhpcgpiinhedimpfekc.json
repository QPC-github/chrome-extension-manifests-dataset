{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "corgi-resources/bg.js"
    ]
  },
  "content_security_policy": "script-src 'self' ; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "corgi-resources/corgi.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "corgi-resources/logo128.png",
    "16": "corgi-resources/logo16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "corgi-resources/options/options.html",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ],
  "short_name": "__MSG_appName__",
  "version": "1.3.0"
}
