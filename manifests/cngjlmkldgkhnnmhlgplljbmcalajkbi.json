{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Wikizero",
  "version": "1.0",
  "description": "Redirects to the wikizero link when clicked on a wikipedia page",
  "permissions": [
    "webRequest",
    "*://*.wikipedia.org/*",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
