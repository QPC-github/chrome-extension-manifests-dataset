{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Driigo",
  "description": "Load Driigo.com in a New Tab",
  "version": "1.1",
  "background": {
    "page": "redirect.html"
  },
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "200": "icon128.png",
    "128": "icon128.png",
    "19": "f.ico"
  },
  "browser_action": {
    "default_title": "Driigo.com",
    "default_icon": "f.ico"
  }
}
