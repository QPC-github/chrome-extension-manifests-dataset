{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fill form with link data",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Fill form with link data parameters. Use link data param (data-fill-form-params='?q=test&q2=test2').",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "http://*/*"
  ]
}
