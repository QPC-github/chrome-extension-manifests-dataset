{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "complit",
  "manifest_version": 2,
  "version": "0.1.2",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "description": "combine and split tabs.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_title": "complit!"
  }
}
