{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "background": {
    "page": "background/background.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "default_locale": "en",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "name": "__MSG_evil_ext_name__",
  "description": "__MSG_evil_ext_desc__",
  "offline_enabled": true,
  "icons": {
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/48.png"
    },
    "default_title": "__MSG_evil_ext_name__"
  },
  "content_scripts": [
    {
      "matches": [
        "https://search.extfans.com/*"
      ],
      "js": [
        "/content/js/address-bar-search.js"
      ],
      "run_at": "document_start"
    }
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_address_bar_search_name__",
      "keyword": "search",
      "favicon_url": "https://search.extfans.com/query/favicon.png",
      "search_url": "https://search.extfans.com/query/?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "permissions": [
    "topSites",
    "unlimitedStorage",
    "alarms",
    "storage",
    "tabs",
    "cookies",
    "background",
    "management",
    "https://*.extfans.com/",
    "http://suggestion.baidu.com/",
    "http://google.com/",
    "https://query.yahooapis.com/",
    "https://www.yahoo.com/",
    "chrome://favicon/"
  ],
  "version": "19.1.9.1727"
}
