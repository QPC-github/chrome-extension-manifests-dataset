{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Custom Search Tool",
  "version": "1.0.14",
  "description": "Custom Search Tool! Search your keyword in a faster way.",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/3005_y128.png"
    }
  },
  "icons": {
    "128": "images/3005_y128.png"
  },
  "manifest_version": 2
}
