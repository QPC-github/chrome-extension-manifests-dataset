{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unlimited Economist",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Unimited paview access on The Economist",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "cookies",
    "webRequest",
    "*://*.economist.com/*"
  ]
}
