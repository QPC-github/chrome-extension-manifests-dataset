{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ampare Image Enlarger",
  "short_name": "Image Resizer",
  "version": "2.0",
  "description": "Smart Enlarge and Resize Image using Image Processing and Filter With Click of Button",
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Enlarge Image",
    "default_icon": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
