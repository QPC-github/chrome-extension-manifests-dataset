{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/freemybrowser_logo_icon_32x32.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_popupDefaultTitle__"
  },
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "images/freemybrowser_logo_icon_128x128.png",
    "256": "images/freemybrowser_logo_icon_256x256.png",
    "32": "images/freemybrowser_logo_icon_32x32.png",
    "64": "images/freemybrowser_logo_icon_64x64.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.freemybrowser.com https://ssl.google-analytics.com; object-src 'self'",
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "22.0",
  "name": "__MSG_extName__",
  "options_page": "settings.html",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "*://*/*",
    "unlimitedStorage",
    "background",
    "management",
    "webNavigation",
    "cookies",
    "storage",
    "unlimitedStorage"
  ],
  "short_name": "FreeMyBrowser",
  "version": "1.1.15",
  "web_accessible_resources": [
    "images/check.gif"
  ]
}
