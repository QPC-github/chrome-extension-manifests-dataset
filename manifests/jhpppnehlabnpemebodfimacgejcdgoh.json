{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "description": "Search highlighted words on Crunchyroll",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "manifest_version": 2,
  "name": "Crunchyroll search",
  "permissions": [
    "contextMenus"
  ],
  "version": "2.1"
}
