{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Qempo",
  "version": "1.2.2",
  "description": "Qempo extension utilitary to help buyers to make purchases even faster, and travelers to easily have a way to create a flight.",
  "homepage_url": "https://qempo.com",
  "icons": {
    "16": "resources/icons/qempo-16.png",
    "19": "resources/icons/qempo-19.png",
    "38": "resources/icons/qempo-38.png",
    "48": "resources/icons/qempo-48.png",
    "128": "resources/icons/qempo-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [],
  "default_locale": "en",
  "browser_action": {
    "default_icon": "resources/icons/qempo-38.png",
    "default_title": "Qempo"
  },
  "web_accessible_resources": [
    "resources/img/logo-qempo.png",
    "resources/img/disabled.png",
    "resources/templates/purchase.html",
    "resources/templates/traveler.html",
    "resources/utils/aside-builder.js"
  ]
}
