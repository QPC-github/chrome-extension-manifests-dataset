{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Minus",
  "description": "Find coupon code,promotion deal easily !",
  "version": "2.0.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://duckduckgo.com/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://www.unripened-encounter.000webhostapp.com/*",
    "https://www.unripened-encounter.000webhostapp.com/*"
  ],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}
