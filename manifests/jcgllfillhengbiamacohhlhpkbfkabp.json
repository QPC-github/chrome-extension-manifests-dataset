{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HTML Filter",
  "version": "1.2.2",
  "description": "A Simple and easy to use HTML filter that allows you to filter the Site content by Media and Element types.",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
