{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keyposer",
  "short_name": "Keyposer",
  "description": "Use your keys to compose songs.",
  "version": "0.0.0.4",
  "icons": {
    "128": "keyposer_128.png",
    "48": "keyposer_48.png",
    "16": "keyposer_16.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Start Keyposer!",
    "default_icon": "icon.png",
    "default_popup": "background.html"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "downloads",
    "unlimitedStorage"
  ]
}
