{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Catified",
  "short_name": "Catified",
  "description": "Replace new tab page with a Cat",
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "offline_enabled": true,
  "browser_action": {
    "default_title": "New Tab"
  },
  "permissions": [
    "unlimitedStorage"
  ]
}
