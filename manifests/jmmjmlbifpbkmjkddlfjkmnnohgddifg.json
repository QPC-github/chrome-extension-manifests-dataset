{
  "background": {
    "persistent": false,
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [],
      "js": [
        "js/jquery-1.8.3.min.js",
        "js/functions.js",
        "js/eventPage.js"
      ],
      "matches": [
        "*://*.telexfree.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Publish your Telexfree Ads, MyDailyAd.com",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "19": "icon_19.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "MyDailyAd.com 4.1",
  "permissions": [
    "cookies",
    "clipboardWrite",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.1",
  "web_accessible_resources": [
    "icon_48.png",
    "js/eventPage.js",
    "js/jquery-1.8.3.min.js"
  ]
}
