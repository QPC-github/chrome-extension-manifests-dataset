{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Modify Headers for Google Chrome™",
  "version": "2.0.7",
  "manifest_version": 2,
  "description": "Modify Headers for Google Chrome™",
  "icons": {
    "16": "img/mh-icon-16.png",
    "48": "img/mh-icon-48.png",
    "128": "img/mh-icon-128.png"
  },
  "browser_action": {
    "default_icon": "img/mh-icon-48.png",
    "default_title": "Modify Headers for Google Chrome™"
  },
  "background": {
    "scripts": [
      "lib/jquery/dist/jquery.js",
      "lib/jStorage/jstorage.min.js",
      "js/bgp.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com; object-src 'self'",
  "minimum_chrome_version": "18",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "cookies",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "fileSystem",
    "activeTab",
    "<all_urls>"
  ]
}
