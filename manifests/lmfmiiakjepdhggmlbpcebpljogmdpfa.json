{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Something Local",
  "description": "Runs Something Awful's forums.combined.js locally",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://forums.somethingawful.com/*"
      ],
      "js": [
        "jquery.min.js",
        "jquery.migrate.js",
        "forums.combined.js"
      ],
      "run_at": "document_start"
    }
  ]
}
