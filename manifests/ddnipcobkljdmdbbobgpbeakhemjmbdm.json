{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mouseless Stack-Calculator App",
  "version": "0.810",
  "manifest_version": 2,
  "description": "An innovative RPN and stack-calculator with keyboard input (as packaged app)",
  "icons": {
    "32": "c32v01.png",
    "48": "c48v01.png",
    "128": "c128v01.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "http://www.ecb.europa.eu/",
    "storage"
  ],
  "optional_permissions": [
    "clipboardRead",
    "clipboardWrite"
  ]
}
