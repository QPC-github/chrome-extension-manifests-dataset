{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Discord Pop Out Channels",
  "short_name": "DPOC",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Allows Pop Out channels on Discord",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "*://discordapp.com/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
