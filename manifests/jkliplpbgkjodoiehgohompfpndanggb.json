{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MCU No Form",
  "description": "Bye bye~, disgusting form.",
  "version": "1.0.0",
  "icons": {
    "128": "data/128.png"
  },
  "author": "中勝 鄭, MCU IM",
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "http://*/*",
