{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "G Favicon Fixer",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Return to the old Google favicon!",
  "content_scripts": [
    {
      "js": [
        "mainscript.js"
      ],
      "matches": [
        "*://www.google.com/*",
        "*://google.com/*",
        "*://images.google.com/*",
        "*://books.google.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "oldfavicon.ico"
  ],
  "icons": {
    "16": "oldfavicon16.png",
    "48": "oldfavicon48.png",
    "128": "oldfavicon128.png"
  }
}
