{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Your Racist Uncle",
  "description": "Chrome extension that replaces 'Donald Trump' with 'your racist uncle'",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ]
}
