{
  "author": "West 78 Group Ltd",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://clinic.g-med.com/*",
        "https://*.g-med.com/*"
      ]
    }
  ],
  "description": "This Google Chrome extension brings screen sharing to the G-Med Online Video Clinic",
  "homepage_url": "https://www.g-med.com",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "G-Med Online Video Clinic",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.2",
  "web_accessible_resources": [
    "icon.png"
  ]
}
