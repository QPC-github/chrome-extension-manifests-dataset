{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WikiDonationBlocker",
  "short_name": "WDB",
  "description": "Disables javascript that runs on wikipedia",
  "version": "0.0.1",
  "icons": {
    "16": "wdb_icon_sm.png",
    "48": "wdb_icon_med.png",
    "128": "wdb_icon.png"
  },
  "browser_action": {
    "default_icon": "wdb_icon_med.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contentSettings"
  ]
}
