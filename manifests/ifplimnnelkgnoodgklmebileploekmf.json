{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MrElvilia - AlerteLive",
  "description": "Soyez au courant quand vos streameurs préférés commence à streamer!",
  "version": "3.0.0",
  "background": {
    "scripts": [
      "jquery.min.js",
      "parameters.js",
      "background.js",
      "chrome.js"
    ]
  },
  "permissions": [
    "https://api.twitch.tv/",
    "https://api.dailymotion.com/",
    "http://ulas-atila.com/",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "iconoff48.png"
  },
  "icons": {
    "16": "iconoff16.png",
    "48": "iconoff48.png",
    "128": "iconoff128.png"
  }
}
