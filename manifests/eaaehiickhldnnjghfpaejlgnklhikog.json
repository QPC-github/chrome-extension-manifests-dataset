  "browser_action": {
    "default_icon": "icon-large.png",
    "default_popup": "amazon.html"
  },
  "description": "Amazon.com Full Store Directory.",
  "icons": {
    "128": "icon-large.png",
    "48": "icon-small.png",
    "16": "icon-bitty.png"
