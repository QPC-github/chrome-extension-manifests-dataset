{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nielsen - Contact Screen Sharing",
  "author": "Nielsen, Inc.",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "localhost It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://contact.nielsen.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "matches": [
        "https://contact.nielsen.com/*"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "logo16.png"
  ]
}
