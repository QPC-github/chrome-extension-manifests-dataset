{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search Focuser",
  "description": "This extension focuses on the search bar on page load",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://*.amazon.com/*"
      ],
      "run_at": "document_idle",
      "js": [
        "main.js"
      ]
    }
  ]
}
