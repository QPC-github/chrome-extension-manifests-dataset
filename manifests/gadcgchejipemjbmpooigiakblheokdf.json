{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyInterests",
  "short_name": "MyInterests",
  "description": "Organize your favorite interests. Organizuj si své oblíbené webové stránky.",
  "version": "0.1.4",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "mytabs.html"
  },
  "icons": {
    "32": "iconMI32x32.png",
    "64": "iconMI64x64.png",
    "96": "iconMI96x96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "history",
    "tabs",
    "storage",
    "downloads",
    "chrome://favicon/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://*.googleapis.com; object-src 'self' "
}
