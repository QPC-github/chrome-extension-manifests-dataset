{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Doorbell Hub",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Resource For Best Wireless Doorbells, Security Locks and Cams",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "identity",
    "cookies",
    "contextMenus",
    "tabs",
    "storage",
    "notifications",
    "https://*/*"
  ]
}
