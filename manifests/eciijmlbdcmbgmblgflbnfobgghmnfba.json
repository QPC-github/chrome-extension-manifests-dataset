{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "./assets/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "./assets/img/pacman-512.png",
    "default_title": "Открыть приложение"
  },
  "description": "Помогает выгрузить и проанализировать вакансии с https://gossluzhba.gov.ru/vacancy",
  "icons": {
    "128": "./assets/img/pacman-512.png",
    "48": "./assets/img/pacman-512.png"
  },
  "web_accessible_resources": [
    "assets/img/*"
  ],
  "manifest_version": 2,
  "name": "Gossluzhba помощник",
  "permissions": [
    "background",
    "*://*/*",
    "tabs",
    "activeTab"
  ],
  "short_name": "GE",
  "version": "1.0.0.2"
}
