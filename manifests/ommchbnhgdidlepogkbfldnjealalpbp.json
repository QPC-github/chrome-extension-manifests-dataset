{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OMAC Themes.",
  "short_name": "OMAC Themes.",
  "description": "Themes for Udacity community discourse.",
  "icons": {
    "128": "i-128.png",
    "48": "i-48.png",
    "16": "i-16.png"
  },
  "content_scripts": [
    {
      "js": [
        "omac.js"
      ],
      "matches": [
        "https://macdiscussions.udacity.com/*",
        "https://discussions.udacity.com/*"
      ]
    }
  ],
  "permissions": [
    "https://macdiscussions.udacity.com/*",
    "https://discussions.udacity.com/*"
  ],
  "manifest_version": 2,
  "version": "2.6.0"
}
