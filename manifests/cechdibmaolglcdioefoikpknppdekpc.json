{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Pirate Bay unblocker",
  "version": "1.15",
  "manifest_version": 2,
  "description": "An extenstion that unblocks The Pirate Bay by redirecting to a proxy",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html"
}
