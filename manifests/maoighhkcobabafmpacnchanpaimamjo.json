{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PicSee",
  "description": "Make your url more attractive",
  "version": "1.3",
  "icons": {
    "128": "images/PicSee_logo_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "https://ajax.googleapis.com/",
    "*://*/*"
  ]
}
