{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stack Onflow",
  "description": "This hides the hot network questions on the Stack Exchange network that you might find irrelevant.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.stackoverflow.com/*",
        "https://*.stackoverflow.com/*",
        "http://*.stackexchange.com/*",
        "https://*.stackexchange.com/*",
        "http://*.stackapps.com/*",
        "https://*.stackapps.com/*",
        "http://*.serverfault.com/*",
        "https://*.serverfault.com/*",
        "http://*.superuser.com/*",
        "https://*.superuser.com/*",
        "http://*.askubuntu.com/*",
        "https://*.askubuntu.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/sof-16.png",
    "48": "icons/sof-48.png",
    "128": "icons/sof-128.png"
  }
}
