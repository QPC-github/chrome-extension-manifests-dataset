{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vestegnens Sprogcenter menu",
  "version": "1.0.3",
  "description": "Vestegnens Sprogcenter menu - GÃ¸r det let at navigere rundt.",
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo19.png"
  },
  "web_accessible_resources": [
    "logo19.png"
  ],
  "permissions": [
    "*://edulife.sprogcenter.net/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://sites.google.com/a/sprogcenter.net",
        "*://sites.google.com/a/sprogcenter.net/*",
        "https://plus.google.com/up/accounts/upgrade",
        "https://plus.google.com/up/accounts/upgrade/*",
        "https://groups.google.com/*",
        "https://plus.google.com/u/*",
        "https://mail.google.com/*",
        "https://drive.google.com/*",
        "https://calendar.google.com/",
        "https://calendar.google.com/*",
        "https://docs.google.com/*",
        "https://maps.google.dk/",
        "https://maps.google.dk/*",
        "*://translate.google.com/",
        "*://translate.google.com/*",
        "*://www.ordbogen.com/",
        "*://www.google.dk/",
        "*://www.google.dk/*",
        "*://www.google.com/",
        "*://www.google.com/*",
        "*://plus.google.com/",
        "*://plus.google.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
