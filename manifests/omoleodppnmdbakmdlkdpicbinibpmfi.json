{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "worldpeace.cc",
  "manifest_version": 2,
  "version": "1.0",
  "background": {
    "scripts": [
      "js/notification.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "default_title": "worldpeace.cc"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "19": "images/icon-19.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "web_accessible_resources": [
    "images/logo.png",
    "images/bk.png"
  ]
}
