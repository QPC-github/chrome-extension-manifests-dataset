{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unrivaled Online Product Reviews",
  "manifest_version": 2,
  "short_name": "OnlineProductReviews",
  "description": "This is your destination for all the latest product release reviews  that will help to transform your results online",
  "version": "2.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "default16.png"
  },
  "icons": {
    "128": "128px_179-1503490585.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
