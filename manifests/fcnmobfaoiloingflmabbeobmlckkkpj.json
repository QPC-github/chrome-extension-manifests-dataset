{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gadget Trace",
  "version": "0.2",
  "description": "Gadget Trace",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "images/logo-32x32.png"
  },
  "permissions": [
    "tabs",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
