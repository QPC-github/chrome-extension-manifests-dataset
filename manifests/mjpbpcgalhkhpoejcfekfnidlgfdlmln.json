{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qubit QA Cookie",
  "version": "1.0",
  "description": "Sets the QA Cookie for The Sub websites",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "cookies",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/mainlogo16.png",
      "32": "images/mainlogo32.png",
      "48": "images/mainlogo48.png",
      "128": "images/mainlogo128.png"
    }
  },
  "icons": {
    "16": "images/mainlogo16.png",
    "32": "images/mainlogo32.png",
    "48": "images/mainlogo48.png",
    "128": "images/mainlogo128.png"
  },
  "manifest_version": 2
}
