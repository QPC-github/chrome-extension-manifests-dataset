{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "mobileView",
  "description": "Mobile Mode Quickly",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": {
      "16": "16x16.png",
      "48": "48x48.png",
      "128": "128x128.png"
    }
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "debugger",
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
