{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "jquery.1.4.2.min.js",
        "inbox.js"
      ],
      "css": [
        "inbox.css"
      ],
      "matches": [
        "http://mail.inbox.lv/*",
        "http://inbox.lv/*",
        "http://www.inbox.lv/*",
        "https://mail.inbox.lv/*",
        "https://inbox.lv/*",
        "https://www.inbox.lv/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Cleaner inbox.",
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png",
    "256": "icon-256x256.png",
    "300": "icon-300x300.png"
  },
  "name": "Cleaner inbox.",
  "permissions": [
    "tabs"
  ],
  "version": "1.0.0.1",
  "web_accessible_resources": [
    "inbox.css"
  ],
  "content_security_policy": "script-src 'self' https://connect.facebook.net https://apis.google.com https://inbox.lv https://mail.inbox.lv https://platform.twitter.com; object-src 'self'",
  "manifest_version": 2
}
