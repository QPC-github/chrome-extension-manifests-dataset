{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-3.1.1.min.js",
        "video-helper-v6.js"
      ],
      "matches": [
        "https://openload.co/*",
        "https://oload.fun/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Redirect instanlty to video, no ads.",
  "manifest_version": 2,
  "name": "Video Helper",
  "permissions": [
    "https://openload.co/*",
    "https://oload.fun/*"
  ],
  "version": "2.3.3",
  "icons": {
    "128": "icos/video-helper-icon.png"
  },
  "web_accessible_resources": [
    "*"
  ]
}
