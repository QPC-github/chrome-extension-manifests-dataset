{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto SOC",
  "version": "1.0.1",
  "description": "A Google Chrome extension that automatically completes SOC's",
  "icons": {
    "128": "french_fries.png"
  },
  "author": "Ben Simpson",
  "browser_action": {
    "name": "Auto SOC",
    "default_icon": "french_fries.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
