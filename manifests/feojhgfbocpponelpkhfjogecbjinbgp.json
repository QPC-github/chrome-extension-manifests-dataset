{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noisey Tabs",
  "description": "Finds all the Noisey tabs",
  "version": "1.1",
  "browser_action": {
    "default_icon": "speaker.png",
    "default_popup": "InfoPane.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    "images/*"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+I",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  }
}
