{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Saver",
  "version": "1.0",
  "description": "Save Tabs in groups to use for later",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "32": "assets/images/tab_icon32.png",
    "16": "assets/images/tab_icon16.png",
    "48": "assets/images/tab_icon48.png",
    "128": "assets/images/tab_icon128.png"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/images/tab_icon16.png",
      "32": "assets/images/tab_icon32.png",
      "48": "assets/images/tab_icon48.png",
      "128": "assets/images/tab_icon128.png"
    }
  },
  "manifest_version": 2
}
