{
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "name": "DimiTech.net",
  "version": "1.0.2.1",
  "description": "С това приложение на DimiTech.net ще сте винаги в крак с новостите в сферата на информационните технологии.",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "DimiTech.net",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.dimitech.net/*",
    "notifications",
    "alarms",
    "bookmarks"
  ],
  "options_page": "options.html",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2
}
