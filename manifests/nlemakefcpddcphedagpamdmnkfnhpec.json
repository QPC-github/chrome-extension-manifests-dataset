{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Newtab changer",
  "version": "0.12",
  "description": "Allows you to change the page opened with a new tab",
  "version_name": "Beta 12",
  "permissions": [
    "tabs",
    "browserSettings",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "128": "icon128.png"
  }
}
