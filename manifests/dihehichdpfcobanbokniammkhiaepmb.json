{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "emotepriority.js"
      ],
      "matches": [
        "*://*.twitch.tv/*"
      ]
    }
  ],
  "description": "Gives emotes priority over usernames when using tab completion on Twitch.tv with BetterTTV installed.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Twitch Tab Name Fix",
  "permissions": [
    "*://*.twitch.tv/*"
  ],
  "version": "1.2",
  "web_accessible_resources": [
    "inject.js"
  ]
}
