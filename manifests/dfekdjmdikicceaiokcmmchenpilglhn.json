{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mainichi",
  "author": "Aimee Sohn",
  "version": "1.0.17",
  "description": "Learn Japanese each time you open a new tab.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "short_name": "mainichi"
}
