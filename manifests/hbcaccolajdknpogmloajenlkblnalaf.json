{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Groovemarklet",
  "version": "0.4.2",
  "manifest_version": 2,
  "description": "Groovemarklet for Chrome",
  "homepage_url": "https://github.com/kampfq/Groovemarklet-Chrome-Extension",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "groovemarklet.js"
  ],
  "browser_action": {
    "default_title": "Groovemarklet"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ]
}
