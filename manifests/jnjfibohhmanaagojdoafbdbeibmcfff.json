{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wencor.com parts search button",
  "short_name": "Wencor search",
  "description": "A fast and easy way to search Wencor.com for parts by number",
  "version": "1.2",
  "icons": {
    "128": "media/PartSearchIcon_1_128x128.png",
    "48": "media/PartSearchIcon_1_48x48.png",
    "16": "media/PartSearchIcon_1_16x16.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "38": "media/PartSearchIcon_1_38x38.png",
      "19": "media/PartSearchIcon_1_19x19.png"
    },
    "default_title": "Click here to search Wencor.com for parts",
    "default_popup": "popup.html"
  }
}
