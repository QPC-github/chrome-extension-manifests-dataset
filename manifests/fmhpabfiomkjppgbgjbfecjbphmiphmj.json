{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PurposeTab - Inspiration & Focus New Tab",
  "author": "PurposeTab",
  "version": "0.0.7.2",
  "description": "See images of your goals, daily habits, to-do's, and more - every time you open a new tab.",
  "offline_enabled": true,
  "incognito": "split",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://app.getbeamer.com/js/beamer-embed.js https://cdn.heapanalytics.com/js/heap-2746501466.js; object-src 'self'",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
