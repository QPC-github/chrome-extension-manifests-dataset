{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Link Webbie",
  "description": "online measurement calculators, random name and number generators, online web tools and more",
  "version": "1.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "browser_action": {
    "default_title": "Open Link Webbie",
    "default_icon": "logo16.png"
  },
  "manifest_version": 2
}
