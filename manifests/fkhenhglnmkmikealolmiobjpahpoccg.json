{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Playkit Grid",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Playkit Grid overlay for comparison and debug",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19-grey.png",
      "38": "images/icon-38-grey.png"
    },
    "default_title": "Playkit Grid"
  }
}
