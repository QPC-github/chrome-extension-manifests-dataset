{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "History Saver",
  "description": "History!",
  "version": "1.0.0.1",
  "icons": {
    "128": "images/logo.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "history",
    "https://raw.githubusercontent.com/*",
    "http://www.avgthreatlabs.com/website-safety-reports/domain/*",
    "https://safeweb.norton.com/report/show/*",
    "http://api.mywot.com/0.4/public_link_json2*",
    "https://sb-ssl.google.com/safebrowsing/api/lookup*",
    "http://api.webthumbnail.org/*",
    "http://sitecheck3.sucuri.net/*"
  ],
  "browser_action": {
    "default_icon": "images/logo.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
