{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SearchSO",
  "version": "1.0",
  "description": "Search tour problem in stack overflow!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icons8-search-24.png"
    }
  },
  "icons": {
    "16": "images/icons8-search-24.png"
  },
  "manifest_version": 2
}
