{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pins",
  "description": "Pins Chrome extension",
  "version": "2.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "Info-48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Add to Pins",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
