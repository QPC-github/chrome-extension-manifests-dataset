{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SaveMoney.es",
  "description": "Busca el mejor precio en todas las tiendas de Amazon en tiempo real y comienza a ahorrar",
  "version": "1.93",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png"
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html",
    "default_title": "SaveMoney.es ahorra comprando en Amazon!"
  },
  "permissions": [
    "tabs",
    "http://savemoney.es/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
