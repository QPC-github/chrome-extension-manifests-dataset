{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "F-Secure Radar Web Scan Recorder",
  "short_name": "Radar Web Scan Recorder",
  "description": "F-Secure Radar Web Scan plugin to record login steps and to limit the scope of the scan",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon-Radar48.png",
    "128": "icons/icon-Radar128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "webNavigation",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "F-Secure Radar Web Scan Recorder",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "version": "2.1.1"
}
