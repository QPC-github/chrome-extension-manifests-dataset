{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Video Logger",
  "description": "This extension logs video requests.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch*",
        "https://www.youtube.com/watch*"
      ],
      "js": [
        "jquery.js",
        "logger.js"
      ]
    }
  ]
}
