{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/common-push-init.js",
      "js/common-push.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png"
    },
    "default_title": "__MSG_AppName__"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "default_locale": "en",
  "description": "__MSG_AppDescription__",
  "icons": {
    "128": "icon-128.png",
    "256": "icon-256.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "512": "icon-512.png",
    "64": "icon-64.png",
    "96": "icon-96.png"
  },
  "manifest_version": 2,
  "name": "__MSG_AppName__",
  "offline_enabled": true,
  "permissions": [
    "topSites",
    "chrome://favicon/*",
    "https://*.wallpaperaddons.com/*",
    "notifications"
  ],
  "version": "1.1"
}
