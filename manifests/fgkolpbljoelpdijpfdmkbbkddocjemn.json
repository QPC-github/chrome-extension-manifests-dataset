{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unblock XtreamYouth Albums",
  "version": "0.0.1",
  "description": "You can download an entire XtreamYouth album using this extension",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "icons": {
    "128": "icons/icon_128.png",
    "48": "icons/icon_48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Unblock XtreamYouth albums",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "js/download.js"
  ]
}
