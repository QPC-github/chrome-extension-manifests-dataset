{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "SOFT112",
  "manifest_version": 2,
  "name": "SOFT112",
  "description": "Enhance your gaming and search experience with Soft112â€™s own Chrome Browser New Tab search extension!",
  "version": "1.0.0.8",
  "short_name": "SOFT112",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "128": "images/icon-128.png"
    },
    "default_popup": "load-new.html"
  },
  "chrome_url_overrides": {
    "newtab": "load-new.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "unlimitedStorage",
    "geolocation",
    "webNavigation",
    "https://*.file-spark.com/",
    "http://defaultsearch.co/",
    "https://flow.lavasoft.com/",
    "https://*.bing.com/",
    "https://*.googleapis.com/",
    "https://*.cloudflare.com/",
    "http://*.googleusercontent.com/",
    "http://ip-api.com/json",
    "https://*.unsplash.com/",
    "https://*.soft112.com/"
  ],
  "content_security_policy": "script-src 'self' ;  object-src 'self'"
}
