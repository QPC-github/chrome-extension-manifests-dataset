{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.3",
  "name": "Game Haven",
  "description": "Play the most popular games in your browser. Get up to date news, tips and tricks delivered to you daily  ",
  "author": "CC",
  "browser_action": {
    "default_title": "Have a good day"
  },
  "chrome_url_overrides": {
    "newtab": "GV.html"
  },
  "permissions": [
    "activeTab"
  ],
  "chrome_settings_overrides": {
    "homepage": "http://www.rpggamerhaven.com/Rec/homepage/hp.html",
    "search_provider": {
      "name": "name.__MSG_url_domain__",
      "keyword": "keyword.__MSG_url_domain__",
      "search_url": "https://www.rpggamerhaven.com/search.html?sub1={searchTerms}",
      "favicon_url": "http://www.rpggamerhaven.com/favicon.ico",
      "suggest_url": "https://www.rpggamerhaven.com/search.html?sub1={searchTerms}",
      "instant_url": "https://www.rpggamerhaven.com/search.html?sub1={searchTerms}",
      "image_url": "https://www.rpggamerhaven.com/search.html?sub1={searchTerms}",
      "search_url_post_params": "search_lang=__MSG_url_domain__",
      "suggest_url_post_params": "suggest_lang=__MSG_url_domain__",
      "instant_url_post_params": "instant_lang=__MSG_url_domain__",
      "image_url_post_params": "image_lang=__MSG_url_domain__",
      "alternate_urls": [
        "https://www.rpggamerhaven.com/search.html?sub1={searchTerms}",
        "https://www.rpggamerhaven.com/search.html?sub1={searchTerms}"
      ],
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://www.rpggamerhaven.com/Rec/homepage/hp.html"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
