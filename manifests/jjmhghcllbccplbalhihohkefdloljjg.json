{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Resimator Oy Video Screen sharing",
  "description": "An extension to allow you to test Twilio Video screen sharing in dresimator app.",
  "version": "0.1.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.resimator.fi/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
