{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Say No To Comic Sans",
  "description": "Say no to Comic Sans",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "NoComicSans.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
