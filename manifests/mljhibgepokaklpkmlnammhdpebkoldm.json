{
  "background": {
    "scripts": [
      "scripts/bg.js"
    ]
  },
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "theme.html"
  },
  "chrome_settings_overrides": {
    "homepage": "http://funsearchbox.com",
    "search_provider": {
      "name": "Fun Search Box",
      "search_url": "http://funsearchbox.com/find/?t={searchTerms}",
      "suggest_url": "http://funsearchbox.com/find/?t={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true,
      "favicon_url": "http://funsearchbox.com/favicon.ico",
      "keyword": "loadz.com"
    }
  },
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "48": "battle-chasers-nightwar-icons/ic48.png",
    "128": "battle-chasers-nightwar-icons/ic128.png"
  },
  "browser_action": {
    "default_title": "Battle Chasers: Nightwar"
  },
  "version": "8.10.15",
  "optional_permissions": [
    "management"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs",
    "topSites"
  ]
}
