{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random Password Generator",
  "version": "1.0.3",
  "description": "Create strong and secure password in a click!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png",
      "128": "img/128.png"
    }
  },
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "manifest_version": 2,
  "author": "https://randompasswordgenerator.org"
}
