{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stopdown",
  "description": "A simple extension that replaces all instances of startup with small project",
  "version": "0.1",
  "icons": {
    "16": "people16.png",
    "64": "people64.png",
    "128": "people128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
