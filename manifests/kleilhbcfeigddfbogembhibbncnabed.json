{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Slang'it",
  "description": "Slang'it is a new extension that is a fast way look up a chat slogan",
  "version": "9.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "browsingData",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "Pictures/icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+S",
        "mac": "MacCtrl+S"
      }
    }
  }
}
