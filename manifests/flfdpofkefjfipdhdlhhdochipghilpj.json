{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WizardPoker",
  "version": "1.0",
  "description": "Replaces 'hearthstone' with 'wizard poker'.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "WizardPoker_Chrome.js"
      ]
    }
  ]
}
