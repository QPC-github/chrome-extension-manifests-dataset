{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Get User ID",
  "description": "This extension will get the User ID",
  "version": "4.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "alarms",
    "identity",
    "identity.email",
    "cookies",
    "https://*.lorensbergs.co.uk/"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
