{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "homepage": "http://search.tabquery.com",
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://search.tabquery.com/favicon.ico",
      "is_default": true,
      "keyword": "search.tabquery.com",
      "name": "SearchTab",
      "search_url": "http://search.tabquery.com/?q={searchTerms}"
    }
  },
  "description": "Set SearchTab as your default search provider",
  "icons": {
    "128": "favicon.png",
    "16": "favicon.png",
    "48": "favicon.png"
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "manifest_version": 2,
  "name": "SearchTab Search Engine",
  "permissions": [
    "http://*.tabquery.com/*",
    "cookies",
    "management"
  ],
  "short_name": "SearchTab Search Engine",
  "version": "0.0.0.5"
}
