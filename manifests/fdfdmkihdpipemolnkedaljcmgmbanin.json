{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "song downloader i12inch",
  "description": "This extension allows you to search for and download songs.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "popup.js",
      "myscript.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>"
  ]
}
