{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sklik plugin",
  "description": "This extension modifies www.sklik.cz/navrh-klicovych-slov. And provides more options for working with data.",
  "version": "2.2",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*.sklik.cz/navrh-klicovych-slov",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
