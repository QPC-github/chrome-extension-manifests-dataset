{
  "background": {
    "scripts": [
      "scripts/back.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_title": "Super Search"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://www.searchi.live/favicon.ico",
      "image_url": "https://www.searchi.live/result.php?q={searchTerms}",
      "image_url_post_params": "",
      "instant_url": "https://www.searchi.live/result.php?q={searchTerms}",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "https://www.searchi.live/",
      "name": "SafeSearch",
      "search_url": "https://www.searchi.live/result.php?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "https://www.searchi.live/result.php?q={searchTerms}",
      "suggest_url_post_params": ""
    },
    "startup_pages": [
      "https://www.searchi.live/"
    ]
  },
  "description": "Makes your search the best ! With your favorite sites in one place.",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png"
  },
  "name": "Super Search",
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "storage",
    "http://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.9"
}
