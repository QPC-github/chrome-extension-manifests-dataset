{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Content Extractor",
  "description": "Extracts and shows only the specific content from configured websites when they are opened.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery-1.10.2.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "options_page": "options.html"
}
