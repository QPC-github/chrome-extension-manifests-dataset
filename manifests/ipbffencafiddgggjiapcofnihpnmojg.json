{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Traffic Map Singapore",
  "version": "2.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Traffic Map Singapore",
    "default_popup": "popup.html"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "manifest_version": 2
}
