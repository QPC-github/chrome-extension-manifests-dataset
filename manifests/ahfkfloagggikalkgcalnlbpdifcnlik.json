{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screensharing for Interpret.World UAT",
  "author": "Interpret.World",
  "version": "2.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This extension enables screensharing for Interpret.World platform.",
  "homepage_url": "https://interpret.world",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://uat.interprefy.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
