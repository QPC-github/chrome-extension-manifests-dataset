{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "1.0.5",
  "default_locale": "ja",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_name": "__MSG_extName__",
    "default_icon": "Speed-Skating-icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "notifications",
    "contextMenus",
    "tabs",
    "*://*/*",
    "topSites"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "topsites.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "128": "Speed-Skating-icon.png"
  }
}
