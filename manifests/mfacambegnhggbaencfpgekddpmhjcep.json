{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "browser_action": {},
  "description": "Replaces images with Pugs",
  "homepage_url": "http://puggin.it",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "manifest_version": 2,
  "name": "Puggin.it v2",
  "permissions": [
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
