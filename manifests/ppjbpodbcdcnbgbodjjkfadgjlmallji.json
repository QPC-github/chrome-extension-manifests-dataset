{
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_popupTitle__"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com http://127.0.0.1 http://localhost https://google.com https://plus.google.com chrome-extension:// chrome://favicon/ chrome://thumb/; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png"
  },
  "manifest_version": 2,
  "name": "Box Newtab",
  "omnibox": {
    "keyword": "Box Newtab"
  },
  "options_page": "options.html",
  "permissions": [
    "management",
    "tabs",
    "history",
    "topSites",
    "webNavigation",
    "pageCapture",
    "debugger",
    "geolocation",
    "bookmarks",
    "activeTab",
    "browsingData",
    "chrome://favicon/",
    "clipboardWrite",
    "contextMenus",
    "cookies",
    "idle",
    "notifications",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
