{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hello Kitty Image Randomizer",
  "description": "Enjoy a Random Hello Kitty Image Search.",
  "version": "1.2",
  "icons": {
    "16": "img/hk.png",
    "48": "img/hk.png",
    "128": "img/hk.png"
  },
  "permissions": [
    "tabs",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "img/hk.png"
  },
  "background": {
    "scripts": [
      "jquery-3.1.1.min.js",
      "retrieveswapi.js"
    ]
  }
}
