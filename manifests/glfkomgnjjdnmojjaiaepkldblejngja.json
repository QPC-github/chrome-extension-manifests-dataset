{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hey, nice jacket",
  "version": "1.0",
  "description": "Official Chrome extension for \"Hey, nice jacket\"",
  "homepage_url": "http://www.heynicejacket.com",
  "minimum_chrome_version": "19",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Hey, nice jacket",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.heynicejacket.com/"
  ],
  "manifest_version": 2
}
