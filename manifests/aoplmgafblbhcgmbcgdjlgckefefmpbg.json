{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "short_name": "__MSG_shrtName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "version": "2.2",
  "browser_action": {
    "default_icon": "icon_24.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_128.png",
    "24": "icon_24.png",
    "48": "icon_128.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "welcome/index.html",
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
