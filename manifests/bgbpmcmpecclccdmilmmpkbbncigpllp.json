{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tunnel Toggle",
  "version": "2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "toggle.js"
    ],
    "persistent": false
  },
  "description": "Toggle SSH tunnel proxy settings on and off with one click",
  "browser_action": {
    "default_icon": "off.png"
  },
  "permissions": [
    "proxy"
  ]
}
