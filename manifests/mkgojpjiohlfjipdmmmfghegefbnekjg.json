{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Revaluate Pro",
  "description": "Revaluate Pro chrome extension",
  "version": "0.5",
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "96": "images/favicon-96x96.png",
    "128": "images/favicon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "js/jquery1-7.js",
        "js/underscore.js",
        "js/inboxsdk.js",
        "js/guage.min.js",
        "js/easy-old.js",
        "js/app.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "web_accessible_resources": [
    "images/stripe.png",
    "images/rev.png",
    "images/paid.png",
    "images/unpaid.png",
    "images/cards/*",
    "sidebarTemplate.html"
  ],
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "96": "images/favicon-96x96.png",
      "128": "images/favicon-128.png"
    },
    "default_popup": "login-popup.html"
  }
}
