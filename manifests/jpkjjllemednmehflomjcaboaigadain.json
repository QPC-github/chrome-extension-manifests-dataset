{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WebHelpSystem [WHS]",
  "description": "This extension helps you to convert your interaction with website into procedure help files",
  "version": "1.1",
  "icons": {
    "128": "128icon.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Action to Text Recorder",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "http://*/",
    "https://*/",
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "tutorial.png",
    "jQuery/nicEditorIcons.gif"
  ]
}
