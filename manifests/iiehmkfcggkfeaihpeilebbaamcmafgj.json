{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Belpost.by tracking number tester",
  "description": "This extension allow track parcel on belpost.by from context menu",
  "version": "1.12",
  "permissions": [
    "*://*/*",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  }
}
