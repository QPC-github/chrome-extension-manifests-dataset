{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CorpQNA Extension",
  "description": "Access CorpQNA directly from Chrome - search, post, and get notifications.",
  "icons": {
    "16": "cq-square-16.png",
    "48": "cq-square-48.png",
    "128": "cq-square-128.png"
  },
  "content_security_policy": "script-src 'self' https://corpqna.com http://localhost:22459; object-src 'self'",
  "version": "1.3.13",
  "background": {
    "page": "background.html"
  },
  "omnibox": {
    "keyword": "cq"
  },
  "browser_action": {
    "default_icon": {
      "19": "cq-square-19.png",
      "38": "cq-square-38.png"
    },
    "default_title": "View CorpQNA Sidebar"
  },
  "manifest_version": 2,
  "permissions": [
    "background",
    "cookies",
    "notifications",
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "cqsb.js"
  ]
}
