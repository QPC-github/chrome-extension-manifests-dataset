{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "pages/background.html"
  },
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_popup": "pages/page_action.html",
    "default_title": "Megma Archive: Using GWG API"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/balloon.css"
      ],
      "js": [
        "js/jQueryUI/js/jquery-1.9.1.min.js",
        "js/balloon.js",
        "js/functions.js",
        "content.js"
      ],
      "match_about_blank": true,
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Megma Dictionary Bubble Archive:  Finnal Year Project FBAS Of IIUI Using Goodwordguide API, Created by Abdullah Riaz 2206 F11-A.",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "Megma Archive and Synonyms: Using GWG API.",
  "omnibox": {
    "keyword": "define"
  },
  "options_page": "pages/options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs",
    "tts"
  ],
  "version": "1.0",
  "web_accessible_resources": [
    "images/spinner.gif",
    "images/close.png",
    "pages/ga.html",
    "pages/options.html"
  ]
}
