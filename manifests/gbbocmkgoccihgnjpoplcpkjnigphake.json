{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "embedvideos.js"
      ],
      "matches": [
        "http://www.texags.com/*",
        "https://www.texags.com/*",
        "http://texags.com/*",
        "https://texags.com/*",
        "http://*.texags.com/*",
        "https://*.texags.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Increased forum functionality for texags.com",
  "icons": {
    "128": "texagsicon.jpeg",
    "48": "taicon.jpeg"
  },
  "manifest_version": 2,
  "name": "TexAgs Embed Videos",
  "short_name": "EBV",
  "version": "1.0.1"
}
