{
  "manifest_version": 2,
  "name": "Sexy New Tab",
  "description": "__MSG_descript__",
  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab",
    "bookmarks",
    "storage",
    "topSites",
    "history",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "/scripts/background.min.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "layout.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "version": "0.10.21.2",
  "version_name": "0.10.21",
  "minimum_chrome_version": "69.0.0",
  "author": "Liubomudr Nazariev"
}
