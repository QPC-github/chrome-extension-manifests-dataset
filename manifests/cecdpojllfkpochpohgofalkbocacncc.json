{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PlexIt",
  "description": "PlexIt for Chrome",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "icon/pi.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "icon/pi.png"
    },
    "default_title": "PlexIt"
  }
}
