{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jigsaw Mockups",
  "version": "0.6.2.5",
  "description": "The sophisticated UX Pal",
  "author": "Shai Kaplun",
  "browser_action": {
    "default_icon": "icon.png",
    "browser_title": "Open Jigsaw"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "MacCtrl+Shift+K"
      },
      "description": "start Jigsaw"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "downloads",
    "unlimitedStorage",
    "https://www.googleapis.com/",
    "https://skaplun.github.io/jigsaw-canvas/"
  ],
  "web_accessible_resources": [
    "patreon.png",
    "positionpointer.html",
    "loader.html",
    "canvas.html",
    "changelog.html",
    "notification.html",
    "colorpicker.html",
    "screenshotprompt.html",
    "templates2.html",
    "template.html",
    "main.html",
    "fonts/glyphicons-halflings-regular.woff2",
    "themes/*",
    "themes/default/assets/fonts/icons.woff2",
    "jquery-3.2.1.min.js"
  ]
}
