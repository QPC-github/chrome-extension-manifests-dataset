{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabyglot",
  "version": "1.0.1",
  "description": "Extension to choose different keyboard layout for each tab.",
  "icons": {
    "128": "pens128.png"
  },
  "browser_action": {
    "default_icon": "pens128.png"
  },
  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
