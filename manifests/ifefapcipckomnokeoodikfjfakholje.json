{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bop.fm Social Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://bop.fm/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://bop.fm/*"
      ],
      "js": [
        "jquery.js",
        "bop_facebook.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
