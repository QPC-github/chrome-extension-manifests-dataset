{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Home Shortcut for Chromebookâ„¢",
  "version": "1.0.5",
  "manifest_version": 2,
  "commands": {
    "goHome": {
      "suggested_key": {
        "default": "Alt+H"
      },
      "description": "Go to Home page"
    }
  },
  "description": "Adds a keyboard shortcut for the Home button.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options39.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
