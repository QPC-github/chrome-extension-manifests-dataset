{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1",
  "name": "Instart Proxy for AdTechDemo",
  "description": "Instart Proxy for Google Chrome on Desktop",
  "author": "pganti",
  "permissions": [
    "webRequest",
    "cookies",
    "proxy",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Instart Proxy AdTech: Disabled"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
