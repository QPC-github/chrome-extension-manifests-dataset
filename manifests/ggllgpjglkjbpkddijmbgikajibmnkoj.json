{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YoutubeIT",
  "short_name": "YoutubeIT",
  "description": "Search selected text in YouTube",
  "author": "Alex Eduardo Chiaranda",
  "homepage_url": "http://www.outofmemory.blog.br/youtube-it/",
  "version": "0.6",
  "icons": {
    "96": "icon_96.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
