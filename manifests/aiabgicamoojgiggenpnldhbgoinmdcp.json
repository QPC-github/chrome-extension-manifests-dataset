{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ampare Grayscale Image To Color",
  "short_name": "Grayscale To Color",
  "version": "1.4",
  "description": "Convert Any Grayscale Image To Color",
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Convert Grayscale Picture To Color",
    "default_icon": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://algorithmia.com; object-src 'self'"
}
