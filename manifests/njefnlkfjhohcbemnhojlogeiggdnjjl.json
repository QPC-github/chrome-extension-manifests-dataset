{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "robochat",
  "description": "робот для брачных агентств",
  "version": "3.5",
  "manifest_version": 2,
  "permissions": [
    "*://robochat.me/*"
  ],
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "js": [
        "base.js"
      ],
      "matches": [
        "*://romancecompass.com/*",
        "*://www.romancecompass.com/*",
        "*://jump4love.com/*",
        "*://www.jump4love.com/*",
        "*://www.j4l.com/*",
        "*://j4l.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
