{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ola Aphrodite - Livestream Notifiers",
  "version": "2.0",
  "description": "Browser extension to know when Ola start a Livestream on twitch.tv",
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  }
}
