{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "더블클릭 스크롤 비활성화",
  "version": "1.0",
  "description": "일부 사이트에서 더블클릭시 스크롤 위치가 변경되는 기능을 비활성화합니다.",
  "author": "koreapyj",
  "short_name": "NODBLCLICKSCROLL",
  "content_scripts": [
    {
      "js": [
        "noscroll-dblclick.user.js"
      ],
      "matches": [
        "http://ppomppu.co.kr/*",
        "http://www.ppomppu.co.kr/*"
      ]
    }
  ],
  "permissions": [
    "http://ppomppu.co.kr/*",
    "http://www.ppomppu.co.kr/*"
  ]
}
