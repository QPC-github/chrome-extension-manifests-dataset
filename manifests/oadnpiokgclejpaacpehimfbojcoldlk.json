{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Geocache Now",
  "description": "Send geocaches coordinates to your smartphone with your camera.",
  "version": "0.0.0.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.geocaching.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "qrcode.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "logo-128.png"
  },
  "browser_action": {
    "default_icon": "logo-128.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
