{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "salestrekker-videoconferencing",
  "version": "0.1.1",
  "description": "SalesTrekker Video Conferencing",
  "keywords": [
    "salestrekker",
    "video conferencing"
  ],
  "homepage": "http://www.salestrekker.com",
  "license": "UNLICENSED",
  "author": {
    "name": "Salestrekker Pty Ltd",
    "email": "support@salestrekker.com",
    "url": "https://www.salestrekker.com"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "50",
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.salestrekker.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
