{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GRIND",
  "description": "Google Rank INDicator - Hi-lights the selected entry on the google search results and displays the position in the organic results.",
  "version": "0.8",
  "author": "Steve Tucker",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "notifications",
    "https://*.google.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  }
}
