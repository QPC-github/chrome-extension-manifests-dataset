{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "和暦変換",
  "version": "1.1",
  "description": "和暦を西暦に変換する",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "tabs",
    "background",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "和暦変換",
    "default_icon": {
      "16": "images/Icon-16.png",
      "32": "images/Icon-32.png",
      "48": "images/Icon-48.png",
      "128": "images/Icon-128.png"
    }
  },
  "icons": {
    "16": "images/Icon-16.png",
    "32": "images/Icon-32.png",
    "48": "images/Icon-48.png",
    "128": "images/Icon-128.png"
  },
  "manifest_version": 2
}
