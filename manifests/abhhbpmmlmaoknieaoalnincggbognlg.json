{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GMaps",
  "version": "1.0",
  "description": "Get latest updated maps and directions.",
  "omnibox": {
    "keyword": "email"
  },
  "icons": {
    "16": "img/maps-ico-16.png",
    "48": "img/maps-ico-48.png",
    "128": "img/maps-ico-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/maps-ico-16.png",
      "48": "img/maps-ico-48.png",
      "128": "img/maps-ico-128.png"
    },
    "default_title": "Maps and Directions"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "tabs",
    "*://soli-taire.club/*"
  ]
}
