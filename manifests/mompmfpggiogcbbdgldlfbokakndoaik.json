{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Blogoinfo"
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "description": "This extension sets your search engine to blogoinfo and customizes your new tab page so you can use it with every search.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": " Better HomePage (blogoinfo.com.pl)",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1"
}
