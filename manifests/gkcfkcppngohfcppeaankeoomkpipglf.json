{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Scroll",
  "description": "Create an icon on webpage and click on it. You can scroll to top/bottom or comments quickly.",
  "version": "1.0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/top.png",
    "images/bottom.png",
    "images/comment.png"
  ]
}
