{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Game of Thrones - New Tab page",
  "description": "Override the new tab page with a new one with Game of Thrones",
  "version": "0.3",
  "homepage_url": "http://free-addons.com",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "privacy",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://json.geoiplookup.io https://query.yahooapis.com; object-src 'self'",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/jquery-3.3.1.js",
      "js/background.js"
    ]
  }
}
