{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Extra fast search in YouTube for You from Help Solutions Team",
  "authors": [
    {
      "name": "Help Solutions Team",
      "email": "pyotr.paladin@gmail.com"
    }
  ],
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "name": "Cool Search",
  "short_name": "Cool Search",
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "optional_permissions": [
    "background",
    "notifications",
    "webNavigation"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webview",
    "webNavigation",
    "webRequest",
    "storage",
    "alarms",
    "notifications"
  ]
}
