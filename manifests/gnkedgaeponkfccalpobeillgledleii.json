{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Devtools Domains",
  "description": "Displays the domains that a page requests with color coded reponses.",
  "version": "0.1.1",
  "devtools_page": "devtools.html",
  "short_name": "Devtools Domains",
  "author": "Brinley Craig, Stacy Gillett, Phillip Slater",
  "minimum_chrome_version": "18",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "128icon.png",
    "64": "64icon.png",
    "32": "32icon.png"
  }
}
