{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BaitBlock",
  "version": "1.1",
  "description": "Crowdsourced clickbait blocking on Twitter. Blocker-in-chief: @CraigCannon.",
  "homepage_url": "http://BaitBlock.org",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "parseScript.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "button1.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    },
    "default_title": "BaitBlock"
  },
  "manifest_version": 2
}
