{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Google Calendar Alerts",
  "description": "Disables the popup alert messages in Google Calendar.",
  "author": "andrew morton <drewish@katherinehouse.com>",
  "homepage_url": "https://github.com/drewish/no_google_calendar_alerts",
  "icons": {
    "128": "icon128.png"
  },
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/*"
      ],
      "js": [
        "no-alert.js"
      ]
    }
  ],
  "manifest_version": 2
}
