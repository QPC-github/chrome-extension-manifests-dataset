{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Super Enlaces",
  "short_name": "SuperEnlaces",
  "version": "3.1",
  "description": "Guardar y capturar enlaces con sus contenidos y organizarlos en grupos. Asignarles inter√©s y asignarles notas.",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "webNavigation"
  ],
  "browser_action": {
    "default_title": "Super Enlaces",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' http://localhost; object-src 'self'"
}
