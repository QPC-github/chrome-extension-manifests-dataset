{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wiki Extension",
  "description": "Redirect The Wiki Page to 0Wiki",
  "version": "0.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Redirect wiki pages to 0wiki"
  },
  "permissions": [
    "webRequest",
    "*://*.wikipedia.org/*",
    "webRequestBlocking"
  ]
}
