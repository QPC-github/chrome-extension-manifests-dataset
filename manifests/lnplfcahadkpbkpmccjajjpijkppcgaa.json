{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Annonce Acacha30 Live",
  "description": "Extension qui permet de savoir quand Acacha30 est en live",
  "version": "1.3",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "notifications",
    "background"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
