{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MUN Program",
  "manifest_version": 2,
  "author": "Spencer Churchill",
  "permissions": [
    "tabs",
    "storage"
  ],
  "short_name": "MUN",
  "version": "9.84",
  "icons": {
    "128": "images/logos/icon.png"
  },
  "description": "A centralized MUN Program for running committee.",
  "background": {
    "scripts": [
      "links/background.js"
    ],
    "persistent": false
  },
