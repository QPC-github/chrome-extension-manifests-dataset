{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better Download",
  "description": "This extension enables customization of download behavior",
  "version": "1.0",
  "author": "Row of Pixels",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "downloads",
    "downloads.shelf"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
