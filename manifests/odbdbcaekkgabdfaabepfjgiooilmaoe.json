{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Disig Web Signer 1.0.7",
  "short_name": "WebSigner",
  "author": "Disig, a.s.",
  "description": "Google Chrome rošírenie pre aplikáciu Disig Web Signer pre podpisovanie dokumentov zaručeným elektronickým podpisom.",
  "version": "0.9.1.0",
  "icons": {
    "16": "icons/dws_16.png",
    "19": "icons/dws_19.png",
    "32": "icons/dws_32.png",
    "38": "icons/dws_38.png",
    "64": "icons/dws_64.png",
    "128": "icons/dws_128.png"
  },
  "default_locale": "sk",
  "background": {
    "scripts": [
      "dispatcher.js",
      "native-host.js",
      "native-host-manager.js",
      "main.js"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
