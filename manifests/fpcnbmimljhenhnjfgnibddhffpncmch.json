{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Text Fu",
  "version": "1.0",
  "description": "Text expansion and substitution engine for textareas.",
  "permissions": [
    "storage",
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery/jquery-2.0.2.min.js",
      "jquery/jquery.textfu.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "TextFu19.png",
    "default_title": "Text Fu"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "minimum_chrome_version": "39",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "jquery/jquery-2.0.2.min.js",
        "jquery/jquery.textfu.js",
        "content_scripts.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "TextFu16.png",
    "48": "TextFu48.png",
    "128": "TextFu128.png",
    "256": "TextFu256.png"
  },
  "manifest_version": 2
}
