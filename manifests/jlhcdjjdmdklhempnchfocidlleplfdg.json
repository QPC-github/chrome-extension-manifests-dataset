{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Link Webbie",
  "version": "1.0",
  "description": "Link Webbie ",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "contextMenus",
    "tabs",
    "bookmarks"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
