{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Audio Fixer",
  "description": "Fix audio on the basis that one side(channel) is playing and the other is not, or one is louder!",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "yt16x16.png",
    "32": "yt32x32.png",
    "64": "yt64x64.png",
    "128": "yt128x128.png",
    "256": "yt256x256.png",
    "512": "yt512x512.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "StereoFixer.js"
      ],
      "css": [
        "StereoFixer.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
