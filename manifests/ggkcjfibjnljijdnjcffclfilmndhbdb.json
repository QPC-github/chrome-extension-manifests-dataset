{
  "browser_action": {
    "default_icon": "19x19.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "scrypt.js",
        "jquery.min.js",
        "jquery.qtip.js"
      ],
      "matches": [
        "*://*.aliexpress.com/*",
        "*://*.ok.ru/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "ru",
  "description": "__MSG_chrome_extension_description__",
  "icons": {
    "128": "128x128.png",
    "48": "48x48.png",
    "16": "16x16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_chrome_extension_name__",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "__MSG_chrome_extension_short_name__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.5",
  "web_accessible_resources": [
    "scrypt.js",
    "jquery.min.js",
    "jquery.qtip.js"
  ]
}
