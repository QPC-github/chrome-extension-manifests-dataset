{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Super Reader FanFiction.net",
  "version": "1.0.1",
  "description": "Makes FanFiction.net a lot more convenient to read",
  "icons": {
    "42": "booksicon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.fanfiction.net/*"
      ],
      "js": [
        "superReader.js"
      ]
    }
  ]
}
