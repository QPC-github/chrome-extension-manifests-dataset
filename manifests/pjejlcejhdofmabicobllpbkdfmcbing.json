{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WD Rocket",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Get empowered in visualizing your Workday data.",
  "homepage_url": "http://www.wdrocket.com/",
  "oauth2": {
    "client_id": "873663459389-2f85m0iunbehgc7a68a2ivt1e15mhkfu.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "key": "",
  "background": {
    "scripts": [
      "js/welcome.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/wdr16.png",
    "default_title": "WD Rocket"
  },
  "default_locale": "en",
  "options_page": "layout.html",
  "icons": {
    "16": "icons/wdr16.png",
    "48": "icons/wdr48.png",
    "128": "icons/wdr128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com/charts/loader.js https://www.google.com/ https://ssl.google-analytics.com https://*.workday.com https://*.myworkday.com https://maps-api-ssl.google.com https://www.gstatic.com/charts/geochart/10/mapfiles/world_COUNTRIES.js https://www.gstatic.com/charts/regioncoder/0/geocodes/countries_en.js https://maps.googleapis.com https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com https://cdnjs.cloudflare.com https://www.google.com; object-src 'self'",
  "permissions": [
    "https://*.google.com/",
    "https://*.googleapis.com/",
    "https://*.workday.com/",
    "https://*.myworkday.com/",
    "http://*.wdrocket.com/",
    "https://maps-api-ssl.google.com/",
    "https://www.gstatic.com/charts/loader.js",
    "https://www.gstatic.com/charts/geochart/10/mapfiles/world_COUNTRIES.js",
    "https://www.gstatic.com/charts/regioncoder/0/geocodes/countries_en.js",
    "storage",
    "tabs",
    "identity",
    "identity.email"
  ]
}
