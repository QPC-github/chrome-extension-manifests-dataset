{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kiosk Keep Awake (System Only)",
  "description": "Keep your kiosk from going to sleep, however allow the screen to be managed via the kiosk app.",
  "version": "0.99",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "power"
  ],
  "icons": {
    "16": "kiosk-icon-16.png",
    "128": "kiosk-icon-128.png"
  },
  "kiosk_enabled": true
}
