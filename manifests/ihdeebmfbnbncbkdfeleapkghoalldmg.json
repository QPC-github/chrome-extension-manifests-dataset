{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stopbot",
  "description": "Easily turn image loading on and off to reduce data usage and speed up page loads.",
  "version": "1.0.4",
  "permissions": [
    "contentSettings"
  ],
  "browser_action": {
    "default_icon": "photos.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
