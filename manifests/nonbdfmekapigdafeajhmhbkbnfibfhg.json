{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitch Legacy Chat",
  "version": "2.1.1",
  "description": "Restores Legacy Chat",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "content.js"
      ]
    }
  ]
}
