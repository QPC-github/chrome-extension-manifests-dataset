{
  "background": {
    "scripts": [
      "js/filter.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon/virtualkeyboard.png",
    "default_title": "Bkav Virtual Keyboard"
  },
  "content_scripts": [
    {
      "css": [
        "css/bkavdialog.css"
      ],
      "js": [
        "js/jquery-1.10.2.min.js",
        "js/contentscripts.js",
        "js/bkavdialog.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Bkav Pro plugin - Tiện ích bảo vệ trình duyệt",
  "homepage_url": "http://www.bkav.com.vn/",
  "icons": {
    "128": "icon/icon128.png",
    "16": "icon/icon16.png",
    "48": "icon/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Bkav Pro plugin - Tiện ích bảo vệ trình duyệt",
  "permissions": [
    "nativeMessaging",
    "<all_urls>",
    "webNavigation",
    "tabs",
    "https://www.facebook.com/*",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1.18",
  "web_accessible_resources": [
    "icon/*.png"
  ]
}
