{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Socialiser",
  "version": "2.18",
  "description": "Make friends via events, groups and other friends",
  "permissions": [
    "activeTab",
    "storage",
    "https://eumetric.com/*",
    "https://m.facebook.com/*",
    "https://www.facebook.com/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "licencing.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/1.png"
    }
  },
  "icons": {
    "128": "images/1.png"
  },
  "manifest_version": 2
}
