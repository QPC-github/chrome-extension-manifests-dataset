{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hudooku",
  "version": "1.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "description": "A global commenting, conversation, question & answer tool - allows you to connect to a discussion about anything, anywhere, anytime!",
  "icons": {
    "128": "browseraction.png"
  },
  "browser_action": {
    "name": "Simply dooku",
    "default_icon": "icon.png",
    "default_title": "Hudooku",
    "default_popup": ""
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://www.Hudooku.com/*"
  ]
}
