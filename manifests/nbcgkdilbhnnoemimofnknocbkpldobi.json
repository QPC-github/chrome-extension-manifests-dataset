{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "#wrap",
  "version": "3.0.0",
  "manifest_version": 2,
  "description": "View all tags on a Tumblr post without clicking “see all”",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://tumblr.com/dashboard/*",
        "*://www.tumblr.com/dashboard/*"
      ],
      "css": [
        "main.css"
      ]
    }
  ]
}
