{
  "name": "Terrific Shopper DS",
  "description": "Chrome Default Search by TerrificSearch lets you quickly search the web from your address bar!",
  "version": "1.0.3",
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "TerrificSearch",
      "keyword": "http://search.terrificshoper.com",
      "search_url": "http://search.terrificshoper.com/find?sid=713&p={searchTerms}",
      "search_url_post_params": "",
      "favicon_url": "http://terrificshoper.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "background": {
    "scripts": [
      "common/background.js"
    ]
  },
  "icons": {
    "128": "logo/logo_128x.png",
    "16": "logo/logo_16x.png",
    "19": "logo/logo_19x.png",
    "48": "logo/logo_48x.png"
  }
}
