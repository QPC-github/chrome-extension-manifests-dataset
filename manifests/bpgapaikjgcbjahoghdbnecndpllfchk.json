{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Weather",
  "manifest_version": 2,
  "description": "Quick weather report based on your location",
  "version": "0.1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://api.openweathermap.org/",
    "storage",
    "geolocation"
  ]
}
