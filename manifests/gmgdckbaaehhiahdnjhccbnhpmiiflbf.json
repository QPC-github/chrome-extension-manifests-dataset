{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UMD Submit Server Modifier",
  "description": "Remove line numbers from submit server",
  "version": "1.0",
  "permissions": [
    "tabs",
    "*://submit.cs.umd.edu/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Remove line numbers"
  },
  "manifest_version": 2
}
