{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Automatically Skip Hulu Ads",
  "description": "A chrome extension that automatically skips Hulu Ads.",
  "version": "1.2",
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "https://www.hulu.com/*"
      ]
    }
  ],
  "icons": {
    "1000": "icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
