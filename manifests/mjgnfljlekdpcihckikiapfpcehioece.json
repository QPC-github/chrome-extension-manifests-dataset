{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Walkthru Player",
  "description": "Player version for domain production 1.0.19",
  "version": "1.0.22.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "lib/jquery321.js",
        "lib/spatial_navigation.js",
        "lib/jquery-ui.min.js",
        "lib/modal.js",
        "lib/selector.js",
        "js/plugin.js",
        "lib/bootstrap-tour-standalone.js",
        "js/config.js",
        "js/html2canvas.min.js",
        "js/utility.js",
        "lib/webcomponents-ce.js",
        "components/popupComp.js",
        "components/feedbackFormComp.js",
        "components/loginFormComp.js",
        "components/modalComp.js",
        "components/screenshotFormComp.js",
        "components/searchListItemsComp.js",
        "components/playComp.js",
        "components/cameraComp.js",
        "lib/pulseposition.js",
        "js/trigger.js",
        "js/silent.js",
        "js/analytics.js",
        "js/report.js",
        "js/entry.js",
        "js/login.js",
        "js/setup.js",
        "js/loading.js",
        "js/playing.js",
        "js/backgroundStorage.js",
        "js/contentToBackground.js",
        "js/backgroundRouter.js"
      ],
      "css": [
        "styles/bootstrap.css",
        "styles/wewalkthruiframe.css",
        "styles/walkthruplayer.css"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "author": "WalkThru"
}
