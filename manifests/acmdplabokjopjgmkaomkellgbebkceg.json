{
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "tab.html?tab=newtab"
  },
  "browser_action": {
    "default_icon": "img/home.png",
    "default_title": "Search Center"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "name": "NewTab",
  "description": "Clean New tab Extension.",
  "icons": {
    "16": "img/home/home16.png",
    "32": "img/home/home32.png",
    "128": "img/home/home128.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.10"
}
