{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "pluginstore.html",
    "default_title": "SketchUcation PluginStore"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "description": "See the most recently released Trimble SketchUp Plugins from SketchUcation in your Chrome browser.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "sketchucation",
  "permissions": [
    "alarms",
    "storage",
    "http://*.sketchucation.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4"
}
