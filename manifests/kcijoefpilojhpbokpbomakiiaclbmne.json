{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "google-my-searchs",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com.ar/*",
        "https://www.google.com/*"
      ],
      "js": [
        "moment.min.js",
        "jquery.min.js",
        "google-my-searchs.js"
      ]
    }
  ],
  "icons": {
    "16": "daisy_16.png",
    "48": "daisy_48.png",
    "128": "daisy_128.png"
  }
}
