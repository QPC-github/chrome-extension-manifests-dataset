{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "fogus",
  "short_name": "fog",
  "version": "1.0.3",
  "description": "The extension fogus/fog helps you focus by blocking webpages according to your time",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "storage",
    "tabs",
    "cookies",
    "webRequestBlocking",
    "webRequest",
    "*://*/"
  ],
  "web_accessible_resources": [
    "blockPage.html"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/images.png",
      "32": "images/images.png",
      "48": "images/images.png",
      "128": "images/images.png"
    }
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/images.png",
    "32": "images/images.png",
    "48": "images/images.png",
    "128": "images/images.png"
  },
  "manifest_version": 2
}
