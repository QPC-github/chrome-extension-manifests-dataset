{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fping",
  "version": "0.1.0",
  "description": "This extension will remove ping from <a href> tags in html",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "icons": {
    "256": "logo.png"
  },
  "content_security_policy": " default-src 'self'"
}
