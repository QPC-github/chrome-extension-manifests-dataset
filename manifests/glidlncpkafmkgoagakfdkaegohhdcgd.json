{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebIZ Tester",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Resize window to different break points to test WebIZ various screen sizes.",
  "homepage_url": "http://www.envisiontechnology.com",
  "author": "John MacKnight",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "WebIZ Tester",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "system",
    "system.cpu",
    "system.memory",
    "system.storage"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "offline_enabled": false
}
