{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Text outstand",
  "description": "Stand out only h tags an p tags",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "text_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "text_19.png"
  }
}
