{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "magiccards.info Commander search",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Adds an easier way search for a Commander on magiccards.info",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://magiccards.info/"
      ],
      "js": [
        "js/content_script.js"
      ]
    }
  ],
  "permissions": [
    "*://magiccards.info/"
  ]
}
