{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "tribal.FM",
  "version": "0.2.0",
  "description": "Get energized to the best tribal house dance music on the Internet at tribal.FM while you browse the web -- all day, every day!",
  "icons": {
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_popup": "popup.html"
  },
  "author": "tribal.FM",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "minimum_chrome_version": "28",
  "permissions": [
    "http://www.tribal.fm/*",
    "tabs",
    "alarms"
  ]
}
