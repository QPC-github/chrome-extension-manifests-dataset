{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Webpage Share by QR Code",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Share web page with wechat by scanning QRCode",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "default.png"
  },
  "browser_action": {
    "default_icon": "default.png",
    "default_popup": "popup.html"
  }
}
