{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Brilliant AS",
  "description": "Opening office files in client",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "https://*.sharepoint.com/*",
    "notifications"
  ],
  "background": {
    "scripts": [
      "js/redirect.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
