{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dialstring Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with Dialstring video conference.",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "dialstring-16x16.png",
    "48": "dialstring-48x48.png",
    "128": "dialstring-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.dialstring.com/*"
    ]
  }
}
