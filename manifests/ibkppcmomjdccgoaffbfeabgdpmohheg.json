{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "SFO IT",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "css/reset.css",
        "css/inject.css"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "js/ShopDatabase.js",
        "js/State.js",
        "js/Notification.js",
        "js/Dispatcher.js",
        "js/DatabaseLookup.js",
        "js/Main.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "Verdien gratis en eenvoudig geld voor Scouting Fons Olterdissen via Sponsorkliks",
  "homepage_url": "https://www.sponsorkliks.com/products/commissions.php?club=6540",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png"
  },
  "manifest_version": 2,
  "name": "Scouting Fons Olterdissen SponsorKliks",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "*://*/*"
  ],
  "version": "1.0.1",
  "web_accessible_resources": [
    "images/*"
  ]
}
