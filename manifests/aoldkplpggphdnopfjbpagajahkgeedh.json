{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Treadmill Review Guru",
  "description": "TreadmillReviewGuru.com, has been providing users with honest unbiased treadmil reviews since 90's.",
  "version": "1.1",
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon.png",
    "128": "image/icon.png"
  },
  "browser_action": {
    "default_icon": "image/icon.png",
    "default_popup": "html/popup.html",
    "default_title": "Treadmill Review Guru"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
