{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Clownbulance Fund",
  "description": "",
  "version": "1.0.1",
  "icons": {
    "128": "/img/ff_icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.booking.com/*"
      ],
      "js": [
        "/js/cs-conf.js",
        "/js/cs-booking.js"
      ],
      "css": [
        "/css/cs-ff-banner.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.gearbest.com/*",
        "https://www.gearbest.com/*"
      ],
      "js": [
        "/js/cs-conf.js",
        "/js/cs-gearbest.js"
      ],
      "css": [
        "/css/cs-ff-banner.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://*.aliexpress.com/*",
        "https://*.aliexpress.com/*"
      ],
      "js": [
        "/js/cs-conf.js",
        "/js/cs-aliexpress.js"
      ],
      "css": [
        "/css/cs-ff-banner.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/img/*"
  ],
  "permissions": [
    "storage"
  ]
}
