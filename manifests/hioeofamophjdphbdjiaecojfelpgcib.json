{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Sponsored Thread Remover",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Used to get rid of reddit sponsored threads.",
  "short_name": "Sponsored Thread Remover",
  "icons": {
    "128": "icon2.png",
    "256": "icon3.png"
  },
  "browser_action": {
    "default_popup": "gui.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/*"
      ],
      "css": [
        "code1.css"
      ]
    }
  ]
}
