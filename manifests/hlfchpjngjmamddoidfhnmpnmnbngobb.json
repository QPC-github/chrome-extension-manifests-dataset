{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Simple Wysiwyg",
  "version": "2.24",
  "manifest_version": 2,
  "description": "Simple Wysiwyg Extension for Chrome browser",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content/browserOverlay.js"
      ],
      "matches": [
        "http://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "minimum_chrome_version": "18.0",
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
