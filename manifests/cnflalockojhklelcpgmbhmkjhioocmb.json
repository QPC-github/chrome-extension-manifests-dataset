{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pocket Colors",
  "version": "1.2",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "storage"
  ],
  "description": "Helps Web Designers to save their colors in one place.",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "author": "Kapil",
  "offline_enabled": true
}
