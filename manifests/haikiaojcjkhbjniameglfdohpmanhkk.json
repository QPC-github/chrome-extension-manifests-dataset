{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SG FirstClass Auto-Login",
  "short_name": "SGFC-auto",
  "version": "0.0.9",
  "description": "Logger automatisk ind p√• Silkeborg Gymnasiums FirstClass-hjemmeside.",
  "author": "Jeppe Klitgaard",
  "options_page": "html/options.html",
  "homepage_url": "https://github.com/dkkline/SGFC_autologin",
  "icons": {
    "16": "icons/16_logo.png",
    "48": "icons/48_logo.png",
    "128": "icons/128_logo.png"
  },
  "web_accessible_resources": [
    "html/options.html"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://puls.sg.dk/*"
      ],
      "js": [
        "js/jquery.js",
        "js/fc_autologin.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "https://puls.sg.dk/*"
  ]
}
