{
  "background": {
    "page": "background/index.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Food HD Live Wallpapers New Tab Theme",
    "default_icon": {
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "content_scripts": [
    {
      "js": [
        "content-scripts/installed.js",
        "content-scripts/event-handler.js"
      ],
      "matches": [
        "*://*.mystart.com/*",
        "*://*.livenewtab.com/*"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.mystart.com/*",
      "*://*.livenewtab.com/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://*.ytimg.com  https://www.googleapis.com https://*.ytimg.com https://*.mystart.com https://*.mystartcdn.com https://*.vmn.net https://api.facebook.com https://*.twitter.com; object-src 'self'",
  "description": "Food can be an adventure when we explore new tastes and cultures. Enjoy live HD wallpapers of tasty food with every new tab.",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "Food HD Live Wallpapers New Tab Theme",
  "offline_enabled": true,
  "permissions": [
    "alarms",
    "bookmarks",
    "management",
    "chrome://favicon/",
    "nativeMessaging",
    "storage",
    "tabs",
    "topSites",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "*://*.mystart.com/*",
    "*://*.livenewtab.com/*"
  ],
  "short_name": "Food HD Live Wallpapers NT",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.18.24"
}
