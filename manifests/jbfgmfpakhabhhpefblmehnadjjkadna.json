{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Canvas Dark Mode",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Dark Mode for Canvas.",
  "homepage_url": "https://degrandis.github.io/Youtube-rss-chrome-plugin/",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "webNavigation",
    "https://canvas.vt.edu/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://canvas.vt.edu/*"
      ],
      "css": [
        "css/styles.css"
      ]
    }
  ]
}
