{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random Text New Tab Page",
  "version": "0.1",
  "description": "Replaces your new tab page wtih random text of your choice",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "New Tab"
  },
  "manifest_version": 2
}
