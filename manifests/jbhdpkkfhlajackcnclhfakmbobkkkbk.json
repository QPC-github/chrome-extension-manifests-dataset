{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "一键添加包裹 - EcoPlus",
  "version": "1.0.2",
  "description": "在淘宝网站上一键添加您的包裹，快速又方便，就在 EcoPlus 中国至马来西亚集运代运专家！",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "http://china.ecoplus.my",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.min.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "taobao.min.js"
      ],
      "css": [
        "taobao.min.css"
      ],
      "matches": [
        "*://*.taobao.com/*"
      ]
    }
  ],
  "permissions": [
    "http://china.ecoplus.my/"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  }
}
