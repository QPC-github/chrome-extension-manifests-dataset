{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "eBay Blacklist",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.ebay.com/*",
        "*://*.ebay.com.au/*",
        "*://*.ebay.co.uk/*",
        "*://*.ebay.com.my/*",
        "*://*.ebay.ca/*",
        "*://*.ebay.com.ar/*",
        "*://*.ebay.at/*",
        "*://*.ebay.be/*",
        "*://*.ebay.com.br/*",
        "*://*.ebay.com.cn/*",
        "*://*.ebay.cz/*",
        "*://*.ebay.dk/*",
        "*://*.ebay.fi/*",
        "*://*.ebay.fr/*",
        "*://*.ebay.de/*",
        "*://*.ebay.gr/*",
        "*://*.ebay.com.hk/*",
        "*://*.ebay.hu/*",
        "*://*.ebay.in/*",
        "*://*.ebay.ie/*",
        "*://*.ebay.it/*",
        "*://*.ebay.nl/*",
        "*://*.ebay.no/*",
        "*://*.ebay.ph/*",
        "*://*.ebay.pl/*",
        "*://*.ebay.pt/*",
        "*://*.ebay.ru/*",
        "*://*.ebay.com.sg/*",
        "*://*.ebay.es/*",
        "*://*.ebay.ch/*",
        "*://*.ebay.co.th/*",
        "*://*.ebay.vn/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "CLAYTON-emoji.png",
    "default_title": "eBay Blacklist",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
