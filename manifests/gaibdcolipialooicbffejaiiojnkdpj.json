{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Upload to Cargo",
  "short_name": "Cargo.KZ",
  "version": "1.1.22",
  "description": "Расширение  CARGO  позволяет быстро и удобно добавлять Ваши грузы на транспортную биржу Cargo.KZ",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "notifications",
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "<all_urls>",
    "cookies"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "img/16x16.png",
      "32": "img/32x32.png",
      "48": "img/48x48.png",
      "64": "img/64x64.png",
      "96": "img/96x96.png",
      "128": "img/128x128.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "img/16x16.png",
    "32": "img/32x32.png",
    "48": "img/48x48.png",
    "64": "img/64x64.png",
    "96": "img/96x96.png",
    "128": "img/128x128.png"
  }
}
