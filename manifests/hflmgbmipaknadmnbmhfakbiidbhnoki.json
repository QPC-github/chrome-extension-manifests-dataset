{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Enlyter",
  "version": "2.1.3",
  "description": "See how websites are using your data in real time",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "*://*.enlyter.com/*",
    "tabs",
    "activeTab"
  ]
}
