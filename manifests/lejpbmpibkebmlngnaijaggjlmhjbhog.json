{
  "manifest_version": 2,
  "name": "mSearch - simple search",
  "short_name": "mSearch",
  "description": "mSearch New Tab page with amazing backgrounds and custom bookmarks",
  "version": "2.0.1",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs",
    "topSites",
    "storage",
    "unlimitedStorage",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icons/128.png",
    "32": "icons/32.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "icons/32.png"
  }
}
