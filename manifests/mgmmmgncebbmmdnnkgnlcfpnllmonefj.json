{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "name": "Magic Clock",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "This is magic polar clock",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "sandbox": {
    "pages": [
      "popup.html"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "minimum_chrome_version": "23"
}
