{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Assembla++",
  "version": "1.3.0",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "description": "Make working with Assembla a little bit easier",
  "author": "Luke Denton <denno020@gmail.com>",
  "content_scripts": [
    {
      "js": [
        "js/content_libraries.js",
        "js/content_script.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "http://*.assembla.com/*",
        "https://*.assembla.com/*"
      ]
    }
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
