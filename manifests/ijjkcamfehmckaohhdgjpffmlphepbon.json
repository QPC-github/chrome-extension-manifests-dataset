{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CerBitShop Your One Stop Shop",
  "manifest_version": 2,
  "short_name": "OnLineStore",
  "description": "CerBitShop is your new OnLine Store that you can visit around the clock",
  "version": "1.1",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_862-1503187983.jpg"
  },
  "icons": {
    "128": "128px_862-1503187998.jpg"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
