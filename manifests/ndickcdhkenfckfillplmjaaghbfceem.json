{
  "name": "Techaeris.com",
  "description": "Get your news from Techaeris.com right in your Chrome browser!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Techaeris.com",
    "default_popup": "main.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "1.0.0.2"
}
