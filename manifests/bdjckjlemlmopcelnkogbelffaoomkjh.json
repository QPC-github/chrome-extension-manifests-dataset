{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Navigation Buttons for YouTubeâ„¢",
  "short_name": "Nav Buttons",
  "version": "0.2",
  "author": "Danny Fratella",
  "description": "Include additional buttons on YouTube to easily access your Video Manager and Analytics from any page.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "*://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "javascript/inject.js"
      ],
      "run_at": "document_end"
    }
  ]
}
