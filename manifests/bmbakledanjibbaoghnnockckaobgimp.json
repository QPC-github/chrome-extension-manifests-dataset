{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Link Unclogger",
  "description": "Bypass the Steam link filter",
  "version": "1.0",
  "background": {
    "scripts": [
      "webrequest.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://steamcommunity.com/"
  ]
}
