{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "description": "PicTab Extension - nice background image, quick access to Horoscope, News, Dreams, Info Date, ect...",
  "icons": {
    "128": "128.png",
    "48": "48.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "128.png"
  },
  "manifest_version": 2,
  "name": "PicTab",
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://apis.google.com/; object-src 'self';",
  "permissions": [
    "activeTab",
    "browsingData",
    "clipboardWrite",
    "contextMenus",
    "cookies",
    "management",
    "idle",
    "notifications",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.3"
}
