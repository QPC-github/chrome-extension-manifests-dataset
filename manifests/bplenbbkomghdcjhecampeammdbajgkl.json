{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ghouri Launcher",
  "description": "Quick Launch any website with different variables",
  "version": "1.0.2",
  "icons": {
    "64": "64x64.png",
    "128": "128x128.png"
  },
  "browser_action": {
    "default_icon": "19x19_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
