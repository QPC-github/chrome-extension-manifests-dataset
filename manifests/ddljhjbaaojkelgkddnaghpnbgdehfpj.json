{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Add support to the omnibox to search the Chromium source code.",
  "name": "Chromium Search",
  "omnibox": {
    "keyword": "src"
  },
  "permissions": [
    "tabs",
    "https://code.google.com/"
  ],
  "version": "5",
  "minimum_chrome_version": "9",
  "manifest_version": 2
}
