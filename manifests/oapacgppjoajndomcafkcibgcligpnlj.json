{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chadk.co/ URL Shortener",
  "description": "This extension will provide you a shortened URL",
  "version": "1.2",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "96": "icon-96.png",
    "256": "icon-256.png"
  },
  "short_name": "chadk.co/",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
