{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RemoveAsk",
  "version": "1.0.0",
  "description": "Removes posts containing links to attention-whoring ask.fm. An implementation of supdegrave's project on github.",
  "homepage_url": "https://github.com/supdegrave/Antiviral/",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "page.js"
  ]
}
