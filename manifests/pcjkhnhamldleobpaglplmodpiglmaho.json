{
  "manifest_version": 2,
  "homepage_url": "http://e-ivanov.ru/projects/imhoreplacer/",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "lib/jquery-1.10.2.js",
        "imho_del.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "description": "Replace 'Imho' onto normal text",
  "icons": {
    "128": "icon48.png",
    "16": "icon19.png",
    "32": "icon48.png",
    "48": "icon48.png"
  },
  "name": "ImhoReplacer",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.0.6"
}
