{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Beepme Angels",
  "description": "Beepme angel Chrome extension",
  "version": "0.0.6",
  "icons": {
    "16": "angels_icon.png",
    "48": "angels_icon.png",
    "128": "angels_icon.png"
  },
  "browser_action": {
    "default_icon": "angels_icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "notifications",
    "<all_urls>",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "angel-sidebar-background.js",
      "jquery-1.9.1.min.js"
    ],
    "persistent": false
  }
}
