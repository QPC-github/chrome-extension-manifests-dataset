{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Super Sahibinden.com",
  "description": "Daha iyi bir sahibinden.com deneyimi i√ßin :)",
  "options_page": "options.html",
  "version": "2.0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.sahibinden.com/ilan/*"
      ],
      "css": [
        "ilan_css.css"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "ilan_scripts.js"
      ]
    },
    {
      "matches": [
        "*://*.sahibinden.com/*"
      ],
      "css": [
        "genel.css"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "genel_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "simge16.png",
      "32": "simge32.png",
      "48": "simge48.png",
      "128": "simge128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Super Sahibinden.com"
  },
  "icons": {
    "16": "simge16.png",
    "32": "simge32.png",
    "48": "simge48.png",
    "128": "simge128.png"
  },
  "permissions": [
    "storage"
  ]
}
