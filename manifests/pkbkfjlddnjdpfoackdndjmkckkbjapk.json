{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "hrConnectum Plugin",
  "short_name": "hrC Plugin",
  "version": "1.2.1",
  "version_name": "1.2.1",
  "description": "hrConnectum Plugin to connect recruiting platforms with the tool.",
  "icons": {
    "16": "icons/128.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "hrConnectum Plugin",
    "default_popup": "pages/popup.html"
  },
  "author": "Ozan Kurt",
  "homepage_url": "https://hrconnectum.com",
  "permissions": [
    "https://www.xing.com/xtm/*",
    "https://xing.com/xtm/*",
    "tabs",
    "identity"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://stats.pusher.com/; object-src 'self'"
}
