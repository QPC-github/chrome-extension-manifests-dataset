{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quran Tab",
  "description": "Quran Tab is islamic new tab page, will keep you inspired by a verse from Quran every time you open a new tab.",
  "version": "2.3.5",
  "manifest_version": 2,
  "permissions": [
    "geolocation",
    "notifications"
  ],
  "icons": {
    "128": "static/imgs/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src"
}
