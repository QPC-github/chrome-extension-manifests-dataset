{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OmniIP Chrome Extension",
  "version": "1.0.2",
  "description": "Ready to use Chrome extension with OmniIP Javascript client",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "omniip.chrome.js",
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "images/ip-icon.png"
  },
  "browser_action": {
    "default_title": "OmniIP",
    "default_popup": "popup.html"
  }
}
