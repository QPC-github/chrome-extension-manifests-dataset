{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pandectas",
  "description": "Tu ayuda para anotaciones en la web",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "board/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "board/js/depends/jquery/external/jquery/jquery.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Anotar en la web"
  },
  "manifest_version": 2
}
