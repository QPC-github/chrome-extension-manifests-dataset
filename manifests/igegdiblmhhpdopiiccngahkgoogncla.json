{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Overlay Note",
  "version": "1.4",
  "description": "overlay note",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "ovnote48.png",
    "128": "ovnote128.png"
  },
  "browser_action": {
    "default_icon": "ovnote48.png",
    "default_title": "Overlay Note"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
