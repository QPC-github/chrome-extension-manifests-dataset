{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Toggle Location",
  "options_page": "options.html",
  "version": "0.3.1",
  "description": "change URL",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon_128.png"
  }
}
