{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Omicron",
  "description": "This extension swaps every o in the page for an omicron",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "window.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
