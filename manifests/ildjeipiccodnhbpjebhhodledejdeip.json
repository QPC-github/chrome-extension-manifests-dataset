{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyanmarFontTagger",
  "version": "2.0.2",
  "manifest_version": 2,
  "description": "Fill the gap between Zawgyi-One and Unicode",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
