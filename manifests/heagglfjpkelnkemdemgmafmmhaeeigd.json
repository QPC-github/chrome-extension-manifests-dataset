{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Sun Online",
  "version": "1.2",
  "description": "Latest news and updates from Maldives and around the world. http://sun.mv/",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Sun Online",
    "default_icon": "news_action.png",
    "default_popup": "feed.html"
  },
  "permissions": [
    "tabs",
    "http://www.sun.mv/*",
    "storage"
  ],
  "manifest_version": 2
}
