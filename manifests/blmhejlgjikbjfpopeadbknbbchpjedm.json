{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quos",
  "version": "1.0.0",
  "description": "See quotes, tips, pictures and quizzes when you open a new tab. You can edit your own collections.",
  "icons": {
    "48": "dist/img/logo48.png"
  },
  "default_locale": "en",
  "homepage_url": "https://rawls-on.com",
  "chrome_url_overrides": {
    "newtab": "tab/newtab.html"
  },
  "permissions": [
    "storage"
  ]
}
