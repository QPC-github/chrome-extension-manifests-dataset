{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "blitz",
  "version": "1.0.5",
  "description": "Instantly load test your app from around the world with just a few clicks. No scripting and no test creation.",
  "background": "background.html",
  "browser_action": {
    "default_icon": {
      "19": "icon48.png"
    },
    "default_title": "Blitz this page!",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/",
    "https://*/"
  ]
}
