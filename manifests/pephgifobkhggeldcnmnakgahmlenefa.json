{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RandomDesign",
  "short_name": "RandomDesign",
  "description": "Get a new inspiring webdesign every time you open a new tab.",
  "version": "1.6",
  "author": "Yarmo Mackenbach <yarmo.mackenbach@gmail.com>, Troy Mackenbach <troy.mackenbach@gmail.com>",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "start.html"
  }
}
