{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "abyss",
  "version": "1.0",
  "description": "abyss",
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon16.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon16.png"
    },
    "default_title": "abyss helper",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "contentscript.js"
    ]
  }
}
