{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Purple",
  "version": "1.1.3",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2,
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_title": "Purple",
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html"
  },
  "options_page": "html/options.html",
  "permissions": [
    "proxy",
    "tabs"
  ]
}
