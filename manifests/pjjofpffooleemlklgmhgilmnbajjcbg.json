{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MealSnag",
  "version": "2.2",
  "description": "MealSnag grocery cart auto-populater",
  "permissions": [
    "webNavigation",
    "cookies",
    "https://www.mealsnag.com/",
    "https://.walmart.com/"
  ],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "jquery-3.3.1.min.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
