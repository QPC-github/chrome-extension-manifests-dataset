{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WowWiki2Pedia",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "This extension redirects every Wowwiki page to the WowPedia page.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "wiki2pedia.js"
      ],
      "matches": [
        "*://*.wowwiki.com/*"
      ],
      "run_at": "document_start"
    }
  ]
}
