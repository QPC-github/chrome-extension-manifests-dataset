{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CryptoKit.COSCOShipping Extension",
  "version": "3.2.0.2",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "http://coscoshippingloans.com/*",
      "https://coscoshippingloans.com/*",
      "http://cosco-spypc.lianyirong.com.cn/*",
      "https://cosco-spypc.lianyirong.com.cn/*",
      "http://cosco-spypc.qhhrly.cn/*",
      "https://cosco-spypc.qhhrly.cn/*",
      "http://cosco-spypc.hrlyit.com/*",
      "https://cosco-spypc.hrlyit.com/*",
      "https://*.coscoshippingloans.com/*",
      "http://*.coscoshippingloans.com/*"
    ]
  }
}
