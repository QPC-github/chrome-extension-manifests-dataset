{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Beautiful Landscape Wallpaper Theme",
  "description": "Set the Beautiful Nature wallpaper on Chrome. ",
  "version": "2.0.0",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "myPage.html"
  },
  "browser_action": {
    "default_icon": "16.png"
  },
  "permissions": [
    "activeTab"
  ]
}
