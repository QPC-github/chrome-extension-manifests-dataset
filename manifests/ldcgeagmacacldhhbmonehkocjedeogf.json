{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sorted Tabs",
  "version": "1.3",
  "description": "Adds a browser action to the toolbar that displays a popup of your tabs. You can then select tabs or close them.",
  "icons": {
    "48": "icons/cado.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/cado.png",
    "default_title": "Sorted Tabs",
    "default_popup": "popup/sort.html"
  }
}
