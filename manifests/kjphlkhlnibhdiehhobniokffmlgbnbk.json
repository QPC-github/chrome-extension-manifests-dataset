{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Obamarmy",
  "description": "An extension that places Obama stickers when you click on newpage",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "index.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "obama.png"
  }
}
