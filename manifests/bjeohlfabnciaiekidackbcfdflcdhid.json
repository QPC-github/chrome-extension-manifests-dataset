{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Not Normal",
  "description": "Reminds users that Trump is a garbage human",
  "version": "0.0.7",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "trumpslatorpro.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "trumpicon.png"
  },
  "icons": {
    "128": "trumpicon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
