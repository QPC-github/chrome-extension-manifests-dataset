{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Multiplayer (Online) Unblocked YupiGames",
  "description": "You can play free 2 player games in the multiplayer games category.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "yupigames"
  }
}
