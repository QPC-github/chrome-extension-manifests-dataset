{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DADO",
  "description": "Randomly discover the most inspiring designers & agencies",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "DADO",
    "default_icon": "dado.png"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 2
}
