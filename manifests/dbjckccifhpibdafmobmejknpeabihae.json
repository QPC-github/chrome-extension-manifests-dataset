{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Start Screenshot Reader",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Start Screenshot Reader",
  "homepage_url": "http://extensionizr.com",
  "options_ui": {
    "page": "src/page_options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Click to activate Screenshot Reader"
  }
}
