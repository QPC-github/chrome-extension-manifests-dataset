{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Managify Organized Bookmarks",
  "description": "View your Managify bookmarks right in the browser",
  "short_name": "organized-Bookmarks",
  "version": "1.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/organized-bookmarks-app.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "storage",
    "tabs"
  ],
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
