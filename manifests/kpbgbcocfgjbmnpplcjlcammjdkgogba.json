{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AVRChick",
  "short_name": "AVRChick",
  "description": "AVRChick is a simple serial uploader for the Arduino and AVR microcontrollers with a serial bootloader.",
  "version": "1",
  "manifest_version": 2,
  "permissions": [
    "serial",
    "notifications",
    "fileSystem"
  ],
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "launch.js"
      ]
    }
  }
}
