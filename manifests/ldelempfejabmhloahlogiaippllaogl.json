{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fancypost Destroyer",
  "description": "This extension brings a semblance of sanity to Fancyposts",
  "version": "1.4.2",
  "permissions": [
    "http://warriorcatsrpg.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://warriorcatsrpg.com/*"
      ],
      "js": [
        "jquery.js",
        "destroyer.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "fancypost_off.png",
    "fancypost_on.png"
  ]
}
