{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WykopPlayerKiller",
  "version": "1.1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.wykop.pl/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
