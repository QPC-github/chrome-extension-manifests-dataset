{
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.7.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Upload images and screenshots to the free Phota.me image hosting service.",
  "icons": {
    "16": "Phota-16.png",
    "32": "Phota-32.png",
    "48": "Phota-48.png",
    "128": "Phota-128.png"
  },
  "name": "Phota.me Image and Screenshot Uploader",
  "browser_action": {
    "default_icon": "Phota-19.png",
    "default_title": "Upload Screenshot"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "Phota-48.png"
  ],
  "version": "1.1"
}
