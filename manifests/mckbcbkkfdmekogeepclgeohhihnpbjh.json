{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Merch Create",
  "version": "3.2",
  "description": "",
  "manifest_version": 2,
  "incognito": "split",
  "icons": {
    "128": "icons/128.png"
  },
  "background": {
    "persistent": true,
    "page": "background/background.html"
  },
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage",
    "<all_urls>"
  ]
}
