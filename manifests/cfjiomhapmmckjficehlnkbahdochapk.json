{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Crawl Filter",
  "version": "0.1",
  "description": "Allows you to view a page as it looks to Google web crawler for indexing.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://www.ft.com/*",
    "http://www.wsj.com/*",
    "https://www.wsj.com/*",
    "http://www.economist.com/*",
    "http://www.nytimes.com/*",
    "https://hbr.org/*",
    "http://www.newyorker.com/*",
    "http://www.forbes.com/*",
    "http://online.barrons.com/*",
    "http://www.barrons.com/*",
    "http://www.investingdaily.com/*",
    "http://realmoney.thestreet.com/*",
    "http://www.washingtonpost.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "gf16.png",
    "48": "gf48.png",
    "128": "gf128.png"
  },
  "manifest_version": 2
}
