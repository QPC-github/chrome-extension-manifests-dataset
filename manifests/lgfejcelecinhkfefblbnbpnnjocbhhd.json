{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GeoIP",
  "short_name": "GeoIp",
  "description": "GeoIP Information tool shows your IP address, state, country and flag.",
  "version": "1.0.1",
  "externally_connectable": {
    "matches": [
      "https://extensionmatrix.com/*"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "https://extensionmatrix.com/geoip/",
    "storage"
  ],
  "browser_action": {
    "default_title": "Set this page's color.",
    "default_icon": "icons/icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "manifest_version": 2
}
