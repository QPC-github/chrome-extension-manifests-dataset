{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "script/background.js",
      "script/jquery.js"
    ]
  },
  "browser_action": {
    "default_title": "AskAbout Quests Checker",
    "default_popup": "popup.html",
    "default_icon": "icon/icon_16.png"
  },
  "description": "Get answers from real people!",
  "icons": {
    "512": "icon/icon_512.png",
    "48": "icon/icon_48.png",
    "16": "icon/icon_16.png",
    "128": "icon/icon_128.png"
  },
  "manifest_version": 2,
  "name": "AskAbout.FM Notifications",
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "version": "1.0"
}
