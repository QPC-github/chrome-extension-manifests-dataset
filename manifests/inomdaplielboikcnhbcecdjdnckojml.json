{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BrowseU",
  "version": "1.2",
  "manifest_version": 2,
  "description": "The Search Engine That Pays Your Student Loans",
  "icons": {
    "128": "Icon.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://browseu.com/",
    "search_provider": {
      "name": "BrowseU",
      "keyword": "browseu.com",
      "search_url": "http://browseu.com/results.php?q={searchTerms}",
      "favicon_url": "http://browseu.com/favicon.ico",
      "search_url_post_params": "search_lang=__MSG_url_domain__",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "permissions": [
    "cookies"
  ]
}
