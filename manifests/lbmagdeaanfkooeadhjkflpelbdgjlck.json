{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FlyInk Custom New Tab | Start Page",
  "permissions": [
    "tabs",
    "history",
    "topSites",
    "bookmarks",
    "*://*/*",
    "chrome://favicon/*"
  ],
  "description": "Custom New Tab | Start Page",
  "version": "2.8",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
