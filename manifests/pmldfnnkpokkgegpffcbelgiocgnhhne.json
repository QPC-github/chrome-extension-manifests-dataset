{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Argemaddon",
  "description": "Calcul gématrique sur sélection de texte / Gematria calculation on selected text",
  "version": "0.0.1",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "tabs",
    "tts"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon.png",
    "64": "icon64.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
