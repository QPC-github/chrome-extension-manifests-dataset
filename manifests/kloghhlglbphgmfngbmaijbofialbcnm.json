{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "XPath tool",
  "icons": {
    "128": "static/icon128.png",
    "32": "static/icon32.png",
    "48": "static/icon48.png"
  },
  "manifest_version": 2,
  "name": "XPath tool",
  "version": "0.0.1",
  "web_accessible_resources": [
    "bar.css",
    "bar.html",
    "bar.js"
  ]
}
