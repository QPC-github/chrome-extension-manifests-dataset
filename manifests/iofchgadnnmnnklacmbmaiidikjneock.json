{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Men's Lift",
  "description": "Men's Lift is a fitness magazine which provides information related to health, fitness, nutrition and style.",
  "version": "0.2",
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon.png",
    "128": "image/icon.png"
  },
  "browser_action": {
    "default_icon": "image/icon.png",
    "default_popup": "html/popup.html",
    "default_title": "Men's Lift!"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
