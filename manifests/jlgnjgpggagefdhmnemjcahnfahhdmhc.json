{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Doctor Chrome App",
  "short_name": "Time Doctor",
  "description": "Say Hello To Productivity",
  "version": "49.1.0",
  "manifest_version": 2,
  "offline_enabled": false,
  "permissions": [
    "contextMenus",
    "desktopCapture",
    "storage",
    "system.display",
    "idle",
    "unlimitedStorage",
    "notifications",
    "alwaysOnTopWindows",
    "https://*.timedoctor.com/*",
    "https://*.timedoctorstaging.com/*",
    "https://*.timedoctortest.com/*",
    "https://*.timedoctordev.com/*",
    "https://www.gstatic.com/*",
    "https://www.googleapis.com/*"
  ],
  "icons": {
    "16": "./assets/logo/logo-16.png",
    "48": "./assets/logo/logo-48.png",
    "128": "./assets/logo/logo.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
