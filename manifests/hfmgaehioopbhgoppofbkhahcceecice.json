{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "www.corrige.org",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Protege tus ojos de las faltas de ortografía mas comunes",
  "author": "Gauthier Dubois",
  "homepage_url": "http://www.corrige.org",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*/*"
      ],
      "js": [
        "gau.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "pluma_peq.png",
    "typo/dictionaries/en_US/en_US.dic",
    "typo/dictionaries/en_US/en_US.aff",
    "typo/dictionaries/es_ES/es_ES.dic",
    "typo/dictionaries/es_ES/es_ES.aff"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Corrector ortográfico",
    "default_popup": "corrige.html"
  }
}
