{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Search Filter by Period",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "Filters Google search results by specified period",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "lmt.html",
    "default_icon": "icons/icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*",
        "https://*.google.co.jp/*",
        "https://*.google.co.uk/*",
        "https://*.google.com.hk/*",
        "https://*.google.co.in/*",
        "https://*.google.com.au/*",
        "https://*.google.ca/*",
        "https://*.google.com.tw/*",
        "https://*.google.fr/*",
        "https://*.google.ru/*",
        "https://*.google.ch/*",
        "https://*.google.de/*",
        "https://*.google.co.kr/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "content.js"
  ],
  "permissions": [
    "storage",
    "tabs"
  ]
}
