{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nada - Temp mail, Disposable Email.",
  "short_name": "Nada - Temp mail",
  "version": "1.0.28",
  "homepage_url": "https://getnada.com/",
  "description": "Nada, a free temp mail service. Protects your private email from spam by providing you a temporary disposable email address.",
  "author": "Nada - Temp mail",
  "default_locale": "en",
  "icons": {
    "16": "icons/16x16.png",
    "20": "icons/20x20.png",
    "24": "icons/24x24.png",
    "32": "icons/32x32.png",
    "40": "icons/40x40.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png",
    "256": "icons/256x256.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "clipboardWrite",
    "https://getnada.com/"
  ],
  "background": {
    "scripts": [
      "vendor/lodash/lodash.min.js",
      "common/Browser.js",
      "common/Constants.js",
      "background/script.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Nada - Temp mail, Disposable Email.",
    "default_popup": "popup/template.html",
    "default_icon": {
      "16": "icons/16x16.png",
      "20": "icons/20x20.png",
      "24": "icons/24x24.png",
      "25": "icons/25x25.png",
      "30": "icons/30x30.png",
      "32": "icons/32x32.png",
      "40": "icons/40x40.png",
      "48": "icons/48x48.png",
      "64": "icons/64x64.png",
      "128": "icons/128x128.png",
      "256": "icons/256x256.png",
      "512": "icons/512x512.png"
    }
  },
  "web_accessible_resources": [
    "icons/*"
  ]
}
