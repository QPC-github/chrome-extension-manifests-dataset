{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Exit blocker",
  "short_name": "Exit blocker",
  "description": "Prevent exits from current page.",
  "author": "contact@equinode.com",
  "icons": {
    "128": "files/img/icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "version": "1.0",
  "manifest_version": 2,
  "page_action": {
    "default_icon": "files/img/icon48.png"
  },
  "background": {
    "scripts": [
      "files/js/background.js"
    ]
  }
}
