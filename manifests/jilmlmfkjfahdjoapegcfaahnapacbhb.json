{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "리틀카우 셀프쇼핑",
  "description": "셀프쇼핑을 편하게 할수 있는 리틀카우 확장 프로그램입니다.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": {
      "16": "app_icon_16.png",
      "32": "app_icon_32.png",
      "64": "app_icon.png"
    },
    "default_title": "리틀카우 셀프쇼핑",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://selfro.com/",
    "https://littlecow.co.kr/"
  ]
}
