{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cyphers homepage helper",
  "description": "Add some usual features to the Cyphers homepage.",
  "version": "0.1",
  "permissions": [
    "tabs",
    "http://cyphers.nexon.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://cyphers.nexon.com/cyphers/article/*"
      ],
      "js": [
        "cpp.js"
      ]
    }
  ]
}
