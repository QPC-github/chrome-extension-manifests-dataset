{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flip Off",
  "version": "1.2",
  "default_locale": "en",
  "incognito": "split",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "128": "icons/128.png"
    },
    "default_title": "Flip Off"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ],
  "author": "Shikhar Srivastava",
  "offline_enabled": true,
  "description": "__MSG_appDesc__",
  "short_name": "Flip Off"
}
