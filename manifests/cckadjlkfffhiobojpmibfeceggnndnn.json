{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IKAG",
  "description": "Shorten URLs, Save Bookmarks, Comments and Page Credentials to your ikag.ca account",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Save to ikag.ca"
  },
  "permissions": [
    "activeTab",
    "https://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "jquery-ui.css"
      ],
      "js": [
        "jquery-1.9.1.js",
        "jquery-ui.js",
        "encode.js"
      ]
    }
  ]
}
