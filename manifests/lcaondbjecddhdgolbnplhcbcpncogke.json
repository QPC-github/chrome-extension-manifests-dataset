{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "云际会议屏幕共享扩展",
  "description": "提供云际会议的屏幕共享功能",
  "version": "1.1.3",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "permissions": [
    "desktopCapture",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "options_page": "options.html"
}
