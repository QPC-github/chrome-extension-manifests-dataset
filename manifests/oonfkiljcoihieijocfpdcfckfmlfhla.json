{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinkedIn, Next and Previous shortcuts",
  "description": "Adding shortcuts to Next and Previous buttons when searching for people on LinkedIn",
  "icons": {
    "16": "fn_16x16.png",
    "48": "fn_48x48.png",
    "128": "fn_128x128.png"
  },
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "LinkedIn, Next and Previous shortcuts"
  },
  "manifest_version": 2
}
