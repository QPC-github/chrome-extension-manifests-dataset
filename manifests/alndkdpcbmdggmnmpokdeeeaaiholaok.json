{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Meseek Search Widget",
  "description": "This extension will allow you to do a search on Meseek",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://*.meseek.com/*"
  ]
}
