{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shades - New Tab Mod",
  "author": "JijoJohn ",
  "description": "Shades will replace your old new tab page with a powerful and beautiful new tab page",
  "version": "1.2",
  "incognito": "split",
  "permissions": [
    "tabs",
    "storage",
    "geolocation",
    "http://*/"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_popup": "shades.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "scripts/jquery-2.1.3.min.js",
      "scripts/core.js",
      "scripts/bootstrap.min.js"
    ]
  }
}
