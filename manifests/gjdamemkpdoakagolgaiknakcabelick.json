{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "enable instagram video controls",
  "description": "Enables instagram video controls",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.instagram.com/*"
      ],
      "js": [
        "instagram.js"
      ]
    }
  ],
  "permissions": [
    "*://*.instagram.com/*"
  ]
}
