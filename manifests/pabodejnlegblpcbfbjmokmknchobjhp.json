{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "icon19x19.png",
    "128": "icon128x128.png"
  },
  "name": "Elec Tools",
  "description": "Contains calculators useful to electrical engineers.",
  "version": "0.0.0.3",
  "browser_action": {
    "default_icon": "icon19x19.png",
    "default_popup": "popup.html",
    "default_title": "Elec Tools."
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
