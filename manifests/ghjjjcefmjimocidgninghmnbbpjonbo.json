{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Vote History",
  "description": "Keep track of your voted reddit comments by user.",
  "version": "2.1.4",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "*://www.reddit.com/"
  ],
  "background": {
    "scripts": [
      "dao.js"
    ]
  },
  "icons": {
    "32": "assets/favicon.ico",
    "48": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.reddit.com/r/*/comments/*",
        "*://www.reddit.com/user/*",
        "*://www.reddit.com/api/*"
      ],
      "js": [
        "lib/jquery.js",
        "script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "votehistory.html"
  ]
}
