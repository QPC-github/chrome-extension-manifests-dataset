{
  "manifest_version": 2,
  "name": "Simple Download Manager",
  "description": "Simple Download Manager",
  "version": "1.8.0",
  "background": {
    "page": "index.html#/background",
    "persistent": true
  },
  "icons": {
    "19": "assets/icon19.png",
    "38": "assets/icon19.png",
    "128": "assets/icon19.png"
  },
  "browser_action": {
    "default_title": "View Downloads!",
    "default_popup": "index.html#/popup"
  },
  "permissions": [
    "notifications",
    "tabs",
    "unlimitedStorage",
    "management",
    "downloads",
    "downloads.open",
    "downloads.shelf"
  ],
  "short_name": "SDM",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ],
  "content_security_policy": "script-src 'self' https://maxcdn.bootstrapcdn.com 'unsafe-eval'; object-src 'self'"
}
