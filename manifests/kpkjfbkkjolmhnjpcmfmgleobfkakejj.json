{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Klaus",
  "manifest_version": 2,
  "description": "Give feedback on your customer support conversations.",
  "browser_action": {
    "default_title": "Klaus",
    "default_icon": "icons/gray/icon-48x48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "identity",
    "storage",
    "alarms"
  ],
  "icons": {
    "48": "icons/icon-48x48.png",
    "72": "icons/icon-72x72.png",
    "96": "icons/icon-96x96.png",
    "128": "icons/icon-128x128.png",
    "144": "icons/icon-144x144.png",
    "152": "icons/icon-152x152.png",
    "192": "icons/icon-192x192.png",
    "384": "icons/icon-384x384.png",
    "512": "icons/icon-512x512.png"
  },
  "background": {
    "scripts": [
      "js/chunk-vendors.js",
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com http://localhost:8098; object-src 'self'",
  "version": "1.5.17"
}
