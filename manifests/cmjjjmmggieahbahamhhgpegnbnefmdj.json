{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Print VSTS cards",
  "short_name": "VSTScards",
  "description": "Extract tasks from a VSTS backlog and print them",
  "author": "Eder Elorriaga <gexplorer8@gmail.com>",
  "version": "3.0.2",
  "icons": {
    "16": "postits_16.png",
    "48": "postits_48.png",
    "128": "postits_128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "Print VSTS cards",
    "default_icon": "postits_16.png"
  },
  "manifest_version": 2
}
