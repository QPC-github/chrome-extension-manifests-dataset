{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "applications": {
    "gecko": {
      "id": "framalinkshortener@zatsunenomokou.eu",
      "strict_min_version": "63.0"
    }
  },
  "manifest_version": 2,
  "name": "Framalink shortener",
  "short_name": "FramalinkShortener",
  "description": "__MSG_extShortDesc__",
  "author": "ZatsuneNoMokou",
  "version": "3.1.1",
  "icons": {
    "96": "/icon_96.png",
    "128": "/icon_128.png"
  },
  "background": {
    "scripts": [
      "data/js/lib/browser-polyfill.js",
      "data/js/chrome-notification-controler.js",
      "data/options-data.js",
      "data/options-api.js",
      "data/js/index_beta.js",
      "index.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "/icon_16.png",
      "48": "/icon_48.png",
      "96": "/icon_96.png",
      "128": "/icon_128.png"
    }
  },
  "default_locale": "en",
  "offline_enabled": false,
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "clipboardWrite",
    "notifications",
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "data/options.html",
    "chrome_style": true
  },
  "minimum_chrome_version": "42.0"
}
