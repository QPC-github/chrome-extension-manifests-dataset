{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Discordyst",
  "description": "Discordyst Chrome Plugin",
  "version": "0.0.3",
  "browser_action": {
    "default_icon": "icon_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://discordapp.com/channels/*"
      ],
      "js": [
        "discordyst.chrome.js"
      ]
    }
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
