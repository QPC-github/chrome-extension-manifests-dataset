{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MLG PRO #360 CHROME SCOPE",
  "description": "Let's you experience the MLG pro life",
  "version": "0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "audio.js"
      ],
      "css": [
        "cursor.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
