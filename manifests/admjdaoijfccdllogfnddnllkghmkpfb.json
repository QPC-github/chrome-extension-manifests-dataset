{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Screenshot",
  "version": "1.4",
  "description": "Allows you to take screenshot of the video and save it instantly to your disk",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
