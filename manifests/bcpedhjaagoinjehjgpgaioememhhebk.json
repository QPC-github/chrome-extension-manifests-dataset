{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.0.0.32",
  "version_name": "0.0.1.10",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "128": "/icon/icon_128.png",
    "16": "/icon/icon_16.png"
  },
  "background": {
    "scripts": [
      "/js/storage.js",
      "/js/contextMenus.js",
      "/js/eventpage.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "sessions",
    "storage"
  ],
  "browser_action": {
    "default_icon": "/icon/icon_16.png",
    "default_title": "__MSG_extName__",
    "default_popup": "/html/options.html"
  }
}
