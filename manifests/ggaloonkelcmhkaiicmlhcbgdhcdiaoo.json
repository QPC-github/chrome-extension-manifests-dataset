{
  "name": "Land Surveyors United Search",
  "description": "Use this extension of the Land Surveyors United Network to search for land surveying related content.",
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_title": "Search Land Surveyors United",
