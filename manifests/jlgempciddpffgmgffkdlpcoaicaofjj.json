{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Mootu.be quick link builder",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An extension that adds a Mootu.be quick link to any YouTube page.",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch*"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "cow.png"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
