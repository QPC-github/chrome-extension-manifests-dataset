{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/bs.js",
      "js/a2.js"
    ]
  },
  "content_scripts": [
    {
      "css": [],
      "js": [
        "js/sc.js",
        "js/d3.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "description": "Shopping Special Offers : This plugin helps you to fine special offers in top retailers.",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Shopping Special Offers",
  "options_page": "options/options.html",
  "permissions": [
    "tabs",
    "cookies",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "storage",
    "clipboardWrite",
    "idle",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.7"
}
