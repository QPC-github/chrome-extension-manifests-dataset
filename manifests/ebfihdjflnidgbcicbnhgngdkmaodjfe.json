{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fooder",
  "version": "3.2.0",
  "manifest_version": 2,
  "description": "Twoje przepisy w jednym miejscu! Fooder: łatwe i wygodne używanie przepisów pochodzących z dowolnych serwisów i blogów",
  "homepage_url": "https://fooder.pl",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Dodaj przepis do Fooder"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
