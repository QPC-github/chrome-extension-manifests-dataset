{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "habrahabr next page shortcut",
  "version": "1",
  "manifest_version": 2,
  "description": "adds a keyboard shortcut to habrahabr",
  "content_scripts": [
    {
      "matches": [
        "http://habrahabr.ru/*"
      ],
      "js": [
        "jquery-1.9.1.js",
        "arrows.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "http://habrahabr.ru/*"
  ]
}
