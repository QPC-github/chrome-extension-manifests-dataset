{
  "manifest_version": 2,
  "name": "Indian Badger",
  "description": "This extension displays AISD Technology Badges.",
  "version": "2.0",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "css/font-awesome-4.5.0/css/font-awesome.css",
        "css/main-style.css"
      ],
      "js": [
        "js/jquery.min.js",
        "js/jquery.simpleWeather.js",
        "js/script.js"
      ],
      "matches": [
        "*://*.google.com/_/chrome/newtab*"
      ]
    }
  ],
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "key": "",
  "oauth2": {
    "client_id": "250995224960-3uomcqh929nhsfrvvrkikmap215adcvd.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://aisboard.alvaradoisd.net/",
    "http://10.1.1.153/",
    "http://127.0.0.1:8000/",
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "/backgrounds/*.jpg",
    "/backgrounds/overlay-vignette.png",
    "/images/AISDGoogle.png",
    "css/font-awesome-4.5.0/fonts/*.*"
  ]
}
