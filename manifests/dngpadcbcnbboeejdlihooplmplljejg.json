{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Krayn live extension",
  "version": "1.2",
  "description": "Permet d'être alerté quand Krayn est en live !",
  "browser_action": {},
  "permissions": [
    "notifications",
    "webRequest",
    "*://*.api.twitch.tv/",
    "alarms"
  ],
  "icons": {
    "64": "img/glouglou.png"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  }
}
