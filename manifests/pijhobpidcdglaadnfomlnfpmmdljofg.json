{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Marginalyser Standard",
  "description": "Manage your margins, increase your profits with Marginalyser",
  "version": "1.1",
  "icons": {
    "19": "img/icon19.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "management"
  ]
}
