{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Random Number Generator Extension",
  "description": "Simple Random Number Generator Extension for Google Chrome",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon16.png",
    "icons": {
      "16": "icon16.png",
      "48": "icon32.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
