{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "OG.NR; On The Ground News Reports",
  "version": "1.2",
  "manifest_version": 2,
  "description": "OG.NR is a community of reporters from across Jamaica and the World. We report the news we see so that others are made aware",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "ognr.html"
  },
  "content_security_policy": "script-src 'self' https://query.yahooapis.com; object-src 'self'",
  "web_accessible_resources": [
    "icon_48.png"
  ]
}
