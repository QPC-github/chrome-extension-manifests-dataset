{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cheetah",
  "short_name": "Cheetah",
  "description": "Transfers all the new downloads to Cheetah desktop app for better speed",
  "author": "Moin Uddin",
  "manifest_version": 2,
  "version": "0.0.0.2",
  "icons": {
    "512": "icon.png"
  },
  "permissions": [
    "*://*/*",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "<all_urls>",
    "downloads"
  ],
  "background": {
    "scripts": [
      "assets/js/bg.js"
    ]
  }
}
