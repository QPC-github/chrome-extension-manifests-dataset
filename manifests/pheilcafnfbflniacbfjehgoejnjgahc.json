{
  "background": {
    "scripts": [
      "blocked_domains.js",
      "background1.js",
      "background2.js"
    ]
  },
  "browser_action": {
    "default_icon": "stool.png",
    "default_popup": "barstoolExtension.html?popup=true",
    "default_title": "Barstool Enhancement Suite"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "content.js",
        "content1.js",
        "content2.js",
        "comments.js"
      ],
      "matches": [
        "http://*.barstoolsports.com/*",
        "https://*.barstoolsports.com/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "css": [
        "inject.css"
      ],
      "js": [
        "jquery.js",
        "inject.js"
      ],
      "matches": [
        "http://*.barstoolsports.com/*",
        "https://*.barstoolsports.com/*",
        "https://*.twitch.tv/*"
      ]
    },
    {
      "all_frames": true,
      "css": [
        "inject1.css"
      ],
      "js": [
        "inject1.js"
      ],
      "matches": [
        "http://*.barstoolsports.com/*",
        "https://*.barstoolsports.com/*"
      ]
    },
    {
      "all_frames": true,
      "js": [
        "jquery.js",
        "audio.js"
      ],
      "matches": [
        "https://*.podcastone.com/*"
      ]
    }
  ],
  "description": "A suite of modules to enhance your Barstool browsing experience.",
  "icons": {
    "128": "icon.png",
    "16": "stool16.png",
    "48": "stool48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Barstool Sports Enhancement Suite",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "storage",
    "tabs",
    "https://*.barstoolsports.com/*"
  ],
  "short_name": "Block Bloggers, Video Utilities, Audio Speed Controls.",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.5",
  "web_accessible_resources": [
    "inject1.css",
    "inject.css",
    "shadow.css"
  ]
}
