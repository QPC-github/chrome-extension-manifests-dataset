{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/landingicon-128.png",
    "default_title": "AU Landing Bookmarker",
    "default_popup": "popup.html"
  },
  "name": "AU Landing Bookmarks",
  "description": "Bookmark a site in Athabasca University's social media platform",
  "homepage_url": "https://github.com/DavidHScott/AULandingChromeBookmarks",
  "options_page": "options.html",
  "icons": {
    "16": "img/landingicon-16.png",
    "48": "img/landingicon-48.png",
    "128": "img/landingicon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "0.9",
  "manifest_version": 2
}
