{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "8Drama Rem",
  "version": "1.0",
  "manifest_version": 2,
  "description": "8Drama Rem",
  "page_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "content_script.js"
      ],
      "matches": [
        "http://8drama.ru/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "http://*/*"
  ]
}
