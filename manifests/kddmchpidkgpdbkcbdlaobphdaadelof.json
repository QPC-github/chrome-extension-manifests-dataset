{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NoMasVice",
  "description": "This extension blocks the undercovered Vice ads in the spanish humour blog finofilipino.org",
  "short_name": "Finomaricon",
  "version": "1.2",
  "author": {
    "name": "JavRok",
    "url": "https://twitter.com/javrok"
  },
  "browser_action": {
    "default_icon": "noMasViceIcon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://finofilipino.org/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
