{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Subredditor",
  "description": "This extension allows quick navigation to specific subreddits.",
  "version": "1.0",
  "omnibox": {
    "keyword": "r"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "256": "icon-256.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
