{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pivotaltracker Iterations Filter",
  "version": "1.2",
  "author": "Divin Divakaran",
  "homepage_url": "https://github.com/divindvm",
  "manifest_version": 2,
  "description": "This is a simple extension to filter the iteration based on user",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.pivotaltracker.com/*"
  ]
}
