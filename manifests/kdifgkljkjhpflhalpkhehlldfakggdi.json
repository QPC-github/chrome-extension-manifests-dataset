{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "my.jsc-solutions.net",
  "description": "my.jsc-solutions.net",
  "version": "20",
  "minimum_chrome_version": "27",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "view-source"
      ]
    }
  },
  "icons": {
    "128": "jsc128.png"
  },
  "permissions": [
    "webview",
    "fullscreen",
    "pointerLock",
    "unlimitedStorage",
    "notifications",
    "storage",
    {
      "socket": [
        "udp-send-to",
        "udp-bind",
        "udp-multicast-membership"
      ]
    }
  ]
}
