{
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/font-awesome.css"
      ],
      "js": [
        "js/jquery.js",
        "js/dic.js",
        "js/config.js",
        "js/script.js",
        "js/linq.js"
      ],
      "matches": [
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.1688.com/*",
        "https://*.1688.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*"
      ]
    },
    {
      "js": [
        "js/jquery.js",
        "js/linq.js",
        "js/scriptvnshop.js"
      ],
      "matches": [
        "http://donhang.hangorder.com/DonHang.aspx?",
        "http://donhang.hangorder.com/DonHang.aspx",
        "http://donhang.hangorder.com/don-hang.html?*",
        "https://donhang.hangorder.com/don-hang.html?*",
        "http://donhang.hangorder.com/don-hang.html",
        "https://donhang.hangorder.com/don-hang.html",
        "https://nhaphangtrungquoc365.com/ajaxs/search/addCartExtension_new.php",
        "https://nhaphangtrungquoc365.com/ajaxs/search/get_cookie_cart_id.php",
        "https://nhaphangtrungquoc365.com/ajaxs/extension/get_rate_exchange.php"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Công cụ hỗ trợ tìm kiếm và đặt hàng Taobao Order taobao Tiếng Việt",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "Nhập Hàng Trung Quốc 365 Extension",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5.6",
  "web_accessible_resources": [
    "template/index.html",
    "template/ListStore.html",
    "css/main.css",
    "css/remodal.css",
    "css/remodal-default-theme.css",
    "css/cn_main.css",
    "images/add_on/save-product-ao.png",
    "images/add_on/icon-arr-ao.png",
    "images/add_on/addon-icon-shopping-cart.png",
    "images/add_on/close-warning-ao.png",
    "images/add_on/icon-seudo-ao.png",
    "images/add_on/addon-icon-orderhang.png",
    "images/add_on/bg-addon.png",
    "images/add_on/not_translate.png",
    "images/add_on/exception_tool.png",
    "images/add_on/cart28x28.png",
    "images/add_on/shop-cart-icon.png",
    "images/add_on/box-nh-background.png",
    "images/add_on/hide.png"
  ]
}
