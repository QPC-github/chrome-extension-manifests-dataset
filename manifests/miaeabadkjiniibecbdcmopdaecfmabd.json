{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Whois Lookup",
  "version": "2.0",
  "description": "This extension will search whois records of the current domain.",
  "icons": {
    "16": "16.png",
    "18": "18.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "18.png",
    "default_title": "Whois Lookup",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "author": "Abhijeet Ashok Muneshwar",
  "homepage_url": "http://abhijeet-muneshwar.github.io/"
}
