{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kicks For Us",
  "description": "Add to cart script to help you cop those hard to get items!",
  "version": "1.8",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "img/icon2.png",
    "48": "img/icon2.png",
    "128": "img/icon2.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon2.png",
      "38": "img/icon2.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "img/icon.png2"
  ]
}
