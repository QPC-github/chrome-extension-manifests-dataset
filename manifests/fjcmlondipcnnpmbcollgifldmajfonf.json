{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab Override",
  "version": "1.0.1",
  "description": "Load a custom URL when opening a new tab.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
