{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JIRA Crawler",
  "version": "0.2.8",
  "author": "ddly",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    }
  },
  "options_page": "main.html",
  "permissions": [
    "<all_urls>",
    "storage",
    "downloads"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}
