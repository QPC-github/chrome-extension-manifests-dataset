{
  "name": "California Apartment News",
  "short_name": "CAA",
  "description": "This small extension allows for quick access to California apartment industry news, as well the complete CAA knowledge base.",
  "browser_action": {
    "default_icon": {
      "38": "img/icon.png"
    },
    "icons": {
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_title": "California Apartment News",
    "default_popup": "caa.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "0.4"
}
