{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kittens and Rainbows",
  "description": "Chrome extension that replaces Donald Trump and Hillary Clinton With Kittens and Rainbows emojis",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "size16.png",
    "48": "size48.png",
    "128": "size128.png"
  }
}
