{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ふれーっす",
  "manifest_version": 2,
  "version": "1.0.3",
  "description": "FRESH LIVEの拡張機能です。",
  "author": "archives",
  "homepage_url": "https://archives.ga",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon19.png"
    },
    "default_title": "ふれーっす"
  },
  "content_scripts": [
    {
      "matches": [
        "*://freshlive.tv/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ]
    }
  ]
}
