{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "rummagur",
  "description": "find random pictures from imgur",
  "version": "0.1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "https://i.imgur.com/*"
  ],
  "icons": {
    "128": "img/128.png"
  }
}
