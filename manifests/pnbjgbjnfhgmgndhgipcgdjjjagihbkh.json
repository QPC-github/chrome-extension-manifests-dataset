{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Reverse Image Search",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Right click on an image and find it's origin in Google image search.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "default_locale": "en",
  "homepage_url": "http://dejanseo.com.au/"
}
