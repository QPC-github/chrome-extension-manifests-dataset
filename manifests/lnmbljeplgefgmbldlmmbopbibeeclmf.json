{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "History Revisited",
  "version": "2.2",
  "description": "Reads your history, categories and shows as tabular form.",
  "permissions": [
    "history",
    "storage"
  ],
  "browser_action": {
    "default_popup": "readhistory.html",
    "default_icon": "hricon16.png"
  },
  "icons": {
    "16": "hricon16.png",
    "64": "hricon64.png",
    "128": "hricon128.png"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js https://ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular.js; object-src 'self'",
  "manifest_version": 2
}
