{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "motivation.html"
  },
  "manifest_version": 2,
  "name": "Palante!",
  "description": "Get motivated every time you open a new tab.",
  "version": "7.1.1",
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
