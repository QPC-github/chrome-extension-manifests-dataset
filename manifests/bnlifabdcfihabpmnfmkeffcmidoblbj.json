{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VK-Script",
  "version": "0.0.10",
  "description": "Doing some profit for vk",
  "content_scripts": [
    {
      "matches": [
        "http://vk.com/*",
        "https://vk.com/*",
        "http://new.vk.com/*",
        "https://new.vk.com/*"
      ],
      "js": [
        "jquery.min.js",
        "popup.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "popup.html",
    "style.css"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png"
  }
}
