{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blogtrackers",
  "version": "1.11",
  "description": "Adds a browser action icon to the toolbar.  Click the button to send the current url to blogtrackers to be tracked.",
  "homepage_url": "http://blogtrackers.host.ualr.edu",
  "icons": {
    "96": "icons/Blogtrackers 96 - Dark Blue.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://cdnjs.cloudflare.com/ajax/libs/crypto-js/*",
    "http://blogtrackers.host.ualr.edu/Blogtrackers"
  ],
  "browser_action": {
    "default_icon": "icons/Blogtrackers 32 - Dark Blue.png",
    "default_title": "Blogtracker",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": []
}
