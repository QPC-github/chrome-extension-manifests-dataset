{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "js/jquery-3.1.1.min.js",
        "js/talkdeck.js"
      ],
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "converted_from_user_script": true,
  "description": "Read TweetDeck out loud!",
  "name": "TalkDeck",
  "version": "16"
}
