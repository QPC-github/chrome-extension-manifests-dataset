{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Androjine androjine@gmail.com",
  "background": {
    "scripts": [
      "storage.js",
      "youtube.js",
      "parameters.js",
      "background.js",
      "chrome.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_off.png",
    "default_title": "Androjine - Offline",
    "default_popup": "popup.html"
  },
  "description": "Ne manquez pas un seul stream de Androjine !",
  "icons": {
    "256": "icons/logo256.png",
    "128": "icons/logo128.png",
    "32": "icons/logo32.png",
    "64": "icons/logo64.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "css": [
        "snow.css"
      ],
      "js": [
        "snow.js"
      ],
      "matches": [
        "https://www.twitch.tv/androjine",
        "https://www.facebook.com/Androjine",
        "https://www.facebook.com/androjine",
        "https://twitter.com/Androjine",
        "https://twitter.com/androjine",
        "https://www.youtube.com/c/androjinesupportlol",
        "https://www.youtube.com/c/AndrojineSupportLoL",
        "https://www.twitch.tv/androjine/*",
        "https://www.facebook.com/Androjine/*",
        "https://www.facebook.com/androjine/*",
        "https://twitter.com/Androjine/*",
        "https://twitter.com/androjine/*",
        "https://www.youtube.com/c/androjinesupportlol/*",
        "https://www.youtube.com/c/AndrojineSupportLoL/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Androjine Live Extension",
  "permissions": [
    "https://api.twitch.tv/",
    "https://www.googleapis.com/",
    "notifications",
    "storage"
  ],
  "version": "1.5.5"
}
