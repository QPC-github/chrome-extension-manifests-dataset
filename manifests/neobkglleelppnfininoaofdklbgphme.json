{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quinn",
  "short_name": "Quinn",
  "version": "1.8.9",
  "version_name": "1.8.9",
  "description": "Spend Less. Save More. Live a Happier, Balanced Life.",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "identity",
    "storage",
    "management",
    "alarms",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "dialog/dialog.html",
    "assets/blacklist.json"
  ],
  "icons": {
    "16": "img/Quinn_Favicon_v2_Color.png",
    "32": "img/Quinn_Favicon_v2_Color.png",
    "48": "img/Quinn_Favicon_v2_Color.png",
    "128": "img/Quinn_Favicon_v2_Color.png"
  },
  "oauth2": {
    "client_id": "440489667790-jvkmegrkpc4t9aeoubvs1519a3iqskfl.apps.googleusercontent.com",
    "scopes": [
      "email",
      "profile"
    ]
  },
  "key": "",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com/ga.js; object-src 'self'"
}
