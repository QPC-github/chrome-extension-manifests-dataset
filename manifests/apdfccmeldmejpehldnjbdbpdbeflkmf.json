{
  "browser_action": {
    "default_icon": "icon/128.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "http://v-ticket-back.dev/kassa-lite/*",
        "https://v-ticket.ua/kassa-lite/*",
        "http://v-ticket.ua/kassa-lite/*",
        "https://www.v-ticket.ua/kassa-lite/*",
        "http://www.v-ticket.ua/kassa-lite/*",
        "http://v-ticket-test.ua/kassa-lite/*",
        "https://v-ticket-test.ua/kassa-lite/*",
        "https://www.v-ticket.com.ua/kassa/*",
        "https://v-ticket.com.ua/kassa/*",
        "http://v-ticket.com.ua/kassa/*",
        "http://www.v-ticket.com.ua/kassa/*",
        "https://v-ticket.dev/kassa/*",
        "http://v-ticket.dev/kassa/*"
      ]
    }
  ],
  "description": "v-Ticket Application for print listener",
  "icons": {
    "128": "icon/128.png",
    "48": "icon/48.png"
  },
  "manifest_version": 2,
  "name": "v-Ticket Printer Extension",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "http://v-ticket-back.dev/kassa-lite/*",
    "https://v-ticket.ua/kassa-lite/*",
    "http://v-ticket.ua/kassa-lite/*",
    "https://www.v-ticket.ua/kassa-lite/*",
    "http://www.v-ticket.ua/kassa-lite/*",
    "http://v-ticket-test.ua/kassa-lite/*",
    "https://v-ticket-test.ua/kassa-lite/*",
    "https://www.v-ticket.com.ua/kassa/*",
    "https://v-ticket.com.ua/kassa/*",
    "http://v-ticket.com.ua/kassa/*",
    "http://www.v-ticket.com.ua/kassa/*",
    "https://v-ticket.dev/kassa/*",
    "http://v-ticket.dev/kassa/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3",
  "web_accessible_resources": [
    "icon/48.png",
    "index.html"
  ]
}
