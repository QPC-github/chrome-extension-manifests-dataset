{
  "manifest_version": 2,
  "name": "刚需找房",
  "version": "1.3.6",
  "icons": {
    "128": "images/logo.png"
  },
  "description": "专业的房屋聚合信息，还原房子最真实的价值，实时跟踪价格曲线，买房快人一步！",
  "permissions": [
    "storage",
    "cookies",
    "http://*.meiwenchain.com/*",
    "https://*.meiwenchain.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/logo.png",
      "38": "images/logo.png"
    },
    "default_popup": "html/popup.html",
    "default_title": "专业的房屋聚合信息，还原房子最真实的价值！v1.3",
    "name": "V1.3"
  },
