{
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "256": "images/icon.png",
      "128": "images/icon128.png",
      "48": "images/icon48.png",
      "16": "images/icon16.png"
    },
    "default_popup": "popup.html",
    "default_title": "PreciseMagnetic"
  },
  "content_scripts": [
    {
      "css": [
        "css/bootstrap-iso.css",
        "css/custom.css",
        "css/datatables.css",
        "css/font-awesome.min.css"
      ],
      "js": [
        "js/jquery.min.js",
        "js/bootstrap.min.js",
        "js/datatables.js",
        "js/var.js",
        "js/function.js",
        "js/contentscript.js",
        "js/md5.min.js",
        "js/background.js"
      ],
      "matches": [
        "https://*.facebook.com/ads/audience-insights/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://ssl.google-analytics.com https://connect.facebook.net; object-src 'self'",
  "description": "Help you to research perfect audience for facebook ads market targeting.",
  "icons": {
    "256": "images/icon.png",
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "options_page": "search.html",
  "manifest_version": 2,
  "key": "",
  "name": "PreciseMagnetic for FB Audience Insights",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "storage",
    "tabs",
    "https://*.facebook.com/ads/audience-insights/*",
    "https://precisemagneticpro.000webhostapp.com/*"
  ],
  "short_name": "PreciseMagnetic",
  "version": "3.3.1",
  "web_accessible_resources": [
    "images/*",
    "css/*",
    "fonts/*",
    "js/*",
    "*.html",
    "manifest.json"
  ]
}
