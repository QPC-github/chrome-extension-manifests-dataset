{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Javadoc Search",
  "version": "1.0",
  "description": "Adds search capabilities to Javadoc pages.",
  "permissions": [
    "activeTab",
    "webNavigation",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "help.png"
  ],
  "manifest_version": 2
}
