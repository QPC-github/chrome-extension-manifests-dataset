{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon48.png",
    "default_popup": "popup.html",
    "default_title": "Seo-Fast.RU"
  },
  "content_scripts": [
    {
      "js": [
        "purify.min.js",
        "jquery.min.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Для заработка не требуется никаких действий, достаточно один раз установить наше расширение и постоянно зарабатывать.",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "SeoFastRU",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "short_name": "SF",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5.8"
}
