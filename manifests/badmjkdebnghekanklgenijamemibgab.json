{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mp3Poisk",
  "version": "0.7.7",
  "permissions": [
    "background",
    "notifications",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "description": "Music on site mp3poisk.net",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_title": "Mp3Poisk",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/jquery.jplayer.min.js",
      "js/jplayer.playlist.min.js",
      "js/background.js",
      "js/popup.js"
    ]
  }
}
