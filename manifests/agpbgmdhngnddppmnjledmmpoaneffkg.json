{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DocStar Cookie Changes Tracker",
  "version": "0.1",
  "description": "Tracks all changes to cookies during a browsing session",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
