{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Which Professor - AUS",
  "description": "This extension helps you decide which professor to register with",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "professor.png",
    "default_popup": "popup.html",
    "default_title": "This extension helps you choose professors"
  },
  "content_scripts": [
    {
      "matches": [
        "https://banner.aus.edu/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "modify.js",
    "facebook.png"
  ]
}
