{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UrlTabSort",
  "description": "Sorts tabs left-2-right by URL - ignoring www. prefix. Works with extensions like The Great Suspender.",
  "version": "1.0",
  "author": "Robert M. MÃ¼nch",
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": [
      "sorttabs.js"
    ],
    "background": false
  },
  "permissions": [
    "tabs"
  ]
}
