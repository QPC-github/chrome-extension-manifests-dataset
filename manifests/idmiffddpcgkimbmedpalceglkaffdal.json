{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ELPhone",
  "description": "Смс в один клик.",
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png"
  },
  "version": "1.32",
  "permissions": [
    "management",
    "https://www.google.com.ua/search*",
    "http://elm.pw/extension/*",
    "http://yandex.ua/yandsearch?*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "js/jquery.js"
    ]
  }
}
