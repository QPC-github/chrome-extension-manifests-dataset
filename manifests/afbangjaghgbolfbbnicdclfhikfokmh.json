{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CloudVoice Web Dialer",
  "version": "1.18.0327",
  "description": "Makes links out of phone numbers on the page, so you can click them to dial with Encore CloudVoice",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "jquery.ba-replacetext.min.js",
      "options.js",
      "contentscript.js"
    ]
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": "icon20.png",
    "default_title": "Phone numbers were linked on this page"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "jquery.ba-replacetext.min.js",
        "options.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
