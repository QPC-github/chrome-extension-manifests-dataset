{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.3.1.js",
        "jquery-ui-1.12.1.js",
        "eangel-chrome-bundle.js"
      ],
      "matches": [
        "http://mail.google.com/*",
        "https://mail.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://*.google.com 'unsafe-eval'; object-src 'self'",
  "description": "eAngel Proofreading for Emails",
  "icons": {
    "16": "icon.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "eAngel Proofreading for Emails",
  "short_name": "eAngel",
  "permissions": [
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.8.1"
}
