{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Em@iler - Fast Email Compose",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Send an email stright from the chrome bar",
  "icons": {
    "16": "icon (16).png",
    "48": "icon (48).png",
    "64": "icon.png",
    "128": "icon (128).png",
    "250": "icon (250).png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
