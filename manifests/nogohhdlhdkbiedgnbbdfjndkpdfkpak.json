{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Intuitive Browsing History Search",
  "description": "This extension is accessing your history.",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "permissions": [
    "history",
    "tabs",
    "http://*/",
    "https://*.google.com/*",
    "https://google.com/*",
    "http://*.google.com/*",
    "http://google.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "typedUrls.html"
  }
}
