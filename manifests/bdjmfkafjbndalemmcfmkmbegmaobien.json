{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Toggle Class",
  "description": "With this extension, you can toggle a class on an element.",
  "version": "1.1.2",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": "active_icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "48_icon.png",
    "128": "128_icon.png"
  }
}
