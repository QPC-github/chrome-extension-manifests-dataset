{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AudiobooksNow",
  "homepage_url": "https://www.audiobooksnow.com/",
  "description": "Audiobook downloads and streaming for less! Over 60,000 Titles!",
  "version": "1.0.2",
  "icons": {
    "128": "ABN-128x128.png",
    "16": "ABN-16x16.png",
