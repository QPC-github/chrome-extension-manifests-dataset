{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EKCKO",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "32.png",
    "default_title": "EKCKO"
  },
  "icons": {
    "128": "32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
