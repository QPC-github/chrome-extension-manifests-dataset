{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome Content Blocker",
  "description": "This extension removes nearly all content from the current page but omits ads.",
  "version": "0.1",
  "author": "Janneck Wullschleger",
  "homepage_url": "https://itsjw.de",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "*://*/*"
  ]
}
