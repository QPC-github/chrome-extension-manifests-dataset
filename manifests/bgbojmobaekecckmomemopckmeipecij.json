{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",
  "homepage_url": "http://www.quickmaps.me",
  "default_locale": "en",
  "version": "2.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com/ https://apis.google.com; object-src 'self'",
  "content_scripts": [
    {
      "all_frames": false,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "tooltip.css"
      ],
      "js": [
        "js/config.js",
        "js/debug.js",
        "js/request.js",
        "js/dom_handle.js",
        "js/init.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/config.js",
      "js/install.js",
      "js/context_menu.js"
    ]
  }
}
