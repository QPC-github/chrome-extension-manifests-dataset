{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PravCalendar",
  "version": "1.0",
  "description": "Православный календарь на каждый день",
  "permissions": [
    "http://prvslv.com/*",
    "https://prvslv.com/*"
  ],
  "browser_action": {
    "default_title": "Pravcalendar",
    "default_icon": "images/icon.png",
    "default_popup": "index.html"
  }
}
