{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IVF Greece Extension",
  "version": "0.5",
  "manifest_version": 2,
  "description": "Mediterranean Fertility Institute",
  "browser_action": {
    "default_icon": "ivfgreece_icon_128_white.png",
    "default_title": "Search and view the recent videos of our channel",
    "default_popup": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "ivfgreece_icon_128_white.png",
    "48": "ivfgreece_icon_48_white.png",
    "16": "ivfgreece_icon_16_white.png"
  }
}
