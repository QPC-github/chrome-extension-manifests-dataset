{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LRC SDPB Media Player Extender",
  "description": "This extension will make the HTML5 Video Player whole screen, So you can skip through the meeting audio faster.",
  "version": "1.0",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "http://sdpb.sd.gov/SDPBPodcast/*",
        "https://sdpb.sd.gov/SDPBPodcast/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
