{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "full colour!",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Automatically turns black & white website into full colour.",
  "homepage_url": "http://www.kaminwebdesign.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "css": [
        "css/styles.css"
      ],
      "js": [
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png"
  }
}
