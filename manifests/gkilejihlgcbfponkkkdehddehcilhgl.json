{
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "루리웹 광고 감추기 & 루리웹 헬퍼"
  },
  "content_scripts": [
    {
      "css": [
        "ruliweb.css"
      ],
      "js": [
        "jquery-3.0.0.min.js",
        "onLoad.js"
      ],
      "matches": [
        "*://*.ruliweb.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "루니지를 위한 새로운 기능들을 제공합니다.",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "key": "",
  "manifest_version": 2,
  "name": "루리웹 광고 감추기 & 루리웹 헬퍼",
  "permissions": [
    "<all_urls>",
    "storage",
    "*://*.ruliweb.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.90"
}
