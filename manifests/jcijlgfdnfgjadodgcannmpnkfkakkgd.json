{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Skimo TV",
  "description": "Skimo TV extension lets you skim the video in an YouTube Page",
  "version": "0.0.8",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "skim_128x128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Skim the Video",
    "default_icon": "skim_16x16.png"
  },
  "manifest_version": 2
}
