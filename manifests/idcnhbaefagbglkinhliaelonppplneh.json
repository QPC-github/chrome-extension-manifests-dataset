{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "MiCounter",
  "manifest_version": 2,
  "version": "2.1.4.6",
  "description": "A simple counter to keep track of what matters in your life",
  "background": {
    "scripts": [
      "settings.js",
      "simpleutils.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "counter2128.png",
    "name": "Click me :)"
  }
}
