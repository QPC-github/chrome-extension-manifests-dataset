{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [],
      "js": [
        "pip.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ]
    }
  ],
  "icons": {
    "128": "Icon.png",
    "16": "Icon.png",
    "48": "Icon.png"
  },
  "description": "Chrome plugin for Mac Application Picture in Picture",
  "manifest_version": 2,
  "name": "Picture in Picture (Mac Application)",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.1"
}
