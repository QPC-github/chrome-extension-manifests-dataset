{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clearbit Connect - Supercharge Gmailâ„¢",
  "description": "Find employee email addresses for any company and display useful contextual data for anyone who emails you.",
  "author": "Clearbit",
  "homepage_url": "https://connect.clearbit.com",
  "short_name": "Clearbit Connect",
  "version": "2.2.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "https://*.clearbit.com/"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
