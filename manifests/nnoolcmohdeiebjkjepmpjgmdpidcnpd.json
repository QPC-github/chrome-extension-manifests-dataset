{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Maximize Your Video",
  "permissions": [
    "tabs",
    "http://www.youtube.com/*"
  ],
  "version": "2.0.0.4",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "page_action": {
    "default_icon": "icon1.png",
    "name": "Click to maximize in the tab"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "http://jquery.com/*"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "content.js",
        "start.js",
        "off.js"
      ]
    }
  ]
}
