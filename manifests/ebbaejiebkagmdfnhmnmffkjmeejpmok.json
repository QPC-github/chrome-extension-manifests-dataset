{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lisk Value Calculator",
  "version": "1.1",
  "description": "Lisk Value Calculator can be used to quickly calculate how much any input amount of Lisk (LSK) is worth at the current market price",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Lisk Value Calculator"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
