{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "js/pagescript.js"
      ],
      "css": [
        "css/pagestyle.css"
      ],
      "matches": [
        "*://*.chiebukuro.yahoo.co.jp/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "画像拡大、伏せ字ID開示等",
  "manifest_version": 2,
  "name": "Yahoo!知恵袋拡張",
  "short_name": "Y!C Ext",
  "options_page": "options.html",
  "permissions": [
    "*://*.chiebukuro.yahoo.co.jp/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.10.1"
}
