{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nocks Nimble",
  "description": "Nocks Nimble herkent Bitcoin- en Gulden portemonnee adressen en geeft je de mogelijkheid direct een betaling uit te voeren.",
  "version": "0.1.2",
  "manifest_version": 2,
  "icons": {
    "128": "img/nocks-nimble-128x128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-2.1.4.min.js",
        "js/nocks.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.gif",
    "images/*",
    "*.png"
  ]
}
