{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Geneanet",
  "description": "__MSG_extDescription__",
  "version": "1.1.0",
  "default_locale": "fr",
  "icons": {
    "16": "img/logo/16.png",
    "128": "img/logo/128.png"
  },
  "browser_action": {
    "default_icon": "img/logo/top.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "notifications",
    "contextMenus",
    "*://*.geneanet.org/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js",
      "start-bg.js"
    ]
  },
  "options_page": "index.html#settings"
}
