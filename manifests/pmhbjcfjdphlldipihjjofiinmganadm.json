{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Horóscopo Virtual",
  "version": "0.1.29",
  "manifest_version": 2,
  "description": "Agora o seu horóscopo diário vai até você!",
  "icons": {
    "16": "img/48x48.png",
    "48": "img/48x48.png",
    "128": "img/128x128.png"
  },
  "background": {
    "scripts": [
      "lib/angular.min.js",
      "js/app.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/48x48.png",
      "38": "img/48x48.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://horoscopovirtual.uol.com.br/horoscopo/xml-geradores/f8923bgd-plugin.xml"
  ],
  "web_accessible_resources": [
    "img/aries.png",
    "img/taurus.png",
    "img/gemini.png",
    "img/cancer.png",
    "img/leo.png",
    "img/virgo.png",
    "img/libra.png",
    "img/scorpio.png",
    "img/sagittarius.png",
    "img/capricorn.png",
    "img/aquarius.png",
    "img/pisces.png"
  ]
}
