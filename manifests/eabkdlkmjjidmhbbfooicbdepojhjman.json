{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SUSHI-BIRTHDAY",
  "description": "Twitter誕生日の風船を寿司にする",
  "version": "1.4",
  "content_scripts": [
    {
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/background.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://twitter.com/*"
  ],
  "icons": {
    "16": "img/icon.png"
  },
  "browser_action": {
    "default_title": "Twitterの誕生日風船を寿司にするプラグイン"
  },
  "web_accessible_resources": [
    "img/maguro-mini.png",
    "img/kabegami.jpg",
    "js/background.js",
    "js/jquery-3.3.1.min.js"
  ],
  "manifest_version": 2
}
