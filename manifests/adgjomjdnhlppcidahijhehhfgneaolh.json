{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery-2.1.0.min.js",
      "js/jquery.cookie.js",
      "js/chromeauth.js",
      "js/chromefraudcookies.js",
      "js/etrade.js",
      "js/controllers/DataController.js",
      "js/controllers/OrderController.js",
      "js/controllers/QuoteController.js",
      "js/controllers/TabController.js",
      "js/controllers/UserController.js",
      "js/models/AccountModel.js",
      "js/models/PortfolioModel.js",
      "js/models/PositionModel.js",
      "js/models/QuoteModel.js",
      "js/models/OrderModel.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icons/etrade-icon-48-grey.png",
    "default_popup": "etch_popup.html",
    "default_title": "E*TRADE Chrome Extension"
  },
  "content_scripts": [
    {
      "js": [
        "js/etrade.parse.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "The E*TRADE Chrome Extension plugs the power of E*TRADE right into your browser.",
  "icons": {
    "128": "img/icons/etrade-icon-128.png",
    "16": "img/icons/etrade-icon-16.png",
    "19": "img/icons/etrade-icon-19.png",
    "256": "img/icons/etrade-icon-256.png",
    "32": "img/icons/etrade-icon-32.png",
    "48": "img/icons/etrade-icon-48.png",
    "64": "img/icons/etrade-icon-64.png"
  },
  "manifest_version": 2,
  "name": "E*TRADE Browser Trading",
  "permissions": [
    "tabs",
    "cookies",
    "alarms",
    "storage",
    "http://*.etrade.com/*",
    "https://*.etrade.com/*"
  ],
  "version": "1.5"
}
