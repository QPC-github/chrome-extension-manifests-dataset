{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cardiology Conferences",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Get a listing of upcoming cardiology cme conferences",
  "icons": {
    "16": "images/GB-19.png",
    "48": "images/GB-48.png",
    "128": "images/GB-128.png"
  },
  "browser_action": {
    "default_icon": "images/GB-19.png",
    "default_title": "Cardiology Conferences",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
