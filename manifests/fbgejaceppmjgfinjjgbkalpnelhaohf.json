{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vocab",
  "short_name": "Helps improve your Vocabulory (GRE/CAT/GMAT/SAT).",
  "version": "1.0",
  "description": "A GRE word everytime you open a Tab. Keep a track of words you learn. (GRE/CAT/GMAT/SAT)",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/art.png",
    "default_popup": "pop-up/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "new-tab/index.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "manifest_version": 2
}
