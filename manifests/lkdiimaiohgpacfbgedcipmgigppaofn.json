{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "McAfee SECURE",
  "short_name": "McAfee SECURE",
  "description": "See which sites are SECURE.",
  "version": "3.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery.min.js",
      "common.js",
      "background.js"
    ]
  },
  "homepage_url": "https://www.mcafeesecure.com/",
  "permissions": [
    "activeTab",
    "background",
    "https://*.mcafeesecure.com/",
    "https://*.trustedsite.com/",
    "https://s3-us-west-2.amazonaws.com/"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_popup": "popup.html",
    "default_title": "McAfee SECURE"
  }
}
