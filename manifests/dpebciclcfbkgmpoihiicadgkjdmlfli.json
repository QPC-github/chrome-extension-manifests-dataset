{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Dart Milestone 1 monitor",
  "version": "0.0.2",
  "description": "Displays the number of open Dart Milestone 1 issues.  Click to visit the webpage",
  "icons": {
    "128": "darticon.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "notifications",
    "http://dartmilestone.appspot.com/"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "dart.png",
    "default_popup": "framed.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
