{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kimola Link Shortening Service",
  "description": "Shortens your links and adds super awesomeness to them!",
  "version": "1.0",
  "author": "Ugur Hasar, Kimola Team",
  "homepage_url": "http://kimo.la",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite",
    "cookies",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
