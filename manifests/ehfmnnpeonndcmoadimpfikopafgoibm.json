{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DistractionOff",
  "version": "1.0",
  "description": "__MSG_extDescription__",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "lib/js/distractionoff.js"
    ]
  },
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "__MSG_extDefault_Title__"
  },
  "default_locale": "en",
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
