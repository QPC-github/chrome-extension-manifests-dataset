{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Looper",
  "version": "2.0.3",
  "description": "Custom loops extension for Youtube videos",
  "permissions": [],
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "parse-1.4.2.min.js",
        "js/youtube-looper.js",
        "init.js"
      ],
      "css": [
        "youtube-looper.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "js/*"
  ]
}
