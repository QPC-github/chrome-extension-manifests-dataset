{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Neue Zürcher Zeitung",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Erhalten Sie in jedem neu geöffneten Tab einen ausgesuchten Artikel aus der «Neuen Zürcher Zeitung».",
  "homepage_url": "https://www.nzz.ch",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "de",
  "chrome_url_overrides": {
    "newtab": "src/extension/index.html"
  },
  "permissions": []
}
