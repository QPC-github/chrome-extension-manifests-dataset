{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "slx History Protection",
  "description": "This extention will help you to keep your history clean",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "48": "icon_gray.png",
      "128": "icon_128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "storage.js",
      "ui.js",
      "background.js"
    ]
  },
  "permissions": [
    "history",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "notifications"
  ]
}
