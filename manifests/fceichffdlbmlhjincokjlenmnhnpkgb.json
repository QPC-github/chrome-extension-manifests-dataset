{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "carrentalinsardinia.com",
  "description": "This extension will change the default background image with an amazing image of Sardinia",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ],
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "carrentalinsardinia.com custom Google page"
  }
}
