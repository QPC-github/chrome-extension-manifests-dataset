{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SimpleDraw",
  "short_name": "SimpleDraw",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "permissions": [
    "fullscreen",
    {
      "fileSystem": [
        "write"
      ]
    }
  ],
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
