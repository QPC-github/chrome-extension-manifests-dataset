{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.1.8",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "default_locale": "zh_CN",
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "19.png",
    "default_popup": "template/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/style.css",
    "images/loading.gif",
    "images/button_2.jpg",
    "images/button_3.jpg",
    "images/close.jpg",
    "images/l_qq.jpg",
    "images/l_sina.jpg",
    "images/l_renren.jpg",
    "images/tuijian.png",
    "images/pu_logo.gif"
  ]
}
