{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Gopi Tyro",
  "browser_action": {
    "default_icon": "img/icon-48.png",
    "default_popup": "popup.html"
  },
  "description": "Free Chrome extention notepad",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "manifest_version": 2,
  "name": "Web Notepad",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "contextMenus"
  ],
  "version": "0.0.1"
}
