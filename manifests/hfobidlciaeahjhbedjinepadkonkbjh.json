{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "Fantasy Football Extention",
  "name": "Fantasy Football Extension",
  "version": "1.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "app/background.js"
    ]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "https://sleeper.app/*"
      ],
      "css": [
        "/static/css/app.css"
      ],
      "js": [
        "/static/js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "/static/css/content.css"
  ]
}
