{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FlyBox",
  "version": "0.1",
  "description": "FlyBox Chrome Extension",
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-1.10.2.min.js",
    "gmail.js",
    "main.js"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "FlyBox"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
