{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "8020 Motivator",
  "description": "Reminds you to integrate the 80/20 rule throughout your day, each time you open a new tab.",
  "version": "1.1",
  "author": "Richard Stewart",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "tab-icon.png"
  }
}
