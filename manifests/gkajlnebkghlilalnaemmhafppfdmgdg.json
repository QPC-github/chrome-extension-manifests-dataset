{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "订单日报采集助手",
  "version": "0.3.3",
  "description": "采集电商日报订单数据",
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "订单助手",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-1.9.0.min.js",
        "js/content-script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "background": {
    "page": "background.html"
  }
}
