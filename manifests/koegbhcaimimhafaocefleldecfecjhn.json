{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Origin",
  "description": "Return to the page from which you opened this tab.",
  "author": "Peter Elmers",
  "version": "1.0.1",
  "icons": {
    "48": "drawing.png",
    "128": "drawing128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "drawing.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested-key": {
        "default": "Ctrl+Shift+U"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
