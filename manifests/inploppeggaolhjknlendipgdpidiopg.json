{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "script.js",
        "popup.js",
        "fancybox/jquery.fancybox.js",
        "fancybox/jquery.fancybox.js",
        "fancybox/helpers/jquery.fancybox-media.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "loader.gif",
    "shipping.png",
    "vk.png",
    "ok.png",
    "fb.png",
    "tw.png",
    "mr.png",
    "favicon.png",
    "alitrack.png",
    "css/font-awesome.css",
    "css/common.css",
    "fonts/fontawesome-webfont.eot",
    "fonts/fontawesome-webfont.svg",
    "fonts/FontAwesome.otf",
    "fonts/fontawesome-webfont.woff",
    "fonts/fontawesome-webfont.woff2",
    "fancybox/jquery.fancybox.css",
    "fancybox/fancybox_sprite.png",
    "fancybox/fancybox_sprite@2x.png",
    "fancybox/fancybox_overlay.png",
    "fancybox/blank.gif",
    "fancybox/fancybox_loading.gif",
    "fancybox/fancybox_loading@2x.gif",
    "fancybox/helper/fancybox_buttons.png"
  ],
  "permissions": [
    "contextMenus",
    "cookies",
    "storage",
    "unlimitedStorage",
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "16": "favicon.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "name": "JDcom.RU",
  "description": "Ассистент покупок и отслеживания посылок на JD.com",
  "version": "2.0.0"
}
