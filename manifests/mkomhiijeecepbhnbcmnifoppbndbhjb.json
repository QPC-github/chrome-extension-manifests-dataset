{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Olffmann",
  "description": "Extension description",
  "version": "1.3",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webNavigation",
    "https://soundcloud.com/olffmann_music/*",
    "https://www.facebook.com/Olffmann/*",
    "https://www.beatport.com/artist/olffmann/551496/*",
    "https://paypal.me/clementfievez/*"
  ],
  "icons": {
    "128": "icon128.png",
    "32": "icon32.png",
    "24": "icon24.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon24.png",
    "default_popup": "popup.html"
  }
}
