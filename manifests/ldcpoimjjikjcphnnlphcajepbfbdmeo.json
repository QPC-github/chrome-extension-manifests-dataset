{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "default_locale": "en",
  "description": "Connect the map in this road building puzzle game.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  },
  "manifest_version": 2,
  "name": "Road Blocks",
  "version": "1.3.0",
  "permissions": [
    "fullscreen",
    "storage"
  ],
  "offline_enabled": true
}
