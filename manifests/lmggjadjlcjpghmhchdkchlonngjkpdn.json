{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "The best new tab for new news.",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Tab For New News",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1.25"
}
