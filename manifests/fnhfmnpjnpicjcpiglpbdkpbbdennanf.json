{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Poor Doge Fund",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/",
    "activeTab",
    "storage"
  ],
  "icons": {
    "460": "doge.png"
  },
  "browser_action": {
    "default_icon": "doge.png"
  }
}
