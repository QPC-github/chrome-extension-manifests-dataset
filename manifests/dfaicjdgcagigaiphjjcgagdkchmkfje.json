{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/paper-16.png",
    "120": "images/paper-120.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "permissions": [
    "idle",
    "https://api.humblepaper.com/*",
    "notifications",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/paper-16.png",
      "36": "images/paper-36.png"
    },
    "default_title": "PaperVC"
  },
  "web_accessible_resources": [
    "images/paper-48.png"
  ]
}
