{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "This extension enables easy tab navigation between links on Hacker News.",
  "name": "HN Tab",
  "icons": {
    "48": "hntab_48x48.png",
    "128": "hntab_128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.0.1"
}
