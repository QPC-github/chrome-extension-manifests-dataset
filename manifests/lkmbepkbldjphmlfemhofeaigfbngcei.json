{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mamba рейтинг",
  "version": "1.1",
  "description": "Оценки и отзывы о пользователях mamba.ru",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://www.mambarating.ru/*",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "browser_action": {
    "default_title": "Mamba рейтинг",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
