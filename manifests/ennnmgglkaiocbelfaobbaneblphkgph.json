{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Force English Wowhead",
  "description": "Redirect to the english wowhead page when loading a localized version.",
  "version": "2.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.wowhead.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
