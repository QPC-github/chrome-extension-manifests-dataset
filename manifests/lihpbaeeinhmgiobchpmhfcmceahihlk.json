{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JavaScript pause/resume",
  "description": "Pauses / resumes JavaScript execution",
  "version": "1.0",
  "permissions": [
    "tabs",
    "debugger"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "debuggerPause.png",
    "default_title": "Pause JavaScript"
  },
  "manifest_version": 2
}
