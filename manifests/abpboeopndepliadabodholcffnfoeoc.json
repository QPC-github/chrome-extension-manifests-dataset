{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "OnlineKhabar.com news extension",
  "version": "1.2.11",
  "description": "",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "OnlineKhabar.com news extension",
    "default_icon": "icon.png",
    "default_popup": "feed.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "content_css.css"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "manifest_version": 2
}
