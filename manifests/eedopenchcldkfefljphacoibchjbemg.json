{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BitCoin Ticker",
  "description": "See price of BitCoin in real time, in EURO.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "http://api.coindesk.com/*"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  }
}
