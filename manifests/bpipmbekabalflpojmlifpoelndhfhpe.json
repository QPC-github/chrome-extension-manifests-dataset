{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "deMobilify",
  "short_name": "deMobilify",
  "version": "0.1.2",
  "description": "Enjoy content at it's fullest - See less of bare, stripped down mobile websites.",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "matches": [
      "*://*/*"
    ],
    "scripts": [
      "lib.js"
    ]
  },
  "icons": {
    "128": "demob.png",
    "64": "demob-64.png",
    "48": "demob-48.png",
    "16": "demob-16.png"
  }
}
