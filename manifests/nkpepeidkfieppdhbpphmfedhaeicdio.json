{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Noticia FM",
  "version": "1.1",
  "description": "Noticia FM",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "Noticia FM"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "http://www.facebook.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "manifest_version": 2
}
