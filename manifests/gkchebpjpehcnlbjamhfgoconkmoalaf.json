{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Group Reddit Saved Links",
  "version": "0.8.2",
  "description": "Group all of your Reddit Saved Links by subReddit.  Automatically loads all saved links in one page.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "savedLinks.js"
      ],
      "matches": [
        "*://*.reddit.com/*"
      ]
    }
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "http://*.reddit.com/*",
    "tabs"
  ]
}
