{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gameroom Online Product Search",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Opens pricing sites",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "popup.js"
    ]
  },
  "permissions": [
    "http://www.google.com/",
    "tabs",
    "activeTab"
  ]
}
