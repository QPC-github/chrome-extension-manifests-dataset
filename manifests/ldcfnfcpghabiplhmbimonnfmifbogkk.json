{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Linkstack",
  "description": "Applicaitons allows creation of multiple links in one",
  "version": "0.0.2.0",
  "author": "Artem Moroz",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "http://www.linxtack.com/*"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/ajax.js"
    ]
  }
}
