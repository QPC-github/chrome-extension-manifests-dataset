{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pronunciation checker",
  "description": "Check and correct your pronunciation in a foreign language",
  "version": "1.1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "web_url": "https://speechpad.pw/prononce.php"
    }
  },
  "icons": {
    "128": "prononce-128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
