{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Paste to YoruFukurou Extension",
  "version": "1.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon48.png",
    "name": "Click to paste the title and URL of the page to YoruFukurou"
  },
  "manifest_version": 2
}
