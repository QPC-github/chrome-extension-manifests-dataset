{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FlipShope",
  "version": "3.0.6",
  "description": "Automatically find and apply coupon codes in one click, Get features like Auto buy, Price Graph, Compare and much more",
  "icons": {
    "128": "images/icon_a.png",
    "19": "images/icon_d.png",
    "32": "images/favicon.png"
  },
  "browser_action": {
    "default_icon": "images/icon_b.png",
    "default_title": "Flipshope",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "bg.html",
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "c0.js"
      ],
      "run_at": "document_end",
      "match_about_blank": false,
      "all_frames": false,
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "homepage_url": "https://flipshope.com",
  "permissions": [
    "clipboardWrite",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "gcm",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "images/*",
    "css/*"
  ],
  "short_name": "Flipshope"
}
