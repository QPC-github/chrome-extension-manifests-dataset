{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DailyEvents",
  "description": "New Tab Page with Daily Events in Google Calendar",
  "version": "1.3.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "identity"
  ],
  "oauth2": {
    "client_id": "616449187551-6g5mvp08tn6olk3auhps2v43rvachcas.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "static/icons/logo_16.png",
      "32": "static/icons/logo_32.png",
      "48": "static/icons/logo_48.png",
      "128": "static/icons/logo_128.png"
    }
  },
  "icons": {
    "16": "static/icons/logo_16.png",
    "32": "static/icons/logo_32.png",
    "48": "static/icons/logo_48.png",
    "128": "static/icons/logo_128.png"
  }
}
