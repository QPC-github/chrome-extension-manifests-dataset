{
  "manifest_version": 2,
  "name": "Share to Teambition",
  "description": "This is a chrome extension to share current webpage to a Teambition project's wall.",
  "version": "1.0.3",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en"
}
