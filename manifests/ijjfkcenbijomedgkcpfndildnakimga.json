{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DarkModeFix",
  "version": "0.1.0",
  "description": "Fixes outlook dark mode",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://outlook.office.com/mail/*",
        "https://outlook.live.com/mail/*"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
