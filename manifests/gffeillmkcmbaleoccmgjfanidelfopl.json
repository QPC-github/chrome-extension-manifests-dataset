{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Startup Refresh/Loading Blocker",
  "description": "This extension blocks initial refresh/loading in background tabs.",
  "version": "1.2",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}
