{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Googleの検索メニューの順番を固定にする。",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Googleの検索メニューの順番を固定にする。",
  "content_scripts": [
    {
      "matches": [
        "https://*.google.co.jp/*",
        "https://*.google.com/*",
        "http://*.google.co.jp/*",
        "http://*.google.com/*"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "run_at": "document_start"
    }
  ]
}
