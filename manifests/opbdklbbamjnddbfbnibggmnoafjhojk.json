{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Switcher",
  "short_name": "Tab Switcher",
  "description": "Displays a list of all tabs in all windows and allows you to switch between them.",
  "version": "0.2",
  "options_page": "options.html",
  "author": "Richard Morris",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "tabswitch.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
