{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "SwissNow - ServiceNOW toolbox",
  "name": "SwissNow - ServiceNOW toolbox",
  "description": "Swiss Knife for ServiceNOW. Lightweight toolbox for ServiceNOW.",
  "version": "0.10.512",
  "version_name": "0.10.512",
  "page_action": {
    "default_icon": "menu_active.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "alarms",
    "notifications",
    "contextMenus",
    "*://*.service-now.com/",
    "file:///*.js"
  ],
  "icons": {
    "16": "menu_active.png",
    "128": "menu_active.png"
  },
  "background": {
    "scripts": [
      "common.js",
      "js/2.5.3-crypto-sha1.js",
      "polling.js",
      "background.js"
    ]
  }
}
