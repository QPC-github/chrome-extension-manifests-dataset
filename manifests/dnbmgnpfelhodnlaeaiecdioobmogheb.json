{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YoukuCast",
  "version": "0.5.2",
  "manifest_version": 2,
  "description": "YoukuCast",
  "minimum_chrome_version": "32",
  "permissions": [
    "http://*.youku.com/*",
    "https://*.youku.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.youku.com/*",
        "https://*.youku.com/*"
      ],
      "js": [
        "youku_addon.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "youku_cast.js",
    "youku_url.js",
    "jquery.js",
    "youku_cast.css",
    "controls.svg",
    "player.css",
    "player.js",
    "cast_off.png",
    "cast_on.png"
  ],
  "icons": {
    "128": "YoukuCast_icon_128.png"
  }
}
