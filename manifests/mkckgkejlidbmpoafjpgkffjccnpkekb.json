{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Coverage Recorder",
  "version": "1.0.03",
  "manifest_version": 2,
  "icons": {
    "16": "16.jpg",
    "48": "48.jpg",
    "128": "128.jpg"
  },
  "description": "Records data about unused code in external stylesheets and scripts and uses recorded data to clean up files from unnecessary stuff",
  "devtools_page": "devtools.html",
  "permissions": [
    "tabs",
    "debugger",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "16.jpg",
    "default_popup": "popup.html"
  },
  "author": "Arto Web Agency",
  "minimum_chrome_version": "71.0.3578.98",
  "version_name": "1.0.03 public beta"
}
