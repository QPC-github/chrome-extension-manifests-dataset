{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "aixigo Jitsi Desktop Streamer",
  "description": "Extension for streaming your desktop into meetings with Jitsi Meet and Jitsi Videobridge. Does only work in company infrastructure",
  "version": "0.0.1",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "jitsi-logo-16x16.png",
    "48": "jitsi-logo-48x48.png",
    "128": "jitsi-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://srvl043.ac.aixigo.de/*"
    ]
  }
}
