{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "ko",
  "name": "__MSG_extName__",
  "short_name": "XenoBookmarks",
  "description": "__MSG_extDescription__",
  "version": "1.9",
  "icons": {
    "16": "image/16x16.png",
    "48": "image/48x48.png",
    "128": "image/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "image/19x19.png",
      "38": "image/38x38.png"
    },
    "default_popup": "bookmarks.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "https://www.google.com/bookmarks/bookmarks.html?hl=ko&sort=title"
  ]
}
