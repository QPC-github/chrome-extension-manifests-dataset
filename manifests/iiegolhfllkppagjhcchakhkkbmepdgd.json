{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iConsumer Button",
  "version": "2.0.5.0",
  "description": "iConsumer Button",
  "homepage_url": "https://www.iconsumer.com",
  "manifest_version": 2,
  "author": "iConsumer.com Holdings LLC",
  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "<all_urls>",
    "cookies"
  ],
  "background": {
    "scripts": [
      "vendors/bundle.js",
      "bg/bundle.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "vendors/bundle.js",
        "content/bundle.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon-24.png",
    "default_title": "iConsumer Button",
    "default_popup": "popup.html"
  },
  "icons": {
    "24": "icon-24.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    "content/*.css",
    "content/*.svg",
    "icon-24.png"
  ]
}
