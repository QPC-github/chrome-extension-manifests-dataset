{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pin My Interest",
  "description": "We can Pin our interest",
  "version": "1.0",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "downloads",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
