{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "uncampaign.me",
  "version": "0.9",
  "description": "Replace mentions of Mitt Romney and Barack Obama on Facebook with vintage presidential campaign ads.",
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "uncampaign.me is ON.  Click to turn OFF."
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "jquery.1.8.1.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery.1.8.1.min.js",
        "facebook.js"
      ]
    }
  ],
