{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Goophim",
  "description": "Getlink, xem phim online không quảng cáo",
  "manifest_version": 2,
  "version": "1.0.23",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Goophim",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon-128.png",
    "48": "images/icon-48.png",
    "16": "images/icon-16.png"
  },
  "web_accessible_resources": [
    "images/icon-16.png"
  ]
}
