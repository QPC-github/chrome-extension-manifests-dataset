{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "C-Strip",
  "description": "Strips css from pages on request",
  "version": "1.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "background",
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
