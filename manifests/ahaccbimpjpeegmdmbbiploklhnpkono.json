{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Prosjek ocjena",
  "version": "1.1",
  "description": "Od početka nove školske godine eDnevnik više ne prikazuje prosjek ocjena. Ovo prošrienje vraća tu funkcionalnost.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://ocjene.skole.hr/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "jquery-3.4.1.min.js"
    ]
  }
}
