{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/popup.js"
    ]
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Quick Script"
  },
  "default_locale": "en",
  "minimum_chrome_version": "14.0",
  "name": "Quick Script",
  "permissions": [
    "tabs",
    "tts",
    "contextMenus",
    "http://*/*"
  ],
  "version": "2.2"
}
