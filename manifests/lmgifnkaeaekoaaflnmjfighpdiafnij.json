{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MasterPBX Click2Call",
  "manifest_version": 2,
  "version": "0.3.0",
  "author": "HoduSoft Pvt. Ltd. (support@hodusoft.com)",
  "description": "It is an Click2Call extension for MasterPBX.",
  "browser_action": {
    "default_icon": {
      "19": "images/ext_icons/icon_19.png",
      "38": "images/ext_icons/icon_38.png"
    },
    "default_title": "MasterPBX Click2Call",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/ext_icons/icon_16.png",
    "48": "images/ext_icons/icon_48.png",
    "128": "images/ext_icons/icon_128.png"
  },
  "options_page": "config.html",
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.js",
      "js/background_n.js"
    ],
    "matches": [
      "*"
    ]
  },
  "permissions": [
    "background",
    "contextMenus",
    "unlimitedStorage",
    "storage",
    "notifications",
    "cookies",
    "*://*/*",
    "<all_urls>",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "*"
  ]
}
