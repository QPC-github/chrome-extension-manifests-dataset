{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DevDocs",
  "short_name": "DevDocs",
  "author": "Sam Collins",
  "description": "Quickly find DevDocs without wasting a tab",
  "version": "0.0.3",
  "manifest_version": 2,
  "offline_enabled": true,
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "DevDocs"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  }
}
