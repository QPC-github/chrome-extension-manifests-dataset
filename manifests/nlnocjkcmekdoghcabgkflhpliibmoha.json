{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple History",
  "description": "A basic history tool to view visted site, no of visists and last visited",
  "version": "1.1",
  "permissions": [
    "history"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "page": "popup.html"
  },
  "web_accessible_resources": [
    "img/icon128.png"
  ]
}
