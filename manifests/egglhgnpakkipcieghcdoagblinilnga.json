{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "[Y] Hacker News = _blank",
  "description": "Simple plugin to set all Hacker News hyperlink targets to '_blank'.",
  "version": "0.21",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
