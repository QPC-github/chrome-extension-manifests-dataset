{
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "loma48.png"
  },
  "description": "Lomadizador de links - Extensao para Chrome",
  "icons": {
    "128": "loma128.png",
    "16": "loma16.png",
    "48": "loma48.png"
  },
  "web_accessible_resources": [
    "error.png",
    "ok.png",
    "wait.png"
  ],
  "name": "Lomadizador de links",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "notifications",
    "http://*.buscape.com/",
    "clipboardWrite"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2.7"
}
