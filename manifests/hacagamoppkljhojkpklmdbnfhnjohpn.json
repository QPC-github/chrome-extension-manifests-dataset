{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Лимит вкладок браузера для корпораций",
  "author": "Клейно Ярослав",
  "version": "1.1",
  "description": "Ограничивает количество открытых вкладок в браузере Chrome до 12 штук",
  "icons": {
    "16": "destroy19.png",
    "48": "destroy48.png",
    "128": "destroy128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Ограничитель количества вкладок до 12",
    "default_icon": "destroy38.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "raindrop.min.js",
      "destroyer.js"
    ]
  }
}
