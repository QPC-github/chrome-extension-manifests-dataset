{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DVP I/O",
  "description": "This extension allows the user to interact with DVP I/O APIs.",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "contextMenus",
    "http://127.0.0.1/*",
    "https://api.dvp.io/*"
  ],
  "background": {
    "scripts": [
      "dvpio.js"
    ],
    "persistent": false
  }
}
