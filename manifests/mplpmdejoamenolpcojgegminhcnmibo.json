{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "drumbit",
  "description": "A very easy to use drum machine.",
  "version": "1.1.20",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "storage",
    {
      "fileSystem": [
        "write"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "128": "icons/icon128.png"
  }
}
