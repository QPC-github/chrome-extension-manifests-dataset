{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "Hit the dot",
  "short_name": "hitthedot",
  "permissions": [
    "storage",
    "notifications"
  ],
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "An easy-to-use and lite Hit-the-dot game",
  "homepage_url": "http://mybrowseraddon.com/hit-the-dot.html",
  "browser_action": {
    "default_icon": "data/icon16.png",
    "default_title": "Hit the dot"
  },
  "icons": {
    "16": "data/icon16.png",
    "32": "data/icon32.png",
    "48": "data/icon48.png",
    "64": "data/icon64.png",
    "128": "data/icon128.png"
  }
}
