{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "My Shareaholic",
  "version": "1.0.2",
  "description": "Share, track, and analyze your links.",
  "manifest_version": 2,
  "app": {
    "urls": [
      "*://*.shareaholic.com/"
    ],
    "launch": {
      "web_url": "https://www.shareaholic.com/login",
      "container": "tab"
    }
  },
  "icons": {
    "128": "images/shareaholic_128x128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "background"
  ]
}
