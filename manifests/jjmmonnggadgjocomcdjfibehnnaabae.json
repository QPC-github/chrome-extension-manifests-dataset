{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Data.com Connect",
  "version": "2.0.3",
  "build_version": "{BUILD_VERSION}",
  "default_locale": "en",
  "description": "Grab & add business contact information from any email account, directly into Data.com Connect, all in just a few clicks.",
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png"
  },
  "browser_action": {
    "default_icon": "img/logo_16.png"
  },
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "window.html"
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "cookies",
    "notifications",
    "contextMenus",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage",
    "identity"
  ]
}
