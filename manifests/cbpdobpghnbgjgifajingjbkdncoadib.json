{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "sublim.nl",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_38.png"
  },
  "content_scripts": [
    {
      "js": [
        "res/js/main.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Enjoy the soothing sounds of pigs squeeling while you type.",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "OinkKeys",
  "short_name": "OinkKeys",
  "version": "0.1",
  "web_accessible_resources": [
    "res/snd/*"
  ]
}
