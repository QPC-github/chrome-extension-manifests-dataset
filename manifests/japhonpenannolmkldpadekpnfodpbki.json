{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "short_name": "'Buy Me a Pie' easy access from Chrome",
  "version": "1.1",
  "description": "Easy access to 'Buy Me a Pie' web application.",
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_title": "'Buy Me a Pie' easy access from Chrome",
    "default_popup": "popup.html"
  },
  "incognito": "split",
  "offline_enabled": false,
  "minimum_chrome_version": "54",
  "permissions": [],
  "optional_permissions": [],
  "commands": {
    "suspend-current": {
      "suggested_key": {
        "default": "Alt+B",
        "mac": "Command+Shift+B",
        "windows": "Alt+B",
        "chromeos": "Alt+B",
        "linux": "Alt+B"
      },
      "description": "Open 'Buy Me a Pie'"
    }
  }
}
