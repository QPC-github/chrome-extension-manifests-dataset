{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Element Path Finder",
  "description": "Displays DOM item details (path, id, class name) when mousing over it.",
  "version": "1.6",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Find element path",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styleskb.css"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "jquery-ui.min.js",
        "jquery_extenstion.js",
        "clipboard.min.js"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
