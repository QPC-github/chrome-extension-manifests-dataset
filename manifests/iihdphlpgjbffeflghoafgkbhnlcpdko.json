{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Html2pdf",
  "version": "2.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "description": "Allows to convert webpages into PDF documents",
  "icons": {
    "16": "pdf.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Convert this page to pdf",
    "default_icon": "pdf.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*"
  ]
}
