{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Links To Embeds",
  "version": "1.0",
  "description": "Turns YouTube links in your active tab to embedded videos.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "youtubetoembed.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
