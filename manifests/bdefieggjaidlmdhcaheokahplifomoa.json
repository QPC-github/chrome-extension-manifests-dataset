{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ecommerce Tally eVanik",
  "version": "5.1",
  "author": "Harsh Ranjan Das",
  "short_name": "Online GST Tally",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://seller.evanik.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Ecommerce Tally eVanik"
  },
  "permissions": [
    "<all_urls>"
  ]
}
