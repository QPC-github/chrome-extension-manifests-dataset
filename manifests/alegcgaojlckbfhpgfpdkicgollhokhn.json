{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NewsBlock",
  "description": "NewsBlock - Facebook Newsfeed Hider",
  "version": "1.0.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "NewsBlock",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "manifest_version": 2
}
