{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save Webpage as Docx",
  "description": "Lưu trang Web dưới dịnh dạng Docx",
  "author": "NTH",
  "version": "0.1.5",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "c.css"
      ],
      "js": [
        "jquery.js",
        "extension.js"
      ]
    }
  ],
  "manifest_version": 2
}
