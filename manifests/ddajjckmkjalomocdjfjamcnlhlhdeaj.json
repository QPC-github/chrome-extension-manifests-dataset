{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SEO Inspector",
  "version": "0.1.4",
  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script/lib/jquery-3.2.1.min.js",
        "script/content.js"
      ]
    }
  ]
}
