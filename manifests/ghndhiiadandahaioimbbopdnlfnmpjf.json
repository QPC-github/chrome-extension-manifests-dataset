{
  "chrome_settings_overrides": {
    "homepage": "http://safe.search.tools",
    "search_provider": {
      "name": "SafeSearch Tools",
      "keyword": "safe.search.tools",
      "search_url": "http://safe.search.tools/search?q={searchTerms}",
      "favicon_url": "http://search.tools/favicon.ico",
      "instant_url": "http://safe.search.tools/search?q={searchTerms}",
      "image_url": "http://search.tools/images?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://safe.search.tools"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "description": "SafeSearch Tools will secure your private identifiable information. Keep your searches private by using our search portal.",
  "icons": {
    "128": "/img/icon128.png",
    "16": "/img/icon16.png",
    "48": "/img/icon48.png"
  },
  "manifest_version": 2,
  "name": "SafeSearch Tools",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs",
    "privacy",
    "webNavigation"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.5.4"
}
