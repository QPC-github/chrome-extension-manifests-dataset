{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Owen CL Helper",
  "version": "0.0.7",
  "description": "Adds extra functionality to owen's courselib pages",
  "content_scripts": [
    {
      "js": [
        "bundle.js"
      ],
      "css": [
        "owen.css"
      ],
      "matches": [
        "https://facweb.cse.msu.edu/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
