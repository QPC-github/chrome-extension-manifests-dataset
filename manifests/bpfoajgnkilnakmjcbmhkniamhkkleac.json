{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tabbex Match",
  "version": "0.0.0.5",
  "description": "A button to close all tabs matching pattens set on options page",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "m19.png",
      "38": "m38.png"
    },
    "default_title": "Close tabs matching list"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "m16.png",
    "48": "m48.png",
    "128": "m128.png"
  },
  "manifest_version": 2
}
