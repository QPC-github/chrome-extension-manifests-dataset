{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Show Me The Points",
  "description": "This extension reveals hotel's points rate in the Hyatt.com search results",
  "version": "2.1",
  "permissions": [
    "*://*.hyatt.com/search/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.hyatt.com/search/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "reveal.js"
      ]
    }
  ]
}
