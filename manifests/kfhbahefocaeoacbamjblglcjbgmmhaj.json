{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Command E extension for Google Chrome",
  "version": "1.3.8",
  "key": "",
  "manifest_version": 2,
  "name": "Command E",
  "short_name": "Command E",
  "permissions": [
    "activeTab",
    "background",
    "bookmarks",
    "cookies",
    "history",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Command E",
    "default_icon": {
      "128": "icon-34.png"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    "falcon.svg"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ]
}
