{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Analytics Views Per Hour/Minute",
  "version": "1.0.2",
  "description": "YouTube Analytics Views Per Hour/Minute",
  "manifest_version": 2,
  "permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "vphm.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
