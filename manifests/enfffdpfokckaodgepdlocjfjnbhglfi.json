{
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/sstk-chrome-ext-128-inactive.png",
      "16": "icons/sstk-chrome-ext-16-inactive.png",
      "48": "icons/sstk-chrome-ext-48-inactive.png"
    },
    "default_title": "视觉中国识图"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "scripts/contentscript.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "default_locale": "zh_CN",
  "description": "__MSG_appDesc__",
  "externally_connectable": {
    "matches": [
      "*://*.vcg.com/*"
    ]
  },
  "icons": {
    "128": "icons/sstk-chrome-ext-128.png",
    "16": "icons/sstk-chrome-ext-16.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "activeTab",
    "webRequest",
    "contextMenus",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*",
    "downloads"
  ],
  "short_name": "VCG识图",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.3",
  "web_accessible_resources": [
    "images/*.png"
  ]
}
