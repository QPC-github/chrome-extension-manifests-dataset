{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "reply.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "自动回复隐藏帖 (要求已登录, 改自同名 Chrome 插件, 版权归原作者所有)",
  "icons": {
    "128": "icon.png"
  },
  "name": "隐藏帖自动回复 (AutoReply 纯净版)",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.0.1"
}
