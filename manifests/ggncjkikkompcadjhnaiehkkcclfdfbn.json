{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "sigbop, LLC",
  "name": "sigbop-Email Tracking & Signatures for Gmail",
  "version": "18.6.26.9",
  "description": "sigbop lets you build dynamic signatures with email tracking and click statistics.",
  "homepage_url": "http://www.sigbop.com",
  "content_security_policy": "script-src 'self' ; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "css": [
        "css/jquery-ui.css",
        "css/email-footer.css"
      ],
      "js": [
        "browser.chrome.js",
        "libs/jquery-1.11.0.js",
        "libs/rangy-core.js",
        "libs/jquery.caret.js",
        "libs/jquery-ui.js",
        "storage/SigBOPapi.js",
        "storage/SigBOPstore.js",
        "storage/jGuid.js",
        "gmail/gmToolBar.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "browser.chrome.js",
      "libs/jquery-1.11.0.js",
      "libs/jquery-ui.js",
      "libs/json2.js",
      "storage/SigBOPapi.js",
      "storage/SigBOPstore.js",
      "storage/jGuid.js",
      "background/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "resources/*",
    "icons/*",
    "images/*",
    "css/*",
    "gmail/gmToolBar.html"
  ],
  "page_action": {
    "default_icon": "icons/s_16.png",
    "default_title": "sigbopâ„¢ Extension",
    "default_popup": "popup/sbpopup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "idle",
    "tabs",
    "webNavigation",
    "https://mail.google.com/*",
    "*://*.sigbop.com/*"
  ],
  "icons": {
    "16": "icons/s_16.png",
    "48": "icons/s_48.png",
    "128": "icons/s_128.png"
  }
}
