{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lynks - Bookmark Manager",
  "short_name": "Lynks",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "Bookmark your favorite links using this extension",
  "icons": {
    "128": "link-symbol-128.png",
    "48": "link-symbol-48.png",
    "16": "link-symbol-16.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "identity",
    "cookies",
    "tabs",
    "*://*/*"
  ],
  "browser_action": {
    "default_title": "Bookmark your favorite links",
    "default_icon": "link-symbol-128.png",
    "default_popup": "popup.html"
  }
}
