{
  "name": "Image Downloader",
  "description": "te31.com(rgrong), slrclub.com, instagram",
  "version": "1.1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "jquery.min.js",
        "contents.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "rightbutton.js",
    "instagram.js",
    "slrclub.js",
    "jquery.min.js"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2
}
