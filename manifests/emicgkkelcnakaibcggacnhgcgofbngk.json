{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LOLrecordChecker (Extension)",
  "version": "1.1.0",
  "version_name": "1.1 Chrome Extension Version",
  "manifest_version": 2,
  "description": "Check all 5 player's record quicker!",
