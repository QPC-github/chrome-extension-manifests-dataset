{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "mK IOGamedead",
  "short_name": "mKIOAuto",
  "description": "ImperiaOnline automation toolkit.",
  "version": "0.9.3",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.imperiaonline.org/imperia/game_v5/*"
      ],
      "js": [
        "defaultOptions.js",
        "iov5/iov5Executer.js",
        "iov5/iov5Alliance_Attack.js",
        "iov5/iov5Attack.js",
        "iov5/iov5Construction.js",
        "iov5/iov5Espionage.js",
        "iov5/iov5Festival.js",
        "iov5/iov5HideArmy.js",
        "iov5/iov5HideResources.js",
        "iov5/iov5Incognito.js",
        "iov5/iov5Market.js",
        "iov5/iov5Population.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://*.imperiaonline.org/*"
      ],
      "js": [
        "defaultOptions.js",
        "iov5/iov5Executer.js",
        "iov5/iov5KeepAlive.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "mK IOv5 Automizer Settings"
  }
}
