{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icons/rsicon48.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Radio Srood chrome extension simply lets you listen to Afghan music from your Google Chrome toolbar",
  "icons": {
    "128": "img/icons/rsicon128.png",
    "16": "img/icons/rsicon16.png",
    "48": "img/icons/rsicon48.png"
  },
  "manifest_version": 2,
  "name": "Radio Srood Player",
  "permissions": [
    "http://www.radiosrood.com/"
  ],
  "version": "2.0"
}
