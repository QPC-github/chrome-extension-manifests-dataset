{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "fix.js"
      ],
      "matches": [
        "http://*.hyves.nl/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://*.hyves.nl/*",
    "contextMenus"
  ],
  "description": "Voeg een 'Download'-knop toe aan foto's op Hyves",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "name": "Hyves fotodownload",
  "version": "1.1"
}
