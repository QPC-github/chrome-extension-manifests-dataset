{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Table Browser Caller for D365FO",
  "short_name": "Table Browser Caller",
  "description": "Table browser caller for Microsoft Dynamics 365 for Finance and Operations.",
  "version": "1.3.2",
  "author": "Arcantos Tools",
  "browser_action": {
    "default_title": "Table Browser Caller",
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://*/.dynamics.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
