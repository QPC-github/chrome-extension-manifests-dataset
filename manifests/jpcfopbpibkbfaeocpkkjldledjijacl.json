{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PDF Converted - Convert Documents",
  "version": "1.0.0",
  "description": "Convert your Documents to PDF and vice-versa format for free.",
  "icons": {
    "128": "logo-pdf-128.png",
    "48": "logo-pdf-48.png",
    "16": "logo-pdf-16.png"
  },
  "container": [
    "GOOGLE_DRIVE"
  ],
  "browser_action": {
    "default_icon": {
      "16": "logo-pdf-16.png",
      "32": "logo-pdf-32.png"
    }
  },
  "background": {
    "scripts": [
      "/js/common-push-init.js",
      "/js/common-push.js",
      "/js/background.js"
    ]
  },
  "permissions": [
    "https://*.pdfconverted.com/*",
    "notifications"
  ]
}
