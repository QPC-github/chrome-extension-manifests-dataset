{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Biased?",
  "version": "1.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "popup.js",
      "Chart.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "activeTab"
  ]
}
