{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Recorder",
  "description": "Record your Web History",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "16": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "icon1.png",
    "default_title": "Record/Stop"
  },
  "background": {
    "scripts": [
      "Record.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "chrome://favicon/",
    "management"
  ]
}
