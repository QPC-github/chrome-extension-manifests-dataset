{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inline Pinterest",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adds an inline Pinterest Pin button to all images viewed in Chrome",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "pinterestinline.js"
      ]
    }
  ]
}
