{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "w3down",
  "description": "Baixe fotos, videos e historias de maneira f√°cil ",
  "homepage_url": "http://w3solution.com.br/w3down",
  "version": "0.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "img/icon-16x16.png",
    "32": "img/icon-32x32.png",
    "128": "img/icon-128x128.png"
  },
  "browser_action": {
    "default_title": "w3down",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "instadown.css"
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://www.instagram.com/"
  ]
}
