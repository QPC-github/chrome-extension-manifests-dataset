{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "lib/jquery.min.js",
        "lib/buy.js",
        "class/IndexedDB.js",
        "class/Note.js",
        "class/Notes.js",
        "class/Notifications.js",
        "class/Store.js",
        "class/Sync.js",
        "class/SyncMethod.js",
        "class/SyncViaMethods.js",
        "class/SyncFileSystem.js",
        "background/general.js",
        "background/background.js"
      ]
    }
  },
  "author": "ProWebJect - Michał Góra",
  "description": "The first, truly sticky app for notes on your desktop! It is easy to manage and customize. Now also supports speech recognition!",
  "icons": {
    "128": "img/icon_128.png",
    "19": "img/icon_19.png",
    "38": "img/icon_38.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "35",
  "name": "Sticky Notes",
  "offline_enabled": true,
  "optional_permissions": [
    "audioCapture",
    "background"
  ],
  "permissions": [
    "notifications",
    "storage",
    "syncFileSystem",
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    },
    "identity",
    "alwaysOnTopWindows",
    "tts",
    "clipboardWrite",
    "clipboardRead",
    "webview",
    "http://prowebject.com/*",
    "http://www.prowebject.com/*",
    "https://www.paypalobjects.com/*"
  ],
  "short_name": "Sticky Notes",
  "version": "1.17.1.6"
}
