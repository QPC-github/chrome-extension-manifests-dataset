{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AJAX Debugger",
  "manifest_version": 2,
  "version": "1.0.4",
  "minimum_chrome_version": "10.0",
  "description": "Logs all AJAX activity to the Dev Tools Console, allowing inspection of AJAX calls, and open calls in new tab with all the inputs.",
  "devtools_page": "debugger.html",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
