{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Danimal Soundboard!",
  "version": "1.0.3",
  "description": "Soundboard for annoying office guests",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "script.js",
      "jquery.js"
    ],
    "persistent": false
  }
}
