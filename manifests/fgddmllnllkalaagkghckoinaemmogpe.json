{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_app_name_title__",
  "short_name": "__MSG_app_short_name_title__",
  "version": "4.9.4.3993",
  "manifest_version": 2,
  "description": "__MSG_app_description_text__",
  "author": "ExpressVPN",
  "icons": {
    "16": "images/toolbar-icon-16.png",
    "32": "images/toolbar-icon-32.png",
    "48": "images/toolbar-icon-48.png",
    "128": "images/toolbar-icon-128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Shift+Ctrl+X",
        "mac": "Shift+Command+O"
      }
    }
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "cookies",
    "nativeMessaging",
    "privacy",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "unlimitedStorage",
    "notifications",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "ExpressVPN",
    "default_popup": "html/popup.html"
  },
  "web_accessible_resources": [
    "images/*",
    "html/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start",
      "js": [
        "scripts/content/gps.js"
      ]
    }
  ],
  "minimum_chrome_version": "55",
  "key": ""
}
