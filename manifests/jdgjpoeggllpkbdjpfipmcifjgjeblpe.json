{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Production Warning Bar",
  "short_name": "Production Warning Bar",
  "version": "0.1.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
