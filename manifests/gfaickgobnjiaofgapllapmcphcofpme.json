{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clasp",
  "description": "A simple, point-and-click record extractor.",
  "version": "1.0.0.6",
  "icons": {
    "16": "img/clasp-48.png",
    "48": "img/clasp-48.png",
    "128": "img/clasp-320.png",
    "320": "img/clasp-320.png"
  },
  "browser_action": {
    "default_icon": "img/clasp-48.png",
    "default_title": "Scrape a list of records from this page."
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "js/event.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "css/nodepick.css",
    "css/frame.css",
    "css/home.css",
    "css/help.css",
    "css/msgbox.css",
    "css/export.css",
    "img/clasp-48.png",
    "img/close-48.png",
    "img/donate-48.png",
    "img/text-48.png",
    "img/link-48.png",
    "img/landscape-48.png",
    "img/help-48.png"
  ]
}
