{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EdVIE Corp Screen share",
  "description": "A simple extension that allows you to stream your desktop into meetings with Edvie Live Class.",
  "version": "0.1.7",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "Edvie_16.png",
    "48": "Edvie_48.png",
    "128": "Edvie_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://corpliveclass.edvie.com/*"
    ]
  }
}
