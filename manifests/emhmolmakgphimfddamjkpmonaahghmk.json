{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "☆*･゜ﾟ･*\\(^O^)/*･゜ﾟ･*☆",
  "short_name": "temoji:)",
  "description": "temoji - your simple emoji library in the browser.",
  "version": "0.1.1",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "temoji.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "manifest_version": 2
}
