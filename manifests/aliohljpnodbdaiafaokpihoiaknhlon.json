{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "5bob.png",
    "default_title": "__MSG_appDesc__"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "5bob.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "storage",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
