{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icons/ip_adress.png",
    "default_popup": "popup.html"
  },
  "description": "This extension will show you website ip",
  "icons": {
    "256": "icons/256.png",
    "128": "icons/128.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name": "WebsiteIp finder",
  "permissions": [
    "management",
    "storage",
    "unlimitedStorage",
    "tabs"
  ],
  "version": "1.0.1"
}
