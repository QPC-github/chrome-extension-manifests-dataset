{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mathrubhumi",
  "description": "Mathrubhumi",
  "version": "2.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popups.html",
    "default_title": "Read our news here!"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
