{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Uncloud your Tab",
  "description": "An Easy to go extension! A new tab organised with all your information.",
  "version": "3.1",
  "author": "Rajasri",
  "browser_action": {
    "default_icon": "images/rajasrifavicon.png"
  },
  "background": {
    "page": "index.html"
  },
  "commands": {
    "duplicate-tab-keyboard-shortcut": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "Duplicate tab"
    },
    "close-other-tabs-shortcut": {
      "description": "Close Other Tabs",
      "suggested_key": {
        "default": "Alt+Shift+C"
      }
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "history",
    "bookmarks",
    "topSites",
    "storage"
  ]
}
