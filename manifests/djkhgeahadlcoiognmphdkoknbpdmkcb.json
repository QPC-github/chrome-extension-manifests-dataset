{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "name": "Email Finder",
  "description": "Email Finder is tool to search, find, sent and download email address on current webpages. Useful Email Finder.",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/ico_19.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "img/ico_16.png",
    "48": "img/ico_48.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "downloads",
    "notifications"
  ]
}
