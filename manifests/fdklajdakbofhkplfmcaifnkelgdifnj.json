{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuickerSell",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Price suggestion for trulia browsing",
  "icons": {
    "128": "icons/tx.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/tx-g.png",
    "default_title": "QuickerSell",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ]
}
