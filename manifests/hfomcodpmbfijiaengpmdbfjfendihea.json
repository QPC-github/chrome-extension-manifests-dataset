{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DpHack",
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.dpstream.net/external_link/*"
      ],
      "js": [
        "open.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "fav.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "fav.png",
    "48": "fav.png",
    "128": "fav.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
