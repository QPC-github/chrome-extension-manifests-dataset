{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AOTool",
  "description": "Thomas More Research Tool | Concept & Content: Alex Wuyts | Coding & Design: Fatjon Zariqi & Hamza Raja",
  "version": "2.0.2",
  "short_name": "The Alpha-Omega-toolbar",
  "icons": {
    "64": "img/64_icon.png",
    "128": "img/128_icon.png",
    "512": "img/512_icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/512_icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
