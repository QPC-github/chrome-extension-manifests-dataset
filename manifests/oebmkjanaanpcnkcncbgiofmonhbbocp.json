{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dec to Bin converter",
  "description": "This extension can convert any decimal number to binary",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Dec-Bin"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
