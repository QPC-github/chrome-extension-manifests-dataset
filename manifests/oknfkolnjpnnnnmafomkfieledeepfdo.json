{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WorkFlowy Bookmark",
  "manifest_version": 2,
  "description": "Organize your brain. This app is a shortcut for the WorkFlowy.com website.",
  "version": "1.1",
  "app": {
    "urls": [
      "*://*.workflowy.com/"
    ],
    "launch": {
      "web_url": "https://workflowy.com/"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "clipboardRead",
    "clipboardWrite"
  ]
}
