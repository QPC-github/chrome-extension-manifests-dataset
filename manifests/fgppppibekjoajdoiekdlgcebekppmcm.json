{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jet Search",
  "version": "0.3",
  "manifest_version": 2,
  "description": "A Middle-Man Solution to poor Search Results",
  "browser_action": {
    "default_icon": {
      "19": "jet.png"
    },
    "default_title": "Jet Search",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
