{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "parameters.js",
      "youtube.js",
      "background.js",
      "chrome.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_off.png",
    "default_title": "LordDVD - Offline",
    "default_popup": "popup.html"
  },
  "description": "Ne manquez plus un seul stream de LordDVD !",
  "icons": {
    "256": "icons/logo256.png",
    "128": "icons/logo128.png",
    "32": "icons/logo32.png",
    "64": "icons/logo64.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2,
  "name": "LordDVD Live Extension",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://api.twitch.tv/",
    "https://www.googleapis.com/",
    "notifications",
    "storage"
  ],
  "version": "1.4.0"
}
