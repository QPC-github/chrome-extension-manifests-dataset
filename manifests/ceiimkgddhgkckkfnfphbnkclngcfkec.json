{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Protect-ID Security audit",
  "description": "Shop safely! This extension helps you evaluate a website for shopping security.",
  "version": "2.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://www.protect-id.com/"
  ]
}
