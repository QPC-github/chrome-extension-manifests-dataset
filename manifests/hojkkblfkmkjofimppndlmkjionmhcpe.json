{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "XKCD News Fixer",
  "version": "0.1.1",
  "permissions": [
    "http://*.cnn.com/*",
    "http://*.nbcnews.com/*",
    "http://news.google.com/*",
    "http://*.foxnews/com/*",
    "http://news.yahoo.com/*",
    "http://*.dallasnews.com/*",
    "http://news.cnet.com/*",
    "http://*.cbsnews.com/*",
    "http://*.npr.org/sections/news/*",
    "http://*.drudgereport.com/*",
    "http://*.usatoday.com/*",
    "http://*.huffingtonpost.com/*",
    "http://www.un.org/news/*",
    "http://*.nydailynews.com/*",
    "http://*.detroitnews.com/*",
    "http://*.nytimes.com/*",
    "http://*.wsj.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.cnn.com/*",
        "http://*.nbcnews.com/*",
        "http://news.google.com/*",
        "http://*.foxnews/com/*",
        "http://news.yahoo.com/*",
        "http://*.dallasnews.com/*",
        "http://news.cnet.com/*",
        "http://*.cbsnews.com/*",
        "http://*.npr.org/sections/news/*",
        "http://*.drudgereport.com/*",
        "http://*.usatoday.com/*",
        "http://*.huffingtonpost.com/*",
        "http://www.un.org/news/*",
        "http://*.nydailynews.com/*",
        "http://*.detroitnews.com/*",
        "http://*.nytimes.com/*",
        "http://*.wsj.com/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
