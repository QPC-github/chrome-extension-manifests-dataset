{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter social tooolkit",
  "description": "Allows you to share text, images, content, links, page, etc to your twitter account",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistant": false
  },
  "icons": {
    "64": "icon.png",
    "32": "icon.png",
    "16": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "contextMenus"
  ]
}
