{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "page_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "punycode.min.js",
      "locache.js",
      "bg.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "homepage_url": "https://dnslytics.com",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "permissions": [
    "https://*.dnslytics.net/*",
    "tabs"
  ],
  "version": "0.1.9",
  "content_security_policy": "default-src 'self'; connect-src https://*.dnslytics.net"
}
