{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WingMan Freemium for OkCupid",
  "version": "0.0.0.8",
  "description": "WingMan is a dating automation extension for online dating services.",
  "author": "WingMan",
  "homepage_url": "https://www.okcupid.com/doubletake",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon_128.png",
    "default_title": "WingMan Freemium for OkCupid",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.okcupid.com/doubletake",
    "https://www.okcupid.com/profile/*",
    "https://www.okcupid.com/match",
    "tabs"
  ],
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.okcupid.com/doubletake"
      ],
      "js": [
        "scripts/navbar_to_back.js"
      ]
    }
  ]
}
