{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "People Widget Begone",
  "description": "Removes the people widget & white space in Gmail",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://mail.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "https://mail.google.com/*"
  ],
  "short_name": "People Widget Begone",
  "version": "1.0"
}
