{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "A theme solely devoted to the king of sarcasm, Chandler Bing from series F.R.I.E.N.D.S. Enjoy :-)",
  "name": "Chandler Bing F.R.I.E.N.D.S Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_popup": "popupChandler.html",
    "default_title": "I'm a pretty little girl :-*"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Chandler-Bing.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
