{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BookMarkCleaner",
  "description": "404 Not Foundになったリンク先の見つからないブックマークを一括削除します。",
  "version": "1.2.1",
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "icons/icon_131780_16.png",
    "48": "icons/icon_131780_48.png",
    "128": "icons/icon_131780_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_131780_16.png",
    "default_title": "BookMarkCleaner",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://www.be-s.co.jp; object-src 'self'"
}
