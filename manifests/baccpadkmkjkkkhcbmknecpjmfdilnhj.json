{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KTH Chrome Login",
  "description": "This extension automatically signs you in with KTH.se.",
  "version": "1.1.1",
  "background": {
    "scripts": [
      "jquery-2.2.3.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://login.kth.se/*",
    "https://www.kth.se/*"
  ]
}
