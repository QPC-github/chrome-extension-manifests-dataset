{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inner Peace - Experience Peace In One Click",
  "short_name": "Peace App",
  "version": "1.6.1",
  "manifest_version": 2,
  "description": "Take a short mindful break to calm down and find inner peace. Just one click away.",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
