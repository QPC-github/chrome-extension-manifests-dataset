{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Szlakiem Jedi Tracker",
  "short_name": "SJTracker",
  "version": "1.0.4",
  "description": "Extension made exclusively for szlakiem-jedi.pl. It allows to track players count on the server and summon whole tracker in a tray",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms",
    "http://szlakiem-jedi.gameclan.pl/gameq/new_tracker.php"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2
}
