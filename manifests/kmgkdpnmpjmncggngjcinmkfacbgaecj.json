{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Related Redmine",
  "version": "0.2.6",
  "manifest_version": 2,
  "description": "Show Redmine issues related by displayed page.",
  "homepage_url": "https://github.com/nishimura/related-redmine",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs"
  ],
  "options_page": "options.html"
}
