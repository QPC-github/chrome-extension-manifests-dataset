{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gomo Learning Suite",
  "description": "This extension provides quick access to the Gomo Learning Suite",
  "version": "1.1",
  "browser_action": {
    "default_icon": "gomo-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.gomolearning.com/"
  ]
}
