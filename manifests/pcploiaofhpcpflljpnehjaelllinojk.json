{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SmartStar",
  "description": "Fast save bookmark to the current page to a predefined folder.",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "icon-19.png"
    },
    "default_title": "Star this page"
  }
}
