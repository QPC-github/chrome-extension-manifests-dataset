{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MovesCollect Plugin",
  "description": "This extension will import an item into your MovesScrapbook account ",
  "version": "1.0",
  "options_page": "/app/html/options.html",
  "options_ui": {
    "page": "/app/html/options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "/public/images/dancer-19px.png",
    "default_popup": "/app/html/popup.html"
  },
  "permissions": [
    "activeTab",
    "*://portal.culturemoves.eu/api/*",
    "storage"
  ]
}
