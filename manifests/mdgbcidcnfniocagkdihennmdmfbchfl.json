{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DinBrasov.com.ro ",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Verifica daca site-ul tau este prezent pe DinBrasov.com.ro",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
