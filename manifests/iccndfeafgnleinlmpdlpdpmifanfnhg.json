{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Insanity for Chrome",
  "description": "Blast holes in the Internet.",
  "version": "0.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "clancy.png",
    "gun-gunshot-02.mp3",
    "bullethole_1.png"
  ],
  "browser_action": {
    "default_title": "Take Aim",
    "default_icon": "icon48.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
