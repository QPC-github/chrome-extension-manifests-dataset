{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facebook News to Cats",
  "description": "Replaces the Facebook news bar (desktop Facebook version) with a random cat GIF",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "facebookcats.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Cat-ify my news feed",
    "default_icon": "icon_32.png"
  },
  "permissions": [
    "activeTab"
  ]
}
