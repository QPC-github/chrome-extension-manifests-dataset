{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Your Fresh Tab",
  "short_name": "YFT",
  "version": "1.0",
  "description": "Energize yourself with new and fresh image whenever you open a new Tab.",
  "author": "Dipak Jain",
  "incognito": "split",
  "permissions": [
    "tabs",
    "chrome://favicon/*"
  ],
  "chrome_url_overrides": {
    "newtab": "fresh-tab.html"
  },
  "browser_action": {
    "default_title": "Your Fresh Tab",
    "default_icon": "app-icon.png"
  },
  "manifest_version": 2
}
