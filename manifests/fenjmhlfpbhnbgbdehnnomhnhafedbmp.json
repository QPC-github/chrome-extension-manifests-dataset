{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "short_name": "motiontabs",
  "description": "__MSG_extDesc__",
  "version": "2.3",
  "default_locale": "en",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "/assets/logo.png",
    "48": "/assets/logo.png",
    "128": "/assets/logo.png"
  },
  "browser_action": {
    "default_icon": "/assets/logo.png"
  },
  "background": {
    "scripts": [
      "/js/global.js",
      "/js/offlineStorage.js",
      "/js/jquery.js",
      "/js/library.js",
      "/js/worker.js"
    ]
  },
  "permissions": [
    "background",
    "storage",
    "unlimitedStorage",
    "notifications",
    "bookmarks",
    "contextMenus",
    "management",
    "topSites",
    "activeTab",
    "history"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://json.geoiplookup.io https://motiontabs.com object-src 'self'",
  "offline_enabled": true,
  "manifest_version": 2,
  "googleAnalyticsID": "UA-137690561-1"
}
