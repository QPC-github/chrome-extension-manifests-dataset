{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "5BARz extension",
  "description": "The most popular browsing insight extension",
  "version": "1.06",
  "background": {
    "scripts": [
      "back.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "history",
    "http://192.168.123.254/boaform/browseInside/",
    "http://192.168.123.254/boaform/browseInside",
    "http://192.168.123.254/boafrm/SerialNumforApp",
    "background",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://cloud1.5barzindia.com/*",
        "http://cloud.5barzindia.com/*"
      ],
      "js": [
        "modifyContent.js"
      ]
    }
  ]
}
