{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qwg",
  "description": "Reprogramme the Chrome Omnibox",
  "version": "1.8",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "omnibox": {
    "keyword": "qwg"
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  }
}
