{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Поисковый помощник",
  "description": "Позволяет добавлять закладки на страницы в Поисковый помощник",
  "version": "1.0",
  "permissions": [
    "http://search_helper.dev/",
    "http://intelligentfinder.ru/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
