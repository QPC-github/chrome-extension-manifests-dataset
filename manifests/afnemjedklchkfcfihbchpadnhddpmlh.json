{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nativ Picture Extension ",
  "description": "Add/Remove pictures",
  "version": "1.48",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "background": {
    "scripts": [
      "sample.js",
      "jquery.min.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js",
        "runSubmission.js"
      ]
    }
  ],
  "manifest_version": 2
}
