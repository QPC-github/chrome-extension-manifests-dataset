    "128": "icon128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "reddit-booru.js"
    ]
  },
  "applications": {
    "gecko": {
      "id": "jid1-ud8I7tLgCJzpTg@jetpack",
      "strict_min_version": "42.0"
    }
  }
}
