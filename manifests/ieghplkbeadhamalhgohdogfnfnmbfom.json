{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nuttit Spank Bank Image Adder",
  "description": "This extension allows any picture from any site to be added to your Nuttit Spank Bank",
  "version": "1.1",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "jquery-1.10.2.min.js",
      "bg.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
    "http://nuttit.com",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
