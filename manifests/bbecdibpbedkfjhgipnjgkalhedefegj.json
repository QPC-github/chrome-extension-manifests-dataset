{
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_popup": "popup.html"
  },
  "description": "Quick form to plan your weekly tasks and journal done items. Share key goals, accomplishments and challenges with your team.",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Weekdone - Plan and share your weekly tasks",
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "https://*.weekdone.com/",
    "http://*.weekdone.com/"
  ],
  "short_name": "Weekdone",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.3"
}
