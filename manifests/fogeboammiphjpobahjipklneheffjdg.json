{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CSC Mailer",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Nominate Sports Scores to Crowd Sports Cast",
  "icons": {
    "128": "icon_128x128.png"
  },
  "browser_action": {
    "default_title": "Send these scores to CrowdSportsCast",
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
