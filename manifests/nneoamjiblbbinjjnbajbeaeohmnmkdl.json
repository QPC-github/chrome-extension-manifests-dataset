{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Marcador",
  "version": "0.0.0.4",
  "description": "Add bookmarks directly to your Dropbox.",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "identity"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
