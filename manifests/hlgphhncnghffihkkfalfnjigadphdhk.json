{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CPC View web edition",
  "short_name": "CPC View",
  "version": "1.6.2",
  "minimum_chrome_version": "44.0.0.0",
  "description": "View and print image files in CPC, multi-page TIFF, CALS, JEDMICS C4 and portable anymap formats",
  "icons": {
    "16": "data/images/AppIcons/icon_16x16@2x.png",
    "32": "data/images/AppIcons/icon_32x32@2x.png",
    "128": "data/images/AppIcons/icon_128x128@2x.png",
    "256": "data/images/AppIcons/icon_256x256@2x.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "file://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "CPCView-WebExtension.min.js"
    ]
  },
  "web_accessible_resources": [
    "*",
    "*/*",
    "*/*/*",
    "*/*/*/*"
  ],
  "options_page": "data/options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "data/options.html"
  },
  "key": ""
}
