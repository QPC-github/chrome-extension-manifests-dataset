{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "A simple extension that allows you to stream your desktop into meetings with viewme webRTC client.",
  "externally_connectable": {
    "matches": [
      "*://hariezuce.evo.upjs.sk/vaas_webrtc/*",
      "*://viewme.ezuce.com/redback/*",
      "*://rbvideo.com.au/webrtc/*"
    ]
  },
  "icons": {
    "128": "viewme-logo-128x128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "redback Desktop Streamer",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.5"
}
