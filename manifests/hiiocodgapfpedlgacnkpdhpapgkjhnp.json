{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vanilla-Icify",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Replaces all n-word mentions with 'friend'",
  "content_scripts": [
    {
      "matches": [
        "http://rapgenius.com/*"
      ],
      "js": [
        "jquery-1.7.1.js",
        "vanillaicify_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
