{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Saver",
  "version": "0.2",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "HTML.html"
  }
}
