{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "f(x) Traducteur pour Spreadsheet",
  "description": "Saisissez votre formule excel française dans la zone de texte et découvrez la formule Google Spreadsheet correspondante.",
  "version": "1.0",
  "icons": {
    "128": "icone-128.png",
    "16": "icone-16.png",
    "32": "icone-32.png",
    "64": "icone-64.png",
    "48": "icone-48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icone-16.png",
    "default_title": "f(x) Traducteur pour Spreadsheet",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
