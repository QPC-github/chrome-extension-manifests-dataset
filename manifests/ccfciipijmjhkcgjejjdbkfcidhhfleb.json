{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GamEffective",
  "version": "3.0.0",
  "manifest_version": 2,
  "description": "GamEffective Client",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "content_script.js",
      "scriptadded.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "GamEffective Client"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "web_accessible_resources": [
    "enwidget.png",
    "closeApp.png"
  ]
}
