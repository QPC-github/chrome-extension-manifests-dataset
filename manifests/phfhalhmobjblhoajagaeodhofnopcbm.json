{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Trial Racing",
  "short_name": "TTR",
  "version": "6.1.0.0",
  "description": "Set personal bests around tracks with your favorite cars!",
  "permissions": [
    "declarativeContent",
    "storage",
    "https://jsonstorage.net/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon128.png"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon128.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}
