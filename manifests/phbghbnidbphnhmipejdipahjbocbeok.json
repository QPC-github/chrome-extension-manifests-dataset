{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "qihl tracker",
  "version": "1.1.2",
  "description": "Tracking users games played and MMR",
  "permissions": [
    "activeTab",
    "https://steamcommunity.com/profile/*",
    "storage",
    "*://qihl.gg/*"
  ],
  "background": {
    "scripts": [
      "steamHandler.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/qihl16.png",
      "32": "images/qihl32.png",
      "48": "images/qihl48.png",
      "128": "images/qihl128.png"
    }
  },
  "icons": {
    "16": "images/qihl16.png",
    "32": "images/qihl32.png",
    "48": "images/qihl48.png",
    "128": "images/qihl128.png"
  },
  "manifest_version": 2
}
