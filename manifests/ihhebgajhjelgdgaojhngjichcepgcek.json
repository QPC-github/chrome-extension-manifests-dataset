{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/icon48.png",
    "default_popup": "/html/popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "/css/style.css"
      ],
      "js": [
        "/libraries/jquery-2.2.1.min.js",
        "/js/script.js"
      ],
      "matches": [
        "*://*.amazon.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "",
  "icons": {
    "128": "img/icon128.png",
    "19": "img/icon48.png",
    "38": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "Cleer Platinum Quick View Subscription",
  "permissions": [
    "*://*.amazon.com/*",
    "webRequest"
  ],
  "version": "1.0.6",
  "web_accessible_resources": [
    "/img/*"
  ]
}
