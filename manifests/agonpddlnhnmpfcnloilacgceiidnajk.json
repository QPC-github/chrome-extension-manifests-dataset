{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remind Me Extension",
  "description": "Schedule your meeting and todo's and set custom background in your browser new tab.",
  "homepage_url": "https://www.remindmeextension.com/",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "https://www.ishtwo.com/meeting_reminder/",
    "notifications"
  ],
  "version": "1.7",
  "browser_action": {
    "default_icon": {
      "16": "public/images/bd-16.png",
      "32": "public/images/bd-32.png",
      "48": "public/images/bd-48.png",
      "128": "public/images/bd-128.png"
    }
  },
  "icons": {
    "16": "public/images/bd-16.png",
    "32": "public/images/bd-32.png",
    "48": "public/images/bd-48.png",
    "128": "public/images/bd-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "landing.html"
  }
}
