{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "quebrador de links",
  "version": "1.3",
  "description": "O LinkCrack permite que você consiga desproteger protetores de links e outros sites do gênero, apenas com um clique.",
  "manifest_version": 2,
  "icons": {
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Quebre, Desproteja, Descriptografe link rapidamente.",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
