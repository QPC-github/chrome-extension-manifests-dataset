{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inbox Guru",
  "version": "2.0.3",
  "description": "Sell Smarter With Email Engagement Tracking",
  "permissions": [
    "tabs"
  ],
  "short_name": "InboxGuru, Sell Smarter",
  "homepage_url": "http://www.inbox.guru/",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "images/icon/InboxguruIcon128.png",
    "16": "images/icon/InboxguruIcon16.png",
    "48": "images/icon/InboxguruIcon48.png",
    "64": "images/icon/InboxguruIcon64.png"
  },
  "web_accessible_resources": [
    "js/jquery.min.js",
    "js/inboxguru.js",
    "js/main.js",
    "js/bootstrap.min.js",
    "js/moment.min.js",
    "js/jquery.datetimepicker.full.js",
    "css/inject.css",
    "css/jquery.datetimepicker.css",
    "css/style.css",
    "css/custom.css",
    "css/dropdown.css",
    "images/caret.png",
    "images/logo.png",
    "images/Ribbon/inboxguru_outlook_icon_TrackLinks.png",
    "images/Ribbon/inboxguru_outlook_icon_InsertTemplate.png",
    "images/Ribbon/inboxguru_outlook_icon_TrackPDF.png",
    "images/Ribbon/inboxguru_outlook_icon_TrackVideo.png",
    "images/loading.gif",
    "html/sidebar_loader.html"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
