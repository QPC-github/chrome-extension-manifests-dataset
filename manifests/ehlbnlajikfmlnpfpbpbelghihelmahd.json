{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Emibyke extension",
  "version": "2.0",
  "description": "Accedi a tutti i servizi in una finestra separata all'interno del browser.",
  "icons": {
    "48": "img/emibyke-tappetino-mouse.png",
    "16": "img/emibyke-tappetino-mouse.png",
    "128": "img/emibyke-tappetino-mouse.png"
  },
  "browser_action": {
    "default_icon": "img/emibyke-tappetino-mouse.png",
    "default_title": "Apri l'estensione emibyke",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
