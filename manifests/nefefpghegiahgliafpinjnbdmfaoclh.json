{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FortunaSaves",
  "version": "1.1",
  "description": "Extra save slots!",
  "permissions": [
    "declarativeContent",
    "storage",
    "cookies",
    "http://cosmosdex.com/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "javascript/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2
}
