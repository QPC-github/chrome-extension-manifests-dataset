{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Scratchpad",
  "version": "0.2.4",
  "description": "Clip and collect your favorite UI elements from around the web and build mockups from them.",
  "icons": {
    "16": "assets/icons/icon-16.png",
    "48": "assets/icons/icon-48.png",
    "128": "assets/icons/icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icons/icon-19.png",
      "38": "assets/icons/icon-38.png"
    },
    "default_title": "Clip for Scratchpad",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "manifest_version": 2
}
