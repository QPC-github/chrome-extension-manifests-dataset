{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DealSquib",
  "description": "Price Comparison & Coupon Extension.",
  "version": "6.11.2",
  "manifest_version": 2,
  "icons": {
    "48": "img/DealSquib_48x48.png",
    "128": "img/DealSquib_128x128.png"
  },
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage",
    "chrome://favicon/*",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "tabs",
    "history",
    "notifications",
    "background",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "DealSquib.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "SearchPage.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://ajax.googleapis.com; object-src 'self'"
}
