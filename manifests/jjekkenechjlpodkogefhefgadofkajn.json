{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HeaderCheck",
  "description": "A basic tool to check server response headers for Security Compliance - Powered by Deucem ltd.",
  "version": "1.0",
  "author": "Justin Middler - Deucem",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png",
    "default_title": "Header Check - Powered by Deucem"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
