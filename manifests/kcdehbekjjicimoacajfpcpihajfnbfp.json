{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CoffeeConsole 2",
  "version": "2.0.2",
  "description": "A CoffeeScript console within the Chrome Dev Tools. Updated/Fixed Version of Coffee Console.",
  "devtools_page": "coffee.html",
  "icons": {
    "128": "128.png"
  },
  "minimum_chrome_version": "18",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
