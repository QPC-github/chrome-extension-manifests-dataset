{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Site Plus - Geo.Saitebi.Ge",
  "short_name": "SP - GSG",
  "description": "Site Plus is changing website design and added some plugins... This one is for geo.saitebi.ge",
  "version": "0.1",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "geo.saitebi.ge.css"
      ],
      "matches": [
        "*://geo.saitebi.ge/*"
      ]
    }
  ],
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "*://geo.saitebi.ge/*"
  ],
  "web_accessible_resources": [
    "over.mp3",
    "press.mp3"
  ],
  "manifest_version": 2
}
