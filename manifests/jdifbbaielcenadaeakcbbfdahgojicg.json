{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FBTrek",
  "description": "Converts Facebook Halloween themed reaction buttons into a Star Trek Theme",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "https://www.facebook.com/v3/*",
    "webRequestBlocking",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "16.png",
    "128": "128.png"
  }
}
