{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Waseef",
  "version": "0.1",
  "description": "Waseef Extension",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "waseef.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js",
        "jquery.min.js"
      ]
    }
  ],
  "permissions": [
    "notifications"
  ]
}
