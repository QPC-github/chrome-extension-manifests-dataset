{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "India Trending",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get google trends result instantly. See what people are searching most currently.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*.google.co.in/*"
  ]
}
