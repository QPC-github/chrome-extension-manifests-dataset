{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "html/index.html"
  },
  "page_action": {
    "default_icon": {
      "19": "img/icon_19.png",
      "38": "img/icon_38.png"
    },
    "default_title": "添加到首页"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "default_locale": "zh_CN",
  "description": "一键添加喜欢的网站到首页，目前chrome应用里面最简洁，方便的应用。打开下载、应用、书签页面方便快捷。",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png"
  },
  "manifest_version": 2,
  "name": "小豆-自定义首页",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "version": "0.1.2"
}
