{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Yoann Frommelt <yoann@frommelt.fr>",
  "name": "Symfony Profiler",
  "version": "1.0.3",
  "description": "Symfony Profiler in Chrome devtools",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
