{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/browserButtons/default/not_logged_in.png",
      "38": "images/browserButtons/default/not_logged_in_retina.png"
    },
    "default_title": "Gmail Toolbox"
  },
  "default_locale": "en",
  "homepage_url": "http://wiseelement.us/GmailToolbox/en/homepage.html",
  "icons": {
    "128": "images/icons/icon_128.png",
    "16": "images/icons/icon_16.png",
    "32": "images/icons/icon_32.png",
    "48": "images/icons/icon_48.png",
    "64": "images/icons/icon_64.png"
  },
  "description": "__MSG_description__",
  "manifest_version": 2,
  "name": "Gmail Toolbox",
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "tts",
    "idle",
    "notifications",
    "storage",
    "tabs",
    "contextMenus",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "*://*.google.com/*"
  ],
  "version": "1.1.4"
}
