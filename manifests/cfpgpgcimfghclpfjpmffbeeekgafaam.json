{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ToReadLater",
  "description": "To Read Later - tool for serving temporary pages for reading later",
  "version": "1.1",
  "browser_action": {
    "default_icon": "images/icon6.png",
    "default_popup": "popup.html",
    "default_title": "ToReadLater"
  },
  "icons": {
    "16": "images/icon6.png",
    "48": "images/icon6.png",
    "128": "images/icon6.png"
  },
  "background": {
    "scripts": [
      "libs/lodash.js",
      "scripts/actions.js",
      "scripts/bs_main.js"
    ]
  },
  "permissions": [
    "activeTab",
    "notifications",
    "storage",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
