{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nota media en la UPM",
  "version": "1.3.1",
  "description": "Calcula tu nota media y cr√©ditos en un solo click! (No se consideran las asignaturas suspensas) Creado por Luis Cavanillas.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
