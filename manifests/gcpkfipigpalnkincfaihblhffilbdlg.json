{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "couchsurfing.user.js"
      ],
      "matches": [
        "http://www.couchsurfing.org/*",
        "https://www.couchsurfing.org/*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "description": "This plugin is built for the New Member Welcome Team on Couchsurfing as they require extra links",
  "name": "CS NMW Team plugin",
  "version": "0.1.0"
}
