{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK song duplicates remover",
  "description": "Removes duplicates of songs in vk.com audio search.",
  "version": "1.1",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}
