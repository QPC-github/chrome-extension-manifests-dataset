{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One Task",
  "description": "Simple Task Management.",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
