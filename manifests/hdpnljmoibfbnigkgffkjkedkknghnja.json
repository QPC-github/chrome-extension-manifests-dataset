{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Splash",
  "version": "1.0",
  "description": "Automatically close annoying splash screens",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "nosplash.user.js"
      ]
    }
  ]
}
