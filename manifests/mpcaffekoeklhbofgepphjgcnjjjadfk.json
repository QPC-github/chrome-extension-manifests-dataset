{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QR URL",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "QR URL",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    }
  },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "author": "Abdelilah Sawab",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
