{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Session Desktop Streamer for SwITNet Ltd",
  "description": "Extension to stream desktop share for switnet.net",
  "version": "0.1.6.1",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "switnet-16x16.png",
    "48": "switnet-48x48.png",
    "128": "switnet-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://session.switnet.net/*"
    ]
  }
}
