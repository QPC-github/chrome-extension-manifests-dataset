{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Easy mp3",
  "version": "2.2",
  "manifest_version": 2,
  "description": "The Easy mp3 Add-on includes a Button on any Page and let you to convert any Video to a mp3 with one click.",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "author": "Theveloper",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "easy-mp3.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
