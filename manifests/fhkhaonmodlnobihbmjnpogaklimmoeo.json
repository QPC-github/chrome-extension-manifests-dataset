{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Desktop Sharing Extension",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "DSE",
  "version": "4.2",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow desktop sharing for WebRTC applications.",
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.csslcloud.net/*",
      "*://*.seentao.com/*",
      "*://*.pixseed.com/*",
      "*://*.kangfuyun.com/*",
      "*://*.csslcloud-as.com/*",
      "*://*.yishengzhan.cn/*",
      "*://*.lks.com/*",
      "*://*.ttlhome.com/*",
      "*://*.likeshuo.com/*",
      "*://*.gaokaobigdata.cn/*"
    ]
  },
  "web_accessible_resources": [
    "woogeen_icon_128.png"
  ],
  "icons": {
    "128": "woogeen_icon_128.png",
    "16": "woogeen_icon_16.png",
    "48": "woogeen_icon_48.png"
  },
  "browser_action": {
    "default_icon": "woogeen_icon_16.png"
  }
}
