{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Realtime JSON-Friendly Web Storage Manager",
  "description": "LocalStorage / SessionStorage / Cookie 3 in 1 Editor",
  "version": "2.1.3",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/browser/icon16.png",
      "24": "assets/browser/icon24.png",
      "32": "assets/browser/icon32.png"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "popup.html"
  ]
}
