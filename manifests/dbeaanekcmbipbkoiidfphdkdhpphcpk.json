{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Tooltip Textareas",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Hide annoying tooltips",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "hide_titles.js"
      ]
    }
  ]
}
