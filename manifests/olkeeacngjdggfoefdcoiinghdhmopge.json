{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Bookmarks Tabbed Home Page",
  "description": "Display all bookmarks on your home page.",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "bookmarks.html"
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "bookmarks"
  ]
}
