{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Baconizator",
  "version": "0.9",
  "homepage_url": "http://www.farbwechselrecords.com",
  "description": "Replaces all images on a given page with pictures of healthy bacons. OMNOMNOMNOMMM!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Baconizator"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js",
      "baconizator.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
