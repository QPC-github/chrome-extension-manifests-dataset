{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Draper to Herb",
  "short_name": "DraperHerb",
  "description": "Replaces the text 'Kevin Draper' with 'Kevin Herb'.",
  "author": "Brian Hubble",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
