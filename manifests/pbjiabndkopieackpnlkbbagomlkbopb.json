{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hover Read",
  "short_name": "Hover Read",
  "version": "2.2.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Hoverzoom for articles.",
  "permissions": [
    "*://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery.hoverIntent.minified.js",
        "parseuri.js",
        "hoverread.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "manifest_version": 2
}
