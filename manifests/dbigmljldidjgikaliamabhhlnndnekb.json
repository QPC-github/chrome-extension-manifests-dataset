{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Young Tomy Live",
  "version": "1.0.0",
  "description": "Extension navigateur pour Young Tomy",
  "manifest_version": 2,
  "permissions": [
    "notifications",
    "tabs",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "young-tomy@kocal.fr"
    }
  },
  "icons": {
    "48": "icons/icon_off_48.png",
    "128": "icons/icon_off_128.png"
  },
  "browser_action": {
    "default_title": "Young Tomy",
    "default_icon": "icons/icon_off_48.png"
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background.js"
    ]
  }
}
