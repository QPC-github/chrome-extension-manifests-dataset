{
  "browser_action": {
    "default_icon": "ico/19x19.png",
    "default_popup": "popup.html",
    "default_title": "Sukiyaki Converter"
  },
  "content_scripts": [
    {
      "js": [
        "jq/jquery-1.11.0.min.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
