  "icons": {
    "128": "ico/128x128.png",
    "48": "ico/48x48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Sukiyaki Converter",
  "permissions": [
    "*://*/*",
    "activeTab",
    "storage"
  ],
  "short_name": "SukiyakiConv",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.5"
}
