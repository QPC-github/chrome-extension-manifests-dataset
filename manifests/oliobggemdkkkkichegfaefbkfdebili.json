{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HikeScreenshot",
  "short_name": "HikeScreenshot.",
  "description": "Hike Screenshot To Capture Visible Browser Screen.",
  "version": "1.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/camera.png",
    "default_title": "Hike Screen Capture !"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "img/icon16-999.png",
    "48": "img/icon48-999.png",
    "128": "img/icon128-999.png"
  }
}
