{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BigQuery Mate",
  "version": "4.0",
  "description": "Companion tool for Google BigQuery UI used by myself and my peers at Viant Inc. Adds some extra functionality to BQ Native UI",
  "icons": {
    "19": "BQMate19T.png",
    "38": "BQMate38T.png",
    "96": "BQMate96T.png",
    "128": "BQMate128T.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://bigquery.cloud.google.com/*",
    "*://console.cloud.google.com/bigquery*",
    "*://cloud.google.com/bigquery*",
    "*://us-central1-mb-sandbox-01.cloudfunctions.net/*",
    "tabs",
    "activeTab",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "BQMate128T.png",
    "full-screen.png",
    "viant_logo.png",
    "potens.png",
    "chart.html",
    "chart.js",
    "pivot.css",
    "pivot.js",
    "c3_renderers.js",
    "export_renderers.js"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com https://*.google.com ; object-src 'self'",
  "browser_action": {
    "default_title": "BQ Mate Navigator",
    "38": "BQMate38T.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://bigquery.cloud.google.com/*",
        "*://console.cloud.google.com/bigquery*"
      ],
      "css": [
        "jquery-ui.min.css"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "jquery-ui.min.js"
      ]
    }
  ]
}
