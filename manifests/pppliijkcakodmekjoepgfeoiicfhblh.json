{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Flirchi"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Уведомления о новых сообщениях и событиях на Flirchi.",
  "manifest_version": 2,
  "name": "Flirchi",
  "permissions": [
    "http://flirchi.ru/",
    "tabs"
  ],
  "version": "0.1.1",
  "icons": {
    "128": "icon.128.png",
    "32": "icon.32.png"
  }
}
