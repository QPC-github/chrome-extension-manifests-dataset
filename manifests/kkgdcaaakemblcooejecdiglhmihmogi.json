{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CC to Nietzsche",
  "short_name": "CC to Nietzsche",
  "description": "Pornhub introduced CC. They're not as funny as we thought. So we made them at least smart. Thus Spoke Zarathustra > [Moaning].",
  "author": "ikko post-digital agency <lars@ikko.agency>",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.pornhub.com/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "zarathustra.json"
  ]
}
