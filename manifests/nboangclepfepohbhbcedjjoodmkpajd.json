{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GP.se Comments",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Make comments on articles at gp.se",
  "content_scripts": [
    {
      "matches": [
        "http://*.gp.se/*"
      ],
      "js": [
        "jquery-1.11.0.min.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
