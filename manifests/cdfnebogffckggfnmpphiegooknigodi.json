{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Automatically sorts results by number of seeders when searching on The Pirate Bay.",
  "name": "The Pirate Bay - Auto-sort by Seeders",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://thepiratebay.se/search/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.0"
}
