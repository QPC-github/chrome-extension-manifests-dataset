{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jotter",
  "description": "Jot down multiple notes with a simple notepad.",
  "version": "1.1.2.7",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
