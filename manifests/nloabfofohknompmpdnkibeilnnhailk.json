{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Dominion Full Card Display",
  "version": "1.1",
  "description": "Display the full card text on dominion.isotropic.org. No more mouse-over and waiting for the pop-up!",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://dominion.isotropic.org/play"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "http://dominion.isotropic.org/play"
  ]
}
