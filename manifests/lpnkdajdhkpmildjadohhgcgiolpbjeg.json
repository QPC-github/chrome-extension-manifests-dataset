{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "You Consumed",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "How many lives has a Youtube video consumed?",
  "homepage_url": "http://maxnanis.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "https://gdata.youtube.com/*",
    "http://youtube.com/*",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*",
    "https://youtube.com/*"
  ],
  "content_security_policy": "script-src 'self' https://gdata.youtube.com/; object-src 'self'"
}
