{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cookiez",
  "version": "1.0",
  "description": "Cookies Counter",
  "manifest_version": 2,
  "author": "phyte.studio",
  "permissions": [
    "storage",
    "cookies",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "onboard.html"
  },
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
