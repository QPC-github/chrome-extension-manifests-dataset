{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HTTP Trace",
  "description": "Tracks all HTTP requests made from the current tab.",
  "version": "0.5",
  "permissions": [
    "activeTab",
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/http_trace_button.png",
    "default_title": "HTTP Trace"
  }
}
