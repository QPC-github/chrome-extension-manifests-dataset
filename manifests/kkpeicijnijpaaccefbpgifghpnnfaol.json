{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fix Google Maps v2",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Muestra correctamente los mapas de la versi√≥n 2 de Google maps, aunque ya no tenga soporte",
  "author": "Manuel Ortiz",
  "background": {
    "persistent": true,
    "scripts": [
      "fixmapscript.js"
    ]
  },
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "browser_action": {
    "default_icon": "logo_16.png",
    "default_title": "Fix Google Maps v2"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.gstatic.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "fixmapscript.js"
      ]
    }
  ]
}
