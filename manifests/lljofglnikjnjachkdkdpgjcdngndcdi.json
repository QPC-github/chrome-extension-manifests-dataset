{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Java Class to JSON",
  "description": "Converte uma classe java em JSON",
  "version": "1.4",
  "browser_action": {
    "default_icon": "json.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://git.ciss.com.br/*",
    "http://*/*",
    "https://*/*",
    "https://ajax.googleapis.com/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "icons": {
    "16": "json.png",
    "48": "json.png",
    "128": "json.png"
  },
  "web_accessible_resources": []
}
