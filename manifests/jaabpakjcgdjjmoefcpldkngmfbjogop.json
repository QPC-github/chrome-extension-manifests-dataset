{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EF Assist",
  "version": "0.2.2",
  "description": "Make life easier for studying english with EF website.",
  "short_name": "ef-assist",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.ef.com.cn/*",
        "http://*.ef.com.cn/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "https://*.ef.com.cn/*"
  ],
  "page_action": {
    "default_name": "EF Assist",
    "default_icon": "img/icon48.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com https://ssl.google-analytics.com; object-src 'self'"
}
