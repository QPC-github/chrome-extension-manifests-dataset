{
  "browser_action": {
    "default_icon": "images/icon/img32.png",
    "default_popup": "index.html"
  },
  "description": "Select a color from an image or web page using the integrated zoom feature!",
  "icons": {
    "128": "images/icon/img128.png",
    "16": "images/icon/img16.png",
    "24": "images/icon/img24.png",
    "32": "images/icon/img32.png",
    "64": "images/icon/img64.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "name": "Simple Color Picker",
  "permissions": [
    "geolocation",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
