{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Merch By Amazon Analytics",
  "version": "1.1.8",
  "manifest_version": 2,
  "description": "Analyze, auto-fill, search t-shirts .. Merch by Amazon",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "*://merch.amazon.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "Merch By Amazon Analytics",
    "default_popup": "page/popup.html"
  },
  "options_page": "page/options.html",
  "background": {
    "scripts": [
      "js/date.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://merch.amazon.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/date.js",
        "js/analytics.js"
      ]
    }
  ]
}
