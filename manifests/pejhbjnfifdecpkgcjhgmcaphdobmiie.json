{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Toast",
  "description": "Browser Tab Organizer",
  "version": "2.5",
  "browser_action": {
    "default_title": "Toast.\nBrowser Tab Organizer."
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/functions.js",
      "js/api.js",
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "*://*.dotoast.com/*",
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "chrome://favicon/"
  ],
  "commands": {
    "Save Session...": {
      "suggested_key": {
        "mac": "MacCtrl+Shift+S",
        "default": "Ctrl+Shift+S"
      },
      "description": "Save Session..."
    },
    "Save and Share Session...": {
      "suggested_key": {
        "mac": "MacCtrl+Shift+E",
        "default": "Ctrl+Shift+E"
      },
      "description": "Save and Share Session..."
    },
    "Add Current Page to Session...": {
      "suggested_key": {
        "mac": "MacCtrl+Shift+U",
        "default": "Ctrl+Shift+U"
      },
      "description": "Add Current Page to Session..."
    },
    "Open Web Session Manager": {
      "suggested_key": {
        "mac": "MacCtrl+Shift+M",
        "default": "Ctrl+Shift+M"
      },
      "description": "Open Web Session Manager"
    }
  }
}
