{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Gaminizer",
  "description": "Load Youtube Gaming Layouts",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Gaminize!",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://www.youtube.com/*",
    "webRequestBlocking",
    "activeTab",
    "tabs"
  ]
}
