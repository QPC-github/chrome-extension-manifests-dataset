{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Notifier",
  "version": "1.0.3",
  "description": "Notifies you of the new videos being uploaded. (Glam and Gore only for now)",
  "permissions": [
    "notifications",
    "gcm",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
