{
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/functions.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icons/19x19.png",
      "38": "img/icons/38x38.png"
    },
    "default_popup": "browser-action.html",
    "default_title": "Oneindia CouponNdeal Finder"
  },
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "img/icons/128x128.png",
    "16": "img/icons/16x16.png",
    "48": "img/icons/48x48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "27",
  "name": "__MSG_appName__",
  "short_name": "COM",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
