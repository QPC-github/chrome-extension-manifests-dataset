  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2",
  "web_accessible_resources": [
    "img/content/logo-small.png",
    "img/content/close-btn.png",
    "img/content/roll-up-btn.png",
    "img/content/back-to-results-btn.png",
    "img/content/ribbon.png",
    "img/content/ribbon-small.png",
    "img/content/ribbon-minimize-btn.png",
    "fonts/Roboto-Regular.ttf",
    "fonts/Roboto-Bold.ttf"
  ]
}
