{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mixbowl Website Demo",
  "version": "1.0",
  "description": "Embed Mixbowl Demo to website",
  "manifest_version": 2,
  "author": "Jingyi Ding",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "widget.js"
      ]
    }
  ],
  "permissions": [
    "declarativeContent",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "image/android-chrome-192x192.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "image/android-chrome-192x192.png"
  }
}
