{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.2",
  "manifest_version": 2,
  "short_name": "javascript-switch",
  "name": "JavaScript Switch ON|OFF",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "http://mybrowseraddon.com/javascript-switch.html",
  "description": "Switch JavaScript engine ON or OFF with just one click!",
  "options_ui": {
    "chrome_style": true,
    "page": "data/options/options.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "contentSettings",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "JavaScript Switch ON|OFF",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
