{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notes for VoteBuilder Chrome Extension",
  "version": "2.2",
  "description": "Adding NOTES to a space that is more relevant for use in VoteBuilder",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.3.3.1.min.js",
        "myscript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery.3.3.1.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "voteBuilder.png"
  }
}
