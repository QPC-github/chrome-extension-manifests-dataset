{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyBrains",
  "description": "MyBrains Application",
  "version": "0.0.3.3",
  "author": "MyBrains",
  "short_name": "MyBrains App",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/mybrains.js"
      ]
    }
  ],
  "browser_action": {
    "name": "Click to create new bookmark",
    "default_title": "MyBrains App",
    "default_icon": {
      "16": "images/icon16.png",
      "19": "images/icon19.png",
      "32": "images/icon32.png",
      "38": "images/icon38.png",
      "64": "images/icon64.png",
      "96": "images/icon96.png",
      "128": "images/icon128.png",
      "164": "images/icon164.png",
      "196": "images/icon196.png",
      "224": "images/icon224.png",
      "256": "images/icon256.png"
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "images/icon16.png",
    "19": "images/icon19.png",
    "32": "images/icon32.png",
    "38": "images/icon38.png",
    "64": "images/icon64.png",
    "96": "images/icon96.png",
    "128": "images/icon128.png",
    "164": "images/icon164.png",
    "196": "images/icon196.png",
    "224": "images/icon224.png",
    "256": "images/icon256.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
