{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MILK â€” Cookie Manager",
  "version": "1.0.3",
  "description": "Cookie's best friend! Modern cookie manager.",
  "permissions": [
    "cookies",
    "tabs",
    "<all_urls>",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "MILK",
    "default_icon": "icon128.png",
    "default_popup": "index.html"
