{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KeyTab",
  "description": "Top sites at your fingertips!",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Top sites at your fingertips!"
  },
  "version": "1.5",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Web",
      "keyword": "Web",
      "search_url": "http://navbest.com/exten/search1.php?q={searchTerms}",
      "favicon_url": "http://navbest.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
