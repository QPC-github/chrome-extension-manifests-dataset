{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CountDown",
  "manifest_version": 2,
  "version": "1",
  "content_scripts": [
    {
      "matches": [
        "http://store.nike.com/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "countdown.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://store.nike.com/*"
  ]
}
