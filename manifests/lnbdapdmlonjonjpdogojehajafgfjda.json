{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DicData",
  "description": "This extension allows browser actions with German, French, Italian and Spainish dictionaries.",
  "version": "2.0.0.7",
  "app": {
    "urls": [
      "http://webapp.dicdata.de/",
      "http://www.webapp.dicdata.de/"
    ],
    "launch": {
      "web_url": "http://webapp.dicdata.de/"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
