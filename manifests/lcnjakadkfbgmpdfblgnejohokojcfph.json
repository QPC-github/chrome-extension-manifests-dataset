{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "366 Degrees",
  "short_name": "366 Degrees",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Engage leads and prospects with simple outreach solutions by 366 Degrees.",
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "browser_action": {
    "default_popup": "src/browser_action/browser_action.html",
    "default_icon": "icons/logo.png",
    "default_title": "366 Degrees"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "downloads"
  ],
  "web_accessible_resources": [
    "src/browser_action/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.min.js",
      "src/bg/background.js"
    ]
  }
}
