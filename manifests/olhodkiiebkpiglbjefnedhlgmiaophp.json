{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TheHub Click To Dial",
  "author": "Code-Desk",
  "version": "0.1.3",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "description": "Click to dial using Yealink Desk Phones!",
  "homepage_url": "http://wavetelbusiness.co.uk/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "resources/icon_032.png",
    "32": "resources/icon_032.png",
    "48": "resources/icon_048.png",
    "128": "resources/icon_128.png"
  },
  "browser_action": {
    "default_icon": "resources/icon_032.png",
    "default_title": "Settings for click to dail",
    "default_popup": "popup.html"
  },
  "externally_connectable": {
    "matches": [
      "https://localhost/*",
      "https://hub.wave-tel.com/*",
      "https://staging.thehubwork.com/*",
      "https://codedesk.thehubwork.com/*"
    ]
  },
  "permissions": [
    "contextMenus",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
