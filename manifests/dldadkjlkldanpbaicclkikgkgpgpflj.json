{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "in5 Animated GIF Maker",
  "version": "2.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "tabCapture",
    "storage"
  ],
  "background": {
    "scripts": [
      "lib/gif.js",
      "lib/gif.worker.js",
      "lib/gifsicle.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/logo16.png",
    "32": "img/logo32.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "web_accessible_resources": [
    "lib/gifsicle.wasm"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
