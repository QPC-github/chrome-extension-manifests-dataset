{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "init.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Debrideur Purevid pour Chrome",
  "manifest_version": 2,
  "name": "PureDebrid",
  "permissions": [
    "<all_urls>"
  ],
  "version": "1.2.3"
}
