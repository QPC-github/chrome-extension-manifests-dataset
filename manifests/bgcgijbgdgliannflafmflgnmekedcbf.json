{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HelloHulu",
  "version": "1.3.1",
  "description": "Gives you quick access to Hulu videos.",
  "icons": {
    "16": "popupicon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "popupicon.png",
    "default_title": "HelloHulu",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
