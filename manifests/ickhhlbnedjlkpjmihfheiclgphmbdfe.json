{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "TeeVid screen sharing",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Screen sharing for TeeVid",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.24",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow screen sharing in TeeVid.",
  "externally_connectable": {
    "matches": [
      "http://localhost:8080/",
      "http://121.28.209.22:24080/*",
      "https://localhost:3004/",
      "https://121.28.209.22:24080/*",
      "https://*.teevid.com:24080/*",
      "https://*.teevid.com/*",
      "https://*.teevid.cn/*",
      "https://*.teevid.com.cn/*",
      "https://*.mylandfar.net/*",
      "https://*.landfar.net/*",
      "https://*.crmeeting.cn/*",
      "https://*.inspur.tk/*",
      "https://*.inspur.tk:24443/*",
      "https://*.litocloud.com/*",
      "https://*.baijiayun.com/*",
      "https://*.zznode.com/*",
      "https://*.potevking.com/*",
      "https://*.innonation.io/*",
      "https://*.ai.innonation.io/*",
      "https://*.sctel.com.cn/*",
      "https://*.speedycloud.cn/*",
      "https://*.video-demo.speedycloud.cn/*",
      "https://*.zte.com.cn/*"
    ]
  },
  "icons": {
    "128": "teevid_icon_128.png",
    "16": "teevid_icon_16.png",
    "48": "teevid_icon_48.png"
  },
  "browser_action": {
    "default_icon": "teevid_icon_16.png"
  }
}
