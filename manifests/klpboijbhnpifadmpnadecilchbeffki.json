{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Strikethrough Trump",
  "version": "1.1",
  "description": "Adds Strikethrough to the words Trump, President Trump, President Donald J. Trump",
  "icons": {
    "16": "trumpicon16.png",
    "48": "trumpicon48.png",
    "128": "trumpicon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
