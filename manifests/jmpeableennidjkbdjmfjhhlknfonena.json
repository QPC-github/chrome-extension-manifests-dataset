{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Idgon beta extension",
  "description": "Stop giving away your email address!!!  Idgon gives you control of your personal information",
  "version": "0.1.4",
  "author": "me",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "https://beta.idgon.com/"
  ],
  "browser_action": {
    "default_title": "Idgon",
    "default_icon": "idgon_logo-128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "idgon_logo.png",
    "128": "idgon_logo-128.png",
    "16": "idgon_logo-16.png"
  },
  "popup": {
    "scripts": [
      "browser-polyfill.js"
    ]
  }
}
