{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "America's Choice Assessments Automatic Tester",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Automatically fills out student tests given their letter answers, and automatically resets a set of tests for incoming students.",
  "page_action": {},
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://assessments.americaschoice.org/",
        "http://assessments.americaschoice.org/index.cfm",
        "http://assessments.americaschoice.org/login.cfm",
        "http://assessments.americaschoice.org/logoff.cfm"
      ],
      "js": [
        "jquery.js",
        "login.js"
      ],
      "css": [],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "http://assessments.americaschoice.org/students/index.cfm"
      ],
      "js": [
        "jquery.js",
        "students-index.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "http://assessments.americaschoice.org/students/test.cfm*",
        "http://assessments.americaschoice.org/students/Test.cfm*",
        "http://assessments.americaschoice.org/students/test_doit.cfm*"
      ],
      "js": [
        "jquery.js",
        "test.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "http://assessments.americaschoice.org/students/response.cfm*"
      ],
      "js": [
        "jquery.js",
        "responses.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "http://assessments.americaschoice.org/students/response_doit.cfm*"
      ],
      "js": [
        "jquery.js",
        "response_doit.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "http://assessments.americaschoice.org/admin/classView.cfm*"
      ],
      "js": [
        "jquery.js",
        "classView.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "http://assessments.americaschoice.org/admin/studentView.cfm*"
      ],
      "js": [
        "jquery.js",
        "studentView.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "permissions": []
}
