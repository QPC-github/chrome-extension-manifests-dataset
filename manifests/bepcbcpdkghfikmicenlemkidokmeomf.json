{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "main.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "曲速穿梭",
    "defualt_icon": {
      "19": "images/warpdrive/logos/logo_green.png",
      "38": "images/warpdrive/logos/logo_green.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "安装简单快速、服务器众多、价格低廉，体验加速的快感。不管是外贸、学术，还是交友、娱乐，曲速穿梭都能满足您的需求！",
  "icons": {
    "128": "images/warpdrive/logos/logo_blue.png",
    "16": "images/warpdrive/logos/logo_blue.png",
    "48": "images/warpdrive/logos/logo_blue.png"
  },
  "manifest_version": 2,
  "name": "曲速穿梭",
  "short_name": "WarpDrive",
  "permissions": [
    "management",
    "proxy",
    "idle",
    "tabs",
    "cookies",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "version": "1.0.3"
}
