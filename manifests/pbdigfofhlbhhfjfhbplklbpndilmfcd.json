{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GithubLight",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://gist.github.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
