{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_extensionName__",
  "version": "2.1",
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "img/seo-helper-16.png",
    "24": "img/seo-helper-24.png",
    "32": "img/seo-helper-32.png",
    "48": "img/seo-helper-48.png",
    "64": "img/seo-helper-64.png",
    "128": "img/seo-helper-128.png",
    "256": "img/seo-helper-256.png"
  },
  "browser_action": {
