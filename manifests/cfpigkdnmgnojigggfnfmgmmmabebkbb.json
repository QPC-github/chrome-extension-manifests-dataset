{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "News wizard",
  "short_name": "Newswiz",
  "description": "This allows you to browse a series of sites in succession. The bookmarks are from subfolders of a folder named 'Newswiz'.",
  "version": "0.2.4",
  "permissions": [
    "activeTab",
    "storage",
    "bookmarks"
  ],
  "icons": {
    "128": "news_128.png"
  },
  "browser_action": {
    "default_icon": "news_256.png",
    "default_title": "News wizard",
    "default_popup": "popup.html"
  }
}
