{
  "background": {
    "persistent": false,
    "scripts": [
      "AutEM.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "AutEM Exportador V2.0"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Exporte assistencias direto para o sistema AutEM Integrated.",
  "externally_connectable": {
    "matches": [
      "https://*.google.com/*"
    ]
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "AutEM Exportador V2.0",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "<all_urls>",
    "activeTab",
    "contextMenus"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
