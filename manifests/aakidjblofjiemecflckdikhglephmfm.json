{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open Tistory as cache",
  "description": "Open Tistory blog as a web cache directly.",
  "version": "1.4",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "redirect-to-cache.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "http://webcache.googleusercontent.com/*"
  ]
}
