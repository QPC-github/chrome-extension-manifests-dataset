{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Virtual Sheet Music",
  "description": "Search for Sheet Music plus Audio Files to download instantly!",
  "version": "1.0.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "VIOLIN"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "128": "images/icon.png"
  }
}
