{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Stop Slacking",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Get motivated to do something worth remembering",
  "background": {
    "persistent": false,
    "scripts": [
      "engine.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "notifications",
    "tabs"
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "48.png"
  ]
}
