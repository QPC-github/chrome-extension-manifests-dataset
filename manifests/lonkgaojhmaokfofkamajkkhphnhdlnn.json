{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hangout2Tel",
  "description": "Extension for contacts.google.com only. It replaces hyperlinks on phone numbers to tel: links instead of hangouts",
  "icons": {
    "16": "tel16.png",
    "48": "tel48.png",
    "128": "tel128.png"
  },
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://contacts.google.com/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ]
}
