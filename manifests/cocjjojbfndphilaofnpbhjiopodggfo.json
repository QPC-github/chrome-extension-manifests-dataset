{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zoom+",
  "version": "0.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Zooms up 10%.",
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "permissions": [
    "tabs"
  ]
}
