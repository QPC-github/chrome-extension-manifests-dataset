{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dartsnieuws plugin",
  "description": "Het laatste dartsnieuws altijd dichtbij!",
  "version": "1.0.3",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "notifications",
    "alarms",
    "background",
    "notifications"
  ],
  "icons": {
    "48": "images/dartsnieuws48.png",
    "128": "images/dartsnieuws128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/dartsnieuws19.png",
      "38": "images/dartsnieuws38.png"
    },
    "default_title": "Dartsnieuws.com"
  },
  "background": {
    "page": "bg.html"
  },
  "manifest_version": 2
}
