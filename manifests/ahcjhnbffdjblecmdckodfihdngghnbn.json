{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TrumpFilter",
  "description": "TrumpFilter for Facebook",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "facebook_filter.js"
      ]
    }
  ]
}
