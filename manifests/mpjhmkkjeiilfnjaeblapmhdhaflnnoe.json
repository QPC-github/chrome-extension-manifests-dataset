{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitterのフォントを変える",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "twitter.com の文字の見た目を変更します。(mobile.twitter.com では変更されません。)",
  "web_accessible_resources": [
    "twitter_font.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "twitter_font.css"
      ]
    }
  ],
  "icons": {
    "128": "negi_icon.png"
  }
}
