{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ImageZoom",
  "version": "1.7.5",
  "author": "moon.aka.sun",
  "description": "Zoom images inline on the current page by holding the middle mouse button down and moving mouse left-right.",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "imagezoom.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "zoom_in_16.png",
    "24": "zoom_in_24.png",
    "32": "zoom_in_32.png",
    "48": "zoom_in_48.png",
    "64": "zoom_in_64.png",
    "128": "zoom_in_C8.png"
  },
  "short_name": "ImageZoom"
}
