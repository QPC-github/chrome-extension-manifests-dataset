{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BlueKai Registry Helper",
  "version": "0.2",
  "manifest_version": 2,
  "description": "BlueKai Registry Helper",
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "BlueKai Registry Helper"
  },
  "background": {
    "page": "eventpage.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage",
    "webNavigation",
    "webRequest"
  ]
}
