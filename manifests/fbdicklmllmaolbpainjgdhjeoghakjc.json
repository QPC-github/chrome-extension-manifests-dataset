{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quero Saúde",
  "version": "1.0",
  "description": "QueroSaúde.net é um dos maiores portal de saúde do Brasil. Acesse facilmente o nosso site à partir de apenas 1 clique.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "activeTab",
    "http://www.querosaude.net/"
  ],
  "browser_action": {
    "default_popup": "typedUrls.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
