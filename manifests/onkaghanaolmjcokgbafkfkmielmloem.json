{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cracked Comments",
  "description": "This extension improves the cracked comment section",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.cracked.com/*"
      ],
      "css": [
        "css/comments.css"
      ],
      "js": [
        "js/jquery.js",
        "js/comments.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
