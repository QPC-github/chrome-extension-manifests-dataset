{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seclave Chrome Extension",
  "short_name": "seclave_ext",
  "description": "Chrome Extension that autofill passwords",
  "version": "0.9",
  "background": {
    "scripts": [
      "const.js",
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icons/seclave_16x16.png",
    "128": "icons/seclave_128x128.png"
  },
  "browser_action": {
    "default_title": "Add password to seclave",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
