{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "description": "Updates XSM Avails when clicked.",
  "icons": {
    "48": "icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "XSM Avails Updater",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
