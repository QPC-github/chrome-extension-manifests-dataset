{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Resource Finder",
  "version": "0.5.2",
  "manifest_version": 2,
  "description": "An extension for searching Bower for front-end packages for use with CodePen and other apps like it.",
  "homepage_url": "https://github.com/HipsterBrown/resource-finder",
  "icons": {
    "16": "icons/Icon16.png",
    "48": "icons/Icon48.png",
    "128": "icons/Icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/Icon19.png",
    "default_title": "Package Search",
    "default_popup": "src/browser_action/index.html"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
