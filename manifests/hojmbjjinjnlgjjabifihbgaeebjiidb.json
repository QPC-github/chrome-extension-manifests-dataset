{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cleaner mirror.enha.kr",
  "description": "This extension removes annoying ads in mirror.enha.kr",
  "version": "1.0",
  "permissions": [
    "https://*.enha.kr/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://mirror.enha.kr/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
