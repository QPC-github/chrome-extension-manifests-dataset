{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Their",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "description": "No more 'his or her'",
  "icons": {
    "16": "images/ttt16.png",
    "48": "images/ttt48.png",
    "128": "images/ttt128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
