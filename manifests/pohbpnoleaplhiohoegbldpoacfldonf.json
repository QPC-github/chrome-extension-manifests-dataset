{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "iconfinder_edit-clear_118917.png",
    "default_popup": "popup.html"
  },
  "description": "Control Your Privacy Settings! Delete browser history. Clear cookies. Clear cache. Remove hosted app data.",
  "manifest_version": 2,
  "name": "Vz Clear Cache",
  "permissions": [
    "browsingData",
    "https://www.google.com/*",
    "https://accounts.google.com/*",
    "https://calendar.google.com/*",
    "https://www.googleapis.com/*",
    "https://app.elastica.net/*",
    "alarms",
    "storage",
    "cookies",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "version": "1.0.0.3"
}
