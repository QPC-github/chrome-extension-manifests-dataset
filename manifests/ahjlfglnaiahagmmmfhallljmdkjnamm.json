{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Schnapper-Software",
  "name": "SchnapperPro",
  "version": "1.12",
  "manifest_version": 2,
  "description": "Chrome Addon fuer SchnapperPro",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "SchnapperPro",
    "default_icon": {
      "19": "icon19.png"
    }
  }
}
