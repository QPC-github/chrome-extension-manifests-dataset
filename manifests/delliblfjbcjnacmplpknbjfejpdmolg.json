{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MassisPost",
  "version": "1.0",
  "description": "Latest News From MassisPost ",
  "icons": {
    "48": "img/48x48.jpg",
    "128": "img/118x118.jpg",
    "16": "img/16x16.jpg"
  },
  "browser_action": {
    "default_icon": "img/118x118.png",
    "default_title": "Press to see latest news!",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
