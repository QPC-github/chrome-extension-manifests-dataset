{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hüseyin Demirtaş ile İngilizce Güzel Sözler",
  "description": "Hüseyin Demirtaş ile İngilizce Güzel Sözler eklentisi ile öğretici İngilizce özlü sözler & çevirilerini öğrenin",
  "version": "1.0.2",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "quotes.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'; connect-src 'self'"
}
