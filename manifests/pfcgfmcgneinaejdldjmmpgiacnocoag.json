{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VkClean",
  "version": "1.71",
  "description": "Сделай свой Вконтакте чистым от рекламы",
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.vk.com/",
    "contextMenus",
    "storage",
    "webNavigation",
    "*://*.vkclean.com/",
    "webRequest"
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/script.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "19": "images/19.png",
    "38": "images/38.png",
    "48": "images/48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png",
      "48": "images/48.png"
    },
    "default_popup": "popup.html"
  }
}
