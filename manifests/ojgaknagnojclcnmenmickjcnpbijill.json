{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AliExpress Helper",
  "short_name": "AliEHelper",
  "version": "0.0.4.23",
  "author": "Platon Devs",
  "description": "Free Helper for AliExpress. Export Orders as CSV. Download videos and user uploaded images. Export reviews.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "notifications",
    "https://*.aliexpress.com/"
  ],
  "background": {
    "scripts": [
      "background.js",
      "xx4.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "*.png"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/AliHelper_icon.png"
    }
  },
  "icons": {
    "128": "images/AliHelper_icon.png"
  },
  "manifest_version": 2
}
