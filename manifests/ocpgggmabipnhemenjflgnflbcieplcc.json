{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Upload Timer",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Upload to youtube at a certain time.",
  "icons": {
    "128": "upload128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://studio.youtube.com/*"
      ],
      "js": [
        "Youtube_Upload_Timer.user.js"
      ]
    }
  ]
}
