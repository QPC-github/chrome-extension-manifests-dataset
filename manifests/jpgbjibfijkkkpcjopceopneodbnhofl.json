{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter Hates <H - Militant™ edition",
  "description": "Tovarish, this extension substitutes the default favourite icon in twitter for desktops with a ☭ and more. For a lazy revolution.",
  "version": "0.81",
  "author": "@xho - https://twitter.com/xho",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "css": [
        "twitter-hate.css"
      ],
      "js": [
        "twitter-hate.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/fist.svg",
    "img/fist-on.svg",
    "img/web_heart_animation.png"
  ],
  "permissions": [
    "*://*.twitter.com/*"
  ],
  "manifest_version": 2
}
