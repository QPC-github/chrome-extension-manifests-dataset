{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Drive Commerce Pacenotes",
  "description": "This extension integrates Drive Commerce Pacenotes into your E-Commerce Environment",
  "version": "1.4",
  "icons": {
    "16": "DriveCommerce_logo_16x16.png",
    "48": "DriveCommerce_logo_48x48.png",
    "132": "DriveCommerce_logo_132x132.png"
  },
  "manifest_version": 2,
  "short_name": "Pacenotes",
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "DriveIcon.png",
    "default_title": "Drive Commerce Pacenotes Sidebar"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.demandware.net/*"
      ],
      "all_frames": true,
      "js": [
        "js/jquery-2.1.3.min.js",
        "js/sidebar.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://*.demandware.net/*",
    "storage"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ],
  "options_page": "options.html"
}
