{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Brennan B2 Chrome Extension",
  "description": "Redirects browser audio to Brennan B2 over home network",
  "version": "0.50",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "JIcon.png",
    "default_popup": "JPopup.html"
  },
  "background": {
    "scripts": [
      "JBackground.js",
      "MediaStreamRecorder.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "tabCapture",
    "<all_urls>"
  ]
}
