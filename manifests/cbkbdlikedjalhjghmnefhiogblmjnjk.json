{
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-1.9.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.9.1.min.js",
        "jquery.ui.position.min.js",
        "content-keydown.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "Trouver à qui appartient un numéro de téléphone grâce à cette extension Gratuite. Annuaire inversé",
  "homepage_url": "http://www.l-annuaire-inverse.com",
  "icons": {
    "16": "icone.png"
  },
  "manifest_version": 2,
  "name": "Annuaire inversé",
  "page_action": {
    "default_icon": "icone.png",
    "default_title": "Appuyez sur Ctrl+Shift+L pour trouver les coordonnées d'un numéro de téléphone."
  },
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
