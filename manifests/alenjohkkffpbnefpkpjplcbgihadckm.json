{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rabbit for Reddit by PiQue",
  "description": "Submit any link to Reddit",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "Rabbit for Reddit! Submit this URL to Reddit.",
    "default_icon": {
      "16": "rabbit16.png",
      "19": "rabbit19.png",
      "38": "rabbit38.png",
      "128": "rabbit128.png"
    }
  },
  "icons": {
    "16": "rabbit16.png",
    "19": "rabbit19.png",
    "38": "rabbit38.png",
    "128": "rabbit128.png"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "options_page": "options.html"
}
