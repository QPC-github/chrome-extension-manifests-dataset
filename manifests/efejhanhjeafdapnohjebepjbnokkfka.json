{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "tdwtf-read.css"
      ],
      "js": [
        "tdwtf-read.js"
      ],
      "matches": [
        "http://www.thedailywtf.com/*",
        "http://thedailywtf.com/*",
        "http://what.thedailywtf.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Enhances thedailywtf.com by hiding left bar and highlighting in-article comments/unicornifications.",
  "icons": {
    "128": "icon-128.png",
    "32": "icon-32.png",
    "64": "icon-64.png"
  },
  "manifest_version": 2,
  "name": "Thedailywtf readability formatter",
  "permissions": [
    "http://thedailywtf.com/*",
    "http://what.thedailywtf.com/*"
  ],
  "short_name": "Thedailywtf readability formatter",
  "version": "1.2.2"
}
