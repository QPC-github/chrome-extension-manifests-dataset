{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ppTBLdrPlugin",
  "description": "A join a conference ppapi plugin in Chrome browsers.",
  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "offline_enabled": true,
  "version": "1.0.0.1",
  "permissions": [
    {
      "fileSystem": [
        "write"
      ]
    },
    "alarms",
    "storage"
  ],
  "platforms": [
    {
      "sub_package_path": "_platform_specific/all/"
    }
  ]
}
