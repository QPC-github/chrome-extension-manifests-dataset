{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LMT Zeppelin Dark Theme",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "Dark theme for local Zeppelin or Zepl.",
  "homepage_url": "https://www.facebook.com/roggertanvus",
  "icons": {
    "16": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.zepl.com/*"
      ],
      "js": [
        "inject.js"
      ],
      "css": [
        "zepl.css"
      ]
    },
    {
      "matches": [
        "http://localhost:8080/*"
      ],
      "js": [
        "inject.js"
      ],
      "css": [
        "local.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "LMT Zeppelin Dark Theme"
  },
  "permissions": [
    "https://www.zepl.com/*",
    "http://localhost:8080/*"
  ],
  "web_accessible_resources": [
    "images/zepLogo.png"
  ]
}
