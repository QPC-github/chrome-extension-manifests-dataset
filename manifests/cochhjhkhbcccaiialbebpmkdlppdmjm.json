{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Liberate",
  "version": "0.2",
  "description": "Send Soundcloud and YouTube pages directly to OffLiberty",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "page_action": {
    "default_title": "Liberate",
    "default_icon": "liberty.png"
  },
  "icons": {
    "128": "liberty_128.png",
    "48": "liberty_48.png",
    "16": "liberty_16.png"
  },
  "manifest_version": 2
}
