{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stoofoo Post Preview",
  "description": "Adds a post preview to the stoofoo post textbox",
  "version": "0.1",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.stoofoo.net/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "post-preview.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
