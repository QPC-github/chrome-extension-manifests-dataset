{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Phsar Plus",
  "description": "This is PhsarPlus Chrome Extension. Phsarplus is the best online social super market in cambodia",
  "version": "1.5",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "3.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "3.js",
      "2.js"
    ]
  },
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "48X48.png"
  ],
  "icons": {
    "16": "16X16.png",
    "48": "48X48.png",
    "128": "128X128.png"
  },
  "browser_action": {
    "default_icon": "16X16.png",
    "default_title": "Phsar Plus",
    "default_popup": "popup.html"
  }
}
