{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Supernatural Türkiye",
  "short_name": "spnturkey",
  "description": "Bu uygulama Supernatural dizisinin Türkiye'deki izleyicilerinin hem siteyi hem de diziyi yakından takip edebilmesi için hazırlandı.",
  "manifest_version": 2,
  "version": "1.1",
  "background": {
    "scripts": [
      "js/arkaplan.js"
    ]
  },
  "permissions": [
    "http://api.supernatural.com.tr/*",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "pencere.html"
  },
  "icons": {
    "32": "img/icon-32.png",
    "64": "img/icon.png",
    "256": "img/icon-256.png"
  }
}
