{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Laff track",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Want to make all Youtube videos funny? Enable Laff track in your tab!",
  "homepage_url": "https://pederfjallstrom.com/lafftrack",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "permissions": [
    "tabCapture",
    "activeTab",
    "*://*.youtube.com/*"
  ],
  "browser_action": {
    "default_title": "Enable Laff track"
  }
}
