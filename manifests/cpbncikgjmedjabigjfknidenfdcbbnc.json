{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy-Pass",
  "description": "Quick and Easy way to store your passwords securely",
  "version": "5.0.0",
  "icons": {
    "128": "shield.png",
    "16": "icon_128.png",
    "32": "shield32.png"
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "permissions": [
    "storage",
    "notifications"
  ]
}
