{
  "name": "__MSG_extName__",
  "description": "This is the best goal management chrome extension.",
  "default_locale": "en",
  "icons": {
    "16": "focus-16.png",
    "128": "focus-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "offline_enabled": true,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1",
  "content_security_policy": "script-src 'self' https://query.yahooapis.com https://www.google-analytics.com/ https://maps.google.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "unlimitedStorage",
    "geolocation",
    "https://*.unsplash.com/*"
  ],
  "background": {
    "scripts": [
      "js/setbackground.js",
      "js/setweather.js"
    ]
  },
  "browser_action": {
    "default_title": "Focus"
  }
}
