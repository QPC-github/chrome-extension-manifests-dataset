{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FaceMote Beta",
  "description": "Emoticons para Facebook",
  "version": "0.1",
  "permissions": [
    "clipboardWrite"
  ],
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "FaceMote",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "popup.html",
    "persistent": false
  },
  "manifest_version": 2
}
