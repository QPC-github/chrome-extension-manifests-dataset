{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Secure Cloud Credit Card Autho App Test",
  "short_name": "CC App Test",
  "description": "Approval return for ext testing",
  "version": "1.2",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "assets/circleCheck.png",
    "128": "assets/circleCheck.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "serial",
    "webview",
    "storage",
    {
      "fileSystem": [
        "write"
      ]
    }
  ]
}
