{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chaturbate Full Page Video",
  "description": "Display the webcam video over the whole page, in the correct ratio.",
  "version": "1.0",
  "icons": {
    "128": "Icon128.png",
    "48": "Icon48.png",
    "16": "Icon16.png"
  },
  "permissions": [
    "*://*.chaturbate.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.chaturbate.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "Icon16.png"
  }
}
