{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Поиск авиабилетов и отелей",
  "version": "0.3.3",
  "manifest_version": 2,
  "description": "Приложение позволяет осуществлять поиск авиабилетов более чем по 700 авиакомпаниями и 5 системам бронирования",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": "icons/icon64.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://nano.aviasales.ru/*"
  ],
  "icons": {
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "homepage_url": "http://travel-tickets.ru/"
}
