{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chainsearchers",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "The Chainsearchers extension allows you to easily interact with the Chainsearchers website.",
  "key": "",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "scripts/jquery.min.js",
      "scripts/url.js",
      "scripts/auth.js",
      "scripts/list_service.js",
      "scripts/tag_service.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "identity",
    "http://www.chainsearchers.com/oauth/authorize*",
    "http://www.chainsearchers.com/oauth/token*"
  ]
}
