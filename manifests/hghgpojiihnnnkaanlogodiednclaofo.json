{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "js/add_button.js"
      ],
      "matches": [
        "*://www.google.am/*",
        "*://www.google.az/*",
        "*://www.google.by/*",
        "*://www.google.com.ua/*",
        "*://www.google.com/*",
        "*://www.google.ee/*",
        "*://www.google.fi/*",
        "*://www.google.ge/*",
        "*://www.google.kg/*",
        "*://www.google.kz/*",
        "*://www.google.lt/*",
        "*://www.google.lv/*",
        "*://www.google.md/*",
        "*://www.google.ru/*",
        "*://www.google.tm/*",
        "*://www.google.tr/*",
        "*://www.google.uz/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png"
  },
  "manifest_version": 2,
  "name": "Кнопка «Поиск по товарам» на странице Google™",
  "version": "1.1"
}
