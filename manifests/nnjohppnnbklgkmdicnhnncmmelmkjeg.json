{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timon++",
  "version": "1.0.10",
  "manifest_version": 2,
  "description": "Better view on Timon",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.1.3.min.js",
        "tpp.js"
      ],
      "run_at": "document_end",
      "matches": [
        "http://timon/*",
        "https://timon/*",
        "http://*.timon.is/*",
        "https://*.timon.is/*"
      ],
      "all_frames": false
    }
  ],
  "permissions": [
    "http://*/",
    "https://*/"
  ]
}
