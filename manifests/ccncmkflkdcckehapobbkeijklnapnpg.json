{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Karthikeyan - karthik21@live.in",
  "name": "Dirt Farmer's Farmville Toolbar",
  "version": "3",
  "manifest_version": 2,
  "description": "Dirt Farmer's Farmville Toolbar",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_title": "Dirt Farmer's Farmville Toolbar",
    "default_popup": "DirtFarmerTools.html"
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "webNavigation",
    "management",
    "tabs",
    "http://*/",
    "https://*/",
    "http://feeds.feedburner.com/blogspot/BvLXR"
  ]
}
