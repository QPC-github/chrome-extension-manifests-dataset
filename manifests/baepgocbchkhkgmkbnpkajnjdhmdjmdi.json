{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Omnibox Word Info",
  "description": "Allows you to get defintions, pos, and synonyms in the omnibox",
  "manifest_version": 2,
  "version": "1.4",
  "omnibox": {
    "keyword": ":"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://cors.io/*"
  ]
}
