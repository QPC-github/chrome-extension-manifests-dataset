{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Search Youtube",
  "version": "1.0",
  "description": "A plugin to allow you to go straight to a search results Youtube page",
  "manifest_version": 2,
  "browser_action": {
    "icons": {
      "16": "16Icon.png",
      "48": "48Icon.png",
      "128": "128Icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://youtube.com/*",
    "http://www.youtube.com/*"
  ]
}
