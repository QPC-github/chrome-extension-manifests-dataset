{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OGV Energy",
  "version": "0.5",
  "description": "Energy sector news daily. Oil, gas and energy publication supported by a global energy project tracker and industry job platform.",
  "permissions": [
    "https://oilandgasvision/api/extension",
    "https://www.oilandgasvisionjobs.com/api/extension"
  ],
  "browser_action": {
    "default_title": "My Bookmarks",
    "default_popup": "pages/popup/popup.html",
    "default_icon": {
      "16": "icons/favicon-96x96.png",
      "48": "icons/favicon-96x96.png",
      "128": "icons/favicon-96x96.png",
      "192": "icons/android-icon-192x192.png"
    }
  },
  "icons": {
    "16": "icons/favicon-96x96.png",
    "48": "icons/favicon-96x96.png",
    "128": "icons/favicon-96x96.png",
    "192": "icons/android-icon-192x192.png"
  },
  "chrome_url_overrides": {
    "newtab": "pages/tab/tab.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://ssl.google-analytics.com; object-src 'self'"
}
