{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Previous Preview",
  "version": "0.4",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/checkForPrev.js"
    ],
    "persistent": false
  },
  "description": "This extension will close any previous preview window generated from Escenic Content Studio.",
  "browser_action": {
    "default_icon": "gfx/icon-16.png"
  },
  "icons": {
    "16": "gfx/icon-16.png",
    "32": "gfx/icon-32.png",
    "48": "gfx/icon-48.png",
    "64": "gfx/icon-64.png",
    "128": "gfx/icon-128.png"
  }
}
