{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AllocineSearch",
  "description": "This extension add a context menu item to launch a search on Allocine website on a new tab",
  "version": "1.0.1",
  "author": "Laurent Gayet",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon-bitty.png",
    "48": "icon-small.png",
    "128": "icon-large.png"
  },
  "background": {
    "scripts": [
      "init.js"
    ]
  }
}
