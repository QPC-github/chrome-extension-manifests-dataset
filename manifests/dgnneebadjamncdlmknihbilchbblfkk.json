{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PPLZ Right Click Uploader",
  "version": "1.3",
  "description": "Закачка картинок и видео правой кнопкой мыши на pplz.ru",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "128": "image128.png",
    "16": "image16.png",
    "48": "image48.png"
  },
  "homepage_url": "http://pplz.ru",
  "minimum_chrome_version": "10.0.0.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
