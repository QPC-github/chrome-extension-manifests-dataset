{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouRead Talking Mail",
  "description": "This extension inserts a link into your email that allows recipients to listen to email, thus increasing chances of being consumed",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "content.js",
        "loader.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "https://code.jquery.com/",
    "https://drive.google.com/",
    "https://audiobookmaker.azurewebsites.net/",
    "https://s3-ap-southeast-2.amazonaws.com/",
    "https://audiobookmaker-doctorx.azurewebsites.net/",
    "https://youread.app/"
  ],
  "manifest_version": 2
}
