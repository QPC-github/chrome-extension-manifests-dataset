{
  "background": {
    "scripts": [
      "js/api.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/19x19.png",
      "38": "img/38x38.png"
    },
    "default_popup": "html/popup.html",
    "default_title": "NetDB.io - click for details"
  },
  "author": "James Jara jamesjara@gmail.com",
  "description": "The NetDB plugin will use techniques like banner grabbing over this website to tell you where is hosted/ports/banners",
  "icons": {
    "128": "img/128x128.png",
    "16": "img/16x16.png",
    "48": "img/48x48.png"
  },
  "manifest_version": 2,
  "name": "NetDB IoT Search Engine",
  "permissions": [
    "tabs",
    "contextMenus",
    "https://api.shodan.io/",
    "https://www.shodan.io/",
    "https://netdb.io/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0"
}
