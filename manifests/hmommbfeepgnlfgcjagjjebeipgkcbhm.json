{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Amity Faculty Feedback Auto Fill",
  "version": "1.0",
  "description": "Auto Fill The Amity University NTCC ",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "defaul_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://student.amizone.net/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://student.amizone.net/*"
  ]
}
