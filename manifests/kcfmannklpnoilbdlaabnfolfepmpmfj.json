{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Check Range",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Shift-click first and last checkbox to select/deselect range.",
  "icons": {
    "16": "16.png",
    "24": "24.png",
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "72": "72.png",
    "128": "128.png",
    "256": "256.png",
    "512": "512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "check_range.user.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
