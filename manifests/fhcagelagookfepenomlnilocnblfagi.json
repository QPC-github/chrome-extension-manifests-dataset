{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "eastNote",
  "description": "take notes - and keep them synced with your chrome account",
  "version": "2.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "enote"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "electricAsia.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
