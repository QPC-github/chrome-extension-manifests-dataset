{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Roller Corrector",
  "author": "John M Fallon - @krinpit - fallonj@gmail.com",
  "description": "Makes Rollercoaster.ie easier to read for those who don't like internet forum abbreviations. Author: John M Fallon - @krinpit",
  "version": "1.3",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.rollercoaster.ie/*"
      ],
      "run_at": "document_start",
      "js": [
        "jquery-2.1.3.min.js",
        "background.js"
      ]
    }
  ],
  "icons": {
    "16": "Baby-icon16px.png",
    "48": "Baby-icon48px.png",
    "128": "Baby-icon128px.png"
  },
  "manifest_version": 2
}
