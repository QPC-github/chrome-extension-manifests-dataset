{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "No Column Width",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Get rid of `width=blah` values from table columns (it never looks good anyway!).",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "remove-column-widths.js"
      ]
    }
  ]
}
