{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Avioos",
  "description": "Avioos",
  "version": "1.5",
  "background": {
    "scripts": [
      "js/background.js",
      "js/options.js",
      "js/lib/angular.min.js",
      "js/popup.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "img/avioon19.png",
    "name": "Open local Avioos",
    "default_title": "Avioos Application Platform.",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "tabs",
    "storage"
  ],
  "manifest_version": 2
}
