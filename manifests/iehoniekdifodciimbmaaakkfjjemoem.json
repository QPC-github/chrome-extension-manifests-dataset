{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "2Pick",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://www.2pick.co/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Pick It!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
