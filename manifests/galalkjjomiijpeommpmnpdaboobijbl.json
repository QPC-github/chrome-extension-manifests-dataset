{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cute Puppy Extension",
  "description": "Change your Search Engine Background to a Puppy photo",
  "homepage_url": "https://www.cutepuppyextension.com/",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "https://www.bex-digital.com/puppy/*"
  ],
  "version": "3.2",
  "browser_action": {
    "default_icon": {
      "16": "images/bd-16.png",
      "32": "images/bd-32.png",
      "48": "images/bd-48.png",
      "128": "images/bd-128.png"
    }
  },
  "icons": {
    "16": "images/bd-16.png",
    "32": "images/bd-32.png",
    "48": "images/bd-48.png",
    "128": "images/bd-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "landing.html"
  },
  "web_accessible_resources": [
    "images/*.*"
  ],
  "externally_connectable": {
    "matches": [
      "https://www.cutepuppyextension.com/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.bex-digital.com/puppy/custom.js https://www.bex-digital.com/puppy/background.js; object-src 'self' ;style-src 'self' 'unsafe-inline' *;media-src *;"
}
