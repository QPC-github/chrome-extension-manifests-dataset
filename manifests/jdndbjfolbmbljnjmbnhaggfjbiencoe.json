{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Praxey",
  "version": "0.01",
  "description": "A chrome extension which enables screensharing for Praxey web app.",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://00962734.ngrok.io/*",
      "*://praxey.herokuapp.com/*",
      "*://praxey.com/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
