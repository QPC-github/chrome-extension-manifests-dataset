    "assets/placeholder.png",
    "assets/profile-box.png",
    "assets/profile-image.jpg",
    "assets/profile-link-bg.png",
    "assets/radio.png",
    "assets/response-rate-ico.png",
    "assets/select-bg.png",
    "assets/sender-ico.png",
    "assets/sep.png",
    "assets/settings-link-bg.png",
