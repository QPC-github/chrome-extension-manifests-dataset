{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_glider__",
  "default_locale": "en",
  "version": "0.0.20",
  "description": "Eliminate distracting emails. Save time in your inbox immediately.",
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Glider.io"
  },
  "content_scripts": [
    {
      "matches": [
        "http://mail.google.com/*",
        "https://mail.google.com/*"
      ],
      "js": [
        "lib/bootstrap.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.glider.io/*",
        "http://www.dev.glider.io:3000/*"
      ],
      "js": [
        "lib/bootstrap_glider.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "storage",
    "notifications",
    "tabs",
    "bookmarks",
    "http://*/*",
    "https://*/*",
    "http://mail.google.com/*",
    "https://mail.google.com/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "assets/avatar1.jpg",
    "assets/avatar2.jpg",
    "assets/avatar3.jpg",
    "assets/avatar4.jpg",
    "assets/avatar5.jpg",
    "assets/avatar-img-2.png",
    "assets/avatar-img-3.png",
    "assets/avatar-img-4.png",
    "assets/avatar-img.png",
    "assets/bars.png",
    "assets/bascet-icon.png",
    "assets/chart.jpg",
    "assets/chart.png",
    "assets/charts-link-bg.png",
    "assets/contact-icon.png",
    "assets/contact-ico.png",
    "assets/control-link-bg.png",
    "assets/conv-ico.png",
    "assets/desktop-ico.png",
    "assets/email-box.png",
    "assets/email-ico.png",
    "assets/email-read-ico.png",
    "assets/fancybox.png",
    "assets/favicon.ico",
    "assets/glider-logo.png",
    "assets/help-button.png",
    "assets/helveticaneueltstd-bd-webfont.eot",
    "assets/helveticaneueltstd-bd-webfont.svg",
    "assets/helveticaneueltstd-bd-webfont.ttf",
    "assets/helveticaneueltstd-bd-webfont.woff",
    "assets/helveticaneueltstd-roman-webfont.eot",
    "assets/helveticaneueltstd-roman-webfont.svg",
    "assets/helveticaneueltstd-roman-webfont.ttf",
    "assets/helveticaneueltstd-roman-webfont.woff",
    "assets/hours.png",
    "assets/image.jpg",
