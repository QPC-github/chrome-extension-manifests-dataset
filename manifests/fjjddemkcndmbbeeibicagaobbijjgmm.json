{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alerte Bons Plans eBuyClub",
  "version": "3.3.2.0",
  "description": "Gagnez de l’argent sur tous vos achats avec l’Alerte Bons Plans eBuyClub !",
  "homepage_url": "https://ebuyclub.com",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "ci.content.pack.js",
        "libs/jquery-3.3.1.min.js",
        "libs/jquery-no-conflict.js",
        "libs/handlebars.runtime.min.js",
        "libs/underscore-min.js",
        "libs/backbone-min.js",
        "libs/backbone-no-conflict.js",
        "libs/underscore-no-conflict.js",
        "libs/jquery.transit.min.js",
        "libs/jquery.mousewheel.min.js",
        "libs/jquery.jscrollpane.min.js",
        "libs/moment.min.js",
        "handlebars-helpers.js",
        "content/style.js",
        "content/templates.js",
        "content/slider.js",
        "content/serp.js",
        "content/gmap.js",
        "content/pushNotification.js",
        "content/couponSlider.js",
        "content/couponProcess.js",
        "content/couponSuccess.js",
        "content/lastChanceSlider.js",
        "content/content.js",
        "content/coupons.js",
        "content/couponsAutoApplier.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "ci.browser.helper.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/18x18.png",
    "default_title": "Alerte Bons Plans eBuyClub"
  },
  "icons": {
    "24": "images/24x24.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  }
}
