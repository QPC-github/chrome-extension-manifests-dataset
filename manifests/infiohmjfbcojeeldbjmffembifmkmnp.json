{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "namehelp",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Helper for namehelp, a tool for improved web performance by AquaLab at Northwestern University.",
  "permissions": [
    "history",
    "http://localhost/*",
    "https://localhost/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "namehelp"
  }
}
