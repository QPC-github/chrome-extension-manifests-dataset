{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_name_local__",
  "version": "6.3",
  "default_locale": "ru",
  "description": "__MSG_descr_local__",
  "permissions": [
    "notifications",
    "storage",
    "alarms",
    "tabs",
    "https://travel-cam.net/"
  ],
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "__MSG_popup_local__"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "options_page": "options.html"
}
