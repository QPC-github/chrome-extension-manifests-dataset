{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Helm",
  "description": "Incremental completion and selection narrowing framework for google chrome.",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "helm"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "http://www.google.com/"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+J",
        "windows": "Ctrl+J",
        "mac": "Command+J",
        "chromeos": "Ctrl+J",
        "linux": "Ctrl+J"
      }
    }
  }
}
