{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "name": "Airlink Easy Access"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://10.10.0.1/*"
      ]
    }
  ],
  "description": "Un modo facile per accedere alla rete Airlink!",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "name": "Airlink Easy Access",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://10.10.0.1/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
