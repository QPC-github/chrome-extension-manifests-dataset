{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BitLnk",
  "description": "Bitlnk shortens long URL's into short, easy to remember tiny links.",
  "version": "0.6",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "manifest_version": 2
}
