{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Debeetle",
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "version": "3",
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
