{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Town Of Salem Assistant",
  "short_name": "TOS Assistant",
  "description": "It helps to keep roles of people and groups, to generate auto will from what you noted. It helps you to solve the mistery!",
  "version": "0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "icons/t-16.png",
      "32": "icons/t-32.png",
      "64": "icons/t-64.png",
      "128": "icons/t-128.png"
    },
    "default_title": "Town of Salem Assistant",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/tos-16.png",
    "128": "icons/tos-128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
