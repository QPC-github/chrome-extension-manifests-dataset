{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SealSign Central Key Control",
  "version": "1.0",
  "manifest_version": 2,
  "description": "SealSign Central Key Control Chrome Extension.",
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "https://*/*"
