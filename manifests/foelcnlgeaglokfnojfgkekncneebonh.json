{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Redmine Assistant",
  "description": "Assistant to manage tasks of Redmine.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "options_page": "options.html",
    "default_popup": "assistant.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/",
    "https://*/",
    "idle"
  ]
}
