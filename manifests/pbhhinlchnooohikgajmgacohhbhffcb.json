{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Forwarder",
  "commands": {
    "alert2": {
      "suggested_key": {
        "default": "Alt+Up"
      },
      "description": "2"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "commands",
    "tabs",
    "webRequestBlocking",
    "http://*/",
    "https://*/",
    "http://www.google.com/"
  ],
  "manifest_version": 2,
  "description": "Allows forwarding through pages",
  "version": "1.1"
}
