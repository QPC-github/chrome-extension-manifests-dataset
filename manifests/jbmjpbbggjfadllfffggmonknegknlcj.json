{
  "manifest_version": 2,
  "name": "RMag EA Extension",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.3",
  "converted_from_user_script": true,
  "description": "RMag EA Extension",
  "icons": {
    "128": "icon_RM.jpg"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://empireassault.com/*/*",
        "http://www.empireassault.com/*/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "explore.js"
      ],
      "matches": [
        "http://empireassault.com/*/explore",
        "http://www.empireassault.com/*/explore"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "military.js"
      ],
      "matches": [
        "http://empireassault.com/*/train",
        "http://www.empireassault.com/*/train"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "buildings.js"
      ],
      "matches": [
        "http://empireassault.com/*/build",
        "http://www.empireassault.com/*/build"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "research.js"
      ],
      "matches": [
        "http://empireassault.com/*/research*",
        "http://www.empireassault.com/*/research*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "attack.js"
      ],
      "matches": [
        "http://empireassault.com/*/attack*",
        "http://www.empireassault.com/*/attack"
      ],
      "run_at": "document_idle"
    }
  ]
}
