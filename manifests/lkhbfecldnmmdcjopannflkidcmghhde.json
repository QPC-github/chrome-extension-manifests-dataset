{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gathering Chrome Extension",
  "description": "Literature information picker linked with Gathering.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "Icon.png",
    "default_title": "Gathering Link"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "nativeMessaging"
  ],
  "author": "crd"
}
