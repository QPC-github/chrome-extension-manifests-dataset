{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "On-Site Editor",
  "description": "Change copy on your website. Make the page editable",
  "version": "0.0.0.1",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page editable"
  },
  "manifest_version": 2
}
