{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "deiframizer.mk",
  "description": "This extension triggers on several popular .mk portals that iframeize other sites, and removes the iframe.",
  "version": "0.42",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "http://www.time.mk/read/*",
        "http://grid.mk/read/*",
        "http://daily.mk/forward/*",
        "http://r.ping.mk/tb/*",
        "http://r.ping.mk/toolbar/*"
      ]
    }
  ],
  "manifest_version": 2
}
