{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fotor Banner Maker",
  "description": "Make photo banner online for free! No photoshop, easy to make & beautiful",
  "version": "1.0.1",
  "app": {
    "urls": [
      "http://banner.fotor.com/"
    ],
    "launch": {
      "web_url": "http://banner.fotor.com/"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
