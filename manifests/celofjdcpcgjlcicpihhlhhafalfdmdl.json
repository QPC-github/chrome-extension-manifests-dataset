{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Omnibox",
  "description": "Browse Reddit with the Address Bar",
  "version": "1.1.3",
  "permissions": [
    "https://www.reddit.com/api/search_reddit_names.json",
    "https://www.reddit.com/api/v1/me/friends.json"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "omnibox": {
    "keyword": "r"
  },
  "manifest_version": 2
}
