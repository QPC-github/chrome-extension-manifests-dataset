{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "remsidebar",
  "short_name": "remsb",
  "author": "Kade Robertson",
  "homepage_url": "http://kaderobertson.pw",
  "description": "Removes the subreddit sidebar on Reddit.",
  "version": "1.2",
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
