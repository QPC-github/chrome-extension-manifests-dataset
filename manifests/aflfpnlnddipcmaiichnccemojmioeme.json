{
  "background": {
    "scripts": [
      "scripts/jquery-1.8.1.min.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "RetornoAnime"
  },
  "description": "Las noticias más recientes de anime, manga, jpop, kpop, cosplay, cultura japonesa, eventos y mucho más",
  "homepage_url": "http://www.retornoanime.com/",
  "icons": {
    "128": "images/128.png",
    "16": "images/icon.png",
    "48": "images/48.png"
  },
  "manifest_version": 2,
  "name": "RetornoAnime",
  "permissions": [
    "http://www.retornoanime.com/feed-services/",
    "tabs",
    "notifications"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1.2"
}
