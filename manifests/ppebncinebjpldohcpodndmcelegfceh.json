{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "BankOfProductivity.com",
  "description": "BankOfProductivity gives control of time back to you. Use this extension to easily change current page's productivity mode",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "16": "images/logo/16_green.png",
    "48": "images/logo/48_green.png",
    "64": "images/logo/64_green.png",
    "128": "images/logo/128_green.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "images/logo/64_black.png"
    },
    "default_title": "Bank of productivity"
  },
  "manifest_version": 2,
  "name": "BankOfProductivity",
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "cookies",
    "https://app.bankofproductivity.com/",
    "https://bankofproductivity.com/"
  ],
  "web_accessible_resources": [
    "images/logo/128_green.png"
  ],
  "version": "0.5.4"
}
