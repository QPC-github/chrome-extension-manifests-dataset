{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Z-PRICE",
  "manifest_version": 2,
  "version": "1.2.1",
  "description": "Сравнение цен интернет-магазинов прямо на странице товара, которая у Вас открыта",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon128.png",
    "default_title": "Z-PRICE"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "/1.png",
    "/2.png",
    "/2_1.png",
    "/3.png",
    "/logo.png",
    "/facebook.png",
    "/google-plus.png",
    "/odnoklassniki.png",
    "/twitter.png",
    "/vkontakte.png",
    "/star-off.png",
    "/star-on.png",
    "/LeftArrow.png",
    "/RightArrow.png",
    "/ArrowHintText.png"
  ],
  "default_locale": "ru"
}
