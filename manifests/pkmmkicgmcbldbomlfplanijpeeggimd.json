{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "name": "noMoreDailyMail",
  "version": "1.2",
  "description": "Colours DM links and adds warning to click.",
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "default_icon": "icon-32.png",
    "default_title": "Daily Mail Warning",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.0.2.min.js",
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "nomoredailymail.css"
      ]
    }
  ]
}
