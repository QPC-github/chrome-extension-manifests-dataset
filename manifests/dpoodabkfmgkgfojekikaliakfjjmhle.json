{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social2Search",
  "version": "2.5",
  "manifest_version": 2,
  "description": "Social search matters.",
  "background": {
    "page": "html/background.html"
  },
  "permissions": [
    "http://*.social2search.com/*",
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "nativeMessaging"
  ],
  "icons": {
    "48": "priam_icon_48x48.png",
    "128": "priam_icon_128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/priam_consts.js",
        "js/priam.js",
        "js/priam_chrome.js"
      ],
      "run_at": "document_start"
    }
  ]
}
