{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scroll UP",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Scroll UP to the top of the page.",
  "author": "Sameer N Miraj",
  "icons": {
    "48": "Document-icon.png",
    "128": "Document-icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "contentScript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "image/*.png"
  ]
}
