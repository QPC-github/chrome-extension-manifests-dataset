{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quip shrink",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This extension shrink multi depth paragraph in quip.com",
  "icons": {
    "16": "images/quip_17.png",
    "48": "images/quip_48.png",
    "128": "images/quip_128.png"
  },
  "browser_action": {
    "default_icon": "images/quip_17.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "quip-extension.js",
    "quip-extension.css",
    "jquery.min.js",
    "images/quip_17.png",
    "images/plus.png",
    "images/minus.png"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
