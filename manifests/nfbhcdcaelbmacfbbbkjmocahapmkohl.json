{
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/searchUtil.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://www.switch2search.com/lp/images/favicon.ico",
      "is_default": true,
      "keyword": "Switch2Search.com",
      "name": "switch2search",
      "search_url": "http://www.switch2search.com/default?gd=EX1000331&ISID=ISID_ID&SearchSource=58&CUI=SB_CUI&UM=6&q={searchTerms}&PCSF=SU_DEFAULT",
      "suggest_url": "http://www.switch2search.com/suggest/CSuggestJson.ashx?prefix={searchTerms}&PCSF=SU_SUGGEST"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Switch2Search extension enhance your social search experience",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "name": "Switch2Search",
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/*",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "*://*.Switch2Search.com/*"
  ],
  "short_name": "Switch2Search",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0.1",
  "web_accessible_resources": [
    "html/NewTab.html"
  ]
}
