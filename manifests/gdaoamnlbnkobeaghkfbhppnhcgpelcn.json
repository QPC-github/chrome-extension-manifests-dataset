{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Embrace the Bernard",
  "description": "Replace Facebook emotions with awkward photos of Bernard Black (from Black Books).",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
