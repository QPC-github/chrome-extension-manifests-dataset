{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "4k To Butt",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.jpg"
  },
  "description": "Replaces 4k with Butt all over the web",
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "contentScript.js"
  ]
}
