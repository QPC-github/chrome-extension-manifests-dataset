{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SEO Links",
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus"
  ],
  "version": "5.01",
  "author": "Tronic",
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/core.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/off.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Touch google position on links",
  "icons": {
    "128": "images/icon-128.png",
    "48": "images/icon-48.png",
    "16": "images/icon-16.png"
  }
}
