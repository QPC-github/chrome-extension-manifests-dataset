{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HN Feed",
  "version": "1.1",
  "description": "A Chrome extension for an HN feed",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "hn.png",
    "default_title": "HN Feed",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://hacker-news.firebaseio.com/v0/*",
    "https://fonts.googleapis.com/",
    "http://fonts.googleapis.com/",
    "tabs"
  ]
}
