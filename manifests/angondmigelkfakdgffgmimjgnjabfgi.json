{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vimeo VR",
  "description": "Watch Vimeo videos in Oculus Rift",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://vimeo.com/couchmode/*",
        "https://vimeo.com/couchmode/*"
      ],
      "js": [
        "jquery.js",
        "vimeo.js"
      ]
    }
  ]
}
