{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "include_globs": [
        "http://www.flightmemory.com/signin/?go=flugdaten_edit*",
        "http://www.flugstatistik.de/login/?go=flugdaten_edit*",
        "http://www.flightmemory.com/signin/?go=flugdaten_neu*",
        "http://www.flugstatistik.de/login/?go=flugdaten_neu*",
        "https://www.flightmemory.com/signin/?go=flugdaten_edit*",
        "https://www.flugstatistik.de/login/?go=flugdaten_edit*",
        "https://www.flightmemory.com/signin/?go=flugdaten_neu*",
        "https://www.flugstatistik.de/login/?go=flugdaten_neu*"
      ],
      "js": [
        "jquery.min.js",
        "BTS-FM.user.js"
      ],
      "matches": [
        "http://www.flightmemory.com/signin/?go=flugdaten_edit*",
        "http://www.flugstatistik.de/login/?go=flugdaten_edit*",
        "http://www.flightmemory.com/signin/?go=flugdaten_neu*",
        "http://www.flugstatistik.de/login/?go=flugdaten_neu*",
        "https://www.flightmemory.com/signin/?go=flugdaten_edit*",
        "https://www.flugstatistik.de/login/?go=flugdaten_edit*",
        "https://www.flightmemory.com/signin/?go=flugdaten_neu*",
        "https://www.flugstatistik.de/login/?go=flugdaten_neu*"
      ],
      "run_at": "document_idle"
    }
  ],
  "converted_from_user_script": true,
  "description": "Search BTS Flight Data to get aircraft registration info into Flight Memory/FlugStatistik",
  "name": "FlightMemory BTS Add-In",
  "version": "0.3.1",
  "homepage_url": "http://wandr.me/",
  "short_name": "BTS-FlightMemory",
  "web_accessible_resources": [
    "BTS-FM.user.js"
  ],
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  }
}
