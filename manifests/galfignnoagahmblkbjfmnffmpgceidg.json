{
  "manifest_version": 2,
  "name": "Peanuts",
  "version": "1.0.8",
  "description": "一款智能网络测速插件.",
  "icons": {
    "16": "images/peanutsman/logos/logo_grey.png",
    "48": "images/peanutsman/logos/logo_grey.png",
    "128": "images/peanutsman/logos/logo_grey.png"
  },
  "browser_action": {
    "defualt_icon": {
      "19": "images/peanutsman/logos/logo_grey.png",
      "38": "images/peanutsman/logos/logo_grey.png"
    },
    "default_title": "Peanuts",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "management",
    "proxy",
    "idle",
    "tabs",
    "cookies",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "background": {
    "page": "main.html"
  },
  "update_url": "http://clients2.google.com/service/update2/crx"
}
