{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.2",
  "description": "A theme solely devoted to one of the best game in the Assassin's Creed Series. Syndicate! Enjoy :-)",
  "name": "Assassin's Creed: Syndicate Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_title": "Assassin's Creed!"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Syndicate.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
