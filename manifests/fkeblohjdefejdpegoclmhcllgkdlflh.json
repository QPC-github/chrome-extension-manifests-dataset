{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Manager",
  "version": "1.3",
  "manifest_version": 2,
  "description": "work scheduler",
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "clock.png"
  }
}
