{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smart Devices",
  "description": "\"Smart Devices\" official extension to control your \"Smart Home\"",
  "version": "1.1.3",
  "manifest_version": 2,
  "homepage_url": "https://smartdevices.io/",
  "background": {
    "scripts": [
      "socket.io.js"
    ]
  },
  "permissions": [
    "notifications",
    "background",
    "contextMenus"
  ],
  "offline_enabled": true,
  "options_ui": {
    "page": "options.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo2.png",
    "128": "logo2.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
