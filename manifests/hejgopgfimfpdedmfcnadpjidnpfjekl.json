{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Learn Languages with Google Translate",
  "version": "2.8",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
