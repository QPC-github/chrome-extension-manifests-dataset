{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Print fra Politiken",
  "version": "1.0.1",
  "description": "Print direkte fra Politiken.dk.",
  "author": "Nikolaj Munk",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "*://*.politiken.dk/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.politiken.dk/*"
      ],
      "css": [
        "buttonstyle.css"
      ],
      "js": [
        "printbutton.js"
      ]
    }
  ]
}
