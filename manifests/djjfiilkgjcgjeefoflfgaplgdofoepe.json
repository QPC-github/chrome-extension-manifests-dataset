{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyMetrix Extension by comScore Inc.",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Allows comScore subscribers to get a quick overview of a site's digital audience size, rank, demographics, and engagement.",
  "homepage_url": "http://comscore.com",
  "icons": {
    "16": "icons/icon16_2018.png",
    "48": "icons/icon48_2018.png",
    "128": "icons/icon128_2018.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19_2018.png",
    "default_title": "MyMetrix Extension",
    "default_popup": "index.html"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs"
  ]
}
