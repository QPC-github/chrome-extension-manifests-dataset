{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Big Map for Openrunner",
  "version": "1.0",
  "description": "Enable fullscreen mode for Openrunner map tools",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.openrunner.com/*"
      ],
      "css": [
        "or.css"
      ],
      "js": [
        "or.js"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
