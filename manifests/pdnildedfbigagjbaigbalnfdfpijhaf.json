{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "author": "Nico Hemkes",
  "name": "MyIP - Host IP, ISP and DNS",
  "short_name": "MyIP",
  "description": "This extension display your public IPv4 address, provider and the dns server.",
  "version": "1.2.1",
  "browser_action": {
    "default_icon": "icons/icon-128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "http://ip-api.com/",
    "http://edns.ip-api.com/"
  ]
}
