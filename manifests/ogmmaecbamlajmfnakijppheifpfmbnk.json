{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Radios.com.br",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Encontre a sua radio favorita direto do Google Chrome",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Radios.com.br",
    "default_popup": "form_busca.html"
  },
  "web_accessible_resources": [
    "icon48.png"
  ],
  "permissions": [
    "http://www.radios.com.br/",
    "tabs",
    "cookies"
  ]
}
