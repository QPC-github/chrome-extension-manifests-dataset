{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SuperCity Access",
  "version": "0.2.2",
  "description": "Play SuperCity using this extension from Playkot LLC.",
  "permissions": [
    "contentSettings",
    "background",
    "*://*.facebook.com/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "assets/icon_48.png"
  },
  "icons": {
    "16": "assets/icon_16.png",
    "32": "assets/icon_32.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "minimum_chrome_version": "59",
  "manifest_version": 2
}
