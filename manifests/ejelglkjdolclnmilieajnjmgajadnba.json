{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "баллы ИТМО [de.ifmo.checker]",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Расширение, которое позволяет просматривать баллы на de.ifmo.ru одним кликом. А так же свой рейтинг среди студентов и расписание",
  "icons": {
    "128": "images/icon_128.png"
  },
  "permissions": [
    "http://*.ifmo.ru/",
    "https://*.ifmo.ru/"
  ],
  "browser_action": {
    "default_title": "de.ifmo.checker",
    "default_icon": "images/icon_19.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
