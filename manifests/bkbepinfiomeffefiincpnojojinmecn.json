{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FOMO3D Companion",
  "version": "1.0.3",
  "description": "Watch the clock without actually watching the clock. Together we can keep round 9 alive forever.",
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background-obfu.js",
      "web3.min.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/fomo-logo.png",
      "32": "images/fomo-logo.png",
      "48": "images/fomo-logo.png",
      "128": "images/fomo-logo.png"
    }
  },
  "manifest_version": 2
}
