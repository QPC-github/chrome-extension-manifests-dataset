{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What to Stream?",
  "description": "A utility for Twitch streamers to maximize their viewership potential.",
  "version": "0.3.1",
  "short_name": "What to Stream?",
  "permissions": [
    "https://api.twitch.tv/kraken"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  }
}
