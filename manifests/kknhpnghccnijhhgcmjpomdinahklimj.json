{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "This extension allows you to stream your desktop via Gibson's video conference.",
  "externally_connectable": {
    "matches": [
      "*://mail.dhgibson.com.au/*",
      "*://conference.dhgibson.com.au/*",
      "*://meet.jit.si/*",
      "*://beta.meet.jit.si/*",
      "*://vuc.jitsi.org/*",
      "*://pawel.jitsi.net/*",
      "*://fo.jitsi.net/*"
    ]
  },
  "icons": {
    "128": "gibson-logo-128x128.png",
    "16": "gibson-logo-16x16.png",
    "48": "gibson-logo-48x48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Gibson Shopfitters Conference",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.4.2"
}
