{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "x83 MOON",
  "description": "On the legend above you can observe three distinct MOON features. The playlist will be accesible on the left sidebar.",
  "version": "42",
  "minimum_chrome_version": "37",
  "offline_enabled": true,
  "externally_connectable": {
    "ids": [
      "*"
    ]
  },
  "app": {
    "background": {
      "persistent": true,
      "scripts": [
        "view-source"
      ]
    }
  },
  "icons": {
    "128": "assets/ChromeMoon/x128.png"
  },
  "permissions": [
    "videoCapture",
    "app.window.alpha",
    "<all_urls>",
    "alwaysOnTopWindows",
    "webview",
    "fullscreen",
    "pointerLock",
    "unlimitedStorage",
    "notifications",
    "storage",
    {
      "socket": [
        "udp-send-to",
        "udp-bind",
        "udp-multicast-membership",
        "tcp-connect",
        "tcp-listen"
      ]
    },
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ]
}
