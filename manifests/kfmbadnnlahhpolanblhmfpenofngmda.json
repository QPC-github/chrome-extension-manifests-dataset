{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Task Extension",
  "description": "Details of Youtube process.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here for details."
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Y",
        "windows": "Ctrl+Y",
        "mac": "Command+Y",
        "chromeos": "Ctrl+Y",
        "linux": "Ctrl+Y"
      }
    }
  }
}
