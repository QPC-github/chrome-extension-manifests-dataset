{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ansi and Ascii Artists Handle Database",
  "description": "Displays the handle of Ansi and Ascii artists on Facebook.",
  "version": "1.6",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
