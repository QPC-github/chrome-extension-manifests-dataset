{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Battlelog Fix",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Changes the new Battlelog.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon48.png"
  },
  "permissions": [
    "http://battlelog.battlefield.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://battlelog.battlefield.com/bf3/*"
      ],
      "css": [
        "bf.css"
      ]
    }
  ]
}
