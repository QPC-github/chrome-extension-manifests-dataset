{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "New Tab, New Cat",
  "version": "1.0",
  "description": "Puts cats on every new tab",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
