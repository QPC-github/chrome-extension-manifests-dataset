{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "32": "images/trash_icon_32px.png",
      "64": "images/trash_icon_64px.png"
    },
    "default_title": "Fake News Spotter Lite"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://www.googletagmanager.com  ; object-src 'self'",
  "description": "Know the news you are reading is fake or not",
  "homepage_url": "http://www.mysmartprice.com/",
  "icons": {
    "128": "images/trash_icon_128px.png",
    "16": "images/trash_icon_16px.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Fake News Spotter Lite",
  "permissions": [
    "tabs",
    "alarms",
    "http://*/*",
    "https://*/*",
    "cookies",
    "notifications",
    "contextMenus",
    "gcm",
    "identity",
    "unlimitedStorage",
    "storage",
    "system.memory",
    "identity.email"
  ],
  "short_name": "FNS",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [
    "images/trash_icon_32px.png"
  ]
}
