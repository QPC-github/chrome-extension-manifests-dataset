{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PlaneswalkerPoints Stat",
  "short_name": "PWP Stats",
  "version": "1.1.1",
  "description": "Show Your PlanesWalker Points statistic",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.wizards.com/Magic/PlaneswalkerPoints/*",
        "https://dci.mtgbox.ru/*"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "jalc.min.js",
        "extentions.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "png16.png",
    "48": "png48.png",
    "128": "png128.png"
  },
  "permissions": [
    "downloads"
  ]
}
