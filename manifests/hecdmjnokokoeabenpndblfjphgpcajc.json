{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickTask",
  "description": "Take a quick look at your tasks",
  "version": "1.9",
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.0/angular.min.js; object-src 'self'",
  "permissions": [
    "storage"
  ]
}
