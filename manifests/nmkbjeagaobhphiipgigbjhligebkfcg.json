{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Abstract Browsing",
  "version": "1.3.0",
  "manifest_version": 2,
  "description": "Abstract browsing",
  "homepage_url": "http://www.abstractbrowsing.net",
  "author": "RafaÃ«l Rozendaal & Reinier Feijen",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "hide.css"
      ],
      "js": [
        "config.js",
        "check.js",
        "jquery-2.1.1.min.js"
      ]
    },
    {
      "matches": [
        "*://chrome.google.com/webstore*"
      ],
      "css": [
        "general.css"
      ]
    }
  ],
  "browser_action": {
    "name": "Start Abstract Browsing",
    "default_icon": "abstract-yes.png",
    "icons": [
      "abstract-yes.png",
      "abstract-no.png"
    ]
  }
}
