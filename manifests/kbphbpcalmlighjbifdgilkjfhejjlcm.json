{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "A5 Launcher",
  "description": "Quick launch A5 Pages",
  "version": "1.0.0",
  "icons": {
    "128": "larger-icon.png"
  },
  "browser_action": {
    "default_icon": "small-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
