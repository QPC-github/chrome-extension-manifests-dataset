{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "applications": {
    "gecko": {
      "id": "window.resizer@soustruh.info",
      "strict_min_version": "48.0"
    }
  },
  "manifest_version": 2,
  "name": "Simple Window Resizer",
  "version": "1.2",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "icon_32.png"
    },
    "default_title": "Resize!"
  },
  "options_ui": {
    "page": "options.html"
  }
}
