{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "pagePos",
  "description": "Save page position by simply clicking the icon.",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "version": "1.2.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "pagepos_icon.png"
  },
  "permissions": [
    "http://*/",
    "tabs",
    "storage"
  ],
  "icons": {
    "48": "pagepos_icon48.png",
    "64": "pagepos_icon64.png",
    "128": "pagepos_icon.png"
  },
  "manifest_version": 2
}
