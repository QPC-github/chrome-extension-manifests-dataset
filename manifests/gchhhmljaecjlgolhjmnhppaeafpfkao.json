{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SimpleClock",
  "description": "Customises your new tab with a simple and clutter-free display.",
  "version": "3",
  "author": "Simple Clock Extensions",
  "browser_action": {
    "default_title": "New Tab",
    "default_icon": "time.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
