{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cenowy Sknera",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "Dominik Lipinski",
  "description": "Pozwala zaoszczędzić pieniądze na zakupach online. Sprawdza i wyświetla cenę artykułu w innych sklepach.",
  "icons": {
    "16": "imgs/dollar_16.png",
    "32": "imgs/dollar_32.png",
    "64": "imgs/dollar_64.png"
  },
  "browser_action": {
    "default_icon": "imgs/dollar_32.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "events.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
