{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kenjo Active Sourcing",
  "description": "Easily source talent to your Kenjo account from social networks.",
  "version": "8.0.0",
  "author": "Kenjo",
  "icons": {
    "16": "images/kenjo-logo16.png",
    "32": "images/kenjo-logo32.png",
    "48": "images/kenjo-logo48.png",
    "128": "images/kenjo-logo128.png"
  },
  "browser_action": {
    "default_title": "Kenjo",
    "default_icon": {
      "16": "images/kenjo-logo16.png",
      "32": "images/kenjo-logo32.png",
      "48": "images/kenjo-logo48.png",
      "128": "images/kenjo-logo128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src-elem 'self' https://unpkg.com/material-components-web@3.0.0/dist/material-components-web.min.js; script-src 'self' https://unpkg.com/material-components-web@3.0.0/dist/material-components-web.min.js; object-src 'self'"
}
