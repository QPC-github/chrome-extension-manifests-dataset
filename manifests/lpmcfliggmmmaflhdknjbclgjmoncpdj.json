{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "one-click dogetip",
  "description": "This extension makes it",
  "version": "1.1",
  "permissions": [
    "https://www.reddit.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "css": [
        "dogetip.css"
      ],
      "js": [
        "jquery.js",
        "dogetip.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/dogecoin.png"
  ]
}
