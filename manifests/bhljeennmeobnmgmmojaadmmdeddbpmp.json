{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Interrupt, Click, Repeat",
  "description": "Attempts to auto click cookie messages and closes interrupters.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "icons": {
    "32": "myicon.png",
    "64": "myicon64.png",
    "128": "myicon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
