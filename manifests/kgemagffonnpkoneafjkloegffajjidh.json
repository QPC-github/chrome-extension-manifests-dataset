{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MongoLab Link Fixer",
  "description": "Fixes an issue with link's containing slashes on MongoLab.com.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mongolab.com/clusters/*"
      ],
      "js": [
        "linkfix.js"
      ]
    }
  ]
}
