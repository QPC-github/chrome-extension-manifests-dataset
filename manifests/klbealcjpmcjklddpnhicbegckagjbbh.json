{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ZUGignore",
  "version": "0.0.5",
  "description": "Zugunder user ignoring script",
  "content_scripts": [
    {
      "matches": [
        "http://zugunder.com/*"
      ],
      "js": [
        "zugignoreChrome.user.js"
      ]
    }
  ]
}
