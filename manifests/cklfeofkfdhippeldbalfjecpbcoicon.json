{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wraparoundkids Desktop Capturing",
  "author": "Dinesh Karthik",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Required extension for desktop sharing feature in wraparoundkids",
  "homepage_url": "https://wraparoundkids.com.au",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "matches": [
        "*://www.wraparoundkids.com.au/*",
        "*://wraparoundkids.com.au/*"
      ]
    }
  ],
  "icons": {
    "64": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
