{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AlarmMe",
  "description": "Alarm you at the given time or after a given time period.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "My Simple Alarm"
  }
}
