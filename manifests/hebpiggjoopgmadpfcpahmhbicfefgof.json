{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Job Count",
  "version": "0.13",
  "manifest_version": 2,
  "description": "Your Job Search - Simplified",
  "icons": {
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "/lib/jquery-2.1.1.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/off128.png",
    "default_popup": "html/popup.html"
  },
  "content_security_policy": "script-src 'self' https://gdc.indeed.com; object-src 'self'",
  "permissions": [
    "tabs",
    "storage",
    "http://api.indeed.com/*"
  ]
}
