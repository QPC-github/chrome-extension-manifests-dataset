{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Paint your ScreenShot",
  "description": "Here we can draw on the ScreenShot",
  "version": "0.1",
  "icons": {
    "16": "highlights.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "ScreenShot",
    "default_icon": "highlights.png"
  },
  "background": {
    "scripts": [
      "highlights.js"
    ]
  },
  "manifest_version": 2
}
