{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stay Connected with Ms 5000 Watts",
  "description": "Opens the last 5 videos of the channel Ms 5000 Watts and when she's live on twitch",
  "version": "1.4",
  "browser_action": {
    "default_icon": "iconcon.png",
    "default_title": "Stay Connected with Ms 5000 Watts",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.youtube.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
