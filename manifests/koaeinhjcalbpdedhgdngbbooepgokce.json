  "name": "Investigate with Lacework",
  "description": "Faster, more accurate insight into the entire cyber kill chain",
  "version": "1.0.5",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "short_name": "Lacework Polygraph",
