{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "cageify.js"
    ]
  },
  "manifest_version": 2,
  "name": "Cageify",
  "description": "Let the One True God embrace your webpages with his graciousness!",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
