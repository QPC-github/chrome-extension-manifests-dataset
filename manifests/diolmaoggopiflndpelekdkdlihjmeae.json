{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YahooMail e GMail Block Publicidade",
  "short_name": "Yahoo Mail e GMail sem publicidades",
  "version": "1.4",
  "description": "Removedor de publicidades do Yahoo Mail e GMail",
  "author": "Francisco R. Sobrinho",
  "homepage_url": "http://www.semx.com.br",
  "content_scripts": [
    {
      "matches": [
        "https://*.mail.yahoo.com/*",
        "https://mail.google.com/mail/*"
      ],
      "css": [
        "ajusta.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
