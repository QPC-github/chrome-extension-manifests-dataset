{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "contextmenu-source",
  "version": "0.7",
  "manifest_version": 2,
  "icons": {
    "16": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/src/content_scripts.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "/lib/micro-location.js",
      "/src/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus"
  ]
}
