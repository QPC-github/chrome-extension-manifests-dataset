{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easylife.com - System Analyzer",
  "short_name": "System Analyzer",
  "description": "Analyse your system performance in realtime",
  "version": "1.0.0.1",
  "version_name": "1.0",
  "default_locale": "en",
  "icons": {
    "16": "assets/icon_16.png",
    "36": "assets/icon_36.png",
    "48": "assets/icon_48.png",
    "78": "assets/icon_78.png",
    "96": "assets/icon_96.png",
    "128": "assets/icon_128.png",
    "144": "assets/icon_144.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "system.cpu",
    "system.display",
    "system.memory",
    "system.network",
    "system.storage"
  ],
  "externally_connectable": {
    "matches": [
      "*://easylife.com/*"
    ]
  }
}
