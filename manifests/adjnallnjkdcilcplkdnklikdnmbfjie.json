{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_specific_settings": {
    "gecko": {
      "id": "ao3@erenrich.net",
      "strict_min_version": "42.0"
    }
  },
  "manifest_version": 2,
  "name": "AO3 history",
  "version": "0.2",
  "description": "Highlights which fics you've read",
  "content_scripts": [
    {
      "matches": [
        "*://*.archiveofourown.org/*"
      ],
      "js": [
        "history.js"
      ]
    }
  ],
  "permissions": [
    "unlimitedStorage",
    "*://*.archiveofourown.org/*",
    "storage"
  ]
}
