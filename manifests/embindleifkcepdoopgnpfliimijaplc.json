{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Right Click Search Media",
  "version": "0.2",
  "description": "right click to search media on youtube, soundcloud, zippytune or wherever you want",
  "icons": {
    "16": "16.png",
    "128": "128.png"
