{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Radio Head Lite - רדיו ישראלי",
  "short_name": "Radio Head Lite",
  "description": "הקיצור שלכם לשמיעת תחנות רדיו ישראלי בשידור חי - גירסת לייט.",
  "version": "1.31",
  "browser_action": {
    "name": "Radio Head",
    "default_popup": "app.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "app.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
