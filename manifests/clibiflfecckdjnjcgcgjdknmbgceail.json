{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Helper Tools for Instagram v2",
  "short_name": "Helper For Insta",
  "description": "Create list of followers/following of an Instagram account, find common users of 2 IG accounts, calculate likes/comments on profile",
  "version": "2.1.39",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "page_action": {
    "default_icon": "img/icon19.png",
    "default_title": "Helper Tools for Instagram.com",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "19": "img/icon19.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "js/env.js",
      "js/PromiseChrome.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "identity",
    "identity.email",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "https://www.instagram.com/*"
  ],
  "incognito": "split"
}
