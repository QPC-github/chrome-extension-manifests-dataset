{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pokemon Tabs!",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "What Pokemon will you find on your browser journeys?",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "<all_urls>"
  ]
}
