{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ariba BaseID Webjumper",
  "short_name": "jumpToBaseId",
  "author": "Renars Rozitis",
  "version": "1.2",
  "description": "Attempt to web-jump to an object on the current SAP Ariba realm by the object's database BaseID",
  "icons": {
    "128": "server.png",
    "64": "server.png",
    "48": "server.png",
    "32": "server.png",
    "24": "server.png",
    "16": "server.png"
  },
  "background": {
    "persisten": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "32": "server.png"
    },
    "default_title": "Ariba BaseID Webjumper"
  },
  "omnibox": {
    "keyword": "oid"
  }
}
