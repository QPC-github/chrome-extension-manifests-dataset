{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Adds functionality for persisting certain configuration selections on http://jsfiddle.net",
  "name": "Fiddle Config",
  "permissions": [
    "*://*.jsfiddle.net/"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.jsfiddle.net/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "0.0.2.4"
}
