{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "RS TaskGroup Browser Extension",
  "version": "1.1.0",
  "description": "RS TaskGroup Browser Extension.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
