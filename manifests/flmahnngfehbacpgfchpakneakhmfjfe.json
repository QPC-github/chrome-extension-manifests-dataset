{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.1.1",
  "name": "Join Images",
  "description": "Join multiple image files together",
  "icons": {
    "1024": "res/icons/icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "res/icons/icon.png",
    "default_title": "Join Images"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  }
}
