{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SMZDM 简易搜索",
  "version": "0.1",
  "description": "简易搜索 SMZDM ，查询历史优惠",
  "options_page": "about.html",
  "author": "Shrugged",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "permissions": [
    "contextMenus",
    "https://*.smzdm.com/*",
    "http://*.smzdm.com/*"
  ],
  "browser_action": {
    "default_title": "SMZDM 简易搜索",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "about.html"
  }
}
