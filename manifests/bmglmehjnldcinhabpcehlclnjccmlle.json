{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GCal Column Colours",
  "description": "Customise the background colours of columns in your google calendar.",
  "version": "0.1",
  "permissions": [
    "https://www.google.com/calendar/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/calendar/*"
      ],
      "css": [
        "gccc_x.css"
      ],
      "js": [
        "gccc_x.js"
      ]
    }
  ]
}
