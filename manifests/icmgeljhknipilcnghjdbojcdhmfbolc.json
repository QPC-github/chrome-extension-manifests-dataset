{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "中国数字时代 Chrome Extension",
  "version": "0.2.4",
  "description": "Launching the Chinese section of China Digital Times (中国数字时代) using Google Chrome extension",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "中国数字时代",
    "default_icon": "icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}
