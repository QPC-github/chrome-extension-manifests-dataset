{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "name": "__MSG_name__",
  "short_name": "shoppingsuggestions",
  "description": "__MSG_description__",
  "author": "Shopping Suggestions",
  "default_locale": "en",
  "content_security_policy": "script-src 'self' https://*.ciuvo.com https://www.google-analytics.com; object-src 'self'",
  "icons": {
    "128": "images/getthemall.main_128.png",
    "16": "images/getthemall.main_16.png",
    "24": "images/getthemall.main_24.png",
    "32": "images/getthemall.main_32.png",
    "48": "images/getthemall.main_48.png",
    "64": "images/getthemall.main_64.png"
  },
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "version": "7.0.1",
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://icon.shopping-suggestions.com/favicons/ss/favicon.ico",
      "is_default": true,
      "keyword": "Shopping Suggestions",
      "name": "Web ",
      "search_url": "https://search.shopping-suggestions.com/query?q={searchTerms}",
      "suggest_url": "https://search.shopping-suggestions.com/suggestion?q={searchTerms}"
    }
  },
  "web_accessible_resources": [
    "images/*",
    "/modules/shop-suggestions/public/*"
  ]
}
