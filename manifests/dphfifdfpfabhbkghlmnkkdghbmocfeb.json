{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "XPath Generator",
  "version": "1.1.0",
  "description": "Parse the html DOM to determine the XPATH of elements",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "declarativeContent",
    "storage",
    "clipboardWrite",
    "contextMenus"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "devtools_page": "devtools/devtools.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "manifest_version": 2
}
