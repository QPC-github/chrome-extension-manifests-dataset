{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Talkdesk Callbar",
  "description": "Experience the Talkdesk Callbar outside of a webpage!",
  "minimum_chrome_version": "51",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.bundle.js"
      ]
    }
  },
  "icons": {
    "16": "img/16.png",
    "19": "img/19.png",
    "38": "img/38.png",
    "48": "img/48.png",
    "128": "img/128.png",
    "256": "img/256.png"
  },
  "permissions": [
    "alwaysOnTopWindows",
    "audioCapture",
    "storage",
    "notifications",
    "browser",
    "identity",
    "https://*.talkdeskid.com/*",
    "https://*.fullstory.com/*",
    "https://fullstory.com/*"
  ],
  "externally_connectable": {
    "ids": [
      "adjgicoimjhccdcjgepkmigomegpjplg"
    ],
    "matches": [
      "https://s3.amazonaws.com/td-p-azores-cti/*",
      "https://*.mytalkdesk.com/*"
    ]
  },
  "version": "1.33.15"
}
