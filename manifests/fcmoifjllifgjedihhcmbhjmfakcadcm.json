{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "manifest_version": 2,
  "short_name": "openinprivatem",
  "name": "Open in Private Mode",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus"
  ],
  "homepage_url": "http://mybrowseraddon.com/open-in-private-mode.html",
  "description": "Open the active tab in a private window via toolbar button or context-menu.",
  "commands": {
    "private-tab": {
      "description": "Open in Private Mode",
      "suggested_key": {
        "default": "Alt+Shift+S",
        "mac": "Alt+Shift+S"
      }
    }
  },
  "browser_action": {
    "default_title": "Open in Private Mode",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
