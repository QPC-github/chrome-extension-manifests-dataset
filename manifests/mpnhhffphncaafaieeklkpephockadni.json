{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "ApplicantPool",
  "version": "1.2.8",
  "description": "It's easy to add new hires into your payroll/hris system directly from this extension.",
  "homepage_url": "http://www.applicantpool.com",
  "author": "ApplicantPool",
  "icons": {
    "16": "icons/mpnhhffphncaafaieeklkpephockadni/16.png",
    "48": "icons/mpnhhffphncaafaieeklkpephockadni/48.png",
    "128": "icons/mpnhhffphncaafaieeklkpephockadni/128.png"
  },
  "browser_action": {
    "default_icon": "icons/mpnhhffphncaafaieeklkpephockadni/128.png",
    "default_title": "ApplicantPool",
    "default_popup": "browser_action/index.html"
  },
  "background": {
    "scripts": [
      "jquery/jquery-min.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "cookies",
    "webNavigation",
    "https://support-tree-207017.appspot.com/*",
    "https://admin.applicantpool.com/*"
  ],
  "web_accessible_resources": [
    "web_access/*",
    "browser_action/person.html",
    "images/header/*",
    "css/popin.css",
    "js/*"
  ],
  "optional_permissions": [
    "*://*/*"
  ]
}
