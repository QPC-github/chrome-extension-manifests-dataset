{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "gfx/icons/icon128.png",
    "default_popup": "search.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Search Warez-BB for what you need",
  "icons": {
    "16": "gfx/icons/icon16.png",
    "48": "gfx/icons/icon48.png",
    "128": "gfx/icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "Warez-BB Search",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.4.2"
}
