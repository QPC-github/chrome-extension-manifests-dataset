{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SledujuSerialy.cz",
  "description": "Doplněk vyhledává na serveru SledujuSerialy.cz, zobrazuje naposledy sledované epizody a umožňuje rychlé spuštění další epizody.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "version": "0.1.0",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "serial"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "*://*.sledujuserialy.cz/*"
  ]
}
