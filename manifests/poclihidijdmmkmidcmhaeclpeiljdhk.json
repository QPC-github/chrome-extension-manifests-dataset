{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Что нового? на MyPage.ru",
  "version": "1.3.1",
  "manifest_version": 2,
  "description": "Что нового? - расширение для Chrome, которое показывает, что нового произошло, пока вас не было на сайте MyPage.ru",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' https://mypage.ru; object-src 'self'",
  "browser_action": {
    "default_icon": "mypage.png",
    "default_title": "Что нового на MyPage?",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://mypage.ru/",
    "http://doranych.net/",
    "notifications",
    "background"
  ],
  "homepage_url": "http://doranych.mypage.ru/knopka_chto_novogo.html"
}
