{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UltraFlyer.com Airline News",
  "description": "UltraFlyer.com airline news, travel news and aviation news delivered to your desktop.",
  "background": {
    "page": "background.html"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "/img/uf.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "*://*/*",
    "https://avatars.githubusercontent.com/*"
  ],
  "version": "0.1"
}
