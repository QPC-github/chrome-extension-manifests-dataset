{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "E-Board 365",
  "short_name": "E-Board365",
  "description": "A web-based event controller allowing you to control events in a meeting room",
  "version": "1.0.8",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "E-BOARD 365 icon_white_16x16.png",
    "32": "E-BOARD 365 icon_white_32x32.png",
    "128": "E-BOARD 365 icon_white_128x128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js",
        "jquery.js"
      ]
    }
  },
  "commands": {
    "HomePage": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "Edit Devices",
      "global": true
    }
  },
  "permissions": [
    "tabs",
    "browser",
    "storage",
    "videoCapture",
    "webview",
    "audio",
    "audioCapture",
    "desktopCapture",
    "http://*/*",
    "https://*/*",
    "fullscreen",
    "overrideEscFullscreen",
    "unlimitedStorage",
    "debugger"
  ],
  "externally_connectable": {
    "matches": [
      "*://voip.imeeting365.com/*",
      "*://e-board365.com/*",
      "*://eboard365.blob.core.windows.net/*",
      "*://imeeting365.com/*"
    ]
  }
}
