{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "스쿱 컬렉션 툴",
  "description": "웹서핑 하다 마음에 드는 이미지를 바로 스쿱 컬렉션에 추가하기",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "downloads"
  ]
}
