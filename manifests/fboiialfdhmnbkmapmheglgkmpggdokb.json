{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Exs Dark Theme",
  "description": "Applies custom styling for gaming community exs.lv",
  "version": "4.4",
  "manifest_version": 2,
  "web_accessible_resources": [
    "js/analytics.js",
    "css/style.css",
    "img/menu.png",
    "img/spoiler.png",
    "img/url.png",
    "img/img.png",
    "img/pepsi.png"
  ],
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/main.js"
      ],
      "matches": [
        "https://exs.lv/*"
      ],
      "run_at": "document_start"
    }
  ],
  "converted_from_user_script": true,
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "storage"
  ]
}
