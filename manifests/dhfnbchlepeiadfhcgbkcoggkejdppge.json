{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Sitebuilder Forums",
  "version": "1.3",
  "manifest_version": 2,
  "description": "__MSG_sitebuilderforums_description__",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/icon32.png",
    "default_title": "Sitebuilder Forums"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "notifications",
    "*://*.warwick.ac.uk/sitebuilder2/user/forums/"
  ],
  "options_page": "options.html"
}
