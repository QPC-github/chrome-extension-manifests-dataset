{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sleuth",
  "description": "Sleuth Speed Test for Chrome",
  "version": "0.0.1.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "sleuth-favicon-02.ico",
    "default_title": "Sleuth",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [],
      "js": [
        "content.js"
      ]
    }
  ]
}
