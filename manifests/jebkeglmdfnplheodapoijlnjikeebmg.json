{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Experimental version.Instantly redirect you to video, no ads",
  "icons": {
    "48": "icos/48.png",
    "128": "icos/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "name": "Video Only",
  "permissions": [
    "<all_urls>"
  ],
  "version": "0.1.7",
  "web_accessible_resources": [
    "*"
  ]
}
