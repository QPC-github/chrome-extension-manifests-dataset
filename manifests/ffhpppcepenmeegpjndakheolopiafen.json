{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "manifest_version": 2,
  "name": "FigLeaf",
  "version": "1.19.3",
  "description": "Protect your identity with the smartest privacy solution ever",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "js/background-chrome.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "",
    "default_icon": {
      "16": "images/icon-16.png",
      "24": "images/icon-24.png",
      "32": "images/icon-32.png"
    },
    "default_title": "FigLeaf"
  },
  "permissions": [
    "privacy",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "activeTab",
    "file:///*",
    "nativeMessaging",
    "<all_urls>",
    "storage"
  ],
  "incognito": "split",
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg",
    "fonts/*.woff",
    "videos/*.mp4",
    "_locales/**/*.json"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/content/style.css"
      ],
      "js": [
        "js/content-chrome.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; font-src * ",
  "default_locale": "en"
}
