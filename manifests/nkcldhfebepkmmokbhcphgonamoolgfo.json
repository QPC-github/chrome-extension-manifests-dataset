{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reading Time",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Add a reading time indicator to Readability",
  "content_scripts": [
    {
      "matches": [
        "http://www.readability.com/*",
        "https://www.readability.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
