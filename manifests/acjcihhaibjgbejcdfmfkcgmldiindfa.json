{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UseClark",
  "description": "UseClark improves your on-screen reading experience to help you read faster, stay focused, and remember more than ever before.",
  "version": "0.5.3",
  "key": "",
  "permissions": [
    "identity",
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "oauth2": {
    "client_id": "665036908500-hao3vsnj5onhbl1asg0nj7emo6j1115i.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "uc-logo-16.png",
    "48": "uc-logo-48.png",
    "128": "uc-logo-128.png"
  },
  "browser_action": {
    "default_title": "UseClark",
    "default_icon": "uc-logo-48.png"
  },
  "content_scripts": [
    {
      "js": [
        "bundle.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "manifest_version": 2
}
