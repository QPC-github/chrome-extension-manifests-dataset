{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Website Templates - Web Template Monster",
  "description": "Create your website with a template!",
  "version": "1.2.1",
  "icons": {
    "16": "a16.png",
    "32": "a32.png",
    "48": "a48.png",
    "128": "a128.png"
  },
  "app": {
    "urls": [
      "http://seo-storm.org/gtmstore"
    ],
    "launch": {
      "web_url": "http://seo-storm.org/gtmstore"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "http://store.templatemonster.com/*"
  ]
}
