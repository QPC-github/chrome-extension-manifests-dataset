{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tactical Gaming",
  "version": "0.99",
  "manifest_version": 2,
  "description": "TG Battlelog Enhancements & Game Launcher. Simply load this plugin and go to http://tacticalg.com/server1/",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": [
    "*://battlelog.battlefield.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://battlelog.battlefield.com/*"
      ],
      "js": [
        "tg.js"
      ]
    }
  ]
}
