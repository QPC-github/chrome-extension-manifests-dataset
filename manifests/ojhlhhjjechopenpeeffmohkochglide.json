{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Manager",
  "version": "1.5",
  "description": "One click to close inactive, non-pinned Google Chrome tabs",
  "author": "Alex Baron",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
