{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Docswave",
  "short_name": "Docswave",
  "description": "Go to Docswave service",
  "default_locale": "en",
  "version": "4.2",
  "icons": {
    "24": "icon-24.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "browser_action": {
    "default_icon": "icon-24.png",
    "default_title": "Go to Docswave!"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "open-docswave": {
      "description": "Open the Docswave!",
      "suggested_key": {
        "default": "Ctrl+Space"
      }
    }
  }
}
