{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "zyKiller",
  "description": "This plugin solves your zyBooks woes",
  "version": "0.1.1",
  "author": "Anoxic and SouICry",
  "icons": {
    "19": "icon.png"
  },
  "background": {
    "scripts": [
      "zyKiller.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.zyante.com/",
    "*://*.zybooks.com/"
  ],
  "web_accessible_resources": [
    "zyBetterAnimator.js",
    "zyKillerApplication.js",
    "defnMatch.js"
  ]
}
