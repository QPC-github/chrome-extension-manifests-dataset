{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide Videos Section from search",
  "description": "Instantly removes 'Videos' section from every search you make.",
  "version": "1.1",
  "author": "Haider Sattar",
  "browser_action": {
    "default_icon": "vidblock.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "storage"
  ]
}
