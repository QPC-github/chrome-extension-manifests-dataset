{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mark.xtac.net bookmarks manager",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Send title and URL of current page to mark.xtac.net",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "browser_action": {
    "default_title": "Save current URL into mark.xtac.net",
    "default_icon": "icon.png"
  }
}
