{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Manager Plus",
  "version": "0.9.9.6",
  "description": "Search bookmarks and do whatever you can imagine.",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icon/icon128.png",
    "48": "icon/icon128.png",
    "64": "icon/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
    "default_icon": "icon/icon128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage",
    "chrome://favicon/*"
  ]
}
