{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Property Listing Analyzer",
  "short_name": "Listing Analyzer",
  "description": "This extension examines Redfin listing pages and shows a pop-up with a deal analysis, including cash-on-cash return.",
  "version": "1.0.1",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_name": "Analyze Deal",
    "default_icon": "icons/icon19.png",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
