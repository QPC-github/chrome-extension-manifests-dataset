{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab Slim",
  "short_name": "nTabSlim",
  "version": "0.2",
  "author": "James Paul",
  "description": "Replace the new tab with a list of your frequent sites.",
  "icons": {
    "128": "nTabSlim128.png"
  },
  "permissions": [
    "topSites"
  ],
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "nTabSlim.html"
  }
}
