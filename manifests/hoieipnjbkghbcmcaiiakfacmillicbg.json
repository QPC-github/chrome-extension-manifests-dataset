{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "license_manager.js",
      "background.js"
    ]
  },
  "description": "User interface enhancements for Kartra authors. Not affilated with, endorsed, or supported by Kartra or Genesis Digital.",
  "icons": {
    "16": "tfk-16.png",
    "32": "tfk-32.png",
    "48": "tfk-48.png",
    "64": "tfk-64.png",
    "128": "tfk-128.png"
  },
  "manifest_version": 2,
  "name": "KCSG Tools For Kartra",
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "16": "tfk-16.png",
      "32": "tfk-32.png",
      "48": "tfk-48.png",
      "64": "tfk-64.png",
      "128": "tfk-128.png"
    },
    "default_popup": "popup.html",
    "default_title": "KCSG Tools For Kartra"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "webNavigation",
    "https://app.kartra.com/"
  ],
  "short_name": "KCSG TFK",
  "version": "2.1.2",
  "web_accessible_resources": [
    "run_in_tab.js",
    "run_in_tab-pro.js"
  ]
}
