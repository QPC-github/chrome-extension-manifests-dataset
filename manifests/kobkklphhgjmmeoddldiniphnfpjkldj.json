{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Redirector",
  "version": "1.17.87",
  "description": "When typing in the address bar a twitter tag (@Hexacircle), it will bring you right to the twitter page.",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "512": "icon.png"
  },
  "browser_action": {},
  "author": "FireController#1847",
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ]
    }
  ]
}
