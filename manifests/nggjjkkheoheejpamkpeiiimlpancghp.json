{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "SCode",
  "description": "A revolutionary markup language implementable almost everywhere on the web.",
  "version": "0.4",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
