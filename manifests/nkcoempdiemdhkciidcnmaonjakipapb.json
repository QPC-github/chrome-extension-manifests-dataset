{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "First Draft GIS",
  "short_name": "FDGIS",
  "description": "Saves names and coordinates of places mentioned on web pages that you visit",
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128x128.png"
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage",
    "storage",
    "downloads",
    "tabs",
    "<all_urls>"
  ]
}
