{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Most Viewed",
  "description": "Removes the Most Viewed from the New Tab page",
  "author": "Rampaigh",
  "version": "1.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/Rampaigh/Remove-Most-Viewed",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "remove.js"
      ]
    }
  ]
}
