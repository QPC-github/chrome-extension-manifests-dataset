{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "version": "1.0",
  "icons": {
    "128": "media/ico-128.png",
    "48": "media/ico-48.png",
    "16": "media/ico-16.png"
  },
  "default_locale": "en",
  "homepage_url": "http://www.majkel.org/",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "storage",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "media/ico-48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/main.js"
    ],
    "persistent": true
  }
}
