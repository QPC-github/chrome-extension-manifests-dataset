{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ポータルアシスト",
  "version": "2.0.0",
  "description": "各ポータルサービスを簡単、便利にスタートするパネルを提供します。",
  "browser_action": {
    "default_icon": "images/at128.png",
    "default_title": "ポータルアシスト",
    "default_popup": "index.html"
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "128": "images/at128.png",
    "256": "images/at256.png"
  }
}
