{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "html/options.html",
  "permissions": [
    "storage",
    "webRequest"
  ],
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "images/*"
  ],
  "name": "v-read",
  "version": "0.3.6",
  "description": ""
}
