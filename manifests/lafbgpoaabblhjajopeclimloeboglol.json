{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chat-Net",
  "version": "1.0.4",
  "description": "Provides a simple chat network that follows you to any web page.",
  "homepage_url": "http://bengrosser.com/",
  "manifest_version": 2,
  "icons": {
    "16": "icon3.png",
    "48": "icon2.png",
    "128": "icon1.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "Chat-Net.user.js"
      ]
    }
  ]
}
