{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Power Search",
  "description": "Search sites without loading their search pages.\n\nConfigured via JSON, you can find a sample/default config in the extension option.",
  "version": "1.5",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_popup": "main.html"
  },
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png"
  },
  "permissions": [
    "debugger",
    "storage"
  ]
}
