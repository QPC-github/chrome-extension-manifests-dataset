{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Xerox Mobile Print Portal",
  "short_name": "Xerox Mobile Print Portal",
  "description": "",
  "version": "1.2.12",
  "minimum_chrome_version": "44",
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery/jquery-1.11.3.js",
      "jquery/angular.min.js",
      "lib/commlib.js",
      "lib/xrxlocale.js",
      "lib/mobileprint.js"
    ]
  },
  "browser_action": {
    "default_title": "Xerox Mobile Print Portal",
    "default_icon": "assets/App_128.png"
