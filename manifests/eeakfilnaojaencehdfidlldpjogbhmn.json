  "permissions": [
    "printerProvider",
    "storage",
    "downloads",
    "contextMenus",
    "https://*/*",
    "notifications",
    "tabs",
    "identity",
    "identity.email"
  ],
  "oauth2": {
    "client_id": "261413447492-0fjpluegdia0tgog663fkcf7fifs3laq.apps.googleusercontent.com",
