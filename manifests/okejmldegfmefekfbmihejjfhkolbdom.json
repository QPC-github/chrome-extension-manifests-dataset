{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Global Satellite Messenger",
  "short_name": "Satellite",
  "description": "Global Satellite Messenger is a multithreaded text messaging application for your Iridium satellite phone.",
  "version": "1",
  "manifest_version": 2,
  "permissions": [
    "serial",
    "notifications"
  ],
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "launch.js"
      ]
    }
  }
}
