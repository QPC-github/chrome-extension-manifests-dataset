{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "comScore Digital Analytix Campaign URLs",
  "description": "Digital Analytix Campaign URL Builder",
  "version": "1.0",
  "permissions": [
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/comscore-logo.png",
    "default_popup": "menu.html"
  },
  "background": {
    "js": "js/menu.js"
  }
}
