{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "default_locale": "zh_CN",
  "content_scripts": [
    {
      "matches": [
        "http://www.erep1.lindasc.tk/*/military/battlefield/*",
        "http://www.erep1.lindasc.tk/*/military/campaigns",
        "http://www.erep2.lindasc.tk/*/military/battlefield/*",
        "http://www.erep2.lindasc.tk/*/military/campaigns",
        "http://www.erepublik.com/*/military/battlefield/*",
        "http://www.erepublik.com/*/military/campaigns"
      ],
      "js": [
        "battle_status.js"
      ]
    }
  ],
  "name": "erepbulik battle status",
  "version": "1.2.4"
}
