{
  "background": "popup.html",
  "name": "Visual Bookmarks - All My Faves",
  "manifest_version": 2,
  "version": "1.4.1",
  "description": "The fastest and easiest way to visually bookmark your favorites while surfing the Web.",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "https://www.allmyfaves.com/"
  ]
}
