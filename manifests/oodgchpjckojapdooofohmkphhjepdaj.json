{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "author": "Interush, Inc.",
  "version": "2.15",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "homepage_url": "https://meet3.iris-one.com/",
