{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alt Text: Engaged",
  "version": "0.1.1",
  "manifest_version": 2,
  "icons": {
    "128": "assets/img/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content/main.js"
      ],
      "css": [
        "css/main.css"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "js/*"
  ]
}
