{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/checkInstalled.js"
      ],
      "matches": [
        "*://cloud.localpeers.com/*",
        "*://live.localpeers.com/*",
        "*://www.vip.localpeers.com/*"
      ]
    }
  ],
  "description": "LocalTV Mod",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "LocalTV Mod",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "20.0"
}
