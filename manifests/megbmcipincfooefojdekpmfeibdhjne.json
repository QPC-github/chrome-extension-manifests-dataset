{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scrollable Suggestions for YouTubeâ„¢",
  "version": "0.9",
  "description": "__MSG_app_description__",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch*",
        "https://www.youtube.com/watch*"
      ],
      "css": [
        "css/main.css"
      ],
      "js": [
        "scripts/jquery-1.4.2.min.js",
        "scripts/main.js"
      ]
    }
  ],
  "permissions": [
    "http://www.youtube.com/watch*",
    "https://www.youtube.com/watch*"
  ],
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png"
  }
}
