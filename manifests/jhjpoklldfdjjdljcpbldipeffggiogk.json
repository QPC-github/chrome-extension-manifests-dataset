{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Show Names in Facebook Chat",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Show names above messages in Facebook chat dialogs.",
  "content_scripts": [
    {
