{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dev TOols",
  "version": "1.0.1.5",
  "permissions": [
    "management",
    "contextMenus",
    "clipboardWrite",
    "clipboardRead"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_title": "Dev Tools",
    "default_popup": "popup.html"
  }
}
