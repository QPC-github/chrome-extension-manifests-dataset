{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' ; object-src 'self' ",
  "name": "NBASharer",
  "version": "1.1",
  "description": "I want to watch NBA. Share a NBA Account to your friends. Made by Hall8. ",
  "icons": {
    "32": "NBA_ICO.png",
    "48": "NBA_ICO.png",
    "128": "NBA_ICO.png"
  },
  "browser_action": {
    "default_title": "I want to watch NBA",
    "default_icon": "NBA_ICO.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "browsingData",
    "*://*.nba.com/",
    "*://watch.nba.com/*",
    "*://localhost/*",
    "*://*.vr95.com/*"
  ]
}
