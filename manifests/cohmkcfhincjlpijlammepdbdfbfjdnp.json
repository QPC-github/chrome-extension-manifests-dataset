{
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Super Mario"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "The Classic Super Mario Game Is Now Available On Your Chrome - Play Super Mario Bros As Much As You Want!",
  "name": "Super Mario",
  "manifest_version": 2,
  "permissions": [
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>",
    "tabs",
    "webNavigation"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'  https://www.google-analytics.com https://ssl.google-analytics.com https://pix.2048-game.review;  object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.2"
}
