{
  "browser_action": {
    "default_icon": "igropoisk.png",
    "default_popup": "popup.html",
    "default_title": "igropoisk.com"
  },
  "content_scripts": [
    {
      "js": [
        "popup.js"
      ],
      "matches": [
        "http://igropoisk.com/*"
      ]
    }
  ],
  "description": "Open in new tab site Igropoisk.com",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "Интернет защитник",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
