{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Xennials to People of Light",
  "short_name": "Snake People",
  "description": "Replaces the text 'Xennials' with 'People of Light'. Fork of MtSPP by Steven Tan, a fork of the original MtSP v1.7.",
  "author": "Steven Tan",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
