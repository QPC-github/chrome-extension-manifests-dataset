{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "/js/jquery-2.1.3.min.js",
        "/js/include.js"
      ],
      "matches": [
        "http://*.svadba.com/chat/*",
        "http://*.top-dates.net/*",
        "http://*.svadba.com/Login/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Automatic send messages on Svadba.com",
  "short_name": "QSB",
  "icons": {
    "128": "img/logo_128.png"
  },
  "permissions": [
    "tabs",
    "http://*.top-dates.net.ua/*",
    "http://*.top-dates.net/*",
    "https://*.anastasiadate.com/*",
    "http://*.qsb.com.ua/*",
    "http://*.svadba.com/*",
    "http://*.jquery.com/*",
    "windows",
    "storage",
    "unlimitedStorage"
  ],
  "manifest_version": 2,
  "name": "QSB - Quick Simple Bot",
  "version": "1.13"
}
