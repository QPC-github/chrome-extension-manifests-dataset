{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stoic",
  "version": "4.3",
  "description": "Replaces new tab page with a dark, minimalist, theme accompanied by stoic quotes",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "img/quotation-mark.png"
  },
  "permissions": [
    "activeTab"
  ]
}
