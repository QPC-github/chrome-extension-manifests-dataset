{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bbCodeInsert",
  "version": "0.3.2",
  "description": "Insert BBCode, HTML, Markdown and Goodreads tags. Use selected text and prompts",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "bbcode-016.png",
    "48": "bbcode-048.png",
    "128": "bbcode-128.png"
  },
  "manifest_version": 2
}
