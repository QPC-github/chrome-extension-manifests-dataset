{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "short_name": "__MSG_appSortName__",
  "description": "__MSG_appDesc__",
  "version": "1.2",
  "default_locale": "en",
  "content_scripts": [
    {
      "js": [
        "nolike.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "128.png",
    "16": "128.png",
    "48": "128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://*.facebook.com/*",
    "https://*.facebook.com/*"
  ]
}
