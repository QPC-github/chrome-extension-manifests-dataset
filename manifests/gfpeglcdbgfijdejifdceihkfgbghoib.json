{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LuckysLock Chrome Extension",
  "description": "Permet de facilement savoir quand LuckysLock est en stream",
  "version": "1.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "off.png"
  },
  "background": {
    "scripts": [
      "jquery-3.2.0.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "notifications",
    "https://static-cdn.jtvnw.net/*"
  ]
}
