{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Heatclix RankChecker",
  "short_name": "Heatclix RankChecker",
  "description": "Analysieren Sie Ihre Website oder die Websites der Konkurrenz in Sekunden",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "app.js",
      "background.js"
    ]
  }
}
