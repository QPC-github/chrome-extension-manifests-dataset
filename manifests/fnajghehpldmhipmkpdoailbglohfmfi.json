{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Palmer Search Launcher",
  "description": "Quickly Launch Websites",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "Tps search.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "Tps search.html"
  },
  "optional_permissions": [
    "tabs",
    "https://palmerleejay.wixsite.com/thepalmerstudioltd"
  ]
}
