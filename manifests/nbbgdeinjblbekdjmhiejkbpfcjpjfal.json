{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ESC Youtube AD",
  "version": "1.0.1",
  "description": "__MSG_appDesc__",
  "default_locale": "ja",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
