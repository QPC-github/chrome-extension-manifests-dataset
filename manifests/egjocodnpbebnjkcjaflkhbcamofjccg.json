{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sexy Go to Top",
  "version": "1.0",
  "description": "Scrolls page to top",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "64": "assets/icon-64.png",
    "128": "assets/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "/js/jquery.js",
        "/js/main.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
