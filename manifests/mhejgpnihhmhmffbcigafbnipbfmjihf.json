{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Webnarius meet1 Streamer",
  "description": "Compartilhe sua tela no webnarius!!!.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "wb-logo-48x48.png",
    "48": "wb-logo-48x48.png",
    "128": "wb-logo-48x48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet1.webnarius.com.br/*"
    ]
  }
}
