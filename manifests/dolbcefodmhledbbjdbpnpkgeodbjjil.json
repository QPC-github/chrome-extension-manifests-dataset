{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cheap Hotels in Australia",
  "manifest_version": 2,
  "short_name": "Cheap Hotels Australia",
  "description": "Compare and Book every hotels deal available online in one search! ",
  "version": "2.1",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "browser_action": {
    "default_icon": "au.png",
    "default_title": "Cheap Hotels Australia",
    "default_popup": "chromiflash.html"
  },
  "icons": {
    "128": "au3.png"
  }
}
