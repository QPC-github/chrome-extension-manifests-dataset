{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bajar de Goear, 2016!!",
  "version": "3.2.0",
  "manifest_version": 2,
  "description": "Bajar musica desde Goear, Actualizado v3.2.0",
  "icons": {
    "256": "256.png"
  },
  "background": {
    "scripts": [
      "cls.js"
    ],
    "persistent": false
  },
  "permissions": [
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.goear.com/*",
        "http://*.goear.com/*"
      ],
      "js": [
        "jquery.js",
        "cls.js"
      ]
    }
  ]
}
