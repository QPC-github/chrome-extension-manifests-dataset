{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IML Debugger",
  "short_name": "IML Debugger",
  "description": "IML Debugger",
  "version": "0.1.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_title": "IML",
    "default_popup": "popup.html"
  },
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "background.js",
      "popup.js"
    ]
  }
}
