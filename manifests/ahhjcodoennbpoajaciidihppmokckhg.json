{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Anti-misfollowing",
  "description": "現在フォローしている人のフォローボタンを見えなくする。（twitter.com / tweetdeck）",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://tweetdeck.twitter.com/"
      ],
      "css": [
        "index.css"
      ]
    }
  ],
  "homepage_url": "https://github.com/SnO2WMaN/anti-misfollowing",
  "icons": {
    "128": "./icon.png"
  }
}
