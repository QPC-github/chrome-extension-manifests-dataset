{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FlexEnvironment",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Save lists of sites to open up for certain environments.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "FlexEnvironment",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ]
}
