{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lipsumizer",
  "version": "2.9.22",
  "manifest_version": 2,
  "description": "Replaces text on a webpage with Lorem Ipsum text",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "permissions": [
    "tabs",
    "downloads",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "motherboard.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Lipsumize!",
    "default_icon": "images/icon_19.png"
  },
  "options_page": "options.html"
}
