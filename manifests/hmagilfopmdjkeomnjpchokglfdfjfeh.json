{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FireLogger for Chrome",
  "version": "1.0.1",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "minimum_chrome_version": "31.0",
  "description": "A sexy server logger console",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
