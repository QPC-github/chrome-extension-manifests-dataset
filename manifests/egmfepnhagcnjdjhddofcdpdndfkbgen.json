{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "X-Note",
  "description": "This extension by Xceptive Solutions LLP",
  "version": "5.23.5",
  "background": {
    "scripts": [
      "js.cookie.js",
      "jquery.min.js",
      "notification.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "128": "icon/128.png"
    }
  },
  "permissions": [
    "cookies",
    "<all_urls>",
    "storage",
    "tabs",
    "alarms"
  ]
}
