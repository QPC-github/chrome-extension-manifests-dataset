{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LinkedIn to HyperTeam CRM",
  "description": "Read profile informations from LinkedIn and import them to the HyperTeam CRM.",
  "version": "1.3.6",
  "browser_action": {
    "default_icon": "Content/hticon.png",
    "default_popup": "Components/Default/addonmaster.html",
    "default_title": "LinkedIn to HyperTeam CRM"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "http://*/",
    "https://*/",
    "storage"
  ]
}
