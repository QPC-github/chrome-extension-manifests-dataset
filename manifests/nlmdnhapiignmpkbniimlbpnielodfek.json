{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.3.1",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  },
  "web_accessible_resources": [
    "delay/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "delayedBrowsing.js"
      ]
    }
  ]
}
