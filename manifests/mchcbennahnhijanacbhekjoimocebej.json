{
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_popup": "popup.html",
    "default_title": "FBCH is ACTIVE!."
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "mggfs.css"
      ],
      "js": [
        "jquery.js",
        "resize.js"
      ],
      "matches": [
        "*://www.facebook.com/*"
      ]
    }
  ],
  "description": "Hide or show the right-side column",
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "Right Side Column Hide",
  "permissions": [
    "http://www.facebook.com/*",
    "https://www.facebook.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
