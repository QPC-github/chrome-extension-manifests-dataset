{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yournet - Free VPN Proxy",
  "short_name": "Yournet",
  "description": "ournet is a free proxy service which unblocks your favourite websites. Access news, social media, internet radio, videos & music",
  "version": "2.0.2",
  "key": "",
  "browser_action": {
    "default_title": "Yournet",
    "default_icon": "data/static/icons/icon_128.png",
    "default_popup": "data/popup/index.html"
  },
  "background": {
    "scripts": [
      "src/application.module.js"
    ]
  },
  "icons": {
    "16": "data/static/icons/icon_16.png",
    "24": "data/static/icons/icon_24.png",
    "48": "data/static/icons/icon_48.png",
    "128": "data/static/icons/icon_128.png"
  },
  "homepage_url": "https://yournet.io/",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "*://*/",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "*://*.yournet.io/*"
    ],
    "accepts_tls_channel_id": false
  }
}
