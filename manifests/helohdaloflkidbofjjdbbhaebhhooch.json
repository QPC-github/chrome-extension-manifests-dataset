{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Condom, Rhythm Methods(Menstrual Calendar)",
  "description": "No Condom, Rhythm Methods(Menstrual Calendar). Show the women's ovulation date, rhythm methods contraception.",
  "version": "2.0",
  "manifest_version": 2,
  "homepage_url": "http://uuware.com/",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "32": "icon-small.png",
    "48": "icon_48.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon-small.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ]
}
