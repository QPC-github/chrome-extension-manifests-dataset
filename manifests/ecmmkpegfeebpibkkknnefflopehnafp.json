{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bubble Fox",
  "manifest_version": 2,
  "description": "Fox loves to shoot bubbles and clear the playing field of a large number of multi-colored balls",
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "version": "1",
  "browser_action": {},
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  }
}
