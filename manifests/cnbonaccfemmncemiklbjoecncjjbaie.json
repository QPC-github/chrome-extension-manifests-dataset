{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Webdreams Meeting",
  "description": "Permite compartir una pantalla del escritorio en Webdreams Meeting.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "webdreams-logo-16x16.png",
    "48": "webdreams-logo-48x48.png",
    "128": "webdreams-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meeting.wdreams.com/*"
    ]
  }
}
