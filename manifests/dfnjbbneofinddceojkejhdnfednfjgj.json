{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "View textual changes to BBC News articles.",
  "name": "BeebDiff",
  "permissions": [
    "https://bbcworker.herokuapp.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.bbc.co.uk/news/*"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "showDiff.js"
      ],
      "css": [
        "beebdiff.css"
      ]
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://bbcworker.herokuapp.com/; object-src 'self'",
  "version": "1.0.5"
}
