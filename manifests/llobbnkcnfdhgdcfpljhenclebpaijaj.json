{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Merch DMCA",
  "version": "1.0.5",
  "description": "Send an unlimited number of DMCA Takedown requests that we handle for you with just a few clicks.",
  "author": "Merch DMCA",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "img/icon.png",
    "64": "img/icon.png",
    "32": "img/icon.png",
    "16": "img/icon.png"
  },
  "incognito": "spanning",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Merch DMCA v1.0.4",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
