{
  "background": {
    "scripts": [
      "lib/mithril.min.js",
      "lib/stream.js",
      "js/config.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/flywheel-square-logo-128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Flywheel Momentus is an amazing e-commerce tool designed to help you do three things very, very well.",
  "homepage_url": "https://www.flywheelfulfillment.com",
  "icons": {
    "128": "images/flywheel-square-logo-128.png"
  },
  "manifest_version": 2,
  "name": "Momentus",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "background",
    "tabs",
    "*://momentus.flywheelfulfillment.com/*",
    "*://www.amazon.com/*",
    "https://www.google-analytics.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.17",
  "web_accessible_resources": [
    "images/flywheel-logo-128.png",
    "images/flywheel-logo-200.jpg",
    "images/flywheel.png",
    "images/flywheel-logo-stacked.png",
    "images/flywheel-logo-white.png",
    "dashboard.html"
  ]
}
