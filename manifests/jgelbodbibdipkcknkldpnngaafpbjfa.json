{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SathyLive",
  "author": "Vase_ectomie/vyxeny",
  "description": "Description de OUF pour des lives de OUF",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "icons": {
    "256": "icons/logo256.png",
    "128": "icons/logo128.png",
    "64": "icons/logo64.png",
    "32": "icons/logo32.png"
  },
  "permissions": [
    "https://api.twitch.tv/sathynandreska",
    "notifications",
    "management",
    "tabs",
    "background"
  ],
  "browser_action": {
    "default_title": "Sathysfaction",
    "default_popup": "popup.html",
    "default_icon": "logo256.png"
  }
}
