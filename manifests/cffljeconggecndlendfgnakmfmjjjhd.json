{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OneFundraise",
  "short_name": "OneFundraise",
  "description": "It's your money, put it to work!",
  "version": "0.0.0.2",
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html",
    "default_title": "Put your money to work!"
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "webNavigation",
    "https://api.onefundraise.com/"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "fetch.js",
      "user.js",
      "notification.js",
      "background.js"
    ],
    "persistent": true
  }
}
