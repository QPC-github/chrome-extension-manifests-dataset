{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Amazonize (Alpha)",
  "description": "Find top-rated Amazon products on any page.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
