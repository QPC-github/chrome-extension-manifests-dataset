{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CryptoKit.ghbank Extension",
  "version": "3.2.0.2",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.ghbank.com.cn/*",
      "https://www.ghibank.com.cn/*",
      "https://www.ghbibank.com.cn/*",
      "http://10.1.86.144:7725/*",
      "http://10.1.86.145:7723/*",
      "http://10.1.86.116:8802/*",
      "http://10.1.86.172:8802/*",
      "http://10.1.86.119:8802/*",
      "http://127.0.0.1:8080/*"
    ]
  }
}
