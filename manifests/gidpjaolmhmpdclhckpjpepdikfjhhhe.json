{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keep Me Logged Out",
  "version": "0.2",
  "description": "Keeps you logged out by unchecking the button",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [],
      "run_at": "document_end",
      "all_frames": true,
      "js": [
        "script.js"
      ]
    }
  ]
}
