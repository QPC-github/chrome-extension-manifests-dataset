{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter Quick Share",
  "version": "1.1",
  "description": "Twitter Quick Share will allow you to quickly share any site to your Twitter stream. Great for sites without Twitter integration.",
  "icons": {
    "16": "t.png",
    "48": "t.png",
    "128": "t.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://www.twitter.com/"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "t.png",
    "default_title": "Twitter Quick Share",
    "default_popup": "options.html"
  }
}
