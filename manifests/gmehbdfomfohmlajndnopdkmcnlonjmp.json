{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Video Screenshot",
  "version": "1.1",
  "description": "Take a screenshot of current visible video area.",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Take screenshot",
    "default_popup": "popup.html"
  }
}
