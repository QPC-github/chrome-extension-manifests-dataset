{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screensharing for cyminar and webphone",
  "description": "Screensharing for cyminar and webphone",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/web*/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "desktopCapture"
  ]
}
