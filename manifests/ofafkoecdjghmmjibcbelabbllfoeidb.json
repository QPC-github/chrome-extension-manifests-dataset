{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Text Fix",
  "description": "A context menu item to make body text into a plain font.",
  "version": "3",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "textfix.js"
    ],
    "persistent": false
  }
}
