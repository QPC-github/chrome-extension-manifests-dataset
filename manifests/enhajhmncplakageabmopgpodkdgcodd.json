{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tap To Tab",
  "author": "Terry Yuen",
  "version": "0.5.5",
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "20": "openinnewtab_20.png",
    "48": "openinnewtab_48.png",
    "96": "openinnewtab_96.png",
    "128": "openinnewtab_128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}
