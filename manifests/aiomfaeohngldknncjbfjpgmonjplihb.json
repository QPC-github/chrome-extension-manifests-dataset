{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "description": "Esta extension se encarga de facitiliar el uso/administracion de sus cuentas de Yopmail.",
  "icons": {
    "48": "img/icon.png"
  },
  "manifest_version": 2,
  "name": "Yopmail Helper",
  "permissions": [
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
