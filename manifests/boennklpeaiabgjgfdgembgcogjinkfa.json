{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "mycss.css"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "myscript.js"
      ],
      "matches": [
        "*://*.trello.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Dashboard view for Trello - Fit more cards in one screen.",
  "manifest_version": 2,
  "name": "Trelldash",
  "permissions": [
    "*://*.trello.com/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
