{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Marvel Avengers New Tab",
  "description": "Set the marvel avengers wallpaper on every new tab. ",
  "version": "1.0.4",
  "icons": {
    "16": "3.png",
    "48": "1.png",
    "128": "2.png"
  },
  "chrome_url_overrides": {
    "newtab": "myPage.html"
  },
  "browser_action": {
    "default_icon": "3.png"
  },
  "permissions": [
    "activeTab"
  ]
}
