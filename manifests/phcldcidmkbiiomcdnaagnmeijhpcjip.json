{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Custom Bookmark Manager",
  "description": "Tool to access Bookmark Links.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "activeTab",
    "tabs"
  ]
}
