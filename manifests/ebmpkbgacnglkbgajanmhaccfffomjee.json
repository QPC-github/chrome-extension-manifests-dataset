{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "0.6.12",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "16": "img/plang-icon-16.png",
    "48": "img/plang-icon-48.png",
    "128": "img/plang-icon-128.png"
  },
  "default_locale": "en",
  "homepage_url": "https://www.myplang.com/",
  "background": {
    "scripts": [
      "content/platformhelpers.js",
      "content/common/constants.js",
      "content/common/logger.js",
      "content/common/helpers.js",
      "content/common/database.js",
      "content/common/updater.js",
      "content/common/model/coupon.js",
      "content/common/model/merchant.js",
      "content/common/model/data.js",
      "content/common/core.js",
      "content/common/libs/jquery-1.6.4.js",
      "content/common/libs/jquery.url.js",
      "content/platformoverlay.js",
      "content/common-webkit/overlay.js",
      "content/common/processors/socialvest.js",
      "content/common/processors/search.js",
      "content/common/processors/google.js",
      "content/common/processors/bing.js",
      "content/common/processors/yahoo.js",
      "content/common/processors/merchant.js"
    ]
  },
  "web_accessible_resources": [
    "options.html",
    "bars/*",
    "img/*",
    "_locales/*"
  ],
  "page_action": {
    "default_icon": "img/plang-status-unsupported.png"
  },
  "options_page": "options.html",
  "minimum_chrome_version": "30.0",
  "offline_enabled": false,
  "permissions": [
    "tabs",
    "notifications",
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}
