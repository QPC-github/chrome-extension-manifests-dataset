  "manifest_version": 2,
  "version": "2.0.0",
  "name": "Skiney - Theme forum Dofus",
  "description": "Thème neutre destiné au forum Dofus.",
  "author": "Sackey",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
