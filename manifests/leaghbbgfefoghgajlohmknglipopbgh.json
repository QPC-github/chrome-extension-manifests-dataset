{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Toaster",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Extends the functionality of your toastmastersclubs.org site.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.toastmastersclubs.org/*"
      ],
      "js": [
        "jquery.js",
        "toaster.js"
      ],
      "css": [
        "toaster.css"
      ]
    }
  ],
  "permissions": [
    "http://*.toastmastersclubs.org/"
  ]
}
