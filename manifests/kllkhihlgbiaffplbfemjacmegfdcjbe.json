{
  "author": "https://www.healthtechalpha.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "48.png",
      "38": "32.png"
    }
  },
  "content_scripts": [
    {
      "js": [
        "injected.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "HealthTech Alpha - Digital Health Intelligence at your finger tips",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "32.png",
    "96": "48.png"
  },
  "manifest_version": 2,
  "name": "HealthTech Alpha",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/",
    "https://*/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.8",
  "web_accessible_resources": [
    "iframe.html"
  ]
}
