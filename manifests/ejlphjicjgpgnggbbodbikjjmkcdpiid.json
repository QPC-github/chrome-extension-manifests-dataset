{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://bnews.kz; script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "name": "BNews.kz",
  "version": "1.1",
  "description": "BNews.kz - это актуальные новости экономики и бизнеса, политики и общества. Оперативно, достоверно, объективно",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*.bnews.kz/"
  ]
}
