{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Egaylity updates",
  "description": "Worldwide updates about LGBT community",
  "minimum_chrome_version": "35.0.0.0",
  "version": "5.18",
  "icons": {
    "16": "egaylity-16.png",
    "128": "egaylity-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "config.js",
        "main.js"
      ]
    }
  },
