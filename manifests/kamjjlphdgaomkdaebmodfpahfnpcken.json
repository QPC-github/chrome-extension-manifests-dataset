{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nay History",
  "description": "Manages browsing history and other items the browser stores",
  "author": "Scott Newman",
  "author_email": "scott.newman50@gmail.com",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "browsingData",
    "cookies",
    "history",
    "storage",
    "<all_urls>"
  ]
}
