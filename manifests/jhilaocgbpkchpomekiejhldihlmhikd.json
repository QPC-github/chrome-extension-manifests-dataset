{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "__MSG_appSName__",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "2.1.5",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "applications": {
    "gecko": {
      "id": "support@vpn99.net",
      "strict_min_version": "57.0"
    }
  },
  "background": {
    "scripts": [
      "dist/browser-polyfill.min.js",
      "dist/background.js"
    ]
  },
  "browser_action": {
    "default_title": "VPN99",
    "default_icon": "dist/img/icon.png",
    "default_popup": "dist/popup.html"
  },
  "permissions": [
    "proxy",
    "storage",
    "notifications",
    "<all_urls>",
    "tabs",
    "webRequestBlocking",
    "webRequest"
  ]
}
