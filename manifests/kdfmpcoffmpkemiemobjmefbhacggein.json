{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MTWP Capture",
  "short_name": "Capture",
  "description": "An intelligent Screen Capture utility for Google Chrome",
  "version": "1.5",
  "author": "@Mat That Was Pat",
  "background": {
    "scripts": [
      "popup.js",
      "jquery-3.3.1.min.js",
      "scraper.js",
      "RI/scraperRI.js",
      "RI/scraperRIBCH.js",
      "RI/scraperRIMCH.js",
      "RI/ScraperRIQ.js",
      "RI/scraperRIDCV.js",
      "RI/scraperRIGI.js",
      "IG/scraperIG.js",
      "IG/scraperIGB.js",
      "IG/scraperIGW.js",
      "IG/scraperIGD.js",
      "INA/ScraperINA.js",
      "INA/ScraperINA1911.js",
      "GV/ScraperGV.js",
      "scraperDB.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "tabs",
    "storage",
    "http://mayo.rootsireland.ie/"
  ],
  "browser_action": {
    "default_icon": "logoTree.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
