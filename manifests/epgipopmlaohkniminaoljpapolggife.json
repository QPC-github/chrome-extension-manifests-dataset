{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SENDIT",
  "short_name": "Sendit",
  "author": "Sendit Paraguay",
  "version": "2.0",
  "description": "La extensión de sendit que te permite cargar los datos de tu casilla en las tiendas online rápidamente",
  "browser_action": {
    "default_title": "Sendit",
    "default_icon": "images/icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "manifest_version": 2
}
