{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Personalize Home with Todo App",
  "description": "This is a project to develop a chrome extension based on a tutorial in medium",
  "version": "1",
  "author": "Khai Fahmi",
  "browser_action": {
    "default_title": "ChromeX beta"
  },
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "permissions": [
    "activeTab"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens home.html"
    }
  }
}
