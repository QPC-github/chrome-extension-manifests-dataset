{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hyvly",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "Reddit chatroom extension.",
  "content_scripts": [
    {
      "matches": [
        "http://reddit.com/*",
        "https://reddit.com/*",
        "http://www.reddit.com/*",
        "https://www.reddit.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
