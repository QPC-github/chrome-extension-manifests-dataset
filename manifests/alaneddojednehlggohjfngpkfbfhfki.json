{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Amazing Linkalizer",
  "description": "Easily generate international Amazon product links and add your affiliate codes",
  "version": "1.0.11",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://amznlnk.com/",
    "https://use.fontawesome.com/",
    "identity",
    "identity.email"
  ],
  "key": "",
  "oauth2": {
    "client_id": "762531851112-3ushci8rrs9orq8jkibm9ggeeg0grri0.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email"
    ]
  }
}
