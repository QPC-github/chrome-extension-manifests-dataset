{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ziotes Click to Call",
  "version": "1.7",
  "description": "Reads your page and check if there's a matching number pattern, then make it a link.",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "assets/js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
