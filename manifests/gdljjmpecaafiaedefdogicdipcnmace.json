{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hacker News",
  "description": "Hacker News Web App (Shortcut to hn.premii.com)",
  "version": "2.1",
  "app": {
    "urls": [
      "http://hn.premii.com/"
    ],
    "launch": {
      "web_url": "http://hn.premii.com/?crm"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
