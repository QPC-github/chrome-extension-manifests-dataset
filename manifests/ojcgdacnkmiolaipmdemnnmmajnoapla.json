{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoEpik Remote Specialist",
  "shorname": "GoEpik RS",
  "author": "GoEpik Industry 4.0",
  "version": "2.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed by https://www.goepik.com.br/. It only capture content of your screen.",
  "homepage_url": "https://www.goepik.com.br/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://dev-er.goepik.io/*",
      "https://beta-er.goepik.io/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
