{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Initab",
  "description": "The New Tab Extension for Developers",
  "version": "1.8.1",
  "homepage_url": "http://initab.com",
  "incognito": "not_allowed",
  "icons": {
    "16": "img/initab-icon-16-alt.png",
    "48": "img/initab-icon-48.png",
    "128": "img/initab-icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "history",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://gist.github.com https://ssl.google-analytics.com https://srv.buysellads.com; object-src 'self'",
  "manifest_version": 2
}
