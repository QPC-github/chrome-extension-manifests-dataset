{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Url Helper",
  "version": "1.4.0",
  "author": "Coco",
  "manifest_version": 2,
  "description": "Best way to handle page multiple url parameters.",
  "permissions": [
    "http://*/",
    "http://*/*",
    "https://*/",
    "https://*/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/",
        "https://*/",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery-2.2.4.min.js",
        "js/url-handle.js"
      ]
    }
  ]
}
