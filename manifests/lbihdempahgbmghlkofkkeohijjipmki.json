{
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Makes text thicker.",
  "name": "BeefyText",
  "permissions": [
    "https://secure.flickr.com/"
  ],
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "16": "img/icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "ext/jquery.js",
        "ext/underscore.js",
        "content_script.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.0"
}
