{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "myjs2.js"
      ],
      "matches": [
        "http://hiroba.dqx.jp/sc/character/*/picture/*"
      ]
    }
  ],
  "description": "ドラゴンクエストXの思い出アルバムを簡単に保存できます.",
  "manifest_version": 2,
  "name": "DQX 思い出アルバム保存ツール",
  "options_page": "option.html",
  "page_action": {
    "default_icon": "icon.gif",
    "default_title": "Do action"
  },
  "permissions": [
    "tabs",
    "http://hiroba.dqx.jp/",
    "http://img.dqx.jp/"
  ],
  "version": "1.3"
}
