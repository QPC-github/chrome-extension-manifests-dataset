{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter-PuzzleScript",
  "version": "1.2",
  "description": "Play puzzlescript.net games right in your Twitter feeds.",
  "icons": {
    "128": "mascot.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "puzzlescript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
