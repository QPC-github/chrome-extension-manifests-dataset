{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Public IP",
  "description": "Displays your public ip address.",
  "version": "1",
  "author": "Rapid Technologies Pakistan",
  "icons": {
    "16": "ip-icon16.png",
    "24": "ip-icon24.png",
    "32": "ip-icon32.png",
    "48": "ip-icon48.png",
    "128": "ip-icon128.png"
  },
  "browser_action": {
    "default_popup": "ReadIP.html"
  },
  "permissions": [
    "activeTab"
  ]
}
