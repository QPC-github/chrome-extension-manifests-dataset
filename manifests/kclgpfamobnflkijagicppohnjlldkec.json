{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vegeto Time - Beta",
  "version": "0.5.0",
  "description": "This is CraftingVegetos very own chrome extension",
  "permissions": [
    "declarativeContent",
    "notifications",
    "activeTab",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "CraftiYikes.png",
    "32": "CraftiYikes.png",
    "48": "CraftiYikes.png",
    "128": "CraftiYikes.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "popup.html",
    "injected_popup.html",
    "Alert.ogg",
    "evil.png",
    "CraftiYikes.png"
  ],
  "manifest_version": 2
}
