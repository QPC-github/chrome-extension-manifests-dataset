{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "pouloto.com's latest items",
  "description": "View latest items added in pouloto.com",
  "version": "2.3",
  "browser_action": {
    "default_title": "pouloto.com's latest items",
    "default_icon": {
      "19": "poulotocom19.png",
      "38": "poulotocom38.png",
      "48": "poulotocom48.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://www.pouloto.com/",
    "alarms"
  ]
}
