{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EAS search",
  "version": "2019.07.29.1",
  "description": "Công cụ tìm kiếm quyên góp việt nam!",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://eas.hotavn.com/image/icon.ico",
      "is_default": true,
      "keyword": "hotavn.com",
      "name": "HotaVN - EAS",
      "search_url": "https://eas.hotavn.com/search?q={searchTerms}&first=1&type=1",
      "suggest_url": "https://horobot.hotavn.com/recommend.php?q={searchTerms}"
    }
  }
}
