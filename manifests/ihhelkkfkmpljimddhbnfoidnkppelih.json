{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mosne Share",
  "version": "2.1",
  "description": "social networks sharing tool.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Share this!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
