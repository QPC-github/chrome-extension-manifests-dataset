{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stop The Disco",
  "description": "This extension stops the flashing that occurs when you scroll on the Woolworths website",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "fix.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "fix.css"
  ],
  "icons": {
    "128": "stopTheDisco.png"
  }
}
