{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Todays News and Updates",
  "manifest_version": 2,
  "short_name": "todaysnews",
  "description": "Get up to date news and updates from the United States and the World",
  "version": "2.02",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_1189-1523768742.png"
  },
  "icons": {
    "128": "128px_1189-1523768763.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
