{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "version": "1.5.2",
  "manifest_version": 2,
  "short_name": "ParentalControlPornBlocker",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "icons": {
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
