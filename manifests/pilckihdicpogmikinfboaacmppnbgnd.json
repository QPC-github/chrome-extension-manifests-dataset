{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/adapters/chrome.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png"
    },
    "default_popup": "views/popup.html"
  },
  "description": "tm.urlcatcher",
  "icons": {
    "16": "images/logo-16.png",
    "32": "images/logo-32.png",
    "48": "images/logo-48.png"
  },
  "manifest_version": 2,
  "name": "tm.urlcatcher",
  "short_name": "tm.urlcatcher",
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab"
  ],
  "version": "1.0.10"
}
