{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.4",
  "default_locale": "en",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "icons": {
    "16": "images/icons/logo-color-16.png",
    "48": "images/icons/logo-color-48.png",
    "128": "images/icons/logo-color-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}
