{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Simple New Page Bookmarks",
  "version": "1.4.1",
  "description": "Displays bookmarks in the new tab page using a dark theme",
  "author": "dev_masta",
  "homepage_url": "https://upwork.com/fl/andrija",
  "icons": {
    "128": "images/icon.png"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "storage",
    "chrome://favicon/"
  ],
  "chrome_url_overrides": {
    "newtab": "main.html"
  }
}
