{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "かんたんコピーくん",
  "description": "コピー禁止のサイトでコピーできるようにします。",
  "icons": {
    "128": "icon_128.png"
  },
  "version": "1.1",
  "background": {
    "scripts": [
      "back.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_16.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
