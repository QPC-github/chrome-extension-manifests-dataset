{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Embed Killer",
  "description": "This extension hides embeded pictures in your tweet feed until you click the tweet.",
  "version": "1.2",
  "homepage_url": "http://iamleft.se/",
  "icons": {
    "48": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ]
}
