{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HelloProfit LLC",
  "short_name": "HelloProfit",
  "description": "Amazon Unit Session Exporter",
  "version": "1.9.10",
  "author": "HelloProfit",
  "homepage_url": "https://helloprofit.com",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "64": "images/icon_64.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon_48.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.min.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://*.amazon.com/*"
    ]
  },
  "permissions": [
    "alarms",
    "https://*.amazon.com/",
    "https://*.amazon.co.uk/",
    "https://*.amazon.ca/",
    "https://*.amazon.com.mx/",
    "https://*.amazon.de/",
    "https://*.amazon.fr/",
    "https://*.amazon.es/",
    "https://*.amazon.it/",
    "https://*.amazon.jp/",
    "*://*.helloprofit.com/",
    "*://*.helloprofit.local/",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "js/jquery-2.2.1.min.js"
  ]
}
