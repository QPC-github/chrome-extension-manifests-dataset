{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Makewell For Chrome",
  "short_name": "Makewell",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Get updated on the latest from the Makewell and get notified when something new is posted!",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://query.yahooapis.com; object-src 'self'"
}
