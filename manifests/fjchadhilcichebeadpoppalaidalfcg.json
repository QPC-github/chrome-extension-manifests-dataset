{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "zxlink.js"
    ]
  },
  "description": "Creates a right-click context menu to send a link to your ZyXEL home NAS device.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "ZyXEL NAS Link Capture",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs",
    "idle"
  ],
  "version": "0.35"
}
