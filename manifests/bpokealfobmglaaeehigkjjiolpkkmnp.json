{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Dump Parser",
  "description": "Parses bookmark sync data dumps adds them to the bookmark bar",
  "version": "1.1",
  "author": "metarmask",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open the Bookmark Dump Parser"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
