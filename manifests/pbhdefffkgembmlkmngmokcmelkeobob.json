{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stock Performer Extension",
  "short_name": "stkprfrmr",
  "version": "0.7",
  "description": "This extension allows Stock Performer users to retrieve sales data from their agencies.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.stockperformer.com/*",
        "*://*.istockphoto.com/*",
        "*://*.123rf.com/*",
        "*://*.dreamstime.com/*",
        "*://*.fotolia.com/*",
        "*://*.123rf.com/*",
        "*://*.dreamstime.com/*",
        "*://*.gettyimages.com/*",
        "*://*.depositphotos.com/*",
        "*://*.pond5.com/*",
        "*://*.bigstock.com/*",
        "http://localhost/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  }
}
