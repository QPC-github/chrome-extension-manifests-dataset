{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ChaoticDashboard",
  "version": "0.1.3",
  "description": "A simple new-tab-dashboard for your daily schedule",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "author": "ChaoticBench",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.min.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "homepage_url": "http://chaoticbench.com",
  "incognito": "spanning",
  "minimum_chrome_version": "40.0.0",
  "offline_enabled": true,
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "short_name": "CB Dashboard",
  "version_name": "0.1.3",
  "web_accessible_resources": [
    "js/jquery-1.12.2.min.js",
    "js/background.min.js",
    "css/dashboard.min.css"
  ]
}
