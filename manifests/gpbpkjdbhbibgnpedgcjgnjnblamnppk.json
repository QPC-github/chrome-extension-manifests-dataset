{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tivibu GO Chrome Uzantısı-EY",
  "version": "3.9.3",
  "description": "Chrome Native Messaging for Tivibu Plugin Player",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*.html",
      "*://*.erlab.com.tr:*/*.html",
      "*://*.erlab.com.tr:*/*.*",
      "*://*.tivibu.com.tr:*/*.*",
      "*://*.tivibu.com.tr:*/*.html"
    ]
  }
}
