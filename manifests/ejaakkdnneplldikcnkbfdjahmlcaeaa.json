{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Daily Dev Blog Extensions (기술 블로그 구독)",
  "description": "개발 블로그를 아카이빙 하여 일별로 조회가 가능합니다.",
  "version": "1.16",
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "permissions": [
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
