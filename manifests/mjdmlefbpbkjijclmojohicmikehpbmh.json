{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "karma Beach",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Karma Beach brings you a NewTab with beautiful backgrounds, Time, Date, Weather and Search",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
