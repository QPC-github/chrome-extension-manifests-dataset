{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Time is Money - Productivity Tracker",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/timeismoney.png",
    "default_popup": "popup.html",
    "default_title": "See how you use your time"
  }
}
