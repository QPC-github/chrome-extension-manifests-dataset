{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Falcon New Tab Extension",
  "short_name": "Falcon",
  "version": "0.9",
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "description": "TBD",
  "manifest_version": 2,
  "author": "Kevin",
  "chrome_url_overrides": {
    "newtab": "/public/index.html"
  },
  "icons": {
    "16": "/public/falconlogo.png",
    "48": "/public/falconlogo.png",
    "128": "/public/falconlogo.png"
  },
  "browser_action": {
    "default_icon": "/public/falconlogo.png",
    "default_title": "Fly Falcon"
  },
  "background": {
    "scripts": [
      "/public/bundle.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com https://api.trello.com/ https://code.jquery.com 'unsafe-eval'; object-src 'self'"
}
