{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Webandcrafts Screencast Chrome Extension",
  "description": "Screenshare sample extension to enable screensharing",
  "version": "1.0.1",
  "author": "Webandcrafts",
  "homepage_url": "http://kidobotikz.com/",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://kidobotikz.com/*",
      "*://www.kidobotikz.com/*"
    ]
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
