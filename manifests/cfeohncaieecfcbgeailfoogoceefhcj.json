{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fl.ru helper",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "https://www.fl.ru/*"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Open account/orders info",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}
