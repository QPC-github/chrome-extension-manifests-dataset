{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Verifique os vendedores no Aliexpress",
  "version": "1.6.1",
  "icons": {
    "16": "icon/icon.png",
    "32": "icon/icon.png",
    "128": "icon/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.aliexpress.com/*"
      ],
      "css": [
        "css/styles.css"
      ],
      "js": [
        "js/jquery.js",
        "js/common.js"
      ]
    }
  ],
  "permissions": [
    "*://*.aliexpress.com/*"
  ],
  "web_accessible_resources": [
    "html/*",
    "js/jquery-2.0.3.min.map"
  ]
}
