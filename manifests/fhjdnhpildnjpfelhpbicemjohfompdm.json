{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "32": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "exclude_globs": [
        ""
      ],
      "include_globs": [
        "*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://podio.com/*",
        "*://podio.com/*"
      ],
      "exclude_matches": [
        "*://podio.com/webforms/*",
        "*://podio.com/login*",
        "*://podio.com/*/*/*/*/edit*"
      ]
    }
  ],
  "description": "Converting custom calculations field into section breaks.",
  "name": "Podio Helper",
  "short_name": "Podio Helper",
  "version": "15.3"
}
