{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "archive.org Site Archive Checker",
  "description": "Bu uzantı ile archive.org Arşiv Kontrolü Yapılır.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "archive.org Arşiv Kontrolü Yap"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
