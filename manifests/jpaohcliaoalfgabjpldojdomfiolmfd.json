  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/onlinecheckwriter.png"
  },
  "manifest_version": 2,
  "version": "1.0",
  "name": "Check Printing Software",
  "short_name": "Online Check Writer",
  "description": "Online check printing software.",
  "icons": {
    "128": "icons/onlinecheckwriter.png",
