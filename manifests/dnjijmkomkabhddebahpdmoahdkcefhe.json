{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Multicolor",
  "description": "Beautiful hex colors in your new tab",
  "version": "1",
  "author": "Logan George",
  "browser_action": {
    "default_title": "Multicolor",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
