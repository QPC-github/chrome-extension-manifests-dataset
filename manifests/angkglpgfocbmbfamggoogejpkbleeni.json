{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search Youtube",
  "default_locale": "en",
  "description": "Search Youtube from right click menu.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "0.0.0.2",
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
