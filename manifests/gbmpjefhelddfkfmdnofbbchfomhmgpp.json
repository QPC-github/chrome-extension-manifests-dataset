{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Croooober Relipper",
  "version": "1.1",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js"
    ]
  },
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "relipper.user.js"
      ],
      "matches": [
        "http://www.croooober.com/*",
        "https://www.croooober.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ]
}
