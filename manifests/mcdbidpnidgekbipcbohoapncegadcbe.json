{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Day2Day - ToDo List",
  "description": "A simple extension to manage your daily tasks.",
  "version": "1.0",
  "author": "Rajasri",
  "browser_action": {
    "default_icon": "image/Icon16x16.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "alarms",
    "identity",
    "identity.email"
  ],
  "web_accessible_resources": [
    "image/Icon16x16.png"
  ]
}
