{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HomerJ VOD Popout",
  "description": "VODs auf HomerJ.de in einem extra Popout Ã¶ffnen",
  "version": "2.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*.homerj.de/index.php?show=vods&play=*",
    "background"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.homerj.de/index.php?show=vods*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "homerj_popout.js"
  ]
}
