{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/chrome.js",
      "js/common.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon32.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_cardse_app_name__"
  },
  "content_scripts": [
    {
      "js": [
        "js/contentScript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_cardse_app_description__",
  "homepage_url": "https://cardse.net",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon19.png",
    "19": "images/icon19.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_cardse_extension_name__",
  "short_name": "Cardse Free Image Upload",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "activeTab",
    "<all_urls>"
  ],
  "version": "1.1.11"
}
