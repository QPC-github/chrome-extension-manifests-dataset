{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "resources/img/favicon_atomshot_32_32.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_extPopUpHoover__"
  },
  "commands": {
    "captureEntire": {
      "description": "Screenshot der gesamten Seite",
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "linux": "Ctrl+Shift+F"
      }
    },
    "captureVisible": {
      "description": "Screenshot des sichtbaren Bereichs",
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "linux": "Ctrl+Shift+Y"
      }
    }
  },
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "resources/img/favicon_atomshot_128_128.png",
    "48": "resources/img/favicon_atomshot_48_48.png",
    "16": "resources/img/favicon_atomshot_16_16.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "permissions": [
    "activeTab",
    "http://*/*",
    "*://code.google.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3.1"
}
