{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide w3schools",
  "description": "This extension simply adds -site:w3schools.com to your google.com and google.co.uk searches to prevent w3schools results.",
  "version": "0.2.1",
  "icons": {
    "128": "hidew3.png"
  },
  "background": {
    "scripts": [
      "w3fix.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "*://*.google.co.uk/*",
    "*://*.google.com/*"
  ]
}
