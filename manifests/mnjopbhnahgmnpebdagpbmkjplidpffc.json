{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "G6 - Social Selling App from Grapevine6",
  "description": "Discover and post content in one touch. No setup, just sign in with social.",
  "version": "1.2.0",
  "manifest_version": 2,
  "externally_connectable": {
    "matches": [
      "https://app.grapevine6.com/*"
    ]
  },
  "icons": {
    "16": "res/icon16.png",
    "32": "res/icon32.png",
    "48": "res/icon48.png",
    "128": "res/icon128.png"
  },
  "key": "",
  "browser_action": {
    "default_icon": {
      "16": "res/icon16.png",
      "32": "res/icon32.png",
      "48": "res/icon48.png",
      "128": "res/icon128.png"
    },
    "default_title": "G6"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://app.grapevine6.com/",
    "cookies"
  ],
  "background": {
    "scripts": [
      "dist/js/jquery-1.9.1.min.js",
      "dist/js/background.js"
    ]
  },
  "web_accessible_resources": [
    "dist/images/close_button_circle.png",
    "dist/frame.html"
  ]
}
