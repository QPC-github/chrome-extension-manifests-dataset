{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CIT Assinatura",
  "description": "Esta extensao permite a assinatura de arquivos via token",
  "version": "1.1.9",
  "browser_action": {
    "default_title": "CIT Assinatura"
  },
  "icons": {
    "16": "icon-citsmart-19.png",
    "128": "icon-citsmart-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "downloads"
  ],
  "externally_connectable": {
    "matches": [
      "https://localhost:8443/cit-portal-web*",
      "https://sigad-homol.centralit.com.br/cit-portal-web*",
      "https://docs.cna.org.br/*",
      "https://www.trt12.jus.br/citsaude*",
      "http://homologacao.trt19.jus.br/citsaude*",
      "https://app.trt19.jus.br/citsaude*",
      "https://ioda.valec.gov.br/cit-portal-web*"
    ]
  }
}
