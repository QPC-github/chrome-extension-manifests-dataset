{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "gControl Air Arabia",
  "version": "0.1",
  "description": "An extension to gControl by CloudCodes.",
  "background": {
    "scripts": [
      "background.js",
      "jquery-1.10.2.min.js",
      "jquery-ui-1.8.2.custom.min.js",
      "jquery.jcryption-1.1.min.js",
      "framework.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "http://mail.google.com/*"
      ],
      "exclude_matches": [
        "https://mail.google.com/mail*/h/*",
        "http://mail.google.com/mail*/h/*",
        "https://mail.google.com/mail*/x/*",
        "http://mail.google.com/mail*/x/*"
      ],
      "js": [
        "lib/bootstrap.js",
        "lib/jquery.1.4.2.js",
        "lib/jquery.ba-bbq.js",
        "lib/lab.js",
        "lib/init.js",
        "lib/gmailr.js",
        "main.js",
        "jquery-1.10.2.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "lib/idletab.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://mail.google.com/mail*/h/*",
        "http://mail.google.com/mail*/h/*"
      ],
      "js": [
        "lib/basic.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://mail.google.com/mail*/x/*",
        "http://mail.google.com/mail*/x/*"
      ],
      "js": [
        "lib/mobile.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://drive.google.com/*",
        "http://drive.google.com/*",
        "https://plus.google.com/*",
        "http://plus.google.com/*",
        "https://www.google.com/contacts/*",
        "https://www.google.com/calendar/*",
        "https://sites.google.com/*",
        "http://sites.google.com/*"
      ],
      "js": [
        "lib/jquery.1.4.2.js",
        "lib/plusjs.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://gcontrolsso.appspot.com/loginpageservlet*",
        "http://gcontrolsso.appspot.com/loginpageservlet*",
        "http://gcontrolsso.appspot.com/directory/ADSSOManagerServlet*",
        "https://gcontrolsso.appspot.com/directory/ADSSOManagerServlet*",
        "https://gcontrolsso.appspot.com/user/login/accountlogin*"
      ],
      "js": [
        "lib/installhelper.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://accounts.google.com/o/oauth2/*",
        "https://accounts.google.com/o/openid2/*"
      ],
      "js": [
        "lib/jquery.1.4.2.js",
        "lib/blockit.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "alarms",
    "idle",
    "management",
    "history",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/",
    "https://gcontrolsso.appspot.com/*"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "manifest.json",
    "lib/bootstrap.js",
    "lib/bootstrapDrive.js",
    "lib/gmailr.js",
    "lib/driver.js",
    "lib/init.js",
    "lib/initDrive.js",
    "lib/jquery.1.4.2.js",
    "lib/jquery.ba-bbq.js",
    "lib/lab.js",
    "main.css",
    "main.js",
    "lib/drivejs.js",
    "lib/installhelper.js",
    "jquery-1.10.2.min.js",
    "gmail.js",
    "mainirm.js",
    "emailclassification.js"
  ]
}
