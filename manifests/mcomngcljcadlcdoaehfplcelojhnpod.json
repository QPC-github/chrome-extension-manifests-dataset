{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "JS/nbascoreloader.js"
      ],
      "matches": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "NBA 2013 Finals Scoreboard by Russel Sese (http://www.facebook.com/russel.sese | russel_se2@yahoo.com)",
  "icons": {
    "128": "PublisherImages/nbascoreboard-orig128.png",
    "16": "PublisherImages/nbascoreboard-orig16.png",
    "48": "PublisherImages/nbascoreboard-orig48.png"
  },
  "manifest_version": 2,
  "name": "NBA 2013 Finals Scoreboard by Russel Sese",
  "page_action": {
    "default_icon": "images/nbascoreboard-orig35.png",
    "default_title": "NBA 2013 Finals Scoreboard"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0"
}
