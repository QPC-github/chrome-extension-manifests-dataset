{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CJs Auto Refresh",
  "short_name": "CJs Refresh",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Auto refresh the page for CJ because CJ is awesome!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "icon128.png"
  }
}
