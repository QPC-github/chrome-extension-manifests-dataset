{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hash your password",
  "icons": {
    "128": "icon128.png"
  },
  "description": "Hash your password with domain name (or any other text)",
  "version": "0.1.0",
  "permissions": [
    "clipboardWrite",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Hash your password",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "author": "Ahmed Alzayani"
}
