{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1",
  "name": "AllHYIPMonitors.com",
  "description": "Browse status of all hyip monitors on one page. Click to view domain details on www.allhyipmonitos.com",
  "homepage_url": "http://www.allhyipmonitors.com/",
  "developer": {
    "name": "www.allhyipmonitors.com"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/browser.js"
    ]
  },
  "browser_action": {
    "default_title": "AllHYIPMonitors.com",
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
