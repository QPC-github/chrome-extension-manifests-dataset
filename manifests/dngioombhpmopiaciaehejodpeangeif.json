{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SL&ENT Dark Theme",
  "version": "1.2.4",
  "manifest_version": 2,
  "description": "Adds a dark theme to SL&ENT",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://slaent.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://slaent.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
