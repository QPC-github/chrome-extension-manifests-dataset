{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nyaagnet",
  "version": "0.0.2",
  "description": "Convert NyaaTorrents download links into magnet links",
  "homepage_url": "https://github.com/matchai/nyaagnet",
  "manifest_version": 2,
  "minimum_chrome_version": "36",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.nyaa.se/*"
  ],
  "icons": {
    "128": "iconx128.png",
    "48": "iconx48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
