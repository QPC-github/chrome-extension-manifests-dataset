{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HiztegiBar",
  "version": "2.3",
  "manifest_version": 2,
  "description": "HiztegiBar hainbat hiztegi eta baliabide linguistikoetan bilaketak egiten laguntzen duen tresna da. ",
  "icons": {
    "256": "irudiak/Book256.png"
  },
  "browser_action": {
    "default_icon": "irudiak/Book.png",
    "default_popup": "hiztegiBar.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "js/es.js",
      "js/eu.js",
      "js/hiztegiak.js",
      "js/hiztegiBar_kargatu.js",
      "js/hiztegiBar_tab.js",
      "js/hiztegiBar_url.js",
      "js/konstanteak.js",
      "js/tabber.js"
    ]
  },
  "sandbox": {
    "content_security_policy": "sandbox allow-scripts; script-src 'self' 'unsafe-eval'; object-src 'self'"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "postDeiak.html",
    "postDeiakUTF8.html",
    "ayuda.html",
    "laguntza.html"
  ]
}
