{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Globish Screen Sharing Extension",
  "author": "Globish Academia",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for https://www.globish.space",
  "homepage_url": "https://www.globish.space",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://www.globish.space/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
