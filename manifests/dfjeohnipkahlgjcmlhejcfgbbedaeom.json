{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cute Piranha Monster",
  "version": "0.1",
  "description": "Have a cute piranha on your chrome new tab",
  "icons": {
    "16": "images/wdmaster16.png",
    "32": "images/wdmaster32.png",
    "48": "images/wdmaster48.png",
    "128": "images/wdmaster128.png"
  },
  "chrome_url_overrides": {
    "newtab": "alien.html"
  },
  "background": {
    "scripts": [
      "events.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "alien.html"
  ]
}
