{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Corral",
  "description": "RSS Feed Viewer for Feed Wrangler",
  "version": "1.0.0",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Corral"
  },
  "background": {
    "scripts": [
      "chrome.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
