{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PMS Approval",
  "description": "Automatic approval on the PMS system (http://pms.webdev.com)",
  "version": "0.0.2",
  "manifest_version": 2,
  "homepage_url": "https://github.com/zhaoda/pms-approval",
  "page_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_title": "自动审批"
  },
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "http://pms.webdev.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://pms.webdev.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
