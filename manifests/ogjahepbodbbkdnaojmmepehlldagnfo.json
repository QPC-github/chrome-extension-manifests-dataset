{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SkliQ Enterprise SaaS Screen Sharing",
  "author": "Squared Methods",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed for https://skliq.com/. It captures content of your screen.",
  "homepage_url": "https://skliq.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.skliq.com/*"
    ]
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
