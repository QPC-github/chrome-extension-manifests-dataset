{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Body Positivity",
  "description": "Replace body-negative words with encouraging words such as \"love\", \"fuel\", and \"amazing body\"",
  "version": "0.11",
  "icons": {
    "16": "heart16.png",
    "48": "heart48.png",
    "128": "heart128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
