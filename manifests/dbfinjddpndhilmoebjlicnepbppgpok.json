{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chaudron Empoisonné",
  "description": "Vous permet de savoir en un coup d'oeil le nombre d'âme actuellement possédé et offre un accès rapide à l'historique des visites.",
  "version": "1.0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://www.chaudron-empoisonne.fr/",
    "http://chaudron-empoisonne.fr/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "omnibox": {
    "keyword": "ce"
  }
}
