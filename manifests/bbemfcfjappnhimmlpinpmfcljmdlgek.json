{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "webux",
  "description": "Faites vos propres filtrages du r√©sultat obtenu",
  "version": "1.41",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Test"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "test.js",
        "anims.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "http://jquery.com/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
