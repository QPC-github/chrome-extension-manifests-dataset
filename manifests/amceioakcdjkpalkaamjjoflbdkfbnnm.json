{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seek and Hide - FocusED",
  "description": "Enhance your focus on any portion of a webpage.",
  "version": "1.6",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/64.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/cropper.min.js",
        "js/main.js"
      ],
      "css": [
        "css/style.css",
        "css/cropper.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/43aspect.png"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
