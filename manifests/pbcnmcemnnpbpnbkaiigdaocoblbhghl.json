{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Surfchat",
  "version": "1.1.31",
  "description": "Chat on any web page",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com https://www.gstatic.com https://cdn.firebase.com https://*.firebaseio.com 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "images/favicon-16x16.png",
    "48": "images/favicon-48x48.png",
    "128": "images/favicon-128x128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "identity"
  ],
  "key": "",
  "browser_action": {
    "default_popup": "home.html"
  },
  "oauth2": {
    "client_id": "17921528894-gf951514dgdnnokkvs1trtidr5klkjcv.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
