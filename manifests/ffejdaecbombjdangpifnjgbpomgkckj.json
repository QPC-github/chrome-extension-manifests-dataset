{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spongebob Meme",
  "short_name": "sponge-meme",
  "version": "1.0",
  "description": "Hit both of your control keys to automatically and randomly vary the capitalization of your letters.",
  "icons": {
    "450": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "meme.js"
      ],
      "run_at": "document_end"
    }
  ]
}
