{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YT Repeat",
  "version": "1.0.3",
  "description": "Repeats YouTube videos.",
  "icons": {
    "16": "icons/16.png",
    "22": "icons/22.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
