{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.5",
  "name": "TeamCal for Google Calendarâ„¢",
  "short_name": "TeamCal",
  "description": "The missing schedule view for Google Calendar",
  "homepage_url": "https://www.teamcalapp.com",
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "permissions": [
    "https://*.teamcalapp.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.teamcalapp.com/*"
    ]
  },
  "browser_action": {
    "default_title": "Open TeamCal",
    "default_icon": {
      "16": "logo_16.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
    },
    "default_popup": "popup.html"
  }
}
