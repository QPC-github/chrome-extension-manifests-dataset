{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "A simple extension that allows you to stream your desktop into meetings with UCCCLoud WebRTC client.",
  "externally_connectable": {
    "matches": [
      "*://www.ucccloud.com/*"
    ]
  },
  "icons": {
    "128": "ucccloud-logo-128x128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "UCCCloud Desktop Streamer",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.5"
}
