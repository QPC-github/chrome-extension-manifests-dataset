{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Boel - App voor bol.com",
  "description": "Zoek eenvoudig en snel een product bij bol.com met behulp van deze chrome extension",
  "version": "1.5",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "128": "images/bol-icon-groot.png",
    "16": "images/bol-icon-bitty.png",
    "48": "images/bol-icon-klein.png"
  },
  "browser_action": {
    "default_icon": "images/bol-icon-groot.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
