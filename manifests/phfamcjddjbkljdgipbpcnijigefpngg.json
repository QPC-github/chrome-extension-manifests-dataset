{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tyler's handy note tracker",
  "description": "sometimes tyler needs help keeping track of things",
  "manifest_version": 2,
  "version": "0.0.0.1",
  "permissions": [
    "management",
    "tabs",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
