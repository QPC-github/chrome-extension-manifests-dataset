{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "6me.us URL Shortener",
  "version": "1.7",
  "description": "The simple, no-frills IPv6-compatible URL shortening service.",
  "icons": {
    "16": "link16.png",
    "48": "link48.png",
    "128": "link128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://6me.us/",
    "tabs",
    "clipboardWrite"
  ]
}
