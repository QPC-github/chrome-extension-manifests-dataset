{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Udostepnianie ekranu (E-Konsultacje)",
  "description": "Rozszerzenie pozwala na udostepnianie ekranu w przeglÄ…darce Chrome na potrzeby aplikacji E-Konsultacje",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
    "64": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://localhost/*",
        "https://localhost/*",
        "http://erepozytorium.pl/*",
        "https://erepozytorium.pl/*",
        "https://10.10.0.140/*",
        "http://10.10.0.140/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "https://localhost/*",
      "http://erepozytorium.pl/*",
      "https://erepozytorium.pl/*",
      "https://10.10.0.140/*",
      "http://10.10.0.140/*"
    ]
  }
}
