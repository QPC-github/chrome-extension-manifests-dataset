{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yoop",
  "version": "3.20.0",
  "manifest_version": 2,
  "description": "Rate webpages",
  "homepage_url": "https://private.yoop.io",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "16": "images/yoop_icon16.png",
    "24": "images/yoop_icon24.png",
    "32": "images/yoop_icon32.png",
    "64": "images/yoop_icon64.png",
    "128": "images/yoop_icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/yoop_icon16.png",
      "24": "images/yoop_icon24.png",
      "32": "images/yoop_icon32.png"
    },
    "default_title": "Rate this page!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "activeTab",
    "tabs",
    "notifications",
    "storage",
    "contextMenus",
    "https://ajax.googleapis.com/",
    "https://support.google.com/"
  ],
  "web_accessible_resources": [
    "popup.html",
    "popup.js",
    "popup.css",
    "jquery-3.2.1.min",
    "listv1.csv",
    "report.html"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
