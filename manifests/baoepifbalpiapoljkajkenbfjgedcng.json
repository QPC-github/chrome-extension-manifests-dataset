{
  "background": {
    "persistent": true,
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_title": "TabWow"
  },
  "chrome_settings_overrides": {
    "homepage": "http://search.tabwow.com",
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://tabwow.com/favicon.ico",
      "is_default": true,
      "keyword": "http://tabwow.com",
      "name": "TabWow",
      "search_url": "http://search.tabwow.com/?q={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "scripts/index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com https://query.yahooapis.com https://platform.twitter.com https://connect.facebook.net; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "manifest_version": 2,
  "name": "TabWow",
  "permissions": [
    "tabs",
    "management",
    "http://*.tabwow.com/*"
  ],
  "short_name": "TabWow",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1.1"
}
