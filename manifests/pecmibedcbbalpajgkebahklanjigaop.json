{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Artical",
  "manifest_version": 2,
  "version": "0.147",
  "description": "Pin Web Pages To Your Google Calendar - Be Notified When It's Time To Read",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Artical",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "oauth2": {
    "client_id": "1036942051703-75el2lam7lgustmhhdb3feetdrqjluts.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "key": "",
  "content_security_policy": "script-src https://*.google.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "identity",
    "*://*.google.com/*",
    "http://calendar.google.com/calendar/*",
    "https://calendar.google.com/calendar/*"
  ]
}
