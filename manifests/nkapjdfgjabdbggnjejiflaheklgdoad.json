{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "128": "img/128.png",
    "256": "img/256.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://mysearcher.site/favicon.ico",
      "image_url": "https://mysearcher.site/search?q={searchTerms}",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "search",
      "name": "Private Search",
      "search_url": "https://mysearcher.site/search?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "https://mysearcher.site/search?q={searchTerms}",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "browser_action": {
    "default_icon": "img/128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "history",
    "unlimitedStorage",
    "chrome://favicon/",
    "contextMenus",
    "storage",
    "topSites",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "2"
}
