{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Run code in each tab",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Run code everywhere",
    "default_popup": "popup.html"
  }
}
