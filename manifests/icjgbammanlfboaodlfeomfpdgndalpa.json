{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Text Editor",
  "version": "1.0",
  "description": "An editor to edit any webpage inline and live. Reload page to remove changes.",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
