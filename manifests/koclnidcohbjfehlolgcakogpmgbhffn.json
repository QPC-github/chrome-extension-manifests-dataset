{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Converts CSGO Market Price into Keys",
  "manifest_version": 2,
  "name": "CSGO Market Price to Key Converter",
  "version": "1.2",
  "short_name": "CSGO Market->Key",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery.js",
      "getRates.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "19": "icon.png",
      "32": "icon.png",
      "64": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Market->Keys",
    "default_popup": "market.html"
  },
  "icons": {
    "16": "icon.png",
    "19": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "webRequest",
    "unlimitedStorage",
    "http://*.yahooapis.com/*",
    "https://*.yahooapis.com/*"
  ],
  "web_accessible_resources": [
    "http://*.yahooapis.com/*",
    "https://*.yahooapis.com/*"
  ]
}
