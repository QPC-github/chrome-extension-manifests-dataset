{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit No New Tab",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Make Reddit links not open in new tabs when user is not logged in.",
  "homepage_url": "http://localhost/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://reddit.com/*",
    "http://*.reddit.com/*",
    "https://reddit.com/*",
    "https://*.reddit.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://reddit.com/*",
        "http://*.reddit.com/*",
        "https://reddit.com/*",
        "https://*.reddit.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
