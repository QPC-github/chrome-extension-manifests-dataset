{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Levné letenky a dovolená | Frantisekjuris.eu",
  "description": "Rozšiření pro monitorováni levných letenek a last minute zájezdů",
  "background": {
    "page": "background.html"
  },
  "default_locale": "sk",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "alarms",
    "notifications",
    "*://*/*"
  ],
  "version": "1.1.9"
}
