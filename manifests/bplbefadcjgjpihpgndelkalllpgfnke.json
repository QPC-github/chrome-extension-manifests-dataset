{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kikoo!",
  "short_name": "Kikoo",
  "description": "__MSG_slogan__",
  "author": "juloo",
  "default_locale": "en",
  "version": "0.1.2",
  "icons": {
    "16": "icons/kikoo_16.png",
    "32": "icons/kikoo_32.png",
    "48": "icons/kikoo_48.png",
    "64": "icons/kikoo_64.png",
    "128": "icons/kikoo_128.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "__MSG_title__",
    "default_icon": "icons/kikoo_32.png",
    "default_popup": "kikoo.html"
  }
}
