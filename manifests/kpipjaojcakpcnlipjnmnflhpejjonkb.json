{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Periodic Data Cleaner",
  "version": "1.0",
  "description": "Extension to clean data periodically with option to restart page!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "browsingData",
    "alarms"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
