{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Font Maximizer",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Font zoom only. Enlarge text font without enlarging images.",
  "default_locale": "es",
  "browser_action": {
    "default_icon": "font.maxim.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
