{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Creative Products for Chrome",
  "manifest_version": 2,
  "short_name": "cproducts",
  "description": "Official Chrome extension for the Creative Products website",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_130-1503957180.png"
  },
  "icons": {
    "128": "128px_130-1503957131.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
