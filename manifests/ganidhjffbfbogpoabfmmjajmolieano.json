{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ColorblindChromelinks",
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "version": "1"
}
