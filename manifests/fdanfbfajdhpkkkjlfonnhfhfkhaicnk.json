{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Platform",
  "description": "Enable advanced experiment features",
  "version": "0.8.0.8",
  "permissions": [
    "tabs",
    "tabCapture",
    "http://localhost:62358/*",
    "http://asbe.cloudapp.net/Applications*",
    "http://asbeexperiments.azurewebsites.net/*",
    "http://gabrielepd.azurewebsites.net/*",
    "contextMenus"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:62358/*",
      "http://asbe.cloudapp.net/Applications*",
      "http://asbeexperiments.azurewebsites.net/*",
      "http://gabrielepd.azurewebsites.net/*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2,
  "short_name": "CU_Exp"
}
