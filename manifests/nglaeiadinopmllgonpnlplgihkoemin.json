{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "All-Star HTML Highlighter",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "version": "1.0",
  "permissions": [
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "backgroundScript.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "icons": {
    "128": "assets/logo.png"
  }
}
