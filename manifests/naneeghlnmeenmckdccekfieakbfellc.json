{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ],
      "matches": [
        "https://us.flow.microsoft.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Instantly Create folders on Microsoft Flow",
  "icons": {
    "128": "icos/144.png",
    "48": "icos/48.png"
  },
  "manifest_version": 2,
  "name": "Flow Folder",
  "permissions": [
    "tabs",
    "https://us.flow.microsoft.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5.3",
  "web_accessible_resources": [
    "*"
  ]
}
