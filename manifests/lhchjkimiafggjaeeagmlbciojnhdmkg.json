{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "wisebirds bookmark",
  "version": "2.0.2",
  "manifest_version": 2,
  "description": "wisebirds bookmark",
  "permissions": [
    "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "와이즈버즈 북마크",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
