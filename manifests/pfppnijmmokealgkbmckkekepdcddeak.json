{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Erkan Kavas Son Yazı",
  "version": "1.3",
  "description": "erkankavas.com'da yayınlanan son yazı göstergeci.",
  "homepage_url": "https://www.erkankavas.com",
  "permissions": [
    "https://www.erkankavas.com/*"
  ],
  "icons": {
    "128": "icon/erkan.png"
  },
  "sandbox": {
    "pages": [
      "popup.html"
    ]
  },
  "browser_action": {
    "default_icon": "icon/erkan.png",
    "default_title": "Erkan Kavas",
    "default_popup": "popup.html"
  }
}
