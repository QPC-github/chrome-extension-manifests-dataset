{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BinaryTS",
  "version": "1.4",
  "description": "Translate from binary to text and from text to binary - Traduce de binario a texto y de texto a binario",
  "manifest_version": 2,
  "author": "Pablo Martin Baz√°n",
  "icons": {
    "16": "src/icon16.png",
    "48": "src/icon48.png",
    "128": "src/icon128.png"
  },
  "browser_action": {
    "default_title": "BinaryTS",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "src/background.js"
  },
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "https://www.youtube.com/*"
  ],
  "web_accessible_resources": [
    "src/icon48.png"
  ]
}
