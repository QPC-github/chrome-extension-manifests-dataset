{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.1.7",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "cookies",
    "https://*/*",
    "http://*/*",
    "https://easylive.fiiser.com/",
    "https://easylivetest.fiiser.com/",
    "https://easylive.koodata.com/",
    "https://easylivetest.koodata.com/",
    "https://easylive.live/"
  ],
  "content_security_policy": "script-src 'self' https://rawgit.com; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/easylive16.png",
      "32": "images/easylive32.png",
      "48": "images/easylive48.png",
      "128": "images/easylive128.png"
    }
  },
  "icons": {
    "16": "images/easylive16.png",
    "32": "images/easylive32.png",
    "48": "images/easylive48.png",
    "128": "images/easylive128.png"
  },
  "manifest_version": 2
}
