{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "colorAWSFrame",
  "description": "Change background color of header & footer on AWS Management Console depending on role name or account.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.aws.amazon.com/*",
        "https://*.aws.amazon.com/*"
      ],
      "js": [
        "js/colorAWSFrame.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
