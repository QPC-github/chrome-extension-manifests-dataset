{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Raccoon",
  "version": "1.9",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.min.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "file://*/*",
    "http://*/",
    "https://*/",
    "input"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Home page"
  },
  "background": {
    "scripts": [
      "background.js",
      "js/new.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "web_accessible_resources": [
    "*.png",
    "*.js",
    "templates/*"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "yahoo",
      "keyword": "Yahoo",
      "search_url": "http://www.racoon.space/?publisher=tingnew&ts=sy&barcodeid=51760003&searchtype=ds&q={searchTerms}&type=TAR_{string}",
      "favicon_url": "http://www.racoon.space/",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
