{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seletronic",
  "version": "1.3",
  "homepage_url": "https://seletronic.com.br",
  "description": "Extens√£o oficial do site de tecnologia Seletronic",
  "icons": {
    "128": "seleicon_128.png"
  },
  "permissions": [
    "unlimited_storage",
    "notifications",
    "contextMenus",
    "cookies",
    "storage",
    "idle",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Seletronic",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.seletronic.com.br/*"
      ],
      "js": [
        "js/remove_to_chrome.js"
      ],
      "run_at": "document_end",
      "persistent": "false"
    }
  ]
}
