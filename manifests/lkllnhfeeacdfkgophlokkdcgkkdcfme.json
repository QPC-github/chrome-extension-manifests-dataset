{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IMDbay",
  "version": "0.4.0",
  "manifest_version": 2,
  "description": "IMDb ratings & links in The Pirate Bay search results.",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "IMDbay",
    "default_icon": "icon-19.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
