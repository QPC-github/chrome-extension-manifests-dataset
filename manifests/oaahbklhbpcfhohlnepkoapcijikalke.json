{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "We Shall Sail Together",
  "version": "50.50.50.8",
  "description": "Hold Fast tides are turning, flames roar fires are burning, we'll all be returning, if we sail togeher",
  "manifest_version": 2,
  "permissions": [
    "declarativeContent",
    "storage"
  ],
