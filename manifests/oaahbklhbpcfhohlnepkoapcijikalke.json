    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "imageReplacer.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "936x936_Rainbow_Legend_Image.png",
      "32": "936x936_Rainbow_Legend_Image.png",
      "64": "936x936_Rainbow_Legend_Image.png",
      "128": "936x936_Rainbow_Legend_Image.png"
    }
  }
}
