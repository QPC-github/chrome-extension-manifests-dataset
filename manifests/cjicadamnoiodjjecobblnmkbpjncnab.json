{
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "escope.js"
      ],
      "matches": [
        "http://youtube.com/*",
        "https://youtube.com/*",
        "https://www.youtube.com/*",
        "http://www.youtube.com/*"
      ],
      "css": [
        "escope.css"
      ]
    }
  ],
  "description": "Make youtube more informal directly from your browser!",
  "icons": {
    "128": "EqualScope.png",
    "16": "EqualScope.png",
    "48": "EqualScope.png"
  },
  "manifest_version": 2,
  "name": "EqualScope",
  "permissions": [
    "http://*.youtube.com/*",
    "https://*.youtube.com/*",
    "https://equalmediatv.net/*",
    "http://equalmediatv.net/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6",
  "web_accessible_resources": [
    "escope.js",
    "jquery.js"
  ]
}
