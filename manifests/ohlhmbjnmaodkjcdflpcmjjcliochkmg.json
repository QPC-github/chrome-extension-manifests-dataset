{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Duplicate Deleter for MyFitnessPal",
  "short_name": "DD4MFP",
  "author": "Dan \"BlindWolf8\" Fischbach",
  "description": "Helping you delete duplicates from MyFitnessPal one click at a time.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.myfitnesspal.com/food/edit/*"
      ],
      "js": [
        "js/dd4mfp.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://*.myfitnesspal.com/food/edit/*"
  ]
}
