{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Double Read",
  "version": "1.6",
  "description": "This extension will allow you to read in two different columns, which act like independent pages.",
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "doubleread.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
