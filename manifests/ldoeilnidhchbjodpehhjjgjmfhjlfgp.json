{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon_19_19.png",
    "default_title": "grab Tabs",
    "popup": "grabTabs.html"
  },
  "description": "The most simple way of grabbing a list of your current open tabs (their urls), across windows.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon_128_128.png",
    "16": "icon_16_16.png",
    "32": "icon_32_32.png",
    "48": "icon_48_48.png"
  },
  "minimum_chrome_version": "6.0.0.0",
  "manifest_version": 2,
  "name": "Grab Tabs",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "version": "0.3"
}
