{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Voice to Editable Elements",
  "description": "Use speech to fill in text fields",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "InputFieldTyping_ViaWebSpeech.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "web_accessible/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
