{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TransmissionGate Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with transmissiongate.com.",
  "version": "0.1.9",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "tg-logo-16x16.png",
    "48": "tg-logo-48x48.png",
    "128": "tg-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://timv.ro/*"
    ]
  }
}
