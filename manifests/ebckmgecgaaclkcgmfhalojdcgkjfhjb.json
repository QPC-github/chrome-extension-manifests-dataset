{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello Card Counter",
  "description": "Show default card count above each list, and show a total card count in the board header.",
  "manifest_version": 2,
  "version": "1.0.0",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://trello.com/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16x16.png",
    "48": "images/icon-48x48.png",
    "128": "images/icon-128x128.png"
  }
}
