{
  "background": {
    "scripts": [
      "libs/jquery.js",
      "core.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "assets/icon/icon-128.png",
      "32": "assets/icon/icon-32.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_app_title__"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_app_description__",
  "icons": {
    "128": "assets/icon/icon-128.png",
    "32": "assets/icon/icon-32.png",
    "38": "assets/icon/icon-38.png",
    "48": "assets/icon/icon-48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_app_name__",
  "permissions": [
    "tabCapture",
    "activeTab",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "__MSG_app_short_name__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.6",
  "version_name": "1.1.6"
}
