{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Play Reversi",
  "description": "Two player, 8 by 8 board game involving abstract strategy",
  "version": "1.6",
  "manifest_version": 2,
  "offline_enabled": true,
  "icons": {
    "16": "chrome_store_assets/Reversi/logo/icon16.png",
    "32": "chrome_store_assets/Reversi/logo/icon32.png",
    "128": "chrome_store_assets/Reversi/logo/icon128.png",
    "256": "chrome_store_assets/Reversi/logo/icon256.png"
  },
  "permissions": [
    "storage"
  ],
  "app": {
    "background": {
      "scripts": [
        "chromeapp/background.js"
      ]
    }
  }
}
