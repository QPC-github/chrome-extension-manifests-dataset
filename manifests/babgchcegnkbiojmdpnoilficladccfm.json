{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Portent's SEO Page Review",
  "description": "Review common SEO elements and issues on the page",
  "version": "1.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon-blue.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
