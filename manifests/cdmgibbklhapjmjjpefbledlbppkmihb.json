{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Focus",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1.1",
  "background": {
    "scripts": [
      "dist/vendor.js",
      "dist/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Focus"
  },
  "permissions": [
    "tabs"
  ]
}
