{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wheebox Tests : Enable Screen Sharing",
  "description": "This chrome extension enables screen sharing support for the duration of test.",
  "version": "2.12",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://cscexam.in/*",
        "https://proctor.cscexam.in/*",
        "https://nielitexam.in/*",
        "https://proctor.nielitexam.in/*",
        "https://uat.cscexam.in/*",
        "https://seb.wheebox.com/*",
        "https://wheebox.com/*",
        "https://uat1.wheebox.com/*",
        "https://uat2.wheebox.com/*",
        "https://dtuonlineexam.in/*",
        "https://wheeboxuat.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "desktopCapture",
    "tabs",
    "system.display"
  ],
  "web_accessible_resources": [
    "icon128.png"
  ]
}
