{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Business Insider One Page",
  "description": "View BI links on one page",
  "version": "2.1.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.businessinsider.com/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
