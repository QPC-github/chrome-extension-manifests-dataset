{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CPAlead Statistics",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "description": "Monitor your CPAlead earnings with the CPAlead statistics plugin.",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://www.cpalead.com/",
  "version": "2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon32.png",
    "default_title": "Statistics"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications"
  ]
}
