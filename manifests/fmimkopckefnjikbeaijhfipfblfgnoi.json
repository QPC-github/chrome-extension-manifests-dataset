{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "description": "Quick access to Google services",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "Google Services Dashboard",
  "page_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.1"
}
