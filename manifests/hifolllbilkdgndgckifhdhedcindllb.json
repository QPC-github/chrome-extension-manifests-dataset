{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Visible Comment Links Opener",
  "short_name": "Comment Opener",
  "author": "Josh Lawson",
  "manifest_version": 2,
  "description": "Opens Reddit comment links that are currently visible to user",
  "version": "0.1.0",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "vendor/*"
  ]
}
