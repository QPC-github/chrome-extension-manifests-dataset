{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "uLme | Anti-Flood",
  "version": "1.2.5",
  "description": "Anti-Flood para o bate-papo UOL",
  "permissions": [
    "https://batepapo.uol.com.br/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "antiflood.js"
      ],
      "matches": [
        "https://batepapo.uol.com.br/*"
      ]
    }
  ]
}
