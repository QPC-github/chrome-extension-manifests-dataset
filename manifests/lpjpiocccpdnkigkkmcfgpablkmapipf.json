{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Will Listen Later",
  "short_name": "WLL",
  "description": "Queue up media for later",
  "version": "0.3.0.0",
  "permissions": [
    "https://www.google.com/*",
    "https://soundcloud.com/*",
    "https://www.youtube.com/*",
    "https://youtube.com/*",
    "https://s.ytimg.com/*",
    "contextMenus",
    "contentSettings",
    "storage",
    "notifications",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://www.youtube.com/ https://s.ytimg.com/ https://w.soundcloud.com/ https://sdk.amazonaws.com/; object-src 'self'",
  "icons": {
    "16": "resources/icon16-blue.png",
    "48": "resources/icon48-blue.png",
    "128": "resources/icon128-blue.png"
  },
  "browser_action": {
    "default_icon": "resources/icon16-blue.png"
  },
  "background": {
    "scripts": [
      "js/utils.js",
      "js/main.js"
    ]
  }
}
