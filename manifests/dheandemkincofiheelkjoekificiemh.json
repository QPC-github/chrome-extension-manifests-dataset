{
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Hỗ trợ canh vé máy bay",
  "icons": {
    "128": "plane-logo.png",
    "32": "plane-logo.png",
    "64": "plane-logo.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Vé Máy Bay",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "notifications",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.1"
}
