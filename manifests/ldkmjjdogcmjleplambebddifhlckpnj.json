{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Manager Extension",
  "version": "1.0.1",
  "description": "An improved bookmark manager.",
  "background": {
    "scripts": [
      "opensMain.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs"
  ],
  "sandbox": {
    "pages": [
      "sandboxed/bookmarkSearch.html",
      "sandboxed/queryValidation.html"
    ]
  }
}
