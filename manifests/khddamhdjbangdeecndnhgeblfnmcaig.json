{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pervy Pastry Puffinator",
  "version": "1.0",
  "description": "Change all instances of \"Scottish Geeks\" to \"Pervy Pastry Puffer.\"",
  "icons": {
    "19": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "popup.js"
      ],
      "run_at": "document_end"
    }
  ]
}
