{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SizzleBurger Notifier",
  "short_name": "SizzleNotify",
  "version": "0.2.2",
  "description": "Notify you about events that are occuring at SizzleBurger.",
  "permissions": [
    "notifications",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://sizzleburger-chrome.glitch.me/*",
        "https://www.roblox.com/*"
      ],
      "js": [
        "socket.io.js",
        "contentScript.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
