{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bSites",
  "description": "A quick dashboard for Oracle Webcenter Sites.",
  "version": "1.1",
  "short_name": "bSites",
  "manifest_version": 2,
  "author": "devble.com",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "notifications",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
