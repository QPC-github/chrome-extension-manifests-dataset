{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Stay Logged In",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Provides custom notifications for inactivity. Remain active so you don't have to log in again.",
  "icons": {},
  "options_page": "userInterface.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "userInterface.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ]
}
