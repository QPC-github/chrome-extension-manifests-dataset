{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "999.md Plus",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://999.md/*",
        "https://999.md/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://999.md/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
