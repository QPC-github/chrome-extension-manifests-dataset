{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alchemy",
  "description": "Alchemy Software",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Alchemy Software"
  },
  "background": {
    "scripts": [
      "js/jquery-2.1.4.js",
      "js/ls-collection.js",
      "js/underscore.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
