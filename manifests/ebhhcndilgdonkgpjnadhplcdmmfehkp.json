{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ops Calendar",
  "description": "Take your Ops Calendar anywhere on the web, giving you easy access to items, checklists, notes, and activity.",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "images/ops-icon-19.png",
    "default_popup": "views/oc-portable.html"
  },
  "icons": {
    "16": "images/ops-icon-16.png",
    "128": "images/ops-icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "https://app.opscalendar.com/"
  ]
}
