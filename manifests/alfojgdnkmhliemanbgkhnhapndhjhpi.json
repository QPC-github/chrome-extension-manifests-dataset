{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Entryless",
  "description": "Entryless Extension",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "logoe48.png",
    "128": "logoe128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Entryless",
    "default_icon": "logoe_19.png"
  },
  "permissions": [
    "tabs"
  ]
}
