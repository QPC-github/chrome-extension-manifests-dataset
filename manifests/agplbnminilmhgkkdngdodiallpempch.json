{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Alpha.App.Net Enhanced",
  "description": "Enhance the Alpha.App.Net web app",
  "homepage_url": "https://chrome.google.com/webstore/detail/agplbnminilmhgkkdngdodiallpempch",
  "version": "0.3",
  "icons": {
    "48": "images/icon48.png",
    "16": "images/icon.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://alpha.app.net/*",
    "https://alpha.app.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://alpha.app.net/*",
        "https://alpha.app.net/*"
      ],
      "css": [
        "adnenh.css"
      ],
      "js": [
        "adnenh.js"
      ]
    }
  ]
}
