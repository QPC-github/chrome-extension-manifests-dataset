{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "贴吧自动签到",
  "description": "帮助用户一键签到常逛的百度贴吧This extension helps user to signup Baidu tieba in one click.",
  "version": "1.1.2",
  "background": {
    "scripts": [
      "signup.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "signup"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
