{
  "author": [
    {
      "github": "serhatakalin",
      "name": "serhatakalin"
    }
  ],
  "background": {
    "scripts": [
      "js/tw-gsmode-bg.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/tw-gsmode.js"
      ],
      "matches": [
        "https://twitter.com/",
        "https://twitter.com/i/cards/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Galatasaray football team mode for twitter",
  "homepage_url": "https://github.com/serhatakalin",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "32": "images/icon32.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Galatasaray Twitter Theme",
  "page_action": {
    "default_icon": "images/icon32.png",
    "default_title": "Extension will be enabled when you open twitter.com"
  },
  "permissions": [
    "https://twitter.com/",
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "web_accessible_resources": [
    "css/main.css"
  ]
}
