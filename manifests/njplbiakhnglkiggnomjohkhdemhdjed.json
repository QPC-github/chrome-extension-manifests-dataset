{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_cubie_mini__",
  "description": "__MSG_cubie_mini_description__",
  "version": "1.2.118",
  "default_locale": "en",
  "manifest_version": 2,
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  },
  "permissions": [
    "storage",
    {
      "fileSystem": [
        "write"
      ]
    },
    "unlimitedStorage",
    "webview",
    "<all_urls>",
    "notifications",
    {
      "socket": [
        "tcp-connect:*:*"
      ]
    },
    "contextMenus",
    "clipboardRead",
    "clipboardWrite",
    "idle"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "minimum_chrome_version": "29"
}
