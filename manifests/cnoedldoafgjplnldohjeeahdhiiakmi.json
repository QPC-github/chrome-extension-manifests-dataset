{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Anonymous Communication",
  "version": "1.0.0.0",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "64": "icon_64.png",
    "128": "icon_128.png",
    "256": "icon_256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
  ]
}
