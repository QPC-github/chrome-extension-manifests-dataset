{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Viirtue Call Center Stats Board Zeus",
  "description": "View call center statistics in realtime with the Virtue Call Center Board.",
  "author": "Dan Rosenrauch [dan@viirtue.com]",
  "version": "1.1.5",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-3.1.1.min.js",
      "date.js",
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "jquery-ui.min.js",
        "cs.js"
      ],
      "css": [
        "cs.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "web_accessible_resources": [
    "images/logo.jpg",
    "fonts/*"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_title": "Call Center Stats Board"
  }
}
