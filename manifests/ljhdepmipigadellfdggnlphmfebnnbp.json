{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Open Custom URL",
  "description": "Open custom URL or local file path in a popup or tab.",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "version": "1.0",
  "options_page": "options.html",
  "browser_action": {},
  "background": {
    "scripts": [
      "js/popup.js"
    ]
  }
}
