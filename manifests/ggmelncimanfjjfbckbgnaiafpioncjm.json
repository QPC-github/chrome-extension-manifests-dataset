{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JiraGo",
  "version": "1.8",
  "manifest_version": 2,
  "description": "Go directly to sf jira with your clipboard content",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "tabs",
    "notifications"
  ],
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Go!"
  },
  "web_accessible_resources": [
    "background.html",
    "background.js"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      }
    }
  }
}
