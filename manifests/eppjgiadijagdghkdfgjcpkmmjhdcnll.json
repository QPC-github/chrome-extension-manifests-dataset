{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "8chan Image expander",
  "version": "1.4.7",
  "description": "Expands images on 8chanX when hovered on.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "jquery-1.11.1.js",
        "script.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "*://8chan.co/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  }
}
