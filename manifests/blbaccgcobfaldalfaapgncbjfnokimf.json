{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cashew Digital Meetings",
  "description": "For sharing desktops while using the meeting app in CD apps.",
  "version": "1.0.0",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "cashewDigitalLogo16x16.png",
    "48": "cashewDigitalLogo48x48.png",
    "128": "cashewDigitalLogo128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meeting.normanwanto.com/*"
    ]
  }
}
