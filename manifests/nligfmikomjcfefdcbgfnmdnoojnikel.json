{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NBRB Metal Statistics",
  "description": "This extension is for collection and showing currency of Belarus National bank",
  "version": "1.0",
  "icons": {
    "19": "icons/ico_19.png",
    "32": "icons/ico_32.png",
    "128": "icons/ico_128.png"
  },
  "browser_action": {
    "default_popup": "templates/popup.html",
    "default_title": "NBRB Precious"
  },
  "permissions": [
    "http://www.nbrb.by/Services/XmlMetals.aspx"
  ]
}
