{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "ru",
  "name": "MnemonicWords plugin",
  "version": "2.6",
  "manifest_version": 2,
  "description": "MnemonicWords plugin",
  "browser_action": {
    "default_title": "MnemonicWords",
    "default_icon": "img/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "cookies",
    "http://api.mnemonicwords.com/",
    "http://api/"
  ],
  "background": {
    "page": "background_training.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self';"
}
