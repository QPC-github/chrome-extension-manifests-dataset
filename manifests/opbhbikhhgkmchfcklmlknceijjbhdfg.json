{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TnT Timeout Avoider",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "tnt-on.png"
  },
  "version": "1.1",
  "description": "Extension that works only on Dell's website and reloads the page in case you have an mbox timeout.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ]
}
