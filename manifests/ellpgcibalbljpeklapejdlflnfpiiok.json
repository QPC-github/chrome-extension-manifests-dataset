{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Visual Enhancements for Trello",
  "version": "1.0.0.2",
  "description": "Background colours and headers for Trello cards",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
