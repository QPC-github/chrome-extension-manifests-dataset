{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Newindianexpress",
  "version": "1.1",
  "description": "Latest news from New Indian Express.",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/nie16.png",
      "32": "images/nie32.png",
      "48": "images/nie48.png",
      "128": "images/nie128.png"
    }
  },
  "icons": {
    "16": "images/nie16.png",
    "32": "images/nie32.png",
    "48": "images/nie48.png",
    "128": "images/nie128.png"
  },
  "manifest_version": 2
}
