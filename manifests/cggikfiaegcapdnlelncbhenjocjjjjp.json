{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Dictionary"
  },
  "name": "Quick Define & Synonym Search",
  "description": "Searches Google Definitions and Web Definitions, opens results in a new tab.",
  "homepage_url": "http://refreshingdesign.biz",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1",
  "manifest_version": 2
}
