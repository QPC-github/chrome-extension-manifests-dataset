{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Haverfordonyms",
  "version": "0.2",
  "description": "Haverfordize your browsing experience!",
  "author": "jkeat",
  "icons": {
    "128": "haverford_128.png",
    "48": "haverford_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
