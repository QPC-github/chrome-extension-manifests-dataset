{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Night Mode - Hacker News",
  "version": "0.0.2",
  "description": "Night Mode for Hacker News (news.ycombinator.com)",
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "ycombinator.js"
      ]
    }
  ],
  "icons": {
    "48": "icon.png",
    "96": "icon@2x.png"
  }
}
