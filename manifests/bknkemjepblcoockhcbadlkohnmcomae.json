{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "face/off",
  "version": "1.0.1",
  "description": "Remove emoticons from your favorite social networking site. (Does not affect chat.)",
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/userstyle.css"
      ],
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ]
    }
  ]
}
