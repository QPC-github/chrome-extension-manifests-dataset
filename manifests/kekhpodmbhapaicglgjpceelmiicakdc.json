{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ayo Education screen sharing",
  "author": "Ayo Education S.A.P.I. de C.V.",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Extensión de Chrome desarrollada para https://app.ayo.com.mx/. Está extensión únicamente captura el contenido de tu pantalla.",
  "homepage_url": "https://www.ayo.com.mx/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.enacment.space/*",
      "https://*.ayo.com.mx/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
