{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Go to Sleep",
  "description": "Go to sleep!",
  "version": "1.2",
  "icons": {
    "16": "icons/zzz.png",
    "48": "icons/zzz.png",
    "128": "icons/icons128.png",
    "168": "icons/zzz.png"
  },
  "browser_action": {
    "default_icon": "icons/zzz.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": false
  }
}
