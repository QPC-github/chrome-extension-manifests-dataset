{
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "*://*.therodinhoods.com/*",
        "*://therodinhoods.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Makes your TheRodinHoods.com experience better :)",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "The RodinHoods - Beautifier",
  "permissions": [
    "tabs",
    "*://*.therodinhoods.com/*",
    "*://therodinhoods.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1.7",
  "web_accessible_resources": [
    "custom.js",
    "fonts.css"
  ]
}
