{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Imgur images downloader",
  "description": "下載伊莉的圖片",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "web_accessible_resources": [
    "jquery-1.11.3.min.js",
    "main.js",
    "dist/jszip.js",
    "FileSaver.js",
    "dist/jszip-utils.js"
  ]
}
