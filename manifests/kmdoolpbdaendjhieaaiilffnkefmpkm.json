{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Update/Add Comic",
  "description": "Updates or Adds a website to the bookmark folder named 'Comics'. If the folder doesn't exist it will be created.",
  "version": "1.1.1",
  "permissions": [
    "contextMenus",
    "bookmarks"
  ],
  "icons": {
    "16": "icon-bitty.png",
    "48": "icon-small.png",
    "128": "icon-large.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
