{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "NullDev",
  "name": "Snippeter",
  "description": "Export StackOverflow.com snippets to JSFiddle.net",
  "version": "1.2.1",
  "short_name": "Snippeter",
  "browser_action": {
    "default_icon": "icon/icon.png",
    "default_popup": "popup/popup.html",
    "default_title": "Snippeter"
  },
  "icons": {
    "128": "icon/icon-big.png",
    "16": "icon/icon-big.png",
    "48": "icon/icon-big.png"
  },
  "content_scripts": [
    {
      "js": [
        "lib/jquery-3.2.1.min.js",
        "content/content-SO.js"
      ],
      "css": [
        "content/custom-SO.css"
      ],
      "matches": [
        "*://stackoverflow.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://stackoverflow.com/*",
    "tabs",
    "storage"
  ]
}
