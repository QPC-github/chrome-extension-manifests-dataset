{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "motivation.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "dashboard.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Real Time counter showing your current age and inspirational quotes on new tabe page",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-128.png",
    "48": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "Inspiration",
  "offline_enabled": true,
  "version": "1.1",
  "permissions": [
    "http://*/*",
    "tabs"
  ]
}
