{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Get QR of page"
  },
  "description": "A simple way to get QR code of any page (by SimpleCode / new.pclenta.ru / developer - Davit Petrosyan).",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "SimpleCode QRer",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "version": "1.1"
}
