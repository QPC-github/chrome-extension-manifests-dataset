{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Neko Paw",
  "version": "0.3",
  "description": "Быстрый переход на Неконяш. Так же помогает сохранять на Неконяше картинки и видео.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "ru",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Goto Neko Nyah"
  },
  "web_accessible_resources": [
    "images/*.png"
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "contextMenus",
    "cookies",
    "fileBrowserHandler",
    "idle",
    "notifications",
    "tabs",
    "unlimitedStorage",
    "activeTab"
  ]
}
