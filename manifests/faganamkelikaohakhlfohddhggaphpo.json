{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "description": "Find For Me is a free right click searcher extension for chrome.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "minimum_chrome_version": "6",
  "name": "Find For Me: Free right click searcher",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0"
}
