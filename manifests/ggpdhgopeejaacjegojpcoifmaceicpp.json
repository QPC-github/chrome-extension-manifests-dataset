{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trekify (for Imgur)",
  "version": "1.0.0",
  "description": "Changes the \"take me up\" button.",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "64": "64x64.png",
    "128": "128x128.png"
  },
  "author": "QQ7ActiveJGamma",
  "content_scripts": [
    {
      "matches": [
        "http://imgur.com/gallery/*",
        "http://imgur.com/r/*",
        "https://imgur.com/gallery/*",
        "https://imgur.com/r/*"
      ],
      "js": [
        "chromesucks.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "Trekify.user.js"
  ],
  "homepage_url": "https://imgur.com",
  "incognito": "split",
  "minimum_chrome_version": "51",
  "short_name": "StarTrek+Imgur",
  "version_name": "1.0.0"
}
