{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The_Donald Block",
  "description": "Block and downvote all T_D posts",
  "version": "1.0",
  "author": "Burak Aslan",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery-3.1.1.min.js",
        "background.js"
      ],
      "matches": [
        "https://reddit.com/r/all/*",
        "https://www.reddit.com/r/all/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Block and downvote all T_D posts"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "manifest_version": 2
}
