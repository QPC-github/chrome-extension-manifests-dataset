{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kenweego, Add this page to your travel board",
  "description": "This extension will Import any website to your travel board",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "add.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
