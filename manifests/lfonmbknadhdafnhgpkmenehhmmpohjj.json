{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZedExt",
  "description": "Set exist proxy",
  "version": "1.7",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "window.html"
  },
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  }
}
