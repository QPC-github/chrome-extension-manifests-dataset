{
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "We review electric shavers, beard trimmers & all other men's grooming products & provide detailed buying guides.",
  "manifest_version": 2,
  "name": "Male Sense Pro",
  "icons": {
    "128": "malesensepro.png",
    "16": "malesensepro.png",
    "48": "malesensepro.png"
  },
  "permissions": [
    "tabs",
    "ActiveTab",
    "https://www.malesensepro.com/*",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
