{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Viewer ServeurMumble.com",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Visualisez le viewer de votre serveur Mumble.",
  "options_page": "options.html",
  "web_accessible_resources": [
    "viewer.js"
  ],
  "permissions": [
    "notifications",
    "storage"
  ],
  "icons": {
    "32": "mumble_option.png",
    "48": "mumble_option.png",
    "128": "mumble_option.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "mumble_ico.png"
    },
    "default_title": "Viewer ServeurMumble.com",
    "default_popup": "viewer.html"
  }
}
