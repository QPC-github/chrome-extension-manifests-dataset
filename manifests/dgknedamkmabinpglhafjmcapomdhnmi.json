{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Decodificador de Binario",
  "description": "Traduce Binario a Texto dentro de cualquier p√°gina web.",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "injectable.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
