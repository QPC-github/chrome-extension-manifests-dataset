{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "cheevo.io opener",
  "version": "1.0",
  "description": "Open movie page on cheevo right from the imdb.com",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {},
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
