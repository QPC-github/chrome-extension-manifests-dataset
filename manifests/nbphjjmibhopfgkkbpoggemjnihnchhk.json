{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spekit",
  "short_name": "Spekit",
  "version": "6.0.1",
  "manifest_version": 2,
  "description": "Your all-in-one Salesforce training and documentation app.",
  "key": "",
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_title": "Spekit"
  },
  "permissions": [
    "tabs",
    "notifications",
    "webNavigation",
    "<all_urls>",
    "contextMenus",
    "alarms"
  ],
  "web_accessible_resources": [
    "browser_action.html",
    "images/*",
    "styles/*",
    "fonts/*",
    "js/browserAction.js",
    "firebase-messaging-sw.js"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://cdn.segment.com/analytics.js/v1/ZpYfuErrPdliP3KJV5RDPwTSCqUoK4y5/analytics.min.js https://www.gstatic.com/firebasejs/4.8.1/firebase-app.js https://www.gstatic.com/firebasejs/4.8.1/firebase-messaging.js; object-src 'self'",
  "version_name": "6.0.1"
}
