{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "thang van ung",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/each-hi.png",
    "default_title": "MeiZhong"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/common.min.css",
        "css/shared.css",
        "css/jstd.css"
      ],
      "js": [
        "minified/utils.min.js",
        "minified/zhoChat.min.js",
        "minified/flexbug.min.js",
        "minified/ellipser.min.js",
        "minified/parakeys.min.js",
        "js/jquery-3.4.1.slim.min.js",
        "minified/draggable.min.js",
        "js/stenopad.js",
        "minified/content.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Any Chinese word...",
  "homepage_url": "https://github.com/thang-ung/",
  "icons": {
    "128": "images/each.png",
    "16": "images/each.png",
    "48": "images/each.png"
  },
  "manifest_version": 2,
  "name": "MeiZhong",
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "short_name": "MeiZhong",
  "version": "1.0.11",
  "web_accessible_resources": [
    "css/*",
    "js/*",
    "images/*"
  ]
}
