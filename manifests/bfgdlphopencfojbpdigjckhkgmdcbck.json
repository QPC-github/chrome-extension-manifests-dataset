{
  "author": "ane_400124",
  "background": {
    "persistent": true,
    "scripts": [
      "/Scripts/Framework/jquery-3.2.1.js",
      "/Scripts/ExtensionOnly/moment.min.js",
      "/Scripts/ExtensionShared/Models/Article.js",
      "/Scripts/ExtensionShared/Models/Feed.js",
      "/Scripts/ExtensionShared/Models/Section.js",
      "/Scripts/ExtensionShared/NewsTab.js",
      "/Scripts/ExtensionShared/Helpers.js",
      "/Scripts/ExtensionShared/Modules/RegionManager.js",
      "/Scripts/ExtensionShared/Modules/SectionsManager.js",
      "/Scripts/ExtensionOnly/Modules/TimeAndWeatherWidget.js",
      "/Scripts/ExtensionOnly/Modules/SportWidget.js",
      "/Scripts/ExtensionOnly/config.js",
      "/Scripts/ExtensionOnly/article.js",
      "/Scripts/ExtensionOnly/common.js",
      "/Scripts/ExtensionOnly/Extension.js",
      "/Scripts/ExtensionOnly/Background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/Media/ExtensionImages/Icons/mlb_orange_icon48.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "/Styles/ExtensionOnly/inject.css"
      ],
      "matches": [
        "*://*/*"
      ]
    },
    {
      "all_frames": true,
      "js": [
        "/Scripts/Framework/jquery-3.2.1.js",
        "/Scripts/ExtensionOnly/config.js",
        "/Scripts/ExtensionOnly/inject.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://suggestqueries.google.com https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "Get top Houston Astros news on your Chrome new tab page. Add any topic or news source.",
  "externally_connectable": {
    "matches": [
      "*://*.newstab.com/*"
    ]
  },
  "homepage_url": "http://www.newstab.com/",
  "icons": {
    "128": "/Media/ExtensionImages/Icons/mlb_orange_icon128.png",
    "16": "/Media/ExtensionImages/emptyIcon.png",
    "19": "/Media/ExtensionImages/Icons/mlb_orange_icon19.png",
    "48": "/Media/ExtensionImages/Icons/mlb_orange_icon48.png",
    "96": "/Media/ExtensionImages/Icons/mlb_orange_icon96.png"
  },
  "manifest_version": 2,
  "name": "Houston Astros News Tab",
  "permissions": [
    "cookies",
    "tabs",
    "topSites",
    "storage",
    "chrome://favicon/",
    "<all_urls>"
  ],
  "short_name": "Houston Astros News Tab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1.0"
}
