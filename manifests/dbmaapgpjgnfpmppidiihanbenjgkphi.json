{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "short_name": "__MSG_shortName__",
  "default_locale": "en",
  "version": "0.99.7",
  "manifest_version": 2,
  "permissions": [
    "management",
    "topSites",
    "http://leopon.org/*",
    "http://leopon.org/wp-admin/post.php?post=*",
    "http://leopon.org/wp-admin/*",
    "chrome://favicon/",
    "history",
