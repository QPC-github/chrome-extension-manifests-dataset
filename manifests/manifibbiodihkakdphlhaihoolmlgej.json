{
  "author": "Knecht Reisen AG",
  "background": {
    "persistent": false,
    "scripts": [
      "background-config.js",
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://www.beratung.knecht-reisen.ch/*"
      ]
    }
  ],
  "description": "Screensharing Extension for Knecht Reisen Beratung",
  "homepage_url": "https://www.knecht-reisen.ch",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Screensharing Ext Knecht Reisen Beratung",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.12.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
