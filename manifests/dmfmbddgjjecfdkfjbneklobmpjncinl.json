{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Toilet Paper & Menstrual Hygiene Products",
  "short_name": "TP&MHP",
  "version": "1.1",
  "description": "Adds menstrual hygiene products everywhere toilet paper appears in your browser",
  "browser_action": {
    "default_icon": "images/TPnMHP48.png"
  },
  "icons": {
    "16": "images/TPnMHP16.png",
    "48": "images/TPnMHP48.png",
    "128": "images/TPnMHP128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
