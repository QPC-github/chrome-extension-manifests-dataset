{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gphone",
  "description": "CRM Integration for Gphone Hosted PBX",
  "short_name": "Gphone",
  "version": "0.13.42",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/Untitle.png"
    }
  },
  "content_security_policy": "script-src 'self' https://maxcdn.bootstrapcdn.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "content1.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "all_frames": true
  },
  "web_accessible_resources": [
    "images/*.png"
  ],
  "icons": {
    "16": "images/Untitle.png",
    "48": "images/Untitle.png"
  }
}
