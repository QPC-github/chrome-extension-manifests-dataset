{
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/check.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon96.png",
    "default_popup": "popup.html",
    "default_title": "TJPC"
  },
  "description": "Cette extension te permet d'etre informer des tous dernier crack et de nous soutenir facilement !",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png"
  },
  "manifest_version": 2,
  "name": "TJPC",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "storage",
    "*://*.amazon.fr/*",
    "https://www.googleapis.com/youtube/v3/*",
    "https://api.twitch.tv/kraken/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
