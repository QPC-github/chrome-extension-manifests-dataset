{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Peepshow",
  "version": "2",
  "manifest_version": 2,
  "description": "Resize and popout a browser window to show a single element.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "name": "Peep",
    "default_icon": "icon19.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "inspector.js",
    "inspector.png",
    "jquery-1.7.1.min.js"
  ]
}
