{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Subdivx Search",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Buscador rapido de subtitulos en subdivx",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*.subdivx.com/*"
  ]
}
