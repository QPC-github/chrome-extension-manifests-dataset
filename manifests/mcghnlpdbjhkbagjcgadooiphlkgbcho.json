{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CoInvest Matcher",
  "description": "CoInvest â€“ Investor's Matcher on AngelList.",
  "version": "1.0.3",
  "content_scripts": [
    {
      "matches": [
        "*://angel.co/*"
      ],
      "css": [
        "fix.css",
        "jPushMenu.css"
      ],
      "js": [
        "jquery.js",
        "content.js",
        "jPushMenu.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://*.azurewebsites.net/*"
    ]
  },
  "permissions": [
    "tabs",
    "*://angel.co/*",
    "*://*.azurewebsites.net/*",
    "notifications",
    "https://www.googleapis.com/auth/userinfo.email",
    "https://www.google.com/accounts/*",
    "https://www.google.com/",
    "https://accounts.google.com/*",
    "http://*.appollocreative.com/*",
    "*://api.angel.co/*",
    "cookies",
    "http://*.google.com/"
  ],
  "web_accessible_resources": [
    "investor_bg.png",
    "logo.png",
    "loading.gif",
    "Avenir_Light.eot",
    "Avenir_Light.woff",
    "Avenir_Light.ttf",
    "Avenir_Light.svg"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
