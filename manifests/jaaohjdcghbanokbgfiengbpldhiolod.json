{
  "manifest_version": 2,
  "name": "AdBlaster1",
  "description": "AdBlaster1 stops annoying ads from ruining your browsing experience.",
  "version": "6.85",
  "background": {
    "scripts": [
      "punycode.min.js",
      "jquery/jquery.min.js",
      "uiscripts/jquery-ui-drgbl.min.js",
      "port.js",
      "functions.js",
      "stats.js",
      "survey.js",
      "filtering/domainset.js",
      "filtering/filteroptions.js",
      "filtering/filtertypes.js",
      "filtering/filterset.js",
      "filtering/myfilters.js",
      "filtering/filternormalizer.js",
      "idlehandler.js",
      "dropbox-datastores.js",
      "gab_question.js",
      "background.js",
      "datacollection.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "bandaids.js"
      ],
      "matches": [
        "*://*.mail.live.com/*",
        "*://*.mastertoons.com/*",
        "*://*.mobilmania.cz/*",
        "*://*.zive.cz/*",
        "*://*.doupe.cz/*",
        "*://*.e15.cz/*",
        "*://*.sportrevue.cz/*",
        "*://*.autorevue.cz/*",
        "*://*.youtube.com/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "js": [
        "port.js",
        "functions.js",
        "jquery/jquery.min.js",
        "filtering/filteroptions.js",
        "adblock_start_common.js",
        "adblock_start_chrome.js",
        "uiscripts/jquery-ui-drgbl.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "uiscripts/blacklisting/rightclick_hook.js",
        "notificationoverlay.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "button/popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; style-src 'self' 'unsafe-inline' 'unsafe-eval'; connect-src *",
  "default_locale": "en",
  "incognito": "spanning",
  "minimum_chrome_version": "23",
  "optional_permissions": [
    "management",
    "tabs",
    "activeTab",
    "storage"
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs",
    "idle",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "alarms",
    "storage",
    "notifications"
  ],
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "options_page": "options/index.html",
  "web_accessible_resources": [
    "img/icon24.png",
    "Frame.html",
    "img/icon48.png",
    "jquery/css/images/ui-bg_inset-hard_100_fcfdfd_1x100.png",
    "jquery/css/images/ui-icons_056b93_256x240.png",
    "jquery/css/images/ui-icons_d8e7f3_256x240.png",
    "jquery/css/jquery-ui.custom.css",
    "jquery/css/override-page.css",
    "chrome_oauth_receiver.html"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
