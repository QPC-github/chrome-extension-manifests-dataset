{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SaveMyBookmarks",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "web_accessible_resources": [
    "res/*",
    "assets/*",
    "icons/*"
  ],
  "description": "Save and categories your bookmarks on one place and eccess them easily.",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://www.savemybookmarks.com/",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icons/button.png",
    "default_title": "Save My Bookmarks"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation"
  ]
}
