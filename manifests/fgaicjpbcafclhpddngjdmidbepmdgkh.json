{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Signature Inbox by @spookev",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Ajoute un signature Ã  vos mail sous Inbox - kevin@mailperso.fr",
  "browser_action": {
    "default_title": "Signature",
    "default_icon": "icon.png"
  },
  "icons": {
    "19": "icon.png",
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://inbox.google.com/*",
    "https://inbox.google.com/*",
    "storage",
    "unlimitedStorage"
  ]
}
