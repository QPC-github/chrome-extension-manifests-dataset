{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page2Images Pin Full Page (PInterest)",
  "short_name": "Pin Full Page",
  "description": "It will take the current browser page, snapshot it and send to Pinterest as a new pin. give the best and real emulator.",
  "version": "1.0.2",
  "background": {
    "scripts": [
      "p2i.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "p2i_logo.png",
    "48": "p2i_logo.png",
    "128": "p2i_logo.png"
  },
  "browser_action": {
    "default_title": "Page2Images Pin Full Page (PIntertest)",
    "default_icon": "p2i_logo.png"
  },
  "manifest_version": 2
}
