{
  "background": {
    "scripts": [
      "/js/jquery-3.1.0.min.js",
      "/js/moment.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "description": "Extension to keep track of BTC price!",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "BTC Alert",
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "webNavigation",
    "storage",
    "background",
    "notifications",
    "webRequest",
    "http://*.google.com/*",
    "https://*.google.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1"
}
