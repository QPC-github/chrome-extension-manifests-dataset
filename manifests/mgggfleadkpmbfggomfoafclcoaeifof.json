{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CMUDirectory",
  "author": "Chad Hilton",
  "version": "1.3.0",
  "short_name": "CMU Directory Search",
  "icons": {
    "128": "images/cmu_128.png"
  },
  "description": "Search the Carnegie Mellon University directory.",
  "browser_action": {
    "default_icon": {
      "19": "images/cmu_19.png",
      "38": "images/cmu_38.png"
    },
    "default_popup": "search.html"
  },
  "background": {
    "scripts": [
      "js/jquery-3.2.1.min.js",
      "js/popup.js"
    ]
  }
}
