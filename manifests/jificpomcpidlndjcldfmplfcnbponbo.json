{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AlibabaOnline.vn Đặt hàng trực tuyến",
  "description": "Hổ trợ đặt hàng trực tuyến các trang bán hàng Trung Quốc",
  "version": "5.9",
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "https://*/",
    "http://*/",
    "https://apis.google.com/",
    "https://alibabaonline.vn/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "scripts/jquery.min.js",
      "scripts/default_function.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "AlibabaOnline.vn Đặt hàng trực tuyến",
    "default_icon": "icon128.png"
  },
  "web_accessible_resources": [
    "template/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "short_name": "Đặt hàng",
  "manifest_version": 2
}
