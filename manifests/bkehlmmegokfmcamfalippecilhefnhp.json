{
  "background": {
    "page": "background/index.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Pink HD Wallpapers New Tab Theme",
    "default_icon": {
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "content_scripts": [
    {
      "js": [
        "content-scripts/installed.js",
        "content-scripts/event-handler.js"
      ],
      "matches": [
        "*://*.glamilife.com/*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.glamilife.com/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://*.ytimg.com  https://www.googleapis.com https://*.ytimg.com https://*.glamilife.com https://*.mystart.com https://*.mystartcdn.com https://*.vmn.net https://api.facebook.com https://*.twitter.com; object-src 'self'",
  "description": "Install this theme and enjoy HD wallpapers of Pink every time you open a new tab.",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "64": "icons/64.png"
  },
  "manifest_version": 2,
  "name": "Pink HD Wallpapers New Tab Theme",
  "offline_enabled": true,
  "permissions": [
    "alarms",
    "bookmarks",
    "management",
    "chrome://favicon/",
    "storage",
    "tabs",
    "topSites",
    "unlimitedStorage",
    "*://*.mystart.com/*",
    "*://*.glamilife.com/*"
  ],
  "short_name": "Pink",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.19.42"
}
