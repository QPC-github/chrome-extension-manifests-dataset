{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TOPER - Лента увлекательных материалов",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Ты онлайн",
    "default_icon": "icons/19.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://toper.club/*"
      ],
      "js": [
        "js.js"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2
}
