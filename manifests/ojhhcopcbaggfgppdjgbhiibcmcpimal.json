{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "img/19.png"
    },
    "default_popup": "popup.html",
    "default_title": "Oyuntadi.com Yeni Oyunlar"
  },
  "content_security_policy": "script-src 'self' https://www.google.com/jsapi https://ajax.googleapis.com; object-src 'self'",
  "description": "Oyuntadi.com 'da, farklı kategorilere ayrılmış binlerce oyunu hemen oyna!",
  "icons": {
    "100": "img/100.png",
    "128": "img/128.png",
    "19": "img/19.png",
    "48": "img/48.png",
    "50": "img/50.png",
    "96": "img/96.png"
  },
  "manifest_version": 2,
  "name": "Oyuntadi.com Yeni Oyunlar",
  "permissions": [
    "notifications",
    "webRequest",
    "*://*.oyuntadi.com/",
    "*://*.google.com/",
    "http://*/*",
    "https://*/*",
    "background"
  ],
  "version": "1.0"
}
