{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Closing notes",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Extension meant to make it easier when closing the tickets",
  "icons": {
    "48": "pencil.png"
  },
  "background": {
    "scripts": [
      "background3.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Created by Kyevi Gentil.",
    "default_icon": "icon4.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
