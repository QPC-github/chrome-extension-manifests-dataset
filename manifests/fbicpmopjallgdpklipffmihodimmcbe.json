{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Right click selected text to search for books on Goodreads.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png"
  },
  "minimum_chrome_version": "6",
  "name": "Goodreads Right Click",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "version": "1.0.1",
  "manifest_version": 2
}
