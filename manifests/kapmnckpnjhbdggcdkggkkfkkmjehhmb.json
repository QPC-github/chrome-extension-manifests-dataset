{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Merch Research - Free",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "Highlight & Search ALL Merch By Amazon +USPTO",
  "icons": {
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/jquery/jquery.min.js",
      "src/bg/search.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "MerchResearch"
  },
  "homepage_url": "http://merchresearch.com/",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
  ]
}
