{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "fontspring-128.png",
    "default_popup": "index.html",
    "default_title": "FontSpring Search"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Search new fonts on FontSpring.com and open the results in a new tab.",
  "icons": {
    "128": "fontspring-128.png",
    "64": "fontspring-64.png",
    "48": "fontspring-48.png",
    "32": "fontspring-32.png",
    "16": "fontspring-16.png"
  },
  "manifest_version": 2,
  "name": "FontSpring Search",
  "permissions": [
    "contextMenus",
    "contextMenus",
    "tabs",
    "https://www.fontspring.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
