{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks",
  "description": "Best bookmark manager",
  "version": "1.4.14",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks",
    "topSites"
  ]
}
