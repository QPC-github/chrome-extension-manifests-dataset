{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ultimate SPOJ Rank Tracker",
  "description": "Extension which checks SPOJ user's rank.",
  "version": "1.0",
  "permissions": [
    "http://*.spoj.com/*",
    "http://*.mariosoft.pl/spoj/*",
    "http://*.brspoj.rro.im/user/*",
    "alarms"
  ],
  "icons": {
    "88": "spoj88.png",
    "128": "spoj128.png"
  },
  "browser_action": {
    "default_icon": "spoj88.png",
    "default_title": "Ultimate SPOJ Rank Tracker by kokosek",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-1.10.2.js",
      "background.js"
    ],
    "persistent": true
  }
}
