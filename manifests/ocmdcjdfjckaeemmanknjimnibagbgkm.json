{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JIRA Issue Assignment Notifier",
  "description": "This extension notifies the users when a JIRA Issue is assigned.",
  "version": "1.3",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "permissions": [
    "*://*/*",
    "cookies",
    "tabs",
    "notifications"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "scripts/jq.js",
      "scripts/notifier.js"
    ]
  }
}
