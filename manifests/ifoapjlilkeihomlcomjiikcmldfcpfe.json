{
  "manifest_version": 2,
  "version": "3.0",
  "chrome_url_overrides": {
    "newtab": "pages/index.html"
  },
  "browser_action": {
    "default_icon": "/img/icone/128.png",
    "default_title": "Full HD NewTab"
  },
  "description": "Get newtab background theme with HD wallpapers for every fan of Tomb Raider 2013 Game.",
  "icons": {
    "128": "/img/icone/128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "chrome-themes-hd",
      "name": "chrome-themes-hd",
      "favicon_url": "http://chrome-themes-hd.com/favicon.ico",
      "search_url": "http://chrome-themes-hd.com/search?q={searchTerms}",
      "is_default": true,
      "encoding": "UTF-8"
    }
  },
  "name": "Tomb Raider 2013 Game Wallpapers",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "contextMenus",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "topSites"
  ],
  "short_name": "Full HD NewTab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "pages/index.html"
  ]
}
