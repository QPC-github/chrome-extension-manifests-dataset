{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icons/rana16.png",
    "default_title": "Frog Bar",
    "default_popup": "src/main.html"
  },
  "icons": {
    "16": "icons/rana16.png",
    "48": "icons/rana48.png",
    "128": "icons/rana128.png"
  },
  "manifest_version": 2,
  "name": "Frog Bar",
  "description": "Barra de herramientas",
  "permissions": [
    "webNavigation",
    "notifications",
    "storage",
    "contentSettings",
    "contextMenus",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com https://*.firebaseio.com; object-src 'self'; ",
  "web_accessible_resources": [
    "js/firebase.js",
    "js/handler.js",
    "css/style.css"
  ],
  "version": "1.0"
}
