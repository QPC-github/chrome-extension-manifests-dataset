{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cat Mario unblocked game",
  "version": "1.0",
  "description": "Cat Mario is a great unblocked game inspired by the classic Super Mario game",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "unlimitedStorage",
    "tabs",
    "storage",
    "https://search.yahoo.com/*",
    "https://us.search.yahoo.com/*",
    "https://www.google.com/*",
    "https://www.google.us/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open Cat Mario game",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
