{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FoxTab",
  "description": "FoxTab is a clean new tab extension for your favorite pages and quick access to downloads, bookmarks, browsing history and apps.",
  "version": "1.0.0.2",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "foxtab.html"
  },
  "icons": {
    "128": "icons/fox_128X128.png",
    "16": "icons/fox_16X16.png",
    "23": "icons/fox_23X23.png",
    "32": "icons/fox_32X32.png",
    "48": "icons/fox_48X48.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "management",
    "history",
    "chrome://favicon/",
    "bookmarks",
    "tabs",
    "*://*/*",
    "contextMenus",
    "notifications",
    "topSites",
    "unlimitedStorage"
  ]
}
