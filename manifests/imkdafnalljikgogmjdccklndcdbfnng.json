{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "aprilFools",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Harmlessly goof up your co-workers browser.",
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "localstorage.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "effects.css"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "process.js"
      ],
      "run_at": "document_end"
    }
  ]
}
