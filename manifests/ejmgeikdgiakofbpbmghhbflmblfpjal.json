{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Simplest Todo App",
  "version": "1",
  "manifest_version": 2,
  "description": "This is a simplest todo app intended to keep it easiest to use.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Simplest Todo App",
    "default_popup": "popup.html"
  }
}
