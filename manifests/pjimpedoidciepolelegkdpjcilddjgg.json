{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zdielanie pracovnej plochy pre Jitsi Meet",
  "description": "Jednoduché rozšírenie pre zdielanie Vašej pracovnej plochy cez Jitsi Meet na stranke meet.qity.sk",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "jitsi-logo-16x16.png",
    "48": "jitsi-logo-48x48.png",
    "128": "jitsi-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.qity.sk/*"
    ]
  }
}
