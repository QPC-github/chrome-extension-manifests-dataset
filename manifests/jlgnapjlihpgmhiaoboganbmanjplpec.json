{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ToDo List",
  "version": "1.0",
  "description": "Add your task list",
  "permissions": [
    "browsingData",
    "background",
    "clipboardRead",
    "clipboardWrite",
    "history",
    "topSites",
    "webRequest",
    "webNavigation",
    "notifications",
    "browsingData",
    "browsingData",
    "browsingData",
    "https://www.krishikul.com"
  ],
  "browser_action": {
    "default_title": "ToDo List",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
