{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wencor.com order status check button",
  "short_name": "Wencor orders",
  "description": "Helps you to check order status at Wencor.com",
  "version": "1.0",
  "icons": {
    "128": "media/OrderIcon_2_128x128.png",
    "48": "media/OrderIcon_2_48x48.png",
    "16": "media/OrderIcon_2_16x16.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "38": "media/OrderIcon_2_38x38.png",
      "19": "media/OrderIcon_2_19x19.png"
    },
    "default_title": "Click here to check order status at Wencor.com",
    "default_popup": "popup.html"
  }
}
