{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BetterCourant",
  "version": "1.0",
  "description": "MediaCourant, maar dan mooi",
  "content_scripts": [
    {
      "matches": [
        "http://www.mediacourant.nl/*",
        "http://mediacourant.nl/*"
      ],
      "css": [
        "default.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.mediacourant.nl/?p=*",
        "http://mediacourant.nl/?p=*"
      ],
      "css": [
        "page.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
