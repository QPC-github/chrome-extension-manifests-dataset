{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Debrid-Link",
  "description": "__MSG_extensionDescription__",
  "author": "Debrid-Link",
  "version": "3.13.0",
  "minimum_chrome_version": "42.0.0",
  "permissions": [
    "downloads",
    "https://*/*",
    "http://*/*",
    "tabs",
    "contextMenus",
    "clipboardWrite",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon-transparent-128.png",
      "48": "images/icon-transparent-48.png",
      "40": "images/icon-transparent-40.png",
      "38": "images/icon-transparent-38.png",
      "35": "images/icon-transparent-35.png",
      "32": "images/icon-transparent-32.png",
      "30": "images/icon-transparent-30.png",
      "25": "images/icon-transparent-25.png",
      "20": "images/icon-transparent-20.png",
      "19": "images/icon-transparent-19.png",
      "16": "images/icon-transparent-16.png"
    }
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/inject-concat.js"
      ],
      "css": [
        "css/inject-concat.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://debrid-link.fr/webapp/stream/*",
        "https://debrid-link.com/webapp/stream/*"
      ],
      "js": [
        "js/inject/streamAuthWebapp.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "180": "images/icon-180.png",
    "128": "images/icon-128.png",
    "48": "images/icon-48.png",
    "40": "images/icon-40.png",
    "32": "images/icon-32.png",
    "30": "images/icon-30.png",
    "25": "images/icon-25.png",
    "20": "images/icon-20.png",
    "16": "images/icon-16.png"
  },
  "web_accessible_resources": [
    "images/icon-transparent-128.png",
    "images/icon-transparent-48.png",
    "images/icon-transparent-40.png",
    "images/icon-transparent-38.png",
    "images/icon-transparent-35.png",
    "images/icon-transparent-32.png",
    "images/icon-transparent-30.png",
    "images/icon-transparent-25.png",
    "images/icon-transparent-20.png",
    "images/icon-transparent-19.png",
    "images/icon-transparent-16.png",
    "images/icon-180.png",
    "images/icon-128.png",
    "images/icon-48.png",
    "images/icon-40.png",
    "images/icon-32.png",
    "images/icon-30.png",
    "images/icon-25.png",
    "images/icon-20.png",
    "images/icon-16.png",
    "player.html"
  ],
  "default_locale": "en"
}
