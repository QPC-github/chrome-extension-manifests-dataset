{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Coachforce Testing",
  "author": "Coachforce",
  "version": "2.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome ext is developed primary for https://coachforce.in/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://coachforce.in/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://coachforce.in/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
