{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "null44",
  "background": {
    "persistent": false,
    "scripts": [
      "lib/jquery-2.1.4.min.js",
      "background.js"
    ]
  },
  "description": "ひまストアラートのChrome Extensions版です。",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "ひまストアラート on Google Chrome",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "himast alert"
  },
  "permissions": [
    "http://himast.in/",
    "alarms",
    "tabs",
    "storage",
    "notifications"
  ],
  "version": "1.2.0"
}
