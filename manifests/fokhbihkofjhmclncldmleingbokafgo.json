{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iframeMonitor",
  "version": "1.0",
  "description": "Remove full-page popup advertisments triggered from iframe clicks.",
  "manifest_version": 2,
  "icons": {
    "16": "syringe_icon.png",
    "48": "ext_icon48.png",
    "128": "ext_icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "remove_popup.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "syringe_icon.png",
    "default_title": "iframe Monitor"
  }
}
