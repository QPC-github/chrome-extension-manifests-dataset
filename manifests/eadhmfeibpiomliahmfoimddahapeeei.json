{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "bid-Watcher Extension",
  "description": "This extension help you add/delete and get notified about ending watched auctions on www.bid-watcher.com.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/logo.png"
  },
  "permissions": [
    "tabs",
    "http://www.bid-watcher.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
