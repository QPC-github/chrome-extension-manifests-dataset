{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GMark This",
  "description": "A simple and elegant way to integrate Chrome and Google Bookmarks",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "gmark"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "GMark This"
  },
  "permissions": [
    "tabs"
  ]
}
