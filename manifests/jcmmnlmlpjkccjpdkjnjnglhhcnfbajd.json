{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extended Degiro summary",
  "description": "Displays some missing account data on Degiro dashboard.",
  "version": "3.2",
  "manifest_version": 2,
  "permissions": [
    "https://trader.degiro.nl/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://trader.degiro.nl/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon-128.png",
    "246": "icon-246.jpg"
  }
}
