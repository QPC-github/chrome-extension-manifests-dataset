{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Internet Surfing Profile",
  "short_name": "web profile",
  "description": "This extension will access History to show you usgae and url to recommend ethical usage of internet",
  "version": "4.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "history16.png",
      "32": "history32.png",
      "128": "history128.png"
    }
  },
  "icons": {
    "16": "history16.png",
    "32": "history32.png",
    "128": "history128.png"
  },
  "permissions": [
    "history",
    "notifications",
    "privacy",
    "topSites",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
