{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Poloniex Fee Calculator",
  "description": "A simple tool to calculate the maker/taker fees before you open a buy/sell order on Poloniex.",
  "version": "1.0.0",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "32": "32.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "script.js",
      "main.js",
      "jquery.min.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Poloniex Fee Calculator",
    "default_icon": "32.png"
  },
  "manifest_version": 2
}
