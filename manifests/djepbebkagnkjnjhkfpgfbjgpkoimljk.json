{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "text to link",
  "version": "1.0.1",
  "description": "Convert unclickable urls & email addresses into clickable links.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "jquery.ba-replacetext.js",
        "clickable_links.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}
