{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GitMoji Finder",
  "description": "This extension allows the user to search and copy github eligible emojis.",
  "version": "1.2.1",
  "browser_action": {
    "default_icon": "githublogo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://www.github.com/",
    "http://www.github.com/"
  ]
}
