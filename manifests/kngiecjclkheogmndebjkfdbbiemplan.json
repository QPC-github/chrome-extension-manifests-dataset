{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "مواقيت الصلاة لمدينة طرابلس",
  "short_name": "مواقيت الصلاة لمدينة طرابلس",
  "version": "2.0",
  "description": "تنبيه بمواعيد الصلاة لمدينة طرابلس, حيث يقوم بعرض تنبيهات عند قرب موعد إقامة الصلاة كما أن مواعيد الأذان متوافقة مع الأوقاف",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "notifications",
    "<all_urls>",
    "contextMenus"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "icon.png"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "مواقيت الصلاة لمدينة طرابلس"
  }
}
