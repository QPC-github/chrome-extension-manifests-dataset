{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mutual Fund Price Forecasts",
  "description": "Get daily price forecasts for your favorite US mutual funds before the market closes.",
  "version": "1.08",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Mutual Fund Price Forecasts"
  },
  "icons": {
    "120": "icon.png"
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ]
}
