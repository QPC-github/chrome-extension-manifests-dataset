{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end",
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Adds IMDb data to movies on Pirate Bay sites.",
  "manifest_version": 2,
  "name": "IMDb for the Pirate Bay",
  "permissions": [
    "http://www.omdbapi.com/*",
    "http://www.imdb.com/find*"
  ],
  "version": "0.41",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
