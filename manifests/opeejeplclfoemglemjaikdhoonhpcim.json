{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "what i'm seeing",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "description": "Take a screenshot of the current tab and gives a url to share. (uploads to d12.co for 5 days)",
  "version": "1.1",
  "icons": {
    "16": "d12.png",
    "48": "d12.png",
    "128": "d12.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "http://d12.co/"
  ],
  "browser_action": {
    "default_icon": "d12.png",
    "default_popup": "popup.html"
  }
}
