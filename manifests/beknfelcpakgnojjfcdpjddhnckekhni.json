{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Loadmill session splitter",
  "description": "Split Loadmill recording sessions",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Split session",
    "default_popup": "browser_action.html",
    "default_icon": "pinmill.png"
  },
  "icons": {
    "16": "images/pinmill16.png",
    "48": "images/pinmill48.png",
    "128": "images/pinmill128.png"
  },
  "manifest_version": 2
}
