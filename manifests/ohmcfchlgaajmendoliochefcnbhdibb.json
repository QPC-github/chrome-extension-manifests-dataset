{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "mySkip",
  "description": "Extension for automatically skipping video ads and closing banner ads on YouTube",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
