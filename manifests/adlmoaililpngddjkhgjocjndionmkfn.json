{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TubeSave",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "description": "Save your favorite online videos to your computer!",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "include.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs"
  ]
}
