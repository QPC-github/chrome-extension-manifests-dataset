{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabpik Tab Manager - Beta",
  "description": "Prioritize and Organize your Browser Tabs with Style",
  "version": "0.9.35",
  "browser_action": {
    "default_title": "Tabpik",
    "default_icon": "./assets/icon.png"
  },
  "background": {
    "scripts": [
      "./assets/jquery.min.js",
      "./assets/eventPage.js",
      "main.76f5d914af8e28a77191.js"
    ],
    "persistent": false
  },
  "minimum_chrome_version": "60",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "icons": {
    "128": "./assets/icon.png"
  }
}
