{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EE2ME",
  "description": "Import from easyenjaz",
  "version": "1.0.0.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Import from Easyenjaz"
  },
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "http://gcchmc.org/Gcc/RegisterForMedicalTest.aspx",
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ]
}
