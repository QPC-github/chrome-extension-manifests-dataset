{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extra Content V.I.P.",
  "description": "Extra Content VIP Extension gives access to Premium VIP content.",
  "version": "2.0",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "http://*/",
    "webRequest",
    "*://*.google.com/",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "analytics.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ],
      "css": [
        "jquery-ui.css"
      ],
      "js": [
        "debug_mode.js",
        "jquery.js",
        "jquery-ui.js",
        "sugar.min.js"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "debug_mode.js",
        "sugar.min.js",
        "URI.js",
        "ads-list.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
