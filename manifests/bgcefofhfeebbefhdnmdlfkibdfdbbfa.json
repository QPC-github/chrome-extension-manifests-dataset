{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Open Random Bookmarks",
  "description": "Open Random Bookmarks",
  "version": "0.2.2",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
