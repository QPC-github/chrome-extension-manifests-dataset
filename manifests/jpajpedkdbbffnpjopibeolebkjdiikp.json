{
  "content_scripts": [
    {
      "exclude_globs": [
        "*youtube.com/watch?v=*&feature=PlayList*"
      ],
      "include_globs": [
        "http://*youtube.com/*"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "description": "Removes Youtube's New Subscription Bar And Autoplay.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "converted_from_user_script": true,
  "name": "Subscription Bar Remover For Youtube",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.1.15",
  "manifest_version": 2
}
