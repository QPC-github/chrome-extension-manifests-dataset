{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "dyna-cors",
  "name": "dyna CORS",
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "48": "images/favicon-32x32.png",
    "128": "images/favicon-32x32.png"
  },
  "version": "2.4.1023",
  "manifest_version": 2,
  "description": "Disable CORS in a better way",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "images/on.png",
    "images/off.png"
  ],
  "browser_action": {
    "default_icon": "images/off.png",
    "default_title": "dyna CORS",
    "default_popup": "index.html"
  },
  "options_page": "index.html"
}
