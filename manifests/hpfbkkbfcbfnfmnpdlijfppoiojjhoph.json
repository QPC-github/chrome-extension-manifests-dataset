{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dailymotion Unblur",
  "description": "Simple CSS trick to remove blur on Dailymotion video preview",
  "version": "1.0",
  "permissions": [
    "*://*.dailymotion.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.dailymotion.com/*"
      ],
      "css": [
        "unblur.css"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2
}
