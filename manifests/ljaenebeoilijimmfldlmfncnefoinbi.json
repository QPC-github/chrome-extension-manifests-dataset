{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Flash Blocker Free",
  "version": "1.0",
  "description": "Flash Blocker free blocks all the flash from any webpage you visit. Enter URL in search bar and page will render without flash.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "homepage_url": "https://www.chromecrxstore.com/",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Flash Block"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
