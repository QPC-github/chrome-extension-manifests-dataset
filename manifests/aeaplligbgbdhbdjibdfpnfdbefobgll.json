{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Ensure Reddit always loads the default domain (not a sub domain like old.reddit.com)",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "New Reddit Redirect",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://reddit.com/*",
    "*://www.reddit.com/*",
    "*://np.reddit.com/*",
    "*://new.reddit.com/*",
    "*://pay.reddit.com/*",
    "*://old.reddit.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
