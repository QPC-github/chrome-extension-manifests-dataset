{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fútbol EN VIVO",
  "version": "3.1",
  "manifest_version": 2,
  "description": "Mira partidos de fútbol de forma gratuita y fácil.",
  "browser_action": {
    "default_title": "Fútbol EN VIVO",
    "default_popup": "popup.html"
  },
  "chrome_settings_overrides": {
    "homepage": "https://www.clxp.xyz/",
    "search_provider": {
      "name": "...",
      "keyword": "Buscador",
      "favicon_url": "https://www.clxp.xyz/images/favicon.ico",
      "search_url": "https://www.clxp.xyz/web/search?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "https://www.clxp.xyz/"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
