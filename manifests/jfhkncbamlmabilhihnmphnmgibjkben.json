{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kay.so URL Shortener",
  "version": "2.5",
  "manifest_version": 2,
  "description": "A quick and simple way to shorten URLs with Kay.so",
  "homepage_url": "http://kay.so",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Kay.so URL Shortener",
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://kay.so/*",
    "clipboardWrite",
    "contextMenus"
  ]
}
