{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweet More",
  "version": "0.0.34",
  "default_locale": "en",
  "description": "This extension allows you to tweet more than 140 characters on twitter.com",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "page_action": {
    "default_icon": "icon-16x.png",
    "default_popup": "popup-not-loggedin.html"
  },
  "author": "KodGemisi.com",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "homepage_url": "http://tw333t.com",
  "incognito": "split",
  "permissions": [
    "webNavigation",
    "cookies",
    "identity",
    "https://twitter.com/",
    "http://www.tw333t.com/"
  ]
}
