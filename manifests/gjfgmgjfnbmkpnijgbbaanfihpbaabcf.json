{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Quick site",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Быстрый доступ к популярным сайтам,всё нужное всегда под рукой.",
  "browser_action": {
    "default_icon": "images/128x128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "64": "images/64x64.png"
  }
}
