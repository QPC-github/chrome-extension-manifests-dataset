{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Donbaler Go",
  "short_name": "Donbaler Go",
  "description": "Donbaler.com microblogging official app",
  "homepage_url": "http://donbaler.com/chrome",
  "version": "1.4.1",
  "author": "Farshad Nematdoust",
  "offline_enabled": false,
  "icons": {
    "16": "img/logo/l16.png",
    "48": "img/logo/l48.png",
    "128": "img/logo/l128.png"
  },
  "permissions": [
    "http://donbaler.com/",
    "storage",
    "notifications",
    "background"
  ],
  "browser_action": {
    "default_icon": "logo-a.png",
    "default_popup": "popup.html",
    "default_title": "Donbaler.com"
  },
  "web_accessible_resources": [
    "favicon.ico",
    "desknotifier_icon.png"
  ],
  "background": {
    "scripts": [
      "js/jq.js",
      "background.js"
    ],
    "persistent": true
  }
}
