{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snake Blocker",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
