{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qure4u Desktop Capture",
  "description": "Allows you to capture your desktop for use in video applications",
  "version": "0.1.4",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.qure4u.com/*",
      "*://*.cure4you.eu/*",
      "*://*.cure4you.dk/*",
      "*://*.qure4u.com/*",
      "*://*.my-care-plan.com/*",
      "*://*.unifiedwomens.com/*",
      "*://*.womancare4u.com/*",
      "*://womancare4u.com/*",
      "*://*.cl.qure4u.com/*",
      "*://*.mynexthealth.de/*",
      "*://*.mein-leben.diqm.de/*",
      "*://*.cure4you.eu/*",
      "*://qure4u.com/*",
      "*://my-care-plan.com/*",
      "*://cl.qure4u.com/*",
      "*://mynexthealth.de/*",
      "*://mein-leben.diqm.de/*",
      "*://*.cure4you.pro/*",
      "*://cure4you.eu/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
