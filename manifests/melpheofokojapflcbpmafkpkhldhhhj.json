{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Angry Baby",
  "description": "Replaces Donald Trump with Angry Baby",
  "version": "1.0",
  "content_scripts": [
    {
      "permissions": [
        "experimental"
      ],
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
