{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Compliance Deputy V4",
  "version": "4.3.1",
  "manifest_version": 2,
  "description": "Compliance Deputy",
  "icons": {
    "16": "/images/icon.png"
  },
  "browser_action": {
    "default_icon": "/images/icon.png",
    "default_popup": "Global.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "cookies",
    "notifications",
    "contextMenus",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "injected.js",
        "js/soapclient.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
