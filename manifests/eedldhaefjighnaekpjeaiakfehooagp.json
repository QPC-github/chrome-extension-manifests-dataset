{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Getgof",
  "version": "1.4.3",
  "author": "Getgof",
  "description": "Getgof — заказ в зарубежных интернет-магазинах",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://my.getgof.com.ua/*"
  ],
  "browser_action": {
    "default_title": "Getgof - simple foreign shopping",
    "default_icon": "img/icon48.png"
  }
}
