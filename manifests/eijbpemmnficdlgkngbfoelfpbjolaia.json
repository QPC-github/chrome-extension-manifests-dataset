{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UW Schedule to Google Calendar",
  "description": "Adds University of Washington class schedule to Google Calendar.",
  "version": "0.0.0.8",
  "author": "Jack Venberg",
  "icons": {
    "128": "img/calendar.png"
  },
  "browser_action": {
    "default_icon": "img/calendar.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://my.uw.edu/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/calendarContentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*.png",
    "js/grabSchedule.js",
    "js/insertButton.js"
  ],
  "permissions": [
    "tabs",
    "identity",
    "storage",
    "*://my.uw.edu/"
  ],
  "optional_permissions": [
    "downloads"
  ],
  "oauth2": {
    "client_id": "622291156743-j4ia5n590engurre07bfdr98q5o74298.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "email",
      "profile",
      "https://www.googleapis.com/auth/calendar"
    ]
  }
}
