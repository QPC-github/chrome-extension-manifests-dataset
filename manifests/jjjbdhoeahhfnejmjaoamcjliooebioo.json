{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Slack to Crack",
  "version": "1.0.1",
  "description": "Slack to Crack is a simple Chrome extension that turns all references to the word `Slack` in your web browser to `Crack`.",
  "icons": {
    "16": "images/c2s16.png",
    "48": "images/c2s48.png",
    "128": "images/c2s128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
