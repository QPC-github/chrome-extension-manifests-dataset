{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Range Calculator for Texas Hold'em",
  "name": "Poker Range Calculator",
  "version": "1.0.1",
  "author": "Poker Range Calculator",
  "short_name": "Poker Range Calculator",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/logo128.png",
    "default_title": "Poker Range Calculator"
  },
  "homepage_url": "https://github.com/forestturner/PokerHandRangeCalc",
  "icons": {
    "128": "img/logo128.png",
    "16": "img/logo16.png",
    "48": "img/logo48.png"
  },
  "offline_enabled": false,
  "permissions": [
    "tabs",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage",
    "browsingData",
    "cookies",
    "identity",
    "notifications",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
