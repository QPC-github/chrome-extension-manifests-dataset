{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "구독이",
  "version": "1.5.4",
  "description": "체계적인 맞구독 시스템을 통하여 빠른 시간내에 네이버TV 구독자를 늘려드립니다.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/navertv.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "font/*.woff2"
  ],
  "permissions": [
    "contextMenus",
    "https://*.naver.com/*",
    "https://youtube.com/*",
    "https://phinf.pstatic.net/*",
    "https://gudogi.kr/*",
    "storage",
    "declarativeContent",
    "notifications"
  ]
}
