{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tag Inspector",
  "version": "2.1",
  "manifest_version": 2,
  "description": "Displays Page Title, Meta Description, Meta Keywords, OpenGraph and Google Analytics Status",
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "code.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "code-16.png",
    "48": "code-48.png",
    "128": "code-128.png"
  }
}
