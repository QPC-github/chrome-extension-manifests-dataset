{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Valryan Live Extension Beta",
  "short_name": "Valryan",
  "description": "Ne ratez aucun live: Soyez informé dès qu'un live commence !",
  "version": "1.0.7.5",
  "author": "Noelie Aka Verane",
  "background": {
    "persistent": true,
    "scripts": [
      "src/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "src/popup.html",
    "default_title": "Valryan Twitch"
  },
  "homepage_url": "https://www.twitch.tv/valryantv",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "permissions": [
    "notifications",
    "tabs",
    "https://api.twitch.tv/",
    "background"
  ]
}
