{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dolos",
  "manifest_version": 2,
  "version": "0.1.0",
  "author": "Charles Quirin",
  "description": "Makes alternate email addresses from your Gmail address.",
  "background": {
    "scripts": [
      "event_page.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "dolos_logo_128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "contextMenus",
    "clipboardWrite",
    "notifications"
  ]
}
