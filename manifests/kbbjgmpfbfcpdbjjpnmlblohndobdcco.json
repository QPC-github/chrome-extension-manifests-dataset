{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitch HTML5",
  "version": "0.1.5",
  "description": "An extension to use the HTML5 Player UI instead of Flash player in every twitch channels",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Hellslicer",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "html5.js"
      ]
    }
  ],
  "manifest_version": 2
}
