{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "reinvented new tab",
  "description": "Display bookmarks on the new tab page",
  "version": "1.5.4",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "start-page.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "geolocation",
    "unlimitedStorage"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://oap.accuweather.com; object-src 'self'"
}
