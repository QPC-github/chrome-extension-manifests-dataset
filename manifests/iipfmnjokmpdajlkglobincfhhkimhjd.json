{
  "manifest_version": 2,
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "b.png",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [],
      "js": [
        "start.js"
      ],
      "matches": [
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Appel du synthetiseur",
  "name": "Synthecall",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.29"
}
