{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "name": "Fox",
  "version": "0.2.4",
  "description": "Fox is a chrome extension which help you do something better: User Interface Testing/ Replay/ Monitor/ Analytics.",
  "icons": {
    "16": "image/icon/app-16.png",
    "32": "image/icon/app-32.png",
    "48": "image/icon/app-48.png",
    "128": "image/icon/app-128.png"
  },
  "browser_action": {
    "default_icon": "image/icon/app-128.png",
    "default_title": "Fox"
  },
  "homepage_url": "https://github.com/soulteary/fox/tree/chrome-extension",
  "options_page": "page/options/main.html",
  "author": "soulteary",
  "background": {
    "page": "page/background/main.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "debugger",
    "*://*/",
    "http://*/",
    "https://*/",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "*"
  ],
  "devtools_page": "page/dashboard/loader.html"
}
