{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Toggle YouTube Comments",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Allows you to turn YouTube comments on or off.",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "permissions": [],
  "options_page": "options.html"
}
