{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CubDomain.com",
  "description": "It is free tool to check Basic Stats, Website Information, Whois Record and more of any website.",
  "version": "1.0.5",
  "browser_action": {
    "default_icon": "img/icon_16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "background": {
    "scripts": [
      "background/script.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
