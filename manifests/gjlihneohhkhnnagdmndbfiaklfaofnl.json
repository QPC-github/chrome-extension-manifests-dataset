{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save Open Images",
  "description": "When you click the extension's icon, it saves all open images tabs",
  "version": "1.0",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_popup": "popup.html",
    "default_title": "Save Images"
  },
  "icons": {
    "64": "icon-19.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
