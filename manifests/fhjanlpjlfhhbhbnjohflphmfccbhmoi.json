{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "manifest_version": 2,
  "name": "Elfster's Elf It!",
  "version": "3.29",
  "description": "It’s a snap adding stuff you like to your Elfster Wish List from any website using the Elf It! Chrome extension.",
  "author": "Ray Winkelman",
  "homepage_url": "https://www.elfster.com/",
  "icons": {
    "16": "static/img/browser-extension-elf-icon_16.png",
    "32": "static/img/browser-extension-elf-icon_32.png",
    "48": "static/img/browser-extension-elf-icon_48.png",
    "128": "static/img/browser-extension-elf-icon_128.png"
  },
  "web_accessible_resources": [
    "index.html"
  ],
  "background": {
    "scripts": [
      "static/js/init-extension.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "static/img/elfster-chrome-ext-icon_16x16.png",
      "32": "static/img/elfster-chrome-ext-icon_32x32.png",
      "48": "static/img/elfster-chrome-ext-icon_48x48.png",
      "64": "static/img/elfster-chrome-ext-icon_64x64.png",
      "128": "static/img/elfster-chrome-ext-icon_128x128.png"
    },
    "default_title": "Elfster’s Elf It!  It’s a snap adding stuff you like to your Elfster Wish List from any website using the Elf It! Chrome extension."
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://www.elfster.com; object-src 'self'"
}
