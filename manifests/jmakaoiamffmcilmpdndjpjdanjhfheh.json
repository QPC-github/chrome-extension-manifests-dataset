{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "3.27",
  "name": "__MSG_extName__",
  "short_name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.aliexpress.com/*",
        "*://*.aliexpress.ru/*",
        "*://*.gearbest.com/*"
      ],
      "js": [
        "scripts.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "*.*"
  ]
}
