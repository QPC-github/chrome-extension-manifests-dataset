{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "name": "voldetrump",
  "description": "converts the word 'Trump' to 'Voldemort' on any webpage",
  "version": "1.0.3",
  "author": "Devin Frenze"
}
