{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "feres",
  "version": "2.2.0",
  "description": "feres 是千手章鱼的自动代理插件，开启千手章鱼并启动调试后 feres 就会自动进行代理。",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "feres",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "unlimitedStorage",
    "storage"
  ]
}
