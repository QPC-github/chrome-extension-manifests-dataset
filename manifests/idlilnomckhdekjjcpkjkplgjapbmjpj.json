{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Talking Spinner",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Why should spinners always remain mute?",
  "background": {
    "page": "background.html"
  },
