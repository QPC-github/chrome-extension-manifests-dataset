{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Club Penguin Brasil",
  "manifest_version": 2,
  "description": "Permite que o Club Penguin Brasil execute automaticamente no Adobe Flash Player.",
  "version": "1.0",
  "permissions": [
    "contentSettings"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icons/puffle_48.png",
    "128": "icons/puffle_128.png"
  }
}
