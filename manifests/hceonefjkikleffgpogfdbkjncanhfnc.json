{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DEWIAR",
  "description": "DEWIAR, опережая будущее! Система связи, создание и расшифровка БАР-КОДОВ.",
  "version": "2",
  "browser_action": {
    "default_icon": "logo_b.png",
    "default_title": "Выполните вход в систему DEWIAR!"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "https://dewiar.com/*",
    "notifications",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://dewiar.com; object-src 'self'",
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/background.js"
    ]
  }
}
