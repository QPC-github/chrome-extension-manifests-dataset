{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "NeverAway",
  "version": "1.3",
  "description": "Stay logged into your open tabs",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
