{
  "author": "Plugtopus Agency",
  "background": {
    "page": "background/index.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/app/icon-128.png",
      "16": "icons/app/icon-16.png",
      "19": "icons/app/icon-19.png",
      "24": "icons/app/icon-24.png",
      "32": "icons/app/icon-32.png",
      "38": "icons/app/icon-38.png",
      "48": "icons/app/icon-48.png",
      "64": "icons/app/icon-64.png",
      "96": "icons/app/icon-96.png"
    },
    "default_title": "__MSG_title__"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content/insert.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_desc__",
  "homepage_url": "https://plugtopus.agency",
  "icons": {
    "128": "icons/app/icon-128.png",
    "16": "icons/app/icon-16.png",
    "19": "icons/app/icon-19.png",
    "24": "icons/app/icon-24.png",
    "32": "icons/app/icon-32.png",
    "38": "icons/app/icon-38.png",
    "48": "icons/app/icon-48.png",
    "64": "icons/app/icon-64.png",
    "96": "icons/app/icon-96.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "55.0",
  "name": "__MSG_name__",
  "short_name": "__MSG_short__",
  "options_ui": {
    "chrome_style": false,
    "open_in_tab": true,
    "page": "options/index.html"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "activeTab",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "13.8.18.1",
  "web_accessible_resources": [
    "select/index.html",
    "select/pointer.css",
    "confirm/index.html"
  ]
}
