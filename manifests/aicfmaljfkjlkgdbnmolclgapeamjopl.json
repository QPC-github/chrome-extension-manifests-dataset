{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Echo Speed Enhancer",
  "version": "1.0",
  "description": "Who doesn't want to watch lectures at 4x speed!",
  "content_scripts": [
    {
      "js": [
        "jquery-slim.js",
        "content.js"
      ],
      "run-at": "document_idle",
      "matches": [
        "https://echo360.org.au/*"
      ]
    }
  ]
}
