{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iPotato",
  "version": "1.1",
  "description": "Helps out where people have accidentally written 'iPhone' instead of 'Potato'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "potato16.gif",
    "32": "potato32.gif",
    "48": "potato48.gif",
    "64": "potato64.gif",
    "128": "potato128.gif",
    "256": "potato256.gif"
  }
}
