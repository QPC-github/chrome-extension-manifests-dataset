{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Finwiz SEC plugin",
  "description": "Helps you graph data in SEC filings.",
  "version": "1.5",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://www.sec.gov/Archives/edgar/data/*"
      ],
      "js": [
        "sec.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
