{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Funny",
  "description": "Show reddit hot funny images at new tab.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "version": "0.0.2.1",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "page_action": {
    "default_title": "Reddit Funny",
    "default_icon": "icon.png"
  }
}
