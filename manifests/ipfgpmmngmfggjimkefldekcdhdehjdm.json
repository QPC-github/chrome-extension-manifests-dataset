{
  "manifest_version": 2,
  "name": "Сонник",
  "short_name": "Сонник",
  "description": "Сонник - это расширение поможет разгадать Ваш сон в одно нажатие кнопки",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.94",
  "permissions": [
    "*//yousonnik.ru/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
