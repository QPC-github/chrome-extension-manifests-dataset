{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SFProxyManualV2",
  "version": "0.3.1",
  "description": "This extension allows user to enter own proxy servers and randomly hop through them.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "author": "StableFlow",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "homepage_url": "http://www.stableflow.com",
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "proxy",
    "storage",
    "tabs",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ]
}
