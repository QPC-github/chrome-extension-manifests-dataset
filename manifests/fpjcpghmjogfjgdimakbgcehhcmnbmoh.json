{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bungee Chrome Extension",
  "version": "1.0",
  "description": "Bungee Extension",
  "homepage_url": "https://bungeetech.com",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/bungee_tran.png",
    "48": "img/bungee_tran.png",
    "128": "img/bungee_tran.png"
  },
  "browser_action": {
    "default_icon": "img/bungee_tran.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "proxy"
  ]
}
