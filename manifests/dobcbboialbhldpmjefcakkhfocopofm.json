{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CE Popup Chat",
  "short_name": "CE.chat",
  "description": "Chat real time with random people in popup by clicking the extension icon!",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
