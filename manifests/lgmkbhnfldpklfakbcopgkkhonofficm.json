{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unshorten.It!",
  "version": "1.1",
  "description": "Right-click on shortened URLs to discover where they actually lead to and get their safety rating.",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "minimum_chrome_version": "8.0.0.0"
}
