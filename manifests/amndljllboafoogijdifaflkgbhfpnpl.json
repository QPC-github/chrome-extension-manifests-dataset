{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TourLife Migrator",
  "version": "1.0.4",
  "description": "Migrate your data effortlessly into TourLife.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "*://*.tourmanagement.com/",
    "downloads"
  ],
  "browser_action": {
    "default_title": "TourLife Migrator",
    "default_popup": "popup.html",
    "default_icon": {
      "72": "images/icon-72x72.png",
      "96": "images/icon-96x96.png",
      "128": "images/icon-128x128.png"
    }
  },
  "icons": {
    "72": "images/icon-72x72.png",
    "96": "images/icon-96x96.png",
    "128": "images/icon-128x128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://unpkg.com/; object-src 'self'"
}
