{
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
    },
    "default_title": "РБК Украина"
  },
  "content_scripts": [
    {
      "js": [
        "js/script.js"
      ],
      "matches": [
        "http://www.rbc.ua/*",
        "https://www.rbc.ua/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Последние новости Украины сегодня",
  "homepage_url": "https://www.rbc.ua/",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "РБК Украина - Последние новости Украины",
  "permissions": [
    "tabs",
    "storage"
  ],
  "short_name": "РБК Украина",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "version_name": "1.0"
}
