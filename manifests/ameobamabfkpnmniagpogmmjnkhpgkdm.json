{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Copy as Jade",
  "description": "Convert and copy selected text as Jade via the context menu (right mouse button)",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "clipboardWrite",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "he.js",
      "html2jade.js",
      "run.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
