{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save your tab",
  "version": "1.1",
  "description": "This extension for slow computer to save their tab",
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js",
      "index.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
