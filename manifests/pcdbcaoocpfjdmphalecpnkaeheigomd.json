{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Studier",
  "short_name": "Studier",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Studier",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Studier",
    "default_icon": "images/studier_16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "images/studier_128.png",
    "48": "images/studier_48.png",
    "16": "images/studier_16.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
