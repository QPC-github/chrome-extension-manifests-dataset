{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jiffi Web Help",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Jiffi Web Help extension to streamline website support requests and bug reporting",
  "browser_action": {
    "default_icon": "icon-999.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+J"
      }
    }
  },
  "externally_connectable": {
    "matches": [
      "https://jiffiwebhelp.com/*",
      "*://*.chromium.org/*"
    ],
    "accepts_tls_channel_id": false
  },
  "permissions": [
    "activeTab",
    "file:///*"
  ],
  "icons": {
    "16": "icon16-999.png",
    "48": "icon48-999.png",
    "128": "icon128-999.png"
  }
}
