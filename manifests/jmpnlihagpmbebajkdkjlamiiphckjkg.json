{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NEU library redirector",
  "version": "2.0",
  "description": "By Hooman Javaheri. This extension provides access to the online literature via Northeastern University Library.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "reload.js"
    ]
  },
  "browser_action": {
    "default_icon": "neu_lib.png",
    "default_title": "Reload via NEU libraries"
  }
}
