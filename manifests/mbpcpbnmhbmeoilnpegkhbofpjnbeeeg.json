{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "filevoid.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Capture screen & upload to FileVoid"
  },
  "description": "Add a button to upload your current screen to filevoid.net and instantly retrieve the URL in your clipboard.",
  "homepage_url": "http://filevoid.net",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "FileVoid",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "<all_urls>"
  ],
  "version": "1.0.2"
}
