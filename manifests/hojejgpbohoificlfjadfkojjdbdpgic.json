{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Muzica Noua | Aksha Mp3",
  "description": "Motor de cautare muzica noua, ajutor pe care iti ofera Aksha.eu. Cauta Muzica in direct cu ajutorul extensiei",
  "version": "1.4.3",
  "author": "Alex Marton",
  "homepage_url": "http://aksha.eu",
  "short_name": "Aksha",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/img/aksha.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
