{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MAJOR KEYWORDS",
  "version": "1.3",
  "description": "Your key to searching with more success.",
  "icons": {
    "16": "images/KEY_ICON_16.png",
    "48": "images/KEY_ICON_48.png",
    "128": "images/KEY_ICON_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
