{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "pimpboard.me",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Pimp your Pinterestâ„¢ board.",
  "icons": {
    "16": "images/icon_16x16.png",
    "48": "images/icon_48x48.png",
    "128": "images/icon_128x128.png"
  },
  "permissions": [
    "tabs",
    "windows",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/jquery-1.8.3.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.pimpboard.me/*"
      ],
      "js": [
        "js/jquery-1.8.3.min.js",
        "js/content.js"
      ]
    },
    {
      "matches": [
        "*://*.pinterest.com/pin/create/bookmarklet/*"
      ],
      "js": [
        "js/jquery-1.8.3.min.js",
        "js/content_pinterest.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/connector.js",
    "js/create_board.js",
    "js/pin_item.js"
  ]
}
