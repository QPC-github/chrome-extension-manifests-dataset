{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better ROBLOX Logo",
  "description": "This extension gets back the old ROBLOX logo.",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": [
        "https://*.roblox.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "ROBLOX enhancer",
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icon.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
