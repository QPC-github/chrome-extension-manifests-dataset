{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Marathon Countdown Timer",
  "description": "Marathon Countdown Timer",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "window.html"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "tab.html"
  },
  "version": "1.101"
}
