{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cryptonium",
  "version": "0.0.1",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "icons": {
    "128": "icon-34.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "notifications",
    "alarms",
    "bookmarks"
  ],
  "description": "A extension for all your crypto needs"
}
