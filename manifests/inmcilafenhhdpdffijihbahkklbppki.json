{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "3.0",
  "short_name": "Run 3 Unblocked",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png"
  },
  "permissions": [
    "contextMenus",
    "webRequestBlocking",
    "webRequest",
    "tabs",
    "https://run3space.com/*",
    "https://www.coolmathgames.com/*",
    "https://www.bing.com/*",
    "https://www.google.com/*",
    "https://www.google.co.uk/*",
    "https://www.google.com.au/*",
    "https://www.google.de/*",
    "https://www.google.fr/*",
    "https://www.google.ca/*",
    "https://www.google.us/*",
    "https://search.yahoo.com/*",
    "https://*.search.yahoo.com/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/48.png"
  }
}
