{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "128": "icon-128-NumberLine.png"
  },
  "manifest_version": 2,
  "name": "Number Line, by The Math Learning Center",
  "short_name": "Number Line",
  "description": "Number Line helps students visualize number sequences and demonstrate strategies for counting, comparing, and arithmetic.",
  "version": "4.0.3",
  "offline_enabled": true,
  "permissions": [
    "background",
    "clipboard",
    "clipboardWrite",
    "storage"
  ]
}
