{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "EpubReader/scripts/background.js"
      ]
    }
  },
  "description": "Simple EPUB Reader for EPUB3 books.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "file_handlers": {
    "any": {
      "extensions": [
        "epub"
      ]
    }
  },
  "manifest_version": 2,
  "name": "Simple EPUB Reader",
  "offline_enabled": true,
  "permissions": [
    "storage",
    "contextMenus",
    "unlimitedStorage",
    "tts",
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    }
  ],
  "version": "4.3"
}
