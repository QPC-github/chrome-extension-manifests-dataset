{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Comic Sans in email for Gmail™",
  "version": "0.5",
  "manifest_version": 2,
  "description": "Remove Comic Sans font from displayed messages in Gmail™.",
  "permissions": [
    "https://mail.google.com/mail/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/mail/*"
      ],
      "js": [
        "no-gmail-comic-sans.user.js"
      ]
    }
  ]
}
