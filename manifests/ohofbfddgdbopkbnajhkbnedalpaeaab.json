{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gentlemint",
  "description": "The Gentlemint tack add extension",
  "version": "1.0.6",
  "homepage_url": "http://gentlemint.com",
  "author": "Gentlemint",
  "icons": {
    "16": "images/32.png",
    "19": "images/38.png",
    "38": "images/76.png",
    "48": "images/96.png",
    "128": "images/256.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/32.png",
      "19": "images/38.png",
      "38": "images/76.png",
      "48": "images/96.png",
      "128": "images/256.png"
    }
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>"
  ],
  "offline_enabled": false
}
