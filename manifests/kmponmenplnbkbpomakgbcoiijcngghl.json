{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Maw.cx URL Shortener"
  },
  "name": "Maw.cx URL Shortener",
  "description": "Free URL shortening service that turn a long URL into a very short and easy to remember URL.",
  "homepage_url": "https://maw.cx",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1",
  "manifest_version": 2
}
