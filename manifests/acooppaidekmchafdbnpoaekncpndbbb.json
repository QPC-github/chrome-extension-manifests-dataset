{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better KBMax Titles",
  "version": "2.1",
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "https://*.kbmax.com/*",
    "https://*.kbmaxnext.com/*"
  ],
  "description": "Shows the name of KBMax pages in the tab title.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
