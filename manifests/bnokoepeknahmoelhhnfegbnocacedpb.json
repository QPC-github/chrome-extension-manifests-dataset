{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fantasy Espn Basketball Player Comment",
  "version": "0.1.0",
  "description": "Lets you add comment about players",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "http://fantasy.espn.com/basketball/*"
      ]
    }
  ]
}
