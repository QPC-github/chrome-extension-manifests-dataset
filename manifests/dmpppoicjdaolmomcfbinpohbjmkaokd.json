{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Where in the World by Take the Tent",
  "optional_permissions": [
    "topSites"
  ],
  "description": "I'll show you an amazing picture. You tell me where it is. I'll tell you how close you are.",
  "version": "1.954",
  "background": {
    "scripts": [
      "js/vendor/angular.min.js",
      "js/listener.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://ssl.google-analytics.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}
