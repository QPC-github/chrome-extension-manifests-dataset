{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Instant Feed",
  "version": "1.0",
  "author": "addonDeveloper",
  "description": "Enjoy subscribing your favorite websites",
  "icons": {
    "48": "/img/icon48.png",
    "96": "/img/icon96.png"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Instant Feed",
    "default_icon": {
      "16": "/img/icon16.png",
      "20": "/img/icon20.png",
      "32": "/img/icon32.png",
      "64": "/img/icon64.png"
    }
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  }
}
