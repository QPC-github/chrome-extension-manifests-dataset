{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Site Fundamentals",
  "description": "Analyze and distill any webpage into its fundamental contents with one click.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
