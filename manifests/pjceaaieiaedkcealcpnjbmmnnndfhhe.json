{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ramon Tiene Mucho Hipo",
  "description": "El mejor notificador de flic-flacs habido y por haber.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "version": "1.24",
  "browser_action": {
    "default_icon": "16.png"
  },
  "permissions": [
    "notifications",
    "http://www.ramontienehipo.com/"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}
