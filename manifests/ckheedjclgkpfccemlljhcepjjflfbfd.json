{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Link4Free",
  "version": "1.7",
  "description": "",
  "permissions": [
    "https://*.linkedin.com/*",
    "https://*.linkedin.com/",
    "https://actualinbox.bid/",
    "https://actualinbox.bid/*",
    "https://actualinbox.bid/*/*",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "datatables.js",
        "typed.js",
        "jquery.cookie.js",
        "popper.min.js",
        "bootstrap.min.js",
        "functions.js",
        "content.js"
      ],
      "css": [
        "bootstrap.min.css",
        "datatables.css",
        "style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*.png",
    "*.css",
    "*.js",
    "*.html"
  ],
  "manifest_version": 2
}
