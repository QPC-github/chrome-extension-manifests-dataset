{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "alt-J",
  "description": "Play Alt-J when you press Alt-J",
  "icons": {
    "128": "icon128.png",
    "380": "icon380.png"
  },
  "version": "1.0",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "launcher.js"
    ],
    "persistent": false
  },
  "commands": {
    "altj": {
      "suggested_key": {
        "default": "Alt+J"
      },
      "description": "Launch Alt-J music"
    }
  },
  "manifest_version": 2
}
