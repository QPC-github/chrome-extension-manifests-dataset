{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chan-Chan",
  "description": "In honor of the birthday of Jackie Chan. Let Jackie will be everywhere!",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-2.1.0.min.js",
        "page.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "manifest_version": 2
}
