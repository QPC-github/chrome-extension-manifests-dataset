{
  "browser_action": {
    "default_icon": "icon.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Clip pages around the web and save it to your SmartFile account.",
  "background": {
    "scripts": [
      "jquery-min.js",
      "smartclip.js"
    ]
  },
  "name": "SmartClip",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "manifest_version": 2,
  "version": "1.2.3"
}
