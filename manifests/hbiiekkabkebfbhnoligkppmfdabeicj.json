{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CCTVNEWS News Reader",
  "manifest_version": 2,
  "description": "CCTVNEWS News Reader",
  "version": "1.1",
  "permissions": [
    "http://applist.cctvnews.cn/",
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
