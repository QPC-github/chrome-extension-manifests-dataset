{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Dê um clique com o botão direito sempre que precisar de um e-mail temporário. Uma nova janela se abrirá com a inbox!",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Extensão SegundoEmail.com",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4"
}
