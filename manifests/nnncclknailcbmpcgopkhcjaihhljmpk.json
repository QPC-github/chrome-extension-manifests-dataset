{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dark Theme",
  "description": "Easy on the eyes.",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content_script.js"
      ]
    }
  ]
}
