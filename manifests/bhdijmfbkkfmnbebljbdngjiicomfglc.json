{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ragin' Binman",
  "description": "Popjustice.com's Ragin' Binman extension replaces mentions of Rag'n'Bone Man with the words Ragin' Binman.",
  "version": "0.1",
  "icons": {
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
