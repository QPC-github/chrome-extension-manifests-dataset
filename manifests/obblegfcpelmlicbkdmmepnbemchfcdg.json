{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BiTMuz Plugin",
  "short_name": "BiTMuz & HiTProject",
  "description": "Виджет поиска музыки и скачивания (виджет подойдет радиостанции для быстрого поиски музыки и скачивания ее)",
  "version": "1.1.25",
  "homepage_url": "https://bitmuz.ru/",
  "icons": {
    "128": "images/logo_128.png",
    "16": "images/logo_16.png",
    "48": "images/logo_48.png",
    "64": "images/logo_64.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Быстрый поиск music для DJ",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*",
    "notifications"
  ]
}
