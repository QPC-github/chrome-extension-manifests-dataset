{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Max Meetings",
  "author": "Max Healthcare",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Max Meetings allows you to share your screen during your telehealth or consulting sessions.",
  "homepage_url": "https://myportal.maxhealthcare.in",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:8000/*",
      "https://*.maxhealthcare.in/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
