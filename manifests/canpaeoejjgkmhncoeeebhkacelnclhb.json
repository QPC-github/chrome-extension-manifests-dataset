{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuickSearch",
  "description": "QuickSearch addon extension.",
  "version": "2.1.1",
  "manifest_version": 2,
  "icons": {
    "128": "quicksearch.png"
  },
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {},
  "author": "Lachlan Walls"
}
