{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ChronoBip",
  "version": "3.2",
  "description": "Extension to time yourself or to remind yourself of something to do",
  "browser_action": {
    "default_icon": "images/timerIcon.png",
    "default_popup": "index.html",
    "default_title": "ChronoBip"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "background",
    "tabs",
    "notifications"
  ],
  "web_accessible_resources": [
    "images/reminder_icon.png"
  ]
}
