{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tumblr Archive Page",
  "version": "1.0",
  "description": "Quick redirect to tumblr archive page",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "name": "Tumblr Archive"
  },
  "permissions": [
    "activeTab"
  ]
}
