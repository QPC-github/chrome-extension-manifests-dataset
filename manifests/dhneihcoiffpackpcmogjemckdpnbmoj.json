{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Вернуть Зефирчика",
  "version": "1.0.1",
  "permissions": [
    "background",
    "https://vk.com/*",
    "http://vk.com/*",
    "https://*.vk.com/*",
    "http://*.vk.com/*"
  ],
  "web_accessible_resources": [
    "zefir.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://vk.com/*",
        "http://*.vk.com/*",
        "https://vk.com/*",
        "https://*.vk.com/*"
      ],
      "css": [
        "zefir.css"
      ]
    }
  ]
}
