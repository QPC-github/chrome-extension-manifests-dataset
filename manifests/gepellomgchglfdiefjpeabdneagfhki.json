{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "数字图书馆阅读",
  "background": {
    "page": "background.html"
  },
  "version": "0.4",
  "description": "使用此插件后,可以在chrome下访问新东方多媒体学习库",
  "options_page": "index.html",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "minimum_chrome_version": "28.0.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://127.0.0.1 https://ssl.google-analytics.com; object-src 'self';",
  "permissions": [
    "management",
    "proxy",
    "idle",
    "tabs",
    "cookies",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "js/install.js"
  ],
  "manifest_version": 2
}
