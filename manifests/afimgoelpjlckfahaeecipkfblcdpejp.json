{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scooch Screen Capture",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Screen capture your entire current page!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "https://go.scoochmein.com/SSOProxy/SaveScreenShot",
    "http://go.scoochmein.com/SSOProxy/SaveScreenShot",
    "https://www.youtube.com/watch?v=E3vwLoDSBLQ&t=5843s/SSOProxy/SaveScreenShot"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
