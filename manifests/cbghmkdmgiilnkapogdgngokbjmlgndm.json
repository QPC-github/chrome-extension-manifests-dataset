{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dots and Murmurs",
  "short_name": "Dots and Murmurs",
  "description": "Remove distracting d√∂ts from The New Yorker.",
  "version": "1.1",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "dots.js"
      ],
      "matches": [
        "https://*.newyorker.com/*"
      ]
    }
  ],
  "permissions": [
    "https://www.newyorker.com/"
  ]
}
