{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HERE Case Extension",
  "version": "1.0.0",
  "description": "This extension is used to change the case of the sentences.",
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
