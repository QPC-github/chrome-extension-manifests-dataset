{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "quiet9.noise@gmail.com",
  "browser_action": {
    "browser_style": true,
    "default_icon": "icon19.png",
    "default_popup": "browser_action.html",
    "default_title": "유튜브 도우미"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "youtube-helper@park9eon.com"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "css_provider.js"
      ],
      "run_at": "document_start",
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*",
        "https://m.youtube.com/*"
      ]
    }
  ],
  "description": "유튜브 영상 가리기, 귀여운 고양이 Progress Bar, Picture in Picture 퀵버튼",
  "homepage_url": "https://park9eon.com",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "55.0.2883",
  "name": "유튜브 도우미",
  "permissions": [
    "tabs",
    "storage",
    "https://youtube.com/",
    "https://www.youtube.com/",
    "https://m.youtube.com/"
  ],
  "version": "1.1.1",
  "web_accessible_resources": [
    "video.css",
    "progress.css",
    "original.gif"
  ]
}
