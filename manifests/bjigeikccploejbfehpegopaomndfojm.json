{
  "author": "Estlex OÜ",
  "background": {
    "persistent": true,
    "scripts": [
      "js/app/config.js",
      "js/app/background.js",
      "js/app/connector.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "images/estlex-128.png",
      "16": "images/estlex-16.png",
      "32": "images/estlex-32.png",
      "48": "images/estlex-48.png"
    },
    "default_popup": "popup.html",
    "default_title": "Estlex"
  },
  "content_scripts": [
    {
      "css": [
        "css/akt_extras_1.css"
      ],
      "js": [
        "js/page/akt_extra.js",
        "js/page/akt_kihid.js",
        "js/page/arutelu.js"
      ],
      "matches": [
        "https://www.riigiteataja.ee/akt/*",
        "https://www.riigiteataja.ee/en/eli/*"
      ]
    },
    {
      "css": [],
      "js": [
        "js/page/estlex.js"
      ],
      "matches": [
        "https://estlex.ee/*"
      ]
    }
  ],
  "description": "Estlex plugin, mis võimaldab Estlexi funktsioone kasutada ka Riigi Teataja veebilehel.",
  "icons": {
    "128": "images/estlex-128.png",
    "16": "images/estlex-16.png",
    "32": "images/estlex-32.png",
    "48": "images/estlex-48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Estlex",
  "omnibox": {
    "keyword": "e"
  },
  "optional_permissions": [
    "*://*/*"
  ],
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "contextMenus",
    "https://estlex.ee/",
    "https://estlex.ee/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.26",
  "web_accessible_resources": [
    "images/akt_extra_menu_24.svg",
    "images/notes.svg",
    "images/carousel.svg",
    "images/tolked_ico.png",
    "images/clipboard.svg",
    "images/star_selected_24.svg",
    "images/star_selected_24_selected.svg",
    "images/forum.svg",
    "images/kuula_loading.gif",
    "images/ICON_INFO.gif",
    "images/ICON_INFO3.gif",
    "images/ICON_RIGHTv3.gif",
    "images/ICON_LEFTv3.gif",
    "images/link.svg",
    "images/icon_plus.gif",
    "images/icon_minus.gif",
    "images/OTSINGLIST.svg",
    "images/NOTIFICMENU.svg",
    "images/NOTEMENU.svg",
    "images/FORUMMENU.svg",
    "images/LEMMIKMENU.svg",
    "images/ICONUSERN.png",
    "images/LOGOUTMENU.svg",
    "images/logo_sml.png",
    "images/info.png",
    "images/idkaart.gif",
    "images/case.png"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "https://estlex.ee/*"
    ],
    "accepts_tls_channel_id": true
  }
}
