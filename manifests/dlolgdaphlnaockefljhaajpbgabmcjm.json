{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Goodreads auxílios",
  "version": "0.1.4",
  "icons": {
    "48": "icon.png"
  },
  "description": "Entensão para axiliar no goodreads",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.goodreads.com/"
      ]
    },
    {
      "js": [
        "content_filter_lgm.js"
      ],
      "matches": [
        "https://www.goodreads.com/work/editions/*"
      ]
    }
  ]
}
