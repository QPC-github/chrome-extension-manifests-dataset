{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Công cụ đặt hàng Kho Vận Chuyển",
  "version": "1.4.4",
  "description": "Công cụ đặt hàng của Kho Vận Chuyển Trung Việt trên trình duyệt Chrome, Cờ Rôm & Cốc Cốc.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs",
    "notifications",
    "http://localhost:3420/*"
  ],
  "web_accessible_resources": [
    "css/font-awesome/fonts/*",
    "template.html",
    "icon.jpg"
  ],
  "content_scripts": [
    {
      "css": [
        "css/bootstrap.min.css",
        "css/admin-forms.css",
        "css/animate.css",
        "css/style.default.css",
        "css/bootstrap-table.css",
        "css/toastr.min.css",
        "css/font-awesome/css/font-awesome.css"
      ],
      "js": [
        "js/jquery.js",
        "js/script.js",
        "js/jquery.tmpl.js",
        "js/neon-login-custom.js",
        "js/jquery.validate.min.js",
        "js/neon-guest-data.js",
        "js/bootstrap.min.js",
        "js/bootstrap-table.js",
        "js/toastr.min.js",
        "js/jquery.battatech.excelexport.js"
      ],
      "matches": [
        "http://world.taobao.com/item/*",
        "https://world.taobao.com/item/*",
        "https://item.taobao.com/*",
        "http://detail.1688.com/*",
        "https://detail.1688.com/*",
        "http://world.tmall.com/*",
        "http://detail.tmall.com/*",
        "https://world.tmall.com/*",
        "https://detail.tmall.com/*",
        "https://detail.tmall.hk/*"
      ]
    }
  ],
  "manifest_version": 2,
  "icons": {
    "16": "icon.jpg",
    "48": "icon.jpg",
    "128": "icon.jpg"
  }
}
