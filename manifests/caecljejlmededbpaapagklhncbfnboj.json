{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Catalyst Desktop Sharing Extension",
  "author": "ASCEND HIT LLC",
  "version": "6.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Catalyst Chrome Extension for Desktop Sharing",
  "homepage_url": "http://ascendhit.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "short_name": "Catalyst",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.catalystmeeting.com:*/*",
      "*://*.catalystmeetingqa.com:*/*",
      "*://*.catalystmeeting.dev:*/*"
    ]
  }
}
