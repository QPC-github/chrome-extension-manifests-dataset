{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Get Page Favicon",
  "version": "1.0.1",
  "permissions": [
    "clipboardWrite",
    "tabs",
    "downloads"
  ],
  "description": "Download and view a pages favicon.",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "favicon.html",
    "default_icon": "icon128.png"
  },
  "manifest_version": 2
}
