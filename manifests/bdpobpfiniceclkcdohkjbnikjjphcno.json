{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LearnQuran.Online User Login",
  "version": "1.6",
  "manifest_version": 2,
  "description": "LearnQuran.Online Chrome extension to easily assess your resources on the website account.",
  "icons": {
    "128": "icons/128.jpg",
    "256": "icons/256.jpg"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/128.jpg",
    "default_title": "LearnQuran.Online User Login"
  },
  "permissions": [
    "tabs"
  ]
}
