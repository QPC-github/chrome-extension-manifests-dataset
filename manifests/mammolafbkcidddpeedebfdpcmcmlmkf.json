{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remembr Connect",
  "version": "17.0",
  "description": "Permet d'ajouter des contacts de LinkedIn directement Ã  Remembr ",
  "content_security_policy": "script-src 'self' 'unsafe-inline' https://cdn.amplitude.com 'unsafe-eval'; object-src 'self' https://cdn.amplitude.com 'unsafe-inline'",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "https://*.linkedin.com/*",
    "https://*.shapr.net/*",
    "https://shapr.co/*",
    "*://remembr.app/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://remembr.app/*"
    ]
  }
}
