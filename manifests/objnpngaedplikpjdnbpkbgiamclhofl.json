{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jira List Opener",
  "author": "Win.Swarr@gmail.com",
  "manifest_version": 2,
  "version": "0.3.7",
  "description": "Open a list of Jira IDs",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "homepage_url": "http://www.swarr.com",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon/icon-48.png",
    "default_title": "Win's Jira List Tab Opener",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  }
}
