{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RLDL",
  "short_name": "RLDL",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Download Helper For Rainer Land",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "http://rainierland.com/movie/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://rainierland.com/movie/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ]
}
