{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Force HTTPS",
  "description": "Converts all http urls into https urls",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.PNG"
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
