{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KeepKey Client",
  "version": "6.4.0.1",
  "description": "A simple cryptocurrency wallet that integrates with the KeepKey device",
  "manifest_version": 2,
  "icons": {
    "128": "images/icon.png"
  },
  "app": {
    "background": {
      "scripts": [
        "Background.js"
      ]
    }
  },
  "minimum_chrome_version": "46.0.2490.86",
  "permissions": [
    "https://shapeshift.io/*",
    "https://*.coinquery.com/api/*",
    "https://coinquery.com/*",
    "http://*.coinquery.com/api/*",
    "http://coinquery.com/*",
    "https://api.blockcypher.com/v1/*",
    "https://api.etherscan.io/api/*",
    "https://keepkey-wallet.keepkey.com/*",
    "https://keepkey.shapeshift.io/*",
    "https://keepkey.shapeshift.com/*",
    "https://auth.shapeshift.io/oauth/",
    "https://bitcoinfees.earn.com/*",
    "hid",
    "usb",
    "identity",
    {
      "usbDevices": [
        {
          "vendorId": 11044,
          "productId": 1
        },
        {
          "vendorId": 11044,
          "productId": 2
        }
      ]
    }
  ]
}
