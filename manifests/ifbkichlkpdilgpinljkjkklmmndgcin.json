{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "WithLIVE for VTuber Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "WithLIVE for VTuber Screen Sharing",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.0.3",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow screen sharing in Agora Web Application.",
  "externally_connectable": {
    "matches": [
      "*://www.withlive.jp/*",
      "*://vtuber.withlive.jp/*",
      "*://withlive-web-staging.appspot.com/*",
      "*://withlive-vtuber-web.appspot.com/*"
    ]
  },
  "icons": {
    "128": "128w.png",
    "16": "16w.png",
    "48": "48w.png"
  },
  "browser_action": {
    "default_icon": "16w.png"
  }
}
