{
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/bg.js",
      "scripts/tabs.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "A simple start page",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "SimpleStart",
  "short_name": "SimpleStart",
  "author": "Horak Jakub",
  "options_page": "options.html",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "chrome://favicon/",
    "<all_urls>"
  ],
  "version": "1.0.0.2"
}
