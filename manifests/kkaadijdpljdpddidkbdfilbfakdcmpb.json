{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://www.ereality.ru/*",
        "*://www.api.ereality.ru/*",
        "*://forum.ereality.ru/*",
        "http://gosov.net/lotery.html",
        "http://*.com/efimerka/",
        "http://sidzoku.ru/landlord/",
        "*://order.ereality.ru/*",
        "*://usercp.ereality.ru/*",
        "http://er-help.ru/scripts/map_shaxt*"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "includes/content.js"
      ]
    }
  ],
  "name": "EReality Extension",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "web_accessible_resources": [
    "res/*",
    "assets/*",
    "icons/*"
  ],
  "description": "Extension for EReality.ru",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://forum.ereality.ru/topic199422/page1.html",
  "version": "1.0.23.1",
  "options_page": "options.html",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/button.png",
    "default_title": "Открыть меню"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications"
  ]
}
