{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CTI-PRODUCTION",
  "description": "CTI-PRODUCTION",
  "version": "1.10",
  "browser_action": {
    "default_title": "CTI-PRODUCTION",
    "default_icon": "icon_prod.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "devtools_page": "",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://*/*",
      "https://*/*"
    ]
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}
