{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/jquery.js",
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "京东优惠券助手",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/jd.css",
        "css/home.css"
      ],
      "js": [
        "js/jquery.js",
        "js/highcharts.js",
        "js/tubiao.js",
        "js/jd.js"
      ],
      "matches": [
        "*://*.jd.com/*",
        "*://*.jd.hk/*"
      ]
    },
    {
      "js": [
        "js/mogujie.js"
      ],
      "matches": [
        "*://*.mogu.com/*"
      ]
    },
    {
      "js": [
        "js/kaola.js"
      ],
      "matches": [
        "*://*.kaola.com/*"
      ]
    }
  ],
  "description": "京东优惠券助手专为用户查找京东大额优惠券，让您花更少的钱购买同样的商品",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "京东优惠券助手",
  "permissions": [
    "http://www.360soquan.com/",
    "http://api.yqfei365.com/",
    "https://cd.jd.com/",
    "https://browser.bijiago.com/"
  ],
  "version": "3.0",
  "web_accessible_resources": [
    "images/*"
  ]
}
