{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "web媒体雷达",
  "description": "Catch the web media for download.\n捕捉网络媒体以供下载。",
  "version": "1.0",
  "icons": {
    "128": "radar128.png"
  },
  "browser_action": {
    "default_icon": "radar24.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "bg.html"
  },
  "permissions": [
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage"
  ]
}
