{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.facebook.com/pokes*"
      ]
    }
  ],
  "description": "Automatically pokes back on Facebook when you have the poke page open. Now enjoy all your free time.",
  "manifest_version": 2,
  "name": "Poke Back",
  "version": "1.2",
  "icons": {
    "16": "pokeicon.png",
    "128": "pokeicon128.png"
  }
}
