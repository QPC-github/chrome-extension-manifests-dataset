{
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "description": "Create your own Kiosk from any website on a local network or from the internet.",
  "icons": {
    "128": "app/img/icons/Kiosk-128.png",
    "16": "app/img/icons/Kiosk-16.png",
    "256": "app/img/icons/Kiosk-256.png",
    "32": "app/img/icons/Kiosk-32.png",
    "64": "app/img/icons/Kiosk-64.png"
  },
  "kiosk_enabled": true,
  "kiosk_only": true,
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "name": "Kiosk App",
  "permissions": [
    "fullscreen",
    "power",
    "storage",
    "webview",
    "audioCapture",
    "videoCapture",
    "geolocation",
    "notifications",
    "alwaysOnTopWindows"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.6.1.0"
}
