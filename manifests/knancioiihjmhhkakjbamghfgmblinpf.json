{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pinsli Pinner",
  "description": "Pinsli Pinner enables you to pin the images you like to your Pinsli.com account.",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "http://*/"
  ]
}
