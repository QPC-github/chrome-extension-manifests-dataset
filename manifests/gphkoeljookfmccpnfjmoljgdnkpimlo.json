{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Rafał Rothenberger",
  "background": {
    "page": "html/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": "ico16.png",
    "default_title": "Ja, Rock!"
  },
  "description": "Wtyczka informująca o streamach i filmach.",
  "icons": {
    "128": "ico128.png",
    "48": "ico48.png",
    "16": "ico16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "name": "Ja, Rock!",
  "short_name": "JaRock",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "alarms",
    "*://*.fbcdn.net/*",
    "*://*.twimg.com/*",
    "*://*.ggpht.com/*",
    "*://*.jarock.pl/*",
    "*://*.jtvnw.net/*",
    "https://fonts.googleapis.com/*"
  ],
  "version": "1.0.2"
}
