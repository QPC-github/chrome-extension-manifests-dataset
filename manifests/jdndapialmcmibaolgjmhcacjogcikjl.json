{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "Zola-Favicon-teal-128.png",
    "default_title": "Add to Zola"
  },
  "name": "Add to Zola Button",
  "short_name": "Add to Zola",
  "description": "Add anything from any store to your Zola registry",
  "homepage_url": "https://www.zola.com/",
  "icons": {
    "16": "Zola-Favicon-teal-16.png",
    "48": "Zola-Favicon-teal-48.png",
    "128": "Zola-Favicon-teal-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "version": "2.0",
  "manifest_version": 2,
  "author": "Jason Toris"
}
