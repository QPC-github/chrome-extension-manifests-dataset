{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoAutoplayDisabler",
  "version": "1.2.2",
  "author": "notanimposter",
  "description": "Disables Autoplay on YouTube playlists",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ]
}
