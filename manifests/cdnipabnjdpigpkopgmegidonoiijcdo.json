{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DualShockers PS4 / XBOne Tracker",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "Track available next gen consoles after entering your zip code.",
  "icons": {
    "16": "images/DSLogoBOLT16.png",
    "128": "images/DSLogoBOLT128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/DSLogoBOLT19.png",
      "38": "images/DSLogoBOLT38.png"
    },
    "default_title": "DualShockers PS4/XBOne Tracker",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "webRequest",
    "geolocation",
    "http://www.dualshockers.com/",
    "http://www.target.com/",
    "http://m.target.com/",
    "http://www.walmart.com/",
    "https://mobile.walmart.com/",
    "http://www.bestbuy.com/",
    "http://m.bestbuy.com/",
    "http://www.gamestop.com/",
    "http://m.gamestop.com/"
  ]
}
