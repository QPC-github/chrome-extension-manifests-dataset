{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AJC Blocker",
  "description": "Block idiot trolls from AJC blogs",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*.ajc.com/*"
      ],
      "js": [
        "jquery.min.js",
        "AJCBlocker.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
