{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Serially",
  "description": "Aggiungi le news preferite nei tuoi magazine Serially",
  "version": "0.7",
  "icons": {
    "48": "logo-small.png"
  },
  "browser_action": {
    "default_icon": "logo-small.png",
    "default_popup": "init.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
