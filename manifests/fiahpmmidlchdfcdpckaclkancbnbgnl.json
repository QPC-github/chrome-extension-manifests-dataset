{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Progress速 Test Studio速 Chrome Recorder",
  "version": "2017.2.823.0",
  "description": "Progress速 Test Studio速 Chrome Recorder Extension",
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "homepage_url": "http://www.telerik.com/teststudio/",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "history",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "injector.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "images/*.png",
    "RecorderFiles/TestStudio/*"
  ]
}
