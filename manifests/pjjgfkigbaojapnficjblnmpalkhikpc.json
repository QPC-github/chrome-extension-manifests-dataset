{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Morze",
  "description": "Chrome extension to turn webpage-text into morse code!",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icons/morze_default_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "morze.js"
      ]
    }
  ]
}
