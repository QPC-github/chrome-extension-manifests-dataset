{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Login Saver",
  "description": "Saves logins to some websites -- currently only farnell.com",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.farnell.com/*",
        "https://*.farnell.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
