{
  "author": "TJ",
  "name": "FooSearch",
  "short_name": "FooSearch",
  "homepage_url": "https://FooBaroo.com",
  "description": "The FooBaroo search extension makes it extremely easy to find answers on FooBaroo.com",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "FooSearch"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://FooBaroo.com/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "FooBaroo",
      "name": "FooBaroo",
      "search_url": "https://FooBaroo.com/index.php?a=web&q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "1.js",
      "2.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "1.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ]
}
