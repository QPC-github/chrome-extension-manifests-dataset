{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Get Referrer URL",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
