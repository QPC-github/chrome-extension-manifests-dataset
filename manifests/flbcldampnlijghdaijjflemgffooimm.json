{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Save and organize Chrome tabs by lists ",
  "version": "0.2.0",
  "name": "Tabs Later",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "src/img/logo-34.png"
  },
  "permissions": [
    "tabs",
    "unlimitedStorage"
  ],
  "icons": {
    "128": "src/img/logo-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
