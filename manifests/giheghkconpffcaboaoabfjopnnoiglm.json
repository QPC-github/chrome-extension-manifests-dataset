{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Finder Bookmarks",
  "version": "1.0.12",
  "author": "Guokai Han",
  "manifest_version": 2,
  "description": "A bookmarks manager like macOS Finder.",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "chrome_url_overrides": {
    "bookmarks": "bookmarks.html"
  },
  "browser_action": {},
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "bookmarks",
    "storage",
    "chrome://favicon/"
  ]
}
