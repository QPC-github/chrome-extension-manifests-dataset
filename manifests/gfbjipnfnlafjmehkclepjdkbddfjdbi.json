{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Rdio Search",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Search the Rdio catalog from anywhere in Chrome with a painless right click.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
