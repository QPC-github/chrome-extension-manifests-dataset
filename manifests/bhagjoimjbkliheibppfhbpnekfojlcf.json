{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JIRA Personal Dashboard",
  "short_name": "JPD",
  "description": "A Chrome extension to organize your JIRA's issues!",
  "version": "4.0.1",
  "browser_action": {
    "default_icon": "images/jpd-icon-19.png",
    "default_title": "JIRA Personal Dashboard",
    "default_popup": "app.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "alarms",
    "<all_urls>",
    "storage",
    "webRequest"
  ],
  "background": {
    "scripts": []
  },
  "icons": {
    "19": "images/jpd-icon-19.png",
    "48": "images/jpd-icon-48.png",
    "128": "images/jpd-icon.png"
  },
  "options_page": "options.html"
}
