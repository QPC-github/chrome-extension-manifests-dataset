{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "jquery.min.js",
        "Amazon.js",
        "BarnesAndNoble.js"
      ],
      "matches": [
        "http://*.amazon.com/*",
        "https://*.amazon.com/*",
        "http://*.barnesandnoble.com/*",
        "https://*.barnesandnoble.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "description": "Adds search links to Yale's library system on the product pages for books on popular websites.",
  "name": "Yale Orbis Search",
  "version": "1.0"
}
