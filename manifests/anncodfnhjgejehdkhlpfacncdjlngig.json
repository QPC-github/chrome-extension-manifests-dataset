{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Onlyhitss",
  "version": "1.2",
  "description": "Onlyhitss.com brings you high quality tamil and english songs for free ",
  "icons": {
    "48": "img/48x48.png",
    "16": "img/16x16.png",
    "128": "img/128x128.png"
  },
  "browser_action": {
    "default_icon": "img/128x128.png",
    "default_title": "Search Music from Onlyhitss.com",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
