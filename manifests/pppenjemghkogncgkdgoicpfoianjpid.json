{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OpenTouch Meeting Manager",
  "short_name": "OT Meeting",
  "version": "2.20.6203",
  "manifest_version": 2,
  "description": "OpenTouch Meeting Manager is a Google Chrome packaged app for scheduling meetings",
  "icons": {
    "128": "otmeetingmanager_128.png",
    "48": "otmeetingmanager_48.png"
  },
  "permissions": [
    "system.display",
    "storage",
    "https://www.google-analytics.com/",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "unlimitedStorage",
    "alwaysOnTopWindows",
    "notifications",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
