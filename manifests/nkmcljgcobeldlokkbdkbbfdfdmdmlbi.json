{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WareJects",
  "version": "1.4",
  "description": "Manage your software and hardware project ideas.",
  "permissions": [
    "storage",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/popup.js",
        "js/main.js"
      ]
    }
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "17",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "logo.png"
  }
}
