{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark Backup",
  "version": "1.8",
  "icons": {
    "128": "assets/browser_bookmark.png"
  },
  "manifest_version": 2,
  "description": "Automatically backup and restore your bookmarks.",
  "permissions": [
    "bookmarks",
    "unlimitedStorage",
    "alarms",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}
