{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Facebook Messenger Quick Switcher",
  "description": "Quickly navigate between your facebook messenger conversations",
  "version": "0.3",
  "icons": {
    "128": "facebook.png"
  },
  "permissions": [
    "https://www.messenger.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.messenger.com/*"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "switch.js"
      ]
    }
  ]
}
