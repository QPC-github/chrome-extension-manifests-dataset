{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Theokoles",
  "description": "Theokoles Bildiri",
  "manifest_version": 2,
  "version": "1.0.13",
  "background": {
    "persistent": true,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Theokoles",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "background"
  ],
  "icons": {
    "128": "icon.png"
  }
}
