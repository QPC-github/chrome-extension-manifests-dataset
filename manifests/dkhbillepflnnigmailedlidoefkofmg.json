{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Looper",
  "description": "Loop your favorite youtube music!",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Loop it!",
    "default_icon": "loop.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
