{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Replicates the current tab",
  "description": "Replicates the current tab 1 to 10 times",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Replicates the current tab",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
