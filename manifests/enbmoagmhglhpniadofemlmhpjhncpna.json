{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vein: a New Social Reading Service.",
  "short_name": "Vein",
  "homepage_url": "https://open.vein.space/",
  "description": "チームを強くする、ソーシャルブックマークサービス",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "externally_connectable"
  ],
  "externally_connectable": {
    "matches": [
      "https://open.vein.space/*",
      "http://localhost:8080/*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "vein_chrome_extension",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "version": "0.1.14"
}
