{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wihopa",
  "version": "1.2",
  "description": "Wihopa.com chrome extension. For bookmark to wihopa.",
  "permissions": [
    "bookmarks",
    "tabs",
    "http://wihopa.com/*"
  ],
  "browser_action": {
    "default_title": "Wihopa",
    "default_icon": "wihopa.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
