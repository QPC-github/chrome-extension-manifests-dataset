{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chreemi: Meemi Notifier",
  "icons": {
    "128": "logo-meemi.png"
  },
  "version": "2.1.5",
  "description": "Resta aggiornato sulle attivit√† di Meemi e controlla quello che viene pubblicato!",
  "background": {
    "scripts": [
      "config.js",
      "jquery.js",
      "background.js"
    ]
  },
  "options_page": "option.html",
  "browser_action": {
    "default_icon": "meemi-icon.png",
    "default_title": "Condividi su Meemi.",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
