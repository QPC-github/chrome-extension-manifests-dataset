{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Words Learning",
  "short_name": "WL",
  "version": "1.1.1",
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "tts"
  ],
  "icons": {
    "16": "dist/logo.png",
    "48": "dist/logo.png"
  },
  "options_page": "dist/options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "dist/sprite.png"
  ],
  "browser_action": {
    "default_title": "Words Learning",
    "default_popup": "dist/popup.html"
  }
}
