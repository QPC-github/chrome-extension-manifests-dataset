{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "lanacion.com - ultimas noticias - LANACION",
  "version": "2.03",
  "description": "¡Entérate Las Últimas noticias en tu escritorio!",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "notifications",
    "<all_urls>",
    "background",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.11.1.min.js",
      "js/background.js",
      "js/jquery.transit.min.js"
    ]
  },
  "browser_action": {
    "default_title": "lanacion.com - ultimas noticias - LANACION",
    "default_icon": "img/icon.png",
    "default_popup": "options.html"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "img/icon.png",
    "img/lanacionchrome.png"
  ]
}
