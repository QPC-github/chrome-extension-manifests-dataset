{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PrivaSee",
  "description": "Privacy for Noobs",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/19-bad.png",
    "default_title": "Toggle Safe-mode"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "css/contentscript.css"
      ],
      "js": [
        "js/jquery-1.7.2.min.js",
        "js/contentscript.js"
      ]
    }
  ],
  "permissions": [
    "declarativeWebRequest",
    "http://*/*",
    "tabs",
    "background"
  ]
}
