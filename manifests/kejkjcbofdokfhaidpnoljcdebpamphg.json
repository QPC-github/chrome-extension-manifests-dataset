{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nice filter Plugin",
  "description": "",
  "version": "1.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.ogcnice.net/*"
      ],
      "js": [
        "filter.js"
      ]
    }
  ]
}
