{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gogetme Search",
  "description": "Quick search by select and a single click.",
  "version": "1.0.5",
  "permissions": [
    "storage",
    "webNavigation",
    "*://*/*"
  ],
  "web_accessible_resources": [
    "images/*",
    "script/*",
    "fonts/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "icons": {
    "48": "images/63x63.jpg"
  },
  "browser_action": {
    "default_title": "Gogetme - press to options."
  },
  "manifest_version": 2
}
