{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better New Tab",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Better New Tab is an extension the brings back the Classic Chrome New Tab Page.",
  "background": {
    "scripts": [
      "js/URI.min.js",
      "js/config.js",
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "default_locale": "en",
  "icons": {
    "128": "images/icon/icon128.png",
    "16": "images/icon/icon16.png",
    "48": "images/icon/icon48.png"
  },
  "permissions": [
    "sessions",
    "tabs",
    "<all_urls>",
    "chrome://favicon/",
    "management",
    "history",
    "bookmarks",
    "topSites",
    "unlimitedStorage",
    "storage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
