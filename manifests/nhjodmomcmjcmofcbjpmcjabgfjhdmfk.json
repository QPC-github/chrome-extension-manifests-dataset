{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SelectConverter",
  "version": "1.1.3",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "common.js"
      ]
    }
  ]
}
