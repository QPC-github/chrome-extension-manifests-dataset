{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0.4",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon-128.png",
    "240": "icon-240.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*.twitter.com/*",
    "*://*.facebook.com/*",
    "*://*.instagram.com/*",
    "*://*.reddit.com/*",
    "*://*.tumblr.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "commands": {
    "open_logger": {
      "description": "Copy top Go Link for current tab.",
      "suggested_key": {
        "default": "Shift+Ctrl+1"
      },
      "global": true
    }
  }
}
