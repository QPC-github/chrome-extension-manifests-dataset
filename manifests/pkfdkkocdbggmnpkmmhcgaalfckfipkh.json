{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sea Reader",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Simple way to read articles on the Seattle Times website",
  "homepage_url": "http://ryanlane.com/products/sea-reader",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Sea Reader",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://seattletimes.com/*",
    "*://blogs.seattletimes.com/*",
    "webRequestBlocking"
  ]
}
