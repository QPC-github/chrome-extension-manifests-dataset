{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BlockVK",
  "description": "Удаляет рекламу из ленты ВК.  ",
  "version": "1.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://new.vk.com/*",
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "run_at": "document_end"
    }
  ]
}
