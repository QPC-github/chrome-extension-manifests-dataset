{
  "background": {
    "persistent": true,
    "page": "bg.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon.png",
      "38": "img/icon@2x.png"
    },
    "default_popup": "popup.html",
    "default_title": "Bitcoin assistant"
  },
  "description": "Bitcoin assistant",
  "icons": {
    "128": "img/app_icon128.png",
    "16": "img/app_icon16.png",
    "48": "img/app_icon48.png"
  },
  "manifest_version": 2,
  "name": "Bitcoin assistant",
  "permissions": [
    "notifications",
    "storage",
    "alarms",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "unlimitedStorage",
    "pageCapture",
    "webNavigation",
    "idle",
    "<all_urls>",
    "https://github.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:;",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.11"
}
