{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "InStock",
  "description": "Check for hidden stock status at Walmart.com",
  "version": "1.0",
  "icons": {
    "16": "bullet-16.png",
    "48": "bullet-48.png",
    "128": "bullet-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "author": "William Holt",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Check Walmart Stock",
    "default_icon": "bullet.png"
  },
  "manifest_version": 2
}
