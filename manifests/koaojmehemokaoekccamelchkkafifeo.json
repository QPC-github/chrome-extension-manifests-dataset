{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "豆瓣FM原味版2017",
  "short_name": "豆瓣FM",
  "version": "5.4.0",
  "manifest_version": 2,
  "description": "原味呈现官方豆瓣电台，更简单，更易用。",
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_title": "豆瓣FM原味版2017",
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "script/jquery-3.1.1.min.js",
      "script/setting.js",
      "script/cache.js",
      "script/com.js",
      "script/player.js",
      "script/douban.js",
      "script/background.js"
    ]
  },
  "permissions": [
    "notifications",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "https://douban.fm/",
    "https://douban.com/",
    "https://www.douban.com/",
    "http://*.douban.com/"
  ]
}
