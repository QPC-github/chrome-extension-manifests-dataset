{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ffocus",
  "version": "1.0",
  "description": "Focus on what you want to read",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "ffocus.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
