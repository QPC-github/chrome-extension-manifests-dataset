{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ProxyMate",
  "version": "1.5",
  "manifest_version": 2,
  "description": "ProxyMate. Safe, reliable proxy.",
  "background": {
    "page": "main.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "proxy",
    "tabs",
    "<all_urls>"
  ],
  "minimum_chrome_version": "13.0.0"
}
