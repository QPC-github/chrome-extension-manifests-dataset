{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ROBOhawk SportNav",
  "short_name": "SportNav",
  "description": "Consolidates game page links for quick navigation. NHL, MLB, NBA, NCAAB, Soccernet.",
  "version": "1.2.12",
  "icons": {
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
