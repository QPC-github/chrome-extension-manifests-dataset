{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "\"Такском\" Выпуск сертификатов",
  "short_name": "Такском МВС",
  "version": "1.0.0.6",
  "manifest_version": 2,
  "description": "Дополнение для Мастера выпуска сертификатов \"Такском\"",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "app-certificat.png"
  },
  "permissions": [
    "nativeMessaging",
    "https://*.taxcom.ru/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.taxcom.ru/*"
    ]
  }
}
