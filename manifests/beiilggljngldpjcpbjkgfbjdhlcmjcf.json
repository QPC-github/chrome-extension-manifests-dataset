{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sumaprop",
  "short_name": "Sumaprop",
  "description": "Sumaprop es un sistema de gesti√≥n inmobiliaria online que permite gestionar propiedades y contactos de manera simple y eficiente.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Ver nuevas consultas",
    "default_popup": "js/popup.html"
  },
  "web_accessible_resources": [
    "img/loading.gif"
  ],
  "permissions": [
    "gcm",
    "tabs",
    "cookies",
    "http://administrador.sumaprop.com/",
    "storage",
    "notifications"
  ]
}
