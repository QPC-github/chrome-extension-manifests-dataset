{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free Kindle UK Books",
  "description": "Free Kindle UK Books are directly in your browser",
  "version": "1.0",
  "permissions": [
    "http://*.freebooksuk.com/"
  ],
  "browser_action": {
    "default_icon": "icon_48.png"
  },
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
