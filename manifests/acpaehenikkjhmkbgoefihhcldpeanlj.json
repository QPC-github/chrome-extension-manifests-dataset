{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "後で見ない",
  "description": "Youtubeの動画のサムネイルに表示される「後で見る」ボタンを消して動画視聴を快適にします。",
  "version": "1.02",
  "content_scripts": [
    {
      "js": [
        "js/background.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "後で見ない"
  },
  "permissions": [
    "activeTab",
    "https://www.youtube.com/*"
  ],
  "manifest_version": 2
}
