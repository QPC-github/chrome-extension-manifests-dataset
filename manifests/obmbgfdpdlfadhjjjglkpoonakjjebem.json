{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search for 4Chan",
  "description": "Search 4Chan from the toolbar.",
  "version": "1.0",
  "permissions": [
    "http://4chan.org/*",
    "https://4chan.org/*",
    "http://*.4chan.org/*",
    "tabs",
    "https://*.4chan.org/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}
