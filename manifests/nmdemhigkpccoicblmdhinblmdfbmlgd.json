{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "pkm getcookie",
  "version": "1.2",
  "description": "获取网页的cookie值!",
  "key": "",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
