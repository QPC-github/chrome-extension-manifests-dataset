{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MostCool",
  "description": "MostCool",
  "version": "1.3",
  "browser_action": {
    "default_icon": "img/48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "64": "img/64.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "notifications",
    "https://mostcool.top/*",
    "http://*/*",
    "https://*/*"
  ]
}
