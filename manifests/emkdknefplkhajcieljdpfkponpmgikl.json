{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Voiper",
  "description": "Shows some of the features of the Context Menus API",
  "version": "0.6",
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "storage",
    "https://voiper-dev.herokuapp.com/*",
    "https://voiper.herokuapp.com/*"
  ],
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2,
  "icons": {
    "16": "Icon16.png",
    "48": "Icon48.png",
    "128": "Icon128.png"
  }
}
