{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NesQuick",
  "author": "El Mounachit Rachid",
  "description": "Create account easly by NesQuick, Choose your prefered email prefix and password... and let NesQuick do the rest.",
  "version": "2.6",
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Quick Test Account Nespresso",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "chrome-search://local-ntp/local-ntp.html"
  ],
  "icons": {
    "16": "img/favicon-16x16.png",
    "48": "img/android-icon-48x48.png"
  }
}
