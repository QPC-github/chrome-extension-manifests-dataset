{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Colorea tu Facebook",
  "version": "0.0.11",
  "manifest_version": 2,
  "description": "Ponle color a tu facebook,aqui tienes disponible miles de plantillas para que puedas cambiar el dise√±o de tu facebook",
  "homepage_url": "http://www.coloreatuface.com",
  "converted_from_user_script": true,
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*",
        "*://www.coloreatuface.com/*"
      ],
      "js": [
        "libjscolor.js",
        "noveled100.js"
      ],
      "css": [
        "jirtsEsroPERko.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "/images/open.png",
    "/images/close.png"
  ],
  "browser_action": {
    "default_icon": "icon/24.png",
    "default_title": "Color!",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/16.png",
    "24": "icon/24.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "storage"
  ]
}
