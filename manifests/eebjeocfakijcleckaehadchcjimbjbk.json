{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.0",
  "name": "S端ddeutsche / n-tv Bildergalerie Navigation",
  "short_name": "S端ddeutsche / n-tv Navigation",
  "description": "Navigation per Pfeiltasten f端r die Bildergalerien der S端ddeutschen Zeitung und von n-tv",
  "author": "Lukas Schulze",
  "homepage_url": "https://aldago.com",
  "permissions": [
    "tabs",
    "storage",
    "*://www.sueddeutsche.de/*",
    "*://www.n-tv.de/*"
  ],
  "icons": {
    "16": "icons/newspaper_16.png",
    "48": "icons/newspaper_48.png",
    "128": "icons/newspaper_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.sueddeutsche.de/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/sueddeutsche.js"
      ]
    },
    {
      "matches": [
        "*://www.n-tv.de/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/ntv.js"
      ]
    }
  ],
  "offline_enabled": false
}
