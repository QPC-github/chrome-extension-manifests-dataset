{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "ajax.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "description": "New message notifications on chat.stylemistake.com",
  "icons": {
    "128": "icon_128.png"
  },
  "name": "Style Mistake Chat notifications",
  "permissions": [
    "tabs",
    "notifications",
    "http://chat.stylemistake.com/"
  ],
  "web_accessible_resources": [
    "icon_128.png"
  ],
  "version": "1.3"
}
