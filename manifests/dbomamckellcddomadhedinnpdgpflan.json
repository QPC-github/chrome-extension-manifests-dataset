{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CETC.AE Publisher",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Publish to cetc.ae website",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Post to cetc.ae"
  },
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
