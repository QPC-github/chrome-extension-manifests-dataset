{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Music and Lyrics",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Get the latest music news from Music and Lyrics.",
  "icons": {
    "128": "logo.png",
    "48": "4848.png"
  },
  "browser_action": {
    "default_title": "Music and Lyrics",
    "default_icon": "logo.png",
    "default_popup": "feed.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*.directlyrics.com/*"
  ]
}
