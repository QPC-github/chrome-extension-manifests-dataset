{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StreamingBot",
  "description": "Get direct video links, say goodbye to clickbaits and invasive ads.",
  "version": "1.4",
  "browser_action": {
    "default_title": "StreamingBot",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "icons": {
    "128": "tv.png"
  }
}
