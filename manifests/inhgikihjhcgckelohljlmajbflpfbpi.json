{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stigmabase Updates",
  "description": "Worldwide updates about social exclusion | Dedicated to education and everyone concerned with social exclusion.",
  "minimum_chrome_version": "35.0.0.0",
  "version": "5.12",
  "icons": {
    "16": "stigma-16.png",
    "128": "stigma-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "config.js",
        "main.js"
      ]
    }
  },
  "permissions": [
    "contextMenus",
    "webview"
  ]
}
