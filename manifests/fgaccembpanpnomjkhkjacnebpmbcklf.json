{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "License Calculator",
  "description": "This calculator is used to Upgrade and Downgrade license keys",
  "version": "1.5.0",
  "icons": {
    "16": "img/img.png",
    "48": "img/img.png",
    "128": "img/img.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/moment.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Click to view the Calculator",
    "default_popup": "popup.html"
  }
}
