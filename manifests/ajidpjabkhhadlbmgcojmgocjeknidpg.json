{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Logo Redirect",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Redirect click on youtube logo to feed/subscriptions",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "modify_page.js"
      ],
      "run_at": "document_end"
    }
  ]
}
