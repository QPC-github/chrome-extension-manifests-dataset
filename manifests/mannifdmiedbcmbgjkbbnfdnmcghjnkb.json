{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kibosh Filter Plugin",
  "description": "This extension will manage the Kibosh Filter.",
  "version": "1.1",
  "author": "Kibosh",
  "background": {
    "scripts": [
      "/js/bg_kibosh.js"
    ]
  },
  "browser_action": {
    "default_icon": "/img/kibosh_icon.png",
    "default_popup": "kibosh.html"
  },
  "permissions": [
    "storage",
    "cookies",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "browsingData"
  ]
}
