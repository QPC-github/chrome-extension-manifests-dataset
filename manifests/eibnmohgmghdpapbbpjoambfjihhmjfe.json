{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RabotuNaidu.ru Поиск работы на дому",
  "description": "Поиск работы на дому. Плагин для Chrome",
  "version": "1.1",
  "permissions": [
    "http://rabotunaidu.ru/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "i128.png",
    "48": "i48.png",
    "16": "i16.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "RabotuNaidu.ru Поиск работы на дому"
  }
}
