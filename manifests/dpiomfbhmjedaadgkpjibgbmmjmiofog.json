{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tiroir Cultcut",
  "version": "1.0.1.2",
  "manifest_version": 2,
  "description": "Accédez à vos Cuts favoris où que vous soyez sur le Web.",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "homepage_url": "http://www.cultcut.com/",
  "browser_action": {
    "default_icon": "icon_16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies"
  ]
}
