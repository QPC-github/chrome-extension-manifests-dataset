{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "WhoIs",
  "description": "Opens whois.domaintools.com on a new tab showing the details of the current domain.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Open WhoIs",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "manifest_version": 2
}
