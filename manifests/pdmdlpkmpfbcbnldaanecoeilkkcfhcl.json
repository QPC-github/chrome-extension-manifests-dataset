{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clext",
  "version": "1",
  "description": "Grammer Clock in your browser",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "grammer.js"
    ]
  },
  "permissions": [
    "notifications"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
