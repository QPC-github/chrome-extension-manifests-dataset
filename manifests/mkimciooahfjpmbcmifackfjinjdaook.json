{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Eligasht",
  "version": "0.8",
  "description": "Eligashtâ€™s official browser extension. Booking flight, hotels, packages from anywhere to anywhere! including special offers.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "https://www.eligasht.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.3.1.min.js"
      ]
    }
  ],
  "icons": {
    "128": "images/icon.png"
  },
  "omnibox": {
    "keyword": "eli"
  },
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  }
}
