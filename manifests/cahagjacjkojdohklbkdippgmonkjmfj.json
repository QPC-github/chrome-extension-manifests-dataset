{
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "name": "DimiTech.eu",
  "version": "1.0.0.1",
  "description": "Use this application, if you want to be in touch with the news in DimiTech.eu.",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "DimiTech.eu",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.dimitech.eu/*",
    "notifications",
    "alarms",
    "bookmarks"
  ],
  "options_page": "options.html",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2
}
