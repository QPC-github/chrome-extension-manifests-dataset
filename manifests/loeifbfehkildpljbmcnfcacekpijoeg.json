{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YourURL Link Shortener",
  "description": "Shorten your links incredibly quickly using this Chrome Extension.",
  "version": "1.32",
  "browser_action": {
    "default_icon": "fav.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://yoururl.co/*"
  ]
}
