{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "acon.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "AcON",
  "short_name": "AcON",
  "description": "Enable autocomplete on all elements in all forms in all frames.",
  "version": "1.0.0.0"
}
