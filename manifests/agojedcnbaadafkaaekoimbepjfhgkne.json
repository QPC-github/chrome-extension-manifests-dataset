{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CryptoKit.QYWJ Extension",
  "version": "3.4.0.3",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.qywgpo.com/*",
      "https://*.gpocn.com/*",
      "https://*.gpoqyw.com/*",
      "https://*.quanyaowang.com/*",
      "http://*.test.qywgpo.com/*",
      "http://*.develop.qywgpo.com/*",
      "http://*.api_test.qywgpo.com/*",
      "http://*.api.qywgpo.com/*",
      "http://*.api_test.wei3dian.com/*",
      "http://*.test.wei3dian.com/*",
      "http://*.api.wei3dian.com/*",
      "http://*.develop.wei3dian.com/*",
      "http://*.develop.api.wei3dian.com/*",
      "http://*.test.api.qywgpo.com/*",
      "http://*.develop.qywgpo.com/*",
      "http://*.develop.api.qywgpo.com/*",
      "http://*.test.qywgpo.com/*",
      "http://*.test.api.qywgpo.com/*",
      "http://*.develop.gpocn.com/*",
      "http://*.develop.api.gpocn.com/*",
      "http://*.test.gpocn.com/*",
      "http://*.test.api.gpocn.com/*",
      "http://*.demo.qywgpo.com/*"
    ]
  }
}
