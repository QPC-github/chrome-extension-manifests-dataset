{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickConfig",
  "description": "An extension to quickly map out QuickBase table dbid's, field names and field IDs.",
  "version": "0.1.0",
  "background": {
    "scripts": [
      "js/check_page_url.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "19": "images/logo.png"
    },
    "default_title": "QuickConfig",
    "default_popup": "index.html"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "clipboardWrite",
    "https://*.quickbase.com/"
  ]
}
