{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "12": "TwitchR9Ksmall.png",
    "48": "TwitchR9Kmed.png",
    "128": "TwitchR9Kbig.png"
  },
  "name": "TwitchR9K",
  "version": "1.0",
  "description": "Hide unoriginal messages in Twitch chat.",
  "page_action": {
    "default_title": "TwitchR9K"
  },
  "permissions": [
    "tabs",
    "http://www.twitch.tv/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.twitch.tv/*"
      ],
      "js": [
        "jquery.js",
        "robot9000.js"
      ],
      "run_at": "document_end"
    }
  ]
}
