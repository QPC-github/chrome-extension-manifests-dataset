{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "version": "1.7",
  "default_locale": "fr",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery-2.1.3.min.js",
      "grabeezy.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Instant add my tab URL to my Grabeezy account",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "manifest_version": 2
}
