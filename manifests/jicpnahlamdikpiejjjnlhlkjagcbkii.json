{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LaCie Network Space 2 Download",
  "description": "Adds a download using the device's web interface.",
  "version": "1.1",
  "author": "Per Rosengren",
  "permissions": [
    "contextMenus",
    "cookies"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "sha1.js",
      "background.js"
    ]
  },
  "icons": {
    "128": "lacie2.png"
  },
  "browser_action": {
    "default_title": "LaCie Network Space 2 Download - Options",
    "default_icon": "lacie2.png",
    "default_popup": "options.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
