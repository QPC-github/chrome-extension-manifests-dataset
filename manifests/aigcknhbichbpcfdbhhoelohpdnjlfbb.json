{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "ServiceNow Framerizer"
  },
  "name": "ServiceNow Framerizer",
  "description": "Click, or press CTRL+SHIFT+S to drop the current ServiceNow page into the ServiceNow frame - By The SN Guys: www.snprotips.com",
  "homepage_url": "http://snprotips.com/sn-framerizer/",
  "icons": {
    "16": "icon-16.gif",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+S",
        "mac": "Ctrl+Shift+S",
        "chromeos": "Ctrl+Shift+S",
        "linux": "Ctrl+Shift+S"
      }
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.3.4",
  "manifest_version": 2
}
