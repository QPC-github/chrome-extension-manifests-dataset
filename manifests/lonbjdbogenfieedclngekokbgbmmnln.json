{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/jingwang.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/styles.css"
      ],
      "js": [
        "js/common.js",
        "js/ecom/jingwang_serp_ad.js",
        "js/ecom/jingwang_ecom_zx_hotel.js",
        "js/ecom/jingwang_ecom_zx_jipiao.js",
        "js/ecom/jingwang_ecom_zx_jiaoyu.js",
        "js/ecom/jingwang_ecom_zx_yiliao.js",
        "js/ps/jingwang_serp_ps.js",
        "js/ps/jingwang_serp_zx.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "icons": {
    "128": "images/jingwang.png",
    "48": "images/jingwang.png"
  },
  "web_accessible_resources": [
    "images/logo.png",
    "images/jingwang.png",
    "images/bg.png",
    "images/cai.png",
    "images/cai.GIF",
    "images/cai2.gif",
    "images/ding.gif"
  ],
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "options.html",
  "description": "__MSG_appDesc__",
  "default_locale": "zh_CN",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*"
  ],
  "version": "2.8.3"
}
