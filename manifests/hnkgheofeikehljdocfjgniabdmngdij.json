{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Napk-in Extension",
  "version": "1.0",
  "description": "Napk-in",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "clipboardWrite",
    "clipboardRead",
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "16": "images/AppIcon60x60@2x.png",
    "48": "images/AppIcon60x60@2x.png",
    "128": "images/appIcon128x128.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_icon": "images/appIcon128x128.png",
    "default_title": "Napk-in Extension",
    "default_popup": "html/popup.html"
  }
}
