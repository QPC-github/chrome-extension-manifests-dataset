{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chinese Odyssey Fix",
  "description": "Fixes Chinese Odyssey online audio for Chrome.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.mychinesehub.com/*"
      ],
      "js": [
        "applyFix.js"
      ]
    }
  ],
  "icons": {
    "16": "Logo16.png",
    "48": "Logo48.png",
    "128": "Logo128.png"
  },
  "web_accessible_resources": [
    "fix.js"
  ]
}
