{
  "authors": [
    {
      "email": "vibinreji123@gmail.com",
      "name": "SmartMoneyBack"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "SmartMoneyBack Lite"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://www.googletagmanager.com https://www.google.com ; object-src 'self'",
  "description": "Get the best price on 10 million+ products across 100+ online stores. Find more! Save more!",
  "homepage_url": "http://smartmoneyback.com/",
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "manifest_version": 2,
  "name": "SmartMoneyBack Lite",
  "permissions": [
    "tabs",
    "alarms",
    "http://*/*",
    "https://*/*",
    "cookies",
    "notifications",
    "contextMenus",
    "gcm",
    "identity",
    "unlimitedStorage",
    "storage",
    "geolocation",
    "idle",
    "webNavigation",
    "clipboardWrite",
    "identity.email",
    "management",
    "history"
  ],
  "short_name": "SmartMoneyBack",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
