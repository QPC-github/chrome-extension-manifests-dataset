{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Pad",
  "version": "0.1",
  "description": "Each new page opens with a beautiful tab, write anything and save it.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "textedit.html"
  }
}
