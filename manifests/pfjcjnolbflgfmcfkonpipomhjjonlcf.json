{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "untermStrich Browser Extension",
  "version": "1.6.1",
  "minimum_chrome_version": "28",
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  },
  "default_locale": "en",
  "description": "__MSG_title__",
  "options_page": "options/index.html",
  "background": {
    "scripts": [
      "libs/micro_ajax.lib.js",
      "libs/background.lib.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon_19b.png"
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
