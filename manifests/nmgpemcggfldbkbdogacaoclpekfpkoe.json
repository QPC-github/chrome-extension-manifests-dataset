{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tube Video Looper",
  "description": "This extension loops youtube for your favorite song",
  "version": "1.0",
  "background": {
    "scripts": [
      "looper.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
