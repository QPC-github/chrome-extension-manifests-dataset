{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "D365FO-NavBarColor",
  "short_name": "D365FO-NavBarColor",
  "description": "A chrome extension that changes the color of navigation bar in D365 FO.",
  "version": "3.2.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "URLColors",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
