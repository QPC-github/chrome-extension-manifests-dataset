{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Synonyms",
  "description": "This extension searches dictionary.com to get the meaning of the selected word.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "dic.png",
    "default_popup": "syn.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
