{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "板栗节点 ",
  "description": "轻易看外面的世界",
  "version": "2.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./view/view.html",
    "default_icon": "./icon/icon.png"
  },
  "icons": {
    "16": "./icon/icon.png",
    "128": "./icon/icon.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "MacCtrl+Shift+Q"
      },
      "description": "轻易看外面的世界"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
