{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Accents Web",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Cette extension permet de faciliter l'ajout d'accents pour les d√©veloppeurs web qui ne travaillent pas avec l'encodage UTF-8.",
  "icons": {
    "16": "images/accents16x16.png",
    "48": "images/accents48x48.png",
    "128": "images/accents128x128.png"
  },
  "browser_action": {
    "default_icon": "images/accents128x128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ]
}
