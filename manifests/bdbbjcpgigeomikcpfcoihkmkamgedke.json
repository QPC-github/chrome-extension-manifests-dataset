{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoBoard Screen Sharing",
  "author": "Tutor Matching Service",
  "version": "0.0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This extension only captures the stream from your screen to share it on GoBoard.com",
  "homepage_url": "https://goboard.com/",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.goboard.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "logo128.png"
  ]
}
