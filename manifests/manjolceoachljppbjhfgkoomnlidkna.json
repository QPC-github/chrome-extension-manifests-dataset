{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_fullName__",
  "short_name": "Surfe.be",
  "description": "__MSG_extDesc__",
  "version": "1.4.3",
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "*://surfe.be/*",
    "*://*.surfe.be/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://surfe.be/*",
      "*://*.surfe.be/*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "file:///*",
        "ftp://*/*",
        "*://mail.google.com/*",
        "*://mail.yandex.ru/*",
        "*://mail.yandex.ua/*",
        "*://mail.yandex.kz/*",
        "*://e.mail.ru/*",
        "*://mail.protonmail.com/*"
      ],
      "js": [
        "content/jquery.min.js",
        "content/json2html.js",
        "content/jquery.json2html.js",
        "content/CommonMemory.js",
        "content/nanobar.js",
        "content/countUp.js",
        "content/ifvisible.js",
        "popup/purify.js",
        "content/track.js",
        "content/content.js"
      ],
      "css": [
        "content/content.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "content/CommonMemory.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Surfe.be",
    "default_popup": "popup/popup.html",
    "default_icon": "img/icon16.png"
  },
  "web_accessible_resources": [
    "fonts/Montserrat.woff2"
  ],
  "manifest_version": 2,
  "homepage_url": "https://surfe.be/earn/"
}
