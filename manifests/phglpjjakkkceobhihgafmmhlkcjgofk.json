{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Genealogie Werkbalk",
  "description": "Doorzoek snel Nederlandse genealogische bronnen voor extra informatie voor uw stamboom.",
  "homepage_url": "http://www.genealogiewerkbalk.nl/",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/gw.png",
    "default_popup": "gw.html"
  },
  "permissions": [
    "http://www.genealogiewerkbalk.nl/"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
