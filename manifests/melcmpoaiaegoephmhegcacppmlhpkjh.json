{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "HandyIP",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adds a toolbar button that shows your IP address in a clean UI.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://api.externalip.net/"
  ]
}
