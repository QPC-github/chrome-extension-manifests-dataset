{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pauper Legal",
  "description": "An extension for quickly checking the legality of magic cards.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "https://api.scryfall.com/*"
  ],
  "icons": {
    "128": "planeswalker1.png"
  }
}
