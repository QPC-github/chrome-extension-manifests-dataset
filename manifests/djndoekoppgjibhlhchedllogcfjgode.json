{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "CA Technologies",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "description": "Save the things you find on the Web to Catchfly.",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "name": "Catchfly Save Button",
  "short_name": "Catchfly",
  "version": "1.0.0"
}
