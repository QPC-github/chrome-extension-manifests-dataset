{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy Access Login",
  "description": "This toolbar help user to provide easy login access ",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "toolbar.html",
    "style.css"
  ]
}
