{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disable Reddit's Next Button",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Time is the most important commodity. Take control.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*",
        "http://*.reddit.com/*"
      ],
      "css": [
        "mystyles.css"
      ]
    }
  ]
}
