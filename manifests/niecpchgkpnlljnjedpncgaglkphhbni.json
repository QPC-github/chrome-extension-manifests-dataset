{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Script Analyzer",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Custom debugging capabilities.",
  "devtools_page": "devtools.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
