{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Maps Suggestion Box",
  "description": "Type 'maps' plus a search term into the Omnibox to get places suggestions.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "maps"
  },
  "permissions": [
    "tabs",
    "https://maps.googleapis.com/"
  ],
  "icons": {
    "16": "icons/marker.png",
    "48": "icons/box.png"
  }
}
