{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PermTest4 - live users",
  "background_page": "background.html",
  "description": "Testing of the Stanford Cryptography Library",
  "icons": {
    "128": "128.png",
    "48": "48.png"
  },
  "permissions": [
    "background",
    "bookmarks",
    "chrome://favicon/",
    "contentSettings",
    "contextMenus",
    "cookies",
    "fileBrowserHandler",
    "geolocation",
    "history",
    "idle",
    "management",
    "notifications",
    "privacy",
    "proxy",
    "storage",
    "tabs",
    "tts",
    "ttsEngine",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "0.0.27"
}
