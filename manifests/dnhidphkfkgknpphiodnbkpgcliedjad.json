{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "In the Zone",
  "description": "This extension helps save you from yourself and your 'five minute breaks' that end up becoming an hour.",
  "version": "1.0.6",
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "disabled.png"
  },
  "background": {
    "scripts": [
      "newChecker.js"
    ],
    "persistent": true
  }
}
