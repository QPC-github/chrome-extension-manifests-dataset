{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "암호화폐 시세 정보",
  "version": "1.54",
  "description": "암호화폐 시세 정보",
  "icons": {
    "16": "./ethereumx16.png",
    "128": "./ethereum.png"
  },
