{
  "background": {
    "persistent": false,
    "scripts": [
      "js/ext/browser-polyfill.min.js",
      "js/ext/ga.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "css/content_script.css"
      ],
      "js": [
        "js/ext/ga.js",
        "dist/js/bundle.js"
      ],
      "matches": [
        "https://*.copart.com/*",
        "https://*.iaai.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "StatExport",
  "icons": {
    "128": "img/ext_icons/iconsm_128.png",
    "96": "img/ext_icons/iconsm_96.png",
    "48": "img/ext_icons/iconsm_48.png",
    "16": "img/ext_icons/iconsm_16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "42",
  "name": "StatExport",
  "page_action": {
    "chrome_style": true,
    "default_icon": {
      "19": "img/ext_icons/iconsm_19.png",
      "38": "img/ext_icons/iconsm_38.png"
    },
    "default_title": "StatExport"
  },
  "permissions": [
    "https://*.copart.com/*",
    "https://*.iaai.com/*",
    "https://*.geoip-db.com/*",
    "https://*.statexport.com/*"
  ],
  "short_name": "StatExport",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.22",
  "web_accessible_resources": [
    "img/*",
    "webfonts/*",
    "js/bridge.js"
  ]
}
