{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "You Shall Pass",
  "short_name": "You Shall Pass",
  "version": "0.2.0",
  "author": "Shaun Saker for AUX Studios",
  "description": "Save your email address, username and password once. Autofill any  Wordpress pages at the click of a button.",
  "icons": {
    "16": "./img/aux16.png",
    "48": "./img/aux48.png",
    "128": "./img/aux128.png"
  },
  "browser_action": {
    "default_icon": "./img/aux16.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "./js/index.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
