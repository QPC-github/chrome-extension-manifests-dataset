{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chromebook Update Nag Screen",
  "description": "Managed Chromebook - check for chromebook updates",
  "version": "1.0.2",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.min.js"
    ]
  },
  "permissions": [
    "background",
    "identity",
    "identity.email",
    "http://*/",
    "https://*/",
    "tabs",
    "enterprise.deviceAttributes"
  ],
  "manifest_version": 2
}
