{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "LiveDeck",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Make any SpeakerDeck presentation live.",
  "permissions": [
    "https://speakerdeck.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://speakerdeck.com/u/*/p/*"
      ],
      "js": [
        "jquery.min.js",
        "do_it_live.js"
      ]
    }
  ]
}
