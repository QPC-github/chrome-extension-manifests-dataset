{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Digodat Tag Toggle",
  "short_name": "tag toggle",
  "version": "0.6.1",
  "icons": {
    "32": "favicon-ultra-block.png",
    "128": "digodat_logo_big.png"
  },
  "browser_action": {
    "default_popup": "cont.html",
    "default_icon": "favicon-ultra-block.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ]
}
