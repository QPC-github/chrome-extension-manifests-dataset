{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Forex Habercisi (Forex Signal)",
  "description": "Forex Sinyal, finans ve ekonomi dünyası, anlık Forex verileri, Ekonomik Takvim, Finans haberleri",
  "version": "1.8.52.51",
  "browser_action": {
    "default_icon": "128-128FX.png",
    "default_popup": "popup.html"
  },
  "author": "X Trade Brokers",
  "icons": {
    "16": "16-16FX.png",
    "19": "19-19FX.png",
    "38": "38-38FX.png",
    "48": "48-48FX.png",
    "128": "128-128FX.png"
  },
  "background": {
    "scripts": [
      "socketio.js",
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "background",
    "idle"
  ]
}
