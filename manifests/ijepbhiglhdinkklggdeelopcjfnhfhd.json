{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TheBrain - BrainBox",
  "short_name": "BrainBox",
  "version": "0.1.0.5",
  "description": "Instantly capture URLs in BrainBox for use later with TheBrain.",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "BrainBox",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "tabs",
    "storage",
    "contextMenus",
    "https://*.thebrain.com/"
  ],
  "icons": {
    "16": "images/tb10-0016.png",
    "32": "images/tb10-0032.png",
    "48": "images/tb10-0048.png",
    "64": "images/tb10-0064.png",
    "128": "images/tb10-0128.png"
  }
}
