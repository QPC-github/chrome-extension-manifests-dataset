{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Image Peek",
  "short_name": "imgpeek",
  "description": "Opens a Google image search result in a new tab, much like 'view image' used to do. Expand the image preview, then click the icon!",
  "version": "1.1",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Image Peek"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
