{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SalesDirector.ai Assist",
  "version": "1.3.0",
  "version_name": "1.3.0 2018-06-02",
  "description": "SalesDirector.ai Assist",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "storage",
    "webNavigation",
    "contextMenus",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_title": "Click to enable/disable SalesDirector.ai Assist"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "components/jquery.js",
        "components/marked.min.js",
        "contentscripts/highlight.js",
        "contentscripts/topbar.js",
        "contentscripts/gmail/app.js"
      ],
      "css": [
        "css/main.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "components/topbar/gmail/view.html",
    "components/topbar/linkedin/view.html"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
