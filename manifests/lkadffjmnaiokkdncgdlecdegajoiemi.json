{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bleaner",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "This cool extension removes bloat for the page leading to a faster leaner browsing experience",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://*/*",
    "tabs",
    "https://*/*"
  ],
  "commands": {
    "Bleaner": {
      "suggested_key": {
        "default": "Alt+Shift+B"
      },
      "description": "Bleaner"
    }
  },
  "version": "0.1",
  "manifest_version": 2
}
