{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon/icon128.png",
    "default_title": "__MSG_extTitle__"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/jquery.min.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "icon/icon128.png",
    "16": "icon/icon16.png",
    "48": "icon/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "version": "3.5"
}
