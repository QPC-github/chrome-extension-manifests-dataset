{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Emby",
  "short_name": "Emby",
  "description": "Watch your movies, tv shows, live tv from your Emby server",
  "author": "Techywarrior",
  "version": "2.0",
  "icons": {
    "16": "emby_ico_16.png",
    "48": "emby_ico_48.png",
    "128": "emby_ico_128.png"
  },
  "permissions": [
    "webview",
    "storage",
    "fullscreen"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
