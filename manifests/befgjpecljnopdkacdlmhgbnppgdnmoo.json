{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "tr",
  "name": "Vikipedin Reloaded",
  "description": "__MSG_extensionDescription__",
  "author": "MRX",
  "version": "1.0",
  "icons": {
    "32": "icons/32.png",
    "64": "icons/64.png"
  },
  "homepage_url": "http://vikipedin.com/",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.wikipedia.org/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
