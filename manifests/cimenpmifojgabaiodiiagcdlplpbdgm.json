{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BlockKejri",
  "short_name": "BlockKejri",
  "description": "Block all content or news on Kejriwal",
  "version": "1.2",
  "icons": {
    "16": "icon.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "BlockKejri.js"
      ]
    }
  ]
}
