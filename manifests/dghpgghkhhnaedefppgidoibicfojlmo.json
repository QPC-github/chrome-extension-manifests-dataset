{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DotA 2 Match History",
  "short_name": "DMH",
  "version": "1.1",
  "manifest_version": 2,
  "description": "View your last DotA 2 Matches",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "storage",
    "http://api.steampowered.com/*",
    "tabs"
  ]
}
