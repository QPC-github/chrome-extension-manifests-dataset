{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wisp Wishlist",
  "description": "Add items from your favourite online shop to your Wishlists",
  "version": "1.0",
  "icons": {
    "16": "/skin/statusbaricon.png",
    "48": "/skin/icon30.png",
    "128": "/skin/addonicon.png"
  },
  "browser_action": {
    "default_icon": "/skin/icon.png"
  },
  "background": {
    "scripts": [
      "wisp.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
