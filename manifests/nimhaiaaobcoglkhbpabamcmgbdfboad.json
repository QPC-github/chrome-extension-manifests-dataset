{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RegEx Playground",
  "version": "1.0.0",
  "description": "Playground to quickly test your regular expressions (By Domenso Inc.)",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "playground.js"
      ]
    }
  ],
  "icons": {
    "128": "radar.png"
  },
  "browser_action": {
    "default_icon": "radar.png",
    "default_popup": "index.html",
    "default_title": "RegEx Playground"
  }
}
