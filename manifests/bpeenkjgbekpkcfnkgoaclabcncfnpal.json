{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save WIX",
  "description": "Saves a wix site snapshot",
  "version": "1.0",
  "browser_action": {
    "default_icon": "iconw.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "pageCapture",
    "https://ajax.googleapis.com/"
  ]
}
