{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Slack Workast image view",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "An extension that provides an alternative way of viewing images on workast.",
  "content_scripts": [
    {
      "matches": [
        "*://*.workast.io/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
