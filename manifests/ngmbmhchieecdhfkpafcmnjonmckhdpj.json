{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Write Text Editor",
  "description": "A customizable, offline and full-screen text-editor",
  "version": "3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "img/QW_128x128.png",
    "16": "img/QW_16x16.png",
    "24": "img/QW_24x24.png",
    "23": "img/QW_23x23_bigger.png",
    "32": "img/QW_32x32.png",
    "48": "img/QW_48x48.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_title": "Quick Write Text Editor",
    "default_icon": "img/QW_23x23_bigger.png"
  },
  "permissions": [
    "tabs"
  ],
  "options_page": "index_edit.html"
}
