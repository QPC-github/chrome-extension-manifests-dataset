{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zulu Trusted Sender Score Email Sender Check",
  "short_name": "Email Sender Check",
  "description": "Stop SPAMMERs & SCAMMERS. Check an email domain before clicking submit | Trust before you buy! ",
  "background": {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "scripts": [
      "popup.js"
    ]
  },
  "manifest_version": 2,
  "version": "1.3",
  "permissions": [
    "contextMenus",
    "background",
    "unlimitedStorage",
    "webRequest",
    "webNavigation",
    "notifications",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Trusted Sender Score Email Sender Check",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "128": "128-icon-trans.png"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "128-icon-trans.png"
  }
}
