{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "file://*/*",
        "*://*/*"
      ]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "name": "GG.BET PARSER",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/*",
    "downloads",
    "<all_urls>"
  ],
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "64": "/icons/64.png",
    "128": "/icons/128.png",
    "256": "/icons/256.png",
    "512": "/icons/512.png"
  },
  "description": "GG.BET History of bets collector",
  "version": "1.0"
}
