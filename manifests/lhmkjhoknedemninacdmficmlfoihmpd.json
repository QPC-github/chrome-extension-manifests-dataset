{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "R1P Resolve IP",
  "version": "1.3.0",
  "manifest_version": 2,
  "description": "Takes current domain name and returns IP Address and other...",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "img/x18.png",
    "48": "img/x48.png",
    "128": "img/x128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Click for resolve current IP"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
