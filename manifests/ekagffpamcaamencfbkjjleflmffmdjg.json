{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Url Share",
  "description": "Share the url through a QR code",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Share your page",
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_128.png",
    "48": "icon_128.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2
}
