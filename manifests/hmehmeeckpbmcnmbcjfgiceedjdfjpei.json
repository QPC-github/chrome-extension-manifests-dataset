{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "I Fancy Facebook",
  "version": "1.1",
  "manifest_version": 2,
  "description": "We make Facebook as fancy as you are.",
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*",
        "http://facebook.com/*",
        "https://facebook.com/*"
      ],
      "js": [
        "fancy.user.js"
      ]
    }
  ],
  "icons": {
    "16": "16icon.png",
    "48": "48icon.png",
    "128": "128icon.png"
  }
}
