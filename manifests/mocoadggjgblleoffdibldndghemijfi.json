{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "32",
  "name": "UserAgent Changer",
  "description": "User friendly extension which allows to change User-Agent name on the fly.",
  "version": "1.0",
  "background": {
    "page": "ua.html"
  },
  "externally_connectable": {
    "matches": [
      "*://*.raddev.pl/*"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ],
  "content_security_policy": "script-src 'self' 'sha256-XFptBV/LCBwFOvyfLNAczERNrhab0N6WwMLiAglHPxY=' 'unsafe-eval'; object-src 'self'"
}
