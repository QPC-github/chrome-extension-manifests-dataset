{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "When2Meet Autofill",
  "description": "Autofills your when2meets using you Google Calendar",
  "version": "1.0.2",
  "permissions": [
    "contextMenus",
    "identity",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-16.png"
  },
  "oauth2": {
    "client_id": "956653605342-4jovif0khvvomeq4etsqom0qqdtbf986.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.when2meet.com/?*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "temp.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "options_page": "options.html"
}
