{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Musica",
  "description": "Simple app to download your favorite music",
  "version": "1.6",
  "manifest_version": 2,
  "permissions": [
    "downloads"
  ],
  "browser_action": {
    "default_icon": {
      "19": "img/musica-19.png",
      "38": "img/musica-38.png"
    },
    "default_popup": "index.html"
  }
}
