{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SkinIO EMR Helper",
  "version": "1.0",
  "description": "Allows easy creation of notes and importing of SkinIO records.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://*.skinio.com/"
  ],
  "background": {
    "scripts": [
      "scripts/skinio/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "views/login.html",
    "default_icon": {
      "16": "images/skinio_16.png",
      "32": "images/skinio_32.png",
      "48": "images/skinio_48.png",
      "128": "images/skinio_128.png"
    }
  },
  "icons": {
    "16": "images/skinio_16.png",
    "32": "images/skinio_32.png",
    "48": "images/skinio_48.png",
    "128": "images/skinio_128.png"
  },
  "manifest_version": 2
}
