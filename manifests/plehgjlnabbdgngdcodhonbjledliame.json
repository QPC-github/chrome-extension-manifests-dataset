{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Query Remover",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension removes query strings of all tabs in the window",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_32.png"
  },
  "background": {
    "scripts": [
      "jquery-2.1.4.min.js",
      "script.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
