{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VMware Cmdlets Searcher",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "32": "images/icon2.png"
    },
    "default_title": "Cmdlets",
    "default_popup": "menu.html"
  },
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
  ],
  "content_security_policy": "script-src 'self' http://localhost; object-src 'self'"
}
