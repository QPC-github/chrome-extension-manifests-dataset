{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "icons": {
    "48": "appicon_48.png",
    "128": "appicon_128.png"
  },
  "name": "QR-URL",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Teleport any webpage to your mobile device via QR-code",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
