{
  "name": "Text Selectable",
  "version": "0.1",
  "description": "Let you select text on all websites that disable this functionality. 解除网页文字不可选的限制。 by LongQiang",
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "css": [
        "main.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
