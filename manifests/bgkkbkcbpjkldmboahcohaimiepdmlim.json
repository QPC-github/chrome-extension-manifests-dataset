{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bitcoin to Beanie Babies",
  "short_name": "Bitcoin to Beanie Babies",
  "description": "This switches out the names for two spectacular investments.",
  "version": "1.0",
  "icons": {
    "16": "images/favicon16.png",
    "32": "images/favicon32.png",
    "48": "images/favicon48.png",
    "128": "images/favicon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
