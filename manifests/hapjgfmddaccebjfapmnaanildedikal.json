{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extension Super CE",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Extension Super CE",
  "permissions": [
    "https://application.opence.fr/extension/webservice",
    "tabs",
    "https://www.couleurce.com/*",
    "webNavigation",
    "notifications",
    "http://*/",
    "https://*/",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "img/logo-superce-browser.png",
    "default_popup": "extension.html"
  },
  "background": {
    "scripts": [
      "util/jquery-2.0.3.js",
      "extension.js"
    ]
  }
}
