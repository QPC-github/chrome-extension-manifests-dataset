{
  "name": "久游",
  "version": "3",
  "manifest_version": 2,
  "description": "最简单易用的科学上网服务，为国际化的互联网工作者量身打造，即装即用。",
  "icons": {
    "16": "assets/icons/logo.png",
    "32": "assets/icons/logo.png",
    "64": "assets/icons/logo.png",
    "128": "assets/icons/logo.png"
  },
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "management",
    "proxy",
    "tabs",
    "cookies",
    "notifications",
    "webRequest",
    "webRequestBlocking"
  ],
  "options_ui": {
    "page": "./options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "assets/icons/logo.png",
    "default_title": "久游",
    "default_popup": "./popup.html"
  },
  "update_url": "http://clients2.google.com/service/update2/crx"
}
