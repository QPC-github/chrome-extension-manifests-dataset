{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "개드립 도우미",
  "version": "0.1",
  "description": "개드립 도우미 크롬 확장 프로그램입니다.",
  "author": "R",
  "icons": {
    "128": "favicon-128.png",
    "32": "favicon-32.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://ajax.googleapis.com/*"
  ],
  "background": {},
  "content_scripts": [
    {
      "matches": [
        "*://www.dogdrip.net/*"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://www.dogdrip.net/*"
      ],
      "js": [
        "common.js",
        "free.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
