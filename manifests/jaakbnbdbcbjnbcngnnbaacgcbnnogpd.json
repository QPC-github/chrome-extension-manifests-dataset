{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "41st Links",
  "version": "1.2",
  "description": "Contains officer links for 41st",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "author": "Sean Brougham",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "clipboard",
    "clipboardRead",
    "clipboardWrite",
    "background"
  ]
}
