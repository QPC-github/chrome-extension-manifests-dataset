{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Malaysia Prayer Times",
  "description": "Prayer times for Malaysia",
  "version": "1.9",
  "author": "Muhammad Nizam Omar & Hasri Juhari",
  "background": {
    "scripts": [
      "jquery-3.3.1.js",
      "DefaultValues.js",
      "JakimXML.js",
      "Notification.js"
    ]
  },
  "short_name": "MPT",
  "permissions": [
    "https://www.e-solat.gov.my/*",
    "storage",
    "alarms",
    "notifications"
  ],
  "icons": {
    "128": "img/logo128.png",
    "16": "img/logo16.png",
    "48": "img/logo48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/logo16.png",
      "32": "img/logo32.png",
      "48": "img/logo48.png",
      "128": "img/logo128.png"
    },
    "default_popup": "popup.html",
    "icons": {
      "16": "img/logo16.png",
      "32": "img/logo32.png",
      "48": "img/logo48.png",
      "128": "img/logo128.png"
    }
  }
}
