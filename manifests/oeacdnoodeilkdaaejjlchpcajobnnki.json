{
  "author": {
    "main": "MathiGraph"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_popup": "popup/popup.html",
    "default_title": "summonerswarfr - Notification"
  },
  "description": "Soyez au courant quand vos streameur préféré commence à streamer!",
  "icons": {
    "128": "icone128.png"
  },
  "manifest_version": 2,
  "name": "summonerswarfr",
  "options_page": "options/options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options/options.html"
  },
  "permissions": [
    "tabs",
    "https://api.twitch.tv/*",
    "notifications",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.0.0"
}
