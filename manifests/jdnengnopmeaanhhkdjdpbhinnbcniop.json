  "version": "1.0.2",
  "short_name": "Forex Robot Reviews",
  "app": {
    "background": {
      "scripts": [
        "js/jquery.min.js",
        "js/jquery.browser.min.js",
        "js/jquery.jfeed.js",
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "38": "images/38.png",
    "48": "images/48.png",
    "96": "images/96.png",
    "128": "images/128.png"
  },
  "permissions": [
    "https://www.reviewforexrobots.com/",
    "storage",
    "unlimitedStorage",
    "notifications"
  ]
}
