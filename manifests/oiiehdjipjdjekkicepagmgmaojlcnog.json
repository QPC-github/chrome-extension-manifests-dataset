{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Copyright Violator",
  "description": "Replace Words with Other Words",
  "version": "0.1",
  "icons": {
    "16": "nocopy16.png",
    "48": "nocopy48.png",
    "128": "nocopy128.png"
  },
  "browser_action": {
    "default_title": "Copyright Violator!",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
