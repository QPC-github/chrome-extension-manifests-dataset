{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search with Stephen Curry",
  "description": "Enjoy awesome GIFs of Stephen Curry every time you search the web.",
  "version": "3.4.10",
  "short_name": "StephCurry",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icons/16.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  },
  "browser_action": {
    "default_icon": "images/icons/48.png",
    "default_popup": "html/popup.html",
    "default_title": "Search with Stephen Curry"
  },
  "background": {
    "page": "./html/background.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/lib/browser-polyfill.js",
        "js/dominjector.js"
      ],
      "matches": [
        "*://*.bing.com/search*",
        "*://*.google.com/search*"
      ],
      "run_at": "document_end"
    }
  ],
  "omnibox": {
    "keyword": "s"
  },
  "manifest_version": 2,
