{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Damn! Take me to the library!",
  "description": "add 'ezp.lib.unimelb.edu.au' to the browser's address, allow you to visit the databases quickly",
  "version": "1.2",
  "browser_action": {
    "default_icon": "unimelb128.png",
    "default_title": "Take me to the library!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
