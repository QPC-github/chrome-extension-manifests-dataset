{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Merrier Extension",
  "description": "The Merrier social shopping plugin.",
  "version": "1.23",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "Images/icon16.png",
    "48": "Images/icon48.png",
    "128": "Images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "Images/icon19.png",
      "38": "Images/icon38.png"
    },
    "default_title": "Start Extension Page"
  }
}
