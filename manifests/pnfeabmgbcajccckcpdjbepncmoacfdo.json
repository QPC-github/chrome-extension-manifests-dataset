{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FloraQueen",
  "version": "0.6",
  "manifest_version": 2,
  "description": "Use FloraQueen's new search plug-in and you'll find the freshest local flowers and gifts at the best value. ",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "minimum_chrome_version": "6",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "floraqueen.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "floraqueen.js",
    "jquery.js",
    "floraqueen_logo_transparent.png",
    "toolbarcontent.html"
  ]
}
