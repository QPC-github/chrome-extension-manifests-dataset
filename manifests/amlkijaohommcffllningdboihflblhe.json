{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "ru",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "permissions": [
    "cookies",
    "tabs",
    "http://iliz.net/",
    "*://*.mamba.ru/*",
    "*://love.mail.ru/*",
    "*://www.love.mail.ru/*",
    "*://love.rambler.ru/*",
    "*://www.love.rambler.ru/*"
  ],
  "version": "1.18",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "name": "__MSG_name__"
  },
  "content_scripts": [
    {
      "js": [
        "common.js",
        "content-script.js"
      ],
      "matches": [
        "*://www.mamba.ru/*",
        "*://love.mail.ru/*",
        "*://www.love.mail.ru/*",
        "*://love.rambler.ru/*",
        "*://www.love.rambler.ru/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "19": "icon19.png",
    "48": "icon48.png"
  },
  "web_accessible_resources": [
    "common.js"
  ]
}
