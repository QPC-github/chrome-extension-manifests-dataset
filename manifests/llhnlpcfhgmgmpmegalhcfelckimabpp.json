{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Poodle"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Poodle helps you find affordable alternatives to products in images and videos.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Poodle",
  "short_name": "Poodle",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "version": "0.2.4",
  "web_accessible_resources": [
    "region.html"
  ]
}
