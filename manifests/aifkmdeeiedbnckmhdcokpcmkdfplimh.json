{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "background.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "It protects you from browsing to malicious websites that may compromise your online security.",
  "icons": {
    "128": "icon_128.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "WebSafe",
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [
    "background.js"
  ]
}
