{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Note Entry Navigator",
  "version": "1.0",
  "description": "this plugin adopts section list to entries in note.mu automatically",
  "content_scripts": [
    {
      "matches": [
        "https://note.mu/*/n/*",
        "https://note.mu/",
        "https://note.mu/*"
      ],
      "css": [
        "navigator.css"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "navigator.js"
      ]
    }
  ]
}
