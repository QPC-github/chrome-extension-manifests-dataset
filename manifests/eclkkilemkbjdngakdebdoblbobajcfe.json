{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MultiTab",
  "version": "1.0",
  "description": "Open Multiple Tabs At Once!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
