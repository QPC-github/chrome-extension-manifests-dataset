{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fishy rush",
  "version": "1.0",
  "description": "Help litle fish to survive in a cruel sea",
  "icons": {
    "48": "img/1.jpg",
    "128": "img/1.jpg",
    "16": "img/1.jpg"
