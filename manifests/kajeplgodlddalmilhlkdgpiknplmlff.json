{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.min.js",
        "base64.js",
        "md5-min.js",
        "readflag.min.js",
        "composeVersion2.min.js"
      ],
      "matches": [
        "*://mail.google.com/*"
      ]
    }
  ],
  "description": "ReadFlag Email Intelligence",
  "icons": {
    "128": "readflag-128.png",
    "16": "readflag-16.png",
    "14": "readflag_white_14x14.png",
    "48": "readflag-48.png"
  },
  "manifest_version": 2,
  "name": "ReadFlag Email Intelligence",
  "options_page": "options.html",
  "permissions": [
    "*://mail.google.com/*",
    "*://*.readflag.com/*",
    "tabs",
    "notifications",
    "unlimitedStorage"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.4",
  "web_accessible_resources": [
    "readflag_logo.png",
    "readflag_white_14x14.png"
  ]
}
