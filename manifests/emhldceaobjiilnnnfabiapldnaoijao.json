{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Docline Screen Sharing",
  "author": "Aplicaciones Salud S.L.",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Esta extensi√≥n permite compartir su escritorio para la videoconsulta de Docline.",
  "homepage_url": "http://docline.es/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.docline.io/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
