{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popup.html",
    "default_title": "What the Font"
  },
  "content_scripts": [
    {
      "css": [
        "styles/sweetalert.css",
        "styles/custom.css"
      ],
      "js": [
        "js/jquery.js",
        "js/sweetalert.min.js",
        "js/content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "short_name": "WhatTheFont",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4.1",
  "web_accessible_resources": [
    "logo.png",
    "background.png",
    "js/*",
    "styles/*"
  ]
}
