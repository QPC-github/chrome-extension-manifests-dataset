{
  "manifest_version": 2,
  "name": "igra-prestoloFF",
  "description": "Расширение для обхода блокировок и быстрого доступа к сайту igra-prestoloff",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "4.0",
  "browser_action": {
    "default_icon": {
      "19": "imgs/icon-19.png",
      "38": "imgs/icon-38.png"
    }
  },
  "icons": {
    "256": "imgs/icon-256.png",
    "128": "imgs/icon-128.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
