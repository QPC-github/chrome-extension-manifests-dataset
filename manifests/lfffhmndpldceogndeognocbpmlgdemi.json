{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Comic Webcam",
  "description": "Take photos from your webcam and apply a lot of funny and beautiful webcam effects!",
  "version": "1.1",
  "app": {
    "urls": [
      "http://comicwebcam.com/"
    ],
    "launch": {
      "web_url": "http://comicwebcam.com/"
    }
  },
  "icons": {
    "128": "icon_128b.png",
    "96": "icon_96.png",
    "16": "icon_16.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
