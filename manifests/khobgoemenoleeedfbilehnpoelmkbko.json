{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "table to csv",
  "description": "Convert table tag to csv",
  "version": "1.0.1",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "table to csv",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
