{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Service Postal Gmail",
  "description": "Service Postal - envoyer vos emails par courrier postal facilement Ã  partir de votre messagerie GMAIL",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Service Postal Gmail"
  },
  "version": "0.5",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
