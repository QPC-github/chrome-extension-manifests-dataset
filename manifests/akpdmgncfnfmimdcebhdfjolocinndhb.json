{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sessions",
  "description": "An extension to save all tabs in a work session and easily switch between different sessions",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Sessions",
    "default_popup": "popup.html"
  }
}
