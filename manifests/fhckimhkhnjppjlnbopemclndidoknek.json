{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Back Backspace",
  "version": "1.0",
  "description": "Back BackSpace navigation to new version of browser",
  "author": "Anton Kedenko",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {},
  "permissions": [
    "http://*"
  ],
  "browser_action": {
    "default_title": "Back Backspace",
    "default_popup": ""
  }
}
