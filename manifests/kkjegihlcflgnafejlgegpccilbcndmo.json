{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "jq191.js",
        "script.js"
      ],
      "matches": [
        "*://vk.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "СБМ",
  "icons": {
    "128": "img/icon_128.png",
    "49": "img/icon_49.png"
  },
  "manifest_version": 2,
  "name": "Кидалы и мошенники сети",
  "short_name": "СБМ",
  "options_page": "options.html",
  "page_action": {
    "default_icon": "img/icon_19.png",
    "default_popup": "popup.html",
    "default_title": "СБМ"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "*://vk.com/*",
    "*://api.vk.com/*"
  ],
  "version": "1.0",
  "web_accessible_resources": [
    "img/icon_128.png",
    "img/admin.png",
    "img/adminBg.png",
    "img/watch.png",
    "img/watchBg.png",
    "img/kidok.png",
    "img/kidokBg.png",
    "img/support.png",
    "img/supportBg.png"
  ]
}
