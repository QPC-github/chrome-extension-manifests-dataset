{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "homepage": "http://search.mecoosh.com/?from=homepage",
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://search.mecoosh.com/favicon.ico",
      "is_default": true,
      "keyword": "search.mecoosh.com",
      "name": "Mecoosh",
      "search_url": "http://search.mecoosh.com/?from=searchbar#q={searchTerms}"
    }
  },
  "description": "Provides quick access to video, movies, tv series and more!",
  "icons": {
    "128": "favicon.png",
    "16": "favicon.png",
    "48": "favicon.png"
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "manifest_version": 2,
  "name": "Mecoosh Search",
  "permissions": [
    "http://*.mecoosh.com/*",
    "cookies",
    "management"
  ],
  "short_name": "Mecoosh Search",
  "version": "0.0.0.1"
}
