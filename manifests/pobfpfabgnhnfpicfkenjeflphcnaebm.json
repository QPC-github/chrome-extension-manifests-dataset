{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GNS SecureLogin with S-Badge",
  "short_name": "GNS SecureLogin",
  "description": "GNS SecureLogin with S-Badge",
  "version": "4.1",
  "browser_action": {
    "default_icon": "notgnsbrowser16.png"
  },
  "icons": {
    "16": "notgnsbrowser16.png",
    "48": "notgnsbrowser48.png",
    "128": "notgnsbrowser128.png"
  },
  "permissions": [
    "nativeMessaging",
    "https://github.com/",
    "https://accounts.google.com/",
    "https://sales.ecwintl.com/",
    "https://cair.cdph.ca.gov/",
    "https://hcp-connect.com/",
    "https://mytools.gatewayedi.com/",
    "https://navinet.navimedix.com/",
    "https://www.blueshieldca.com/",
    "https://cignaforhcp.cigna.com/",
    "https://caaafy1c0mqm4vngz5app.ecwcloud.com/",
    "https://my.eclinicalworks.com/",
    "https://kiosk.s-badge.com/",
    "http://kiosk.s-badge.com/",
    "https://star-edu.s-badge.com:8080/",
    "http://star-edu.s-badge.com:8080/"
  ],
  "background": {
    "scripts": [
      "gns.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "http://192.168.1.10:8080/",
      "http://kiosk.s-badge.com/*",
      "https://kiosk.s-badge.com/*",
      "http://star-edu.s-badge.com:8080/*",
      "https://star-edu.s-badge.com:8080/*"
    ]
  }
}
