{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CertEnrollment.ZhengXinBank Extension",
  "version": "3.2.0.1",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "http://zxbk.corbank/*",
      "http://zxbk.inmanage/*",
      "https://www.zxbk.com.cn/*",
      "https://www.zhengxinbank.com.cn/*",
      "https://www.zhengxinbank.com/*",
      "https://www.zhengxinbank.net/*",
      "https://www.zhengxinbk.com.cn/*",
      "https://www.zhengxinbk.com/*",
      "https://www.zhengxinbk.cn/*",
      "https://www.zhengxinbk.net/*",
      "https://www.zhengxinyinhang.com.cn/*",
      "https://www.zhengxinyinhang.com/*",
      "https://www.zhengxinyinhang.cn/*",
      "https://www.zhengxinyinhang.net/*",
      "https://www.zxbank.net/*"
    ]
  }
}
