{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Expose Your Interests",
  "description": "Expose Your Interests",
  "version": "1.3.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://www.google.com/*",
    "activeTab",
    "storage",
    "downloads"
  ]
}
