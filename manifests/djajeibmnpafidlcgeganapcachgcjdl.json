{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HF Dark Flat",
  "version": "1.0",
  "manifest_version": 2,
  "author": "2825755",
  "description": "Dark Flat Theme for Hackforums.net",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "web_accessible_resources": [
    "images/*",
    "fonts/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://hackforums.net/*",
        "http://www.hackforums.net/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
