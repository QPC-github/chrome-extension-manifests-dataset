{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Show password",
  "version": "1.01",
  "description": "Ability to show passwords in input fields!",
  "permissions": [
    "declarativeContent",
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "96": "icon96.png",
    "128": "icon128.png"
  }
}
