{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MinuteDock",
  "version": "0.60",
  "description": "Start and stop your MinuteDock timer from your Chrome toolbar.",
  "browser_action": {
    "default_title": "MinuteDock",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "permissions": [
    "tabs",
    "http://minutedock.com/",
    "https://minutedock.com/"
  ],
  "manifest_version": 2
}
