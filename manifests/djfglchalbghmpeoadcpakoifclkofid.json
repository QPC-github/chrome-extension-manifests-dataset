{
  "background": {
    "page": "background/index.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Search Fortnite"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://search.lovelytab.com/favicon.ico",
      "image_url": "",
      "instant_url": "",
      "is_default": true,
      "keyword": "{searchTerms}",
      "name": "Search Fortnite",
      "search_url": "https://r.lovelytab.com/?page=redirect&ext=154753860046&name=Search Fortnite&feed=1337&q={searchTerms}"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "tabContent.js",
        "content.js"
      ],
      "matches": [
        "*://s.lovelytab.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Easily Search For Daily Fortnite News And Updates....",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Search Fortnite",
  "omnibox": {
    "keyword": "Fortnite"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.googletagmanager.com https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "*://*.lovelytab.com/*",
    "cookies",
    "storage",
    "tabs",
    "management",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "Search Fortnite",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.2",
  "web_accessible_resources": [
    "/icons/icon16.png"
  ]
}
