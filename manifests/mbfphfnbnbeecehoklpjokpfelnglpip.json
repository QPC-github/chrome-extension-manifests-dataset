{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quil√°",
  "description": "Helps you revise youtube videos that you skipped",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "*://www.youtube.com/*"
      ],
      "js": [
        "js/jqueryxxx.js",
        "js/content.js"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "js/jqueryxxx.js",
    "js/main.js",
    "icons/*.png",
    "siren.mp3"
  ]
}
