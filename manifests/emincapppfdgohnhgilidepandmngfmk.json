{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noir",
  "version": "0.0.7",
  "description": "A minimlist new tab page for Chrome.",
  "icons": {},
  "options_page": "options.html",
  "permissions": [
    "storage",
    "bookmarks",
    "chrome://favicon/"
  ],
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  }
}
