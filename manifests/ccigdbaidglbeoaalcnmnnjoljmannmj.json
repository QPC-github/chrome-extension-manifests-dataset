{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mayonnaizer",
  "manifest_version": 2,
  "version": "0.1",
  "author": "Wieden + Kennedy",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Mayonnaizer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "images/0.png",
    "images/1.png",
    "images/2.png",
    "images/3.png",
    "images/4.png",
    "images/5.png",
    "images/6.png",
    "images/7.png",
    "images/font.png"
  ]
}
