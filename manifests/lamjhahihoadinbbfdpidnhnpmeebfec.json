{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Home+",
  "description": "Home+ je dodatek za brskalnik, ki vam vsak nov tab spremeni na vijol’čno.",
  "version": "3.0",
  "chrome_settings_overrides": {
    "homepage": "http://home.nkmaribor.com",
    "startup_pages": [
      "http://home.nkmaribor.com"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "start.html"
  },
  "permissions": [
    "webRequest",
    "cookies",
    "webRequestBlocking",
    "*://*.nkmaribor.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  }
}
