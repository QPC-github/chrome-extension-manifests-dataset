{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shopify autofiller",
  "version": "1.2",
  "description": "Shopify autofill",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "http://yeezysupply.com/*",
    "https://yeezysupply.com/*",
    "*://checkout.shopifycs.com/*",
    "*://eflash-us.doverstreetmarket.com/*",
    "*://kith.com/*",
    "*://bdgastore.com/*",
    "*://cncpts.com/*",
    "*://undefeated.com/*",
    "*://eflash.doverstreetmarket.com/*",
    "*://eflash-sg.doverstreetmarket.com/*",
    "*://eflash-jp.doverstreetmarket.com/*",
    "background"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://yeezysupply.com/*",
        "https://yeezysupply.com/*",
        "*://checkout.shopifycs.com/*",
        "*://eflash-us.doverstreetmarket.com/*",
        "*://kith.com/*",
        "*://bdgastore.com/*",
        "*://cncpts.com/*",
        "*://undefeated.com/*",
        "*://eflash.doverstreetmarket.com/*",
        "*://eflash-sg.doverstreetmarket.com/*",
        "*://eflash-jp.doverstreetmarket.com/*"
      ],
      "js": [
        "content_frame.js",
        "jquery.js"
      ],
      "all_frames": true
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
