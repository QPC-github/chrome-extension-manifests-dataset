{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/content.js",
        "js/jquery.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Scroll",
  "icons": {
    "128": "images/128x128.png",
    "16": "images/16x16.png",
    "48": "images/48x48.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/__up.png",
    "images/__down.png"
  ],
  "name": "Scroll up",
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "storage"
  ],
  "version": "1.1.2"
}
