{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TagGen",
  "description": "Generates tags for selected text",
  "version": "1.0",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "icons": {
    "32": "tag-32.png",
    "128": "tag.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
