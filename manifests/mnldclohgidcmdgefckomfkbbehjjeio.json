{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OpenUp",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "web_accessible_resources": [
    "res/*",
    "assets/*",
    "icons/*"
  ],
  "description": "Share your data with us and get rewards for doing so",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://open-up.io/",
  "version": "2.1.0.6",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "login.html",
    "default_icon": "icons/icon32.png",
    "default_title": "OpenUp"
  },
  "permissions": [
    "tabs",
    "history",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications"
  ]
}
