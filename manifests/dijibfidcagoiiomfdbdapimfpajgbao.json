{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "notif.html"
  },
  "manifest_version": 2,
  "name": "FunGame's WebTV Notification",
  "description": "Extension pour recevoir les notifications de la WebTV de Fun Game's",
  "version": "4",
  "permissions": [
    "http://fungames.fr.cr/live/UTILS/notif.php",
    "http://fungames.fr.cr/live",
    "notifications",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "notif.html",
    "default_icon": "icon.png"
  }
}
