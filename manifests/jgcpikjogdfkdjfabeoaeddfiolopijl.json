{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zipppo",
  "version": "0.1",
  "description": "Скрипт для просмотра статистики в игре БЕЗУМИЕ",
  "permissions": [
    "http://mad-vk.playflock.com/*",
    "http://bezumie.sk-k.ru/*"
  ],
  "author": "Zipppo",
  "browser_action": {
    "default_icon": "Images/icon.png",
    "default_title": "Zipppo"
  },
  "content_scripts": [
    {
      "matches": [
        "http://vkontakte.ru/*",
        "http://vk.com/*"
      ],
      "js": [
        "zhipppo.js"
      ]
    }
  ]
}
