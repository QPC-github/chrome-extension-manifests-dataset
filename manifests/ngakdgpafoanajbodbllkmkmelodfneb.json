{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bitcoin Central",
  "description": "Suivez en temps reel le cours du Bitcoin",
  "version": "1.2.3",
  "short_name": "Bitcoin",
  "permissions": [
    "https://bitcoin-central.net/api/v1/data/eur/ticker",
    "https://bitcoin-central.net/api/v1/data/eur/trades",
    "https://bitcoin-central.net/api/v1/data/eur/depth"
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  }
}
