{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TabGroup",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Group tabs like in Firefox, and optionally save them for later use.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "page": "tabgroup.html"
  },
  "permissions": [
    "tabs"
  ]
}
