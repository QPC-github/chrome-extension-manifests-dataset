{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.0",
  "manifest_version": 2,
  "short_name": "Canvas Tool",
  "name": "Tool for Canvas LMS",
  "description": "Adds functionality to Canvas LMS",
  "icons": {
    "16": "16x16.png",
    "64": "64x64.png",
    "128": "128x128.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
