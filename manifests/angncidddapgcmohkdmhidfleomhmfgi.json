{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LoginCat",
  "version": "3.3.23",
  "manifest_version": 2,
  "description": "LoginCat Extension for Cyber Security",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "js": [
        "content_script.js",
        "aes.js",
        "lcCryptfe.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "logincat_48.png",
    "128": "logincat_128.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "tabs",
    "cookies",
    "ftp://*/*",
    "*://*/*"
  ]
}
