{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yeetine DMG",
  "description": "Dank Memes Gang notifications disabler.",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "icons": {
    "19": "icon19.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "third-party/jquery.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
