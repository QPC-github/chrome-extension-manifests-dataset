{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "deadwood phrase remover",
  "version": "0.1.1",
  "description": "Replaces the 'deadwood phrases' with 'something simple'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "deadwoodtosimple.js"
      ],
      "run_at": "document_end"
    }
  ]
}
