{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AyeStudy Screen Sharing",
  "description": "An extension to allow you to video screen sharing locally.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.ayestudy.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
