{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Dieses Plugin entfernt die Warteschlangen, die bei Benutzung der Webseiten der Ticketb√∂rse Viagogo erscheinen.",
  "manifest_version": 2,
  "name": "Gogo Ghost",
  "version": "1.3",
  "homepage_url": "https://www.viagogo.de",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.viagogo.de/*"
      ],
      "js": [
        "cleangogo.js"
      ]
    },
    {
      "matches": [
        "*://*.viagogo.com/*"
      ],
      "js": [
        "cleangogo.js"
      ]
    },
    {
      "matches": [
        "*://*.viagogo.co.uk/*"
      ],
      "js": [
        "cleangogo.js"
      ]
    }
  ]
}
