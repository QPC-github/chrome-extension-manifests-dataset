{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Browser history calculator",
  "version": "1.2.8",
  "manifest_version": 2,
  "description": "Browser history calculator",
  "icons": {
    "19": "images/logo_19.png",
    "38": "images/logo_38.png",
    "128": "images/logo_128.png"
  },
  "browser_action": {
    "default_title": "Click to get browser history",
    "default_icon": {
      "19": "images/logo_19.png",
      "38": "images/logo_38.png"
    },
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "history",
    "http://*/",
    "https://*/",
    "storage",
    "identity",
    "identity.email"
  ],
  "web_accessible_resources": [
    "css/*.css",
    "js/common/*.js"
  ],
  "options_page": "html/options.html",
  "homepage_url": "http://yourdomainhere.com/"
}
