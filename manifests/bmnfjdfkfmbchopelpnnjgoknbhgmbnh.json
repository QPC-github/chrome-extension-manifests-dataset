{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alutecs Seo",
  "short_name": "SeoRank",
  "description": "Obtén un informe SEO gratis de tu sitio. Con este inspector SEO podrás analizar pagina web y mejorar tu posicionamiento en la red.",
  "version": "2.3.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "app.js",
      "background.js"
    ]
  }
}
