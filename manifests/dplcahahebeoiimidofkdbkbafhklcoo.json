{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Thinkr!",
  "version": "0.1.1",
  "description": "Innovation, smarts, and creativity - at scale. Play Thinkr.",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "./js/jquery-3.4.1.min.js",
      "./js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logos/default/img16.png",
      "32": "images/logos/default/img32.png",
      "48": "images/logos/default/img48.png",
      "128": "images/logos/default/img128.png"
    },
    "tracked_icon": {
      "16": "images/logos/tracked/img16.png",
      "32": "images/logos/tracked/img32.png",
      "48": "images/logos/tracked/img48.png",
      "128": "images/logos/tracked/img128.png"
    },
    "alert_icon": {
      "16": "images/logos/alert/img16.png",
      "32": "images/logos/alert/img32.png",
      "48": "images/logos/alert/img48.png",
      "128": "images/logos/alert/img128.png"
    }
  },
  "icons": {
    "16": "images/logos/default/img16.png",
    "32": "images/logos/default/img32.png",
    "48": "images/logos/default/img48.png",
    "128": "images/logos/default/img128.png"
  },
  "options_page": "./html/options.html"
}
