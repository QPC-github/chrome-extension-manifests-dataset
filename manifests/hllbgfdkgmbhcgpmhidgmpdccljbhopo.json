{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Text Look",
  "description": "Syncronize Text/Link Between Android Device and Desktop",
  "version": "1.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "32": "32.png",
    "16": "16.png"
  },
  "permissions": [
    "activeTab"
  ]
}
