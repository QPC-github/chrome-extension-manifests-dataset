{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "dDot Origin",
  "version": "1.0",
  "description": "Test distribution service for utilization of abstract modifications.",
  "icons": {
    "48": "icons/ddot-48.png",
    "96": "icons/ddot-96.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dDot Origin.js"
      ]
    }
  ]
}
