{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoQR",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "QR Code Generator.",
  "browser_action": {
    "default_icon": "misc/24.png",
    "default_title": "GoQR",
    "default_popup": "index.html"
  },
  "icons": {
    "24": "misc/24.png",
    "48": "misc/48.png",
    "96": "misc/96.png",
    "128": "misc/128.png"
  },
  "permissions": [
    "tabs"
  ]
}
