{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "New Tab"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://findprivate.online/favicon.png",
      "instant_url": "https://findprivate.online/results.php?q={searchTerms}",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "Secure",
      "name": "Secure",
      "search_url": "https://findprivate.online/results.php?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "https://findprivate.online/results.php?q={searchTerms}"
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "FP App configures your default search settings to FindPrivate.Online",
  "homepage_url": "https://findprivate.online/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "FP App",
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "check.png"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.2"
}
