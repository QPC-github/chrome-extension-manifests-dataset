{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Inject jQuery",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adds jQuery to the current tab.",
  "options_page": "options.html",
  "icons": {
    "128": "icon_large.png",
    "16": "icon_large.png",
    "256": "icon_large.png",
    "48": "icon_large.png"
  },
  "browser_action": {
    "default_icon": "icon_small.png",
    "default_title": "Add jQuery to current tab"
  },
  "background": {
    "scripts": [
      "jquery-1.8.1.min.js",
      "background.js",
      "options.js",
      "enable.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "jquery-1.8.1.min.js"
  ]
}
