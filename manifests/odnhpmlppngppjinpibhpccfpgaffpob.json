{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Exit fullscreen when video ends",
  "description": "This extension doesn't (read or change or store ... etc) anything as the permission says.",
  "version": "0.8",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?v=*",
        "http://www.youtube.com/watch?v=*"
      ],
      "js": [
        "injectedScript.js"
      ]
    }
  ]
}
