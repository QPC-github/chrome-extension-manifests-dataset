{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Collapse Blockquote",
  "version": "3.1",
  "description": "Collapse annoying blockquote that pollute the screen",
  "permissions": [
    "activeTab",
    "contextMenus"
