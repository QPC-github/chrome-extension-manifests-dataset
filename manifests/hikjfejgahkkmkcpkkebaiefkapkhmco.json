{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*://www.google.*/*"
      ],
      "css": [
        "css/google-search.css"
      ],
      "js": [
        "js/libs/jquery.js",
        "js/libs/debug.js",
        "js/libs/google/mp.google.thumbnails.image.js",
        "js/google-search.js"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Google Search Preview",
  "homepage_url": "http://leonardociaccio.wordpress.com/",
  "icons": {
    "128": "img/icon-128.png",
    "48": "img/icon-48.png",
    "24": "img/icon-24.png",
    "16": "img/icon-16.png"
  },
  "minimum_chrome_version": "31.0.0.0",
  "manifest_version": 2,
  "name": "Google Search Preview",
  "version": "1.0.0.3"
}
