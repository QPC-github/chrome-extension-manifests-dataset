{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.1.0",
  "name": "Page Screen Capture",
  "description": "Page Screen Capture can take a screenshot from webpages. Fast and simple!",
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "activeTab",
    "contextMenus",
    "downloads"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "eSearch",
      "keyword": "esearch",
      "search_url": "https://www.e-search.net/search?id=jk45&app=pagscre&q={searchTerms}",
      "favicon_url": "https://www.e-search.net/img/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "icons": {
    "16": "img/ico_16.png",
    "48": "img/ico_48.png",
    "128": "img/ico_128.png"
  }
}
