{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Plok",
  "version": "1.0.3",
  "default_locale": "en",
  "description": "__MSG_ext_description__",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19_off.png",
      "38": "img/icon38_off.png"
    },
    "default_popup": ""
  },
  "permissions": [
    "tabs",
    "cookies",
    "contextMenus",
    "storage",
    "notifications",
    "*://plok.mn/"
  ],
  "short_name": "Plok"
}
