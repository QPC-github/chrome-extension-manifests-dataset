{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Meetrics OptOut",
  "description": "Disables tracking via Meetrics by blocking every request",
  "version": "1.0.0",
  "permissions": [
    "http://*/",
    "https://*/",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "48": "Icon_48x48.png"
  },
  "background": {
    "scripts": [
      "unread.js"
    ],
    "persistent": true
  }
}
