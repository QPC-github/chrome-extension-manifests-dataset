{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Apple iTunes For PC",
  "description": "Search over 10000 Itunes ringtones for Iphone. Instructions to install ringtones on Iphone.",
  "version": "1.5",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  }
}
