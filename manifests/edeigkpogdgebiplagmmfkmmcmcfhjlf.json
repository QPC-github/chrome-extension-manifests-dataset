{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cliveman Twilio Video Screen sharing",
  "description": "An extension to allow you to test Twilio Video screen sharing in Cliveman website.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.cliveman.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
