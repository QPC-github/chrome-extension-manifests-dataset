{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stowr – Samla alla recept på ett ställe",
  "version": "1.2.1",
  "description": "Spara recept direkt från Chrome till Stowr.",
  "author": "Aller Media AB",
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16.png",
      "19": "img/icon-19.png",
      "32": "img/icon-32.png",
      "38": "img/icon-38.png",
      "40": "img/icon-40.png",
      "48": "img/icon-48.png",
      "96": "img/icon-96.png",
      "128": "img/icon-128.png",
      "256": "img/icon-256.png"
    },
    "default_title": "Spara"
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "img/icon-16.png",
    "19": "img/icon-19.png",
    "32": "img/icon-32.png",
    "38": "img/icon-38.png",
    "40": "img/icon-40.png",
    "48": "img/icon-48.png",
    "96": "img/icon-96.png",
    "128": "img/icon-128.png",
    "256": "img/icon-256.png"
  }
}
