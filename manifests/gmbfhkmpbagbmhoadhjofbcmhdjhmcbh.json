{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One QRCode",
  "description": "One QRCode for Chrome",
  "version": "1.0.8",
  "icons": {
    "16": "icon-128.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_title": "One QRCode",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
