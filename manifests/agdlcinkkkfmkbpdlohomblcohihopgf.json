{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BiSoga",
  "description": "The web's best URL shortener, pastebin, and file host - even easier access with the brand new BiSoga chrome extension.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
