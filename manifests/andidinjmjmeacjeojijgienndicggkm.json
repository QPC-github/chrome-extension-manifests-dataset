{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarker",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "An intelligent way of organising bookmarks. Data is full secured with encryption",
  "icons": {
    "16": "images/icon-16.png",
    "39": "images/icon-39.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Set this page's color.",
    "default_icon": "images/icon-39.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "history",
    "*://*.bookmarker-intelligent.herokuapp.com/*"
  ]
}
