{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wiki Rocket Science",
  "description": "Limit width of the page on wikimedia sites to improve readability.",
  "version": "1.0",
  "minimum_chrome_version": "45.0",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "https://*.wikibooks.org/*",
        "https://*.wikidata.org/*",
        "https://*.wikimedia.org/*",
        "https://*.wikinews.org/*",
        "https://*.wikipedia.org/*",
        "https://*.wikisource.org/*",
        "https://*.wikiversity.org/*",
        "https://*.wikivoyage.org/*",
        "https://*.wiktionary.org/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}
