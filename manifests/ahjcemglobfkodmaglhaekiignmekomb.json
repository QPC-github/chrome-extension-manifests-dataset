{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Unofficial Tshirt Sales from Facebook",
  "description": "Remove Unofficial Sales from Facebook",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "facebook_filter.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
