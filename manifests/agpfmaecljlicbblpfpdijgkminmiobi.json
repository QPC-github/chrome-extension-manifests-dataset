{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Old Reader Fullscreen",
  "description": "Press SHIFT + F to enter magic world of FULLSCREEN!!!",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "http://theoldreader.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://theoldreader.com/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "js.js"
      ],
      "run_at": "document_end"
    }
  ]
}
