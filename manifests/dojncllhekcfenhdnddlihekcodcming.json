{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Resoomer",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "Summarize your articles online in 1 clic",
  "icons": {
    "16": "resoomer16x16.png",
    "48": "resoomer48x48.png",
    "128": "resoomer128x128.png"
  },
  "background": {
    "scripts": [
      "resoomer.js"
    ]
  },
  "browser_action": {
    "default_icon": "resoomer48x48.png"
  },
  "permissions": [
    "tabs"
  ]
}
