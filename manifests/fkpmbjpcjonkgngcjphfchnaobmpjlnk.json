{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save Me",
  "short_name": "Save Me",
  "description": "This application let you manage your reading list and bookmark",
  "version": "0.0.0.4",
  "minimum_chrome_version": "38",
  "background": {
    "scripts": [
      "event_script.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "/assets/share_16.png",
    "64": "/assets/share_64.png",
    "128": "/assets/share_128.png"
  },
  "permissions": [
    "contextMenus",
    "notifications"
  ]
}
