{
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "infinite-scroll.js"
      ],
      "matches": [
        "*://*.designernews.co/",
        "*://*.designernews.co/stories",
        "*://*.designernews.co/new",
        "*://*.designernews.co/discussions"
      ]
    }
  ],
  "description": "Add infinite scroll to Designer News",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "name": "DN Infinite Scroll",
  "permissions": [
    "webRequest"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
