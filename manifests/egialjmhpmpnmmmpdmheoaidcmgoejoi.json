{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "name": "Notesi save",
  "description": "This extension initate saving website as notesi bookmark.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "LOGO_small2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
