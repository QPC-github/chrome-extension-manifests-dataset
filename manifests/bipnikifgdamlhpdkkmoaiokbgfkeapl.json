{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tampery",
  "version": "0.0.4",
  "minimum_chrome_version": "63",
  "description": "Tamper browser requests in flight",
  "homepage_url": "https://github.com/pd4d10/tampery",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_popup": "dist/popup.html"
  }
}
