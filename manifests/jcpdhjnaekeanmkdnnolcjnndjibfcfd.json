{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTubeAddToWatchList",
  "description": "This allows us to open the YouTube video context menu when we right click on a video.",
  "author": "Yousif Yassi",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
        "*://youtu.be/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "YTAddToWatchList.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.jpg",
    "48": "images/icon48.jpg",
    "128": "images/icon128.jpg"
  }
}
