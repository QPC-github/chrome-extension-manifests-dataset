{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "USD to INR",
  "description": "Gets the exchange rate for INR from USD using alphavantage api",
  "version": "0.5",
  "background": {
    "scripts": [
      "jquery.js",
      "script.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://www.alphavantage.co/query/*",
    "contextMenus"
  ]
}
