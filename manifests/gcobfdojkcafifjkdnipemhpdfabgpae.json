{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wantily",
  "description": "Wantily Chrome extension to select and save items to Wantily account.",
  "version": "3.3.10",
  "background": {
    "scripts": [
      "bundled/background.min.js"
    ]
  },
  "web_accessible_resources": [
    "bookmark.html",
    "loginForm.html",
    "left-arrow.png",
    "logo_big.png",
    "bloading.gif",
    "loading.gif",
    "right-arrow.png",
    "touch_select.png",
    "bundled/normalize.css",
    "bundled/foundation.css",
    "bundled/bootstrap-tagsinput.css",
    "plugins/bootstrap-tagsinput.js",
    "bundled/style.css",
    "x.png",
    "wlogo.png"
  ],
  "permissions": [
    "tabs",
    "<all_urls>",
    "webNavigation",
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "ws.png"
  }
}
