{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Arrows10 アマゾン KWインデックス＆検索順位 チェックツール",
  "version": "0.1.8",
  "manifest_version": 2,
  "description": "アマゾンでの検索キーワード表示順位とインデクス化状況チェックツール AX Arrows10 - 最高の精度を誇るキーワードリサーチツール",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "ja",
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "gcm",
    "contextMenus",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "tabs",
    "identity",
    "*://*.amazon.ca/*",
    "*://*.amazon.com/*",
    "*://*.amazon.co.uk/*",
    "*://*.amazon.de/*",
    "*://*.amazon.fr/*",
    "*://*.amazon.es/*",
    "*://*.amazon.it/*",
    "*://*.amazon.co.jp/*",
    "*://*.amazon.in/*",
    "*://*.amazon.com.au/*",
    "*://*.amazon.com.mx/*"
  ],
  "oauth2": {
    "client_id": "1026408443454-g7o468pdr1n1l2lga3jrquuommaseat3.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "key": "",
  "content_scripts": [
    {
      "matches": [
        "*://*.amazon.ca/*",
        "*://*.amazon.com/*",
        "*://*.amazon.co.uk/*",
        "*://*.amazon.de/*",
        "*://*.amazon.fr/*",
        "*://*.amazon.es/*",
        "*://*.amazon.it/*",
        "*://*.amazon.co.jp/*",
        "*://*.amazon.in/*",
        "*://*.amazon.com.au/*",
        "*://*.amazon.com.mx/*",
        "*://*.arrows10.com/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    }
  },
  "externally_connectable": {
    "matches": [
      "*://arrows10.com/*",
      "*://*.arrows10.com/*"
    ]
  }
}
