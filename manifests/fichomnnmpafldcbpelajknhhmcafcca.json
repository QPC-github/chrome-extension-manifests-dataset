{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "OGFlickr.js"
    ],
    "matches": [
      "*.flickr.com/*"
    ]
  },
  "description": "Right click on a flickr photo page to get access to it's original size image",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "6.0.0.0",
  "name": "OG Flickr",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.2"
}
