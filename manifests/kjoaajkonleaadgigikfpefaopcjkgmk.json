{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Threads",
  "description": "Threads extension for uploading images",
  "version": "1.0.11",
  "short_name": "Threads",
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.mythreadsapp.co/*",
        "https://*.mythreadsapp.co/*",
        "http://localhost:4001/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "contextJs.js"
    ]
  },
  "author": "BCGDV",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
