{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "popup.html"
  },
  "description": "Plugin Play video fast on TVHAY",
  "externally_connectable": {
    "matches": [
      "*://*.tvhay.org/*"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "TVHAY",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.tvhay.org/*",
    "*://tvhay.org/*",
    "*://*.fbcdn.net/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
