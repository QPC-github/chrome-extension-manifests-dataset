{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DATY checker",
  "version": "0.9.3",
  "manifest_version": 2,
  "description": "DATY checker is the best tool for finding details of all Czech companies in quick and convenient way.",
  "homepage_url": "http://daty.cz",
  "icons": {
    "48": "icons/icon48.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/daty.js"
      ]
    }
  ]
}
