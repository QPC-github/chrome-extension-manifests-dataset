{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bealls List",
  "version": "1.0",
  "description": "Beall's list of predatory publishers",
  "icons": {
    "48": "icons/beallslist_x48.png",
    "64": "icons/beallslist_x64.png",
    "72": "icons/beallslist_x72.png",
    "96": "icons/beallslist_x96.png",
    "128": "icons/beallslist_x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "beallslist.js"
      ]
    }
  ]
}
