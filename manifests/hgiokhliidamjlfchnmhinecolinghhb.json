{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {},
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://opticsearch.pro/i/logo.png",
      "is_default": true,
      "keyword": "o",
      "name": "Optic",
      "search_url": "http://opticsearch.pro/go/?q={searchTerms}"
    }
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Optical search - your personal search engine!",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Optic Search",
  "permissions": [],
  "short_name": "oSearch",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1",
  "web_accessible_resources": [
    "icon16.png"
  ]
}
