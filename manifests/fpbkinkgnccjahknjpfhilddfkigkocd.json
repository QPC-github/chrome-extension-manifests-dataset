{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "魔镜",
  "description": "网店研究和关键词研究的好帮手",
  "version": "1.32",
  "permissions": [
    "cookies",
    "contextMenus",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "management",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "jquery-1.8.2.js",
      "init.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://www.baidu.com/*",
        "https://www.baidu.com/*"
      ],
      "js": [
        "jquery-1.8.2.js",
        "js/ironman_depend.min.js",
        "js/ironman.js",
        "js/ironman_static.min.js",
        "content.js"
      ],
      "css": [
        "css/bootstrap.css",
        "css/ironman_m.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://www.baidu.com/*",
        "https://www.baidu.com/*"
      ],
      "js": [
        "jquery-1.8.2.js",
        "js/ironman_depend.min.js",
        "js/ironman.js",
        "js/ironman_static.min.js",
        "content.js"
      ],
      "css": [
        "css/bootstrap.css",
        "css/ironman_m.css"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ironman.moojing.com; object-src 'self'",
  "browser_action": {
    "default_icon": "mojing_48x48.png",
    "default_title": "魔镜",
    "default_popup": "popup.html"
  },
  "icons": {
    "19": "mojing_19x19.png",
    "38": "mojing_38x38.png",
    "16": "mojing_16x16.png",
    "48": "mojing_48x48.png",
    "128": "mojing_128x128.png"
  },
  "browser_actions": {
    "popup": "popup.html"
  }
}
