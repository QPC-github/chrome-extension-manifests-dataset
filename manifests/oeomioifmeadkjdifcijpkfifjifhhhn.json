{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "name": "UpGuard Cyber Security Ratings",
  "short_name": "UpGuard",
  "description": "Take UpGuard with you and see the security rating for every website you visit.",
  "version": "3.1.3",
  "background": {
    "scripts": [
      "scripts/background-3-1-3.min.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "include_globs": [
        "http://www.google.*/*",
        "https://www.google.*/*"
      ],
      "css": [
        "/stylesheets/popover-3-1-3.css"
      ],
      "js": [
        "/scripts/common_augment-3-1-3.min.js",
        "/scripts/google_augment-3-1-3.min.js"
      ]
    },
    {
      "matches": [
        "https://www.bing.com/*",
        "http://www.bing.com/*"
      ],
      "css": [
        "/stylesheets/popover-3-1-3.css"
      ],
      "js": [
        "/scripts/common_augment-3-1-3.min.js",
        "/scripts/bing_augment-3-1-3.min.js"
      ]
    },
    {
      "matches": [
        "https://duckduckgo.com/*",
        "http://duckduckgo.com/*"
      ],
      "css": [
        "/stylesheets/popover-3-1-3.css"
      ],
      "js": [
        "/scripts/common_augment-3-1-3.min.js",
        "/scripts/duckduckgo_augment-3-1-3.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "/assets/inactive-3-1-3.png",
    "default_title": "CSR",
    "default_popup": "templates/popup-3-1-3.html"
  },
  "icons": {
    "16": "/assets/inactive-3-1-3.png",
    "128": "/assets/128_icon-3-1-3.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://webscan.cyber-risk.upguard.com/api/v1/*",
    "https://*/*",
    "http://*/*",
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"
  ],
  "web_accessible_resources": [
    "/assets/amber-3-1-3.png",
    "/assets/csr_logo_white-3-1-3.png",
    "/assets/green-3-1-3.png",
    "/assets/inactive-3-1-3.png",
    "/assets/pause-3-1-3.png",
    "/assets/pin-3-1-3.png",
    "/assets/red-3-1-3.png",
    "/assets/right_arrow-3-1-3.png",
    "/assets/skull-3-1-3.png",
    "/assets/ug_logo_white-3-1-3.png",
    "/assets/orange-3-1-3.png",
    "/assets/icon-failed-3-1-3.png",
    "/assets/icon-passed-3-1-3.png",
    "/assets/rock-3-1-3.png",
    "/assets/Lato-Light.ttf"
  ]
}
