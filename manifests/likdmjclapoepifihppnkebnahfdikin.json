{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "rslash",
  "description": "Seamlessly integrating Reddit straight into the URL bar",
  "version": "0.2.5",
  "omnibox": {
    "keyword": "r\\"
  },
  "icons": {
    "16": "icon16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": []
}
