{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KorbenYoutubePatch",
  "version": "1.0",
  "description": "Patches l'affichage en plein écran des vidéos Youtube sur korben.info",
  "content_scripts": [
    {
      "exclude_globs": [],
      "exclude_matches": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ]
}
