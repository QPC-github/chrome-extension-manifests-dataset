{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cyber Cookie Clicker",
  "version": "1.0",
  "description": "A cool and addictive game!",
  "minimum_chrome_version": "46",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "A fun and addicting game!"
  },
  "manifest_version": 2
}
