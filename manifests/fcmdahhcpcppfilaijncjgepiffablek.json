{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Hangrz",
  "version": "1.3",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "background"
  ],
  "browser_action": {
    "default_title": "Hangrz",
    "default_icon": "hangrz-ffx-chrome-button16x16.png"
  },
  "icons": {
    "16": "hangrz-ffx-chrome-button16x16.png",
    "48": "hangrz-ffx-chrome-button48x48.png",
    "128": "hangrz-ffx-chrome-button128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "http://www.hangrz.com",
  "description": "The Hangrz extension allows users to quickly add images from any site to their Hangz.com account."
}
