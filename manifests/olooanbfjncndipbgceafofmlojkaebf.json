{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Le Stream Drilmo",
  "description": "Extension Chrome de Drilmo !",
  "version": "1.0.2.2",
  "permissions": [
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "img/green_16.png",
    "48": "img/green_48.png",
    "128": "img/green_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
