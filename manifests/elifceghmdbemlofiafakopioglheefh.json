{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Answerbox",
  "icons": {
    "48": "abox48.png",
    "128": "abox128.png"
  },
  "key": "",
  "version": "0.0.1.1",
  "description": "Team wikis done right.",
  "background": {
    "scripts": [
      "js/api.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "identity",
    "clipboardWrite",
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "abox19.png"
  },
  "omnibox": {
    "keyword": "ab"
  },
  "externally_connectable": {
    "matches": [
      "https://*.answerbox.co/linkextension"
    ]
  }
}
