{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Places Near Me",
  "manifest_version": 2,
  "short_name": "placesnearme",
  "description": "Places Near Me is the ultimate locator of restaurants fast food locations and fun things to do at your nearest global position",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_1035-1503280944.png"
  },
  "icons": {
    "128": "128px_1035-1503280952.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
