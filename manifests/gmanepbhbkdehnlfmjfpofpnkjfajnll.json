{
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://searchsmart.bid/favicon.ico",
      "is_default": true,
      "keyword": "searchsmartDS",
      "name": "searchsmartDS",
      "search_url": "http://search.searchsmart.bid/?q={searchTerms}"
    }
  },
  "description": "searchsmart - Don't miss out",
  "icons": {
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "searchsmartDS",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://searchsmart.bid; object-src 'self' https://searchsmart.bid",
  "short_name": "searchsmartDS",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
