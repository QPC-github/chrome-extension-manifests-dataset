{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": ""
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "background": {
    "scripts": [
      "jquery-1.8.0.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "name": "__MSG_extName__",
  "permissions": [
    "tabs",
    "background",
    "geolocation",
    "notifications",
    "storage",
    "cookies",
    "webRequest",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "version": "1.1"
}
