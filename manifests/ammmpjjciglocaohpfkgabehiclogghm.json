  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "description": "1net Fast and Easy Login, 1net is an ecommerce technology solution.",
  "homepage_url": "https://1net.me",
  "permissions": [
    "tabs"
  ]
}
