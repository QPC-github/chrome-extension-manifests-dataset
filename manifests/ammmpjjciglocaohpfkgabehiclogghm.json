{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "1net.me",
  "version": "1.1.0",
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
