{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kill sticky headers",
  "description": "Make the current pages sticky headers disappear",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Kill sticky headers"
  },
  "manifest_version": 2
}
