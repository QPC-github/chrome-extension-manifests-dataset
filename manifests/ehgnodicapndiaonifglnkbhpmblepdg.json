{
  "author": "Mindquilt, Inc.",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*.mindquilt.com/*",
        "*://*.knowledgecoop.com/*"
      ]
    }
  ],
  "description": "This extension is currently used for internal testing.",
  "homepage_url": "https://www.mindquilt.com",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "MindQuilt Extension",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.3",
  "web_accessible_resources": [
    "icon.png"
  ]
}
