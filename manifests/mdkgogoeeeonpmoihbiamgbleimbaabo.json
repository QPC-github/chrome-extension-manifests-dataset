{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickTrack",
  "short_name": "A Chrome extension for Marketers",
  "description": "This extension efficiently adds utm tracking links to your current active tab, brought to you by Fede Behrens @rafabehrens",
  "version": "2.93",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Get Tracking!"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
