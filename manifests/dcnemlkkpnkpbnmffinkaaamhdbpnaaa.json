{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "js/datepicker.js",
        "js/vk_inject2.js"
      ],
      "css": [
        "css/datepicker.css",
        "css/vk_inject.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "option.html",
    "default_icon": {
      "48": "img/re22.png",
      "32": "img/32.png"
    },
    "default_title": "Reposter"
  },
  "description": "Reposter",
  "name": "Reposter",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage",
    "*://vk.com/*",
    "https://api.vk.com/method*"
  ],
  "version": "1.0.5"
}
