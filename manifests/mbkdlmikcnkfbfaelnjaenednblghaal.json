{
  "background": {
    "persistent": false,
    "scripts": [
      "js/bg_thefinancials.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "feed.html",
    "default_title": "The Financials"
  },
  "content_scripts": [
    {
      "js": [
        "js/checkinstall.js"
      ],
      "matches": [
        "http://*.thefinancials.org/*",
        "http://*.thefinancials.org/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "description": "The Financials",
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "The Financials",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "http://*.TheFinancials.org/*",
    "http://*.thefinancials.org/*",
    "http://*.feeds.feedburner.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.7.1"
}
