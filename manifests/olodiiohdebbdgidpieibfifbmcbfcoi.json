{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Whois tool",
  "description": "Domain Whois tool - etools.io",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-1.9.0.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://etools.io/"
  ],
  "content_security_policy": "script-src 'self'; 'http://etools.io/';unsafe-inline; object-src 'self'"
}
