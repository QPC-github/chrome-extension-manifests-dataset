{
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Disable upload file input",
  "icons": {
    "128": "upload_disable.png"
  },
  "manifest_version": 2,
  "name": "File upload block",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [
    "notavailable.html",
    "notavailable.js",
    "notavailable.css"
  ]
}
