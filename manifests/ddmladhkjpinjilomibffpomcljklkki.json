{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Text to Cat Puns",
  "description": "Click the extension button and any web page's text will turn into cat puns.",
  "version": "0.4",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "cats.png"
  },
  "icons": {
    "16": "cats16.png",
    "48": "cats48.png",
    "128": "cats.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
