{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LPlus/CAT Optimizer",
  "description": "Mit dieser Erweiterung wird LPlus/CAT um einige n√ºtzliche Funktionen erweitert.",
  "version": "2.1.2",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/QuestionPage.aspx"
      ],
      "js": [
        "LPlus.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "declarativeContent"
  ],
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
