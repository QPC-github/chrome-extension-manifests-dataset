{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "Super Smash Flash 2",
  "name": "Super Smash Flash 2 Unblocked",
  "version": "2",
  "version_name": "ssf 2",
  "description": "Super Smash Flash 2 Unblocked legendary flash game for school in this extension.",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src https://topaddon.com/game.swf 'self'",
  "icons": {
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://www.bing.com/*",
    "https://search.yahoo.com/*",
    "https://*.search.yahoo.com/*",
    "https://www.google.com/search*",
    "https://www.google.com.br/search*",
    "https://www.google.com.mx/search*",
    "https://www.google.com.au/search*",
    "https://www.google.co.uk/search*",
    "https://www.google.co.jp/search*",
    "https://www.google.com.au/search*",
    "https://www.google.de/search*",
    "https://www.google.fr/search*",
    "https://www.google.ca/search*",
    "https://www.google.us/search*"
  ],
  "browser_action": {
    "default_icon": {
      "128": "icons/128.png"
    }
  }
}
