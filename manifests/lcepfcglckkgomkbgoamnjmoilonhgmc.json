{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Style Manager",
  "description": "Rectifies web pages with incorrect style formats",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "*://www.google.co.uk/*"
      ],
      "run_at": "document_end",
      "js": [
        "scripts.js"
      ]
    }
  ],
  "permissions": [
    "*://www.google.co.uk/*"
  ]
}
