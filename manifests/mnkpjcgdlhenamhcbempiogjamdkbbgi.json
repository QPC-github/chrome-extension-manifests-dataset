{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tagbird",
  "short_name": "Tagbird",
  "description": "Get the best view on your online data collection",
  "version": "1.8",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "96": "img/icon_96.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "96": "img/icon_96.png",
      "128": "img/icon_128.png"
    },
    "default_title": "Tagbird",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "css": [
        "iziToast.min.css",
        "inpage.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inpage.js",
    "img/aa_s.png",
    "img/aa_l.png",
    "img/ads_s.png",
    "img/ads_l.png",
    "img/awin_s.png",
    "img/awin_l.png",
    "img/dc_s.png",
    "img/dc_l.png",
    "img/deq_s.png",
    "img/deq_l.png",
    "img/fb_s.png",
    "img/fb_l.png",
    "img/ga_s.png",
    "img/ga_l.png",
    "img/gtm_s.png",
    "img/gtm_l.png",
    "img/mtm_s.png",
    "img/mtm_l.png",
    "img/r42_s.png",
    "img/r42_l.png",
    "img/tlm_iq_s.png",
    "img/tlm_iq_l.png",
    "img/tlm_ss_s.png",
    "img/tlm_ss_l.png"
  ]
}
