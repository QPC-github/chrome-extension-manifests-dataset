{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "pause",
  "description": "pause is a time manager which tells you when to focus and when to take a break.",
  "version": "0.1.3",
  "manifest_version": 2,
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "components/moment/moment.js",
        "scripts/TimerHelpers.js",
        "background.js"
      ],
      "persistent": false
    }
  },
  "icons": {
    "128": "pause-128.png"
  },
  "permissions": [
    "alarms",
    "idle",
    "notifications",
    "storage"
  ]
}
