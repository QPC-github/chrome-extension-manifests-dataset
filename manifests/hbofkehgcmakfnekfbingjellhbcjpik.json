{
  "background": "popup.html",
  "content_scripts": [
    {
      "js": [
        "jquery-1.11.3.min.js",
        "popup.js"
      ],
      "matches": [
        "http://www.rightmove.co.uk/property-for-sale/property-*",
        "http://www.rightmove.co.uk/new-homes-for-sale/property-*",
        "https://www.rightmove.co.uk/property-for-sale/property-*",
        "https://www.rightmove.co.uk/new-homes-for-sale/property-*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Get more information in your search on Rightmove.co.uk. See propcision.com",
  "manifest_version": 2,
  "name": "Propcision",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "https://ajax.googleapis.com/",
    "https://www.propcision.com/*",
    "background",
    "https://*/*",
    "webNavigation"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.1",
  "web_accessible_resources": [
    "ga.js"
  ]
}
