{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "百度搜索助手",
  "version": "1.1.2",
  "manifest_version": 2,
  "icons": {
    "16": "1.jpg",
    "48": "2.jpg",
    "128": "2.jpg"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "autopage.js"
      ],
      "css": [
        "autopage.css"
      ],
      "matches": [
        "http://www.baidu.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
