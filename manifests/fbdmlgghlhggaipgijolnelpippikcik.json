{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Resize Input",
  "version": "0.1.2",
  "description": "Allows you to resize input fields like you can with textareas.",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
