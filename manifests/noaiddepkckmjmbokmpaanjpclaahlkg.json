{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ATSKonnect",
  "description": "ATS Finder for given website.",
  "version": "3.0",
  "web_accessible_resources": [
    "popup.html",
    "js/popup.js",
    "css/popup.css"
  ],
  "permissions": [
    "storage",
    "https://bizkonnector.bizkonnect.com/techfinder/processRequest/*",
    "http://bizkonnector.bizkonnect.com/techfinder/*",
    "https://bizkonnector.bizkonnect.com/techfinder/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://bizkonnector.bizkonnect.com/techfinder/processRequest ; object-src 'self'"
}
