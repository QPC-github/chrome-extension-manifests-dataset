{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Woofer",
  "description": "Purges your TL of non-doggo content",
  "version": "1.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "doge48.png"
  },
  "icons": {
    "48": "doge48.png",
    "128": "doge128.png"
  },
  "permissions": [
    "tabs",
    "https://www.twitter.com/*",
    "https://twitter.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
