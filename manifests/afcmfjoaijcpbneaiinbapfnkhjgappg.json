{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump, Begone!",
  "short_name": "trumpbegone",
  "version": "0.1",
  "author": "Jon Dayton",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.washingtonpost.com/*",
        "*://*.cnn.com/*",
        "*://*.foxnews.com/*",
        "*://*.breitbart.com/*",
        "*://abcnews.go.com/*",
        "*://*.huffingtonpost.com/*",
        "*://*.wsj.com/*",
        "*://*.nytimes.com/*",
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery-3.2.1.slim.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
