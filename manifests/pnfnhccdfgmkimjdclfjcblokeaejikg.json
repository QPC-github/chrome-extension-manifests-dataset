{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MinimalTwitter",
  "description": "Redesigns Twitter's UI with minimalism in mind.",
  "version": "1.4.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "minimal.css"
      ]
    }
  ],
  "permissions": [
    "https://twitter.com/*"
  ]
}
