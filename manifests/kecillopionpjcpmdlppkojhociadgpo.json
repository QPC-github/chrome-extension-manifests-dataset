{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Archiver",
  "description": "Archive your bookmarks using archive.org",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "https://web.archive.org/"
  ]
}
