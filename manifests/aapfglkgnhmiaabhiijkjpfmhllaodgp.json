{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nymeria: The best personal email finder",
  "version": "3.2.2",
  "manifest_version": 2,
  "description": "Nymeria is the best email finder for finding email addresses on social or professional networking sites.",
  "homepage_url": "https://www.nymeria.io",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_title": "Browse and click to find emails!",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "src/needle.js"
    ]
  },
  "permissions": [
    "*://*.nymeria.io/*",
    "*://*.linkedin.com/*",
    "*://*.github.com/*",
    "*://*.dev.to/*",
    "*://*.stackoverflow.com/*",
    "tabs",
    "cookies"
  ]
}
