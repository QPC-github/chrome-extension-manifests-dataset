{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.1",
  "name": "сохранялка",
  "description": "сохранение изображений со страницы",
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus",
    "notifications",
    "<all_urls>",
    "downloads"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js",
      "jszip.js",
      "save-images.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "512": "data/icons/512.png"
  },
  "web_accessible_resources": [
    "data/inject/index.html"
  ],
  "homepage_url": "http://clould.ru"
}
