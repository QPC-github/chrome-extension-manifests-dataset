{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "com.commsl.browser_extension",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://*.archpelago.com/*",
      "http://*.archpelago.com/*"
    ]
  },
  "version": "1.4",
  "manifest_version": 2,
  "description": "Cloud CRM Extension.",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ]
}
