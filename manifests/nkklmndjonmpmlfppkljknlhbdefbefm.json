{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery.min.js"
    ]
  },
  "name": "Searchify",
  "author": "Karol Nowi≈Ñski",
  "description": "This extension allows the user to search lyrics of the song which is playing on the current YouTube page.",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Let me search lyrics!"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
