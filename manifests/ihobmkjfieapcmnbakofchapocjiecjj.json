{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Your Tab",
  "description": "Allows you to personalize your new tab page with notes and quick links. Also includes time and weather",
  "version": "1.1",
  "icons": {
    "16": "icon.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "geolocation"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
