{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hookmark",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "【URLシェアの新しいカタチ】\n・HookmarkはオフラインでURLすばやく共有するのに最善のサービスです。\n・シェアするURLを入力する。生成された画像を見せる。ただそれだけです。\n・特別なアプリは必要ありません。",
  "icons": {
    "48": "img/icon48x48.png",
    "128": "img/icon128x128.png"
  },
  "browser_action": {
    "default_icon": "img/icon48x48.png",
    "default_title": "Hookmark",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ]
}
