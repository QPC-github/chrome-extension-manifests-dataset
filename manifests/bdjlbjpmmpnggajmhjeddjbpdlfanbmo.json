{
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "incognito": "split",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "__MSG_appName__",
  "description": "__MSG_application_description__",
  "default_locale": "tr",
  "version": "2.0.0",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "İstanbul Araştırmaları Enstitüsü",
  "icons": {
    "128": "icons/128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "*://*.google.com/*"
  ]
}
