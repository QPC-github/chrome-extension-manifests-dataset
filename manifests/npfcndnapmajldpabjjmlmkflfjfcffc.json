{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "force.com Schema Utils",
  "description": "Extension provide maximize button to maximize canvas area and export to image facility.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "contextMenus",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.0.3.js",
        "html2canvas.js",
        "script.js"
      ]
    }
  ],
  "manifest_version": 2
}
