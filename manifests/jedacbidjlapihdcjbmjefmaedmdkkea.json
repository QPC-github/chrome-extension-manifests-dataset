{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UnDisable",
  "description": "Allows you to enable all the disabled elements on the page.",
  "version": "1.1",
  "icons": {
    "16": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "script.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enable, disabled fields.",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
