{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "closure-compiled.js"
      ],
      "matches": [
        "https://mail.google.com/"
      ]
    }
  ],
  "icons": {
    "128": "youtube-icon.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "page_action": {
    "default_icon": "youtube-icon.png",
    "name": "Load video"
  },
  "manifest_version": 2,
  "name": "Video in a box!",
  "permissions": [
    "tabs",
    "https://mail.google.com/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0.0.3"
}
