{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BeToken",
  "version": "1.0",
  "description": "BeToken Chrome Extension.",
  "browser_action": {
    "default_icon": {
      "16": "favicon.png",
      "24": "favicon.png",
      "32": "favicon.png"
    }
  },
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://api.betoken.app/"
  ]
}
