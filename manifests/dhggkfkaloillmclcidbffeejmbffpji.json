{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub Notifier",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Turns GitHub notifications into desktop notifications with audio notification also",
  "homepage_url": "https://chrome.google.com/webstore/detail/dhggkfkaloillmclcidbffeejmbffpji",
  "icons": {
    "16": "icons/icon-social-github-16.png",
    "48": "icons/icon-social-github-48.png",
    "128": "icons/icon-social-github-128.png"
  },
  "background": {
    "scripts": [
      "js/store.js",
      "js/jquery-2.1.3.min.js",
      "js/poller.js"
    ]
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "cookies",
    "notifications",
    "https://*.github.com/*"
  ]
}
