{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random /pol/ Post",
  "version": "0.5.1",
  "manifest_version": 2,
  "description": "Takes random posts from the 4chan board /pol/ and displays them for maximum comedy",
  "browser_action": {
    "default_icon": "icon_small.png",
    "default_title": "Get Post",
    "default_popup": "main.html"
  },
  "permissions": [
    "tts"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "icon.png"
  }
}
