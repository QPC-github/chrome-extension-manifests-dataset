{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spark for viewer",
  "short_name": "Spark",
  "description": "SparkExtension",
  "version": "2.1.5",
  "browser_action": {
    "default_icon": "ICONS/logo-03.png",
    "default_popup": "client.html"
  },
  "content_security_policy": "script-src 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "https://static-cdn.jtvnw.net/",
    "https://ajax.googleapis.com/",
    "https://spark-esport.cleverapps.io/*"
  ],
  "icons": {
    "128": "ICONS/logo-03.png"
  }
}
