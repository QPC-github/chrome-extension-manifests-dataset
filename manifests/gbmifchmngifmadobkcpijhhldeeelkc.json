{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "jquerify.jpg",
    "name": "Embed jQuery library to the current page."
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "128": "jquerify_128.png"
  },
  "manifest_version": 2,
  "name": "jQuerify",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.5.0"
}
