{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sites Notes",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "128x128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Hidden Text",
    "default_icon": "128x128.png",
    "default_popup": "popup.html"
  }
}
