{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Перекупы Авто",
  "version": "2.60",
  "description": "Узнай, кто перекуп на сайтах drom.ru, auto.ru, avito.ru, а кто нет.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "*://*/"
  ],
  "web_accessible_resources": [
    "shared/*"
  ],
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; frame-src 'self' * data: chrome-extension-resource:;"
}
