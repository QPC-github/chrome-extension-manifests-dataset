{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.cloodhq.net; object-src 'self' https://www.cloudhq.net",
  "web_accessible_resources": [
    "javascripts/*",
    "icons/*"
  ],
  "name": "Save emails to OneDrive by cloudHQ",
  "short_name": "Save emails to OneDrive directly from Gmailâ„¢ by cloudHQ",
  "icons": {
    "128": "icons/icon-128x128.png",
    "48": "icons/icon-48x48.png"
  },
  "browser_action": {
    "default_icon": "icons/save_to_onedrive.png"
  },
  "converted_from_user_script": true,
  "background": {
    "persistent": true,
    "scripts": [
      "javascripts/jquery-1.4.4.js",
      "javascripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "exclude_globs": [],
      "css": [
        "stylesheets/jquery.dropdown.css",
        "stylesheets/popover.css",
        "stylesheets/file_browser.css",
        "stylesheets/main_save_to_onedrive.css"
      ],
      "include_globs": [],
      "js": [
        "javascripts/inboxsdk.js",
        "javascripts/main_save_to_onedrive_loader.js"
      ],
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ]
    }
  ],
  "version": "1.0.0.8",
  "description": "Save any Gmail email to OneDrive with one click. Autosave of all emails in a label. Multiple export formats: PDF, HTML, TXT, ...",
  "permissions": [
    "storage",
    "background",
    "https://mail.google.com/",
    "https://inbox.google.com/",
    "https://www.cloudhq.net/"
  ],
  "manifest_version": 2
}
