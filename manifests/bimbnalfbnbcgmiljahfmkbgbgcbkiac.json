{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Defense War",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Defense War",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon19.jpg",
      "38": "icon38.jpg"
    }
  }
}
