{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.2.1",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "panel.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "main.js"
    ],
    "persistent": true
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "background",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "minimum_chrome_version": "18",
  "manifest_version": 2
}
