{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "MyLinkBag",
  "version": "0.15",
  "manifest_version": 2,
  "description": "Goodbye Bookmarks, Hello MyLinkBag",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://mylinkbag.com/",
    "tabs"
  ]
}
