{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Developers are Monkeys",
  "description": "This extension replaces all occurences of the word \"Developer\" with the word \"Monkey\"",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "developersaremonkeys.js"
      ]
    }
  ]
}
