{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "description": "Search youtube for keyword. You can Search in youtube video easily ",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "Youtube Search",
  "permissions": [
    "contextMenus",
    "activeTab",
    "downloads",
    "http://*.youtube.com/*",
    "https://*.youtube.com/*",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
