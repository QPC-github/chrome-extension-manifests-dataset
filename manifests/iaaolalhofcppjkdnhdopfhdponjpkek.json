{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "author": "Minh Huy",
  "name": "Ctrack",
  "description": "Ctrack extension",
  "short_name": "genCRM Data Tool",
  "version": "1.0.4",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
