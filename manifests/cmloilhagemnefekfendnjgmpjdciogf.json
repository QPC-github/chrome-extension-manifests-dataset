{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "捨て星ツール - Suteboshi Extension",
  "description": "Suteboshi-tool for extention for Google Chrome.",
  "version": "0.1.2",
  "browser_action": {
    "default_popup": "opener.html",
    "default_title": "SutEx",
    "default_icon": {
      "16": "icons/sute_icon.png",
      "32": "icons/sute_icon32.png"
    }
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens suteboshi.html"
    }
  }
}
