{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SmartBIGSURVEY",
  "description": "Provides patch to NetCommunity and Internet Solutions forms editors to support SmartBIGSURVEY",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://*/cms/customformcontenteditor*",
        "https://*/*/cms/customformcontenteditor*"
      ],
      "js": [
        "jquery.js",
        "smartsurvey.js"
      ],
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "inpage.js"
  ]
}
