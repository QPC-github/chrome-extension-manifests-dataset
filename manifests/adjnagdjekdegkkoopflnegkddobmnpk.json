{
  "manifest_version": 2,
  "description": "Basically URL Shortener | 13 more service",
  "name": "Play Shorter | 13+ URL Shorting Services",
  "version": "1.0.1",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://api.bitly.com/*",
    "http://tinyurl.com/*",
    "http://b54.in/api/*",
    "http://chilp.it/*",
    "http://cut.by/*",
    "http://merky.de/*",
    "http://mcaf.ee/*",
    "http://s.coop/*",
    "http://surl.me/*",
    "http://ux.nu/api/*",
    "https://v.gd/*",
    "http://www.vai.la/*",
    "https://is.gd/",
    "tabs",
    "activeTab",
    "storage"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "Pages\\Popup.html"
  },
  "options_page": "Pages\\Options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
