{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ChrisGUI",
  "description": "Replaces words with emojis in the Alliance and Global chat windows in Illyriad.",
  "version": "0.5",
  "icons": {
    "48": "icon/chrisg.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "icon/chrisg.png"
    },
    "default_area": "navbar",
    "default_title": "emoji Codes",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./emojiMap4.js",
        "./substitute2.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "icon/chrisg.png",
    "popup.html",
    "chrisgui-page.js"
  ]
}
