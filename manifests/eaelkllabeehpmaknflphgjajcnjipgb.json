{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github download button",
  "version": "0.1",
  "description": "Add a Github download button",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "author": "ukyo",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://github.com/*"
  ]
}
