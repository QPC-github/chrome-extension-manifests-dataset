{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MathHook",
  "version": "0.1.0",
  "description": "Solve Math Problems in One Click",
  "permissions": [
    "tabs",
    "webRequest",
    "gcm",
    "webRequestBlocking",
    "*://symbolab.com/*",
    "*://mathhook.herokuapp.com/*",
    "notifications",
    "https://www.google-analytics.com/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://www.symbolab.com/; object-src 'self';",
  "icons": {
    "16": "assets/mathhook.png",
    "48": "assets/mathhook.png",
    "128": "assets/mathhook.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/mathhook.png",
      "24": "assets/mathhook.png",
      "32": "assets/mathhook.png"
    },
    "default_popup": "/testData/solutionData.html",
    "default_title": "MathHook"
  },
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "javascripts/jquery-3.2.1.min.js",
    "javascripts/service/co.js"
  ],
  "manifest_version": 2,
  "author": "Ahmed Mashhour",
  "incognito": "spanning"
}
