{
  "browser_action": {
    "default_icon": "img/icone/128.png",
    "default_title": "Full HD NewTab"
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "description": "Install this theme to get Full HD wallpapers in every newtab.",
  "icons": {
    "128": "img/icone/128.png"
  },
  "manifest_version": 2,
  "name": "Full HD Wallpapers for New Tab ",
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "chrome://favicon/*",
    "bookmarks",
    "management",
    "history",
    "bookmarks",
    "storage",
    "sessions",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "tabCapture",
    "contextMenus",
    "webRequest",
    "cookies",
    "topSites"
  ],
  "short_name": "Full HD NewTab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.5",
  "web_accessible_resources": [
    "pages/newtab.html"
  ]
}
