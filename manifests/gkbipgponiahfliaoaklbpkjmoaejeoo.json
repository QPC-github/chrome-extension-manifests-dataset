{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickDesk Chrome Extension",
  "description": "This is my QuickDesk extension!",
  "version": "1.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://manage.quickdesk.io",
    "https://quickdesk.io",
    "*://*.quickdesk.io/*",
    "https://manage.quickdesk.io/login"
  ]
}
