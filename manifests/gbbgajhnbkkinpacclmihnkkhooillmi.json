{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MIT News Dashboard",
  "description": "Get the latest stories from MIT News every time you open a new tab!",
  "version": "3.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "./images/logo.png"
  },
  "permissions": [
    "http://m.mit.edu/apis",
    "https://m.mit.edu/apis",
    "storage"
  ]
}
