{
  "name": "Намба почта. Плагин для Google Chrome",
  "version": "3.5.5",
  "manifest_version": 2,
  "description": "Получай оповещение о новых письмах с Namba",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/namba_gray.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/namba48.png"
  ],
  "icons": {
    "128": "img/namba128.png",
    "16": "img/namba.png",
    "48": "img/namba48.png",
    "64": "img/namba64.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
