{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "sskaje",
  "name": "GSPxy",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Proxy for Google static files, such as scripts, stylesheets, fonts...",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "gsp_config.js",
      "gsp_extension.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://sskaje.me/gspxy/"
}
