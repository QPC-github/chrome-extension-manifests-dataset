{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Delete Dell Cookies",
  "version": "1.63",
  "manifest_version": 2,
  "description": "Press the button to delete all cookies with \"dell\" in the domain.  Also, clear MPP and ANAV stickiness",
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "cookie.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
