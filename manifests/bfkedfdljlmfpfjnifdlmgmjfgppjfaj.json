{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Keyboard Shortcuts for Nulu",
  "description": "Adds keyboard shortcuts to Nulu.com",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.nulu.com/*/stories/*",
        "https://www.nulu.com/*/stories/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "components/jquery/jquery.js",
        "index.js",
        "components/mousetrap/mousetrap.js"
      ],
      "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  ]
}
