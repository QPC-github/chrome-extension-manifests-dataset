{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuoteTabs",
  "short_name": "Quotes",
  "description": "Inspirational quotes for your tabs!",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "quotes.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "quotes.png",
    "64": "quotes.png",
    "32": "quotes.png",
    "16": "quotes.png"
  }
}
