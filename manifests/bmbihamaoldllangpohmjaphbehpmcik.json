{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My Booked Courses",
  "description": "Catch your booked courses and send it to your mobile app",
  "version": "2.0",
  "short_name": "MyEF courses",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>",
    "debugger"
  ],
  "browser_action": {
    "default_icon": "icon-88.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
