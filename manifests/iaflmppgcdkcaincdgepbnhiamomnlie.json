{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "name": "A Safex Holders Toolkit",
  "description": "Easily view Safex stats, quickly access important Safex web links and calulate your dividends with the inbuilt calculator.",
  "version": "0.0.1",
  "short_name": "Safex",
  "homepage_url": "https://safex.io/",
  "icons": {
    "16": "Images/safex16.png",
    "38": "Images/safex38.png",
    "128": "Images/safex128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "Images/safex16.png",
      "38": "Images/safex38.png"
    },
    "default_title": "Safex",
    "default_popup": "safex.html"
  },
  "background": {
    "scripts": [
      "jquery-3.1.1.js",
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "background",
    "contextMenus",
    "tabs"
  ]
}
