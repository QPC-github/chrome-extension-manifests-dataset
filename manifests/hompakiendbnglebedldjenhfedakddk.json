{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Email Streaks",
  "description": "Streaks for the Outlook Web App!",
  "version": "1.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "Images/icon.png"
  },
  "icons": {
    "128": "Images/icon1.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://webmail.gihs.sa.edu.au/*",
        "https://webmail.gihs.sa.edu.au/*"
      ],
      "js": [
        "Streaks/Main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "Images/icon.png"
  ]
}
