{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Top Forex Robots",
  "description": "Top Performing Forex Robots based on myfxbook live performance results, a detailed comparison between the forex robots profitability",
  "version": "1.0.2",
  "short_name": "CashEncode",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "images/logo/16.png",
    "32": "images/logo/32.png",
    "48": "images/logo/48.png",
    "128": "images/logo/128.png"
  },
  "permissions": [
    "tabs",
    "https://www.reviewforexrobots.com/*",
    "storage",
    "notifications",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "images/logo/19_off.png",
    "default_popup": "popup.html"
  }
}
