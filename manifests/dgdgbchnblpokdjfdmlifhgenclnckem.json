{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Currency exchange",
  "version": "0.0.1",
  "description": "Live currency exchange within chrome !",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/currency16.png",
      "32": "images/currency32.png",
      "48": "images/currency48.png",
      "128": "images/currency128.png"
    }
  },
  "icons": {
    "16": "images/currency16.png",
    "32": "images/currency32.png",
    "48": "images/currency48.png",
    "128": "images/currency128.png"
  },
  "manifest_version": 2
}
