{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "WikiAll",
  "version": "0.6",
  "manifest_version": 2,
  "description": "Search with every service of the Wikimedia Foundation...and not only",
  "icons": {
    "48": "wiki48.png",
    "128": "wiki128.png"
  },
  "browser_action": {
    "default_icon": "wiki19.png",
    "default_title": "WikiAll",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
