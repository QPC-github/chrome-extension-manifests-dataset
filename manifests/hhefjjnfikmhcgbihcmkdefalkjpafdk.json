{
  "background": {
    "scripts": [
      "JeSuisCharlie/bg.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "lib/jq/jquery-2.1.1.min.js",
        "JeSuisCharlie/JeSuisCharlie.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "This extension is a protest against the terrorist attack on the French satirical magazine 'Charlie Hebdo' in January 2015.",
  "icons": {
    "128": "JeSuisCharlie/logo128.png",
    "16": "JeSuisCharlie/logo16.png"
  },
  "manifest_version": 2,
  "name": "Je Suis Charlie",
  "options_page": "JeSuisCharlie/options/options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "JeSuisCharlie/options/options.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "webRequest",
    "<all_urls>"
  ],
  "short_name": "Je Suis Charlie",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.70"
}
