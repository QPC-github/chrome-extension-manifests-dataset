{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cyrano",
  "version": "1.005",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "menu2.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-LmhS+9eIu0WLYh2SEbiRNoS2OCcQ+/SUFqnj4+5XB/Y='; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "jquery-3.3.1.min.js",
        "background.js",
        "browser_action.js"
      ],
      "matches": [
        "https://cyrano.dating/*",
        "https://www.facebook.com/*",
        "https://tinder.com/*"
      ]
    }
  ],
  "permissions": [
    "identity",
    "storage",
    "notifications",
    "https://cyrano.dating/*",
    "https://www.facebook.com/*",
    "https://tinder.com/*"
  ]
}
