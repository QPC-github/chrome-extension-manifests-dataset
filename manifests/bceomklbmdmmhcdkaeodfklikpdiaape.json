{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "clock_16.png",
    "default_title": "Wepware Autologin",
    "default_popup": "popup.html"
  },
  "description": "Allows you to stay signed into Wepware forever. It Automatically Signs in at a given interval.",
  "homepage_url": "https://www.wepware.com",
  "icons": {
    "128": "clock_128.png",
    "16": "clock_16.png"
  },
  "manifest_version": 2,
  "name": "Wepware Auto-login",
  "short_name": "Wepware Auto-login",
  "permissions": [
    "https://www.wepware.com/*",
    "alarms",
    "storage"
  ],
  "version": "1.0"
}
