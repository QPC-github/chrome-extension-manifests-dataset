{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CSNPhilly Video Remover",
  "description": "This extension will remove all videos from articles on csnphilly.com",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "videoRemover.js"
      ],
      "matches": [
        "http://www.csnphilly.com/*",
        "https://www.csnphilly.com/*"
      ],
      "run_at": "document_start"
    }
  ]
}
