{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "description": "Adds a context menu to frames that allows opening them in a new tab, a new window, etc.",
  "icons": {
    "16": "icon16.png"
  },
  "name": "\"This Frame\" Context Menu",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "version": "0.4",
  "manifest_version": 2
}
