{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DeGendering",
  "version": "1.2",
  "description": "Replacing Gender Specific Pronouns with Person",
  "browser_action": {
    "icon": {
      "16": "images/de16.png",
      "48": "images/de48.png",
      "128": "images/de128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "myscript.js"
        ],
        "run_at": "document_end"
      }
    ]
  }
}
