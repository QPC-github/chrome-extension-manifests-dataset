{
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/common.js"
      ],
      "matches": [
        "http://www.pixiv.net/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon.png"
  },
  "name": "Pikki",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Pixivイラストを簡単閲覧！サムネイルをクリックでオリジナルサイズを表示",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "css/common.css",
    "js/common.js",
    "js/jquery.js",
    "icons/fav.png",
    "icons/loading.gif",
    "icons/download.png",
    "icons/full.png",
    "icons/page.png",
    "icons/close.png"
  ]
}
