{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Study Tabs",
  "version": "0.2",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {}
}
