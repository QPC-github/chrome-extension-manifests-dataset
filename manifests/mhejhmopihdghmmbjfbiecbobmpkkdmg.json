{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gemini Quick Access",
  "version": "1.2",
  "description": "Quick access to 'My Work' items in the Countersoft Gemini",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "geminiquickaccess.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html"
}
