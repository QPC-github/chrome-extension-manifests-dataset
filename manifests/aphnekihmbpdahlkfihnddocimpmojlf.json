{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Telenav Chrome Helper",
  "description": "A quick way to map routes on google maps from xml test suites",
  "version": "5.3",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "background": {},
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "img/icon128.png",
    "countries.json"
  ]
}
