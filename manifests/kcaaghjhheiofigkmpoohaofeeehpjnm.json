{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "roll20-superpowers",
  "description": "super powers for roll20",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://app.roll20.net/*"
      ],
      "js": [
        "vendor/zepto.min.js",
        "src/roll20-superpowers.js"
      ]
    }
  ],
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  }
}
