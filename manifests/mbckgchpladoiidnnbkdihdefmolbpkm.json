{
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "name": "Comunidad Gamers Latinos",
  "version": "0.2.3",
  "description": "Extension oficial de la Comunidad CGL, puedes ver las ultimas publicaciones de nuestro sitio web directamente desde tu navegador",
  "short_name": "Extension Comunidad CGL",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "Comunidad Gamers Latinos",
    "default_popup": "cgl.html"
  }
}
