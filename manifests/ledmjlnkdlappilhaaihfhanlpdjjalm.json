  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "name": "Rock Hand",
  "version": "1.1.4",
  "web_accessible_resources": [
    "rockhand.png"
  ]
}
