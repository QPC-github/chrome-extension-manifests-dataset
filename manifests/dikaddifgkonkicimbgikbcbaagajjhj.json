{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mars@Home - Image Labeling using AI",
  "description": "Public Volunteer Computing Project. V1: Label the Unsplash Images using Deep Learning",
  "version": "1.1",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
