{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TomjomoviesGame-Uploads",
  "version": "2.0",
  "description": "De nieuwste video's ",
  "icons": {
    "48": "img/48.jpg",
    "128": "img/128.jpg",
    "16": "img/16.jpg"
  },
  "browser_action": {
    "default_icon": "img/128.jpg",
    "default_title": "Bekijk tomjomovies nieuwste uploads",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ]
}
