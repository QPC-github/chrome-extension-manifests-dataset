{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome old bookmarks manager",
  "short_name": "Classic bookmarks",
  "description": "Old style Chrome bookmark manager extension for people who don't like Chrome's new bookmark manager.",
  "version": "1.0.2",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "homepage_url": "http://bookmarks.tabmark.me",
  "background": {
    "page": "bg.html"
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19-gray.png",
      "38": "images/icon-38-gray.png"
    },
    "default_title": "Old Style Bookmark",
    "default_popup": "action.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "<all_urls>",
    "bookmarks",
    "storage",
    "tabs"
  ]
}
