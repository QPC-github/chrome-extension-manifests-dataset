{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Astrum Cookie Cleaner",
  "description": "Clears user data when all Chrome browser windows are closed.",
  "version": "1.1.1",
  "permissions": [
    "cookies",
    "browsingData",
    "background"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}
