{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "jquery.highlight-3.js",
        "selection.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Search and highlight strings in webpages",
  "manifest_version": 2,
  "name": "PageHighlighter",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
