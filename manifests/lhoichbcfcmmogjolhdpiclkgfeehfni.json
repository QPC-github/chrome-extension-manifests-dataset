{
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "default_locale": "ru",
  "description": "Daitsuna Group Search for Community",
  "icons": {
    "128": "assets/img/icon-128.png",
    "16": "assets/img/icon-16.png",
    "48": "assets/img/icon-48.png"
  },
  "browser_action": {
    "default_icon": "assets/img/icon-48.png",
    "default_title": "Daitsuna Group"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://static.daitsuna.com/favicon.ico",
      "is_default": true,
      "keyword": "daitsuna",
      "name": "Daitsuna Group",
      "search_url": "https://daitsuna.com/search/?q={searchTerms}&addon=chrome&addonversion=0.0.1",
      "suggest_url": "https://daitsuna.com/search/?q={searchTerms}&type=list&mkt=__MSG_@@ui_locale__"
    }
  },
  "homepage_url": "https://daitsuna.com/",
  "content_scripts": [
    {
      "js": [
        "js/funnel-visibility.js"
      ],
      "matches": [
        "*://*.daitsuna.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "externally_connectable": {
    "matches": [
      "*://*.daitsuna.com/*"
    ]
  },
  "name": "Daitsuna Group Search",
  "permissions": [
    "*://*.daitsuna.com/*",
    "activeTab",
    "contextMenus",
    "cookies",
    "storage",
    "topSites"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0"
}
