{
  "name": "SCP Markdown",
  "version": "1",
  "description": "Generate markdown (.md) files from SCP articles",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": {
      "16": "icon16.png",
      "19": "icon19.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_icon": {
      "16": "icon16.png",
      "19": "icon19.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "persistent": false,
    "scripts": [
      "click-event.js"
    ]
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
