{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Humantic AI",
  "description": "Know anyoneâ€™s personality, personalize each message, close every deal.",
  "version": "2.19",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/bootstrap.min.js",
        "js/jquery-ui.min.js",
        "js/intro.js",
        "js/tooltipster.bundle.min.js",
        "js/script_cs.js",
        "js/moment.min.js",
        "js/underscore.js",
        "js/unslider.js",
        "js/genric.js",
        "js/email_personalisation.js",
        "js/autosize.min.js"
      ],
      "css": [
        "css/bootstrap.min.css",
        "css/jquery-ui.min.css",
        "css/tooltipster.bundle.min.css",
        "css/introjs.css",
        "css/genric.css",
        "css/unslider.css",
        "css/style.css",
        "css/tooltipster-sideTip-shadow.min.css",
        "css/email_personalisation.css",
        "css/font-awesome.min.css"
      ],
      "matches": [
        "*://*.linkedin.com/*",
        "*://*.twitter.com/*"
      ],
      "exclude_matches": [
        "https://scout.frrole.ai/*",
        "http://scout.frrole.ai/*",
        "*://*.greenhouse.io/*",
        "https://app.frrole.ai/*",
        "http://localhost/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/eventPage.js",
      "js/scrapingPage.js",
      "js/fetch.js",
      "js/checkAccess.js",
      "js/htmlData.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://devapi.humantic.ai; object-src 'self'; img-src * 'self' data: https:",
  "permissions": [
    "management",
    "activeTab",
    "tabs",
    "contextMenus",
    "http://localhost/*",
    "storage",
    "https://s7.addthis.com/*",
    "https://api.humantic.ai/*",
    "https://devapi.humantic.ai/*",
    "cookies",
    "https://www.linkedin.com/*"
  ],
  "web_accessible_resources": [
    "images/*",
    "images/*/*",
    "js/*",
    "css/*",
    "css/*/*"
  ]
}
