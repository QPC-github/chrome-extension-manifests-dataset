{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "meJane.com",
  "name": "meJane.com - online films kijken",
  "version": "1.24",
  "description": "meJane.com - online films kijken en downloaden",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": "icon_19.png"
  },
  "permissions": [
    "tabs",
    "http://www.mejane.com/*",
    "https://*.google.com/*"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
