{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "hide-twitter-live",
  "version": "1.0.1",
  "description": "Removes the annoying twitter live block from the page on load.",
  "author": "Harry Parton",
  "content_scripts": [
    {
      "matches": [
        "*://www.twitter.com/*",
        "*://twitter.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
