{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Daniel Granatta has taken over this title.",
  "description": "This extension allows Dani Granatta to take over things for you.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "https://secure.flickr.com/",
    "activeTab",
    "tabs"
  ]
}
