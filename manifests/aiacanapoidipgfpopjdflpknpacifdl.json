{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Refine",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Search engine and NewTab that helps you Refine your searches",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Refine Search"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://refine.coinup.org/favicon.ico",
      "is_default": true,
      "keyword": "Refine Search",
      "name": "Refine Search",
      "search_url": "https://refine.coinup.org/search.php?a=0305&q={searchTerms}"
    },
    "startup_pages": [
      "https://refine.coinup.org/index.php?a=0305"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  }
}
