{
  "name": "BaiduSearch by ilovn",
  "version": "0.1",
  "manifest_version": 2,
  "description": "添加百度搜索到你的右键菜单！适合于默认使用Google的用户临时切换搜索！",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "basic.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "update_url": "http://clients2.google.com/service/update2/crx"
}
