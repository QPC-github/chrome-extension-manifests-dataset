{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "云上实训助手",
  "version": "1.0.2",
  "description": "云上实训，答案表格导出功能，导出为csv文件，在本地可用excel打开，并可转换为xlsx文件。",
  "icons": {
    "16": "img/icon.png",
    "32": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "homepage_url": "https://mydw.xyz",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "表格导出",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "http://*.yunsx.com/*",
    "https://*.yunsx.com/*"
  ],
  "web_accessible_resources": [
    "dist/*",
    "dist/**/*",
    "js/*"
  ]
}
