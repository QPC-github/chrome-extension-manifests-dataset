{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ownpage Tracking Checker",
  "description": "Ownpage Tracking Checker",
  "version": "1.5",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Ownpage Tracking Checker",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
