{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Typoglycemia",
  "version": "0.1",
  "author": "Derek Mayer",
  "description": "Takes the letters in between the first and last letter of a word and scrambles them.",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ]
}
