{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NanoSignage Fullscreen Extension",
  "version": "0.44",
  "manifest_version": 2,
  "description": "NanoSignage Fullscreen Extension",
  "permissions": [
    "tabs",
    "https://*/*"
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "arrow_in.png",
    "default_title": "NanoSignage Fullscreen"
  }
}
