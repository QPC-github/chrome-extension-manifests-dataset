{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "noBook",
  "description": "This extension hides your facebook news feed, while leaving messaging, events, and everything else accessible.",
  "version": "1.2.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/",
        "https://www.facebook.com/*"
      ],
      "css": [
        "facebook.css"
      ]
    }
  ]
}
