{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "cloudsave.html"
  },
  "description": "根据Cloudsave修改，快速分享（发布）图片到微博。目前只支持新浪微博。用户可以在任意的web页面中的图片右键发布到微博。",
  "icons": {
    "128": "icon/128.png",
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "512": "icon/512.png",
    "64": "icon/64.png"
  },
  "name": "快捷图片发微",
  "permissions": [
    "*://*/",
    "tabs",
    "contextMenus",
    "unlimitedStorage",
    "notifications"
  ],
  "version": "1.1.1",
  "manifest_version": 2
}
