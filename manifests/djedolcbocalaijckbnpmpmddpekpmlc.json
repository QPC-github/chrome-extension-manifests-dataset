{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hamilton Lottery",
  "short_name": "Hamilton Lottery",
  "version": "0.0.2",
  "description": "Automatically enters the Hamilton lottery in SF",
  "author": "Rahul Matta",
  "permissions": [
    "background",
    "idle",
    "storage",
    "notifications",
    "https://www.luckyseat.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "config.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.luckyseat.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "fill.js"
      ]
    }
  ]
}
