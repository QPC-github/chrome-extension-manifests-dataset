{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Make Quora Readable Again",
  "description": "Neutralizes annoying parts of the Quora reading experience and lets you read again.",
  "version": "0.0.2",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://*.quora.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": []
}
