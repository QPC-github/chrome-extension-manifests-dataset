{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/cmenu.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/vkstyle_48.png",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/vkstyle.css"
      ],
      "js": [
        "js/vkstyle.js"
      ],
      "matches": [
        "*://vk.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Измените фон и стиль своих персональных страниц ВКонтакте. Плюс удобная кнопка для доступа к своей страничке в ВКонтакте!",
  "icons": {
    "128": "icons/vkstyle_128.png",
    "48": "icons/vkstyle_48.png"
  },
  "manifest_version": 2,
  "homepage_url": "http://vk.com/newvkstyle",
  "name": "VK Style",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.1"
}
