{
  "manifest_version": 2,
  "version": "2.1.12",
  "chrome_url_overrides": {
    "newtab": "pages/index.html"
  },
  "browser_action": {
    "default_icon": "/images/img/icone/128.png",
    "default_title": "Full HD NewTab"
  },
  "description": "Make the most of your display with beautiful wallpapers Avatar and advanced features",
  "icons": {
    "128": "/images/img/icone/128.png"
  },
  "name": "Wallpapers Avatar",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "contextMenus",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "topSites"
  ],
  "short_name": "Full HD NewTab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "pages/index.html"
  ]
}
