{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flickr - Display lens model",
  "description": "Displays the lens model next to camera info.",
  "content_scripts": [
    {
      "matches": [
        "http://www.flickr.com/*"
      ],
      "js": [
        "flickr.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "version": "0.1",
  "manifest_version": 2
}
