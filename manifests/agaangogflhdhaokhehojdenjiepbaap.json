{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Netboard.me",
  "description": "Create own beautiful Web Pages for free! Use this extension to post found content to your page while surfing the Web.",
  "version": "1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://netboard.me/",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
