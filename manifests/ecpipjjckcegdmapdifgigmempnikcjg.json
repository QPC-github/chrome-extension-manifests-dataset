{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Freezetab",
  "version": "1.9.0",
  "icons": {
    "16": "./icons/logo16.png",
    "32": "./icons/logo32.png",
    "48": "./icons/logo48.png",
    "64": "./icons/logo64.png",
    "128": "./icons/logo128.png"
  },
  "description": "A powerful alternative to bookmarks.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "commands": {
    "SaveAll": {
      "suggested_key": {
        "default": "Shift+Alt+A"
      },
      "description": "Save all tabs."
    },
    "SaveCurrent": {
      "suggested_key": {
        "default": "Shift+Alt+C"
      },
      "description": "Save the current tab."
    },
    "SaveRight": {
      "suggested_key": {
        "default": "Shift+Alt+M"
      },
      "description": "Save everything to the right of the current tab."
    },
    "SaveLeft": {
      "suggested_key": {
        "default": "Shift+Alt+N"
      },
      "description": "Save everything to the left of the current tab."
    }
  }
}
