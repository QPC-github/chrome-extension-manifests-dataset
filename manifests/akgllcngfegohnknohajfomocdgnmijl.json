{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Select last Buttons",
  "description": "Click this Button and last radio buttons on the current website will be selected ",
  "version": "0.1.3",
  "browser_action": {
    "default_icon": "three.jpg",
    "default_title": "Randomize!"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  }
}
