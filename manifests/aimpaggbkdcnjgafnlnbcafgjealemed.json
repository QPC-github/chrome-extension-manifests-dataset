{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Are You Stuck?",
  "description": "A reminder to get you un-stuck",
  "version": "0.0.0.2",
  "background": {
    "scripts": [
      "URI.js",
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "notifications",
    "tabs",
    "idle",
    "alarms",
    "storage"
  ],
  "browser_action": {
    "default_title": "GetUnStuck",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "turtle_v4_16.png",
    "48": "turtle_v4_48.png",
    "128": "turtle_v4_128.png"
  },
  "web_accessible_resources": [
    "turtle_v4_128.png",
    "turtle_v4_48.png"
  ]
}
