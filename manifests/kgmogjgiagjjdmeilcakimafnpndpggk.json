{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mCoffee",
  "version": "1.2.5",
  "author": "Thaddaeus Kong",
  "description": "launch bookmarks based on day of the week, day of the month, daily and time range.",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "scripts": [
      "mcoffee.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
