{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No New Members",
  "description": "Removes posts made by new members on the devforum",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "*://devforum.roblox.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://devforum.roblox.com/*"
      ],
      "js": [
        "begoneNewMembers.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
