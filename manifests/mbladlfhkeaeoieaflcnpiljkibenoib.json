{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "graph.png",
    "128": "graph.png"
  },
  "browser_action": {
    "default_icon": "graph.png",
    "default_title": "vk messages stats."
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://vk.com https://use.fontawesome.com/releases/v5.1.0/js/all.js; object-src 'self'",
  "description": "Статистика личных сообщений и визуализация общения в Вконтакте",
  "manifest_version": 2,
  "name": "VK Messages Visual Statistics",
  "short_name": "VK Visual Stats",
  "minimum_chrome_version": "27",
  "permissions": [
    "tabs",
    "*://*.userapi.com/*",
    "*://api.vk.com/*",
    "*://*.vk.me/*",
    "*://*.fontawesome.com/*",
    "*://vk.com/images/*",
    "storage",
    "unlimitedStorage"
  ],
  "version": "0.1.0"
}
