{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SuperSorter",
  "version": "0.5.3",
  "description": "Clean up your bookmark mess! Sort bookmarks, delete duplicates, merge folders, sort automatically, etc.",
  "background": {
    "scripts": [
      "jquery-3.4.1.min.js",
      "ss.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "go-jump-48.png",
    "default_title": "Sort bookmarks"
  },
  "options_page": "options.html",
  "icons": {
    "48": "go-jump-48.png",
    "128": "go-jump-128.png"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "alarms",
    "activeTab"
  ]
}
