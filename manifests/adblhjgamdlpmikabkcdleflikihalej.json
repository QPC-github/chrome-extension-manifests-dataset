{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Compare preços, fretes e teste cupom | Reduza",
  "short_name": "Reduza",
  "description": "Dinheiro na hora, melhor que cashback. Economize na Black Friday comprando em centenas de lojas confiáveis",
  "version": "3.2",
  "icons": {
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/js/contentscript.js"
      ],
      "run_at": "document_start"
    }
  ]
}
