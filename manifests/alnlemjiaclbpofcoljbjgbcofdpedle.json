{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Faker Breaker",
  "description": "Fake news has no business in your timeline. This extension calls out unreliable news sources.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "icons": {
        "128": "logo.png"
      },
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
