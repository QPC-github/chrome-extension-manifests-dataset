{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "another search",
  "description": "使用其他搜索引擎",
  "version": "1.4.0",
  "manifest_version": 2,
  "icons": {
    "128": "icons/icon_128.png",
    "48": "icons/icon_48.png",
    "16": "icons/icon_16.png"
  },
  "browser_action": {
    "default_icon": "icons/24.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
