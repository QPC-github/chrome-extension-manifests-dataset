{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hynt - The Social Wishlist",
  "description": "This extension check product name, price and added to your Hynt wishlist.",
  "version": "2.2.2",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "https://hynt.io/",
    "http://hynt.io/",
    "<all_urls>",
    "tabs"
  ]
}
