{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.2.2",
  "name": "T-Inbox",
  "description": "Receive tweets and organize them just like as an inbox",
  "icons": {
    "16": "icons/t-inbox16.png",
    "48": "icons/t-inbox48.png",
    "128": "icons/t-inbox128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/t-inbox16.png",
      "48": "icons/t-inbox48.png",
      "128": "icons/t-inbox128.png"
    },
    "default_title": "T-Inbox",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "https://t-inbox-api.herokuapp.com/",
    "*://www.local.dev/*"
  ],
  "background": {
    "scripts": [
      "static/js/main.d1fc3321.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://www.local.dev/*"
    ]
  }
}
