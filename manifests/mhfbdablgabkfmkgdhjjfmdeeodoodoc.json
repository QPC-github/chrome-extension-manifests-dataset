{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Fireballed",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Put a fireballed link next to the permalink on daringfireball.net",
  "content_scripts": [
    {
      "matches": [
        "http://daringfireball.net/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://daringfireball.com/*"
  ]
}
