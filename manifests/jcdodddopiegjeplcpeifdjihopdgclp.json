{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Personal Data Multi-Query",
  "icons": {
    "24": "icons/icon24.png",
    "48": "icons/icon48.png"
  },
  "description": "This extension automates queries to various databases",
  "version": "1.1.6",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icons/icon24.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "https://wyszukiwarkaregon.stat.gov.pl/*",
    "http://panoramafirm.pl/*"
  ]
}
