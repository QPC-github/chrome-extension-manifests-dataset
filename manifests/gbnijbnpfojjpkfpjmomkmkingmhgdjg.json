{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "N4G Pro",
  "description": "N4G enhancement suite. Resizable pictures, language filter correction, quickquoting, and more",
  "version": "1.3.1",
  "icons": {
    "16": "n4gIcon16.png",
    "48": "n4gIcon48.png",
    "128": "n4gIcon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://n4g.freeforums.net/*"
      ],
      "js": [
        "blacklist.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/thread/*"
      ],
      "js": [
        "modifyNavButtons.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/conversation/*"
      ],
      "js": [
        "modifyNavButtons.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/thread/*"
      ],
      "js": [
        "quickquote.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/thread/*"
      ],
      "js": [
        "unchainingOurTongues.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/conversation/*"
      ],
      "js": [
        "unchainingOurTongues.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/thread/*"
      ],
      "js": [
        "shrinkn4gpics.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/conversation/*"
      ],
      "js": [
        "shrinkn4gpics.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/thread/*"
      ],
      "js": [
        "replaceGfyLinks.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/conversation/*"
      ],
      "js": [
        "replaceGfyLinks.js"
      ]
    },
    {
      "matches": [
        "*://n4g.freeforums.net/thread/*"
      ],
      "js": [
        "shareNsfw.js"
      ]
    }
  ]
}
