{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LJG Blocker",
  "description": "Block leather jacket guy articles from Broadsheet.ie",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "http://www.broadsheet.ie/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "http://www.broadsheet.ie/*"
  ]
}
