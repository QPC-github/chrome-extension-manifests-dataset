{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2.0",
  "name": "Canary Theme",
  "manifest_version": 2,
  "description": "A pale yellow theme, perfect for Chrome's Canary build. Supports both Aero and non-Aero.",
  "theme": {
    "images": {
      "theme_toolbar": "assets/yellow.jpg",
      "theme_ntp_attribution": "assets/canary.png"
    },
    "colors": {
      "toolbar": [
        247,
        244,
        192
      ]
    },
    "properties": {
      "ntp_background_alignment": "bottom",
      "ntp_background_repeat": "no-repeat",
      "ntp_logo_alternate": 0
    },
    "tints": {
      "buttons": [
        0.13,
        0.47,
        0.45
      ]
    }
  }
}
