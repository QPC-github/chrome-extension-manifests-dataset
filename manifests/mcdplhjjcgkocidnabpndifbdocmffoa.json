{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shop-Reward.de - Cashback",
  "version": "1.0",
  "description": "Sparen Sie bei jedem Einkauf bares Geld",
  "permissions": [
    "<all_urls>",
    "tts",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "logo128.png",
    "default_title": "Shop-Reward.de - Cashback",
    "default_popup": "cashback.html"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "logo32.png",
    "sad.png"
  ],
  "minimum_chrome_version": "14",
  "homepage_url": "http://www.shop-reward.de",
  "icons": {
    "128": "logo128.png",
    "64": "logo64.png",
    "32": "logo32.png"
  },
  "manifest_version": 2
}
