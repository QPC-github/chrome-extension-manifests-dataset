{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark Search",
  "manifest_version": 2,
  "description": "Lets you search your bookmarks right from the URL bar. Press 'm' followed by space, then start typing your bookmark's name.",
  "version": "0.2",
  "omnibox": {
    "keyword": "m"
  },
  "icons": {
    "16": "icon16.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage"
  ],
  "options_page": "options.html"
}
