{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Agille",
  "description": "Search all your documents, files and tasks in one place",
  "commands": {
    "openOverlayZ": {
      "description": "Open Agille",
      "suggested_key": {
        "default": "Alt+Z"
      }
    },
    "openOverlayK": {
      "description": "Open Agille",
      "suggested_key": {
        "default": "Alt+K"
      }
    },
    "openOverlayF": {
      "description": "Open Agille",
      "suggested_key": {
        "default": "Alt+F"
      }
    }
  },
  "browser_action": {
    "default_icon": {
      "32": "img/icon_32.png"
    },
    "default_title": "Open Agille"
  },
  "background": {
    "scripts": [
      "/js/bg.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "https://app.agille.io/*",
      "https://agille.io/*",
      "https://web.agille.io/*"
    ]
  },
  "icons": {
    "128": "/img/icon_128.png",
    "48": "/img/icon_48.png",
    "16": "/img/icon_16.png"
  },
  "manifest_version": 2,
  "key": "",
  "permissions": [
    "tabs",
    "identity",
    "activeTab",
    "notifications",
    "history",
    "storage",
    "notifications",
    "chrome://favicon/"
  ],
  "version": "0.9.2",
  "content_security_policy": "script-src 'self' https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js https://accounts.google.com https://content.googleapis.com/ https://ssl.gstatic.com/ https://apis.google.com/ https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "dashboard.html"
  ]
}
