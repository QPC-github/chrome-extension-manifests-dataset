{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Poemap.live 번역 유틸",
  "description": "poemap.live 한글로 바꾸는 확장프로그램입니다.",
  "version": "1.8",
  "content_scripts": [
    {
      "matches": [
        "https://poemap.live/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "data/*.json"
  ],
  "permissions": [
    "https://poemap.live/*"
  ]
}
