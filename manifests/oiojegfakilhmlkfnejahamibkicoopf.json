{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "DataVault Extension",
  "manifest_version": 2,
  "name": "DataVault",
  "version": "6.3.1.3",
  "icons": {
    "48": "images/DataVault.ico"
  },
  "applications": {
    "gecko": {
      "id": "datavault@ascendo.inc",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/datavault.png",
    "default_popup": "popup.html",
    "default_title": "DataVault",
    "browser_style": true
  },
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
