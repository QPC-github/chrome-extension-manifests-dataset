{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "优度新网址导航",
  "version": "1.1",
  "description": "优度新网址导航，帮助用户设定自己喜欢的网址，并且添加当天农历显示，通过优度新网址导航，您轻松管理您的chrome浏览器，还提供实用工具如公交、地图、万年历，手机、IP查询、火车/机票查询，方便快捷，给您上网简单清新的感觉",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "default_title": "navigation of website",
    "default_icon": "log128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "log16.png",
    "128": "log128.png"
  },
  "options_page": "options.html",
  "content_security_policy": "default-src * 'unsafe-inline'; script-src 'self'; object-src 'self'; frame-src about:",
  "manifest_version": 2
}
