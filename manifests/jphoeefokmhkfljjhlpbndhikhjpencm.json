{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "3.0.96",
  "description": "Smarty.Sale Cash Back",
  "default_locale": "ru",
  "browser_action": {
    "default_icon": {
      "19": "img/icons/icon19.png",
      "38": "img/icons/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icons/icon16.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "web_accessible_resources": [
    "img/*.png",
    "img/*.png",
    "img/*.svg",
    "js/inject*.js",
    "css/inject.css"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.google.com https://*.gstatic.com; object-src 'self' https://*.google.com https://*.gstatic.com",
  "content_scripts": [
    {
      "css": [
        "css/content.css"
      ],
      "js": [
        "libs/jquery.min.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "management",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "homepage_url": "https://smarty.sale"
}
