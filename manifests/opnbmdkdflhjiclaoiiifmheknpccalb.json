{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "8.1.14",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "author": "Notifications for Instagram",
  "default_locale": "en",
  "homepage_url": "https://www.notifications-for-instagram.com/",
  "short_name": "notificationsforinstagram",
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Notifications for Instagram",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://*.instagram.com https://pc.findanalytic.com; object-src 'self'"
}
