{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "重大教务处 Fix",
  "description": "让重大教务处网站可以在 Chrome 下比较正常的浏览",
  "version": "1.0.2",
  "permissions": [
    "<all_urls>",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "js/events.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "web_accessible_resources": [
    "js/jquery-2.0.3.min.js",
    "js/fix_home.js",
    "js/fix_mainframe.js",
    "js/fix_news.js"
  ]
}
