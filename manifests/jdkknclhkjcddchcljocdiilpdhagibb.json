{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Recruiting",
  "version": "0.31",
  "manifest_version": 2,
  "description": "Recruiting",
  "permissions": [
    "contextMenus",
    "http://www.linkedin.com/*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/recruiter/*"
      ],
      "js": [
        "jquery.js",
        "application.js"
      ]
    }
  ]
}
