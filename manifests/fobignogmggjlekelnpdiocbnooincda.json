{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ExplainPlease New Tab",
  "description": "Replace chrome's new tab with a customizable new tab",
  "version": "1.0.0",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon/icon_128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "https://www.explainplease.com/"
  ],
  "content_security_policy": "script-src 'self' https://www.explainplease.com/; object-src 'self'",
  "manifest_version": 2
}
