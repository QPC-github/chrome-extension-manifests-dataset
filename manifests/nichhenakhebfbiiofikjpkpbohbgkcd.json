{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.3",
  "description": "For all those kids and adults who love the inventions and cuteness of Doraemon. Enjoy :-)",
  "name": "Doraemon Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_popup": "popupDoraemon.html",
    "default_title": "Doraemon :-)"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Doraemon.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
