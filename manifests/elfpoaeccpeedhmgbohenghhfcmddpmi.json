{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Helps save and paste text into web page text fields with proper id.",
  "manifest_version": 2,
  "name": "Jirozaur",
  "version": "0.3",
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "128": "icons/128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus"
  ]
}
