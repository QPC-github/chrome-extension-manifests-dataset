{
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-latest.min.js",
        "jquery.blockUI.js",
        "doTimeout.js",
        "jquery.easing.js",
        "mobile01_pic_loader_chrome.js"
      ],
      "matches": [
        "http://*.mobile01.com/*",
        "http://*.play01.com/*",
        "http://*.5i01.com/*",
        "http://*.shopping01.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Auto Load All Images and Video on Mobile01",
  "icons": {
    "128": "icon/m01-icon-128.png",
    "16": "icon/m01-icon-16.png",
    "32": "icon/m01-icon-32.png",
    "48": "icon/m01-icon-48.png"
  },
  "manifest_version": 2,
  "name": "Mobile01 Picture Auto Loader",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.40",
  "web_accessible_resources": [
    "images/*.png"
  ]
}
