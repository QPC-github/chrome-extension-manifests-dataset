{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Shorthand",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Shortcut for going to different sub-reddits.",
  "permissions": [
    "tabs"
  ],
  "default_locale": "en",
  "icons": {
    "32": "32.png",
    "16": "16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "r"
  }
}
