{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "btce plus",
  "description": "More useful BTCe stats",
  "version": "1.1.1",
  "icons": {
    "16": "icon_s.png",
    "48": "icon_m.png",
    "128": "icon_l.png"
  },
  "permissions": [
    "tabs",
    "https://*.btc-e.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "newstats.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "https://*.btc-e.com/*"
      ]
    }
  ]
}
