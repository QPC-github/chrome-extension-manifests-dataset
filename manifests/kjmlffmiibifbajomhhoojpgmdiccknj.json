{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remind me later",
  "version": "0.9",
  "manifest_version": 2,
  "description": "Reminds you a website in a given time",
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "web_accessible_resources": [
    "icon.png"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_title": "Remind me this page",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery-1.11.0.min.js",
      "js/background.js"
    ],
    "persistent": true
  }
}
