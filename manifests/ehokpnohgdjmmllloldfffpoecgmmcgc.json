{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SIS Helper",
  "version": "0.3.15",
  "author": "SIS Authors",
  "description": "Image processor.",
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "<all_urls>",
    "storage",
    "activeTab",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "contextmenu.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.fotolia.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
