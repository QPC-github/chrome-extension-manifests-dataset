{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hyperlink",
  "version": "4.0",
  "description": "Get push notifications when your links get clicked!",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "icons": {
    "128": "logo.png"
  }
}
