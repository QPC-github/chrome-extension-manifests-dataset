{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Photobucket Fix",
  "version": "1.0.1",
  "description": "This extension can, for free and without any setup, fix broken Photobucket images.",
  "icons": {
    "384": "icons/icon384.png"
  },
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.photobucket.com/*"
  ]
}
