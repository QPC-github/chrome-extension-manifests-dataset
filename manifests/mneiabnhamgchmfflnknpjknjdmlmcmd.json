{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bible News Feed",
  "version": "1.0.2",
  "description": "Replace your news feed with bible verses",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery.min.js",
        "bible.js",
        "kill.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
