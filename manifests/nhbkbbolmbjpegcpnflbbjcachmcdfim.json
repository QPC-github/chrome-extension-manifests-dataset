{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FragRadio WebPlayer",
  "description": "Easily access the WebPlayer from anywhere",
  "version": "0.0.0.8",
  "author": "Duncan S",
  "icons": {
    "16": "default.png",
    "48": "default.png",
    "128": "default.png"
  },
  "browser_action": {
    "default_icon": "default.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://fragradio.com*"
  ]
}
