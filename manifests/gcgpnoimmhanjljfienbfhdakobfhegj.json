{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "XUI Assist",
  "version": "1.0",
  "description": "providing powerful shotcuts to Xero Users",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "https://go.xero.com/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "popup.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "XUI Assist",
    "default_icons": {
      "16": "images/xpu16.png",
      "32": "images/xpu32.png",
      "48": "images/xpu48.png",
      "128": "images/xpu128.png"
    }
  },
  "icons": {
    "16": "images/xpu16.png",
    "32": "images/xpu32.png",
    "48": "images/xpu48.png",
    "128": "images/xpu128.png"
  },
  "manifest_version": 2
}
