{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Video Expander",
  "version": "0.0.0.4",
  "manifest_version": 2,
  "description": "Embeds the current YouTube video within the entire Chrome window.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
