{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CritterMod",
  "description": "CritterMod is a mod for Box Critters developed by sked adding multiple extra features and fun/useful commands.",
  "version": "1.4",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "*://*.boxcritters.com/*",
    "webRequestBlocking"
  ]
}
