{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Instant Base64",
  "version": "0.2.0",
  "description": "Instantly converts an image to its Base64 string value and copies it to the clipboard.",
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "manifest_version": 2
}
