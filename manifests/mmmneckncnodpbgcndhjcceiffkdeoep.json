{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "思达国际下单助手",
  "short_name": "思达国际物流下单",
  "description": "思达国际下单助手-物流快递信息一键填报",
  "icons": {
    "128": "logo.png",
    "48": "logo48.png",
    "16": "logo16.png"
  },
  "version": "1.0.0.1",
  "background": {
    "page": "bg.html"
  },
  "author": "晟网信息科技",
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "sweetalert2.min.css"
      ],
      "js": [
        "jquery.js",
        "sweetalert2.min.js",
        "run.js"
      ],
      "matches": [
        "https://buyertrade.taobao.com/*"
      ]
    }
  ],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "downloads",
    "*://*/*",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://yifus.win https://cdn.bootcss.com; object-src 'self';"
}
