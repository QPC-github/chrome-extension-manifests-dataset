{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html",
    "default_title": "Bot Ads - Tìm kiếm đối tượng quảng cáo @ Bot Bán Hàng"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Bot Ads - Tìm kiếm đối tượng quảng cáo @ Bot Bán Hàng",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "Bot Ads @ Bot Bán Hàng",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.0"
}
