{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iCoach - Il Motore di Ricerca sul Coaching",
  "short_name": "iCoach",
  "description": "iCoach - Il Motore di Ricerca sul Coaching. Inserisci una parola chiave, ottieni il significato ed approfondimenti utili.",
  "version": "1.1.1",
  "minimum_chrome_version": "23",
  "offline_enabled": true,
  "browser_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png",
      "128": "128.png"
    },
    "default_title": "iCoach - Il Motore di Ricerca sul Coaching",
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "iCoach"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "<all_urls>"
  ]
}
