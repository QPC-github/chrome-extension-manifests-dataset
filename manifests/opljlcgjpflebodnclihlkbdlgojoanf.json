{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Setuper",
  "version": "1.16",
  "default_locale": "ru",
  "description": "__MSG_extension_description_short__",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "author": "Completo",
  "content_scripts": [
    {
      "matches": [
        "https://guideme.local/*",
        "https://setuper.ru/*",
        "https://profile.setuper.ru/*",
        "https://instagram.com/*",
        "https://facebook.com/*",
        "https://www.facebook.com/*",
        "https://web.facebook.com/*",
        "https://business.facebook.com/*",
        "https://vk.com/*",
        "https://youtube.com/*",
        "https://google.com/analytics/*",
        "https://analytics.google.com/analytics/*",
        "https://amocrm.ru/*",
        "https://bitrix24.ru/*",
        "https://roistat.ru/*",
        "https://metrika.yandex.ru/*",
        "https://app.leeloo.ai/*",
        "https://*.bitrix24.ru/*",
        "https://*.univer.ru/*"
      ],
      "css": [
        "css/app.css"
      ],
      "js": [
        "js/jquery.js",
        "js/jquery-ui.min.js",
        "js/app.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "images/*.*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    },
    "default_title": "Setuper",
    "default_popup": "html/popup.html"
  }
}
