{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ÖsszeFon",
  "description": "Támogass egy jó ügyet, fogadj örökbe egy projektet, gondozd, népszerűsítsd és oszd meg másokkal is!",
  "version": "1.1.0",
  "icons": {
    "128": "assets/media/images/ao_ikon_128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "assets/media/images/ao_ikon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "background",
    "alarms",
    "storage"
  ]
}
