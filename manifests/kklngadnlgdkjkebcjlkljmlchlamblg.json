  "description": "ApplySquare provides graduate school application assistance.",
  "homepage_url": "https://www.applysquare.com",
  "default_locale": "en",
  "icons": {
    "16": "img/a2/icon-16.png",
    "48": "img/a2/icon-48.png",
    "128": "img/a2/icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "https://www.applysquare.com/*"
