{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Connoisseur Media Long Island",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Listen to local Long Island radio stations: WALK 97.5, 103.1 MAX FM, K-98.3, 94.3 The Shark & 1100WHLI!",
  "homepage_url": "http://connoisseurmediali.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "chrome://favicon/",
    "notifications",
    "tabs"
  ]
}
