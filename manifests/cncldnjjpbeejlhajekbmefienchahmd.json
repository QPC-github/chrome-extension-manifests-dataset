{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Product Hunt Upvoter",
  "short_name": "Product Hunt Upvoter",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Upvotes All Products On The Product Hunt Homepage.",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://www.producthunt.com/*",
    "https://producthunt.com/*"
  ]
}
