{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Context Menu Bookmarks",
  "short_name": "Ctx Bookmarks",
  "version": "2.2.0",
  "description": "Use bookmarks and bookmarklets from the context menu.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "activeTab"
  ],
  "manifest_version": 2
}
