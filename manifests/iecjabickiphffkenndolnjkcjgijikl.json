{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Respect Beer",
  "version": "0.2",
  "description": "Hides the social drinking game on Facebook.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery-2.0.2.min.js",
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-2.0.2.min.map"
  ],
  "manifest_version": 2
}
