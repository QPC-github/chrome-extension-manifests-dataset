{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Linking Worlds Quote request",
  "description": "This extension request quotes",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "mail_128x128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_title": "Request A Quote",
    "default_icon": "email_16x16.png"
  },
  "manifest_version": 2
}
