{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Showbie",
  "description": "The paperless classroom made simple",
  "version": "3.4.0",
  "manifest_version": 2,
  "app": {
    "urls": [
      "https://my.showbie.com/"
    ],
    "launch": {
      "web_url": "https://my.showbie.com/"
    }
  },
  "container": [
    "GOOGLE_DRIVE"
  ],
  "permissions": [
    "http://my.showbie.com/*",
    "https://my.showbie.com/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png",
    "250": "icon-250.png"
  }
}
