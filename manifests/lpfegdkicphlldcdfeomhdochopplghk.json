{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "2.2.3",
  "manifest_version": 2,
  "default_locale": "ru",
  "browser_action": {
    "default_icon": {
      "19": "img/38.png",
      "38": "img/38.png"
    },
    "name": "bI",
    "default_title": "Webfonting it!"
  },
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "img/*.svg",
    "img/*.png"
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "applications": {
    "gecko": {
      "id": "pull@fontstorage.com"
    }
  }
}
