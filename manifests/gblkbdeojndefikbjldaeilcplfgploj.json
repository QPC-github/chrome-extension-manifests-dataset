{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BAHSİNE",
  "short_name": "Güncel Bahsine Adresine Giriş Yapın",
  "description": "Bu eklenti ile her zaman bahsine güncel adresine ulaşabilirsiniz.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "bahsine.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  }
}
