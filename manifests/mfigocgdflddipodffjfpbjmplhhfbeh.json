{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PhishBlock",
  "description": "Helps to block phishing attempts through email.",
  "version": "0.0.0.4",
  "manifest_version": 2,
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "jquery-1.11.3.min.js",
        "gmail.js"
      ],
      "css": [
        "gmail.css"
      ]
    }
  ],
  "icons": {
    "48": "icon_48_01.png",
    "128": "icon_128_01.png"
  }
}
