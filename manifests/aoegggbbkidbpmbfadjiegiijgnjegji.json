{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IceCast",
  "version": "1.1.7",
  "description": "Download individual product information, photos, videos and manuals from Icecat in one file.",
  "page_action": {
    "default_icon": {
      "24": "images/icecatICO.png",
      "64": "images/icecatICO.png",
      "128": "images/icecatICO.png"
    },
    "default_title": "FOHelper"
  },
  "web_accessible_resources": [
    "images/*.png",
    "data/promptFileSelectStyle.css",
    "data/*.js"
  ],
  "author": "Martin Grunwald",
  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "icons": {
    "24": "images/icecatICO.png",
    "48": "images/icecatICO.png",
    "128": "images/icecatICO.png"
  },
  "homepage_url": "https://www.icecat.biz",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
