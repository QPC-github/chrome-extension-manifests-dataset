{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ro-Customize",
  "version": "0.2",
  "browser_action": {
    "default_icon": "Images/icon.png"
  },
  "web_accessible_resources": [
    "Images/Logo.png",
    "Images/background.jpg"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.roblox.com/*"
      ],
      "js": [
        "WallPaper.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "Background.js"
    ]
  }
}
