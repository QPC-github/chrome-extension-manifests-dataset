{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Win7 Scrollbars",
  "version": "1.1",
  "description": "Scrollbars that match Windows 7's native appearance.",
  "icons": {
    "48": "scrollbars-48.png",
    "128": "scrollbars-128.png"
  },
  "content_scripts": [
    {
      "css": [
        "scrollbars.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
