{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "static/19.png",
      "38": "static/38.png"
    },
    "default_title": "Open 'Downloads' Folder"
  },
  "description": "Quickly open the 'Downloads' folder on Chrome OS devices right in the browser window.",
  "homepage_url": "https://browsernative.com/chromebook-download-folder-launcher/",
  "icons": {
    "128": "static/128.png",
    "48": "static/48.png"
  },
  "manifest_version": 2,
  "name": "Downloads Folder Launcher",
  "offline_enabled": true,
  "permissions": [
    "downloads"
  ],
  "version": "0.8",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+J"
      }
    }
  }
}
