{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MCM Browse",
  "author": "Dan Ellis 2017",
  "description": "Fast species information, and regex search. ",
  "version": "0.2",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "128ico.png",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "smiles_mined.csv",
    "images/*"
  ],
  "permissions": [
    "tabs"
  ]
}
