{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YourAdvalue",
  "version": "1.0.6",
  "description": "Estimating user value by tapping on RTB",
  "permissions": [
    "storage",
    "cookies",
    "<all_urls>",
    "management",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "libs/jquery-3.2.1.min.js",
      "background.js",
      "adblock_detection.js"
    ]
  },
  "browser_action": {
    "default_title": "Monitor charged prices",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/start_im.png"
  },
  "manifest_version": 2
}
