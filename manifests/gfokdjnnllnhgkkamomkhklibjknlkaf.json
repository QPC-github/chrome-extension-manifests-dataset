{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "daapr",
  "description": "The daapr extension allows users to share and save links directly from the browser (https://daapr.com).",
  "version": "0.0.0.3",
  "icons": {
    "128": "daapr_logo.png"
  },
  "browser_action": {
    "default_icon": "daapr_logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "https://www.daapr.com/",
    "http://www.daapr.com/",
    "https://daapr.com/",
    "http://daapr.com/"
  ]
}
