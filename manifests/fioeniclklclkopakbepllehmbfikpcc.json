{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.3.10",
  "version_name": "1.3.10 stable",
  "minimum_chrome_version": "23",
  "offline_enabled": true,
  "kiosk_enabled": true,
