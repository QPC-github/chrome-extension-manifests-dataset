{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mit DanDial Chrome udvidelse",
  "description": "Med Mit DanDial-udvidelsen er telefonnumre på hjemmesider klikbare og kan ringes op direkte fra siden, når du klikker på dem.",
  "version": "1.0.9",
  "author": "DanDial",
  "browser_action": {
    "default_popup": "login.html",
    "default_title": "Mit DanDial Chrome udvidelse",
    "default_icon": {
      "128": "gfx/dandial/icon128x128.png",
      "16": "gfx/dandial/icon16x16.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "scripts/jquery-3.3.1.min.js",
        "scripts/content.js"
      ],
      "css": [
        "styles/content.css",
        "styles/panel.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://jira.flexfone.dk/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "scripts/jquery-3.3.1.min.js",
      "scripts/background.js",
      "scripts/sha1.js",
      "scripts/rabbit.js",
      "scripts/md5.js"
    ]
  },
  "icons": {
    "16": "gfx/dandial/icon16x16.png",
    "128": "gfx/dandial/icon128x128.png"
  },
  "omnibox": {
    "keyword": "mitdandial"
  },
  "web_accessible_resources": [
    "gfx/*"
  ],
  "permissions": [
    "tabs",
    "https://mitdandial.dk/*",
    "webRequest",
    "notifications",
    "storage",
    "contextMenus",
    "identity"
  ]
}
