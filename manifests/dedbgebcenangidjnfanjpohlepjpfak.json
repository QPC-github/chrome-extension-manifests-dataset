{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Speed Up Browsing",
  "description": "Loads mobile version of sites to improve speed",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "enabled.png"
  },
  "icons": {
    "16": "enabled.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
