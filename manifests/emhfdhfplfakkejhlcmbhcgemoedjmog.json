{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Noobix Uploader",
  "description": "Upload selected image to Noobix",
  "version": "1.0.1",
  "permissions": [
    "http://noobbit.ru/*",
    "contextMenus",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
