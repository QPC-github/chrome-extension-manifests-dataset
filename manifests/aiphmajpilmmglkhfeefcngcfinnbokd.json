{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Propsnap",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "Propsnap",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/*",
    "storage",
    "activeTab",
    "<all_urls>",
    "https://www.realtor.com/*"
  ]
}
