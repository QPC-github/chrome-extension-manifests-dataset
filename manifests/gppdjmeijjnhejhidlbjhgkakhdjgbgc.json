{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Backgrounds & Goals",
  "description": "Beautiful HD pictures from Unsplash and your personal goals show up in each new Chrome tab.",
  "version": "2.2.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "assets/img/icon/icon_16.png",
    "48": "assets/img/icon/icon_48.png",
    "128": "assets/img/icon/icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage"
  ],
  "options_page": "options.html"
}
