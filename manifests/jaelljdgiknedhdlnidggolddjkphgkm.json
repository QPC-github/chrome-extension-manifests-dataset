{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Window Merger",
  "description": "Easily merge tabs from different chrome windows into the current active window.",
  "version": "1.1",
  "browser_action": {
    "default_title": "Window Merger",
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
