{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "pyGet P2P file sharing",
  "short_name": "ipyget",
  "description": "pyGet lets you share files with remote computers and smart-phones without the need of any plugins.",
  "author": "James Fray",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "background"
