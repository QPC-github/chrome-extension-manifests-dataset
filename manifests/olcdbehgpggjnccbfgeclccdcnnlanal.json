{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IIIF Hosting",
  "description": "Open large images directly from Google Drive",
  "version": "1.0.2",
  "manifest_version": 2,
  "app": {
    "urls": [
      "http://www.iiifhosting.com/"
    ],
    "launch": {
      "web_url": "https://admin.iiifhosting.com/"
    }
  },
  "container": [
    "DOMAIN_INSTALLABLE",
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "946611837451",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "image/tiff",
          "image/jpeg",
          "image/x-tiff",
          "application/vnd.google.drive.ext-type.jpeg",
          "application/vnd.google.drive.ext-type.jpg",
          "application/vnd.google.drive.ext-type.tiff",
          "application/vnd.google.drive.ext-type.tif"
        ],
        "href": "http://www.iiifhosting.com/upload-from-drive/",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "128": "iiifhosting-logo-square-128.png"
  }
}
