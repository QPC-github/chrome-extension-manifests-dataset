{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Robin Auto-Grow ",
  "description": "click Grow by default",
  "version": "0.0.0.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/robin/*"
      ],
      "js": [
        "joinrobin.js"
      ]
    }
  ],
  "manifest_version": 2
}
