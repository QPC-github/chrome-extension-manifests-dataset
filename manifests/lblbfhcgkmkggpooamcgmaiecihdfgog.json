{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jobberwocky Vorpal",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "vorpal_logo@16.png",
    "48": "vorpal_logo@48.png",
    "128": "vorpal_logo@128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://localhost:3000/",
    "https://progress.appacademy.io/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Easily record job applications on Jobberwocky, without leaving the page.",
  "version": "0.1.1"
}
