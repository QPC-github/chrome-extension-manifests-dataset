{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "White Screensharing",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Screen sharing for White",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.1",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow screen sharing in White Application.",
  "externally_connectable": {
    "matches": [
      "*://usapp.herewhite.com/*",
      "*://app.herewhite.com/*",
      "*://edge.herewhite.com/*",
      "http://localhost:3000/*"
    ]
  },
  "icons": {
    "128": "128w.png",
    "16": "16w.png",
    "48": "48w.png"
  },
  "browser_action": {
    "default_icon": "16w.png"
  }
}
