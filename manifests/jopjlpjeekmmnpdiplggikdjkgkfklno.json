{
  "name": "IPinator VPN",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "lib/jquery-2.1.1.min.js",
      "lib/chrome-storage.js",
      "storage.js",
      "proxyAdapter.js",
      "dataService.js",
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/disabled.png"
    },
    "default_popup": "popup.html",
    "default_title": "IPinator VPN"
  },
  "default_locale": "en",
  "description": "IP Protection with IPinator",
  "icons": {
    "48": "img/icon.png"
  },
  "manifest_version": 2,
  "short_name": "IPINATOR",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.11"
}
