{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Klay_Printer",
  "description": "Plugin que utiliza la aplicacion PrintNode para mandar impresiones a traves de la nube",
  "version": "1.3.1",
  "icons": {
    "128": "assets/klay_printer.png"
  },
  "browser_action": {
    "default_title": "Imprimir",
    "default_icon": "assets/klay_printer.png",
    "default_popup": "printer.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "storage"
  ],
  "options_page": "options.html"
}
