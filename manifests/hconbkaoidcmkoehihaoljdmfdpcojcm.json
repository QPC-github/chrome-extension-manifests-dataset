{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Everything 2 Table - easy web scraping",
  "short_name": "Everything 2 Table",
  "description": "Copy lots of data from a web page in 2 clicks.",
  "version": "1.0.2",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "60": "logo60.png",
    "128": "logo128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "clipboardRead",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "manifest_version": 2
}
