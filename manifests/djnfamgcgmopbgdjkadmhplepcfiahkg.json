{
  "name": "Wallpaper Spider Man New tab page",
  "short_name": "Spider Man Wallpapers",
  "description": "Wallpaper Spider Man New Tab Is A Cool Extension With Spider Man Wallpapers.",
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.5",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "assets/icons/128.png",
    "32": "assets/icons/32.png",
    "16": "assets/icons/16.png"
  },
  "offline_enabled": true,
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "http://*/",
    "https://*/",
    "chrome://favicon/",
    "history",
    "tabs",
    "storage",
    "activeTab",
    "topSites"
  ]
}
