{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "stil.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_opis__",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "/img/logo128.png",
    "48": "/img/logo48.png",
    "16": "/img/logo16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_V7__",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "version": "2.7.3"
}
