{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Add desktop notifications to Grove.io's web client",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "name": "Grove.io",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "16": "img/icon16.png"
  },
  "homepage_url": "https://grove.io",
  "content_scripts": [
    {
      "matches": [
        "https://grove.io/*",
        "http://grove.io/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.1.0"
}
