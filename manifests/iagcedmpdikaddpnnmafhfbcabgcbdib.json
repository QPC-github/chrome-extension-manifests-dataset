{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Test credit cards",
  "version": "1.1",
  "description": "Autofill with test credit cards",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "hot-reload.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_text": "Stripe test credit cards"
  },
  "icons": {
    "16": "icons/credit-card-16.png",
    "128": "icons/credit-card-128.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage"
  ]
}
