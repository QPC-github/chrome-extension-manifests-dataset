{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YEPF Debug For Chrome",
  "description": "YEPF Debug extension for Google Chrome(@jimmydong)",
  "version": "0.1",
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon/!!.png",
    "default_title": "YEPF Debug",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon/128.png",
    "24": "icon/24.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "64": "icon/64.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "icon/48.png"
  ]
}
