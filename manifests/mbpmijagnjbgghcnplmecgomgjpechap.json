{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "3XSocializer",
  "version": "0.10.3",
  "manifest_version": 2,
  "description": "3XSocializer for Chrome",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "background",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "3XSocializer",
    "default_popup": "popup.html"
  },
  "options_page": "configuration.html"
}
