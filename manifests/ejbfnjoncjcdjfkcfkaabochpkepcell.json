{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Binary Json Base64 URI Tools",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Markit On Demand Tools for Chrome",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "clipboardRead",
    "clipboardWrite"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
