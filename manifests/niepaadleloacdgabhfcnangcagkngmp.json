{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "js/background.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/script.js"
      ],
      "css": [
        "css/style.css"
      ],
      "run_at": "document_end",
      "matches": [
        "https://mail.google.com/*"
      ]
    },
    {
      "css": [
        "css/sidebar.css"
      ],
      "run_at": "document_start",
      "matches": [
        "https://login.mothernode.com/cinfo.php?email=*"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon_20.png",
    "default_title": "Mothernode CRM Gmail Extension Settings",
    "default_popup": "js/popup.html"
  },
  "icons": {
    "128": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "/icons/*"
  ],
  "description": "Mothernode CRM Gmail Extension",
  "manifest_version": 2,
  "name": "Mothernode CRM Gmail Extension",
  "short_name": "Mothernode CRM Gmail Extension",
  "version": "0.0.13"
}
