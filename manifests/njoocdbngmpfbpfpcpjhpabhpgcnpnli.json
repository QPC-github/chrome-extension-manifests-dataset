{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "MyCangKu",
  "description": "Send images/files in the webpages to MyCangKu",
  "version": "0.1",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "default-src 'self'",
  "icons": {
    "16": "MyCangKu16.png",
    "48": "MyCangKu48.png",
    "128": "MyCangKu.png"
  },
  "manifest_version": 2
}
