{
  "content_scripts": [
    {
      "css": [
        "grey.css"
      ],
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ]
    }
  ],
  "description": "Make Youtube less distracting",
  "icons": {
    "128": "icon128.png",
    "16": "icon32.png",
    "32": "icon32.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "GreyTube - distraction free YouTube",
  "permissions": [
    "activeTab"
  ],
  "short_name": "GreyTube",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.32"
}
