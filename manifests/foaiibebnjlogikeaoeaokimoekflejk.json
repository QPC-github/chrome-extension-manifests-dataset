{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vanish Mail - Disposable and Temporary Emails",
  "version": "1.0",
  "description": "Get a temporary email at any time!",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "browser_action": {
    "default_icon": "images/icon16.png"
  },
  "permissions": [
    "activeTab"
  ]
}
