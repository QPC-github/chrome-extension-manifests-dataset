{
  "background": {
    "scripts": [
      "js/tusou.js"
    ]
  },
  "browser_action": {
    "default_icon": "image/icon16.png",
    "default_popup": "html/tusou.html",
    "default_title": "一起火图搜助手"
  },
  "content_scripts": [
    {
      "css": [
        "css/main.css"
      ],
      "js": [
        "js/lib/jquery-2.1.1.min.js",
        "js/content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "default_locale": "zh_CN",
  "description": "一起火图搜助手——图片搜款、标题搜款助你极速找款！",
  "icons": {
    "128": "image/icon128.png",
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "96": "image/icon96.png"
  },
  "manifest_version": 2,
  "name": "一起火图搜助手",
  "permissions": [
    "contextMenus",
    "tabs",
    "management"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1.5"
}
