{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lovely My Tab",
  "description": "새로운 Chrome 탭의 배경화면을 원하는대로 바꿀 수 있습니다. 당신이 관심있는 주제들의 멋진 사진들로 하루를 상쾌하게 유지하세요.",
  "version": "0.4.2",
  "browser_action": {
    "default_icon": "imgs/icons/icon2.png"
  },
  "icons": {
    "128": "imgs/icons/icon2.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
