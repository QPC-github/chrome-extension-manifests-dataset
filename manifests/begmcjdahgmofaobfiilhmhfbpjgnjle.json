{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Linicom Links Skipper",
  "description": "The end to the annoying linicom links",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://linicom.co.il/*"
      ],
      "css": [
        "skipad.css"
      ],
      "js": [
        "skipad.js"
      ]
    }
  ]
}
