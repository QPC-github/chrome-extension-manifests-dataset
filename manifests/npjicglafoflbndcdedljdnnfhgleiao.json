{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Read Later Vue",
  "description": "read web few minutes later",
  "version": "1.39",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "static/ninja-16.png",
      "32": "static/ninja.png",
      "48": "static/ninja.png"
    },
    "default_popup": "hello.html",
    "default_title": "Click here!"
  },
  "icons": {
    "16": "static/ninja-16.png",
    "32": "static/ninja.png",
    "48": "static/ninja.png"
  },
  "background": {
    "scripts": [
      "readLater.js"
    ]
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Open Read Later"
    }
  }
}
