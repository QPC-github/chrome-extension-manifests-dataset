{
  "name": "Kotree Helper",
  "description": "Kotree Helper",
  "version": "1.8.2",
  "icons": {
    "128": "cicon.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "web_accessible_resources": [
    "images/*.png"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "background",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*.amazon.com/*",
    "*://*.kotree.com/*",
    "*://*.nespresso.com/*"
  ],
  "manifest_version": 2
}
