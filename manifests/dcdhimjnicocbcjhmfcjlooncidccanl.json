{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.0.2",
  "name": "Replay It",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {},
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "windows",
    "contentSettings"
  ],
  "web_accessible_resources": [
    "element-picker.html"
  ]
}
