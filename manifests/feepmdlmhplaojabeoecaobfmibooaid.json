{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Texthelp PDF Reader",
  "description": "Experience PDFs in a whole new way with Texthelpâ€™s new PDF Reader.",
  "homepage_url": "https://pdf.texthelp.com",
  "version": "5.1.226",
  "": "",
  "icons": {
    "128": "icon_128x128.png",
    "48": "icon_favicon_48x48.png",
    "16": "icon_favicon_16x16.png"
  },
  "permissions": [
    "https://www.google-analytics.com/",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "gcm",
    "idle",
    "alarms",
    "webNavigation",
    "storage",
    "management",
    "identity",
    "identity.email",
    "activeTab"
