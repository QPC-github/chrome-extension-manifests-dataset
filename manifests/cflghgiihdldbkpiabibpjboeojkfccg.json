{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mideast Tunes",
  "description": "Mideast Tunes is the ultimate platform to discover underground musicians in the Middle East and North Africa.",
  "version": "1.1",
  "icons": {
    "16": "./images/64px@2x.png",
    "48": "./images/64px@2x.png",
    "128": "./images/64px@2x.png"
  },
  "browser_action": {
    "default_icon": "./images/64px@2x.png",
    "default_popup": "index.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://localhost:3000/",
    "https://mideastunes.com/",
    "http://mideastunes.staging.devguru.co/",
    "background"
  ]
}
