{
  "name": "TwitColumns for Twitter",
  "version": "1.2",
  "description": "Swaps the columns on Twitter around.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "include_globs": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "css": [
        "main.css"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "update_url": "http://clients2.google.com/service/update2/crx"
}
