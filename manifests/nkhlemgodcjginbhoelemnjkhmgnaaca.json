{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/contrib/sha256.js",
      "js/contrib/pbkdf2.js",
      "js/utils.js",
      "js/bg/core.js",
      "js/bg/trigger.js",
      "js/bg/api.js",
      "js/bg/download.js",
      "js/bg/hooks.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/smoozed19_off.png",
      "38": "img/smoozed38_off.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_title__"
  },
  "icons": {
    "128": "img/smoozed128.png",
    "16": "img/smoozed16.png",
    "32": "img/smoozed32.png",
    "48": "img/smoozed48.png"
  },
  "web_accessible_resources": [
    "js/contrib/prototype.js",
    "middleware.html",
    "download.html",
    "js/fg/middleware.js",
    "js/fg/plugins/*",
    "img/*"
  ],
  "default_locale": "de",
  "description": "__MSG_description__",
  "options_page": "options.html",
  "manifest_version": 2,
  "name": "smoozed.com",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "downloads",
    "downloads.open",
    "background",
    "tabs"
  ],
  "version": "2.0.8"
}
