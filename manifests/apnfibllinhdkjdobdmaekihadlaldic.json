{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Acclivity Health - Stage",
  "version": "1.2",
  "description": "Shows fast access list.",
  "permissions": [
    "tabs",
    "notifications",
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_icon": "logoBw.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js",
      "lib/jquery.min.js",
      "lib/moment.min.js"
    ],
    "matches": [
      "https://stage.acclivityhealth.com/*"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://stage.acclivityhealth.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
