{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "침착맨의 원피스 헬퍼",
  "description": "원트크를 조금더 쉽게 즐기는 방법.",
  "version": "0.3.0.1",
  "browser_action": {
    "default_icon": "icon-64x64.png",
    "default_popup": "popup.html",
    "default_title": "침착맨의 원피스 헬퍼"
  },
  "content_scripts": [
    {
      "matches": [
        "https://optckr.github.io/*"
      ],
      "js": [
        "app/js/popup.js"
      ]
    }
  ],
  "permissions": [
    "cookies",
    "notifications",
    "https://optctimer.com/*"
  ]
}
