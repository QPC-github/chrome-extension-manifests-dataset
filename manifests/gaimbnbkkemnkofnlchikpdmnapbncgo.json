{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WOL Tools",
  "version": "0.0.15",
  "permissions": [
    "http://wol.jw.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://wol.jw.org/*/*"
      ],
      "js": [
        "jquery.js",
        "jquery.cookie.js",
        "jquery.querystring.js",
        "moment.js",
        "dbr.js",
        "woltools.js"
      ],
      "css": [
        "woltools.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "wrench16.png",
    "48": "wrench48.png",
    "128": "wrench128.png"
  }
}
