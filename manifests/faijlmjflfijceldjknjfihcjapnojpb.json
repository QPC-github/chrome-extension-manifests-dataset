{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescr__",
  "version": "1.2.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png"
  },
  "permissions": [
    "storage",
    "https://www.google.com/*"
  ],
  "incognito": "split",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/options.js",
        "js/myscript.js"
      ],
      "matches": [
        "https://wallet.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "js/inject.js"
  ]
}
