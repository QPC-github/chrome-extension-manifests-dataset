{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "コミュニケーションLive",
  "description": "コミュニケーションLive",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "home.html"
  },
  "permissions": [
    "http://*/",
    "activeTab",
    "notifications",
    "tabs",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  }
}
