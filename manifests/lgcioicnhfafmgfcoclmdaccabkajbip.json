{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js",
      "js/jquery.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png"
    },
    "default_popup": "html/popup.html"
  },
  "chrome_settings_overrides": {
    "homepage": "https://origgon.com/?src=chromehomeexe",
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://cdn.origgon.com/icon/icon.ico",
      "instant_url": "https://origgon.com/search?q={searchTerms}&src=chromeexe&type=chromeinstant",
      "is_default": true,
      "keyword": "origgon",
      "name": "Origgon",
      "search_url": "https://origgon.com/search?q={searchTerms}&src=chromeexe&type=chrome-exe-sbox",
      "short_name": "Origgon",
      "suggest_url": "https://origgon.net/autocomplete.php?q={searchTerms}&type=listchrome"
    }
  },
  "description": "Origgon is world's first of it kind social search engine.",
  "externally_connectable": {
    "matches": [
      "*://*.origgon.com/*"
    ]
  },
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "Origgon for Chrome",
  "omnibox": {
    "keyword": "o"
  },
  "options_page": "html/options.html",
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "identity",
    "management",
    "webNavigation",
    "history"
  ],
  "version": "2018.06.02",
  "web_accessible_resources": [
    "img/16.png",
    "html/action.html"
  ]
}
