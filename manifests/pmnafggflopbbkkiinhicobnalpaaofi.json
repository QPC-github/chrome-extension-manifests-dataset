{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quotab",
  "description": "üòá‚àù Quotab is a simple Google Chrome Extension with the ability to make your mood a bit better.",
  "version": "1.2.3",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "gfx/icon-16x.png",
      "32": "gfx/icon-32x.png",
      "48": "gfx/icon-48x.png",
      "128": "gfx/icon-128x.png"
    }
  },
  "icons": {
    "16": "gfx/icon-16x.png",
    "32": "gfx/icon-32x.png",
    "48": "gfx/icon-48x.png",
    "128": "gfx/icon-128x.png"
  }
}
