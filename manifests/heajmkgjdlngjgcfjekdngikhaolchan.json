{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "view.js"
    ]
  },
  "browser_action": {
    "default_icon": "spymyself.png",
    "default_popup": "index.html"
  },
  "options_page": "index.html",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "crack.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Spy what this append, when you're away",
  "icons": {
    "128": "spymyself.png",
    "16": "spymyself.png",
    "48": "spymyself.png"
  },
  "manifest_version": 2,
  "name": "SpyMySelf",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "webNavigation"
  ],
  "version": "3.8"
}
