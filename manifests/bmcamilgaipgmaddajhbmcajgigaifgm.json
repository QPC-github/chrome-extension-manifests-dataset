{
  "manifest_version": 2,
  "name": "MakeaGIF Tab",
  "version": "1.0.5",
  "description": "Turn your browser into a new GIF every time! Enjoy the power of MakeaGIF TV.",
  "icons": {
    "128": "/img/icon_128.png",
    "16": "/img/icon_16.png",
    "48": "/img/icon_48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "homepage_url": "http://makeagif.com",
  "omnibox": {
    "keyword": "/mag"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "*://makeagif.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": ""
}
