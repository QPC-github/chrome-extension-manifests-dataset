{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OpenedTabs",
  "description": "A simple Tab Manager Extension Shows,Copy,Reloads & Close Opened tabs in the Chrome Window without clicking on going on the tab",
  "version": "1.4",
  "icons": {
    "48": "icon_48.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "clipboardWrite",
    "notifications"
  ],
  "web_accessible_resources": [
    "/Images/icon_48.png"
  ],
  "browser_action": {
    "default_icon": "/Images/favicon.png",
    "default_popup": "tablist.html"
  }
}
