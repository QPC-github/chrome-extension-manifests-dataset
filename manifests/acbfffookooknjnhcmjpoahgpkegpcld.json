{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto-Like for YouTubeâ„¢",
  "version": "1.1",
  "description": "Automatically like your favourite Youtube videos without even having to click!",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "js/autolike-cs.js"
      ]
    }
  ],
  "permissions": [
    "*://*.youtube.com/*"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "js/listen.js"
  ]
}
