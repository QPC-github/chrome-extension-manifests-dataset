{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.1",
  "manifest_version": 2,
  "name": "Cookie Auto Delete",
  "short_name": "cookie-auto-delete",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "homepage_url": "http://mybrowseraddon.com/cookie-auto-delete.html",
  "options_ui": {
    "chrome_style": true,
    "page": "data/options/options.html"
  },
  "description": "Automatically delete cookies when a tab or the browser is closed!",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "cookies",
    "notifications",
    "contentSettings"
  ],
  "browser_action": {
    "default_title": "Cookie Auto Delete",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
