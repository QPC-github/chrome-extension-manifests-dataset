{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Oono Live",
  "version": "1.1.1",
  "description": "Oono Live est un addon chrome permettant de savoir si Oono est actuellement en stream ou non !",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "64": "img/oono_logo.png"
  },
  "web_accessible_resources": [
    "img/oono_logo.png"
  ],
  "permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "live.js"
    ]
  }
}
