{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Discord Servers",
  "short_name": "Discord Servers",
  "version": "1.0.0",
  "description": "Find discord servers to join using the new tab discord server list.",
  "author": "Kohai",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_popup": "discord.html",
    "default_title": "Discord Server List"
  },
  "chrome_url_overrides": {
    "newtab": "discord.html"
  },
  "offline_enabled": false
}
