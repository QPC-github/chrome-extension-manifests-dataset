{
  "author": "KK-PROD",
  "background": {
    "scripts": [
      "libs/jquery.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "64": "img/icon64.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "64": "img/icon64.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "options_page": "index.html",
  "permissions": [
    "storage",
    "*://*.facebook.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5.3",
  "web_accessible_resources": [
    "*"
  ]
}
