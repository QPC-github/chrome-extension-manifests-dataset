{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Get it Done! To-Do-List",
  "version": "0.1",
  "manifest_version": 2,
  "description": "A new way to get more from your day. With add and delete buttons.",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "GiDlogo.png",
    "default_title": "A new way to get more from your day. With add and delete buttons.",
    "default_popup": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "GiDlogo.png"
  }
}
