{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SaveRess by Fearius",
  "version": "2.4",
  "manifest_version": 2,
  "description": "Fragt Handelsgebühren ab und füllt maximale Werte aus.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://last-war.de/*",
    "http://www.last-war.de/*",
    "https://last-war.de/*",
    "https://www.last-war.de/*",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
