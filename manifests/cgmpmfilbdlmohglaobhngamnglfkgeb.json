{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "short_name": "__MSG_short_name__",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "<all_urls>"
  ],
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/active.png"
  },
  "icons": {
    "128": "img/active.png"
  },
  "manifest_version": 2
}
