{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Trend Blocker",
  "version": "1.0.2.2",
  "manifest_version": 2,
  "description": "Blocks curiosity-clicks to Twitter trends.",
  "icons": {
    "14": "icon14.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ],
  "homepage_url": "http://klivk.com/trend-blocker"
}
