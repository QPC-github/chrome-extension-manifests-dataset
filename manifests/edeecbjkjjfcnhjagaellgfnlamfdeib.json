{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ScraperWiki-Gist",
  "version": "1.1",
  "description": "Runs the code in the Gist on the current page",
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "GistRunner"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
