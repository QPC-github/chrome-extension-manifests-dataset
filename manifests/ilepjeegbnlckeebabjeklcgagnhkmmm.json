{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "envMakeup",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Modify the homepage for each Envato Market. Removes the banner and place the section withnew items on top of all other sections.",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "http://themeforest.net/*",
        "http://codecanyon.net/*",
        "http://videohive.net/*",
        "http://audiojungle.net/*",
        "http://graphicriver.net/*",
        "http://photodune.net/*",
        "http://3docean.net/*",
        "http://activeden.net/*"
      ]
    }
  ]
}
