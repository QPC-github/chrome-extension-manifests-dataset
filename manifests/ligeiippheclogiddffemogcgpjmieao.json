  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_title": "__MSG_browserActionTitle__"
  },
  "permissions": [
    "tabs",
    "downloads",
    "https://*/*"
  ]
