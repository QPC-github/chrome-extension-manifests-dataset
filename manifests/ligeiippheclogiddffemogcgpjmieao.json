{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extensionName__",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "__MSG_extensionDescription__",
  "homepage_url": "http://www.localseoguide.com?utm_source=Tools&utm_medium=Chrome%20Extension&utm_campaign=NAP%20Hunter%20Lite",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/options/lib/store.js",
      "background.js"
