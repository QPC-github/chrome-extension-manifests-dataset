{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Focus WebPhone",
  "description": "Focus WebPhone",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Focus WebPhone"
  },
  "background": {
    "scripts": [
      "config.js",
      "setup.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icons/logo.png"
  }
}
