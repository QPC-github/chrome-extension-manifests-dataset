{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Expand Facebook",
  "description": "This extension enfold whole facebook timeline",
  "version": "1.0",
  "icons": {
    "32": "icon_032.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "icon_032.png"
    },
    "default_title": "Expand Facebook",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
