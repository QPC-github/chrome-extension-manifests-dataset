{
  "manifest_version": 2,
  "name": "EZCoupon - Joybuy & Gearbest Price History",
  "version": "1.2.0.5",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "23",
  "description": "Multi Price History Tracker & Daily Coupons",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "short_name": "EZCoupon",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "script.js",
      "Chart.min.js"
    ]
  },
  "web_accessible_resources": [
    "close.png"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery-3.2.1.min.js",
        "content.js",
        "Chart.min.js"
      ],
      "all_frames": false,
      "css": [
        "style.css"
      ],
      "matches": [
        "*://*.gearbest.com/*",
        "*://*.joybuy.com/*",
        "*://*.joybuy.es/*",
        "*://*.jd.ru/*"
      ],
      "run_at": "document_end"
    }
  ]
}
