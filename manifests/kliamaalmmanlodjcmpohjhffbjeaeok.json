{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CreepMode",
  "version": "1.0.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_tile": "CreepMode",
    "default_icon": "creepout.png"
  },
  "permissions": [
    "tabs",
    "*://*.facebook.com/*"
  ],
  "icons": {
    "128": "creeper.png"
  }
}
