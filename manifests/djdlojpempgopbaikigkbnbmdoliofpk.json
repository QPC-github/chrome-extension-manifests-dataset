{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CricNama",
  "description": "CricNama provides Cricket News, Updates, Videos, Reports, Match Analysis, Interviews, Statistics and Records in Urdu and English.",
  "version": "1.1",
  "icons": {
    "16": "cricnama-16.png",
    "32": "cricnama-32.png",
    "64": "cricnama-64.png",
    "128": "cricnama-128.png"
  },
  "app": {
    "urls": [
      "http://cricnama.com/"
    ],
    "launch": {
      "web_url": "http://cricnama.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
