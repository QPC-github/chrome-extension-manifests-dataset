{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "UtopiaEH",
  "background": {
    "page": "popup.html",
    "persistent": false
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "assets/icons/m16.png",
      "20": "assets/icons/m20.png",
      "32": "assets/icons/m32.png",
      "38": "assets/icons/m38.png",
      "40": "assets/icons/m40.png"
    },
    "default_popup": "popup.html",
    "default_title": "Mentor - Protejeazăte de noutăți false."
  },
  "content_scripts": [
    {
      "js": [
        "js/popup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "assets/icons/m16.png",
    "32": "assets/icons/m32.png",
    "48": "assets/icons/m48.png",
    "64": "assets/icons/m64.png",
    "128": "assets/icons/m128.png"
  },
  "description": "Protejeazăte de noutăți false.",
  "manifest_version": 2,
  "homepage_url": "http://mentorul.net/",
  "name": "Mentor",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "version": "2.0.0"
}
