{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AdToast",
  "description": "Youtube AdToast- toasting youtube ads",
  "version": "1.0.1.4",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
        "*://youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "pieblock-min.js"
      ]
    }
  ]
}
