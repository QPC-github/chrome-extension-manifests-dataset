{
  "version": "3.2.5",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "my_page.html"
  },
  "manifest_version": 2,
  "name": "Fortnite Background New Tab",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "idle",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "unlimitedStorage",
    "storage",
    "cookies"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'  https://www.google-analytics.com https://ssl.google-analytics.com https://themeries.club;  object-src 'self'",
  "browser_action": {
    "default_icon": "128-pics.png",
    "default_title": "Fortnite Background New Tab"
  },
  "background": {
    "scripts": [
      "backgrounds.js"
    ]
  },
  "description": "__MSG_locDesc__",
  "icons": {
    "128": "128-pics.png"
  }
}
