{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Price Analysis Lite",
  "description": "Navigate to StubHub Price Analysis",
  "manifest_version": 2,
  "version": "3.0.1",
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Price Analysis Lite"
  },
  "background": {
    "scripts": [
      "src/on_install.js",
      "src/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "24": "icons/icon24.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus"
  ]
}
