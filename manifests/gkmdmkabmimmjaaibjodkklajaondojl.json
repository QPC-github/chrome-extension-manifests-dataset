{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide Jim Jones",
  "description": "Hide Jim Jones comments on Voice of San Diego and replace them with a picture of Hazel the dog.",
  "version": "1.11",
  "content_scripts": [
    {
      "matches": [
        "http://*.voiceofsandiego.org/*"
      ],
      "js": [
        "jquery.js",
        "content_script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
