{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Strategium.ru",
  "description": "Уведомления с форума Strategium",
  "version": "0.20",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon64.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "https://*.strategium.ru/forum/*",
    "alarms",
    "notifications"
  ]
}
