{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ayele",
  "description": "Find and play popular Nigerian sound memes like 'Ayele o' and 'chai' on in an easy-to-use app, built with love by Faffers!",
  "version": "1.0.5",
  "browser_action": {
    "default_icon": "img/icons/drawable-xxxhdpi-icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "icons": {
    "48": "img/icons/drawable-48-icon.png",
    "128": "img/icons/drawable-128-icon.png"
  },
  "permissions": [
    "activeTab",
    "https://maxcdn.bootstrapcdn.com/"
  ]
}
