{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Živě.cz pro Google Chrome™",
  "version": "1.6",
  "description": "Nejnovější články webů Živě.cz přímo v prohlížeči Google Chrome™",
  "icons": {
    "128": "news_icon.png"
  },
  "browser_action": {
    "default_title": "Živě.cz pro Google Chrome™",
    "default_icon": "news_action.png",
    "default_popup": "feed.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://rss.zive.cz/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
