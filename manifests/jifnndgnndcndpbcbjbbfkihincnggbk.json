{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CopyRight",
  "description": "Aids the search of online databases for copy-righted content. ",
  "version": "1.2",
  "icons": {
    "128": "copyright128.png",
    "48": "copyright48.png",
    "16": "copyright16.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ]
}
