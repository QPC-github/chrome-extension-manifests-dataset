{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HTML Validator",
  "description": "An extension to validate the webpage's html using a validation service",
  "manifest_version": 2,
  "version": "0.0.0.1",
  "permissions": [
    "<all_urls>",
    "activeTab"
  ],
  "icons": {
    "64": "icon48.png",
    "128": "icon96.png"
  },
  "browser_action": {
    "default_title": "Validate this page's HTML",
    "default_popup": "popup.html"
  }
}
