{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "APOD",
  "version": "2.1",
  "description": "The famous APOD : Astronomy Picture Of the Day",
  "permissions": [
    "notifications"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "apod16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "apod16.png",
    "48": "apod48.png",
    "128": "apod128.png"
  }
}
