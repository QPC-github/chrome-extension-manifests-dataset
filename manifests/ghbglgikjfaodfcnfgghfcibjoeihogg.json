{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Newsfeed Notify",
  "version": "0.6",
  "description": "Notifies you when something happens on your SharePoint 2013 newsfeed.",
  "icons": {
    "16": "news-16.png",
    "128": "news-128.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery-1.11.0.min.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "commands": {
    "launch-feed": {
      "suggested_key": {
        "default": "Ctrl+Shift+5"
      },
      "description": "Launch Newsfeed",
      "global": true
    }
  }
}
