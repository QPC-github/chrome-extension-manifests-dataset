{
  "name": "CNRTL - CNRTL.fr",
  "description": "CNRTL/ATILF Portail - Trésor de la Langue Française, dictionnaires, synonymes, antonymes, lexiques, vocabulaire, ...",
  "icons": {
    "128": "logo_CNRTL_128.png",
    "16": "logo_CNRTL_16.png",
    "32": "logo_CNRTL_32.png",
    "64": "logo_CNRTL_64.png"
  },
  "version": "0.2.3",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "word2cnrtl.js"
    ]
  },
  "options_page": "option.html",
  "web_accessible_resources": [
    "options.js"
  ],
  "browser_action": {
    "default_icon": "logo_CNRTL.png",
    "default_popup": "popup.html"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2
}
