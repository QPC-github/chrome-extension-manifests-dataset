{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab Plus",
  "description": "A Custom New Tab Page",
  "version": "1.0.3",
  "icons": {
    "1080": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "file:///*"
  ],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  }
}
