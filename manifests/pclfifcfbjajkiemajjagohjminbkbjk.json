{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "defailt_title": "SSLight",
    "default_icon": "images/icons/icon19.png",
    "default_popup": "/popup/popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "stylesheet.css"
      ],
      "js": [
        "SSLight.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Insecure login alert and auto redirect to HTTPS. (This is the new version published with our new account.)",
  "icons": {
    "128": "images/icons/icon128.png",
    "48": "images/icons/icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "20",
  "name": "SSLight",
  "offline_enabled": false,
  "options_page": "/options/options.html",
  "permissions": [
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.00",
