{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Islamway Ramadan | طريق الإسلام",
  "short_name": "Islamway",
  "description": "أضف قيمة جديدة ليومك بهذه الإضافة الرائعة ذات التصميم الجذاب، بها عدّاد رمضان، مواقيت الصلاة، البحث في موقع طريق الإسلام، وغيرها..",
  "version": "1.3.5",
  "permissions": [
    "notifications",
    "storage",
    "https://*.islamway.net/*",
    "http://*.islamway.net/*"
  ],
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "islamway.html"
  },
  "browser_action": {
    "default_icon": "assets/img/icons/icon.png"
  },
  "icons": {
    "16": "assets/img/icons/icon16.png",
    "32": "assets/img/icons/icon32.png",
    "48": "assets/img/icons/icon48.png",
    "64": "assets/img/icons/icon64.png",
    "128": "assets/img/icons/icon128.png"
  },
  "manifest_version": 2
}
