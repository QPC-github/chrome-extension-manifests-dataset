{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Collaboratecenter.com Desktop Streamer",
  "description": "Collaboratecenter.com Extension will enable you to share your screen with your colleagues in a Collaboratecenter.com Session.",
  "version": "0.1.7",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "tabeeb_16.png",
    "48": "tabeeb_48.png",
    "128": "tabeeb_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.collaborate.center/*",
      "*://*.collaboratecenter.com/*",
      "*://192.168.13.141/*"
    ]
  }
}
