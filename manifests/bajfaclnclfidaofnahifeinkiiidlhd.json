{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Информер KAROS",
  "version": "0.14",
  "manifest_version": 2,
  "description": "Расширение позволит не пропустить осаду замков, оценить курс на GXP аукционе, узнать статус серверов и последние новости.",
  "permissions": [
    "tabs",
    "http://*.gamexp.ru/*"
  ],
  "icons": {
    "128": "icon-128.png",
    "48": "icon-128.png",
    "16": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "Информер KAROS",
    "default_popup": "popup.html"
  }
}
