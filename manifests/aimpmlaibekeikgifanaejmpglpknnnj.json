{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "aguse popup tool",
  "version": "1.1.1",
  "description": "ウェブサイトの安全性を調査します。",
  "browser_action": {
    "default_icon": "images/b32.png",
    "popup": "config.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "images/b16.png",
    "32": "images/b32.png",
    "48": "images/b48.png",
    "128": "images/b128.png"
  },
  "background": "background.html"
}
