{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dom Tree and CSS Playground",
  "version": "5.6.8",
  "description": "For Web designers, developers, web instructors. On live web pages, inspect, edit, and test hundreds of [new] CSS styles per second.",
  "homepage_url": "http://www.sugarscript.com/",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "icons/pine-tree-32.png",
    "default_title": "Dom Tree CSS Picker"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "icons/pine-tree-32.png"
  },
  "web_accessible_resources": [
    "icons/*.png",
    "images/*.gif",
    "images/*.jpg",
    "images/*.png"
  ]
}
