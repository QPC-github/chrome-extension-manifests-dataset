{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tech Madness",
  "description": "Buried in browser tabs? Auto-mute new tabs! Mute all tabs fast! Prevent annoying experiences from distracting you!",
  "version": "0.3",
  "version_name": "initial-fix1",
  "icons": {
    "128": "images/logo128.png",
    "48": "images/logo48.png"
  },
  "incognito": "spanning",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "images/logo48.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
