{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "All I Wish Is",
  "description": "Collect and share you wishes with your friends and families.",
  "version": "1.1",
  "icons": {
    "16": "16aiwi.png",
    "48": "48aiwi.png",
    "128": "128aiwi.png"
  },
  "browser_action": {
    "default_icon": "16aiwi.png",
    "default_popup": "popup.html",
    "default_title": "Wish this!"
  },
  "permissions": [
    "tabs",
    "cookies",
    "activeTab",
    "http://www.alliwishis.com/",
    "http://*/*",
    "https://*/*"
  ]
}
