{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aliens",
  "description": "Aliens are attacking you! Run! Shoot! Dodge!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "Icon.png",
    "default_popup": "Home.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "http://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
