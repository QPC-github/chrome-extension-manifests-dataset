{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fake News to Propaganda",
  "description": "Changes any instance of the phrase 'fake news' to 'propaganda'",
  "version": "1.0",
  "icons": {
    "128": "propaganda-512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "myScript.js"
      ]
    }
  ]
}
