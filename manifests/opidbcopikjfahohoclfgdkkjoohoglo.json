{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ISM GO Screen Share",
  "short-name": "Ism Screen",
  "version": "2.0.6.4",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.intelligentsm.net/*",
      "*://*.crimecoms.org/*",
      "*://*.9blue3.org/*",
      "*://*.bullittlink.org/*",
      "*://*.danalink.org/*",
      "*://*.dpdops.us/*",
      "*://*.doravillepd.us/*",
      "*://*.opnet6.net/*",
      "*://*.ismfirst.net/*",
      "*://*.ismunited.net/*",
      "*://*.aiexcellence.net/*",
      "*://*/*",
      "<all_urls>"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
