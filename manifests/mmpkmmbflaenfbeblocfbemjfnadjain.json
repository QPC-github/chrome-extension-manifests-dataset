{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Header Headbutt",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Removes position:fixed from site headers/navbars",
  "browser_action": {
    "default_icon": "icon39.png",
    "default_title": "Header Headbutt"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "128": "icon.png"
  }
}
