{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Billify",
  "version": "1.0.0",
  "description": "The perfect extension for Bill.",
  "author": "Jeremy T.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_title": "Nothing Suspicious."
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frams": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/bill.PNG"
  ],
  "manifest_version": 2
}
