{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "News For You",
  "short_name": "Personalized News",
  "version": "1.2.9",
  "manifest_version": 2,
  "description": "불특정 다수에게 제공되는 뉴스가 아닌 온전히 개인을 위한 최신 뉴스 기사 제공 서비스입니다.",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "webRequest",
    "storage",
    "contextMenus"
  ],
  "icons": {
    "16": "img/version_2_0_0/icon16.png",
    "48": "img/version_2_0_0/icon48.png",
    "128": "img/version_2_0_0/icon128.png"
  },
  "background": {
    "scripts": [
      "lib/mouse.js"
    ]
  },
  "author": "CtwoD.lab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'"
}
