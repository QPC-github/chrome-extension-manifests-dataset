{
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    },
    "default_popup": "popup/index.html",
    "default_title": "esyshopy"
  },
  "content_scripts": [
    {
      "css": [
        "assets/bootstrap.min.css",
        "assets/animate.css",
        "assets/style.css"
      ],
      "js": [
        "assets/jquery-3.3.1.min.js",
        "assets/bootstrap.min.js",
        "assets/main.js"
      ],
      "matches": [
        "*://*.aliexpress.com/item/*",
        "*://*.aliexpress.com/*/productDetail.htm*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://esyshopy.com/; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "https://esyshopy.com/"
  ],
  "short_name": "Extension for https://esyshopy.com/",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.6"
}
