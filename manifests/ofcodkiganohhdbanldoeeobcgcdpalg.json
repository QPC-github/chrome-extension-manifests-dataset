{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vídeos PeperaioHardwareBR no Youtube",
  "short_name": "PeperaioHardwareBR",
  "browser_action": {
    "default_icon": {
      "19": "images/19.png",
      "32": "images/32.png"
    },
    "default_popup": "popup.html"
  },
  "description": "Instale a Extensão no seu navegador e seja avisado sobre novos vídeos do canal PeperaioHardwareBR de forma elegante e discreta.",
  "version": "1.7",
  "icons": {
    "19": "images/19.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "play.png"
  ]
}
