{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mini-Campfire",
  "manifest_version": 2,
  "version": "1.22",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.campfirenow.com/*"
      ],
      "css": [
        "miniCF.css"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
