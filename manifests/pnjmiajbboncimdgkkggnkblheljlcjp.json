{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Price saver",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Price saving during online shopping",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}
