{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PrintHand",
  "short_name": "PrintHand",
  "description": "__MSG_application_description__",
  "manifest_version": 2,
  "minimum_chrome_version": "44",
  "version": "1.2.1",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon_16x16.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  },
  "permissions": [
    "printerProvider",
    "tabs",
    "cookies",
    "http://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
