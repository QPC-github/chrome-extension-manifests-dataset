{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Velocity",
  "description": "A New Tab to get you started and keep you informed",
  "version": "1.2.4.4",
  "icons": {
    "16": "icons/v_icon_16.png",
    "48": "icons/v_icon_48.png",
    "128": "icons/v_icon_128.png"
  },
  "permissions": [
    "alarms",
    "activeTab",
    "tabs",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "geolocation",
    "history",
    "system.cpu",
    "topSites",
    "https://dev.frostmournemc.com/Velocity/api/api_dev.php",
    "https://besticon-demo.herokuapp.com/allicons.json",
    "http://www.google.com/s2/",
    "https://suggestqueries.google.com/complete/search",
    "https://www.reddit.com/r/worldnews/.rss",
    "*://images.unsplash.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://suggestqueries.google.com; object-src 'self'"
}
