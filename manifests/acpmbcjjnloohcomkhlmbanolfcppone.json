{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Uncheck the button",
  "version": "0.4",
  "description": "Allows to uncheck any radio button of a form.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
