{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Skyscanner price calendar",
  "version": "1.03",
  "manifest_version": 2,
  "description": "Create a flexible dates calendar.",
  "permissions": [
    "tabs",
    "*://*.skyscanner.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*.skyscanner.com/*",
        "*://*.skyscanner.com.ua/*",
        "*://*.skyscanner.net/*",
        "*://*.skyscanner.de/*",
        "*://*.skyscanner.ru/*",
        "*://*.skyscanner.ca/*",
        "*://*.skyscanner.com.br/*",
        "*://*.skyscanner.com.au/*",
        "*://*.skyscanner.co.in/*",
        "*://*.skyscanner.fr/*"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "js/background.js"
      ]
    }
  ]
}
