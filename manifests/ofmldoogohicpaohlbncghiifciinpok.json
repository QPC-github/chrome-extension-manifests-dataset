{
  "author": "Narendran",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/inject.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "This extension forces links in a page to open in new tab.",
  "homepage_url": "http://twitter.com/narens",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Open link in new tab bug fix",
  "options_page": "options/index.html",
  "permissions": [
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.7"
}
