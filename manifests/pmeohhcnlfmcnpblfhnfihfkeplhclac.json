{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mStickies",
  "minimum_chrome_version": "23",
  "description": "Locally stored sticky notes",
  "manifest_version": 2,
  "version": "1",
  "offline_enabled": true,
  "icons": {
    "128": "128.png",
    "16": "16.png"
  },
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "unlimitedStorage"
  ]
}
