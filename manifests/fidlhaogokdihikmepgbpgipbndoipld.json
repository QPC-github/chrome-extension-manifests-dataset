{
  "icons": {
    "128": "sloth_128px.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "history",
    "tabs",
    "topSites"
  ],
  "background": {
    "scripts": [
      "back_js.js"
    ]
  },
  "browser_action": {
    "default_icon": "sloth_128px.png",
    "default_title": "Sloth - New Tab in HD",
    "default_popup": "newtab_page.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab_page.html"
  },
  "manifest_version": 2,
  "name": "__MSG_headTitle__",
  "version": "2.0",
  "default_locale": "en",
  "description": "25 lazy and lovely sloth images in HD for Chrome new tab."
}
