{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/jquery.clickecho.js",
        "ripple.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "description": "Give Chrome that touch screen ripple effect!",
  "icons": {
    "48": "48x48.png"
  },
  "manifest_version": 2,
  "name": "RippleMe",
  "permissions": [
    "http://*/*",
    "tabs"
  ],
  "short_name": "RippleMe",
  "version": "1.0.1"
}
