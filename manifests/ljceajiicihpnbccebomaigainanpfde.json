{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Track Attachments",
  "version": "9",
  "description": "We help you tracking your attachments. Send a file and get a notification when itÂ´s downloaded.",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "http://mail.google.com/*"
      ],
      "js": [
        "lib/bootstrap.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/",
    "storage"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "web_accessible_resources": [
    "lib/lab.js",
    "lib/bootstrap.js",
    "lib/gmailr.js",
    "lib/init.js",
    "lib/jquery.1.4.2.js",
    "lib/jquery.ba-bbq.js",
    "main.css",
    "main.js",
    "logo_docxpresso.png",
    "attach.png",
    "attach-btn.png",
    "cancel.png",
    "email.png",
    "submit.png",
    "upload.png",
    "search.png",
    "next.png",
    "back.png",
    "document-icon.png"
  ]
}
