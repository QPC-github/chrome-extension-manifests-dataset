{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EC Site",
  "version": "1.1",
  "description": "Extension for the EC",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/ec16.png",
      "32": "images/ec32.png",
      "48": "images/ec48.png",
      "128": "images/ec128.png"
    }
  },
  "icons": {
    "16": "images/ec16.png",
    "32": "images/ec32.png",
    "48": "images/ec48.png",
    "128": "images/ec128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
