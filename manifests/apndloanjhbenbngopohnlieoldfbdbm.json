{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wowpedia Editor's Toolkit",
  "short_name": "wet",
  "version": "2.0.9",
  "description": "Converts Wowhead pages for quests, missions and NPCs into Wowpedia wiki markup.",
  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png",
    "128": "icons/logo-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "clipboardWrite",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "browser_style": true,
    "default_icon": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png",
      "128": "icons/icon-128.png"
    },
    "default_title": "Wowpedia Editor's Toolkit: Parse current page"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{22caf078-bc44-41ae-9f12-2c4e08a074ac}"
    }
  }
}
