{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notey Notepad",
  "description": "Take Quick Notes",
  "version": "1.0",
  "icons": {
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
