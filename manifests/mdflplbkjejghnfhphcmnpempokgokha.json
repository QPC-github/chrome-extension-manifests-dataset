{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Adwords Blocker",
  "version": "1.8",
  "description": "Adwords Blocker blocks adwords directly when you are searching for information on Google.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Adwords Blocker",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "unlimitedStorage",
    "browsingData",
    "contentSettings",
    "cookies"
  ],
  "manifest_version": 2
}
