{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "alurie",
  "shortname": "alurie",
  "version": "2.5.2.0",
  "description": "Aplicação para automatizar cadastros, quetionarios ou integrar seu whatsweb atravez de um painel ou API",
  "permissions": [
    "storage"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "image/128_alurie.png"
    }
  },
  "icons": {
    "128": "image/128_alurie.png"
  },
  "content_scripts": [
    {
      "js": [
        "app.ab9a07b16921bb88bbbff8145f2ae0bc.js"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "app.ab9a07b16921bb88bbbff8145f2ae0bc_t.js"
  ],
  "manifest_version": 2
}
