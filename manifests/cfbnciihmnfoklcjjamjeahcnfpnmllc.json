{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Currenter",
  "description": "Convert money directly into your browser.",
  "default_locale": "en_US",
  "version": "1.8",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Button"
  },
  "author": "v1lmar",
  "homepage_url": "http://v1lmar.github.io/currenter",
  "permissions": [
    "storage",
    "http://*.free.currencyconverterapi.com/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
