{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GiveBack",
  "description": "We made “GiveBack” as a way for people to (pun intended) give back to our supported non-profit organisations.",
  "version": "2.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "all_frames": false
    }
  ]
}
