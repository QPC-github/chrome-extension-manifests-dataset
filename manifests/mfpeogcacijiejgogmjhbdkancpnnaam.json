{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bypass Quora Login screens",
  "version": "0.0.0.1",
  "description": "Bypass Quora Login screens",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "manifest_version": 2
}
