{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Pesquisar Domínios",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Extensão para pesquisar informações sobre dominios. (Whois, DNS, Pagerank e etc.)",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Pesquisar Dominios",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
