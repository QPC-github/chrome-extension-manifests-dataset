{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mybuddy plugin-NewDemo",
  "author": "Gaurav Srivastava",
  "version": "3.9.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Plugin helps MyBuddy  to interact user more effectively!",
  "homepage_url": "https://ec2-52-207-2-4.compute-1.amazonaws.com/*",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "matches": [
        "https://ec2-52-207-2-4.compute-1.amazonaws.com/*"
      ]
    }
  ],
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
