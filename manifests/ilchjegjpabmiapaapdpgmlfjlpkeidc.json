{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "webCap🧢　 アスペクト比を維持して、かんたんスクリーンショット",
  "short_name": "webCap🧢",
  "version": "0.2",
  "description": "アスペクト比を維持したまま、かんたんにスクリーンショットを取ることができます。This extension will provide a capture image of the active tab.",
  "icons": {
    "64": "images/icons/x64.png",
    "128": "images/icons/x128.png",
    "512": "images/icons/x512.png"
  },
  "browser_action": {
    "default_title": "📷 Capture this website! 📷",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}
