{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZhihuPlus Beta",
  "version": "1.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "知乎问答模块简化插件。",
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "assets/zhihu.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "assets/popup.html",
    "name": "Click to change the icon's color"
  },
  "offline_enabled": true,
  "permissions": [
    "contextMenus",
    "storage",
    "<all_urls>"
  ]
}
