{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Foros del web: Panel de Control",
  "version": "0.6.2",
  "manifest_version": 2,
  "description": "Acceso directo al panel de control de Foros del web",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Foros del web",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "icon48.png"
  ],
  "permissions": [
    "tabs",
    "notifications",
    "http://www.forosdelweb.com/"
  ],
  "optional_permissions": [
    "background"
  ]
}
