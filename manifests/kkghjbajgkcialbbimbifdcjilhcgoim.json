{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "offline_enabled": true,
  "name": "Sketchpad 3.5",
  "short_name": "Sketchpad",
  "version": "3.5.2.23",
  "description": "Draw, Create, Share!",
  "icons": {
    "16": "media/ux/favicon/16.png",
    "48": "media/ux/favicon/48.png",
    "128": "media/ux/favicon/128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "ChromeBackground.js"
      ]
    }
  },
  "optional_permissions": [
    "videoCapture"
  ],
  "permissions": [
    "https://sketch.io/",
    "https://apis.google.com/",
    "https://www.google-analytics.com/",
    "identity",
    "storage",
    "fileSystem",
    "fileSystem.write",
    "fullscreen",
    "unlimitedStorage"
  ]
}
