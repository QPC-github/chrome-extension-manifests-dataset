{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FUCLA!",
  "version": "1.3",
  "description": "Replace UCLA with FUCLA",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon-fucla16.png",
    "48": "images/icon-fucla48.png",
    "128": "images/icon-fucla128.png"
  }
}
