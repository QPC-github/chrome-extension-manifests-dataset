{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Excavator",
  "short_name": "Excavator",
  "description": "Excavator tool for Diggernaut.com service",
  "version": "1.0.11",
  "minimum_chrome_version": "45",
  "icons": {
    "16": "img/icon16.png",
    "19": "img/icon19.png",
    "32": "img/icon32.png",
    "38": "img/icon38.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "webview",
    "system.display",
    "http://*/",
    "https://*/"
  ],
  "app": {
    "background": {
      "scripts": [
        "js/jquery.js",
        "js/ace/ace.js",
        "js/ace/theme-monokai.js",
        "js/ace/snippets/yaml.js",
        "background.js",
        "js/js-yaml.js",
        "js/bootstrap.min.js"
      ]
    }
  }
}
