{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shop 4 Less",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Compare other major sites and buy at right price",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon 48.png",
    "128": "icon 128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}
