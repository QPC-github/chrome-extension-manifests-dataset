{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free Mobile Auto Login",
  "version": "1.2",
  "description": "Insert automatiquement votre identifiant Free mobile sur la page d'identification de votre espace abonn√©.",
  "icons": {
    "128": "freemobileautologin-128x128.png",
    "48": "freemobileautologin-48x48.png",
    "16": "freemobileautologin-16x16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mobile.free.fr/moncompte/*"
      ],
      "js": [
        "freemobileautologin.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
