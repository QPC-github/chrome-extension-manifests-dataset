{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter Square Icons",
  "description": "Revert the latest stupid Twitter change",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
