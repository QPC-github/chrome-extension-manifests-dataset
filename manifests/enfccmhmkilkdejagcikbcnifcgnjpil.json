{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Homepage Quick Search Engine",
  "version": "1.1",
  "manifest_version": 2,
  "default_locale": "en",
  "author": "tabssearch",
  "description": "__MSG_appDesc__",
  "browser_action": {
    "default_title": "Homepage Quick Search Engine"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "homepage": "https://tabssearch.com",
    "search_provider": {
      "name": "Search...",
      "keyword": "tabssearch",
      "favicon_url": "https://tabssearch.com/favicon.ico",
      "search_url": "https://tabssearch.com/web/search?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "https://tabssearch.com"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
