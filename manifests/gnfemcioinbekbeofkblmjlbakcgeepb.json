{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HDzal.net Checker",
  "description": "Отображение обновлений фильмов и сериалов сайта HDzal.net",
  "version": "2.0",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "HDzal.net Checker",
    "default_icon": "img/ico_panel.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/16_activ.png",
    "48": "img/48_activ.png",
    "128": "img/128_activ.png"
  },
  "permissions": [
    "idle",
    "http://hdzal.net/*"
  ]
}
