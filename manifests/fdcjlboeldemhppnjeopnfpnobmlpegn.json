{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Embed Screen Jump Code for YouTubeâ„¢",
  "version": "0.1",
  "manifest_version": 2,
  "description": "You can add start point to YouTube embed code",
  "icons": {
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "author": "naoyashiga",
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch?v=*"
      ],
      "js": [
        "js/jquery.js",
        "js/contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_icon": "icon48.png",
    "default_title": "Embed Screen Jump For YouTube",
    "default_popup": "popup.html"
  }
}
