{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/19x19-icon.png",
      "38": "images/38x38-icon.png"
    },
    "default_title": "Amulyam - Beautify New Tab",
    "default_popup": "amupopup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "description": "Beautify Chrome's new tab page with awesome background images. And, get free recharge quickly on amulyam.",
  "icons": {
    "38": "images/38x38-icon.png"
  },
  "manifest_version": 2,
  "name": "Amulyam - Beautify New Tab",
  "short_name": "Beautify New Tab",
  "options_page": "amupopup.html",
  "permissions": [
    "tabs"
  ],
  "version": "1.0.6"
}
