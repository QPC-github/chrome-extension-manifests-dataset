{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Madhup",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://olehc.com/*"
      ]
    }
  ],
  "description": "Chrome extension for https://olehc.com/. It is used for sharing your screen with others.",
  "homepage_url": "https://olehc.com/",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "OpenTok Screen Sharing for olehc.com",
  "permissions": [
    "desktopCapture"
  ],
  "version": "1.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
