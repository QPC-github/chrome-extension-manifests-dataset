{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Offline Dictionary",
  "description": "An offline dictionary based on Wiktionary.",
  "version": "1.0.2",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "dictionary.html"
    }
  },
  "minimum_chrome_version": "9",
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "permissions": [
    "unlimitedStorage"
  ]
}
