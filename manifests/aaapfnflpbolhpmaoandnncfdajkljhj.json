{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Periodic Table of Elements",
  "version": "1.1.14",
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "https://*/*",
    "cookies",
    "tabs"
  ],
  "icons": {
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "cs/table.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "js": [
        "cs/main.js"
      ],
      "matches": [
        "https://*/baptism.html"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/*.js"
  ],
  "description": "Periodic Table of Elements",
  "browser_action": {
    "default_icon": {
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  }
}
