{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SAP Zenhub name instead of UserId",
  "description": "Display full name instead of UserId for SAP Zenhub instance",
  "version": "0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "index.bundle.js"
      ],
      "matches": [
        "https://zenhub.mo.sap.corp/app/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
