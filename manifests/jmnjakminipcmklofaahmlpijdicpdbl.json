{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit anonymized",
  "version": "1.0.1",
  "description": "Need to read reddit discretly ? This extension allows you to read some AskReddit refined.",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "logo_128.png",
    "48": "logo_48.png",
    "16": "logo_16.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
