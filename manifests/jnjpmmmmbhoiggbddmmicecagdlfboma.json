{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "RPlurkSmiley.js"
      ],
      "matches": [
        "http://www.plurk.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "support more smileys for plurk.",
  "format_version": 1,
  "manifest_version": 2,
  "icons": {
    "128": "icon/128.png",
    "48": "icon/48.png"
  },
  "name": "RPlurkSmileys",
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "tabs",
    "background"
  ],
  "background": {
    "scripts": [
      "options.js"
    ]
  },
  "version": "3.6"
}
