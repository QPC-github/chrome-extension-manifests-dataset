{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo16.png",
    "default_title": "brule"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "bmath-1.0.0.js",
        "content_scripts.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Browser rule, Control by \"Alt + H\"ï¼Œ \"Ctrl/Command + click\" to Multiple choice, \"shift+click\" to horizontal or vertical.",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "name": "Brule",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "ftp://*/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
