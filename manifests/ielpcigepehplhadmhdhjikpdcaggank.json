{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mocker",
  "version": "1.0",
  "description": "Edit & save live web pages.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/mocker_16.png",
    "32": "images/mocker_32.png",
    "48": "images/mocker_48.png",
    "128": "images/mocker_128.png"
  },
  "browser_action": {},
  "manifest_version": 2
}
