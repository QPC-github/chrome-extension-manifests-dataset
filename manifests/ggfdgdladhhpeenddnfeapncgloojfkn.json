{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Steam Linkfilter Blocker",
  "manifest_version": 2,
  "version": "1.0.1",
  "description": "Bypasses the steam link filter without any delay.",
  "icons": {
    "100": "icon100.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://steamcommunity.com/linkfilter/*"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  }
}
