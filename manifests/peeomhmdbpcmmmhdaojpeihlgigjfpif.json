{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*.etherlabs.io/*"
      ]
    }
  ],
  "description": "This is the extension utility for screen sharing support in EtherMeet video calls",
  "externally_connectable": {
    "matches": [
      "https://*.etherlabs.io/*"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "EtherMeet Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "EtherMeet screen sharing",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.8"
}
