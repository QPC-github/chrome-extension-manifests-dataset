{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/18.png",
    "default_title": "Cashdo - קאשדו",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "libs/jquery.js",
        "libs/handlebars.runtime.js",
        "libs/jquery-ui.js",
        "libs/underscore.js",
        "libs/backbone.js",
        "libs/moment.js",
        "libs/accounting.js",
        "libs/jquery.transit.js",
        "content/templates.js",
        "content/notification.js",
        "content/content.js",
        "content/searchEngines.js",
        "libs/utils.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/style.css"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "js": [
        "libs/jquery.js",
        "libs/handlebars.runtime.js",
        "libs/jquery-ui.js",
        "libs/underscore.js",
        "libs/backbone.js",
        "libs/moment.js",
        "libs/accounting.js",
        "libs/jquery.transit.js",
        "content/templates.js",
        "content/notification.js",
        "content/content.js",
        "libs/utils.js"
      ],
      "matches": [
        "*://cashdo.co.il/*",
        "*://www.cashdo.co.il/*",
        "*://www.cashdo.com/*",
        "*://cashdo.com/*",
        "*://www.cashdo.uk/*",
        "*://www.cashdo.co.il/*",
        "*://cashdo.co.il/*",
        "*://www.cashdo.ru/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "קאשדו - מקבלים קאשבק מהחנויות המובילות בארץ ובעולם, חברי האתר יכולים לקבל עד 30% קאשבק עבור כל קנייה.",
  "homepage_url": "https://cashdo.co.il/",
  "icons": {
    "128": "images/128.png",
    "16": "images/16.png",
    "18": "images/18.png",
    "32": "images/48.png",
    "48": "images/48.png",
    "64": "images/128.png"
  },
  "manifest_version": 2,
  "name": "Cashdo - קאשדו",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "cookies",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "images/*",
    "fonts/*",
    "templates/*"
  ],
  "version": "13.0",
  "short_name": "Cashdo - קאשדו"
}
