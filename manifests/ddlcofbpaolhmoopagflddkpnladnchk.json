{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "thumbnailify",
  "description": "This extension extracts the thumbnail of youtube videos",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "thumbnailify.js"
      ]
    }
  ]
}
