{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "De-Politicize",
  "version": "1.0",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
