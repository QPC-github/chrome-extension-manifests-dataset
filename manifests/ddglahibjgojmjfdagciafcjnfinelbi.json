{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "What Coffee should I have?",
  "version": "4.0.0",
  "manifest_version": 2,
  "description": "Not sure what coffee to have? Let us pick for you with over 35 different coffees and growing.",
  "browser_action": {
    "default_icon": "assets/img/logo/coffee.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "assets/img/logo/coffee.png"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "assets/css/bootstrap/*.css",
    "assets/css/myStyles.css",
    "assets/css/fonts/*",
    "assets/js/*.js",
    "assets/dist/css/bootstrap/*.css",
    "assets/dist/css/style.min.css",
    "assets/dist/css/fonts/*",
    "assets/dist/js/*.js",
    "assets/dist/css/*.min.css"
  ]
}
