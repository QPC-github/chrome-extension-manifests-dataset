{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Seller-Pulse Chrome Extension",
  "version": "2.1",
  "manifest_version": 2,
  "description": "Retrieve amazon pages",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "launcher.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "https://www.amazon.com/*",
    "https://www.amazon.co.uk/*",
    "https://www.amazon.ca/*",
    "https://www.amazon.fr/*",
    "https://www.amazon.it/*",
    "https://www.amazon.de/*",
    "https://www.amazon.es/*"
  ]
}
