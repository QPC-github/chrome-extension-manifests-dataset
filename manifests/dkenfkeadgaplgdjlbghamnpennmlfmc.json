{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EveryElement",
  "version": "1.0.0",
  "description": "EveryElement parses any html page and identifies every element with a blue border.",
  "icons": {
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "contentscript.js"
  ]
}
