{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Domain Closer",
  "manifest_version": 2,
  "version": "2",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "tab-closer16.png",
      "24": "tab-closer24.png",
      "32": "tab-closer32.png",
      "48": "tab-closer48.png",
      "128": "tab-closer128.png"
    },
    "default_title": "Close tabs by domain",
    "default_popup": "menu.html"
  }
}
