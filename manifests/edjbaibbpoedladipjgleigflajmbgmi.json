{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Web to QRCode to smartphone",
  "short_name": "Web to QRCode to smartphone",
  "description": "Generate QRCode of any URL from your browser and scan it with smartphone.",
  "version": "1.1",
  "author": "ProWebJect - Michal Gora",
  "icons": {
    "19": "img/icon_19.png",
    "38": "img/icon_38.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19.png",
      "38": "img/icon_38.png",
      "128": "img/icon_128.png"
    },
    "default_title": "Create QRCode for this website!"
  },
  "offline_enabled": true,
  "permissions": [
    "notifications",
    "storage",
    "contextMenus",
    "tabs"
  ],
  "web_accessible_resources": [],
  "background": {
    "page": "/background/background.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
