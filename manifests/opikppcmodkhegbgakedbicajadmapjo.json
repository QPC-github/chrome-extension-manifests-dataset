{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Redirector",
  "description": "This extension redirect to specific page according to url data.",
  "version": "1.1",
  "background": {
    "scripts": [
      "redirect.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "*://*/*",
    "webRequestBlocking",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
