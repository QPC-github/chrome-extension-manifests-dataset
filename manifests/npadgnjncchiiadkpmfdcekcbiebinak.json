{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "1-Click Toolbar Button to Developer Dashboard",
  "version": "1.2",
  "description": "Add a toolbar button to open a web link in a new tab.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open Link"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon.png"
  }
}
