{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ads Finder",
  "version": "1.0",
  "description": "Find the Ads",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icons.png"
    }
  },
  "manifest_version": 2
}
