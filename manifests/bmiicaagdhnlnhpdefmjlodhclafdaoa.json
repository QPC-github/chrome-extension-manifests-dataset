{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Game Jolt - Chat Tweaks",
  "version": "1.3",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "icons": {
    "16": "ChatIcon.png",
    "48": "ChatIcon.png",
    "128": "ChatIcon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "zepto.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "ChatIcon.png"
  }
}
