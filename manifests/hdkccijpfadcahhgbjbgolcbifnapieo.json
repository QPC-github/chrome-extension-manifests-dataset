{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Easy Shipping Tracker",
  "description": "Track packages via USPS, UPS, DHL and FEDEX",
  "version": "1.0.0.3",
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
