{
  "name": "Lifehacker News Feed",
  "description": "Gives you the latest lifehacker updates and search functionality right in your browser",
  "browser_action": {
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "Lifehacker News Feed and Search",
    "default_popup": "boxcarr.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "0.1",
  "icons": {
    "48": "48.png",
    "128": "128.png"
  }
}
