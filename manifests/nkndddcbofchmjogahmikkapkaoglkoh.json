{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SoundCloud Likes to Playlist",
  "description": "Extension to add all your SoundCloud Likes to a Playlist in seconds.",
  "version": "2.0.1",
  "browser_action": {},
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "64": "logo.png",
    "128": "logo.png",
    "180": "logo.png",
    "256": "logo.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "app.js"
    ]
  },
  "permissions": [
    "*://soundcloud.com/*",
    "*://www.soundcloud.com/*",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "web_accessible_resources": [
    "arceus.min.css",
    "arceus.js",
    "logo.png"
  ]
}
