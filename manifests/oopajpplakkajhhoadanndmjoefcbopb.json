{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Facebook Privatizer"
  },
  "name": "Facebook Privatizer",
  "description": "This extension should go through and (try to) mark all of your recent posts as Private (visible to 'only me').",
  "homepage_url": "https://chrome.google.com/webstore/detail/fb-privatizer/oopajpplakkajhhoadanndmjoefcbopb",
  "icons": {
    "36": "icon-16.png",
    "48": "icon-48.png",
    "512": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.4",
  "manifest_version": 2
}
