{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "enticle",
  "version": "1.0",
  "description": "enticleで紹介する英語ニュースの辞書引きを簡単に",
  "permissions": [
    "https://api.parse.com/"
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/jquery-2.1.1.min.js",
        "js/jquery.balloon.min.js",
        "js/enticle.js"
      ]
    }
  ]
}
