{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "18web",
  "version": "0.6.9",
  "description": "accelerate and secure your internet.",
  "browser_action": {
    "default_icon": "icon128off.png",
    "default_popup": "popup.html",
    "default_title": "18web"
  },
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "minimum_chrome_version": "40",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://18web.oss-cn-qingdao.aliyuncs.com/",
    "*://*/*",
    "<all_urls>",
    "proxy",
    "notifications"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
