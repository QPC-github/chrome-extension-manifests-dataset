{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "replace.js"
      ],
      "matches": [
        "*://torrentz.eu/*",
        "*://torrentz.me/*",
        "*://torrentz.in/*",
        "*://torrentz.ph/*",
        "*://torrentz.ch/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Adds; Magnet link, languages for non English movies, IMDB rating for movies. All code repurposed from other extensions.",
  "icons": {
    "512": "512pirate.png",
    "128": "128pirate.png",
    "48": "48pirate.png"
  },
  "manifest_version": 2,
  "name": "Torrentz.EU Magnet Links And IMDB Helper",
  "version": "1.0.0"
}
