{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Checkboxes Mouseover",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Check and uncheck by moving your mouse over checkboxes. Click extension icon to turn on and off.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "checkboxes.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16bw.png"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
