{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CarCandies by Turo",
  "description": "Replace new tab page with a beautiful car picture. Refreshed daily. Feeding every car enthusiast's guilty pleasure.",
  "version": "4.8",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "assets/logo_square.png",
    "default_title": "New Tab"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "assets/logo_square.png",
    "48": "assets/logo_square.png",
    "128": "assets/logo_square.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "unlimitedStorage",
    "*://s3.amazonaws.com/*/",
    "*://marketing.turo.com/*/",
    "*://*.tandemvault.com/*/",
    "*://cdn.segment.com/analytics.js/*/"
  ]
}
