{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Medeputize Systems",
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/*",
        "https://shopping.yahoo.co.jp/*",
        "https://*.yahoo.co.jp/*",
        "https://*.rakuten.co.jp/*",
        "http://*.rakuten.co.jp/*",
        "http://www.yodobashi.com/*",
        "https://nttxstore.jp/*",
        "https://www.monotaro.com/*",
        "https://www.askul.co.jp/*",
        "http://www.askul.co.jp/*",
        "https://lohaco.jp/*",
        "http://lohaco.jp/*",
        "https://www.mercari.com/jp/*",
        "https://*.aliexpress.com/*",
        "http://kakaku.com/*",
        "http://*.kakaku.com/*",
        "https://olsassistant.jimdo.com/*",
        "https://mnsearch.com/*",
        "https://clickpost.jp/*",
        "http://www.ksdenki.com/*",
        "https://www.saiyasune.com/*"
      ],
      "exclude_matches": [
        "https://item.rakuten.co.jp/*",
        "http://item.rakuten.co.jp/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "jquery-ui.min.js",
        "jquery.alerts.js",
        "jquery.autoexpand.js",
        "images.js",
        "itembtnsettings.js",
        "script.js",
        "help.js",
        "ecl.js"
      ],
      "css": [
        "normalize.css",
        "script.css",
        "jquery.alerts.css",
        "jquery-ui.min.css",
        "jquery-ui.structure.min.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "Icon_16.png",
    "48": "Icon_48.png",
    "128": "Icon_128.png"
  },
  "browser_action": {
    "default_icon": "Icon_16.png",
    "default_title": "OLSAssistant",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_page": "options.html",
  "description": "効率的なオンラインショッピングを手助けします",
  "manifest_version": 2,
  "name": "賢く通販★縦横無尽",
  "permissions": [
    "tabs",
    "background",
    "storage",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "1.0.4"
}
