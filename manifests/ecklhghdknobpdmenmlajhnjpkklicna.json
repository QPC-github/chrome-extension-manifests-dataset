{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Winnipeg Free Press Enhancer",
  "description": "Makes Winnipeg Free Press a bit better.",
  "version": "2.1",
  "permissions": [
    "tabs",
    "webNavigation",
    "http://www.winnipegfreepress.com/*",
    "https://www.winnipegfreepress.com/*"
  ],
  "icons": {
    "128": "wfp_128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "WFP Enchancer"
  },
  "manifest_version": 2
}
