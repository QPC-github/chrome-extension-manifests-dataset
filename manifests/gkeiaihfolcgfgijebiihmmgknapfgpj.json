{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YTFind",
  "description": "YTFind allows you to search for the dialogue in Youtube videos.",
  "version": "1.35.7",
  "manifest_version": 2,
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "*://ytfind.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "xml2json.js",
        "background.js"
      ],
      "matches": [
        "*://www.youtube.com/*"
      ]
    }
  ]
}
