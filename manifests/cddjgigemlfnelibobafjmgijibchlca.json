{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "carum.cz",
  "version": "0.3",
  "manifest_version": 2,
  "description": "SOLO-PROPHYLAXE nákup zboží pro zubní lékaře a hygienistky",
  "permissions": [
    "http://carum.cz/*"
  ],
  "icons": {
