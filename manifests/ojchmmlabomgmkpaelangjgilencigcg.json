{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MapMyRun GPX Exporter",
  "description": "Adds a button to MapMyRun workouts to export as a GPX file",
  "author": "Bryan Leboff",
  "version": "0.2",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "lib/jquery-2.1.1.min.js",
        "lib/XMLWriter.js",
        "lib/analytics.js",
        "js/gpxexport.js"
      ],
      "matches": [
        "*://*.mapmyfitness.com/workout/*",
        "*://*.mapmyrun.com/workout/*",
        "*://*.mapmyride.com/workout/*",
        "*://*.mapmywalk.com/workout/*",
        "*://*.mapmyhike.com/workout/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
