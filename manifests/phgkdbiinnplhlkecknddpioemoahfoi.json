{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bring Back The Old Jira",
  "version": "0.1",
  "description": "Adds a parameter so you only ever have to work with the old Jira!",
  "background": {
    "scripts": [
      "background-1.js"
    ]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.ripenecommerce.atlassian.net/browse/*",
    "webRequestBlocking"
  ]
}
