{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FormFillr",
  "description": "Experiments in autofilling forms",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "filllllllll"
  },
  "manifest_version": 2
}
