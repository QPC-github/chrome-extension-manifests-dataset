{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Conjug8",
  "description": "Generate Spanish verbs conjugation tests",
  "version": "0.6",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "*"
  ],
  "browser_action": {
    "default_icon": "conjug8_48.png"
  },
  "icons": {
    "16": "conjug8_16.png",
    "30": "conjug8_30.png",
    "48": "conjug8_48.png"
  },
  "manifest_version": 2,
  "author": "Turbatu Dumitru Titus titus.tur.dum@gmail.com"
}
