{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Automatisk logon med UNI-Login",
  "description": "Log automatisk ind med UNI-Login",
  "version": "1.4.5",
  "manifest_version": 2,
  "icons": {
    "16": "login16px.png",
    "64": "login64px.png",
    "128": "login128px.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "login16px.png",
    "default_title": "Autologin indstillinger",
    "default_popup": "options.html"
  }
}
