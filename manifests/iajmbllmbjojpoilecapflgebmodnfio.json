{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyAnimeList Searcher",
  "description": "Search Anime on MyAnimeList",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "search.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "MALSearcher"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
