{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ChannelAdvisor Tracking Assistance",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Checks for ChannelAdvisor tracking and provides basic troubleshooting",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
