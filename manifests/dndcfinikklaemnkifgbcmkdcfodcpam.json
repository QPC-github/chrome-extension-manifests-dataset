{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple CRM Import",
  "description": "Chrome extension to import data into Simple CRM",
  "version": "1.9",
  "author": "dev@scrm-group.eu",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://www.linkedin.com/*",
    "https://www.local.ch/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "48": "sc_logo_48.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "sc_logo_48.png",
    "128": "sc_logo_128.png"
  }
}
