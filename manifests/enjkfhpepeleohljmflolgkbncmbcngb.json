{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ulimi Recruitment Manager",
  "description": "Allows users of ulimi.io dashboard to access the chat feature.Users can ask questions about job postings,pay rates,shift times, etc.",
  "version": "1.2",
  "browser_action": {
    "default_popup": "ulimi.html",
    "default_icon": "ulimicon.png"
  },
  "short_name": "Ulimi Recruitment Manager",
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  },
  "icons": {
    "128": "ulimicon.png"
  }
}
