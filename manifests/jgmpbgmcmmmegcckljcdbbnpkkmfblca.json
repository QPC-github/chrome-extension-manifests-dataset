{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Visual JavaScript Free",
  "version": "0.2",
  "description": "javascript code generator thru a Visual Programming paradigm.",
  "manifest_version": 2,
  "minimum_chrome_version": "6",
  "browser_action": {
    "default_icon": "img/vjs_128.png",
    "default_title": "Visual-JS"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "comm.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "img/vjs_128.png",
    "16": "img/vjs_16.png",
    "32": "img/vjs_32.png",
    "48": "img/vjs_48.png"
  },
  "web_accessible_resources": [
    "img/vjs_48.png"
  ],
  "permissions": [
    "*://*/*",
    "contextMenus",
    "tabs",
    "background"
  ]
}
