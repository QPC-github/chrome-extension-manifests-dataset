{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Traveler: Background photo for chrome new tab",
  "version": "2.5",
  "background": {
    "scripts": [
      "main.js",
      "clock.js",
      "options.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "description": "We are 2 travelers sharing our best photos as new tab page background like Pixlr, Momentum or Flickr extensions",
  "permissions": [
    "tabs",
    "storage",
    "https://milesandlove.com/random.json"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
