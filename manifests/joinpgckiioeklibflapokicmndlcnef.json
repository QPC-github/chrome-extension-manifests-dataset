{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "2.1.2",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "assets/logo_16x16.png",
    "48": "assets/logo_48x48.png",
    "128": "assets/logo_128x128.png"
  },
  "default_locale": "zh_CN",
  "browser_action": {
    "default_icon": "assets/logo_19x19.png",
    "default_title": "__MSG_actionTitle__"
  },
  "background": {
    "scripts": [
      "js/main.js"
    ]
  },
  "homepage_url": "http://note.youdao.com",
  "permissions": [
    "http://note.youdao.com/*",
    "http://note.corp.youdao.com/*",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "js/clipper-min.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://note.youdao.com/ https://hm.baidu.com/; unsafe-inline; object-src 'self'"
}
