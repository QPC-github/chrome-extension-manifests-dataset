{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snake The Game",
  "short_name": "Snake Game",
  "description": "Simple snake game. Eat apples, have fun!",
  "version": "1.0.0.2",
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png"
  },
  "browser_action": {
    "default_icon": "48x48.png",
    "default_title": "Snake",
    "default_popup": "popup.html"
  },
  "options_page": "snake.html",
  "web_accessible_resources": [
    "snake.html",
    "style.css",
    "js.js"
  ]
}
