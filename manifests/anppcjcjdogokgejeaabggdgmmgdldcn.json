{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Countdown",
  "version": "0.9",
  "description": "Customizable countdown...",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "manifest_version": 2,
  "permissions": [
    "history",
    "tabs",
    "<all_urls>"
  ]
}
