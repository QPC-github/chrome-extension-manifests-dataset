{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Поиск в ПрессИндекс",
  "version": "1.0.2",
  "description": "Быстрый поиск в системе ПрессИндекс",
  "manifest_version": 2,
  "author": "Alena <cgwbmp@gmail.com>",
  "icons": {
    "128": "./icons/128.png",
    "48": "./icons/48.png",
    "32": "./icons/32.png",
    "16": "./icons/16.png"
  },
  "browser_action": {
    "default_icon": "./icons/32.png"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "options_ui": {
    "page": "./options.html"
  },
  "background": {
    "scripts": [
      "./background.js"
    ]
  }
}
