{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bundle It",
  "version": "0.0.1",
  "description": "Save, view and print your daily information stream.",
  "icons": {
    "128": "icon128.png",
    "48": "icon32.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "./popup/popup.html"
  },
  "background": {
    "scripts": [
      "./event/badges.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "notifications",
    "tabs"
  ]
}
