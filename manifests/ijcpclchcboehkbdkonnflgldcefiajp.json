{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VKMessageExpander",
  "description": "Automatically show nested message contents",
  "version": "0.1",
  "permissions": [
    "http://www.vk.com/*",
    "https://www.vk.com/*",
    "http://vk.com/*",
    "https://vk.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.vk.com/*",
        "https://www.vk.com/*",
        "http://vk.com/*",
        "https://vk.com/*"
      ],
      "js": [
        "expander.js"
      ]
    }
  ],
  "manifest_version": 2
}
