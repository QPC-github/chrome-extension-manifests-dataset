{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TF.TV Moron Remover",
  "description": "Allows you to filter posters on teamfortress.tv. Stores filter in your browsers local storage for tf.tv.",
  "version": "1.0.1",
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [
        "js/contentscript.js"
      ],
      "matches": [
        "http://teamfortress.tv/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/tfmoron.js",
    "js/json3.min.js",
    "css/morons.css",
    "img/hide.png",
    "img/show.png"
  ],
  "permissions": [
    "http://teamfortress.tv/*"
  ],
  "icons": {
    "128": "icon.png"
  }
}
