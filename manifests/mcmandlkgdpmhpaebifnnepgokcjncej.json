{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gmail Gifline",
  "version": "1.7",
  "description": "The fastest way to include gif in your emails",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://mail.google.com/*",
    "tabs"
  ],
  "web_accessible_resources": [
    "jquery-3.4.1.min.js",
    "gmail.js",
    "main.js"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2
}
