{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Muthukumaran Navaneethakrishnan(https://github.com/muthuishere)",
  "background": {
    "scripts": [
      "js/global.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Enable you to Convert list of flash based files to PDF Ebook ",
  "icons": {
    "128": "images/flash-to-pdf-ebook-128.png",
    "48": "images/flash-to-pdf-ebook-48.png",
    "64": "images/flash-to-pdf-ebook-64.png"
  },
  "browser_action": {
    "default_title": "Convert Flash files to Pdf ebook",
    "default_icon": "images/flash-to-pdf-ebook-19.png"
  },
  "options_page": "local-scripts/generate.html",
  "manifest_version": 2,
  "name": "Flash Files to Pdf ebook converter",
  "permissions": [
    "tabs",
    "declarativeContent",
    "tabCapture",
    "storage",
    "notifications",
    "<all_urls>",
    "unlimitedStorage"
  ],
  "short_name": "convert-flash-files-to-pdf",
  "version": "1.1.0",
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg",
    "faq/images/*.png",
    "js/*",
    "local-scripts/*"
  ]
}
