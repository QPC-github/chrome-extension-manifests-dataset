{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rise Up virtual classrooom",
  "author": "Rise Up",
  "version": "2.3",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Partage d'Ã©cran pour les classes virtuelles du LMS Rise Up",
  "homepage_url": "https://riseup.ai/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "matches": [
        "https://*.upgraduate.com/*",
        "https://*.riseup.ai/*",
        "https://*.monpitch.com/*/session/*",
        "https://*.aldes.com/*/session/*",
        "https://www.plateformenge.com/*/session/*",
        "https://campus-digital.schneider-electric.fr/*/session/*",
        "https://campus.wk-formation.fr/*/session/*"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
