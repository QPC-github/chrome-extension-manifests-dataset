{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Apache Felix Helper",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Checking Felix bundles.",
  "icons": {
    "19": "icon19.png",
    "128": "icon128.png"
  },
  "page_action": {
    "name": "CQ",
    "default_popup": "popup.html",
    "default_icon": "icon19.png"
  },
  "options_page": "options.html",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}
