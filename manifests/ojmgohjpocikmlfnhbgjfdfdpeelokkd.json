{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PageHeight",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Get position of page on bottom",
  "icons": {
    "16": "icons/off.png",
    "48": "icons/icon48.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "icon"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/slide/slide.js"
      ],
      "run_at": "document_end"
    }
  ]
}
