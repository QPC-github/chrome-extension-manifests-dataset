{
  "author": "Spencer",
  "name": "EPIC",
  "short_name": "EPIC",
  "homepage_url": "https://EpicSearches.com",
  "description": "The new EPIC browser extension uses search keyword bookmarks and makes it simple to use the EpicSearches.com Search Engine.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "EPIC"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://EpicSearches.com/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "EPIC",
      "name": "EPIC",
      "search_url": "https://EpicSearches.com/?a=web&q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "1.js",
      "2.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "1.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ]
}
