{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QRCode Website",
  "description": "Opens the current URL as QRCode for mobile devices!",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "qrcode.html"
  },
  "icons": {
    "128": "icon.png"
  }
}
