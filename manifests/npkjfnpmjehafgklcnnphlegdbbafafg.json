{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Images",
  "version": "1.5.1",
  "manifest_version": 2,
  "description": "Hides images/backgrounds/video/multimedia/flash elements on any page for increased privacy and readability",
  "background": {
    "scripts": [
      "launch.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "Hide Images",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
