{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vajehyab",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Look up persian words using Vajehyab",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "vajehyab.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "author": "Zia Zafari"
}
