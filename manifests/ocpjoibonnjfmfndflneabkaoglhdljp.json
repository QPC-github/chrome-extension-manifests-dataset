{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Content Filter",
  "description": "Select the section you want to keep, press enter and read!",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "black-funnel-16.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "filter.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
