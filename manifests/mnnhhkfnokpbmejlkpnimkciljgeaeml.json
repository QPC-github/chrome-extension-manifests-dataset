{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nativ Send Report!",
  "version": "1.472",
  "description": "Send a Report to Nativ.",
  "browser_action": {
    "default_icon": "nativusa.png",
    "default_popup": "nativ.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
