{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Site-Verifier",
  "version": "0.1",
  "description": "Check Websites for data privacy",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/verifier.png"
    },
    "default_title": "Site Verifier",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
