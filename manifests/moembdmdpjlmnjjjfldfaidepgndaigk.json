{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "20/20 Vision: Save Your Eyes",
  "description": "Get Notified every 20 minutes to take a 20 second break, to preserve your eye health",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "alarms",
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
