{
  "background": {
    "scripts": [
      "scripts/back.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_title": "צפייה באיכות HD"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://www.search-o.com/favicon.ico",
      "image_url": "https://www.search-o.com/web?q={searchTerms}",
      "image_url_post_params": "",
      "instant_url": "https://www.search-o.com/web?q={searchTerms}",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "https://www.search-o.com/",
      "name": "BetterSearch",
      "search_url": "https://www.search-o.com/web?q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url": "https://www.search-o.com/web?q={searchTerms}",
      "suggest_url_post_params": ""
    },
    "startup_pages": [
      "https://www.search-o.com/?r=s1"
    ]
  },
  "description": "Allow you to surf with secure browser, Virus Defender.",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.search-o.com/*",
      "*://*.serethd.net/*"
    ]
  },
  "manifest_version": 2,
  "name": "צפייה באיכות HD",
  "permissions": [
    "storage",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4.3"
}
