{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TagPlus - Sistema de Gestão On-line",
  "short_name": "TagPlus",
  "description": "Sistema On-line com NF-e, Loja Virtual, Financeiro, Vendas, Estoque, Relatórios e muito mais para alavancar a gestão da sua empresa!",
  "homepage_url": "https://www.tagplus.com.br/?ref=extension_chrome",
  "version": "1.1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "TagPlus",
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
