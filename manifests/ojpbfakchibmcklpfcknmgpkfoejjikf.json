{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "neutral",
  "version": "1.11.0",
  "description": "Neutral is a universal browser extension for the Twitter website.",
  "manifest_version": 2,
  "homepage_url": "https://github.com/r-obert/neutral.js",
  "icons": {
    "16": "./images/icon-16x16.png",
    "48": "./images/icon-48x48.png",
    "128": "./images/icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_start"
    }
  ]
}
