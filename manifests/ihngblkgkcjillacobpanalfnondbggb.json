{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark View",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "description": "ブックマークを画像で確認するBookmark View",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/icon_19.png",
    "default_title": "Bookmark View"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}
