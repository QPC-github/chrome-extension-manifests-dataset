{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "cDeveloper",
  "short_name": "cdev",
  "version": "1.3.2",
  "manifest_version": 2,
  "description": "A multitool. Let us help you improve your workflow!",
  "icons": {
    "16": "images/ico16.png",
    "32": "images/ico32.png",
    "48": "images/ico48.png",
    "128": "images/ico128.png"
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_title": "cDeveloper helps you to improve your workflow.",
    "default_icon": {
      "16": "images/ico16.png",
      "32": "images/ico32.png",
      "48": "images/ico48.png",
      "128": "images/ico128.png"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ]
}
