{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "default-src 'self' *; script-src 'self'; object-src 'self'; media-src 'self' *; connect-src 'self' *",
  "browser_action": {
    "default_title": "Rádio Atlântida"
  },
  "manifest_version": 2,
  "description": "Rádio Atlântida - Encontre todas as notícias e informações sobre Pretinho Básico, bandas e artistas da Rádio Atlântida",
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "19": "19.png"
  },
  "name": "Rádio Atlântida",
  "permissions": [
    "tabs",
    "notifications",
    "http://www.ricardofernandes.art.br/*"
  ],
  "version": "2.0"
}
