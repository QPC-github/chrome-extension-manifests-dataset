{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Smooth Fonts",
  "description": "This lightweight extension significantly improves font rendering on computers running OS X.",
  "version": "10.1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
