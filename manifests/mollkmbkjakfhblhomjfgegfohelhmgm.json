{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "kisu.me CopyClipboard",
  "version": "1.0",
  "description": "URL短縮サイト kisu.me でURLを短縮し、クリップボードへコピーする拡張機能です",
  "permissions": [
    "http://kisu.me/api/shorten.php",
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "19": "kisume19x19.png",
    "48": "kisume48x48.png",
    "128": "kisume128x128.png"
  },
  "browser_action": {
    "default_title": "kisu.me CopyClipboard",
    "default_icon": "kisume19x19.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
