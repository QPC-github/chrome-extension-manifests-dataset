{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Form Stash",
  "author": "Seth Painter",
  "description": "Quickly stash the contents of HTML forms and reload them later.",
  "version": "1.0.1",
  "icons": {
    "128": "icon_128.png",
    "19": "icon_19.png"
  },
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "<all_urls>",
    "storage"
  ]
}
