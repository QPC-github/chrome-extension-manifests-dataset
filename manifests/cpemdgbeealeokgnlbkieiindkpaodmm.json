{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extMainTitle__",
  "version": "3.0.0.197",
  "version_name": "0.0.11.37",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "/icon/icon_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "/js/storage.js",
      "/js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "options_page": "/html/options.html",
  "chrome_url_overrides": {
    "newtab": "/html/blank.html"
  },
  "browser_action": {
    "default_icon": "/icon/transparent.png",
    "default_title": "__MSG_extMainTitle__"
  }
}
