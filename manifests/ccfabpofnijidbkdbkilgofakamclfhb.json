{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vikipedi Açar",
  "description": "Asıl işi o an çalışan hangi servis varsa o yapar, eklenti sadece sizi her seferinde birkaç kopyala yapıştırdan kurtarır.",
  "version": "0.3",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*.wikipedia.org/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
