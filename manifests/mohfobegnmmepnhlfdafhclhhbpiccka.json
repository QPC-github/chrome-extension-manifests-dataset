{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nutriatlas pro Google Chrome™",
  "short_name": "Nutriatlas",
  "version": "0.2",
  "description": "Rychle vyhledáte Éčka, potraviny, recepty, informace z článků a diskusí",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "permissions": [
    "cookies",
    "contextMenus",
    "*://*.zdravapotravina.cz/",
    "http://*.emulgatory.cz/",
    "http://*.chemievjidle.cz/",
    "http://*.zdraverecepty.cz/",
    "http://*.zdraveprodejny.cz/",
    "http://*.lidicky.cz/",
    "http://*.nutriatlas.cz/",
    "http://*.ecka.info/"
  ],
  "browser_action": {
    "default_title": "Nutriatlas pro Chrome",
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  }
}
