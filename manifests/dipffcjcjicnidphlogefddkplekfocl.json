{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ace Navigator",
  "description": "Follow any link on the screen easily with the keyboard",
  "version": "0.0.4",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "ace.css"
      ],
      "js": [
        "jquery.js",
        "keypress.js",
        "ace.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
