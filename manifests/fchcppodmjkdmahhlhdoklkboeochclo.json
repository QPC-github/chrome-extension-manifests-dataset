{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_title": "Color Search"
  },
  "description": "Color Search Extension",
  "icons": {
    "128": "icons/favicon-128x128.png",
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png"
  },
  "manifest_version": 2,
  "name": "Color Search",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "https://*/*",
    "http://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.0.3"
}
