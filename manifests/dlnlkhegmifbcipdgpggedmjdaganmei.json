{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "no-beforeunload",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ]
}
