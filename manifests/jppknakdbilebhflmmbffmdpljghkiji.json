{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IDS Manager",
  "short_name": "IDS Manager",
  "description": "This extension allows you to quickly access IDS Manager.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "ids.png",
    "default_popup": "popup.html",
    "default_title": "IDS Manager!",
    "default_image": "ids.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://ids.greyb.com/"
  ]
}
