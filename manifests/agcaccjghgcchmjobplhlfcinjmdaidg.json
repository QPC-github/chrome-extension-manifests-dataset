{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wadav",
  "version": "1.0",
  "description": "The latest coupons and promo codes can be found here at wadav.com for popular online stores. Get your coupon code and save money.",
  "browser_action": {
    "default_title": "wadav.com"
  },
  "permissions": [
    "https://www.wadav.com/"
  ],
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
