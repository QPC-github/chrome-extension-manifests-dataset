{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Hancock.it",
  "manifest_version": 2,
  "version": "1.0.11",
  "default_locale": "en",
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "frameworks/jquery.js",
        "frameworks/q.js",
        "frameworks/underscore.js",
        "frameworks/underscore.string.js",
        "js/utils.js",
        "js/config_new_layout.js",
        "js/config.js",
        "js/main.js",
        "js/popup.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://hancock.it/updatedata/*"
      ],
      "js": [
        "frameworks/jquery.js",
        "js/utils.js",
        "js/forceUpdate.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://sig.hancock.it/*"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html"
  },
  "description": "Manage your companyâ€™s signatures and taglines for Google Apps and GMail."
}
