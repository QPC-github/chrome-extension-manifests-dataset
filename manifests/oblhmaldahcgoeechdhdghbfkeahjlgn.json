{
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "64": "icons/64.png"
    }
  },
  "icons": {
    "128": "icons/128.png",
    "64": "icons/64.png"
  },
  "permissions": [
    "tabs",
    "browsingData",
    "storage",
    "unlimitedStorage",
    "topSites",
    "cookies",
    "https://topaddon.com/*",
    "http://topaddon.com/*",
    "https://www.topaddon.com/*",
    "https://duckduckgo.com/*",
    "https://www.bing.com/*",
    "https://www.yahoo.com/*",
    "https://www.google.com/*",
    "https://www.google.fr/*",
    "https://www.google.co.in/*",
    "https://www.google.pt/*",
    "https://www.google.ca/*",
    "https://www.google.es/*",
    "https://www.google.de/*",
    "https://play-search.com/*",
    "https://www.play-search.com/*",
    "https://www.skytorrents.lol/*"
  ],
  "minimum_chrome_version": "29",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "persistent": true,
    "scripts": [
      "js/http.js",
      "js/background.js",
      "js/analytics.js"
    ]
  }
}
