{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "State of Bamboo",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Chrome extension for colouring Atlassian Bamboo's wallboard appropriately on State of Origin days",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click to Origin-ify the wallboard!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
