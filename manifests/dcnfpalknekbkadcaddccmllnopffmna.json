{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SociaLook Advocacy",
  "description": "SociaLook enables you and your colleagues to share interesting content about your company on social media.",
  "version": "1.0.22",
  "permissions": [
    "tabs",
    "storage",
    "https://secure.flickr.com/",
    "https://twitter.com/",
    "https://platform.twitter.com/",
    "https://*.platform.twitter.com/",
    "http://api.socialook.net/",
    "https://socialook.atlassian.net/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "home_remus.html"
  }
}
