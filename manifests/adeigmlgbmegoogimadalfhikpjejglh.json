{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gitlab - Time tracking",
  "version": "1.1",
  "description": "Gitlab time tracking",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "webNavigation",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "injected/js/analyseBody.js"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/default16.png",
      "32": "images/default32.png",
      "48": "images/default48.png",
      "128": "images/default128.png"
    }
  },
  "icons": {
    "16": "images/default16.png",
    "32": "images/default32.png",
    "48": "images/default48.png",
    "128": "images/default128.png"
  },
  "manifest_version": 2
}
