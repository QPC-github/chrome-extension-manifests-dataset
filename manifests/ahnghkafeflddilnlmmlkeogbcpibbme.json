{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clear",
  "description": "Easily block specific video ads.",
  "author": "Tyler Lafayette",
  "icons": {
    "1000": "logo.png"
  },
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
