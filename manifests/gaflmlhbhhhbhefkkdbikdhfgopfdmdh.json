{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Gigaw.at",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Gigaw.at URL Shortner.",
  "icons": {
    "128": "128icon.png"
  },
  "homepage_url": "http://gigaw.at",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery.js",
      "gigawat.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "http://gigaw.at/",
    "http://127.0.0.1/"
  ]
}
