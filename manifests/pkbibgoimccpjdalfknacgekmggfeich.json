{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Easy Login",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Quickly login to frequently used websites",
  "homepage_url": "https://sathishpaul.github.io/EasyLogin",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "topSites",
    "webNavigation"
  ]
}
