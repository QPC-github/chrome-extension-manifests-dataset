{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "1Stream Hosted Solutions",
  "background": {
    "scripts": [
      "js/jquery-1.10.2.min.js",
      "js/Process.js",
      "js/Background.js",
      "options.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "images/Icon128.png",
      "19": "images/Icon18.png",
      "32": "images/Icon32.png",
      "48": "images/Icon48.png"
    },
    "default_popup": "popup.html",
    "default_title": "Sefeko Click To Dial"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "content.css"
      ],
      "js": [
        "js/jquery-1.10.2.min.js",
        "js/ContentPage.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "OneAgent click to dial from webpage",
  "icons": {
    "128": "images/Icon128.png",
    "19": "images/Icon18.png",
    "32": "images/Icon32.png",
    "48": "images/Icon48.png"
  },
  "manifest_version": 2,
  "name": "Sefeko 1Stream Click to Dial",
  "options_ui": {
    "chrome_style": true,
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "activeTab",
    "https://scripts.1stream.co.za/*"
  ],
  "version": "1.0.2",
  "web_accessible_resources": [
    "js/jquery-1.10.2.min.map"
  ]
}
