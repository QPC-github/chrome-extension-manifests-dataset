{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zaokrouhlovač",
  "description": "Zaokrouhluje 90 haléřů na celé koruny.",
  "version": "1.4",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "round.js"
      ]
    }
  ]
}
