{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pretty Twitch",
  "description": "Redesign of Twitch.tv",
  "version": "1.2.1.1",
  "version_name": "1.0 pre-beta",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.twitch.tv/*"
      ],
      "js": [
        "jquery.js",
        "twitch.js"
      ],
      "css": [
        "twitch.css",
        "chat.css"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
