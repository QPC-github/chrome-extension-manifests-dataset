{
  "author": "EmiCantero",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "EmiSports Pro"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "popup.js",
        "emisports.js",
        "jquery.dynatable.js"
      ],
      "matches": [
        "http://arenavision.ru/*]",
        "http://arenavision2018.ga/*",
        "http://www.emicantero.com/emitv/emisportspro/*",
        "http://www.emicantero.com/emitv/arenavision/*"
      ]
    }
  ],
  "description": "Eventos deportivos en HD..",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "options_page": "emisports.html",
  "options_ui": {
    "chrome_style": false,
    "open_in_tab": true,
    "page": "emisports.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "webRequest"
  ],
  "name": "EmiSports Pro, Deportes en streaming y en HD",
  "short_name": "EmiSports Pro",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3.9"
}
