{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SMITE Pick and Ban",
  "short_name": "Pick and Ban",
  "description": "Smite Pick and Ban for Streaming Tournaments!",
  "version": "0.6.20",
  "manifest_version": 2,
  "icons": {
    "16": "SMITE_PB_16.png",
    "48": "SMITE_PB_48.png",
    "128": "SMITE_PB_128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "syncFileSystem",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "http://*/"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
