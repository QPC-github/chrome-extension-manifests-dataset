{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Go2 Domain Root",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Find the root of your domain.",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "author": "Jase Pellerin",
  "browser_action": {
    "default_icon": "images/icon.png"
  }
}
