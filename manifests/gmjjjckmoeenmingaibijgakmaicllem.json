{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PicSide smart and social photo",
  "description": "Picside allows you to discover the Other Side of the Picture! Install our plug-in to flip pictures and discover beautiful stories.",
  "version": "1.1",
  "icons": {
    "16": "Icon-32.png",
    "48": "Icon-48.png",
    "64": "Icon-64.png",
    "128": "logo128.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "downloads",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
