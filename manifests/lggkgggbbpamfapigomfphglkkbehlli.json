{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Editable Tab",
  "version": "1.0",
  "description": "Opens a new content-editable tab.",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Editable Tab",
    "default_icon": "icon_19.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
