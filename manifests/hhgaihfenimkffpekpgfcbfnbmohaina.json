{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RaciallyUncharged",
  "version": "1.1",
  "description": "Replace the term 'racially charged' to 'racist' on every website to better reflect the truth",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
