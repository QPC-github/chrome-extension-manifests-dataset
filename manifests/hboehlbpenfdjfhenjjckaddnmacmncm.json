{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TrixWS Short URL Creator",
  "version": "1.3",
  "manifest_version": 2,
  "description": "TrixWS Short URL Creator.",
  "background": {
    "scripts": [
      "ContextMenu.js"
    ]
  },
  "icons": {
    "16": "Logo16.png",
    "48": "Logo48.png",
    "128": "Logo128.png"
  },
  "browser_action": {
    "default_icon": "Logo128.png",
    "default_popup": "popup.html"
  },
  "homepage_url": "http://www.trix.ws/",
  "permissions": [
    "http://*.trix.ws/",
    "tabs",
    "clipboardWrite",
    "contextMenus"
  ]
}
