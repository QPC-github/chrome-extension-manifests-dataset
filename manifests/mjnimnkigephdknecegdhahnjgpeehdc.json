{
  "name": "Search Minus The Plus",
  "description": "Get rid of all the Google+ junk in Google Search.",
  "version": "2.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon128a.png",
    "48": "icon48a.png",
    "19": "icon19a.png"
  },
  "content_scripts": [
    {
      "css": [
        "search.css"
      ],
      "matches": [
        "*://www.google.com/*",
        "*://encrypted.google.com/*",
        "*://www.google.co.uk/*",
        "*://encrypted.google.co.uk/*",
        "*://www.google.com.au/*",
        "*://encrypted.google.com.au/*",
        "*://www.google.com.mx/*",
        "*://encrypted.google.com.mx/*",
        "*://www.google.ca/*",
        "*://encrypted.google.ca/*",
        "*://www.google.fr/*",
        "*://encrypted.google.fr/*",
        "*://www.google.ca/*",
        "*://encrypted.google.ca/*",
        "*://www.google.co.jp/*",
        "*://encrypted.google.co.jp/*"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
