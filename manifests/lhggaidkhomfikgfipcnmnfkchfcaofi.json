{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmarks window",
  "description": "Shows a simple window with a bookmarks grid of customizable width",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "19": "icon_19.png",
    "32": "icon_32.png",
    "48": "icon_48.png"
  },
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ]
}
