{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "msstat - Pagerank, and Advanced Stats",
  "version": "1.5",
  "description": "Shows the google pagerank of the site that you are viewing, and also offers a whole set of advanced statistics when clicked.",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://toolbarqueries.google.com/",
    "http://ajax.googleapis.com/",
    "http://data.alexa.com/",
    "http://msstat.com/overview/",
    "tabs"
  ],
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon48.png"
  },
  "content_security_policy": "script-src 'self' chrome-extension://; object-src 'self'"
}
