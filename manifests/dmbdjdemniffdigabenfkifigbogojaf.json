{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "#TheChadIsBad",
  "version": "1.1",
  "description": "The world doesn't need more Chad, let's refer to him as Rod.",
  "icons": {
    "16": "images/potato16.png",
    "32": "images/potato_32.png",
    "48": "images/potato_48.png",
    "128": "images/potato_128.png"
  },
  "content_script": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
