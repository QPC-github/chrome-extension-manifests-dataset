{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "inject_backbone",
  "version": "0.5",
  "description": "inject backbone",
  "manifest_version": 2,
  "icons": {
    "128": "backbone.png",
    "16": "backbone.png",
    "256": "backbone.png",
    "48": "backbone.png"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "backbone.png",
    "default_title": "just click to inject backbone 1.0.0"
  },
  "web_accessible_resources": [
    "libs/jquery.min.js",
    "libs/underscore-min.js",
    "libs/json2.js",
    "libs/backbone-min.js",
    "libs/sinon-1.3.4.js",
    "libs/jquerypp.js",
    "libs/sugar-1.3-full.min.js",
    "libs/jquery.cookie.js",
    "libs/bootstrap.min.js",
    "styles/bootstrap.css"
  ]
}
