{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Boredom Button",
  "version": "0.0.1",
  "description": "Waste time, save lives.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "background/utils.js",
      "background/api.js",
      "background/analytics.js",
      "background/button.js",
      "background/content.js",
      "background/notifications.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "notifications"
  ]
}
