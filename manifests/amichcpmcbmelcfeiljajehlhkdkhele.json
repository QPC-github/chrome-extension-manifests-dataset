{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Plinks.io",
  "version": "1.0",
  "description": "Push your links",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/image19.png",
      "38": "images/image38.png"
    },
    "default_title": "Plinks.io"
  },
  "chrome_url_overrides": {},
  "permissions": [
    "history",
    "bookmarks",
    "tabs",
    "storage",
    "http://plinks.io/"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "/js/jquery.min.js",
      "/js/underscore-min.js",
      "/js/lib.js",
      "background.js"
    ]
  },
  "options_page": "options.html"
}
