{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wiki This",
  "version": "1.2",
  "description": "Quick wikipedia search for highlighted text",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "manifest_version": 2
}
