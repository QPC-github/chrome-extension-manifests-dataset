{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "cursedCats",
  "permissions": [
    "activeTab"
  ],
  "version": "0.1",
  "description": "cursed cats",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "cats.js"
      ]
    }
  ]
}
