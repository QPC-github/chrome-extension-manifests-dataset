{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Taboola Pixel Helper",
  "description": "This extension allows the user to troubleshoot problems when using Taboola Pixels.",
  "version": "1.1.12",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open Taboola Pixel Helper"
  },
  "background": {
    "scripts": [
      "app/background.min.js"
    ]
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "app/content.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "app/injected.min.js"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "webRequest",
    "storage",
    "<all_urls>"
  ]
}
