{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bea Blocker",
  "version": "1.0.1",
  "description": "",
  "icons": {
    "19": "img/icon19.png",
    "32": "img/icon32.png",
    "38": "img/icon38.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "img/bea_status.jpg",
    "img/bea_avatar.jpg"
  ]
}
