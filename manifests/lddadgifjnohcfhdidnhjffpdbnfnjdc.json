{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SkipAnkalert",
  "description": "Cette extension enlève le délai de 8 secondes nécessaire à l'accès d'une page tierce via la sécurité Ankama.",
  "author": "Guichi",
  "version": "1.0",
  "icons": {
    "128": "icons/128.png"
  },
  "permissions": [
    "*://go.ankama.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://go.ankama.com/*"
      ],
      "css": [
        "site.css"
      ]
    }
  ]
}
