{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "event.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://glosbe.com https://en.wiktionary.org https://ta.wiktionary.org ; object-src 'self'",
  "description": "சொற்களம்:தமிழ்<->ஆங்கிலம்[கூகுள்,குளோசுப்,விக்கி,தமிழ் இணையக் கல்விக்கழக கலைச்சொற்கள் ஒருங்கிணைப்பு முயற்சி]",
  "icons": {
    "128": "icon128px.png",
    "16": "icon16px.png",
    "48": "icon48px.png"
  },
  "manifest_version": 2,
  "name": "Sorkalam",
  "permissions": [
    "tabs",
    "https://glosbe.com/",
    "http://*/*",
    "https://*/*",
    "https://en.wiktionary.org/",
    "https://ta.wiktionary.org/",
    "gcm",
    "storage",
    "notifications",
    "fontSettings"
  ],
  "version": "5.5"
}
