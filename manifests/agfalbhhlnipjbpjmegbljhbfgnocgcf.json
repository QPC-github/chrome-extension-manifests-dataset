{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "1.2",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "default_locale": "tr",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "settings.html"
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "webRequestBlocking",
    "storage"
  ],
  "icons": {
    "16": "images/img16.png",
    "32": "images/img32.png",
    "48": "images/img48.png",
    "128": "images/img128.png"
  }
}
