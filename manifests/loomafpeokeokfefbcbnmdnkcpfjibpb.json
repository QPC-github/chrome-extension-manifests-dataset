{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon@16.png",
      "32": "img/icon@32.png"
    },
    "default_popup": "popup.html",
    "default_title": "Currency Exchange Calculator"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://free.currencyconverterapi.com; object-src 'self'",
  "description": "The best currency exchange calculator on the web. This extension is easy to use and provides instantaneous and accurate results.",
  "icons": {
    "48": "img/icon@48.png",
    "96": "img/icon@96.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Currency Exchange Calculator",
  "permissions": [
    "tabs",
    "http://free.currencyconverterapi.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4",
  "web_accessible_resources": [
    "css/popup.css"
  ]
}
