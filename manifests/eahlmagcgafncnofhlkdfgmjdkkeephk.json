{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "ClickLearn ApS",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "ClickLearn Chrome Assist for web assistance"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "description": "ClickLearn Chrome Assist for web assistance",
  "icons": {
    "16": "icon.png",
    "24": "icon-24.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "ClickLearn Chrome Assist",
  "permissions": [
    "tabs"
  ],
  "optional_permissions": [],
  "version": "11.0.0.75"
}
