{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Center",
  "version": "0.5",
  "description": "It display bookmarks.",
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "bookmark"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
