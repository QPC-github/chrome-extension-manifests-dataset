{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Hover",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Open Youtube movies on mouse over. Works on many sites (Facebook, Twitter, Flickr, Reddit, Amazon, Tumblr, etc).",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "jquery-2.0.0.min.js",
        "myscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "play-button.png",
    "play-button-hover.png"
  ]
}
