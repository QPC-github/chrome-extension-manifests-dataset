{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2020.7.22.0",
  "short_name": "AutoBcc",
  "name": "Auto BCC for Gmailâ„¢",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "all_frames": false,
      "run_at": "document_end",
      "css": [
        "sweetalert.css"
      ],
      "js": [
        "jquery.js",
        "sweetalert.js",
        "bcc.js"
      ],
      "matches": [
        "https://mail.google.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Auto Bcc"
  },
  "permissions": [
    "https://*.autobcc.com/*",
    "https://mail.google.com/*"
  ],
  "manifest_version": 2
}
