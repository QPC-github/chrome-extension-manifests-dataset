{
  "manifest_version": 2,
  "background": {
    "scripts": [
      "TabUtils.js",
      "TabUtilsAppend.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "default_locale": "en_US",
  "description": "__MSG_extDesc__",
  "icons": {
    "128": "rg128.png",
    "16": "rg16.png",
    "32": "rg32.png",
    "64": "rg64.png"
  },
  "name": "Rocker Gesture",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.9"
}
