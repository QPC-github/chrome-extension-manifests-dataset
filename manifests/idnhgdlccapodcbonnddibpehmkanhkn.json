{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JIRA Chrome Tab",
  "description": "This is a test extension for JIRA",
  "version": "0.2",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "48": "extIcon48.png",
    "128": "extIcon128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
