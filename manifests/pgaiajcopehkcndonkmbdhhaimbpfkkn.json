{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Iterando Screen Sharing",
  "author": "Iterando<developers@iterando.mx>",
  "version": "0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "DOCCTIUS Sharing Screen Extension",
  "homepage_url": "https://docttius.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://docttius.localhost.com/*",
      "https://docttius.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
