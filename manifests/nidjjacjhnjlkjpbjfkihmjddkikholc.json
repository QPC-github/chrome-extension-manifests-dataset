{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "http://www.amzsharp.com",
  "homepage_url": "http://www.amzsharp.com",
  "manifest_version": 2,
  "name": "AMZSharp for Amazon Sellers",
  "version": "0.1.0",
  "description": "帮助亚马逊卖家同步订单Feedback，匹配差评Review订单ID，查看竞争对手Listing每日销量、库存，关键词排名变化、历史价格变化",
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "background": {
    "scripts": [
      "libs/jquery/3.2.1/jquery.js",
      "js/dexie.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "这是AMZSharp插件",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://sellercentral.amazon.com/*",
        "https://sellercentral.amazon.ca/*",
        "https://sellercentral.amazon.com.mx/*",
        "https://sellercentral.amazon.co.uk/*",
        "https://sellercentral.amazon.de/*",
        "https://sellercentral.amazon.fr/*",
        "https://sellercentral.amazon.it/*",
        "https://sellercentral.amazon.es/*",
        "https://sellercentral-europe.amazon.com/*",
        "https://sellercentral-japan.amazon.com/*"
      ],
      "css": [
        "libs/bootstrap/4.0.0-beta.2/css/bootstrap.amalyze.css",
        "libs/jqueryui/1.12.1/jquery-ui.css",
        "cs/css/main.css",
        "cs/css/font-awesome.css",
        "css/content.css"
      ],
      "js": [
        "libs/handlebars/4.0.11/handlebars.js",
        "libs/popper/1.13.0/popper.js",
        "libs/jquery/3.2.1/jquery.js",
        "libs/jqueryui/1.12.1/jquery-ui.js",
        "libs/bootstrap/4.0.0-beta.2/js/bootstrap.js",
        "js/content-script.js",
        "cs/js/cs.js",
        "modules/index.checker/cs/js/template.js",
        "cs/js/init.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "alarms",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "inject/*",
    "cs/js/pages/*",
    "cs/tpl/*",
    "cs/img/*",
    "cs/css/*",
    "fonts/*",
    "sounds/*",
    "modules/index.checker/cs/tpl/*",
    "modules/keyword.amalyzer/cs/tpl/*"
  ],
  "default_locale": "zh_CN"
}
