{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LEGA B Start",
  "description": "Personalizza la tua pagina iniziale e tutte le nuove schede con start.legab.it.",
  "version": "2.9",
  "chrome_settings_overrides": {
    "homepage": "http://start.legab.it/",
    "startup_pages": [
      "http://start.legab.it/"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "start.html"
  },
  "permissions": [
    "webRequest",
    "cookies",
    "webRequestBlocking",
    "*://*.legab.it/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  }
}
