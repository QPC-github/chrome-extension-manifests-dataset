{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Minimal Astronomy Picture of the Day (APOD)",
  "version": "1.1.1",
  "description": "Get a stunning NASA astronomy picture each day when you open a new tab.",
  "icons": {
    "16": "icons/apod-icon-16.png",
    "32": "icons/apod-icon-32.png",
    "48": "icons/apod-icon-48.png",
    "128": "icons/apod-icon-128.png"
  },
  "author": "Jack Yeh",
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
