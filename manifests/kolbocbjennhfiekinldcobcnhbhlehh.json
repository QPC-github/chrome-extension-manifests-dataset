{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Курсы валют онлайн",
  "description": "Онлайн курсы доллара к рублю, евро к рублю и евро к доллару",
  "version": "1.2",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "bg.js"
    ]
  },
  "permissions": [
    "storage"
  ]
}
