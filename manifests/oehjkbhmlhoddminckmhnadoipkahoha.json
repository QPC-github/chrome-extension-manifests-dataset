{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ads Tracker",
  "description": "Captures Youtube Video ads and send the details to the server.",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "debugger",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Ads Tracker"
  },
  "manifest_version": 2
}
