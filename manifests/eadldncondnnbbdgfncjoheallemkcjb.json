{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Skip Google Redirect",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Skips the Google redirect pages and takes you straight there",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.google.com/url*"
  ]
}
