{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DRest Utility",
  "version": "1.0",
  "description": "Rest utility for testing request and response for the backend developers. This utility is developed by Xoriant Solutions.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs",
    "background",
    "<all_urls>"
  ]
}
