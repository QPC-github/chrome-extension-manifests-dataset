{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Enable Debug URL",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Appends \"debug=1\" parameter to the URL",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "omnibox": {
    "keyword": "go"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
