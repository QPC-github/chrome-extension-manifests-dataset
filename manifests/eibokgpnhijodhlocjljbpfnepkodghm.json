{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Watch My Video",
  "description": "lightening john",
  "version": "1.0",
  "browser_action": {
    "default_icon": "jack.jpg",
    "default_title": "WATCH",
    "js": [
      "audio.js"
    ]
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
