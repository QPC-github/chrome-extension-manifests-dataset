{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rabbit Chat Resizer",
  "description": "Resizes the Rabbit Chat window.  Also abolishes the ugly emoji.  Made for Maren! <3",
  "version": "1.3.69",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "browser_action.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://rabb.it/*",
        "https://rabb.it/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://rabb.it/*"
  ]
}
