{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Knockout+",
  "version": "0.1.1",
  "options_page": "pages/options.html",
  "background": {
    "scripts": [
      "js/optionDefinitions.js",
      "js/storage.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://knockout.chat/*"
      ],
      "run_at": "document_end",
      "js": [
        "js/optionDefinitions.js",
        "js/storage.js",
        "js/http.js",
        "js/customCSS.js",
        "js/subscriptions.js",
        "js/content.js"
      ],
      "css": [
        "css/userprofile.css"
      ]
    }
  ],
  "icons": {
    "128": "images/icon.png",
    "64": "images/icon.png",
    "32": "images/icon.png",
    "16": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "storage",
    "https://knockout.chat/"
  ]
}
