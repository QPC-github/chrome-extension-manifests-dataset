{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "[NFNDZ]Garmin Activities Batch Downloader",
  "description": "This extension can download multiple activities from Garmin Connect",
  "version": "1.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "[NFNDZ]Garmin Activities Batch Downloader"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "alarms",
    "webRequest",
    "cookies",
    "downloads",
    "tabs",
    "<all_urls>"
  ]
}
