{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "description": "Save a bunch of favorites to access quickly!",
  "name": "NewTab",
  "permissions": [
    "management",
    "storage"
  ],
  "icons": {
    "48": "icon48.jpg",
    "128": "icon128.jpg",
    "16": "icon16.jpg"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "version": "1.22"
}
