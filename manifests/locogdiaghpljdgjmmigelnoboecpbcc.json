{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hacker News",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "16": "images/hacker_logo.png",
    "48": "images/hacker_logo.png",
    "128": "images/hacker_logo.png"
  },
  "description": "Trying a new look for HN.",
  "homepage_url": "http://www.hackernewsletter.com/",
  "background": {
    "scripts": [
      "js/script.js",
      "js/jquery.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://news.ycombinator.com/"
  ],
  "browser_action": {
    "default_title": "HN",
    "default_icon": "images/hacker_logo.png",
    "default_popup": "popup.html"
  }
}
