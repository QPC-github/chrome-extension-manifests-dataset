{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "QR Code PopUp",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "name": "QR Code PopUp",
  "permissions": [
    "management",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.4"
}
