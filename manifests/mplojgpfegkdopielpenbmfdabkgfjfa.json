{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "Read few read resources at the same time with our help",
  "name": "Read reminds",
  "version": "0.3",
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
