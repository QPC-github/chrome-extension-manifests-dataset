{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LexaaOff",
  "version": "1.1",
  "description": "Ne rate plus aucun live de LexaaOff",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "64": "images/icon.png"
  },
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "notifications",
    "https://www.thedark.xyz/stream/lexaaoff/*"
  ]
}
