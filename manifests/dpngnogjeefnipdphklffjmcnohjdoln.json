{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Красивая Новая Вкладка с Яндексом™",
  "short_name": "Яндекс-Фоны",
  "description": "Укрась Новую Вкладку с Яндексом роскошными фонами из разных категорий на любой вкус и цвет.",
  "version": "0.65",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "unlimitedStorage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab-init.html"
  },
  "browser_action": {
    "default_title": "Красивая Новая Вкладка с Яндексом",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/logo-16px.png",
    "48": "img/logo-48px.png",
    "128": "img/logo-128px.png"
  },
  "background": {
    "scripts": [
      "lib/jquery-1.11.1.min.js",
      "js/tools.js",
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.googleadservices.com; object-src 'self' https://www.googleadservices.com;",
  "manifest_version": 2
}
