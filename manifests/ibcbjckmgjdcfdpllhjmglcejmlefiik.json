{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "bmswap",
  "description": "bmswap - an awesome way to share bookmarks.",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "bmswap.js"
    ]
  },
  "icons": {
    "128": "bmswap.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "bmswap.com",
    "default_icon": "bmswap_s.png"
  }
}
