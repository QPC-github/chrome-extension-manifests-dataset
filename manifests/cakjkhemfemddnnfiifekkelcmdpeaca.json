{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Facebook TrEnder",
  "description": "This extension deletes the 'Now Trending' section on facebook",
  "version": "1.0.4",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "delete.js"
      ]
    }
  ]
}
