{
  "background": {
    "scripts": [
      "lib/jquery.js",
      "lib/lodash.js",
      "source/company/default_check.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/c-logo.png",
      "128": "icons/c-logo.png"
    },
    "default_title": "Clodura.AI"
  },
  "content_scripts": [
    {
      "css": [
        "css/new_extension.css",
        "css/new_ui.css"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "js/jquery/jquery-ui.min.js",
        "js/mutation-summary.js",
        "js/sweetalert/dist/sweetalert.min.js",
        "source/common/constants.js",
        "source/content-script.js",
        "source/common/api.js",
        "source/linkedin/company.js",
        "source/linkedin/prospect.js",
        "source/common/http.js",
        "source/sales_nav/prospect.js",
        "source/sales_nav/company.js",
        "source/common/company_parse.js",
        "source/common/prospect_parse.js"
      ],
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Clodura is a Sales triggers and Automation engine.",
  "icons": {
    "128": "icons/c-logo.png",
    "16": "icons/c-logo.png",
    "48": "icons/c-logo.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "30",
  "name": "Clodura",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "<all_urls>",
    "activeTab"
  ],
  "short_name": "Clodura",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1.5.7",
  "web_accessible_resources": [
    "fonts/GothamLightRegular.otf",
    "fonts/GothamMediumRegular.ttf",
    "fonts/fontawesome-webfont.eot",
    "fonts/fontawesome-webfont.svg",
    "fonts/fontawesome-webfont.ttf",
    "fonts/fontawesome-webfont.woff",
    "source/common/constants.js",
    "fonts/fontawesome-webfont.woff2",
    "fonts/FontAwesome.otf",
    "img/logo.png",
    "img/nopic.png",
    "img/ajax_loader.gif",
    "img/Add.png",
    "/img/Ico_Edit2.png",
    "/img/tick.png",
    "/img/Search_logout2.png",
    "/img/loader1.gif",
    "/img/person_add.jpeg",
    "img/clogocheck.png",
    "/img/error_icon.png",
    "/img/blacklist.jpeg",
    "/img/add.png",
    "/img/added.png",
    "/img/added_comp.png",
    "/source/extension_index.html",
    "/source/company/new_login.html",
    "/source/company/company.html",
    "/source/company/person.html"
  ]
