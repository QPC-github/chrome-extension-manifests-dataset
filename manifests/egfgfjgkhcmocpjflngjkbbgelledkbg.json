{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CoinBought",
  "short_name": "CoinBought",
  "version": "1.200",
  "description": "Buy anything off of Amazon with Bitcoin, Ethereum, Litecoin, Monero, Bitcoin Cash and more!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.amazon.com/*",
        "*://www.amazon.co.uk/*",
        "*://www.amazon.de/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "*://www.amazon.com/*",
    "*://www.amazon.co.uk/*",
    "*://www.amazon.de/*"
  ],
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "key": ""
}
