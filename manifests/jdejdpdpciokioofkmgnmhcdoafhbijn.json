{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GSMspain Chrome Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Extension Chrome para foros GSMspain",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.jpg"
  },
  "browser_action": {
    "default_icon": "logo48.png",
    "default_title": "GSMspain Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "http://www.gsmspain.com/foros"
}
