{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "do.la extension",
  "version": "0.2.2",
  "description": "do.la bookmark extension",
  "permissions": [
    "storage",
    "http://do.la/api/s1/r.php",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "Save bookmark",
    "default_icon": "img/plus.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
