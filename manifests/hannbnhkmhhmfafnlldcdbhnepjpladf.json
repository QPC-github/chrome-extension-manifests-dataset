{
  "background": {
    "scripts": [
      "lib/jquery.js",
      "options/lib/store.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "lib/jquery.js",
        "content.documentEnd.js"
      ],
      "matches": [
        "*://*.dieverdammten.de/"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/content.css"
      ],
      "js": [
        "content.documentStart.js"
      ],
      "matches": [
        "*://*.dieverdammten.de/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Helferlein bietet einige nützliche Funktionen für das Survival-Browsergame DieVerdammten.de",
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "name": "DieVerdammten Helferlein",
  "options_page": "options/index.html",
  "permissions": [
    "*://*.dieverdammten.de/*",
    "*://*.dieverdammten.net/*",
    "*://fm.omameier.net/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0",
  "web_accessible_resources": [
    "css/compactMode.css",
    "css/expertMode.css",
    "img/status.png",
    "img/icon-fataMorgana.png",
    "img/icon-ovalOffice.png"
  ]
}
