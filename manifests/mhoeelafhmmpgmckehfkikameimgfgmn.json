{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tools",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "logo/19.png",
    "default_popup": "popup.html"
  },
  "description": "some tools for fun. ",
  "icons": {
    "128": "logo/128.png",
    "48": "logo/48.png",
    "16": "logo/16.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
