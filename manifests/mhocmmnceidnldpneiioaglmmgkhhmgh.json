{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ClouDoc Chrome Ext",
  "version": "0.0.0.23",
  "description": "ClouDoc Crome Ext",
  "background": {
    "persistent": true,
    "scripts": [
      "get_current_url.js"
    ]
  },
  "browser_action": {
    "default_icon": "cloudoc.png",
    "default_popup": "cloudoc.html"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "tabs",
    "management"
  ],
  "manifest_version": 2
}
