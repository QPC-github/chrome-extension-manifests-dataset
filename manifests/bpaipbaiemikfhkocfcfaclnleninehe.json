{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "sodaG",
  "version": "1.0.3",
  "description": "Returns sodaG (and friends) to Twitch chat.",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "32": "sodaG_32.png",
    "64": "sodaG_64.png",
    "128": "sodaG_128.png"
  }
}
