{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.4",
  "default_locale": "en",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "options.html"
  ]
}
