{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BBE_to_YouGlish",
  "short_name": "BTY",
  "version": "2.5",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "bookshelf.png"
  },
  "browser_action": {
    "default_icon": "bookshelf.png"
  }
}
