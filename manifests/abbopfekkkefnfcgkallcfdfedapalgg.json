{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "This extension disables the annoying ctrl + scroll zoom",
  "manifest_version": 2,
  "name": "Stop annoying ctrl + scroll Zoom",
  "version": "1.3",
  "web_accessible_resources": [
    "StopZoom.js"
  ]
}
