{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "360谷谷哒",
  "version": "0.3",
  "author": "小影",
  "description": "使用由 360 网站卫士 CDN 驱动的前端公共库替代网站中 Google 公共库&字体库的请求。",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://ajax.googleapis.com/",
    "http://fonts.googleapis.com/"
  ]
}
