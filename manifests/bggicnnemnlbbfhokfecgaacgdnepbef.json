{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Geometry Dash Popup On Scratch",
  "version": "1.0",
  "author": "XXXtwoXXX (Scratch Username)",
  "description": "You can play Geometry Dash on an extension using Scratch!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
