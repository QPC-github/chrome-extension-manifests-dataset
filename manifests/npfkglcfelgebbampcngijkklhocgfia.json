{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "Adds links to Jira tickets on GitHub pull request pages",
  "name": "GitHub-Jira Link",
  "version": "0.1.11",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://www.github.com/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "options.html",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png"
    }
  }
}
