{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Org Search for Salesforce",
  "description": "Search anything quickly in your Salesforce Org",
  "version": "1.5.8",
  "icons": {
    "16": "sfdcLogo16.png",
    "48": "sfdcLogo48.png",
    "128": "sfdcLogo128.png"
  },
  "background": {
    "scripts": [
      "search_multi.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options_multi.html",
  "permissions": [
    "contextMenus",
    "storage"
  ]
}
