{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search Aria People",
  "description": "To use, type 'aria' plus a search term into the Omnibox.",
  "version": "1.1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://people.us.oracle.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "omnibox": {
    "keyword": "aria"
  },
  "manifest_version": 2
}
