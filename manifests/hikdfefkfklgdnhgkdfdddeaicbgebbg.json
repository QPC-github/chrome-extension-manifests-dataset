{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Talkpush",
  "version": "1.1.4",
  "manifest_version": 2,
  "icons": {
    "16": "icon/Icon-Small.png",
    "48": "icon/Icon-Small-50.png",
    "128": "icon/Icon-72@2x.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/Icon-Small.png",
      "38": "icon/Icon-Small-50.png"
    },
    "default_popup": "html/action.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ]
}
