{
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/icons/imageinfo-16.png",
    "default_title": "Wooaliexpress Importer - Woocommerce Aliexpress Importer"
  },
  "content_scripts": [
    {
      "js": [
        "assets/js/libs/jquery.js",
        "scripts/content/content_import_page.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/stylesheets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/common.js",
        "scripts/content/product.js"
      ],
      "matches": [
        "*://*.aliexpress.com/item/*",
        "*://*.aliexpress.com/store/product/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/stylesheets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/common.js",
        "scripts/content/cart.js"
      ],
      "matches": [
        "*://shoppingcart.aliexpress.com/shopcart/shopcartDetail.htm*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/stylesheets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/common.js",
        "scripts/content/checkout.js"
      ],
      "matches": [
        "*://shoppingcart.aliexpress.com/order/*"
      ],
      "run_at": "document_end"
    },
    {
      "all_frames": true,
      "js": [
        "assets/js/libs/jquery.js",
        "scripts/content/all_pages.js"
      ],
      "matches": [
        "http://*.aliexpress.com/*",
        "https://*.aliexpress.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Extention for Wooaliexpress Importer plugin. Features: import products variations and automatically order fulfillment.",
  "homepage_url": "https://scapasolution.com/downloads/woocommerce-aliexpress-importer/",
  "icons": {
    "128": "assets/icons/imageinfo-128.png",
    "16": "assets/icons/imageinfo-16.png",
    "48": "assets/icons/imageinfo-48.png"
  },
  "manifest_version": 2,
  "name": "Wooaliexpress Importer",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.10",
  "web_accessible_resources": [
    "scripts/content/wpe_api.js",
    "assets/images/logo-small.png"
  ]
}
