{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "VF Xtn",
  "name": "VirtualFolders Extension",
  "description": "Add the current browser url to VirtualFolders database. These URLs can then be synched to the iOS VirtualFolders App",
  "version": "1.1",
  "browser_action": {
    "default_icon": "VFextn_icon19.png",
    "default_popup": "VFExtn.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "VFOptions.html",
    "chrome_style": true
  }
}
