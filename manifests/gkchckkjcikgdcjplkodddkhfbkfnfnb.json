{
  "background": {
    "scripts": [
      "analytic.js",
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_popup": "popup.html",
    "default_title": "IIUI Chrome"
  },
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "This extension is specialy made for IIUI to provide a, new tab with search, quick links interface and speech engine.",
  "homepage_url": "http://mjmirza.com",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png",
    "64": "64.png"
  },
  "manifest_version": 2,
  "name": "IIUI",
  "omnibox": {
    "keyword": "a"
  },
  "permissions": [
    "notifications",
    "unlimitedStorage",
    "contextMenus",
    "tts",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.6",
  "web_accessible_resources": [
    "64.png"
  ]
}
