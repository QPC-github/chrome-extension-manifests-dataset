{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Omaha News Diet",
  "description": "Take a break from the naitonal news and start a Omaha news diet!",
  "version": "1.03",
  "author": "Inside Omaha",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Start a Omaha news diet.",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://code.getmdl.io/ https://use.fontawesome.com/ https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "webRequest",
    "webNavigation",
    "storage",
    "<all_urls>",
    "webRequestBlocking",
    "background"
  ]
}
