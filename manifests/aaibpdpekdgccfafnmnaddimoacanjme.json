{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Baby Animals HD Wallpaper New Tab Theme",
  "version": "4.0.7",
  "description": "Enjoy amazing HD images of baby animals kittens deer goats puppies and more on every new tab page!",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Baby Animals HD Wallpaper New Tab Theme",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
