{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gmail title reorder",
  "description": "This extension changes the order of the unread count in the gmail title bar",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "bg.js"
      ]
    }
  ],
  "permissions": [
    "http://mail.google.com/",
    "https://mail.google.com/"
  ]
}
