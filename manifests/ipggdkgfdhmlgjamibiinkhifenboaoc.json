{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Replace your new tab with colorful pixel wallpapers that changes based on time of day, a digital clock and a personal greeting.",
  "icons": {
    "16": "img/icon/16.png",
    "48": "img/icon/48.png",
    "128": "img/icon/128.png"
  },
  "manifest_version": 2,
  "name": "BitTab",
  "short_name": "BitTab",
  "permissions": [
    "storage"
  ],
  "version": "1.0.0"
}
