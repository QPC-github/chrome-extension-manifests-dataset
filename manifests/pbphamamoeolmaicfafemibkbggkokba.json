{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chroma fullscreen dark",
  "version": "1.0",
  "description": "make Razer Chroma LEDs go dark when entering fullscreen",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
