{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tabs",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Save your open tabs.",
  "icons": {
    "19": "pass-19.png"
  },
  "browser_action": {
    "default_icon": "pass-19.png",
    "default_title": "Tabs",
    "default_popup": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js",
      "main.js"
    ]
  }
}
