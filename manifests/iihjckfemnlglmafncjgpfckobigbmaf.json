{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timeout",
  "version": "1.2",
  "description": "Hey, take a break.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "notifications",
    "tabs",
    "idle"
  ],
  "browser_action": {
    "default_title": "Timeout",
    "default_icon": "16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "short_name": "Timeout",
  "options_page": "options.html",
  "manifest_version": 2,
  "web_accessible_resources": [
    "48.png"
  ]
}
