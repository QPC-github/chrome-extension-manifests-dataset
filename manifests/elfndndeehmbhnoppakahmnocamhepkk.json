{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tiny-go",
  "description": "To use, type 'g' plus a keyword",
  "icons": {
    "16": "tinygo-16.png",
    "48": "tinygo-48.png",
    "128": "tinygo-128.png",
    "512": "tinygo-512.png"
  },
  "permissions": [
    "storage"
  ],
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "g"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
