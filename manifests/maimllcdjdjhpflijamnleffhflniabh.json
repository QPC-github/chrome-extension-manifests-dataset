{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CatFud",
  "description": "This extension fetches cat stuff including a picture/gif, a random adoption link, and cat trivia",
  "version": "0.0.2",
  "author": "srobertson421",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://thecatapi.com/*",
    "http://catfacts-api.appspot.com/*"
  ]
}
