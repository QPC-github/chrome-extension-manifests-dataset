{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/inject.js"
      ],
      "matches": [
        "*://*.multiviscloud.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Share the screen from your computer with up to 8 people in a video conversation, to show presentations, photos and documents. ",
  "icons": {
    "128": "images/icon_128.png",
    "16": "images/icon_16.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "multiviscloud.com screen sharing",
  "offline_enabled": true,
  "permissions": [
    "desktopCapture",
    "*://multiviscloud.com/*",
    "activeTab",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "short_name": "multiviscloud.com screen sharing",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "8.2.3"
}
