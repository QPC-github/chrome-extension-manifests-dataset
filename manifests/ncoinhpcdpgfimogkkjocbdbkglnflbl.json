{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Netflix Titles Rater",
  "description": "This extension adds IMDb rating to the titles",
  "version": "0.0.1",
  "author": "Harshul Pandav",
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/browse"
      ],
      "js": [
        "jquery.min.js",
        "netflix_extension.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "netflix-icon.png"
  }
}
