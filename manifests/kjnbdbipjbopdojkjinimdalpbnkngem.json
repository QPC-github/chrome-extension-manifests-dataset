{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "For the most awaited and the most anticipated movie of the decade Avengers-Infinity Wars. Enjoy :-)",
  "name": "Avengers Infinity Wars Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_popup": "popupAvengersIW.html",
    "default_title": "Infinity Wars!"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Thanos.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
