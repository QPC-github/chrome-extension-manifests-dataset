{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BitcoinsToBeans",
  "version": "0.0.2",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "bitcoin.png"
  }
}
