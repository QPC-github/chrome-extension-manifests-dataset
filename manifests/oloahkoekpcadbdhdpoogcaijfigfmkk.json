{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "Split Tabs",
  "name": "Split Tabs",
  "description": "Moves all tabs right of the current one into a new window",
  "version": "0.1.1",
  "background": {
    "scripts": [
      "split.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "split19.png",
      "38": "split38.png"
    },
    "tooltip": "Moves all tabs right of the current one into a new window"
  },
  "icons": {
    "16": "split16.png",
    "48": "split48.png",
    "128": "split128.png"
  },
  "permissions": [
    "tabs"
  ]
}
