{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scheduler For WhatsApp - Chrome Extension",
  "version": "2.0",
  "description": "This is required addon for Scheduler For WhatsApp to start for the first time.",
  "browser_action": {
    "default_icon": {
      "19": "logo.png",
      "38": "logo.png"
    },
    "default_title": "Scheduler For WhatsApp",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "https://*.whatsapp.com/*"
  ]
}
