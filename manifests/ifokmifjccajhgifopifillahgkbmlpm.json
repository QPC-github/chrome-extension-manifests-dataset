{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Helppo Screen Sharing(Test Azure)",
  "author": "Helppo",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for http://tokbox.com/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://test-senseionlinetutor.azurewebsites.net/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.test-senseionlinetutor.azurewebsites.net/*"
    ]
  },
  "icons": {
    "128": "Helppo2.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
