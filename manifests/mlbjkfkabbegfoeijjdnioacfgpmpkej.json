{
  "author": "Genesis Digital, LLC",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://*.genndidev.com/*"
    ]
  },
  "description": "WebinarJam screensharing extension to record and broadcast your desktop footage live to your audience.",
  "homepage_url": "https://www.webinarjam.com/",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "WebinarJam Screen Sharing Genndidev",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.5",
  "web_accessible_resources": [
    "icon.png"
  ]
}
