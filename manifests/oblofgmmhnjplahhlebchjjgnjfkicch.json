{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pundit",
  "description": "Pundit replaces Chrome new tab page with daily top news stories from The New York Times",
  "version": "0.2",
  "permissions": [
    "http://pundit-nyt-reader.herokuapp.com/",
    "http://pundit-nyt-reader.herokuapp.com/pundit",
    "http://www.w3.org/2000/svg",
    "http://www.google.com/jsapi"
  ],
  "background": {
    "scripts": [
      "pundit.js"
    ]
  },
  "icons": {
    "48": "pundit48.png",
    "128": "pundit128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
