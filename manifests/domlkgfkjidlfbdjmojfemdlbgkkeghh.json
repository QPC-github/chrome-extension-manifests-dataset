{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CaMaK",
  "description": "Ne manquez plus un live de CaMaK grâce à cette extension. Prochain live réseaux sociaux message perso et autres.",
  "version": "1.6.4",
  "permissions": [
    "storage",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "js/notif.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-38.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "38": "img/icon-38.png",
    "16": "img/icon.png"
  }
}
