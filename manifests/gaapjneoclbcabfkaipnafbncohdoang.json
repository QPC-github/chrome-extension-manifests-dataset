{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bettermark",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Save any link for later with bettermark.",
  "homepage_url": "https://bettermarkapp.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "Save page to bettermark"
  },
  "permissions": [
    "https://bettermarkapp.com/",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "inject.js"
    ]
  },
  "web_accessible_resources": [
    "logo_sq.png",
    "extension_fonts.css"
  ]
}
