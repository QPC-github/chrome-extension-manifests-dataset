{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "/js/bookmarksEventPage.js"
    ]
  },
  "browser_action": {
    "default_icon": "/img/icon128.png",
    "default_title": "Bookmarks Sidebar ( Ctrl+B or âŒ˜+B )"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "windows": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "content_scripts": [
    {
      "css": [
        "/css/bookmarksStyles.css"
      ],
      "js": [
        "/js/vendor/jquery-2.2.3.min.js",
        "/js/vendor/underscore-min.js",
        "/js/vendor/classie.js",
        "/js/bookmarksContentScript.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "description": "A tree-view bookmark sidebar which allows for easier navigation",
  "icons": {
    "128": "/img/icon128.png",
    "16": "/img/icon16.png",
    "48": "/img/icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "9",
  "name": "Candy Bookmarks Sidebar",
  "permissions": [
    "<all_urls>",
    "chrome://favicon/",
    "http://www.google.com/",
    "bookmarks",
    "tabs",
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "http://*/",
    "https://*/",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.31",
  "web_accessible_resources": [
    "*.*"
  ]
}
