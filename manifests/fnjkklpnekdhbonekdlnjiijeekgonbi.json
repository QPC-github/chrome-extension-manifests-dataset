{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MagicExtension",
  "description": "Ne ratez aucun live ! twitch.tv/MagicvdK",
  "version": "3.0",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "image/MagicLogo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "image/MagicLogo128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/Live.js"
    ]
  }
}
