{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "EZSub",
  "description": "Client side comic subscriptions",
  "version": "14",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "img/green-balloon-icon-16.png",
    "128": "img/green-balloon-icon-128.png"
  }
}
