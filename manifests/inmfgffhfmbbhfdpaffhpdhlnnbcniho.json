{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "wiki.html",
    "default_title": "Wikipedia Companion"
  },
  "content_security_policy": "script-src 'self' https://*.wikipedia.org https://*.wikimedia.org; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "Lookup Companion for Wikipedia(Fixed version)",
  "short_name": "Lookup Companion for Wikipedia(Fixed version)",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ],
  "key": "",
  "version": "2.2"
}
