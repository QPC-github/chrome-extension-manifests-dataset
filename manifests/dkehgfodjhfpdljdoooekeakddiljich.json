{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Forwards.Cloud",
  "description": "Replaces your New Tab default page with a Forward from Forwards.Cloud",
  "manifest_version": 2,
  "version": "0.1.1",
  "author": "Surya Chandra Rao Gandreddi, Forwards.Cloud",
  "chrome_url_overrides": {
    "newtab": "forwards.html"
  },
  "offline_enabled": true,
  "browser_action": {
    "default_title": "Forwards.Cloud"
  },
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "permissions": [
    "storage"
  ]
}
