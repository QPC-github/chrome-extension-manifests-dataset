{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.5",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "coffeecup16.png",
    "48": "coffeecup48.png",
    "128": "coffeecup128.png"
