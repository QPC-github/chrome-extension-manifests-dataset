{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mapsy",
  "description": "Searching an adress on Google Maps at the tip of your finger!",
  "version": "0.0.1",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}
