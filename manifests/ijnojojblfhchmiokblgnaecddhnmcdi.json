{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "enabled.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "flickr.css"
      ],
      "js": [
        "enselect.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "description": "The extension re-enables the copy/paste functionality, context menu and native highlighting feature on web pages.",
  "name": "HLP-Trick",
  "version": "1.1",
  "icons": {
    "16": "enabled.png",
    "128": "ddcheat.png"
  }
}
