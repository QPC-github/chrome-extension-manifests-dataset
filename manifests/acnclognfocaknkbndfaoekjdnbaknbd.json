{
  "name": "Partis",
  "description": "Partis",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css.css"
      ],
      "js": [
        "partis.js"
      ],
      "matches": [
        "http://www.partis.si/*",
        "http://partis.si/*"
      ]
    }
  ],
  "icons": {
    "16": "icon48.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "*://*.partis.si/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.5.2"
}
