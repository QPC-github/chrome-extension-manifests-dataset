{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TribuneUnchained",
  "description": "This gets rid of log-in popups and shows article text by default on loogooteetribune.com",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://loogooteetribune.com/*"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
