{
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "datapath.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "datapath.js"
      ],
      "matches": [
        "https://*.console.aws.amazon.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "This extension adds the Datapath.io console to the AWS console",
  "key": "",
  "manifest_version": 2,
  "name": "Datapath.io AWS Console",
  "permissions": [
    "tabs",
    "https://*.console.aws.amazon.com/*",
    "https://console.datapath.io/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
