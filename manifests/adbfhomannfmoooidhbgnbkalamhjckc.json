{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TCL Analytics",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "description": "Caching PowerBI",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon_128.jpg",
    "default_title": "TCL Analytics"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.app.powerbi.com/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "tabs",
    "webRequest",
    "activeTab",
    "storage",
    "management"
  ]
}
