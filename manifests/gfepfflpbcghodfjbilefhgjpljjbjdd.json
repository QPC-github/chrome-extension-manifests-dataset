{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dial2Web",
  "description": "Enter Phone number to access URL or Mailbox",
  "version": "1.0.3",
  "icons": {
    "16": "dial2web.png",
    "48": "dial2web.png",
    "128": "dial2web.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
