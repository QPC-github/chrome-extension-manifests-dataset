{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wellness Telecom Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with Jitsi Meet adapted version for Wellness Telecom.",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "wt_logo16x16.png",
    "48": "wt_logo48x48.png",
    "128": "wt_logo128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://89.140.10.39/*"
    ]
  }
}
