{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "description": "불편한 스팀잇 직접 뜯어 고쳐보자 !!",
  "icons": {
    "128": "images/icon_128.png",
    "16": "images/icon_128.png",
    "32": "images/icon_128.png",
    "48": "images/icon_128.png"
  },
  "manifest_version": 2,
  "name": "스팀잇 글쓰기 확장팩",
  "page_action": {
    "default_icon": {
      "128": "images/icon_128.png",
      "16": "images/icon_128.png",
      "32": "images/icon_128.png",
      "48": "images/icon_128.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://steemit.com/"
  ],
  "version": "0.0.3"
}
