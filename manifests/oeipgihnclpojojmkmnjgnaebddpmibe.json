{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Plus Right Stuff Hider",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Hides the content (What's Trending, etc.) and the Chat thing on the right side of Google+",
  "content_scripts": [
    {
      "matches": [
        "https://plus.google.com/*"
      ],
      "css": [
        "noRightSide.css"
      ]
    }
  ]
}
