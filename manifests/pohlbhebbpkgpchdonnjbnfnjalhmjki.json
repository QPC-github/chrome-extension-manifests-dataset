{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NUBraim Bookmark",
  "description": "NUBraim 書籤加入工具",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "NUBraim Bookmark",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
