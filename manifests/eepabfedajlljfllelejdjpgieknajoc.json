{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Just Raleigh",
  "version": "1.0",
  "description": "Replaces 'Raleigh-Durham' with 'Raleigh'",
  "icons": {
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
