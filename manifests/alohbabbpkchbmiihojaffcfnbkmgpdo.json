{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "SecureBrain",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "20": "icon20.png",
      "48": "icon48.png"
    },
    "default_title": "PhishWall"
  },
  "description": "PhishWall Extension",
  "icons": {
    "20": "icon20.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "PhishWall",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "version": "6.1.21.0"
}
