{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/jquery-1.11.0.min.js",
      "js/background.js"
    ]
  },
  "name": "NirvanaMC Network Extension",
  "version": "1.2",
  "description": "The Google Chrome Extension for NirvanaMC Players",
  "short_name": "NirvanaMC",
  "author": "Luke Brown",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "permissions": [
    "http://app.NirvanaMC.info/"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
