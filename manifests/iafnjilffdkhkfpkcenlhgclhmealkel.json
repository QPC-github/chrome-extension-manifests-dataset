{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Tweet Day",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Finally, you can have no tweet day, at least on your computer",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "tweet_nothing.css"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "tweet_nothing_128.png"
  }
}
