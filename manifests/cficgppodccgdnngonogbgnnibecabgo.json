{
  "author": "Thomas Edwards",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "An easy to use and useful Dashboard for Google Chrome",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Ilkotech Dashboard",
  "permissions": [
    "bookmarks",
    "management",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
