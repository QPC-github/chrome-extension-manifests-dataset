{
  "background": {
    "scripts": [
      "cmd.js",
      "kikf.js"
    ]
  },
  "description": "You can use PrivSearcher to prevent big websites from tracking your searches online",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "PrivSearcher",
  "permissions": [
    "chrome://favicon/",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "cookies",
    "topSites",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.1"
}
