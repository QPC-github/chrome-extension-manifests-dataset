{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Untick",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tired of unchecking all checkboxes by hand? Install this extension",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
