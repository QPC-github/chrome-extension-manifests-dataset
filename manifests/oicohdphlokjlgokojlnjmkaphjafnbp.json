{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Changemoji",
  "description": "Changes emojis into emoticons.",
  "version": "1.0",
  "browser_action": {
    "default_title": "Changemoji - change emojis into emoticons",
    "default_icon": {
      "32": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://*.roblox.com/*"
  ]
}
