{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cancel Loading Pages at Startup",
  "description": "This extension cancels loading pages at startup until tab is selected.",
  "version": "0.0.0.6",
  "author": "wasd",
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "cancel.js"
    ]
  }
}
