{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tickld Vote Mover",
  "description": "This extension moves the vote buttons AND NOW COMMMENTS TOO to the bottom of the post on tickld.com!",
  "version": "2.0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.tickld.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "http://tickld.com/",
    "storage"
  ],
  "options_page": "options.html"
}
