{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_manifest_name__",
  "short_name": "__MSG_manifest_short_name__",
  "description": "__MSG_manifest_description__",
  "version": "1.99.2",
  "minimum_chrome_version": "20",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "clipboardWrite",
    "*://*/*"
  ],
  "web_accessible_resources": [
    "scripts/jquery/jquery-1.11.1.min.map",
    "sidebar.html",
    "sidebar.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles/chrome-extension-finder.css"
      ],
      "js": [
        "scripts/jquery/jquery-1.11.1.min.js",
        "scripts/jquery/jquery-ui.min.js",
        "scripts/jquery/jquery.md5.js",
        "scripts/gibberish-aes-1.0.0.min.js",
        "scripts/diff-match-patch.js",
        "portable.js",
        "config.js",
        "helpers.js",
        "autotry_helpers.js",
        "autotry_visuals.js",
        "autotry_core.js",
        "engine.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon_inactive_19.png",
      "38": "images/icon_inactive_38.png"
    },
    "default_popup": "",
    "default_title": "__MSG_manifest_default_title__"
  }
}
