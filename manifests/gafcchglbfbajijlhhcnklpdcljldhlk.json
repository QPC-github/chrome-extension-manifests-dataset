{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Search Console Extended",
  "version": "1.4",
  "description": "Extends the new Google Search Console",
  "permissions": [
    "https://search.google.com/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://search.google.com/*"
      ],
      "css": [
        "gsc.css"
      ],
      "js": [
        "gsc.js"
      ]
    }
  ],
  "icons": {
    "128": "gsc-e.png"
  },
  "manifest_version": 2
}
