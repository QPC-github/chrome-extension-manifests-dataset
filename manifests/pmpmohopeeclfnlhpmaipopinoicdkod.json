{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UESP Wiki Redirector",
  "author": "SSyl, modified from proEndreeper's RSWikiRedirector",
  "version": "1.1",
  "description": "Redirects Elder Scrolls Wikia/Fandom requests to UESP",
  "permissions": [
    "storage",
    "*://elderscrolls.wikia.com/*",
    "*://elderscrolls.fandom.com/*",
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "UESP Wiki Redirector",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
