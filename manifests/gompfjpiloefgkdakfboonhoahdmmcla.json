{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitter retweets kopen",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Social Blast heeft goedkope retweets in de aanbieding. Binnen 24 uur na bestelling geleverd!",
  "homepage_url": "https://socialblast.nl/twitter/retweets/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
