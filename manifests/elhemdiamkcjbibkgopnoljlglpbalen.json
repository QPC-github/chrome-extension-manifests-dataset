{
  "browser_action": {
    "default_icon": "total-cart.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/shopping/shortlists/*/l/*"
      ]
    }
  ],
  "description": "Light extension that does only that !",
  "key": "",
  "manifest_version": 2,
  "name": "Total Price for Google Shopping Shortlists",
  "permissions": [
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.04"
}
