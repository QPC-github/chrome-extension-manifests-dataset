{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*",
        "*://*.vk.com/*"
      ],
      "js": [
        "includes/injected.js"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "VK Photo editor",
  "version": "1.0",
  "icons": {
    "92": "92x92.png",
    "64": "64x64.png"
  },
  "developer": {
    "url": "http://vk.com/id86751827",
    "name": "Ilya Snegirev"
  },
  "permissions": [
    "*://vk.com/*",
    "*://*.vk.com/*"
  ],
  "description": "VK Photo editor - фоторедактор для социальной сети ВКонтакте.",
  "background": {
    "page": "index.html"
  }
}
