{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GIFxTesnsion",
  "description": "For imgur and gif fans â€“ the easiest way to discover new gifs :)",
  "version": "1.0.11",
  "author": "GIFxTension",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "vendor/jquery.min.js",
        "vendor/lodash.min.js",
        "vendor/google-analytics-bundle.js",
        "content/script.js"
      ],
      "css": [
        "style/style.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "vendor/jquery.min.js",
      "background/script.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "images/*",
    "content/templates/*.html"
  ],
  "permissions": [
    "https://www.google-analytics.com/",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "tabs",
    "cookies",
    "notifications",
    "contextMenus",
    "webNavigation",
    "unlimitedStorage",
    "storage"
  ]
}
