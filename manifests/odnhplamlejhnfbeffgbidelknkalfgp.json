{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video Embed for Netgamers Forum",
  "icons": {
    "16": "ngi-16.png",
    "48": "ngi-48.png",
    "128": "ngi-128.png"
  },
  "version": "3.2",
  "author": "Crius rev 1.0 modified by grimpressive",
  "manifest_version": 2,
  "description": "Allows users of Netgamers Forum forum to see embedded video content AGAIN",
  "background": {
    "page": "background.html"
  },
  "options_page": "fancy-settings/source/index.html",
  "page_action": {
    "default_icon": "ngi-16.png",
    "default_title": "Video Embedding enabled"
  },
  "content_scripts": [
    {
      "matches": [
        "https://netgamers.it/showthread.php*",
        "https://netgamers.it/showpost.php*",
        "https://*.netgamers.it/showthread.php*",
        "https://netgamers.it/showpost.php*"
      ],
      "js": [
        "video_embed_for_ngi.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://netgamers.it/",
    "tabs"
  ]
}
