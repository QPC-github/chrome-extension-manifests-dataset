{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Device42 AutoLogin",
  "version": "1.2",
  "description": "Autologin from Device42 password manager",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "images/lockicon32.png",
      "48": "images/lockicon48.png",
      "128": "images/lockicon128.png"
    }
  },
  "icons": {
    "32": "images/lockicon32.png",
    "48": "images/lockicon48.png",
    "128": "images/lockicon128.png"
  },
  "manifest_version": 2
}
