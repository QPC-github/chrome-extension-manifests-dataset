{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Comcast Boycotter",
  "version": "1.0",
  "description": "Warns users they are accessing websites owned by Comcast.",
  "background": {
    "persistent": true,
    "scripts": [
      "interrupt.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*/"
  ],
  "manifest_version": 2
}
