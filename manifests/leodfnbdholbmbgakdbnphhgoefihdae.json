{
  "browser_action": {
    "default_icon": {
      "38": "icon/no.png"
    }
  },
  "content_scripts": [
    {
      "css": [
        "css/meh.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://exhentai.org/g/*",
        "https://e-hentai.org/g/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self'  ; object-src 'self'",
  "default_locale": "en",
  "description": "hide description in gallery",
  "icons": {
    "128": "icon/no.png"
  },
  "manifest_version": 2,
  "name": "exHide",
  "permissions": [
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1",
  "web_accessible_resources": [
    "font/*",
    "audio/*",
    "icon/*",
    "image/*",
    "lib/*"
  ]
}
