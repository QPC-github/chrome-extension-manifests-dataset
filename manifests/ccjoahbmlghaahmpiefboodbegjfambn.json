{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LookupContact",
  "version": "0.0.1",
  "description": "Easily find email address and other contact information from a social media profile.",
  "icons": {
    "256": "icons/icon256.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
