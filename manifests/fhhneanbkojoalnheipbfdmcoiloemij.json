{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dart",
  "version": "2.5",
  "manifest_version": 2,
  "description": "Share the web privately with friends.",
  "homepage_url": "http://tossdarts.com",
  "options_page": "src/options/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Dart",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "storage",
    "cookies",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*",
    "http://tossdarts.com"
  ]
}
