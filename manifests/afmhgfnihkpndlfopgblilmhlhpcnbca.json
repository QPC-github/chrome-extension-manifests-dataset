{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One Key Password Manager Application",
  "version": "1.1",
  "manifest_version": 2,
  "description": "This is easy to use file system based password manager.",
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/appbackground.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "<all_urls>",
    "alwaysOnTopWindows",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ]
}
