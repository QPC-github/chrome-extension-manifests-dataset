{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "http://mail.google.com/*",
        "https://www.google.com/calendar/*",
        "https://www.google.com/contacts/*",
        "http://go.collabspot.com/*",
        "https://go.collabspot.com/*",
        "http://www.collabspot.com/*",
        "https://accounts.google.com/*"
      ],
      "run_at": "document_end",
      "css": [
        "lib/gdropdown.css",
        "lib/vendors/chosen/chosen.css",
        "lib/vendors/select2/select2.css",
        "lib/vendors/select2/select2-bootstrap.css",
        "lib/vendors/backbone.modal.css",
        "lib/vendors/backbone.modal.theme.css",
        "css/font-awesome.css",
        "css/toastr.css",
        "css/nprogress.css",
        "css/introjs.css"
      ],
      "js": [
        "insights-min.js"
      ]
    },
    {
      "matches": [
        "http://*.demo.sugarcrm.com/*",
        "https://*.demo.sugarcrm.com/*"
      ],
      "js": [
        "lib/demo_builder/sugar/main.js"
      ]
    },
    {
      "matches": [
        "http://demobuilder.sugarcrmdemo.com/*",
        "https://demobuilder.sugarcrmdemo.com/*"
      ],
      "js": [
        "lib/demo_builder/sugar/scraper.js"
      ]
    }
