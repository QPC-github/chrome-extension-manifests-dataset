  "manifest_version": 2,
  "externally_connectable": {
    "matches": [
      "*://mail.google.com/*",
      "*://wmphighrise.appspot.com/*",
      "*://*.wmphighrise.appspot.com/*",
      "*://www.collabspot.com/*",
      "*://r.clb.pt/*",
      "*://accounts.google.com/*",
      "*://mail-attachment.googleusercontent.com/*",
      "*://wmp-highrise-plugin-2.appspot.com/*"
    ],
    "ids": [
      "*"
    ]
  },
  "version": "2020.9.18.1940",
  "background": {
    "scripts": [
      "back-insights-min.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "tabs",
    "alarms",
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
