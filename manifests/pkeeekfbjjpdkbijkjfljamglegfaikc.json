{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bell Countdown Extension",
  "description": "An extension that displays the time remaining in classes.",
  "version": "4.9.0",
  "content_security_policy": "script-src 'self' https://bell.plus; object-src 'self';",
  "browser_action": {
    "default_icon": "favicons/lime.png",
    "default_popup": "chrome_extension/popup.html",
    "default_title": "Bell Countdown"
  },
  "background": {
    "page": "chrome_extension/background.html"
  },
  "permissions": [
    "alarms",
    "http://bell.plus/*",
    "https://bell.plus/*",
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "http://bell.plus/*",
      "https://bell.plus/*"
    ]
  },
  "icons": {
    "128": "icons/icon128.png"
  },
  "key": ""
}
