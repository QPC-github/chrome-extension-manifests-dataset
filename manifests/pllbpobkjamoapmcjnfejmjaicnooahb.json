{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "indexOf",
  "version": "0.1",
  "description": "Show images on the gallery",
  "permissions": [
    "contentSettings",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "background.js",
        "myscript.js"
      ]
    }
  ]
}
