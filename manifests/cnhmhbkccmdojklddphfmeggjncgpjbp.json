{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "lib/jquery-3.1.0.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Branding Boss"
  },
  "description": "Your own business or personal brand name on a extension. Now customers can always have you in eyesight when browsing the internet.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Branding Boss",
  "permissions": [
    "tabs",
    "storage",
    "identity"
  ],
  "version": "0.0.1",
  "web_accessible_resources": [
    "*/*"
  ],
  "content_security_policy": "script-src 'self' https://*.wistia.com https://*.wistia.net; object-src 'self'"
}
