{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Basecamp ToDo segments",
  "version": "1.0.5",
  "manifest_version": 2,
  "icons": {
    "48": "basecamp-icon48.png",
    "128": "basecamp-icon128.png"
  },
  "description": "Add headings inside todo lists in basecamp.",
  "content_scripts": [
    {
      "matches": [
        "https://*.basecamp.com/*"
      ],
      "js": [
        "jquery.js",
        "content-script.js"
      ]
    }
  ]
}
