{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ScrollUp",
  "description": "ScrollUp and Back on every site you want",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "code.js"
    ]
  },
  "browser_action": {
    "name": "Click to scroll",
    "default_icon": "arrow.png"
  }
}
