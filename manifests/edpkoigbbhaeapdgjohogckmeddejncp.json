{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Capian",
  "description": "Capian helps you make usability evaluations and expert reviews more quickly.",
  "version": "0.1.40",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "rollbar.js",
      "rollbar.umd.nojson.js",
      "rollbar-jquery.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Capian"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "fabric.min.js",
    "canvas.js",
    "capture.html",
    "capture.js",
    "style.css",
    "widget.html",
    "widget.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "commands": {
    "capture-visible": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "MacCtrl+Shift+Y"
      },
      "description": "Capture screen"
    }
  }
}
