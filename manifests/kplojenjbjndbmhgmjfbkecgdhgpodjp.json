{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "二维码生成器",
  "manifest_version": 2,
  "version": "1.1",
  "description": "快捷生成当前页面二维码",
  "browser_action": {
    "default_icon": "icon.new.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon.new.png",
    "48": "icon.new.png",
    "128": "icon.new.png"
  }
}
