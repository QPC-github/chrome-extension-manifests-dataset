{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js",
      "js/panelBackground.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-128.png",
    "default_popup": "popup.html",
    "default_title": "Commanders Act Assistant"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "js": [
        "js/jquery.min.js",
        "js/panelContent.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "js/taglistener.js"
  ],
  "name": "Commanders Act Assistant",
  "description": "Commanders Act Assistant helps to troubleshoot TagCommander implementation and more",
  "homepage_url": "http://www.commandersact.com",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "cookies",
    "storage",
    "notifications",
    "alarms"
  ],
  "version": "1.2.8",
  "manifest_version": 2,
  "devtools_page": "devtools.html"
}
