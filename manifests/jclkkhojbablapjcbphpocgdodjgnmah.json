{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qualibrate",
  "description": "Qualibrate Recording & Playback Chrome Extension",
  "version": "1.87.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "proxy",
    "activeTab",
    "storage",
    "tabs",
    "background",
    "webNavigation",
    "management",
    "webRequest",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-3.3.1.min.js",
      "qfpJsWebsocketClient.js",
      "qfpJsStart.js"
    ]
  }
}
