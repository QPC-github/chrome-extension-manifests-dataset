{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Egg Basket",
  "description": "A bookmark app for developers. Don't just save URLs: save notes or code snippets too.",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "scrambled.html"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "scratch48.png",
    "128": "scratch128.png"
  },
  "permissions": [
    "activeTab",
    "http://codelikeachicken.com/"
  ]
}
