{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZIC Jobs | Agency",
  "short_name": "ZJ Agency",
  "description": "ZIC Jobs provide artificial intelligence powered platforms for agencies to reach unlimited job seekers globally.",
  "version": "0.1.1",
  "offline_enabled": true,
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "cookies",
    "*://*.hralex.com/",
    "*://zicjobs.co.uk/",
    "*://*.google.com/",
    "history"
  ],
  "icons": {
    "128": "ZICJobs.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "zicjobs_icon.png",
    "default_title": "ZICJobs - Agency",
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
