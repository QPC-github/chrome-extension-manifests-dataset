{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Barra Destra",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Distratti da tette e gattini? Stanchi di leggere notizie inutili? Dite no alla barra destra di Repubblica.it",
  "homepage_url": "http://www.behindtheapps.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://www.repubblica.it/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
