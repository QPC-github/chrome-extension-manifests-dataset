{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "upscore - chat",
  "description": "Der chat von upscore.ch macht deine Unterhaltungen noch lebendiger - schnell, unkompliziert und ohne Anmeldung.",
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "chat.upscore.ch",
    "default_icon": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png",
      "48": "favicon-32x32.png",
      "64": "favicon-32x32.png"
    }
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "favicon-32x32.png",
    "64": "favicon-32x32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
