{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Adequa",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png"
    },
    "default_title": "Adequa",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/contentscript/contentscript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "fr",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "51.0",
  "web_accessible_resources": [
    "/contentscript/page.js"
  ],
  "name": "Adequa",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "cookies",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "Adequa",
  "version": "1.0.8"
}
