{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "AdFly countdown remover",
  "description": "An extension that removes the AdFly countdown.",
  "icons": {
    "48": "icon48.png",
    "128": "icon.png"
  },
  "version": "1.1",
  "permissions": [
    "http://adf.ly/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://adf.ly/*"
      ],
      "js": [
        "remove.js"
      ]
    }
  ],
  "manifest_version": 2
}
