{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AmPartnerSearch",
  "description": "Allows you to use a custom Google shopping search to see what is available through stores in partnership with Amway co.",
  "version": "3",
  "author": "LivinLifeDev",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "AmPartnerSearch"
  },
  "chrome_url_overrides": {},
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search?safe=active&tbm=shop&q=*"
      ],
      "js": [
        "modifysearch.js"
      ]
    }
  ],
  "permissions": []
}
