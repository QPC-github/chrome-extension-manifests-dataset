{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Ads TV",
  "version": "1.0",
  "description": "Remove the annoying ads in TV :)",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/ad-block-tv16.png",
      "32": "images/ad-block-tv32.png",
      "48": "images/ad-block-tv48.png",
      "128": "images/ad-block-tv128.png"
    }
  },
  "icons": {
    "16": "images/ad-block-tv16.png",
    "32": "images/ad-block-tv32.png",
    "48": "images/ad-block-tv48.png",
    "128": "images/ad-block-tv128.png"
  },
  "manifest_version": 2
}
