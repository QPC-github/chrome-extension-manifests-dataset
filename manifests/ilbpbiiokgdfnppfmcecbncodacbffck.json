{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "notes by s00d",
  "short_name": "notes_by_s00d",
  "description": "You will like to create, edit and even delete notes.",
  "version": "0.2.1",
  "minimum_chrome_version": "38",
  "browser_action": {
    "default_icon": "assets/icon_48.png",
    "default_popup": "main.html",
    "options_page": "options.html",
    "chrome_style": true,
    "default_title": "Notes by s00d"
  },
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png",
    "48": "assets/icon_48.png"
  },
  "homepage_url": "http://notes.braidy.ru/",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs"
  ],
  "web_accessible_resources": [
    "assets/icon_48.png"
  ]
}
