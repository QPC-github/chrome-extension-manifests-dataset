{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NYC Wallpaper",
  "description": "Change your browser new tab background with custom NYC wallpaper.",
  "homepage_url": "http://nycwallpaper.com/",
  "version": "2.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "public/images/bd-16.png",
      "32": "public/images/bd-32.png",
      "48": "public/images/bd-48.png",
      "128": "public/images/bd-128.png"
    }
  },
  "icons": {
    "16": "public/images/bd-16.png",
    "32": "public/images/bd-32.png",
    "48": "public/images/bd-48.png",
    "128": "public/images/bd-128.png"
  },
  "permissions": [
    "storage",
    "https://www.ishtwo.com/nycwallpaper/"
  ],
  "chrome_url_overrides": {
    "newtab": "landing.html"
  },
  "manifest_version": 2
}
