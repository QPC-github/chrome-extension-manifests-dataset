{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rainbow URL",
  "description": "Change browser action color when its icon is clicked",
  "version": "2.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "rainbowurls_icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "history"
  ],
  "manifest_version": 2
}
