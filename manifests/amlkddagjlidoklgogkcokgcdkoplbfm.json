{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GS Downloads Settings",
  "short_name": "GS Downloads Settings",
  "version": "1.1",
  "description": "This Extension lets you change download settings, overwrite download files, create sub folder base on file extensions.",
  "permissions": [
    "downloads",
    "storage"
  ],
  "background": {
    "scripts": [
      "jquery-1.10.2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ico16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "ico32.png",
    "48": "ico48.png"
  },
  "manifest_version": 2
}
