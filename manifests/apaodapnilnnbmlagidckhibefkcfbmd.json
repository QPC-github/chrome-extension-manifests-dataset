{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Calendar Day",
  "description": "Useful Calendar for Students of Detroit Country Day School",
  "version": "0.21.87",
  "browser_action": {
    "default_icon": "img/calday_19.png",
    "default_title": "Calendar Day",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/calday_128.png",
    "64": "img/calday_64.png",
    "48": "img/calday_48.png",
    "19": "img/calday_19.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jQuery.js",
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "background",
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
