{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Posher Shopper",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "description": "Find the best deals on Poshmark.com by sorting by biggest savings!",
  "content_scripts": [
    {
      "matches": [
        "https://poshmark.com/*"
      ],
      "js": [
        "posher_shopper.js"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
