{
  "background": {
    "persistent": false,
    "scripts": [
      "bootstrap.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icongray.png",
      "38": "icongray.png"
    },
    "default_popup": "popup.html",
    "default_title": "17-18 KDS Powerschool Grade Calculator"
  },
  "description": "Displays percentages of grades and current semester GPA in Powerschool and allows the user to calculate updates to their grade.",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Kent Powerschool Percentage Calculator",
  "permissions": [
    "http://powerschool.kentdenver.org/",
    "https://powerschool.kentdenver.org/",
    "https://powerschool.kentdenver.org/public/",
    "tabs",
    "storage",
    "cookies",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "5.0.4",
  "web_accessible_resources": [
    "icon.png",
    "icon32.png"
  ]
}
