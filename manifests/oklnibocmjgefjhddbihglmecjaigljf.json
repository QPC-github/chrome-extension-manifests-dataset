{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "res/icon24.png",
    "default_popup": "pop.html"
  },
  "content_security_policy": "default-src * 'unsafe-inline'; script-src 'self'; object-src 'self'; frame-src about: http:// https://",
  "description": "这个插件提供了一些大家上网经常要用的一些功能，如IP地址查询，二维码，域名WHOIS信息查询，手机号码归属地查询和身份证信息查询等",
  "icons": {
    "128": "res/icon128.png",
    "16": "res/icon16.png",
    "48": "res/icon48.png"
  },
  "manifest_version": 2,
  "name": "查询助手",
  "permissions": [
    "notifications",
    "alarms",
    "cookies",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "version": "1.0.1"
}
