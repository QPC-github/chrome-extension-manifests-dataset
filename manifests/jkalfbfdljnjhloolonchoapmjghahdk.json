{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Habrahabr Post Additionals",
  "version": "1.0.2",
  "description": "Перемещение панели с голосованием, закладками, просмотрами и переходом к комментариям в начало статьи на сайте habr.com.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://habr.com/*/post/*/",
        "https://habr.com/*/blog/*/",
        "https://habr.com/*/article/*/",
        "https://habr.com/*/news/*/"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
