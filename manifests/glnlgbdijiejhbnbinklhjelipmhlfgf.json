{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "USMap Puzzle Game",
  "description": "Teaches the states location and names",
  "version": "2.0",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage"
  ],
  "offline_enabled": true
}
