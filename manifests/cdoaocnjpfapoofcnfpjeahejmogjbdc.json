{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Motivation Lab",
  "short_name": "Motivation Lab",
  "version": "1.8",
  "author": "nirajkaushal",
  "description": "Motivational videos in your New Tab.",
  "icons": {
    "16": "img/icon/icon_16.png",
    "19": "img/icon/icon_19.png",
    "32": "img/icon/icon_32.png",
    "48": "img/icon/icon_48.png",
    "64": "img/icon/icon_64.png",
    "128": "img/icon/icon_128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
