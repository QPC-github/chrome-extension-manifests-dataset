{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DailyMale Diskusia",
  "version": "0.1",
  "description": "Zvýrazní príspevky v diskusii od posledného zobrazenia článku.",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "Daily.ico",
    "default_title": "DailyMale zvýrazňovač diskusie"
  },
  "icons": {
    "16": "Daily.ico",
    "48": "Daily.png",
    "128": "Daily128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "http://www.dailymale.sk/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.dailymale.sk/*"
  ],
  "manifest_version": 2
}
