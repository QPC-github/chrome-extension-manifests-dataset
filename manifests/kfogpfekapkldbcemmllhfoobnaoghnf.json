{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Add a context menu item to search for coins on coinmarketcap.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "minimum_chrome_version": "6",
  "name": "Search CoinMarketCap Right Click",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "version": "1.1"
}
