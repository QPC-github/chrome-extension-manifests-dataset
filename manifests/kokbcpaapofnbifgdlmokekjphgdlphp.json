{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.1",
  "name": "Picture Downloader",
  "description": "Picture Downloader. Browse and save - image downloader on webpages.",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/ico_19.png",
    "default_popup": "popup.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "eSearch",
      "keyword": "esearch",
      "search_url": "https://www.e-search.net/search?id=jk45&app=picdown&q={searchTerms}",
      "favicon_url": "https://www.e-search.net/img/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "128": "img/ico_128.png",
    "16": "img/ico_16.png",
    "48": "img/ico_48.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>",
    "downloads"
  ]
}
