{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disco the page",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Extentention what makes active page rainbow on click on extentention icon.",
  "homepage_url": "https://disco.micev.icu",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Make this page rainbow!"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
