{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BullVPN - VPN Proxy Unlock Website & Security",
  "short_name": "BullVPN",
  "description": "BullVPN - VPN Proxy Access websites blocked in your country or company.",
  "version": "5.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "bullvpn-chrome-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "*://*.bullvpn.com/",
    "*://*.blackbull.news/",
    "*://*.narutoonline.info/",
    "<all_urls>",
    "tabs",
    "notifications",
    "alarms",
    "storage"
  ]
}
