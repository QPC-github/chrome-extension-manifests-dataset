{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Into Events",
  "description": "Win free tickets, just by inviting your Facebook friends to your favorite events.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.5",
  "permissions": [
    "cookies",
    "*://*/*",
    "*://*.facebook.com/events*",
    "tabs"
  ],
  "page_action": {
    "default_title": "Into.events"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
