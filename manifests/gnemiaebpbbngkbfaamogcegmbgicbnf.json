{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Helper.World",
  "short_name": "Helper",
  "description": "The official Helper.World extension has been built to join incoming support session in your browser directly.",
  "version": "19.3.0",
  "icons": {
    "128": "images/register.png"
  },
  "author": "EGOTEC GmbH",
  "browser_action": {
    "default_icon": "images/offline.png",
    "default_popup": "plugin.html"
  },
  "background": {
    "scripts": [
      "lib/jquery/jquery.js",
      "js/init.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "alarms",
    "notifications"
  ]
}
