{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SunfyreTV ",
  "description": "Be informed when SunfyreTV is streaming.",
  "version": "0.36",
  "permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.4.1.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/sunfyretv_logo.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/sunfyretv_logo.png"
  ],
  "icons": {
    "70": "img/sunfyretv_logo.png"
  }
}
