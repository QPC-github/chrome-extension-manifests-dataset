{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Timer",
  "description": "Shows an alert after predetermined number of minutes",
  "version": "1.0",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "timer20.png",
    "48": "timer48.png",
    "128": "timer128.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ]
}
