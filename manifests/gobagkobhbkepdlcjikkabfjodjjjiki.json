{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Graph Search Die",
  "description": "This extension hides Facebook's new Graph Search interface..",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://facebook.com/*",
        "https://facebook.com/*",
        "http://www.facebook.com/*",
        "https://www.facebook.com/"
      ],
      "css": [
        "fbd.css"
      ]
    }
  ]
}
