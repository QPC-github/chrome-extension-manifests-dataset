{
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/Extension-Iocn-48.png",
    "default_title": "done4today"
  },
  "content_scripts": [
    {
      "css": [
        "css/style.css",
        "css/sweetalert.css",
        "css/jquery-ui.min.css",
        "css/evol-colorpicker.min.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "jquery.js",
        "content.js",
        "nicEdit-latest.js",
        "sweetalert-dev.js",
        "jquery-ui.js",
        "evol-colorpicker.min.js"
      ]
    }
  ],
  "description": "done4today Template Manager tool",
  "icons": {
    "48": "images/Extension-Iocn-48.png",
    "128": "images/Extension-Iocn-128.png"
  },
  "manifest_version": 2,
  "name": "done4today",
  "web_accessible_resources": [
    "images/Extension-Iocn-48.png",
    "images/nicEditIcons-latest.gif",
    "gmail.js",
    "images/loading.gif",
    "images/logo.png",
    "images/color.png"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage",
    "management",
    "identity",
    "cookies",
    "alarms"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.7"
}
