{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/svadba.css",
        "css/profileContent.css"
      ],
      "js": [
        "js/jquery.js",
        "js/translations.js",
        "js/cookie.js",
        "js/profileContent.js",
        "js/hashchange.js"
      ],
      "matches": [
        "*://*.svadba.com/Login/Men/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/svadba.css"
      ],
      "js": [
        "js/jquery.js",
        "js/cookie.js",
        "js/content.live.js",
        "js/hashchange.js"
      ],
      "matches": [
        "*://*.svadba.com/chat/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "css/svadba_helper.css"
      ],
      "js": [
        "js/jquery.js",
        "js/translations.js",
        "js/svadba_helper.js"
      ],
      "matches": [
        "*://*.svadba.com/Login/Home.aspx"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.chatoptimizer.com; object-src 'self'; ",
  "default_locale": "ru",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "page_action": {
    "default_icon": "img/16.png",
    "default_popup": "index.live.html",
    "default_title": "ChatOS Svadba chat - meaning of love"
  },
  "permissions": [
    "tabs",
    "*://*.svadba.com/*",
    "*://*.chatoptimizer.com/*",
    "*://*.anastasiadate.com/*",
    "notifications"
  ],
  "version": "1.9.8"
}
