{
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Have fun with the name of President Donald Trump. Replaces the text 'Trump' with 'Doofus'.",
  "manifest_version": 2,
  "name": "Trump To Doofus",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.4",
  "browser_action": {
    "default_icon": "Doofus.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  }
}
