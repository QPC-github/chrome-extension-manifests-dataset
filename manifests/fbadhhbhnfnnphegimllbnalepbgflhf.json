{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "gobi3.js"
      ],
      "matches": [
        "https://*.gobi3.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "This extension will search titles on a Gobi page and display the number of matches found in Duke University Libraries",
  "manifest_version": 2,
  "name": "Duke University Library GOBI Linky",
  "permissions": [
    "*://find.library.duke.edu/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
