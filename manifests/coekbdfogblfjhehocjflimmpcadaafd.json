{
  "content_scripts": [
    {
      "run_at": "document_end",
      "all_frames": true,
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ],
  "description": "Audio panner for MultiYouTube",
  "icons": {
    "152": "icons/icon152.png"
  },
