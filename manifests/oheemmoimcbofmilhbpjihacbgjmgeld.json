{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LabStats Website Application Tracking",
  "description": "A lightweight script to gather smart statistical data. Used with the LabStats client.",
  "version": "7.0.7.804",
  "manifest_version": 2,
  "short_name": "LabStats Website Application Tracking",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "labstats.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "homepage_url": "https://labstats.com/",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "http://localhost:10122/ReportWebApps"
  ]
}
