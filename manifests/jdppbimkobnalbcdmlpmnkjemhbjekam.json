{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Product Reading List Share",
  "description": "This extension sends the article to Product Reading List",
  "version": "1.40",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Send to PRL"
