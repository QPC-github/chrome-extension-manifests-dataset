{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TermSearch EN-FR",
  "description": "Highlight text and search it in IATE and UNterm",
  "version": "1.0",
  "browser_action": {
    "default_title": "TermSearch EN-FR",
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>"
  ]
}
