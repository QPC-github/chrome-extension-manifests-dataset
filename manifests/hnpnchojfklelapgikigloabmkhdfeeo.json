{
  "author": "George",
  "name": "Z Results",
  "short_name": "Z Results",
  "homepage_url": "https://Z-Results.com",
  "description": "Just type Z, press tab and get instant results from Z-Results.com, the best search option around.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Z Results"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://Z-Results.com/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "Z",
      "name": "Z Results",
      "search_url": "https://Z-Results.com/?a=web&q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "config.js",
      "settings.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "searchtab.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ]
}
