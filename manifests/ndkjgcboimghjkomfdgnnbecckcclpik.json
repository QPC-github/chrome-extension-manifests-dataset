{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sense-M Chrome Extension",
  "description": "This extension records and transmits your Web browsing history to your private data bucket.",
  "version": "0.8",
  "oauth2": {
    "client_id": "855320492772-n8uq7ppqfqm9803p2hpc8624cu2g900j.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "key": "",
  "permissions": [
    "history",
    "identity",
    "https://*.googleapis.com/*",
    "https://googleapis.com/*",
    "http://*.hissohathair.com/*",
    "https://*.hissohathair.com/*"
  ],
  "icons": {
    "128": "icons/chemist_128.png",
    "64": "icons/chemist_64.png",
    "32": "icons/chemist_32.png",
    "16": "icons/chemist_16.png"
  },
  "browser_action": {
    "default_icon": "icons/chemist_32.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'"
}
