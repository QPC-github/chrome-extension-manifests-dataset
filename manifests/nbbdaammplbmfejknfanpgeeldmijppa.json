{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Megogo",
  "version": "1.0.1",
  "description": "Megogo.net",
  "permissions": [
    "storage",
    "tabs",
    "http://megogo.net/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Megogo.net",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
