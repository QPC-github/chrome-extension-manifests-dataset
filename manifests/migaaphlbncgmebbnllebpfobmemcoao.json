{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Calculator for Chrome",
  "short_name": "Calculator",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Best Calculator extension for Chrome, simple and beautiful.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
