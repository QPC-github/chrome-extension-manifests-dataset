{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "128": "img/128.png",
    "256": "img/256.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "browser_action": {
    "default_icon": "img/128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "history",
    "unlimitedStorage",
    "chrome://favicon/",
    "contextMenus",
    "storage",
    "topSites",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "1.2"
}
