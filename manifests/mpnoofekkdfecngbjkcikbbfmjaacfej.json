{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tendance Ouest",
  "version": "1.0",
  "description": "Retrouvez le meilleur de Tendance Ouest au sein de Google Chrome",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "background",
    "tabs",
    "notifications",
    "contextMenus",
    "*://www.tendanceouest.com/*"
  ],
  "background": {
    "scripts": [
      "test.js"
    ]
  },
  "browser_action": {
    "default_title": "Tendance Ouest",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "homepage_url": "http://www.tendanceouest.com",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
