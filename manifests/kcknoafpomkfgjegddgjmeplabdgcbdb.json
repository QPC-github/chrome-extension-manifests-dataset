{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NoteShred",
  "description": "Send Encrypted, Password Protected Notes That Shred After Reading",
  "version": "1.2",
  "homepage_url": "https://www.noteshred.com",
  "permissions": [
    "https://www.noteshred.com/"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Create An Encrypted Note"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
