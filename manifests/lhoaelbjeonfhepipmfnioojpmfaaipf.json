{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Downloads",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Access the folder of the last downloaded file, on click and on completion.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "64": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Downloads"
  },
  "permissions": [
    "background",
    "downloads",
    "downloads.shelf",
    "storage"
  ]
}
