{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "Trading View Binomo - Теханализ для торговой платформы брокера бинарных опционов Binomo. Разработан ( Бывалый Трейдер - YouTube ).",
  "name": "Trading View Binomo",
  "short_name": "Trading View Binomo",
  "version": "1.1.0",
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "js/jquery.js",
        "js/IQInfo.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "http://binomo.com/*",
        "https://binomo.com/*",
        "http://*.binomo.com/*",
        "https://*.binomo.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://blnomo.ru/*"
  ],
  "browser_action": {
    "default_title": "Trading View Binomo",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "converted_from_user_script": true
}
