{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free Texas",
  "description": "This extension allows the user to see free events in popular cities across Texas.",
  "icons": {
    "128": "freeTex128.png"
  },
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "freeTexas19.png",
    "default_popup": "popup.html",
    "default_title": "Free Texas!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
