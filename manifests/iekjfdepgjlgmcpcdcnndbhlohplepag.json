{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dropmark",
  "description": "All your stuff in one simple, visual, private place.",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "1.0.0",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "webview"
  ]
}
