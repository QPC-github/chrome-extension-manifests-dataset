{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quuu Discover",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Quick access to the discover service",
  "background": {
    "scripts": [
      "content.js"
    ]
  },
  "icons": {
    "16": "Icons/16.png",
    "32": "Icons/32.png",
    "48": "Icons/48.png",
    "128": "Icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "Icons/16.png",
      "32": "Icons/32.png",
      "48": "Icons/48.png",
      "128": "Icons/128.png"
    }
  },
  "permissions": [
    "tabs",
    "http://app.quuu.co/*"
  ],
  "chrome_url_overrides": {
    "newtab": "init.html"
  }
}
