{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "QR Me",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get a QR Code for the page you're currently on.",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "qr.html"
  },
  "permissions": [
    "tabs"
  ]
}
