{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GoGobblesTab",
  "short_name": "GoGobblesTab",
  "author": "Hong Shick Pak",
  "version": "0.9.1",
  "description": "Chrome extension for GoGobbles lists.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "gt.html"
  },
  "permissions": [
    "https://gogobbles.com/",
    "storage"
  ]
}
