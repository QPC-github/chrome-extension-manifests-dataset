{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mindful Bells ",
  "description": "A simple mindful reminder to keep you focused",
  "version": "0.0.0.2",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Mindful Bells",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications"
  ]
}
