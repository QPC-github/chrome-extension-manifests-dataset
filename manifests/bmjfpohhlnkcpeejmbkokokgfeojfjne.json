{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/showthread.js"
      ],
      "matches": [
        "*://*.hackforums.net/showthread.php?tid=*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "js/jquery.js",
        "js/forumdisplay.js"
      ],
      "matches": [
        "*://*.hackforums.net/forumdisplay.php?fid=*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Never have to click Next Page again!",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Never ending HackForums",
  "permissions": [
    "*://*.hackforums.net/*"
  ],
  "version": "1.0.2",
  "web_accessible_resources": [
    "img/enable.png",
    "js/jquery.js",
    "js/forumdisplay.js",
    "js/main.js",
    "img/disable.png",
    "img/pp.png"
  ]
}
