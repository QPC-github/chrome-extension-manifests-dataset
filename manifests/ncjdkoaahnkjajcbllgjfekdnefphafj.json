{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple New Tab Page",
  "version": "1.0.4",
  "version_name": "1.0.4-stable",
  "description": "Simple New Tab Page for Chrome",
  "author": "YUH APPS",
  "chrome_url_overrides": {
    "newtab": "ntp.html"
  },
  "permissions": [
    "bookmarks",
    "contentSettings",
    "cookies",
    "downloads",
    "geolocation",
    "history",
    "identity",
    "storage",
    "tabs",
    "topSites",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "app_icon.png"
  },
  "icons": {
    "128": "app_icon.png"
  },
  "options_page": "settings.html"
}
