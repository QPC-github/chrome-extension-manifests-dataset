{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Domain in tab",
  "description": "This extension add domain to the tab title.",
  "version": "1.2",
  "icons": {
    "128": "128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "dit.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Domain in tab",
    "default_icon": "128x128.png"
  }
}
