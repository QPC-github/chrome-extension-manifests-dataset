{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "eDeseos",
  "version": "1.25",
  "description": "eDeseos te ayuda a hacer realidad tus sue√±os y los de las personas que comparten tu vida.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon_button_edeseos.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "default_locale": "en"
}
