{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Domain Availability Checker",
  "description": "Check the availability of any domain",
  "version": "3",
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://iniciocorreo.email/"
  ]
}
