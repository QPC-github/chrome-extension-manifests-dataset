{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Cascaded Phish Detector",
  "version": "1.0.2.6",
  "description": "Detect phish in a cascaded learning framework.",
  "background": {
    "scripts": [
      "background.js",
      "jquery-1.8.0.min.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://webcantina.appspot.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "logo_big.png",
    "default_title": "Cascaded Phish Detector"
  },
  "manifest_version": 2,
  "icons": {
    "128": "logo_small.png",
    "256": "logo_big.png"
  },
  "web_accessible_resources": [
    "evil_128_128.png",
    "safe_128_128.png"
  ]
}
