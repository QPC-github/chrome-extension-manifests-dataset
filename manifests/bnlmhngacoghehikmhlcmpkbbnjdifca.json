{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "X-Cycle",
  "short_name": "xcycle",
  "description": "Cycles X-Forwarded-For Header from a set of configured IP addresses.",
  "version": "0.2.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' ; default-src 'self' ",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*"
  ],
  "icons": {
    "32": "icon.png",
    "48": "icon48.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "eventHandlers.js"
    ]
  }
}
