{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AT Internet Tag Inspector",
  "short_name": "Tag Inspector (dev)",
  "version": "1.7.7",
  "author": "AT Internet",
  "homepage_url": "http://www.atinternet.com",
  "description": "Tag Inspector Analytics Tool (dev)",
  "devtools_page": "src/chrome/devtools.html",
  "background": {
    "scripts": [
      "src/chrome/bg.js"
    ]
  },
  "browser_action": {
    "default_popup": "src/chrome/popup.html",
    "default_icon": "src/images/at-32-dev.png"
  },
  "icons": {
    "16": "src/images/at-16-dev.png",
    "32": "src/images/at-32-dev.png",
    "48": "src/images/at-48-dev.png",
    "64": "src/images/at-64-dev.png",
    "128": "src/images/at-128-dev.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "webRequest"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "minimum_chrome_version": "58"
}
