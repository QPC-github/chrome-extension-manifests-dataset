{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "网页二维码",
  "version": "0.0.1",
  "description": "生成当前页面的二维码",
  "manifest_version": 2,
  "icons": {
    "16": "assets/img/icon_16.png",
    "19": "assets/img/icon_19.png",
    "48": "assets/img/icon_48.png",
    "128": "assets/img/icon_128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "assets/img/icon_19.png",
    "default_title": "生成当前页面二维码"
  },
  "permissions": [
    "http://*/",
    "bookmarks",
    "tabs",
    "history",
    "storage",
    "activeTab",
    "https://*/*"
  ],
  "homepage_url": "http://shulk.me/"
}
