{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search Mouser: ",
  "short_name": "Mouser Search",
  "version": "1.0.3",
  "description": "Call Mouser's search accelerator page to display results for selected text.",
  "permissions": [
    "contextMenus",
    "http://www.mouser.com/"
  ],
  "background": {
    "scripts": [
      "chrome_mouser_context_menu.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
