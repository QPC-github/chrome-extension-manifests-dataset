{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Phonygamer",
  "description": "A Place to play games on the web and for free!",
  "version": "1",
  "browser_action": {
    "default_icon": {
      "16": "phonygamerIcon16x16.png",
      "48": "phonygamerIcon48x48.png",
      "128": "phonygamerIcon128x128.png"
    }
  },
  "icons": {
    "16": "phonygamerIcon16x16.png",
    "48": "phonygamerIcon48x48.png",
    "128": "phonygamerIcon128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
