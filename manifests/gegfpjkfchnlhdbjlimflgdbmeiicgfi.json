{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snake Attack",
  "short_name": "Play Snake Attack Game",
  "description": "Have fun eating as many fresh fruits as possible while avoiding being eaten by other snakes.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "snakeattack128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
