{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sinker",
  "description": "A handy utility to export your bookmarks from Google Chrome™ to Google Bookmarks™.",
  "version": "0.1",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "Push Chrome Bookmarks™ to Google Bookmarks™",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "*://*.google.com/bookmarks"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
