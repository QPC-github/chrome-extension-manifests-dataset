{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Twitter Moments",
  "manifest_version": 2,
  "version": "1",
  "description": "Hides the Moments tab on twitter",
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
