{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "WOW Trk"
  },
  "description": "Quickly keep up to date with your WOW Trk stats.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "WOW Trk",
  "options_page": "Settings.html",
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "2.5.1",
  "web_accessible_resources": [
    "icon48.png"
  ]
}
