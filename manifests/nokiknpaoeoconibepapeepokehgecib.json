{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JCS Quick Links",
  "description": "Use JCS Quick Links to navigate easier.",
  "version": "1.9",
  "browser_action": {
    "default_popup": "quicklinks.html",
    "default_icon": "JCSLINK_yellow.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens JCS Quick Links for ease of navigation. "
    }
  }
}
