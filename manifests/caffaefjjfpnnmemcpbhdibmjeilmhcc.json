{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Statistics Hider",
  "version": "0.0.0.1",
  "description": "Hide statistics on social media websites.",
  "permissions": [],
  "content_scripts": [
    {
      "css": [
        "css/youtube.css"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    },
    {
      "css": [
        "css/twitch.css"
      ],
      "matches": [
        "https://www.twitch.tv/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16x16.png",
    "32": "images/icon-48x48.png",
    "48": "images/icon-48x48.png",
    "128": "images/icon-128x128.png"
  },
  "manifest_version": 2
}
