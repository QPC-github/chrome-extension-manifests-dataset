{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Eliminar Pop up de Registro en La Prensa",
  "version": "1.5.2",
  "description": "Esta extensión se hizo con el propósito de saltarse el pop up de registro de el diario La Prensa Nicaragua.",
  "icons": {
    "16": "images/deleteicon16.png",
    "48": "images/deleteicon48.png",
    "128": "images/deleteicon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.laprensa.com.ni/*"
      ],
      "js": [
        "js/deletepopup.js"
      ]
    }
  ],
  "manifest_version": 2
}
