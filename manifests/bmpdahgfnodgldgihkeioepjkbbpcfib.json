{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TF",
  "version": "2.4",
  "description": "A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
  "permissions": [
    "tabs",
    "activeTab",
    "chrome://favicon/",
    "storage",
    "cookies",
    "*://*.google.com/*",
    "http://api-sandbox.tonguefriend.com/",
    "http://api-sandbox.tonguefriend.com/userc/login/",
    "http://tonguefriend.com/",
    "https://maps.googleapis.com/"
  ],
  "browser_action": {
    "default_title": "TF",
    "default_icon": "icon.png",
    "default_popup": "facebook-login.html"
  },
  "icons": {
    "64": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}
