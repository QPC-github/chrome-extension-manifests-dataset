{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Locklear",
  "version": "3.0.10",
  "description": "L'extension Locklear permet d'être notifié lorsque Locklear démarre un nouveau stream ou sort une nouvelle vidéo.",
  "browser_action": {
    "default_title": "Locklear",
    "default_popup": "index.html",
    "default_icon": {
      "16": "img/icons/icon16.png",
      "32": "img/icons/icon32.png",
      "64": "img/icons/icon64.png",
      "128": "img/icons/icon128.png"
    }
  },
  "icons": {
    "16": "img/icons/icon16.png",
    "32": "img/icons/icon32.png",
    "64": "img/icons/icon64.png",
    "128": "img/icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/libs/socket.io.js",
      "js/libs/jquery.js",
      "js/libs/moment.js",
      "js/libs/moment-timezone.js",
      "js/background/background.js",
      "js/background/background-twitch.js",
      "js/background/background-planning.js",
      "js/background/background-youtube.js",
      "js/background/background-instagram.js",
      "js/common/badges.js"
    ]
  },
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "*://static-cdn.jtvnw.net/*",
    "*://i.ytimg.com/*"
  ]
}
