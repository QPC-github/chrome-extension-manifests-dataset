{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AdWords & Google Ads API Web Navi",
  "description": "This extension shows the related pages of the AdWords API and Google Ads API websites based on the current Google Ads UI web page.",
  "version": "2.6",
  "permissions": [
    "tabs",
    "declarativeContent"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "bg_compiled.js"
    ]
  },
  "icons": {
    "128": "assets/shimesu-icon-128.png"
  },
  "page_action": {
    "default_icon": "assets/shimesu-icon-128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
