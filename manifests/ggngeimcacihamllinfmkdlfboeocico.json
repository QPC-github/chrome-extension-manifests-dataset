{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Short Links",
  "version": "1.0.4",
  "description": "Shorten URLs without leaving the current tab.",
  "icons": {
    "16": "shortlinks-16.png",
    "19": "shortlinks-19.png",
    "32": "shortlinks-32.png",
    "48": "shortlinks-48.png"
  },
  "browser_action": {
    "default_icon": "shortlinks-32.png",
    "default_popup": "popup.html",
    "default_title": "Google Short Links"
  },
  "permissions": [
    "tabs"
  ]
}
