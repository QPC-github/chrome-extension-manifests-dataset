{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Purple Port Driving Distances",
  "short_name": "PP Driving",
  "version": "0.1.6.2",
  "description": "Adds driving distances and times on purpleport.com profiles and sorting by distance and duration to search results.",
  "permissions": [
    "storage",
    "http://purpleport.com/*",
    "https://purpleport.com/*",
    "https://maps.googleapis.com/*",
    "https://router.project-osrm.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://purpleport.com/*",
        "https://purpleport.com/*"
      ],
      "js": [
        "jquery.js",
        "jquery.inview.min.js",
        "lz-string.min.js",
        "shuffle.min.js",
        "pp.min.js"
      ],
      "css": [
        "pp.min.css"
      ]
    }
  ],
  "icons": {
    "16": "pp-ext-16.png",
    "32": "pp-ext-32.png",
    "48": "pp-ext-48.png",
    "64": "pp-ext-64.png",
    "128": "pp-ext-128.png"
  },
  "manifest_version": 2
}
