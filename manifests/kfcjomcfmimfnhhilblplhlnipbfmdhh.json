{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Require a library in Chrome console.",
  "version": "1.0.0",
  "name": "Require",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Click to enable <require> in console"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
