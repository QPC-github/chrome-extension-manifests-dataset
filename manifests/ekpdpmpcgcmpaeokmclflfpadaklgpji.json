{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Network and Internet tools",
  "version": "1.78",
  "manifest_version": 2,
  "minimum_chrome_version": "19",
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "description": "Quick access to our online tools ping, tracert, domain search, dns blackhole list, dns lookup, reverse ip and whois lookup.",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "iconb32.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
