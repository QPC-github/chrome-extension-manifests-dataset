{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flag visitor counter",
  "manifest_version": 2,
  "short_name": "flagzzupp",
  "description": "Flag Counter is a free visitors counter for your webpage Our service will allow you to track website traffic and visitors details",
  "version": "1.1",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "default16.png"
  },
  "icons": {
    "128": "128px_171-1505098227.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
