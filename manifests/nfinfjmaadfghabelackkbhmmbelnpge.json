{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.1.8",
  "name": "Get Gabe!",
  "description": "A Chrome extension that replaces the images of a webpage with Gabe Newell.",
  "homepage_url": "http://getgabe.net",
  "icons": {
    "512": "icons/icon_512.png",
    "256": "icons/icon_256.png",
    "128": "icons/icon_128.png",
    "64": "icons/icon_64.png",
    "32": "icons/icon_32.png",
    "16": "icons/icon_16.png"
  },
  "content_scripts": [
    {
      "js": [
        "core/jquery-2.1.1.min.js",
        "core/script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
