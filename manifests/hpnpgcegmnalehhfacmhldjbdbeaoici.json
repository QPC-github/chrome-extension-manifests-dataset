{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "inject tangram"
  },
  "description": "Injects tangram to current page by one click.",
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "name": "One Click Tangram",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "manifest_version": 2,
  "version": "1.0.2"
}
