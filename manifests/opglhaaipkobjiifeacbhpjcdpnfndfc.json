{
  "author": "Mohammad Eskandari",
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "html": [
        "inject/html/mainh.html"
      ],
      "js": [
        "inject/script/jquery.min.js",
        "inject/script/content.js"
      ],
      "css": [
        "inject/style/main.css",
        "inject/style/fonts.css"
      ],
      "matches": [
        "*://blog.ir/panel/*"
      ]
    }
  ],
  "description": "This extension helps users change font of the admin's panel in blog.ir (Blog Bayan) .",
  "homepage_url": "https://donyait.blog.ir",
  "icons": {
    "128": "icon/icon128.png",
    "16": "icon/icon16.png",
    "19": "icon/icon19.png",
    "24": "icon/icon24.png",
    "256": "icon/icon256.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "512": "icon/icon512.png",
    "64": "icon/icon64.png"
  },
  "permissions": [
    "*://blog.ir/*"
  ],
  "key": "",
  "manifest_version": 2,
  "name": "Blog.ir font changer",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
