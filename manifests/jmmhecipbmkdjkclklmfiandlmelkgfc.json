{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SSRS Fix",
  "description": "This extension will fix blank SSRS reports",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "SSRS Fix",
    "default_icon": "ssrs.png"
  },
  "manifest_version": 2
}
