{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PixelBlock",
  "version": "2019.04.09",
  "permissions": [
    "*://*.googleusercontent.com/proxy/*",
    "*://mail.google.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "description": "PixelBlock is a Gmail extension that blocks people from tracking when you open their emails.",
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "scripts/jquery.js",
        "cs.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "scripts/*.js",
    "images/*.png",
    "styles/*.css"
  ]
}
