{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Партия Великое Отечество",
  "description": "Данное расширение автоматически публикует материалы из группы Партии Великое Отечество на вашу стену.",
  "version": "1",
  "background": {
    "scripts": [
      "event.js",
      "jquery-3.1.0.min.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "http://vk.pvo-party.ru/",
    "http://vk.pvo-party.ru/index.php",
    "browsingData"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
