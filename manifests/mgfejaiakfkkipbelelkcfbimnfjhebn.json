{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Search",
  "description": "__MSG_description__",
  "default_locale": "en",
  "version": "0.3",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_tabsearch_button_details__"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl + Period"
      },
      "description": "Open Tab Search"
    }
  }
}
