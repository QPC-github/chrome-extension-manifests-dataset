{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NewTabBookmarks",
  "description": "Newtab-Page showing Bookmarks",
  "version": "0.95",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "NewTabBookmarks.html"
  },
  "icons": {
    "48": "img/icon-48.png"
  },
  "options_ui": {
    "page": "NewTabBookmarks.html",
    "chrome_style": true
  }
}
