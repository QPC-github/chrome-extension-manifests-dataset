{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.0",
  "name": "Twitter [ANTP]",
  "description": "Awesome Twitter widget for Awesome New Tab Page. Resizable",
  "icons": {
    "256": "i256.png",
    "128": "i128.png"
  },
  "permissions": [
    "https://www.google.com/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com https://ssl.google-analytics.com https://api.flattr.com; object-src 'self'",
  "web_accessible_resources": [
    "widget.html"
  ]
}
