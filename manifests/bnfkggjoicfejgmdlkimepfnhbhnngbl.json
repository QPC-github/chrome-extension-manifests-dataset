{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebP Please!",
  "version": "1",
  "description": "Attempts to serve WebP images when possible.",
  "permissions": [
    "declarativeWebRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "rules.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 2
}
