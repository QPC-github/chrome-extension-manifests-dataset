{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inbox Favicon Changer",
  "description": "Changes Google Inbox Favicons to represent the current page",
  "version": "5.1.9",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://inbox.google.com/*"
      ],
      "js": [
        "damnit-again-2.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
