{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Theremin",
  "description": "This extension will turn Chrome into a Theremin",
  "version": "0.0.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
