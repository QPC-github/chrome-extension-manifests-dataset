{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "纽约时报中文网",
  "version": "0.0.6",
  "manifest_version": 2,
  "icons": {
    "16": "nyt-16.png",
    "128": "nyt-128.png"
  },
  "description": "纽约时报中文网 Chrome扩展应用",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "browser_action": {
    "default_icon": {
      "16": "nyt-16.png"
    },
    "default_title": "纽约时报中文网"
  }
}
