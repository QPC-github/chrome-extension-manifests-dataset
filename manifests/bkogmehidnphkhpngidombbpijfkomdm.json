{
  "background": {
    "scripts": [
      "upa_background.js"
    ]
  },
  "browser_action": {
    "default_icon": "upa_48.png",
    "default_popup": "upa_popup.html",
    "default_title": "Upw Payments Analytics"
  },
  "content_scripts": [
    {
      "css": [
        "upa_content.css"
      ],
      "js": [
        "upa_content.js"
      ],
      "matches": [
        "https://www.upwork.com/earnings-history"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Upw Payments Analytics",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.3"
}
