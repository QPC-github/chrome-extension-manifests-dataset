{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SnapTab",
  "description": "Keeping tabs on your tabs",
  "version": "1.1.1",
  "permissions": [
    "storage",
    "tabs",
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "newicon1.png",
    "default_popup": "snaptab.html"
  }
}
