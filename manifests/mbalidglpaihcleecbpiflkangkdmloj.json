{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sarahah Filter",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "description": "Are you irritated with the Sarahah posts on your Facebook?",
  "homepage_url": "https://ashiknesin.com/sarahah-filter",
