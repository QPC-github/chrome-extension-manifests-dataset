{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Appsmashups",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Ultimas noticias de appsmashups.com",
  "icons": {
    "48": "images/icon_appsmashups.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/core.js",
      "js/bg_mashup13.js"
    ]
  },
  "browser_action": {
    "default_title": "AppsMashups",
    "default_icon": "images/icon_bar.png",
    "default_popup": "feed.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://www.appsmashups.com/*",
    "https://apis.google.com/*"
  ]
}
