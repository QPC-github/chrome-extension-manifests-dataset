{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DeltaX - Insights",
  "description": "The DeltaX Insights Plugin can be used to view dashboard",
  "version": "1.1",
  "icons": {
    "16": "icons/dx-measurement-16.png",
    "32": "icons/dx-measurement-32.png",
    "48": "icons/dx-measurement-48.png",
    "64": "icons/dx-measurement-64.png",
    "128": "icons/dx-measurement-128.png",
    "256": "icons/dx-measurement-256.png"
  },
  "browser_action": {
    "default_icon": "icons/dx-measurement-48.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
