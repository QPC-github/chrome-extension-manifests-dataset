{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DocuVantage OnDemand Gmail Connector",
  "version": "1.0.0.5",
  "timeZone": "America/New_York",
  "dependencies": {},
  "webapp": {
    "access": "MYSELF",
    "executeAs": "USER_DEPLOYING"
  },
  "exceptionLogging": "STACKDRIVER",
  "oauthScopes": [
    "https://www.googleapis.com/auth/gmail.addons.execute",
    "https://www.googleapis.com/auth/gmail.addons.current.message.metadata",
    "https://www.googleapis.com/auth/userinfo.email",
    "https://www.googleapis.com/auth/script.locale"
  ],
  "urlFetchWhitelist": [
    "https://www.docuvantage.com/",
    "https://www.docuvantageondemand.com/"
  ],
  "gmail": {
    "name": "DocuVantage OnDemand AddOn",
    "logoUrl": "https://www.docuvantageondemand.com/static/1-3.5.1.404/themes/custom/images/gmailconnector/logo-small-32.png",
    "contextualTriggers": [
      {
        "unconditional": {},
        "onTriggerFunction": "buildTheButtons"
      }
    ],
    "universalActions": [
      {
        "text": "Need help?",
        "openLink": "http://help.docuvantageondemand.com/gmail-connector"
      }
    ],
    "primaryColor": "#638196",
    "secondaryColor": "#91b3c9",
    "openLinkUrlPrefixes": [
      "https://mail.google.com/",
      "https://script.google.com/a/google.com/d/",
      "https://drive.google.com/a/google.com/file/d/",
      "https://en.wikipedia.org/wiki/",
      "https://www.docuvantage.com/",
      "https://www.docuvantageondemand.com/"
    ]
  }
}
