{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QLGE - the QuakeLive GUI Enhancer",
  "version": "0.5.0",
  "manifest_version": 2,
  "description": "makes Quakelive.com user interface handier",
  "icons": {
    "16": "icon_16.png",
    "19": "icon_19.png",
    "38": "icon_38.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "http://www.quakelive.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.quakelive.com/*"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "content.js"
      ]
    }
  ]
}
