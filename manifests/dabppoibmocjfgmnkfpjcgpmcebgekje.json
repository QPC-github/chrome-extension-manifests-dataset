{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LifeLogMono - Time Tracker",
  "version": "0.2.2",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "main page",
    "default_badge": "This"
  },
  "background": {
    "scripts": [
      "background.js",
      "lib.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "description": "activate",
      "global": true
    }
  },
  "icons": {
    "16": "images/icon/LL_16.png",
    "32": "images/icon/LL_32.png",
    "48": "images/icon/LL_48.png",
    "128": "images/icon/LL_128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "options_page": "options.html"
}
