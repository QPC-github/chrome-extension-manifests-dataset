{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "U tabs",
  "description": "This extension will merge all the tabs from different windows into one window",
  "version": "1.0",
  "background": {
    "scripts": [
      "merge.js"
    ]
  },
  "icons": {
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "19": "icon19.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png",
      "128": "icon128.png"
    },
    "default_title": "Merge all tabs"
  },
  "permissions": [
    "tabs"
  ]
}
