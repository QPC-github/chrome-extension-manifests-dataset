{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Visual ARIA",
  "short_name": "VA",
  "version": "2020.4.9.1",
  "manifest_version": 2,
  "description": "Displays ARIA used in web technologies, includes live region, and widget roles, proper nesting and focus management.",
  "homepage_url": "https://github.com/accdc/visual-aria",
  "author": "VA by Bryan Garaventa. Extention by Laurence lewis",
  "background": {
    "scripts": [
      "load-va.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
    },
    "default_title": "Visual ARIA"
  },
  "permissions": [
    "activeTab"
  ]
}
