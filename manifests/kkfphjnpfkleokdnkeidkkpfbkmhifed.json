{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Helper League-17",
  "version": "1.6",
  "icons": {
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png"
  },
  "permissions": [
    "http://game.league17.ru/*",
    "http://wiki.league17.ru/*",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://game.league17.ru/*",
        "https://wiki.league17.ru/*"
      ],
      "js": [
        "js/background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Helper League-17",
    "default_icon": "icons/48x48.png",
    "default_popup": "popup.html",
    "options_page": "options.html"
  }
}
