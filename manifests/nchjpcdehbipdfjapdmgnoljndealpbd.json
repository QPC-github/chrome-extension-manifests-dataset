{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Add support to the omnibox to search the GW2TP.net item database.",
  "name": "GW2TP",
  "omnibox": {
    "keyword": "ii"
  },
  "icons": {
    "16": "icon-16.png"
  },
  "permissions": [
    "tabs",
    "http://www.gw2tp.net/"
  ],
  "version": "1.1",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
