{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CrypWatch",
  "description": "CrypWatch Extension",
  "short_name": "CrypWatch",
  "version": "0.8.9.7",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/l.png",
    "default_title": "CrypWatch"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
