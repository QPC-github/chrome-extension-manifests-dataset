{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Bad Pixel Test",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Verify your screen for bad, dead and stuck pixels using Google Chrome extension.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "homepage_url": "http://karpolan.com/services/bad-pixel-test/",
  "offline_enabled": true
}
