{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WebCrumbs",
  "description": "This extension displays the time you spend on websites in a pie chart",
  "version": "0.2.7.1",
  "browser_action": {
    "default_icon": "icon38.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "windows"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon128.png"
  }
}
