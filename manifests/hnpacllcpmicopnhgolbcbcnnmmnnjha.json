{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Everleagues WebApp Conference Extension",
  "description": "A simple extension that allows you to share your desktop in conferences with other Everleagues users.",
  "version": "0.1.10",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo_16x16.png",
    "48": "logo_48x48.png",
    "128": "logo_128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://testconf3.everleagues.com/*",
      "*://conference.everleagues.com/*"
    ]
  }
}
