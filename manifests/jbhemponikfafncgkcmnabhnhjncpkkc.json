{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WindowFisher",
  "short_name": "WindowFisher",
  "description": "Enables the WindowFisher desktop application to fish for Chrome Tabs.",
  "version": "1.0.18089.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "icons": {
    "16": "images/fish-16.png",
    "32": "images/fish-32.png",
    "48": "images/fish-48.png",
    "128": "images/fish-128.png"
  }
}
