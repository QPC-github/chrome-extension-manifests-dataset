{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Linkpage.co link saver.",
  "description": "Screenshot your links from anywhere on the web.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "app.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "*://*.linkpage.co/*",
    "https://linpage.co/",
    "http://linkpage.co/",
    "tabs",
    "<all_urls>"
  ]
}
