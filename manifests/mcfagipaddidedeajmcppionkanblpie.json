{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ezView for Desktop Sharing Extension",
  "description": "Desktop sharing for ezView",
  "version": "1.0.8",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://webrtc.saeha.com/*",
      "*://webrtc.ezview.kr/*",
      "*://localhost/*",
      "https://172.16.34.109:9091/*",
      "*://webrtcdev.ezview.kr/*",
      "*://webrtcqc.ezview.kr/*",
      "*://www.webrtc-sangsang-coding.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ],
  "icons": {
    "128": "saeha128.png",
    "16": "saeha16.png",
    "48": "saeha48.png"
  },
  "browser_action": {
    "default_icon": "saeha16.png"
  }
}
