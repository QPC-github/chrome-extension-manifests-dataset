{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "simple-performance-statistics",
  "version": "0.0.1",
  "description": "simple static of performance metrics",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
