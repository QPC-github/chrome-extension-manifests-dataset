{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Show Me Shows",
  "description": "This extension shows shows in your area",
  "version": "0.0.0.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Show shows!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/*",
    "geolocation",
    "https://accounts.spotify.com/*",
    "tabs",
    "webNavigation",
    "activeTab",
    "storage",
    "identity",
    "declarativeContent",
    "https://accounts.spotify.com/*",
    "https://accounts.spotify.com/authorize/*",
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery.min.js",
        "popup.js",
        "background.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.min.js"
    ]
  }
}
