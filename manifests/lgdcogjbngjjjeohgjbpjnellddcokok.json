{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Hide Google Play Link",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Hides the Google Play link in the Google bar.",
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "*://*.google.com/"
  ]
}
