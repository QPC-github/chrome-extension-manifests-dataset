{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Dima PM",
  "manifest_version": 2,
  "name": "Show Password Text",
  "version": "0.2",
  "description": "Shows text in password input",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ]
}
