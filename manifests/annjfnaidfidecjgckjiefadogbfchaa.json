{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Favicon title changer",
  "description": "",
  "version": "1.1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.12.4.min.js",
        "popup.js"
      ]
    }
  ]
}
