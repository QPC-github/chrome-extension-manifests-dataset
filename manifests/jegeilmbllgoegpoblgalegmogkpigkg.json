{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cachie",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "description": "Get cached version of the current page",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
    "Cachie": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      },
      "description": "Cachie"
    }
  },
  "version": "0.1",
  "manifest_version": 2
}
