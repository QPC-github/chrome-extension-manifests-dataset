{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "오늘의메뉴는?",
  "description": "오늘의메뉴를 알아보자",
  "version": "0.9.2",
  "icons": {
    "48": "icon_spoon.png"
  },
  "background": {
    "scripts": [
      "jquery-1.10.2.min.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/"
  ],
  "browser_action": {
    "default_icon": "icon_spoon.png",
    "default_popup": "popup.html"
  }
}
