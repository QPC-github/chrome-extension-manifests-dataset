{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hamsta - Simple Budgeting",
  "short_name": "Hamsta",
  "description": "Faster way of creating new transactions for Hamsta Budgeting App.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "82": "icon82.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "https://hamsta.biz/",
    "https://*.hamsta.biz/*",
    "http://localhost:3000/",
    "http://*.localhost:3000/*",
    "cookies"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com/analytics.js https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js; object-src 'self'"
}
