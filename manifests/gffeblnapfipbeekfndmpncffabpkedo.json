{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Opens up a user defined set of pages only for the first launch of the browser on a day",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png"
  },
  "name": "Morning Pages",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "manifest_version": 2
}
