{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No History",
  "description": "Disables history logging",
  "version": "1.0.1",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "history"
  ],
  "background": {
    "scripts": [
      "no_history.js"
    ],
    "persistent": false
  }
}
