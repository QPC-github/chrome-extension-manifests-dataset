{
  "background": {
    "scripts": [
      "main.js",
      "omnibox.js"
    ]
  },
  "author": "Yoann Thiebault",
  "browser_action": {
    "default_icon": {
      "19": "ico19.png",
      "38": "ico38.png"
    },
    "default_popup": "main.html",
    "default_title": "GRF Quick Search"
  },
  "omnibox": {
    "keyword": "grf"
  },
  "description": "Recherche rapide sur l'ensemble des sites du Groupe Revue Fiduciaire",
  "icons": {
    "128": "ico128.png",
    "16": "ico16.png",
    "48": "ico48.png"
  },
  "manifest_version": 2,
  "name": "GRF Quick Search",
  "permissions": [
    "contextMenus",
    "tabs",
    "bookmarks"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.1"
}
