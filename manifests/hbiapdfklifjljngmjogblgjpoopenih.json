{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Yazarx Extension",
  "version": "0.1.80",
  "manifest_version": 2,
  "description": "Gazeteoku.com ve www.tumkoseyazilari.com sitelerinde köşe yazarlarını okurken gereksiz sayfa çerçevelerini kaldırır.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
