{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bit Bucket Feeder",
  "description": "RSS feed of Bit Buckit.",
  "version": "1.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "crypto.js",
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "tabs",
    "*://*.bitbucket.org/"
  ]
}
