{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fuse AI Easy video watch",
  "description": "Watch video",
  "version": "1.0.0",
  "icons": {
    "128": "assets/favicon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/favicon-16x16.png",
      "38": "assets/favicon-32x32.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+P",
        "mac": "Alt+P",
        "chromeos": "Alt+P",
        "linux": "Alt+P"
      }
    }
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "minimum_chrome_version": "69.0.3483.0",
  "manifest_version": 2
}
