{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WDB",
  "description": "Web Developer Blogs - Programming Blogs Aggregator.",
  "version": "1.2",
  "browser_action": {
    "name": "WDB",
    "default_icon": "icon.png",
    "default_popup": "wdb.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "http://www.webdeveloperblogs.com/ajax"
  ]
}
