{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Online Dino",
  "version": "0.0.0.1",
  "description": "Chrome's offline dinosaur game, but now you can play it anytime you want! Press up or space bar to jump, and down to duck!",
  "icons": {
    "16": "100-error-offline.png",
    "48": "100-error-offline.png",
    "128": "100-error-offline.png"
  },
  "browser_action": {
    "default_title": "Online Dino",
    "default_icon": "100-error-offline.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
