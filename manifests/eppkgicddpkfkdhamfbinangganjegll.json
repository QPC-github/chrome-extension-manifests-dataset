{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iStock Time",
  "description": "Show the current time in iStockville.",
  "version": "0.0.4",
  "icons": {
    "128": "time-icon-1.png"
  },
  "content_scripts": [
    {
      "js": [
        "theasis_istock_time.user.js"
      ],
      "matches": [
        "http://*.istockphoto.com/*",
        "https://*.istockphoto.com/*"
      ]
    }
  ]
}
