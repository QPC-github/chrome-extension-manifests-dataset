{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ChutaDic Context Menu Extension",
  "description": "Lookup selected text on The Reading Tutor Web Dictionary",
  "version": "1.0",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "chu.png",
    "32": "chu_32.png",
    "48": "chu_48.png",
    "64": "chu_64.png",
    "128": "chu_128.png"
  },
  "manifest_version": 2
}
