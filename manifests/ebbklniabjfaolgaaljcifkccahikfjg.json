{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Schick und alles im Blick mit Ihrer neuen Startseite von die-Startseite.net",
  "manifest_version": 2,
  "name": "die-startseite.net",
  "version": "1.2",
  "homepage_url": "http://www.die-startseite.net",
  "incognito": "split",
  "icons": {
    "16": "favicon.ico",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "minimum_chrome_version": "22",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.die-startseite.net",
    "startup_pages": [
      "http://www.die-startseite.net"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "optional_permissions": [
    "tabs"
  ]
}
