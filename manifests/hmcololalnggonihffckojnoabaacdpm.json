{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to JobTracked",
  "version": "1.0",
  "description": "Save job postings to JobTracked",
  "homepage_url": "https://www.jobtracked.com",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "background": {
    "scripts": [
      "backgroundscripts/dispatch.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "storage",
    "tabs",
    "https://www.jobtracked.com/api/*",
    "https://www.jobtracked.com/Security/Account/SignedIn/*"
  ],
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Save to JobTracked"
  }
}
