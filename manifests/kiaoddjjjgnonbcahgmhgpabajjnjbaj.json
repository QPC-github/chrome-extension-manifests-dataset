{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shiny Browser Data",
  "version": "1.1",
  "description": "Clean one year or all of your browsing data in one simple click!",
  "permissions": [
    "browsingData"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
