{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "opsct",
  "description": "To know when opsct goes live !",
  "version": "2.4.13",
  "background": {
    "scripts": [
      "parameters.js",
      "btnlive.js",
      "chrome.js"
    ]
  },
  "permissions": [
    "https://api.twitch.tv/",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "iconoff48.png"
  },
  "icons": {
    "16": "iconoff16.png",
    "48": "iconoff48.png",
    "128": "iconoff128.png"
  }
}
