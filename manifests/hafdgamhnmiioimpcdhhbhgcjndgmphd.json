{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "js/bg/tuna.js",
      "js/bg/jungle.js",
      "js/common.js",
      "js/bg/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "style/off.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_mainTitle__"
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "icons": {
    "128": "style/on.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "offline_enabled": true,
  "options_page": "options.html",
  "permissions": [
    "tabCapture",
    "tabs",
    "storage"
  ],
  "version": "2.6.5"
}
