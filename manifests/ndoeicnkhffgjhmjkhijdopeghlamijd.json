{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fj Screen ",
  "author": "FJ, Inc.",
  "version": "1.2.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for Https://www.financialjuice.com to do screen sharing",
  "homepage_url": "Https://www.financialjuice.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "matches": [
        "*://*.financialjuice.com/*"
      ]
    }
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
