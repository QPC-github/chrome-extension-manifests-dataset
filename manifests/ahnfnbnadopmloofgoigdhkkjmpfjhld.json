{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Accordion Display",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Show more or less text on with number keys.",
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Show/Hide Text"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "web_accessible_resources": [
    "accordion.js"
  ]
}
