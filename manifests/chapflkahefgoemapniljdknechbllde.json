{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Word Unscrambler",
  "description": "Enter you letters and find out all the words that can be formed with our lighning fast Word Unscrambler.",
  "browser_action": {
    "default_icon": "assets/images/default_x32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "assets/images/default_x128.png",
    "48": "assets/images/default_x48.png",
    "16": "assets/images/default_x16.png"
  },
  "permissions": [
    "https://wordunscrambler.org/api/v1/anagrams-of/*"
  ],
  "version": "1.1.0",
  "manifest_version": 2
}
