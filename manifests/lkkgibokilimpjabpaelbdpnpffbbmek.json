{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "2.2",
  "manifest_version": 2,
  "description": "__MSG_appDesc__",
  "default_locale": "pl",
  "content_scripts": [
    {
      "matches": [
        "http://allegro.pl/*",
        "http://aukro.cz/*",
        "http://aukro.ua/*",
        "http://molotok.ru/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js",
        "popup.js"
      ],
      "run_at": "document_end"
    }
  ],
  "author": "www.snajper.net",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
