{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "default_locale": "en",
  "icons": {
    "48": "images/icon-small.png",
    "128": "images/icon-big.png"
  },
  "version": "5.2.0",
  "manifest_version": 2,
  "optional_permissions": [
    "management"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_title": "Gintama.: Shirogane no Tamashii-hen"
  },
  "description": "__MSG_description__",
  "chrome_settings_overrides": {
    "homepage": "http://funsearchbox.com",
    "search_provider": {
      "name": "Fun Search Box",
      "search_url": "http://funsearchbox.com/search/{searchTerms}",
      "suggest_url": "http://funsearchbox.com/suggest/{searchTerms}",
      "encoding": "UTF-8",
      "is_default": true,
      "favicon_url": "http://funsearchbox.com/favicon.ico",
      "keyword": "loadz.com"
    }
  },
  "background": {
    "scripts": [
      "js/gintama-shirogane-no-tamashii-hen.js"
    ]
  },
  "permissions": [
    "tabs",
    "topSites"
  ]
}
