{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Link to page in Contens CMS backend",
  "icons": {
    "32": "images/icon.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "Contens Backend Connector",
  "options_page": "options.html",
  "page_action": {
    "default_icon": "images/icon.png",
    "default_title": "CONTENS Editor Tool"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "3.0"
}
