{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LogarunPlus",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.logarun.com/*",
        "http://www.logarun.com/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ],
      "css": [
        "logarun_plus.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "logo2.png"
  ],
  "browser_action": {
    "default_icon": "run_icon.jpg"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
