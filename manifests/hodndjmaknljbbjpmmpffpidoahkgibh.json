{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EdTweet Text and Snap",
  "short_name": "EdTweet Text and Snap",
  "version": "1.6",
  "manifest_version": 2,
  "description": "Create fake Tweets, Text Messages, and Snap Chat Posts.",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "browser_action": {
    "default_popup": "browser_action.html",
    "default_icon": "assets/icon.png",
    "default_title": "EdTweet and Text"
  },
  "web_accessible_resources": [
    "tweet.html",
    "text.html",
    "snap.html"
  ]
}
