{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "We Love HTML5 LOGO!",
  "manifest_version": 2,
  "version": "0.5.55",
  "description": "It is adapted for all the sites in HTML5 Font.",
  "icons": {
    "48": "HTML5_Logo_48.png",
    "128": "HTML5_Logo_128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
