{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Caller Name Lookup",
  "version": "1.7",
  "description": "Look up the most recent owner and the location of that number.",
  "permissions": [
    "storage",
    "https://callername.com/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon_x16.png",
      "32": "img/icon_x32.png",
      "48": "img/icon_x48.png",
      "128": "img/icon_x128.png"
    }
  },
  "icons": {
    "16": "img/icon_x16.png",
    "32": "img/icon_x32.png",
    "48": "img/icon_x48.png",
    "128": "img/icon_x128.png"
  },
  "manifest_version": 2
}
