{
  "background": {
    "scripts": [
      "byrusd/bg.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "lib/jq/jquery-2.1.1.min.js",
        "byrusd/byrusd.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "default_locale": "ru",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "byrusd/logo128.png",
    "16": "byrusd/logo16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "byrusd/options/options.html",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "notifications"
  ],
  "short_name": "__MSG_appName__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.12",
  "browser_action": {
    "default_title": "BYR to USD: включено",
    "default_icon": "byrusd/bar.png"
  },
  "web_accessible_resources": [
    "byrusd/logo128.png"
  ]
}
