{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Isam Shabandri",
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "index.html",
    "default_title": "NetFortris for Office 365"
  },
  "manifest_version": 2,
  "name": "NetFortris for Office 365",
  "short_name": "O3I App",
  "icons": {
    "128": "icon-128.png"
  },
  "description": "Integrate your NetFortris Fonality cloud phone system into Microsoft Office 365â„¢.",
  "version": "1.68",
  "web_accessible_resources": [
    "phone.svg",
    "phone-icon.png",
    "loading.gif"
  ],
  "permissions": [
    "storage",
    "<all_urls>",
    "idle",
    "tabs"
  ],
  "background": {
    "scripts": [
      "/static/js/main.1640f86e.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "matches": [
        "https://*.office.com/*",
        "https://*.office365.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
