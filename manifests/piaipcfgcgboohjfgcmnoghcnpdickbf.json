{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "THOT ERASER",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "https://www.quora.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://www.quora.com/"
  ]
}
