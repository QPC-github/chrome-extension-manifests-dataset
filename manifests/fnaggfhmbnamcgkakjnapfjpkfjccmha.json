{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Filibuster",
  "description": "Twitter Without Politics",
  "version": "1.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "JS/main.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "JS/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon/iconOff.png"
  },
  "web_accessible_resources": [
    "Java/weightedWords.js"
  ],
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}
