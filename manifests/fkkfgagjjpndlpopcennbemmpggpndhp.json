{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Stockstream.tv Screenshare (dev)",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Stockstream.tv Screenshare (dev)",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.0.1",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow screen sharing in Stockstream.tv Web App (dev).",
  "externally_connectable": {
    "matches": [
      "https://*.jazzypail.com/*",
      "http://localhost:8080/*"
    ]
  },
  "icons": {
    "128": "128w.png",
    "16": "16w.png",
    "48": "48w.png"
  },
  "browser_action": {
    "default_icon": "16w.png"
  }
}
