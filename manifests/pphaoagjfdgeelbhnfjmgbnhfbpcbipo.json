{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Dr. PDA",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Central de notificações do Dr. PDA Mobile: receba avisos de posts e acesse facilmente o nosso site à partir de apenas 1 clique.",
  "permissions": [
    "background",
    "notifications",
    "http://feeds.feedburner.com/*"
  ],
  "icons": {
    "24": "images/icons/header_24.ico",
    "48": "images/icons/header_48.ico",
    "16": "images/icons/header_16.ico",
    "32": "images/icons/header_32.ico",
    "56": "images/icons/header_56.ico",
    "40": "images/icons/header_40.ico",
    "120": "images/icons/header_120.ico",
    "88": "images/icons/header_88.ico",
    "64": "images/icons/header_64.ico",
    "72": "images/icons/header_72.ico",
    "112": "images/icons/header_112.ico",
    "128": "images/icons/header_128.ico",
    "8": "images/icons/header_8.ico",
    "80": "images/icons/header_80.ico",
    "104": "images/icons/header_104.ico",
    "96": "images/icons/header_96.ico"
  },
  "background": {
    "page": "./background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icons/header_32.ico"
  }
}
