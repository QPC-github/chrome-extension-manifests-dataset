{
  "author": "Louder",
  "manifest_version": 2,
  "minimum_chrome_version": "32",
  "name": "Louder - Amplify your music",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webview",
    "identity",
    "browser",
    "notifications",
    "gcm",
    "storage"
  ],
  "short_name": "Louder",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.5.6",
  "app": {
    "background": {
      "scripts": [
        "/assets/js/vendor/jquery.min.js",
        "/assets/js/config.js",
        "/assets/js/background.js",
        "/assets/js/vendor/firebase.js"
      ]
    }
  },
  "commands": {
    "play": {
      "description": "Play, pause or stop playback",
      "suggested_key": {
        "default": "MediaPlayPause",
        "mac": "MacCtrl+Shift+S",
        "windows": "Ctrl+Shift+S"
      }
    },
    "skip": {
      "description": "Skip song",
      "suggested_key": {
        "default": "MediaNextTrack",
        "mac": "MacCtrl+Shift+X",
        "windows": "Ctrl+Shift+X"
      }
    },
    "thumbdown": {
      "description": "Thumbdown",
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "MacCtrl+Shift+Z",
        "windows": "Ctrl+Shift+Z"
      }
    },
    "thumbup": {
      "description": "Thumbup",
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "MacCtrl+Shift+A",
        "windows": "Ctrl+Shift+A"
      }
    }
  },
  "description": "Louder helps you access your favorite music apps from your desktop.",
  "icons": {
    "128": "/assets/img/128.png",
    "16": "/assets/img/16.png",
    "256": "/assets/img/256.png",
    "48": "/assets/img/48.png",
    "96": "/assets/img/96.png"
  }
}
