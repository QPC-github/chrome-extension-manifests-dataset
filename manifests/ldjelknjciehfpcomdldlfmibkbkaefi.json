{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Interactive Periodic Table",
  "description": "Clickable and interactive periodic table of elements.",
  "version": "0.0.0.1",
  "offline_enabled": true,
  "app": {
    "urls": [
      "http://www.freeperiodictable.com/?chromeapp"
    ],
    "launch": {
      "web_url": "http://www.freeperiodictable.com/?chromeapp"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
