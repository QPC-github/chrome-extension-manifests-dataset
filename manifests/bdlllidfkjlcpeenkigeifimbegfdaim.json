{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vipon Extension",
  "description": "This extension makes requesting an offer Vipon easy and fast, with one click.",
  "version": "1.0",
  "background": {
    "scripts": [
      "jquery.js",
      "myscript.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.vipon.com/"
      ],
      "js": [
        "jquery.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
