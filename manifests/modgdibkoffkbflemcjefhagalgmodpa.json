{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Suporte Mov Sistemas",
  "version": "1.0.7",
  "manifest_version": 2,
  "description": "Está com dúvida? Entre em contato conosco.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Suporte Mov",
    "default_icon": "i128.png"
  },
  "icons": {
    "16": "i16.png",
    "48": "i48.png",
    "128": "i128.png",
    "300": "i300.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
