{
  "name": "IP Whois & Flags Chrome & Websites Rating",
  "version": "4.0",
  "default_locale": "en",
  "manifest_version": 2,
  "description": "Display Website Country Flag. Website Whois - how many Visitors are visiting, Reputation Reviews, Antivirus Check, Alexa, Incognito",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "homepage_url": "https://myip.ms",
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "images/world.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://plugin.myip.ms/*",
    "tabs",
    "chrome://favicon/",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
