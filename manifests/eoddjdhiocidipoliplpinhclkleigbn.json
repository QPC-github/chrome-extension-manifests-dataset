{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Groen Growshop",
  "version": "1.0",
  "description": "Extension desarrollada por Groen Growshop para facilitar acceso a clientes",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "Groen Growshop",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "unlimitedStorage",
    "notifications",
    "https://ajax.googleapis.com/",
    "http://*.groen.cl/"
  ]
}
