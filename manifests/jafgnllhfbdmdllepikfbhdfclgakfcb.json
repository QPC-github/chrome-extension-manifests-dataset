    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon19.png"
    },
    "default_title": "バンジーHY"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitcasting.tv/*",
        "*://twitcasting.tv/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/html2canvas.js",
        "js/content.js"
      ]
    }
  ]
}
