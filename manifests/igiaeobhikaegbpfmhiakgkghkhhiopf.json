{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scout by LeadIQ - find leads while you browse",
  "short_name": "Scout",
  "version": "1.4.0",
  "manifest_version": 2,
  "description": "Scout helps you find contacts and emails at your target accounts",
  "icons": {
    "32": "images/good-pupper-32.png",
    "128": "images/good-pupper-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "scout-popup.html"
  },
  "content_security_policy": "script-src 'self'; frame-src https://*.leadiq.com https://accounts.google.com; object-src 'self'"
}
