{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One Click",
  "version": "1.0",
  "description": "One click to open the popular websites",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
