{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WWW Search",
  "short_name": "WWW Search",
  "description": "Expands the context menu's 'search for' allowing search the selected text with different search engines..",
  "version": "1.0.2",
  "minimum_chrome_version": "35.0.1916",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "cm_script.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "cs_script.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "128_wsearch.png",
    "19": "128_wsearch.png",
    "38": "128_wsearch.png",
    "48": "128_wsearch.png",
    "128": "128_wsearch.png"
  },
  "browser_action": {
    "default_icon": "128_wsearch.png",
    "default_popup": "popup.html"
  }
}
