{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CoreJet JIRA extracter",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Creates CoreJet test skeletons from scenarios on the current JIRA ticket page",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
