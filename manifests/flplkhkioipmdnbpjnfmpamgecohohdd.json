{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dharana Online Extension",
  "homepage_url": "https://dharana.online/",
  "description": "Ad free Internet by direct rewards to Creators",
  "default_locale": "en",
  "icons": {
    "16": "icons/do_logo_16.png",
    "48": "icons/do_logo_48.png",
    "128": "icons/do_logo_128.png"
  },
  "permissions": [
    "identity",
    "notifications",
    "storage",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "Dharana Online",
    "default_icon": {
      "16": "icons/do_logo_16.png",
      "48": "icons/do_logo_48.png"
    }
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "version": "4.0.0"
}
