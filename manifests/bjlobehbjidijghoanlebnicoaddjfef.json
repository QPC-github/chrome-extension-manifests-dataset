{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Loadgeneral Home Page",
  "short_name": "LG Home Page",
  "version": "1.0.2",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "This allows you to quickly navigate to the Loadgeneral testing tool.",
  "icons": {
    "32": "lg32.png",
    "64": "lg64.png",
    "128": "lg128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "lg.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
