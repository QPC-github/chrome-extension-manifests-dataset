{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Squaresie",
  "version": "1.0.1",
  "description": "Removes rounded corners from images on Facebook.",
  "icons": {
    "48": "icon.png",
    "96": "icon@2x.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "squaresie.js"
      ]
    }
  ]
}
