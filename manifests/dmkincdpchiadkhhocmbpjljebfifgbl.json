{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ChromeDuino",
  "description": "Create Arduino-compatible sketches on Chromebook or Chrome Browser",
  "version": "1.2",
  "manifest_version": 2,
  "permissions": [
    "serial",
    "notifications",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ],
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
