{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Background Process",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Keeps a background process open.",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "background"
  ]
}
