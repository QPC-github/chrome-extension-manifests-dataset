{
  "manifest_version": 2,
  "name": "Twitch VOD Spoiler Free",
  "version": "1.3.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*",
        "https://www.youtube.com/*"
      ],
      "css": [
        "assets/twitchVodPrevention.css"
      ]
    }
  ],
  "icons": {
    "128": "assets/iconSpoilerFree.png"
  },
  "browser_action": {
    "default_icon": "assets/iconSpoilerFree.png"
  },
  "short_name": "TWVODFREE",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "assets/twitchVodPrevention.css"
  ]
}
