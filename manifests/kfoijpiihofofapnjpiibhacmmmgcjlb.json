{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LeadCookie clipper",
  "description": "Send clipped leads to AirTable",
  "version": "0.2.7.4",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "cookies",
    "webNavigation",
    "https://api.airtable.com/",
    "http://localhost:4000/",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "img/icon-16px.png",
    "default_title": "leadcookie"
  },
  "background": {
    "page": "templates/background.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/lib/jquery.js",
        "js/contentscript.js"
      ],
      "matches": [
        "https://www.linkedin.com/sales/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "img/icon-128px.png",
    "16": "img/icon-16px.png",
    "48": "img/icon-48px.png"
  }
}
