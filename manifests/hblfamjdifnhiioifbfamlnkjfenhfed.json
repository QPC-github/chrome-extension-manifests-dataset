{
  "background": {
    "persistent": true,
    "scripts": [
      "js/bin/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/38.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/smPopup.css",
        "css/tip.css",
        "css/jquery-ui.css",
        "css/detail.css",
        "css/calculator.css",
        "css/swiper.min.css"
      ],
      "js": [
        "js/bin/content_scripts.js"
      ],
      "matches": [
        "*://www.amazon.com/*",
        "*://www.amazon.co.jp/*",
        "*://www.amazon.co.uk/*",
        "*://www.amazon.fr/*",
        "*://www.amazon.de/*",
        "*://www.amazon.es/*",
        "*://www.amazon.it/*",
        "*://www.amazon.com.au/*",
        "*://www.amazon.ca/*",
        "*://www.amazon.in/*",
        "*://www.amazon.com.br/*",
        "*://www.amazon.com.mx/*",
        "*://www.amazon.ae/*",
        "*://www.amazon.com.tr/*",
        "*://sellercentral.amazon.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "__MSG_description__",
  "homepage_url": "https://www.sellermotor.com",
  "icons": {
    "128": "images/128.png",
    "38": "images/38.png",
    "48": "images/48.png"
  },
  "manifest_version": 2,
  "name": "SellerMotor",
  "optional_permissions": [
    "*://www.amazon.it/*",
    "*://www.amazon.es/*",
    "*://*.amazonservices.in/*",
    "*://www.amazon.co.jp/*"
  ],
  "permissions": [
    "*://*.qq.com/*",
    "*://*.sellermotor.com/*",
    "tabs",
    "background",
    "storage",
    "unlimitedStorage",
    "*://www.amazon.com/*",
    "*://www.amazon.fr/*",
    "*://www.amazon.de/*",
    "*://www.amazon.co.uk/*",
    "*://www.amazon.ca/*",
    "*://www.amazon.in/*",
    "*://www.amazon.com.au/*",
    "*://*.amazonservices.com/*",
    "*://*.amazonservices.ca/*",
    "*://www.amazon.com.br/*",
    "*://www.amazon.com.mx/*",
    "*://www.amazon.ae/*",
    "*://www.amazon.com.tr/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.4.0.0908",
  "web_accessible_resources": [
    "*"
  ],
  "default_locale": "en"
}
