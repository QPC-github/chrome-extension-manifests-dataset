{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pengi Paint",
  "description": "Simple Photo Editor for Google Chromeâ„¢",
  "version": "0.8.21",
  "manifest_version": 2,
  "offline_enabled": true,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": [
        "min.js",
        "app/min.js",
        "app/scripts/min.js",
        "app/scripts/LostUI/min.js"
      ]
    }
  },
  "file_handlers": {
    "image": {
      "types": [
        "image/png",
        "image/jpeg",
        "image/jpg",
        "image/bmp",
        "image/gif"
      ],
      "extensions": [
        "jpg",
        "bmp",
        "png",
        "gif"
      ],
      "title": "Pengi Paint"
    }
  },
  "permissions": [
    "fileSystem",
    "fileSystem.write",
    "storage"
  ],
  "optional_permissions": [
    "clipboardRead"
  ],
  "icons": {
    "16": "icon_small.png",
    "128": "icon.png"
  }
}
