{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Where to buy",
  "version": "1.0",
  "manifest_version": 2,
  "description": "World's first search engine for shopping.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "where.html"
  }
}
