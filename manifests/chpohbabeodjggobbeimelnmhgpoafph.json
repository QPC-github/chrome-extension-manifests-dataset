{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nobody's Fool",
  "version": "0.0.1",
  "description": "Provides a safeguard against falling for online pranks on or near April Fool's day.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "scripts.js"
      ]
    }
  ]
}
