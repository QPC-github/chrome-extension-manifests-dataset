{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Classic World Clocks",
  "short_name": "World Clocks",
  "version": "2.2",
  "author": "justdecodeme",
  "description": "Super useful classic and stylish Chrome extension to add world clock of your choice, and keep up to date with time of any place!",
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.min.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
