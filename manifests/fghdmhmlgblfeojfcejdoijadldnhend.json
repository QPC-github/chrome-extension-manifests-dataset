{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reverse Watch Later List on YouTube",
  "description": "Adds a button to reverse the order of your Watch Later List",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery-1.8.3.min.js",
        "youtubes.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "version": "1.3",
  "manifest_version": 2
}
