{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "0.0.2",
  "short_name": "__MSG_short_name__",
  "manifest_version": 2,
  "description": "__MSG_description__",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/yikke-logo-19.png",
      "38": "icons/yikke-logo-38.png"
    },
    "default_title": "__MSG_name__"
  },
  "icons": {
    "16": "icons/yikke-logo-16.png",
    "48": "icons/yikke-logo-48.png",
    "128": "icons/yikke-logo-128.png"
  }
}
