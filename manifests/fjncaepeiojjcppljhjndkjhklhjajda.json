{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AID Cincinnati",
  "short_name": "AID_Cincy",
  "description": "This extension gives updates about the upcoming AID Cincinnati events.",
  "version": "1.4",
  "permissions": [
    "https://www.corsproxy.com/cincinnati.aidindia.org",
    "http://cincinnati.aidindia.org",
    "https://fonts.googleapis.com/css?family=Karma",
    "https://secure.aidindia.org/donate/cincinnati",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "aid.gif",
    "default_popup": "page.html"
  }
}
