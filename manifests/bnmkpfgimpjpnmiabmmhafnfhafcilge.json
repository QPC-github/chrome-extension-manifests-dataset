{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ceheed Screen Cast",
  "short_name": "CSC",
  "description": "The extension records screen for Ceheed",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://httpswww.ceheed.com/*",
      "*://httpswww.ceheed.com/*",
      "*://*.ceheed.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
