{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better Mixer Chat",
  "version": "1.0.8",
  "description": "Blurs deleted chat messages",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://mixer.com/*"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "bremix16.png",
      "32": "bremix32.png",
      "48": "bremix48.png",
      "128": "bremix128.png"
    }
  },
  "icons": {
    "16": "bremix16.png",
    "32": "bremix32.png",
    "48": "bremix48.png",
    "128": "bremix128.png"
  }
}
