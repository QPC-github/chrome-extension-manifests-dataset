{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kick Them Out",
  "version": "0.5",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.mitbbs.com/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
