{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "me2day KickQuickOut",
  "version": "0.1",
  "manifest_version": 2,
  "description": "미투데이 퀵뷰, 추천친구 비활성화 / Kick me2day quickview feature out",
  "icons": {
    "16": "me2day.ico",
    "48": "me2day.ico",
    "128": "me2day.ico"
  },
  "content_security_policy": "default-src 'self'",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "me2day_kickquickout.js"
      ],
      "matches": [
        "http://me2day.net/*"
      ]
    }
  ]
}
