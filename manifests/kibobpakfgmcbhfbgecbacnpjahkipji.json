{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "Google Bookmark",
  "name": "Google Bookmark",
  "description": "Google Bookmarklet Chrome Extension",
  "version": "1.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Google Bookmarklet",
    "default_icon": "bookmark_16x16.png"
  },
  "manifest_version": 2
}
