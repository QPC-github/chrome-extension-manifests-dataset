{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "COSSbot Private API Validation",
  "version": "0.0.3",
  "description": "Request private API validation from logged in chrome session of COSS.io",
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "http://localhost/"
  ],
  "browser_action": {
    "default_icon": "cossbot128.png",
    "default_title": "COSSbot Validator",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "cossbot48.png",
    "128": "cossbot128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "minimum_chrome_version": "41",
  "manifest_version": 2
}
