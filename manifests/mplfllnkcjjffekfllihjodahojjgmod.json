{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Phone audit",
  "description": "This extension detects phone numbers in web page and mark them by adding green border and gray background",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "main.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "imgs/16.png",
    "32": "imgs/32.png",
    "64": "imgs/64.png",
    "128": "imgs/128.png"
  },
  "permissions": [
    "activeTab",
    "background",
    "bookmarks",
    "contentSettings",
    "tabs",
    "topSites",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ]
}
