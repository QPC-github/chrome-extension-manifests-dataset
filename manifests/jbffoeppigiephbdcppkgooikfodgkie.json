{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DigiNovo Screensharing for A1",
  "description": "DigiNovo screensharing extension needed for A1.",
  "version": "1.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "short_name": "DigiNovo Screensharing",
  "icons": {},
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://a1.diginovo.io/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://a1.diginovo.io/*"
    ]
  }
}
