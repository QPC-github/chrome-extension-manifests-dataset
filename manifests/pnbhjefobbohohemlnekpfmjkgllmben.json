  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png"
  },
  "name": "KU PT Wrangler",
  "description": "Extension to input KU PT allocations in a batch.",
  "version": "1.0.2",
  "background": {
