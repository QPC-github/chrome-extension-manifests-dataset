{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "UrlToQR",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Converts the current URL to a QR code",
  "homepage_url": "http://sqlzilla.com",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "ToQR",
    "default_popup": "render.html"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
