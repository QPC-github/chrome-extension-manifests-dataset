{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ExtensionTunes",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Play music from your toolbar",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "32": "ico1.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "ico.png"
  },
  "permissions": [
    "notifications"
  ]
}
