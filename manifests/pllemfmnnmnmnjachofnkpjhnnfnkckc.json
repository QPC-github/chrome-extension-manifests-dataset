{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "React",
  "version": "0.1",
  "author": "Rishi Mohan",
  "description": "A Minimal New Tab Page",
  "homepage_url": "http://omgminimal.com",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
