{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Follower for Soundcloud",
  "version": "0.0.3.3",
  "manifest_version": 2,
  "description": "Mass Follow,Unfollow,Like,Unlike,Repost and Unrepost in Soundcloud",
  "homepage_url": "http://toolsfor.us",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "key": "",
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Click to Open",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "https://soundcloud.com/*",
    "identity",
    "storage"
  ]
}
