{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "True&Simple - Informações",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "back.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Informações",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
