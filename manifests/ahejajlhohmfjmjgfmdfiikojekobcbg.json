{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GBF for Chrome",
  "version": "0.0.0.14",
  "description": "GBF for Chrome",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "main.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
