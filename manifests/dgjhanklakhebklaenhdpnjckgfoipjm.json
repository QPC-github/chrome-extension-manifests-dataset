{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Code Rovers",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Teaches you how to code in an interactive way.",
  "browser_action": {
    "default_icon": "/icon.png",
    "default_title": "Code Rovers",
    "default_popup": "/popup.html"
