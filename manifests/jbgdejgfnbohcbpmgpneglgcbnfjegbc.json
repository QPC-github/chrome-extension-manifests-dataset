{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "New Tab Blankificator",
  "author": "Lonnie Waugh - Oneduality",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Just makes the new tab blank!",
  "homepage_url": "https://www.oneduality.com",
  "chrome_url_overrides": {
    "newtab": "src/bg/background.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "*://*/*/chrome/newtab"
  ]
}
