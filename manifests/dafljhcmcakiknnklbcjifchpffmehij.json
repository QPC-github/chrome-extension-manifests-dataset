{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Atlas",
  "version": "1.9",
  "manifest_version": 2,
  "description": "Find postal codes quickly.",
  "permissions": [
    "https://maps.googleapis.com/*",
    "geolocation"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Atlas",
    "default_popup": "popup.htm"
  }
}
