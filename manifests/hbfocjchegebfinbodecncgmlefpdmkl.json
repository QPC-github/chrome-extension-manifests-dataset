{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "shuqian.site书签网收藏插件",
  "version": "2.4",
  "description": "A browser action with a popup dump of all bookmarks, including add.一个浏览器操作，包含所有书签的弹出，包括添加等。",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "书签保存",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
