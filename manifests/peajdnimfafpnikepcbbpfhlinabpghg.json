{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitch OnAir",
  "version": "1.8.6",
  "author": "secvencya AT gmail DOT com",
  "manifest_version": 2,
  "description": "Check your best twitch players when do they playing.",
  "options_page": "options.html",
  "icons": {
    "16": "twitch16.png",
    "48": "twitch48.png",
    "128": "twitch128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "twitch.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js",
      "twbg.js",
      "noti.js"
    ]
  },
  "permissions": [
    "storage"
  ]
}
