{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "short_name": "Widilo",
  "description": "__MSG_extensionDescription__",
  "author": "https://www.widilo.com",
  "homepage_url": "https://chrome.google.com/webstore/detail/widilo-cashback-reminder/jlcaefilcommembbekhjjkibeieffchd",
  "default_locale": "en_GB",
  "version": "2.0.0.3301",
  "browser_action": {
    "default_icon": "icons/browser_action/logo_small_primary_32.png",
    "default_popup": "index.html#/popup"
  },
  "icons": {
    "16": "icons/main/logo_small_primary_16.png",
    "32": "icons/main/logo_small_primary_32.png",
    "48": "icons/main/logo_small_primary_48.png",
    "64": "icons/main/logo_small_primary_64.png",
    "128": "icons/main/logo_small_primary_128.png"
  },
  "background": {
    "page": "index.html#/background",
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "https://www.widilo.fr/*",
      "https://www.widilo.es/*",
      "https://www.widilo.co.uk/*",
      "https://www.widilo.it/*",
      "https://www.widilo-gutscheine.de/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://elian.blob.core.windows.net/app-extension/ https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self' https://elian.blob.core.windows.net/app-extension/; style-src 'self' https://elian.blob.core.windows.net/app-extension/ https://fonts.googleapis.com 'unsafe-inline'",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "alarms",
    "<all_urls>"
  ]
}
