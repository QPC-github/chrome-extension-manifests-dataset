{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TagMan URL Changer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Replace the requests on a page",
  "browser_action": {
    "default_title": "TagMan URL Changer",
    "default_icon": "48.png"
  },
  "icons": {
    "128": "128.png",
    "48": "48.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
