{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tril",
  "author": "Nick Gottschlich @NickGottschlich",
  "version": "1.0.1",
  "description": "A Google Chrome Extension that swaps @dril and @realDonaldTrump tweets",
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.twitter.com/*",
        "https://*.twitter.com/*"
      ],
      "js": [
        "tril.js"
      ]
    }
  ],
  "permissions": [
    "http://www.twitter.com/"
  ]
}
