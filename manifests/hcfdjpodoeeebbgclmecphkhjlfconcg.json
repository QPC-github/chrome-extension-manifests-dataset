{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Goodnite",
  "version": "0.3",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "custom-block.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "goodnite64.png"
  },
  "permissions": [
    "storage"
  ]
}
