{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DEVELOPERS",
  "description": "Steve Ballmer yells the word 'DEVELOPERS' when you mouse over it",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "inject.js"
      ]
    }
  ]
}
