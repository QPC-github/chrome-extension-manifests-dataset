{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "殲滅t.co",
  "description": "ブラウザでTwitterを表示する際にt.coを展開する拡張です",
  "version": "1.7",
  "permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
