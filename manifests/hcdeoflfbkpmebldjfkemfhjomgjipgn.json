{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuickClean",
  "description": "Clear ALL Browsing Data by deleting downloads, cached files and browsing history.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "browsingData"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "clear-browsing-data": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      },
      "description": "QuickClean"
    }
  }
}
