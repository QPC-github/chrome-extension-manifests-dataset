{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NoteTaker",
  "description": "Take quick notes and save them without moving away from your browser!",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "logo3.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "FileSaver.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "FileSaver.js",
        "background.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}
