{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Screenshare Extension",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "TutorMeet+ - Screenshare Extension",
  "permissions": [
    "desktopCapture",
    "<all_urls>"
  ],
  "short_name": "Meet anyone, anytime, anywhere, Free",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
