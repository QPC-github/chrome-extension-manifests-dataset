{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Duckpower 1.1",
  "short_name": "Duckpower",
  "description": "Horsepower is so last year. Measure power in duckpower. This plugin will replace horsepower with vital duckpower measurements.",
  "version": "1.1.0",
  "icons": {
    "32": "duckpower32.png",
    "64": "duckpower64.png",
    "128": "duckpower128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
