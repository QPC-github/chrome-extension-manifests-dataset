{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Info",
  "description": "This extension helps you to get the basic details of the page with a few Developer Info!",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Trinath Reddy Inturi",
  "permissions": [
    "printerProvider",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "img.png"
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.slim.min.js",
      "popper.min.js",
      "bootstrap.min.js",
      "script.js"
    ],
    "persistent": false
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Shortcut",
      "global": true
    }
  }
}
