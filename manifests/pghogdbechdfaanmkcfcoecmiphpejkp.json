{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Space Invaders",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Play the arcade classic Space Invaders right now.",
  "background": {
    "scripts": [
      "js/jquery-2.1.4.js",
      "js/underscore.js",
      "js/background.js",
      "build/nes.js",
      "build/nes.min.js",
      "index.js"
    ]
  },
  "permissions": [
    "idle",
    "tabs",
    "storage",
    "background"
  ],
  "browser_action": {
    "default_icon": "images/icon_19.png",
    "default_title": "Invasion NES",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon_128.png",
    "64": "images/icon_64.png",
    "32": "images/icon_32.png"
  }
}
