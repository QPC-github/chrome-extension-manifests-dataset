{
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/funcs.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/128x128.png",
    "default_popup": "popup.html",
    "default_title": "Расширения для блокировка спама в VK"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/funcs.js",
        "js/ant.js"
      ],
      "matches": [
        "*://vk.com/*"
      ]
    }
  ],
  "description": "Расширения для блокировки спама в VK",
  "icons": {
    "128": "icons/128x128.png",
    "256": "icons/256x256.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Антиспам VK.COM",
  "permissions": [
    "alarms",
    "tabs",
    "contextMenus",
    "notifications",
    "*://*/*",
    "geolocation"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.3"
}
