{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Loadgeneral Proxy Switcher",
  "short_name": "ProxySwitcher",
  "version": "1.0.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "This allows you to quickly change your proxy settings to the Loadgeneral proxy.",
  "icons": {
    "64": "GreenLight64.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "RedLight.ico"
  },
  "permissions": [
    "storage",
    "tabs",
    "proxy"
  ]
}
