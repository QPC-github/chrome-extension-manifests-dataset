{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Create New CodePen",
  "author": "Levi Olson",
  "description": "Open a new tab pointing to https://codepen.io/pen",
  "manifest_version": 2,
  "version": "1.0.3",
  "icons": {
    "32": "icon-32.png",
    "96": "icon-96.png"
  },
  "browser_action": {
    "default_icon": "icon-32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
