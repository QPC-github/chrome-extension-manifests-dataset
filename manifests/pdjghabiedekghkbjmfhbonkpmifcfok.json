{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Baca Saja",
  "version": "1.1.2",
  "description": "Menampilkan isi artikel saja, simply give you easy way to read the news without any inconvenience of website elements",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "image/icon-news128.png",
    "64": "image/icon-news64.png",
    "32": "image/icon-news32.png",
    "16": "image/icon-news16.png"
  },
  "background": {
    "page": "popup.html"
  },
  "browser_action": {
    "default_icon": "/image/icon-news32.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "author": "Abdul Gafur"
}
