{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [
        "*bankofamerica.com*"
      ],
      "include_globs": [
        "*"
      ],
      "js": [
        "jquery.js",
        "jquery-ui.js",
        "jquery.jgrowl.js",
        "NCGCHover.js"
      ],
      "css": [
        "jquery-ui.css",
        "jquery.jgrowl.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "NCATS Find",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png"
  },
  "converted_from_user_script": true,
  "description": "Structure ID parser / chemical structure viewer",
  "name": "NCATS Find",
  "version": "3.0.1",
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs",
    "contextMenus",
    "clipboardWrite"
  ],
  "commands": {
    "capture": {
      "suggested_key": {
        "default": "Shift+Alt+K",
        "mac": "Shift+Alt+K"
      },
      "global": true,
      "description": "Select screenshot area to convert to chemical structure"
    }
  },
  "web_accessible_resources": [
    "ketcher/ketcher.html"
  ]
}
