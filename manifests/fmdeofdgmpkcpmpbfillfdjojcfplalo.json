{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Constant Contact",
  "version": "2.0.1",
  "permissions": [
    "tabs",
    "http://*.constantcontact.com/"
  ],
  "description": "Quick links for ease of using Constant Contact.",
  "browser_action": {
    "default_icon": "default_icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
