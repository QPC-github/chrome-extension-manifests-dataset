{
  "author": "WhatDrugs",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/skin/icon.png",
    "default_popup": "/popup.html",
    "default_title": "Switcher I/O"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery-1.10.2.min.js",
        "js/injection.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Switcher I/O - исправление непереключенной раскладки",
  "homepage_url": "http://switcher.ml/",
  "icons": {
    "128": "skin/128.png",
    "16": "skin/16.png",
    "48": "skin/48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Switcher I/O",
  "permissions": [
    "contextMenus"
  ],
  "short_name": "Смена раскладки",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0.6"
}
