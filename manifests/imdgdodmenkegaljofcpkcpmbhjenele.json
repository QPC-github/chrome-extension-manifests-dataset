{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AntiUPnext",
  "version": "2.0.2",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.facebook.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
