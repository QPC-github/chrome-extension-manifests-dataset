{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Workfront Restyler",
  "version": "0.1.1",
  "short_name": "Restyles the storyboard views",
  "description": "Restyles the storyboard views",
  "author": "Wendtcode",
  "homepage_url": "https://chrome.google.com/webstore/detail/workfront-restyler/nhmaokpikadkjofbcnmpjeeojjlgobfc",
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Workfront Restyler"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.attask-ondemand.com/*",
        "https://*.attask-ondemand.com/*"
      ],
      "css": [
        "contentscript.css"
      ]
    }
  ],
  "permissions": [
    "http://*.attask-ondemand.com/*",
    "https://*.attask-ondemand.com/*"
  ]
}
