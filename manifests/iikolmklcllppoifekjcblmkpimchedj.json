{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quickr",
  "description": "Quicly access your local page with a QR-code",
  "version": "0.78",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "quickr.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
