{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Highfive, Inc.",
  "description": "Launch Highfive App from Chrome",
  "icons": {
    "128": "icon_128.png"
  },
  "externally_connectable": {
    "matches": [
      "https://*.highfive.com/*"
    ]
  },
  "background": {
    "scripts": [
      "background.min.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Highfive",
  "optional_permissions": [],
  "permissions": [
    "nativeMessaging",
    "https://*.highfive.com/*",
    "desktopCapture",
    "https://www.google.com/calendar/*",
    "https://calendar.google.com/calendar/*"
  ],
  "version": "0.3.2"
}
