{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "OSX Resizer",
  "manifest_version": 2,
  "version": "0.0.0.2",
  "description": "Allow OSX users to view sites in a window smaller than 400px",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "mobile.png",
    "default_popup": "window.html"
  }
}
