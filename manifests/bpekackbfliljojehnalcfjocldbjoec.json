{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smartdocs",
  "version": "0.0.0.10",
  "manifest_version": 2,
  "description": "Assinatura digital de documentos do SmartFactor.",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "icons": {
    "16": "smartdocs32.png",
    "32": "smartdocs32.png",
    "48": "smartdocs32.png",
    "128": "smartdocs32.png"
  },
  "externally_connectable": {
    "matches": [
      "*://127.0.0.1/*",
      "*://localhost/*",
      "*://www.smartfactor.com.br/*",
      "*://www2.smartfactor.com.br/*",
      "*://smartfactor.com.br/*",
      "*://www.smartdocs.com.br/*",
      "*://smartdocs.com.br/*",
      "*://www2.smartdocs.com.br/*",
      "*://smartdocs.us-east-1.elasticbeanstalk.com/*",
      "*://www.doc2you.com.br/*"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "*://127.0.0.1/*",
    "*://localhost/*",
    "*://www.smartfactor.com.br/*",
    "*://www2.smartfactor.com.br/*",
    "*://smartfactor.com.br/*",
    "*://www.smartdocs.com.br/*",
    "*://smartdocs.com.br/*",
    "*://www2.smartdocs.com.br/*",
    "*://smartdocs.us-east-1.elasticbeanstalk.com/*",
    "*://www.doc2you.com.br/*"
  ]
}
