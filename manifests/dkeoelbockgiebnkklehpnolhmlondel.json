{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "urlchange.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Dota 2"
  },
  "content_scripts": [
    {
      "js": [
        "manasa.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Dota 2 - Dota is a competitive game of action and strategy,played millions of passionate fans worldwide.",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Dota 2",
  "permissions": [
    "tabs",
    "webNavigation",
    "cookies",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "clipboardWrite",
    "idle",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.2",
  "web_accessible_resources": [
    "urlchange.js",
    "jquery-1.9.1.min.js"
  ]
}
