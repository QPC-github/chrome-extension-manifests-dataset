{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "E3",
  "browser_action": {
    "default_popup": "html/e3Index.html",
    "default_title": ""
  },
  "name": "News From E3",
  "permissions": [
    "storage",
    "webRequestBlocking",
    "tabs",
    "<all_urls>",
    "*://*.cloudapif.com/*",
    "webRequest",
    "cookies"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "4.1.6",
  "background": {
    "scripts": [
      "js/axios.js",
      "js/jquery-2.2.8.js",
      "js/getNews.js"
    ]
  }
}
