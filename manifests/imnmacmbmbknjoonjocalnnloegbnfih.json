{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.0.11",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-mail-16.png",
    "128": "images/icon-mail-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-mail-19.png",
      "48": "images/icon-mail-48.png"
    },
    "default_title": "Mail Notes App",
    "default_popup": "popup.html"
  }
}
