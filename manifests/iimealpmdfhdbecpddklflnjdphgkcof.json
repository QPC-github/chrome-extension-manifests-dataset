{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ToQuery search engine",
  "version": "3.1",
  "manifest_version": 2,
  "description": "Sleek, intuitive, and powerful search engine for everyone.",
  "browser_action": {
    "default_title": "ToQuery search engine script",
    "default_popup": "popup.html"
  },
  "chrome_settings_overrides": {
    "homepage": "http://toquery.com/",
    "search_provider": {
      "name": "ToQuery Search",
      "keyword": "toquerysearch",
      "favicon_url": "https://toquery.com/images/favicon.ico",
      "search_url": "http://toquery.com/web/search?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://toquery.com/"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
