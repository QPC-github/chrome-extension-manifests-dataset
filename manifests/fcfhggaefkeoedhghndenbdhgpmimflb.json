{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Watch",
  "description": "You can watch Youtube videos in the browser",
  "version": "1.1",
  "browser_action": {
    "default_icon": "watchicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
