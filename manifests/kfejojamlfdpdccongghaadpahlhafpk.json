{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome Download Filter",
  "description": "Фильтр загружаемых файлов для стандартного загрузчика chrome",
  "version": "1.11",
  "permissions": [
    "downloads",
    "storage",
    "http://*/*"
  ],
  "icons": {
    "128": "./res/baricon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/filter.js"
    ]
  },
  "options_page": "options/index.html"
}
