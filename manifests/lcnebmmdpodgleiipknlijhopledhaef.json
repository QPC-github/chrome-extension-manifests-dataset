{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Search Text Fixer for RES Users",
  "version": "1.2",
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "reddit_color.user.js"
      ],
      "matches": [
        "http://www.reddit.com/*search?q=*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "This extension changes the color of previous search text for RES users using night mode."
}
