{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Milashi :: New Orders",
  "version": "1.0.1",
  "description": "Уведомления о новых заказах на сайте milashi.ru",
  "background": {
    "scripts": [
      "underscore-min.js",
      "jquery-1.7.2.min.js",
      "main.js"
    ],
    "persistent": true
  },
  "incognito": "spanning",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "128": "icon/logo-128.png",
    "48": "icon/logo-48.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://www.milashi.ru/*",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_title": "Milashi :: New Orders",
    "default_icon": "icon/logo-20.png"
  },
  "web_accessible_resources": [
    "icon/logo-48.png"
  ]
}
