{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "gbenhance.js"
      ],
      "matches": [
        "http://*.gamebanana.com/*",
        "https://*.gamebanana.com/*",
        "http://gamebanana.com/*",
        "https://gamebanana.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Various tweaks for GameBanana.",
  "icons": {
    "128": "logo128.png",
    "38": "logo38.png"
  },
  "manifest_version": 2,
  "name": "GameBanana Enhancer",
  "options_page": "opt.html",
  "permissions": [
    "storage",
    "tabs",
    "http://*.gamebanana.com/*",
    "https://*.gamebanana.com/*"
  ],
  "version": "1.3.1"
}
