{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sickle",
  "version": "1.32",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.harvestapp.com/*"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ]
    }
  ],
  "description": "Keyboard extensions for Harvest",
  "permissions": [
    "https://*.harvestapp.com/"
  ]
}
