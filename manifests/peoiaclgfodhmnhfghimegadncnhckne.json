{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "/img/icon-32.png",
    "default_title": "LeadBolt Earnings Checker"
  },
  "content_security_policy": "script-src 'self' https://www.leadbolt.net https://ajax.googleapis.com; object-src 'self'",
  "description": "Shows LeadBolt earnings for the current day and since your last payment. Refreshes each 5 minutes.",
  "icons": {
    "16": "/img/icon-16.png",
    "24": "/img/icon-24.png",
    "32": "/img/icon-32.png",
    "48": "/img/icon-48.png",
    "64": "/img/icon-64.png",
    "128": "/img/icon-128.png",
    "256": "/img/icon-256.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "4.0.302.2",
  "name": "LeadBolt Earnings Checker",
  "permissions": [
    "https://www.leadbolt.net/*",
    "tabs",
    "notifications"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
