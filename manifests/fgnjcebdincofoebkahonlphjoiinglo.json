{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitSense",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Find answers faster in GitHub and GitLab",
  "homepage_url": "https://gitsense.com",
  "options_page": "options.html",
  "icons": {
    "128": "images/icon-128.png"
  },
  "web_accessible_resources": [
    "frame.html",
    "3rdparty/octicons/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "optional_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "https://api.github.com/*",
    "https://github.com/*",
    "https://gitlab.com/*",
    "https://public.gitsense.com/*",
    "storage"
  ]
}
