{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Weather App",
  "Short name": "Weather",
  "description": "Weather App by Nadeem.",
  "manifest_version": 2,
  "version": "1.0.7",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "16": "img/icon.png",
    "64": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "geolocation",
    "storage",
    "<all_urls>"
  ]
}
