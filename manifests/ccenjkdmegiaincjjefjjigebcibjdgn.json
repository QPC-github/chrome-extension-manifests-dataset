{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Multipass",
  "description": "Multipass Chrome extension",
  "version": "1.0",
  "key": "",
  "oauth2": {
    "client_id": "427533339495-m5kq8oph2shmammc33jr3gi9dulj2d4a.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/drive.appdata",
      "https://www.googleapis.com/auth/admin.directory.user",
      "https://www.googleapis.com/auth/admin.directory.user.readonly",
      "https://www.googleapis.com/auth/admin.directory.group",
      "https://www.googleapis.com/auth/admin.directory.group.readonly"
    ]
  },
  "background": {
    "scripts": [
      "event_page.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Multipass",
    "default_icon": {
      "16": "images/icon-16.png",
      "24": "images/icon-24.png",
      "32": "images/icon-32.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "alarms",
    "cookies",
    "identity",
    "identity.email",
    "management",
    "notifications",
    "storage",
    "tabs",
    "windows",
    "https://payments.getmultipass.com/",
    "https://apis.google.com/",
    "https://www.googleapis.com/",
    "https://content.googleapis.com/"
  ],
  "web_accessible_resources": [
    "images/*",
    "fonts/roboto/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com https://www.googleapis.com https://content.googleapis.com; object-src 'self';"
}
