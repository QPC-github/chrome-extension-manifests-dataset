{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "aede.js"
    ]
  },
  "description": "Bloquea los sitios web asociados a la ley AEDE",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "AEDE block",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "version": "1.0",
  "web_accessible_resources": [
    "aede.html"
  ]
}
