{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FindWide Search Protection",
  "short_name": "FindWide",
  "description": "Protect Chrome from search engines that aren't from the Web Store.",
  "version": "1.0.7",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "findwide-icon-128.png",
    "48": "findwide-icon-48.png",
    "16": "findwide-icon-16.png"
  },
  "web_accessible_resources": [
    "pinnedSearch.html"
  ],
  "chrome_url_overrides": {
    "newtab": "pinnedSearch.htm"
  },
  "permissions": [
    "tabs",
    "storage",
    "browsingData",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
