{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "blacker"
  },
  "name": "Privasee",
  "version": "0.1.1",
  "description": "Makes sure no one can see what you're doing. Including yourself",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "content.js"
    ],
    "persistent": true
  }
}
