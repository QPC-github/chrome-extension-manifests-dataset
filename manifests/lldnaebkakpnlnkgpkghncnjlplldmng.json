{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Speed Reader",
  "description": "Creates a speed reading app for selected text.",
  "version": "1.0",
  "icons": {
    "16": "play.png",
    "48": "play.png",
    "128": "play.png"
  },
  "browser_action": {
    "default_icon": "play.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
