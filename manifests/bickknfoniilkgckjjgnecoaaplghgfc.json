{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StartPanels",
  "description": "New page with organized panels to improve your productivity looking at the right link in few seconds",
  "version": "1.7",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "author": "Davide Francesco Merico",
  "background": {
    "persitent": false
  },
  "permissions": [
    "bookmarks",
    "history",
    "sessions",
    "tabs",
    "chrome://favicon/",
    "chrome://favicon/*",
    "storage"
  ]
}
