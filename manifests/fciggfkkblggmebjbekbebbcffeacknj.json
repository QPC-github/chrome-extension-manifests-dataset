{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TIFF viewer",
  "description": "Intercepts any resource requests that end in .tif or .tiff and converts them into data urls using an emscripten port of libtiff.",
  "version": "1.1.0",
  "homepage_url": "https://github.com/my-codeworks/tiff-viewer-extension",
  "incognito": "split",
  "author": "my codeworks",
  "background": {
    "scripts": [
      "lib/tiff.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "options_ui": {
    "page": "options/options.html"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  }
}
