{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Advanced Site Checker",
  "version": "0.8.1",
  "description": "Check your site for issues.",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "Check page",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "favicon.png",
    "96": "favicon-96.png",
    "128": "favicon-128.png"
  }
}
