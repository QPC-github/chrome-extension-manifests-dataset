{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BartoLOL",
  "version": "0.0.0.1",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "description": "Replaces the game of Baseball with the game of Bartolo",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "bartolol.js"
      ],
      "run_at": "document_end"
    }
  ]
}
