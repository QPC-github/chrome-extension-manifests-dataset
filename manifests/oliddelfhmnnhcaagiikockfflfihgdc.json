{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NoTrumpAzon",
  "version": "1.0.1",
  "icons": {
    "128": "/logo.png"
  },
  "browser_action": {
    "default_icon": "/logo.png"
  },
  "background": {
    "scripts": [
      "/bg.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://www.amazon.com/*",
    "webRequest",
    "webRequestBlocking"
  ]
}
