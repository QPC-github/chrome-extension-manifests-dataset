{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "name": "SpPrice.Net",
  "description": "You can find products and price from multiple shops.",
  "version": "1.4.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "spp.html",
    "default_title": "SpPrice.Net"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "http://ip-api.com/json",
    "https://spprice.net/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
