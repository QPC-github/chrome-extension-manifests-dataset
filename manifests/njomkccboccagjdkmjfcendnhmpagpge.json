{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Cleanse",
  "short_name": "Cleanse",
  "version": "1.80",
  "offline_enabled": true,
  "manifest_version": 2,
  "description": "Deletes browsing data with the push of a button.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "history",
    "browsingData",
    "storage",
    "unlimitedStorage",
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "Icons/icon2.png",
    "default_title": "Cleanse"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "Icons/icon1.png"
  }
}
