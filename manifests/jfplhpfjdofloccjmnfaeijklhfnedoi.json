{
  "manifest_version": 2,
  "name": "Рейтинг и сравнение кэшбэк сервисов",
  "short_name": "Рейтинг и сравнение кэшбэк сервисов",
  "description": "Рейтинг, топ и сравнение лучших кэшбэк сайтов и сервисов, поможет найти самый выгодный cashback в инетернет магазинах",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "gw.js",
    "bg.js"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "bg.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
