{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DailyDota2 Twitch Notifier V2",
  "version": "2.13",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.twitch.tv/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-1.11.1.min.js",
    "contentscript.js"
  ]
}
