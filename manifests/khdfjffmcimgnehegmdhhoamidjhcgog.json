{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SO UnAnswered Filter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Clean up your SO Questions",
  "homepage_url": "http://frankthoughts.azurewebsites.net/",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Remove Answers"
  },
  "permissions": [
    "activeTab"
  ]
}
