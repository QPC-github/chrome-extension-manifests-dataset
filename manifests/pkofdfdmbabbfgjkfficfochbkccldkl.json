{
  "browser_action": {
    "default_icon": "stop-2.png",
    "default_title": "PushStop"
  },
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "manifest_version": 2,
  "name": "PushStop",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "icons": {
    "256": "stop.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
