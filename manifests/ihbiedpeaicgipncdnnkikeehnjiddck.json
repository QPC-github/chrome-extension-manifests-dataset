{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Weather",
  "version": "2.0.6.6",
  "manifest_version": 2,
  "description": "__MSG_manifest_desc__",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "32": "icon-32x32.png",
    "128": "icon-128x128.png"
  },
  "browser_action": {
    "default_icon": "icon-32x32.png",
    "default_title": "Weather",
    "default_popup": "popup.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://www.weather-extension.com/img/icons/favicon-32x32.png",
      "is_default": true,
      "keyword": "websearch",
      "name": "Web ",
      "search_url": "https://search.weather-extension.com/query?q={searchTerms}",
      "suggest_url": "https://search.weather-extension.com/suggestion?q={searchTerms}"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://maps.googleapis.com;  object-src 'self'"
}
