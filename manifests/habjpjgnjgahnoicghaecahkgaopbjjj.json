{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bitbucket Code Styler",
  "description": "Bitbucket code-diff dark color theme",
  "version": "1.0",
  "permissions": [
    "*://*.bitbucket.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.bitbucket.org/*"
      ],
      "css": [
        "code.css"
      ]
    }
  ]
}
