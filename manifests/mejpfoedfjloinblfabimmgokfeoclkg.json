{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SomethingAwful Bookmark Opener",
  "short_name": "SA Bookmark Opener",
  "description": "Opens all unread SomethingAwful bookmarks in seperate tabs",
  "version": "1.0.4",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://forums.somethingawful.com/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://forums.somethingawful.com/usercp.php"
      ],
      "js": [
        "dummy.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  }
}
