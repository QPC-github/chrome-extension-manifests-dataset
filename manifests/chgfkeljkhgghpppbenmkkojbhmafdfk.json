{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TwitterGadget",
  "description": "A fully functional, small footprint, Twitter client.",
  "version": "1.5",
  "manifest_version": 2,
  "web_accessible_resources": [
    "gadget.asp"
  ],
  "app": {
    "urls": [
      "http://twittergadget.com/gadget.asp",
      "http://www.twittergadget.com/gadget.asp"
    ],
    "launch": {
      "web_url": "http://twittergadget.com/gadget.asp"
    }
  },
  "icons": {
    "128": "twittergadget_logo_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
