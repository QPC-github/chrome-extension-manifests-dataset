{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Grambos-ifier",
  "icons": {
    "16": "popup.png",
    "48": "popup.png",
    "128": "popup.png"
  },
  "version": "1.1",
  "browser_action": {
    "default_icon": "popup.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
