{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0",
  "author": "JHK",
  "default_locale": "zh_CN",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background",
    "pageCapture",
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "*://*.walkeam.com/*",
      "*://localhost/*"
    ]
  },
  "web_accessible_resources": [
    "3d1819e7-4594-4707-b887-7a184e4f4474.html",
    "manifest.json"
  ]
}
