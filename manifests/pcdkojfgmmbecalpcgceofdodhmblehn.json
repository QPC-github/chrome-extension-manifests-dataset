{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FJ Image Flipper",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Flips images on Funnyjunk.",
  "content_scripts": [
    {
      "matches": [
        "http://www.funnyjunk.com/*",
        "http://funnyjunk.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
