{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "HTML for Basecamp",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Simple extension to allow typing html directly into the editor instead of using the wysiwyg editor.",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://basecamp.com/*"
      ],
      "js": [
        "jquery.js",
        "go.js",
        "resize.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
