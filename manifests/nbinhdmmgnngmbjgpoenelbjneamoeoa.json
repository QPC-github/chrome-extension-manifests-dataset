{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Spreadr Inspector",
  "version": "1.0",
  "description": "Show products appear in online videos",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery-1.8.3.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://spreadr.tv/*/"
  ],
  "manifest_version": 2
}
