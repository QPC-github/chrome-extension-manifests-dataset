{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Lets you share your desktop with your Time To Gather participants.",
  "externally_connectable": {
    "matches": [
      "*://*.timetogather.co.uk/*"
    ]
  },
  "icons": {
    "128": "gather-128x128.png",
    "48": "gather-48x48.png",
    "16": "gather-16x16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Time To Gather Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "version": "0.1.4.5"
}
