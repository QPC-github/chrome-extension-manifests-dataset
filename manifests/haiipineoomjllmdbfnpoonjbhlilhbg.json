{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Browsingtime",
  "version": "0.2.28",
  "description": "Browsingtime keeps track of time you spend on your web browser",
  "manifest_version": 2,
  "minimum_chrome_version": "60",
  "browser_action": {},
  "icons": {
    "16": "icons/128.png",
    "48": "icons/128.png",
    "128": "icons/128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "tabs",
    "idle"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
