{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Iva Videoconference Agent",
  "description": "Iva Videoconference Agent extension",
  "manifest_version": 2,
  "version": "3.0.0",
  "version_name": "3.0",
  "default_locale": "ru",
  "background": {
    "scripts": [
      "/js/comet.js",
      "/js/notification.js",
      "/js/background.js"
    ]
  },
  "icons": {
    "16": "/images/icon_16.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "/images/offline.png",
    "default_title": "Iva VCS Agent"
  },
  "permissions": [
    "*://*/*",
    "background",
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "idle"
  ],
  "options_page": "/html/options.html"
}
