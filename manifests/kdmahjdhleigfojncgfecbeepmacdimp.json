{
  "browser_action": {
    "default_icon": "clear.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "128": "clear.png",
    "16": "clear.png",
    "48": "clear.png"
  },
  "author": "Dominix",
  "version": "1.0",
  "manifest_version": 2,
  "offline_enabled": true,
  "name": "Clear Data",
  "description": "Delete your browsing data easily",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
