{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hexometer Website Analyzer Tool",
  "description": "Website's Technical Stack analysis, Assets Scanner, Domain Whois Lookup, and other Audit Tools",
  "version": "0.2",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Hexometer Tool"
  },
  "omnibox": {
    "keyword": "whois"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "fav-sq.png",
    "48": "fav-sq.png",
    "128": "fav-sq.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://*.google.com https://*.gstatic.com 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
