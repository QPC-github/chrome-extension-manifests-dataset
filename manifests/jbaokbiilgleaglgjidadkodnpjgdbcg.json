{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vishal's Tips",
  "short_name": "Vishal's Tips",
  "description": "Wisdom from our favorite Indian roommate",
  "version": "1.0.1",
  "browser_action": {
    "name": "Vishal's Tips",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.2.0.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
