{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Organize",
  "version": "1",
  "manifest_version": 2,
  "description": "Organize, filter your tabs alphabetically",
  "icons": {
    "128": "icon_128.png",
    "256": "icon_256.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icone.png"
  }
}
