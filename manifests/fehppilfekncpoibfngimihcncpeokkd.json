{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HistoryGuardian",
  "description": "HistoryGuardian",
  "version": "1.2",
  "browser_action": {
    "default_title": "HistoryGuardian"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "history",
    "storage"
  ],
  "icons": {
    "48": "history48.png",
    "128": "history128.png"
  }
}
