{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zap - x100 your Linkedin network",
  "short_name": "Zap",
  "description": "This extension allows Linkedin users to add all the 400+ suggested Linkedin connections with 1 click of a button.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "<all_urls>"
  ]
}
