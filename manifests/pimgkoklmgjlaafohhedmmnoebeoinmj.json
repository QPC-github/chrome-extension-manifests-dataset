{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Don't Tweet",
  "version": "1.0",
  "description": "Reminds you not to tweet",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
