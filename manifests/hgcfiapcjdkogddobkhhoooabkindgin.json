{
  "name": "Start Here 新标签页",
  "version": "2.3.1",
  "manifest_version": 2,
  "description": "从这里开始，打开好心情。",
  "homepage_url": "https://start.ihansen.org/",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "96": "img/icon.png",
    "128": "img/icon.png"
  },
  "page_action": {
    "default_icon": "img/icon.png",
    "default_title": "Start Here"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://www.baidu.com https://sp0.baidu.com https://api.ihansen.org 'unsafe-eval' ; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "background",
    "cookies",
    "https://www.baidu.com/",
    "https://www.google.com/",
    "https://static-ali.ihansen.org/",
    "https://api.ihansen.org/"
  ],
  "optional_permissions": [],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
