{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EPL Tracker",
  "short_name": "EPL",
  "author": "Nikunj Mall",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "https://api.football-data.org/v2/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "EPL Tracker"
  }
}
