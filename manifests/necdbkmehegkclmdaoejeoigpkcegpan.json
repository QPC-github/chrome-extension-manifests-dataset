{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WikiEmoji",
  "manifest_version": 2,
  "version": "0.0.1",
  "short_name": "WikiEmoji",
  "icons": {
    "48": "1.png",
    "128": "2.png"
  },
  "description": "Input de Emoji",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery/jquery-2.0.3.min.js",
        "ll/js/jquery.emojipicker.js",
        "ll/js/jquery.emojis.js",
        "inject.js"
      ],
      "css": [
        "ll/css/jquery.emojipicker.css",
        "ll/css/jquery.emojipicker.tw.css"
      ],
      "png": [
        "ll/png/emoji_spritesheet_0.png"
      ]
    }
  ],
  "web_accessible_resources": []
}
