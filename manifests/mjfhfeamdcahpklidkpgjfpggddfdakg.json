{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Scootaloo Once",
  "version": "1.7",
  "description": "Make every /scootaloo emoticon say 'Once!' when clicked.",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/",
        "http://*.reddit.com/",
        "http://www.reddit.com/*",
        "http://*.reddit.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
