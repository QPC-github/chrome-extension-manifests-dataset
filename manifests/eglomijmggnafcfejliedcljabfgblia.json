{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/16.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  },
  "permissions": [
    "tabs"
  ],
  "name": "HTML 标签检测器|HTML TAG CHECKER",
  "manifest_version": 2,
  "version": "0.3"
}
