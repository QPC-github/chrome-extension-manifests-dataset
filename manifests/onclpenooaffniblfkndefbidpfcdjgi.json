{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Browser Bookmarks",
  "version": "1.4.4",
  "manifest_version": 2,
  "short_name": "American Internet Matrix",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "externally_connectable": {
    "matches": [
      "https://www.americaninternetmatrix.com/bookmarks"
    ]
  },
  "icons": {
    "128": "images.png"
  }
}
