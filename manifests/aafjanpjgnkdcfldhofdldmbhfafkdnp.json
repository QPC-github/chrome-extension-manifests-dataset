{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SMBC Tooltip Expander",
  "version": "0.1",
  "description": "Shows the tooltip from the image below the comic",
  "icons": {
    "16": "cereal.png",
    "48": "cereal.png",
    "128": "cereal.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.smbc-comics.com/*"
      ],
      "css": [],
      "js": [
        "tooltip-expander.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
