{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JustOpen for WhatsApp",
  "short_name": "JOWA",
  "version": "1.0",
  "author": "ishootbw",
  "description": "JustOpen the WhatsApp App with a Click !",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "96": "icon96.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+L"
      },
      "description": "JustOpen WhatsApp DM"
    }
  }
}
