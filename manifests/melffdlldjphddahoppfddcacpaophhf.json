{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Cite Marker",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Mark snippets and quotes quickly then easily retrieve and cite them later.",
  "permissions": [
    "contextMenus",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "citemark.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
