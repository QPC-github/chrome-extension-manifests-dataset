{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social Dock",
  "version": "1.2.1",
  "description": "One stop for all your social media profiles",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "options_page": "options.html"
}
