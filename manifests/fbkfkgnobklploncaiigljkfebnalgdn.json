{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Oh f**k yeah Reddit",
  "description": "This is a Chrome extension which replaces certain words on Reddit to '...eddit' suffixes. EG: Share = Spreddit",
  "permissions": [
    "tabs",
    "*://*.reddit.com/*"
  ],
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
