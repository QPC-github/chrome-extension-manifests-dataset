{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hacker News All Day!",
  "author": "Mart√≠n Sagastume (martin@thecodehunter.com)",
  "description": "Access the latest top stories from Hacker News every time you open a new blank tab!",
  "version": "1.3.0",
  "chrome_url_overrides": {
    "newtab": "hnews.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    "js/vendor/jquery.min.js",
    "js/script.js",
    "css/custom.css",
    "fonts/MYRIADPRO-CONDIT.woff"
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
