{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free Your Inbox | Le Blog RSS",
  "version": "2",
  "description": "Recevez les derniers articles de notre Blog d'astuces Gmail et Iphone.",
  "icons": {
    "16": "images/icon.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "homepage_url": "http://blog.freeyourinbox.com/",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://blog.freeyourinbox.com/feed"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Free Your Inbox | Le Blog",
    "default_popup": "popup.html"
  }
}
