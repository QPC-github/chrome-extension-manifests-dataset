{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "火车票查询",
  "description": "查询火车列车时刻表、余票信息",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "火车票查询",
    "default_popup": "popup.html"
  }
}
