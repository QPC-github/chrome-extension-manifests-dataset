{
  "background": {
    "scripts": [
      "js/searchUtil_iSelfie.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "html/newTab_iSelfie.html"
  },
  "description": "If you want to create cool selfie you must use this app",
  "browser_action": {
    "default_icon": "images/icons/icon-48x48.png"
  },
  "icons": {
    "128": "images/icons/icon-128x128.png",
    "16": "images/icons/icon-16x16.png",
    "48": "images/icons/icon-48x48.png"
  },
  "manifest_version": 2,
  "name": "selfiefun",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "ilove-selfie",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.1",
  "web_accessible_resources": [
    "html/newTab_iSelfie.html"
  ]
}
