{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Multiple Accounts for Outlook.com",
  "description": "Get access to your email, contacts, calendar, and office online apps across multiple accounts.",
  "version": "0.11",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon-16.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "fullscreen",
    "webview",
    "<all_urls>",
    "storage"
  ]
}
