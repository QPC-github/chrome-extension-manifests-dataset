{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Verified Access Sample Extension",
  "version": "3.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "certinstaller_56.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "56": "certinstaller_56.png",
    "128": "certinstaller_128.png"
  },
  "permissions": [
    "http://*/dvproxy/getchallenge*",
    "http://*/dvproxy/verifychallengeresponse?signeddata=*",
    "https://*/dvproxy/getchallenge*",
    "https://*/dvproxy/verifychallengeresponse?signeddata=*",
    "https://*.googleapis.com/*",
    "http://*.sandbox.google.com/*",
    "https://*.sandbox.google.com/*",
    "enterprise.platformKeys",
    "storage"
  ]
}
