{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Weibo Mini"
  },
  "description": "Weibo Mini",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "微博急简 WC",
  "permissions": [
    "tabs",
    "storage",
    "http://weibo.com/*",
    "http://*.weibo.com/*"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "wbMini.css"
  ],
  "version": "11.0"
}
