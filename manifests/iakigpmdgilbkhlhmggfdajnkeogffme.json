{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RedditRestore",
  "version": "0.2",
  "description": "Restores edited, deleted and removed posts and comments on Reddit",
  "content_scripts": [
    {
      "matches": [
        "https://*.reddit.com/r/*/comments/*/*/*"
      ],
      "js": [
        "marked.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [],
  "icons": {
    "128": "icon128.png"
  }
}
