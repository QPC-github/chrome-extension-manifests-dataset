{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CryptoKit.HandPay Extension",
  "version": "3.4.0.1",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "http://www.handpay.com.cn/*",
      "https://www.handpay.com.cn/*",
      "http://oper.handpay.cn/*",
      "https://oper.handpay.cn/*"
    ]
  }
}
