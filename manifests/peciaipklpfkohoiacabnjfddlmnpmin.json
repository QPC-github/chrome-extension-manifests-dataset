{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Faithlife Jira Links for Flowdock",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "Auto-detects and pretty-prints Jira cases/links in Flowdock.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery-2.2.4.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.2.4.min.js",
        "link-jira.js"
      ],
      "matches": [
        "https://www.flowdock.com/app*"
      ]
    }
  ]
}
