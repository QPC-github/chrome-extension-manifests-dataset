{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "IRUnfilterg",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This will apply proxy and remove host header from HTTP.",
  "background": {
    "scripts": [
      "proxy.js",
      "main.js",
      "headerer.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "proxy",
    "webRequest",
    "webRequestBlocking"
  ]
}
