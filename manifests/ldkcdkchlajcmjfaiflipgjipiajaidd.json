{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stamp Maker",
  "description": "a simple tool that create stamp samples",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "popup.js",
      "createjs.js",
      "easeljs-textarc.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
