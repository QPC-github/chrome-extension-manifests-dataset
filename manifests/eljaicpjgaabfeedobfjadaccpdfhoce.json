{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ESL fast controller",
  "description": "ESL fast controller",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*.eslfast.com/*",
        "http://eslfast.com/*"
      ],
      "js": [
        "eslfast.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "http://*.eslfast.com/*",
    "http://eslfast.com/*"
  ]
}
