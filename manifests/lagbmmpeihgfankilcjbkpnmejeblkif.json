{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wiktionary Language Filter",
  "description": "This simple Chrome extension adds '#MyLanguage' to Wiktionary URLs in order that your browser will always jump to your language.",
  "version": "1.1.3",
  "background": {
    "scripts": [
      "storage.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/Icon_19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "declarativeContent",
    "webNavigation"
  ],
  "icons": {
    "16": "icons/Icon_16.png",
    "48": "icons/Icon_48.png",
    "128": "icons/Icon_128.png"
  }
}
