{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweet Engine",
  "description": "This Extension lets you Search through Tweets and Live Twitter Stream aka anonymous data about the query you enter.",
  "version": "0.1",
  "permissions": [
    "http://sampledriveapi.appspot.com/twitterscraper/scraper",
    "tabs"
  ],
  "icons": {
    "16": "twitter_16.png",
    "48": "twitter_48.png",
    "128": "twitter_25.png"
  },
  "browser_action": {
    "default_icon": "twitter_25.png",
    "default_popup": "filename.html"
  }
}
