{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JPG to PDF Converter",
  "default_locale": "en",
  "description": "Get your jpg images converted to PDF document instantly from your home and new tab page!",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "2.0",
  "icons": {
    "16": "images/pdf-16.png",
    "48": "images/pdf-48.png",
    "128": "images/pdf-128.png"
  },
  "browser_action": {
    "default_icon": "images/pdf-128.png"
  }
}
