{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Employee Brand Advocacy",
  "short_name": "Circulate.it Employee Brand Advocacy",
  "description": "Make the content you create and curate easily shareable by the rest of your organization with Circulate.it",
  "version": "1.3",
  "app": {
    "urls": [
      "https://circulate.it/"
    ],
    "launch": {
      "web_url": "https://circulate.it/"
    }
  },
  "icons": {
    "16": "icon-16px.png",
    "48": "icon-48px.png",
    "128": "icon-128px.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
