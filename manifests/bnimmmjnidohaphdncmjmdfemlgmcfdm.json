{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Open Daily",
  "description": "Open Daily gives you a hand in making your day brighter and more productive through a simple, pleasant default tab. ",
  "version": "1.3",
  "author": "Jackson Huether",
  "browser_action": {
    "default_title": "Open Daily",
    "default_icon": "icon128image.png"
  },
  "icons": {
    "16": "icon16.ico",
    "48": "icon48.ico",
    "128": "icon128.ico"
  },
  "chrome_url_overrides": {
    "newtab": "OpenDaily.html"
  },
  "permissions": [
    "activeTab"
  ]
}
