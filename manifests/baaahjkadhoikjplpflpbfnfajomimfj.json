{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "pixiv たのしす！",
  "version": "1.2.0",
  "description": "pixiv の利用が楽しくなりそうな機能を詰めこんだ拡張です。",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "インターシティ",
  "content_scripts": [
    {
      "matches": [
        "http://www.pixiv.net/*"
      ],
      "js": [
        "jquery.min.js",
        "min/script.min.js"
      ],
      "run_at": "document_end"
    }
  ]
}
