{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "gooタブ",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "新しいタブを追加するたびにgooが表示されます",
  "icons": {
    "128": "pic/goo_128.png"
  },
  "browser_action": {
    "default_icon": "pic/goo_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "html/gooTab.html"
  },
  "permissions": [
    "topSites",
    "history"
  ],
  "content_security_policy": "script-src 'self' https://www.goo.ne.jp; object-src 'self'"
}
