{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fav Brother",
  "version": "1.0",
  "description": "Favorite image manager",
  "icons": {
    "48": "Icons/heart-48.png"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "options_page": "browser_action/Options/option.html",
  "browser_action": {
    "default_icon": "Icons/heart-32.png",
    "default_title": "Fav Brother",
    "default_popup": "browser_action/Popup/favBrother.html"
  }
}
