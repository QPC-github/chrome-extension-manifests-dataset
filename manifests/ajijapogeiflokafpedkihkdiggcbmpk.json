{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Copy Yamsafer Images",
  "version": "0.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "smooch.png"
  },
  "icons": {
    "16": "smooch.png",
    "48": "smooch.png",
    "128": "smooch.png"
  }
}
