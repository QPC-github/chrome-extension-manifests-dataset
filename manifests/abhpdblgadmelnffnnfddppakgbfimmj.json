{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CenturyLink Cloud Status",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Real-time updates of status.ctl.io",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_title": "CenturyLink Cloud Status",
    "default_icon": "icon-op.png"
  },
  "permissions": [
    "https://status.ctl.io/"
  ]
}
