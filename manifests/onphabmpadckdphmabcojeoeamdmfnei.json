{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Linknect",
  "description": "Linknect is an online bookmark management system which allows you to store, share and access your bookmark online.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "fav2.png",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
