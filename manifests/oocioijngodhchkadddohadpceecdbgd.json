{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "domain capture",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Realtime Domain Query",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*",
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "/assets/logo.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "/assets/logo.png",
    "48": "/assets/logo.png"
  }
}
