{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "social stickies",
  "description": "Comment memo on to the web pages.",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/jquery-2.1.1.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/icon_16.png",
    "default_title": "stickies"
  },
  "icons": {
    "128": "img/icon_16.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab"
  ]
}
