{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Animate Youtube Thumbnails",
  "version": "1.0.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "ytat.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "description": "Animate Youtube Homepage & Related Videos Thumbnails",
  "converted_from_user_script": true,
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
