{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extension_name__",
  "version": "1.0.0.1007",
  "description": "__MSG_extension_description__",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "monitor_analytic.js",
      "Demon.js",
      "md5.js",
      "MainProc.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "__MSG_extension_title__",
    "default_popup": "popup.html",
    "default_icon": "main.png"
  },
  "icons": {
    "32": "main.png",
    "16": "main.png",
    "48": "main.png",
    "64": "main.png",
    "96": "main.png",
    "128": "main.png"
  },
  "manifest_version": 2,
  "default_locale": "zh_CN"
}
