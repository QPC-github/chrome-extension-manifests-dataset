{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flash for Trade-Box",
  "version": "0.3",
  "description": "This extension will enable Flash for Trade-box.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "chrome_icon_blue_white.png"
  },
  "permissions": [
    "contentSettings",
    "activeTab"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://trade-box.me/*",
      "*://otrader2019.info/*",
      "*://ligatrade.club/*",
      "*://atrade.club/*",
      "*://hold22.club/*",
      "*://megabox22.club/*"
    ]
  }
}
