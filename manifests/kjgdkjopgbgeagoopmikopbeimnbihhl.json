{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "9gag dark theme",
  "description": "Changes the 9gag theme",
  "version": "1.15",
  "content_scripts": [
    {
      "matches": [
        "*://*.9gag.com/*"
      ],
      "css": [
        "theme.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "128.png"
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "*://*.9gag.com/*"
  ]
}
