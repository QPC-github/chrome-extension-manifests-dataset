{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OpenSAPNote",
  "description": "Select a SAP Note number from a web page, right click, and open the note directly in the web browser.",
  "version": "1.16.9",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "openNote.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
