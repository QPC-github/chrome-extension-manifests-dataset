{
  "manifest_version": 2,
  "name": "My IP Address",
  "description": "Find out your IP Address and other informations related to it.",
  "version": "1.1",
  "permissions": [
    "http://www.ipdetails.net/"
  ],
  "browser_action": {
    "default_icon": "ip_16.png",
    "default_title": "My IP Address"
