{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Currency converter +",
  "description": "Convert to several currencies",
  "version": "1.31",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab"
  ],
  "scripts": [
    {
      "js": [
        "jquery-3.3.1.min.js",
        "popup.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/currency_logo.png",
      "38": "images/currency_logo.png"
    },
    "default_title": "Currency converter +",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/currency_logo16.png",
    "48": "images/currency_logo48.png",
    "64": "images/currency_logo64.png",
    "128": "images/currency_logo.png"
  }
}
