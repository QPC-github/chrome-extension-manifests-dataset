{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Uyumsoft.com Haberler",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Uyumsoft Blog'dan son dakika ERP,HRM ve CRM haberleri ile sektörü en önde takip edin!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "pencere.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "eklenti.js"
    ]
  },
  "permissions": [
    "https://www.uyumsoft.com/haberler-plugin/",
    "unlimitedStorage"
  ]
}
