{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinkChai.net plugin",
  "description": "This extension tracks links in your email message when using the outlook web app ",
  "version": "1.4",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "tea_cup_128x128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "<all_urls>",
    "clipboardWrite",
    "clipboardRead",
    "notifications"
  ],
  "browser_action": {
    "default_title": "LinkChai Plugin",
    "default_icon": "tea_cup_16x16.png"
  },
  "web_accessible_resources": [
    "jquery.min.map"
  ],
  "manifest_version": 2
}
