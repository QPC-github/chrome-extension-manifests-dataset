{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Givee",
  "description": "Send gifts in just one click",
  "version": "1.2.1",
  "browser_action": {
    "default_icon": "givee_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
