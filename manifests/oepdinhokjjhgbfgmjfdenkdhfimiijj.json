{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Live New England Foliage Map",
  "description": "This extension shows live foliage map of the New England Area",
  "version": "1.2",
  "browser_action": {
    "default_icon": "19x19.png",
    "default_popup": ""
  },
  "icons": {
    "19": "19x19.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "foliagemap.html"
  },
  "permissions": [
    "management"
  ]
}
