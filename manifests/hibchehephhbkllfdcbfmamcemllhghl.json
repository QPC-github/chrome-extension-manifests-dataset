{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "校园网",
  "author": "钟武",
  "version": "2.0",
  "description": "校园网自动登录插件简易版，请关注后续更新",
  "content_scripts": [
    {
      "matches": [
        "http://172.16.8.8/*"
      ],
      "js": [
        "js/wolverine.js",
        "js/logan.js"
      ]
    }
  ],
  "icons": {
    "16": "images/02.png",
    "48": "images/02.png",
    "128": "images/02.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/02.png",
      "38": "images/02.png"
    },
    "default_title": "校园网"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
