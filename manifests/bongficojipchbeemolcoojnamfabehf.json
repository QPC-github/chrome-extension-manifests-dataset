{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ProdPad",
  "version": "2.2.1",
  "manifest_version": 2,
  "description": "This extension lets you quickly add an idea to your ProdPad project from any website you're on.",
  "homepage_url": "http://www.prodpad.com",
  "browser_action": {
    "default_title": "Quick add to ProdPad",
    "default_icon": "./dist/img/32x32.png",
    "default_popup": "./html/index.html"
  },
  "icons": {
    "16": "./dist/img/16x16.png",
    "32": "./dist/img/32x32.png",
    "48": "./dist/img/48x48.png",
    "128": "./dist/img/128x128.png"
  },
  "permissions": [
    "storage"
  ]
}
