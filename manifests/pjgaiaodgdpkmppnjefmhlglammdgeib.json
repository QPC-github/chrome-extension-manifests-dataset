{
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "events_19.png",
      "38": "events_38.png",
      "128": "icon.png"
    },
    "default_title": "Футбол на Куличках",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "http://*/",
    "tabs"
  ],
  "description": "Расширение “Футбола на Куличках” позволит вам всегда иметь бесперебойный доступ к его основному сайту!",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Футбол на Куличках (ФНК)",
  "short_name": "Футбол на Куличках",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.24",
  "web_accessible_resources": [
    "icon.png",
    "check.html"
  ]
}
