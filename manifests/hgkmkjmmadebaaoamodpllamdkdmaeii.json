{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Маршрутка UA",
  "description": "Быстрый и удобный поиск городского, коммунального и общественного транспорта Украины",
  "version": "1.1.14",
  "short_name": "Маршрутка",
  "icons": {
    "16": "/img/icon-16.png",
    "128": "/img/icon-128.png"
  },
  "browser_action": {
    "default_title": "Маршрутка UA",
    "default_icon": {
      "128": "/img/icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "*://*.marshrutka.com.ua/"
  ]
}
