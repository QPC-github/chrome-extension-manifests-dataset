{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SayWhat.LOL text to Gif generator",
  "short_name": "SayWhat.LOL",
  "description": "Make everything you say sound funny",
  "version": "1.4",
  "permissions": [
    "activeTab",
    "https://saywhat.lol/",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "parakeet.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
