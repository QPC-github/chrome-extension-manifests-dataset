{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PS WebRequest Whitelist Test",
  "description": "Testing Webrequests",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
