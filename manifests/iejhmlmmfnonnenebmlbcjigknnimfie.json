{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shoptellie",
  "description": "Shop what you watch online.",
  "version": "0.1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Hello"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "css/*.woff2",
    "css/*.woff",
    "css/*.ttf",
    "images/*.png",
    "images/*.gif",
    "images/*.jpg",
    "*.js"
  ],
  "permissions": [
    "tabs",
    "https://www.youtube.com/*",
    "https://youtube.com/*",
    "https://youtube.com/*",
    "http://abc.go.com/*",
    "https://abc.go.com/*",
    "http://www.hulu.com/*",
    "https://www.hulu.com/*",
    "activeTab",
    "tabs",
    "http://shoptellie.com/",
    "http://shoptellie.com/*"
  ]
}
