{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/toolbar-icon.png",
      "38": "icons/toolbar-icon2x.png"
    },
    "default_title": "Post to ViralJokers.com (Ctrl+Q)"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Q"
      }
    }
  },
  "description": "Save joke to ViralJokers.com right from your browser toolbar!",
  "homepage_url": "https://viraljokers.com",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "name": "Post to ViralJokers.com",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "5.0"
}
