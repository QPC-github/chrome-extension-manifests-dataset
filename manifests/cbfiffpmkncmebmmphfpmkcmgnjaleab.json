{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content/iForum.js"
      ],
      "matches": [
        "http://*.forum.ge/*",
        "https://*.forum.ge/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "გაფართოება Forum.ge-ს მომხმარებლებისთვის",
  "icons": {
    "128": "images/icon_128.gif",
    "16": "images/icon.gif",
    "32": "images/icon_32.gif",
    "48": "images/icon_48.gif"
  },
  "manifest_version": 2,
  "name": "iForum",
  "version": "3.1.0"
}
