{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facebook is a trap!",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Alerts you if you are on facebook to remind you if you are wasting time.",
  "author": "Jason Schallenberger",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "event.js"
      ]
    }
  ]
}
