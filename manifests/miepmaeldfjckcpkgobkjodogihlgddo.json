{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2,
  "name": "iFrame Freedom",
  "description": "This extension is a developer tool to enable and facilitate iframes despite xframe denial.",
  "version": "1.20",
  "browser_action": {
    "default_icon": "sw-chrome-logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.googleapis.com/",
    "*://*.google.com/",
    "*://*.com/",
    "storage",
    "identity",
    "tabs"
  ],
  "oauth2": {
    "client_id": "407396887697-o5tskn0t48qejcr03o3lt2vcrprgii53.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  }
}
