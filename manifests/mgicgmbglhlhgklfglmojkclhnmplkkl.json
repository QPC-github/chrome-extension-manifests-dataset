{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "A simple extension that allows you to stream your desktop into meetings with Webinar.cc",
  "externally_connectable": {
    "matches": [
      "*://www.cliqmeet.local/*",
      "*://*.cliqmeet.com/*",
      "*://*.webinar.cc/*"
    ]
  },
  "icons": {
    "128": "webinar-logo-128x128.png",
    "16": "webinar-logo-16x16.png",
    "48": "webinar-logo-48x48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Webinar.cc Screenshare",
  "short_name": "Desktop and Screen sharing to any device",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.0.3"
}
