{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "FU LJS!",
  "version": "1.0.13.3",
  "permissions": [
    "tabs",
    "http://*.journalstar.com/*"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.journalstar.com/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "f-ljs_16.gif",
    "48": "f-ljs_48.png",
    "128": "f-ljs_128.png"
  }
}
