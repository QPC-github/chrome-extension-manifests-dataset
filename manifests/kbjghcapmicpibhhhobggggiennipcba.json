{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Elance Notifier",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get notified of new inbox messages and preview your Elance Inbox messages.",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "favicon.ico",
    "48": "elance-notif48.png",
    "128": "elance-notif128.png"
  },
  "permissions": [
    "background",
    "https://www.elance.com/"
  ]
}
