{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cluefeed",
  "description": "Transfer the currently opened URL into your Cluefeed.com bookmark archive.",
  "version": "1.0",
  "author": "Cluefeed.com",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://cluefeed.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Archive on Cluefeed.com"
  }
}
