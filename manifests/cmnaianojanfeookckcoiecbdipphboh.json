{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jetson - Your shopping assistant",
  "short_name": "Jetson",
  "description": "by shopjetstream.com",
  "manifest_version": 2,
  "version": "6.18.2016",
  "browser_action": {
    "default_icon": "img/jetsonicon.png",
    "default_popup": "jetson.html"
  },
  "background": {
    "scripts": [
      "js/jetsonbg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "activeTab",
    "background",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "img/jetsonicon.png",
    "48": "img/jetsonicon48.png",
    "128": "img/jetsonicon128.png"
  }
}
