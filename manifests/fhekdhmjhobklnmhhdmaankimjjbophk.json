{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "www.Thereal-Website.com",
  "description": "Easy navigation to thereal-website. Will have more features in the future.",
  "version": "0.1111",
  "browser_action": {
    "default_icon": {
      "36": "images/icon36.png"
    },
    "default_title": "www.Thereal-Website.com",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon48.png",
    "48": "images/icon128.png",
    "128": "images/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "contextMenu.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ]
}
