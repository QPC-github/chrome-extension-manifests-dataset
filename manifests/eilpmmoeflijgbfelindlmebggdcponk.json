{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IMDB Navbar Query Focus FFS",
  "description": "This extension demonstrates a IMDB lack of focus on search.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.imdb.com/*",
        "http://www.imdb.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.imdb.com/",
    "http://www.imdb.com/"
  ]
}
