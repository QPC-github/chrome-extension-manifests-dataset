{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Temporary Bookmark",
  "description": "This extension will add the current url to a user's Phittle account.",
  "version": "2.4",
  "browser_action": {
    "default_icon": "phittleIcon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "phittleIcon16.png",
    "64": "phittleIcon64.png",
    "128": "phittleIcon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies"
  ]
}
