{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/underscore.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Trumpet"
  },
  "description": "Trumpet is a url redirect tool for developer",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "Trumpet",
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/*"
  ],
  "version": "1.0"
}
