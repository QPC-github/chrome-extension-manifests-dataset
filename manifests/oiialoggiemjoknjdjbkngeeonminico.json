{
  "background": {
    "scripts": [
      "ons1.js",
      "ons2.js"
    ]
  },
  "description": "One-Finder Browse internet fast and secure. Find what you are looking for!",
  "icons": {
    "128": "ons.png"
  },
  "manifest_version": 2,
  "name": "One Searcher",
  "permissions": [
    "chrome://favicon/",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "cookies",
    "topSites",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0"
}
