{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Position Tracker"
  },
  "permissions": [
    "activeTab"
  ],
  "description": "Easily Track Positions on Google Search Engine.",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "content.js"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Position Tracker",
  "version": "0.1",
  "author": "Amir Shah"
}
