{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Unofficial Tweakers.net Nieuws Notifier",
  "version": "0.4.1",
  "manifest_version": 2,
  "description": "Zal een melding geven als er een nieuw nieuws item op T.Net verschijnt.",
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/tweakers-19.png",
    "default_title": "Geen nieuws"
  },
  "icons": {
    "16": "images/tweakers-16.png",
    "48": "images/tweakers-48.png",
    "128": "images/tweakers-128.png"
  },
  "permissions": [
    "http://tweakers.net/*",
    "http://feeds.feedburner.com/*",
    "notifications"
  ]
}
