{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "i/icon_grey_rss_19x19.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_extDesc__"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "md5_base64.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "i/icon_orange_rss_16x16.png",
    "48": "i/icon_orange_rss_48x48.png",
    "128": "i/icon_orange_rss_128x128.png"
  },
  "name": "__MSG_extName__",
  "version": "1.0",
  "permissions": [
    "tabs",
    "history",
    "cookies",
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2
}
