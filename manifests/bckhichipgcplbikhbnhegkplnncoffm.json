{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Web Fonts enhanced by TypeDNA",
  "description": "TypeDNA Font Manager Application",
  "version": "0.0.75",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "gfx/icon_16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "gfx/icon_16.png",
    "48": "gfx/icon_48.png",
    "128": "gfx/icon_128.png"
  },
  "web_accessible_resources": [
    "db.json",
    "js/jquery.js",
    "js/jquery.dataset.js",
    "js/jquery.class.js",
    "js/webfonts.js",
    "js/typedna.index.js",
    "js/typedna.baseurl.js",
    "js/typedna.fontmanager.js",
    "gfx/info.png",
    "gfx/html5google/TypeDNA_logo.png"
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
