{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Remove \"Recommended Tumblelogs\"",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Removes \"Recommended Tumblelogs\" from tumblr's dashboard.",
  "content_scripts": [
    {
      "matches": [
        "http://www.tumblr.com/*"
      ],
      "css": [
        "remove_recommended_tumblelogs.css"
      ]
    }
  ]
}
