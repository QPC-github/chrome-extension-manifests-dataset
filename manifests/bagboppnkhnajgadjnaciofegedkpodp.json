{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Show one random repo from your starred repos on the GitHub homepage",
  "version": "0.0.3",
  "name": "starred",
  "content_scripts": [
    {
      "js": [
        "content.bundle.js"
      ],
      "matches": [
        "https://github.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
