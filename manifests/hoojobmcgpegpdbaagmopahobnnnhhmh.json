{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Little Timer",
  "description": "__MSG_extDescription__",
  "version": "1.2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "timer16.png",
      "19": "timer19.png",
      "128": "timer128.png"
    },
    "default_title": "Little Timer",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "permissions": [
    "tabs",
    "http://*/*",
    "background"
  ],
  "icons": {
    "19": "timer19.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
