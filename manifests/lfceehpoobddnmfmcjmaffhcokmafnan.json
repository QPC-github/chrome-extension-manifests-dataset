{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VSTS box lines",
  "developer": {
    "name": "Otto Streifel"
  },
  "description": "This extension draws lines around the boxes in the new form on Visual Studio Team Services",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.visualstudio.com/*"
      ],
      "css": [
        "boxStyling.css"
      ],
      "js": []
    }
  ]
}
