{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Morning Reader Blockchain News",
  "description": "The easiest way to keep up with blockchain and crypto news, on your new tab page.",
  "version": "1.0.2",
  "icons": [
    {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "ico48.png",
      "128": "icon128.png"
    }
  ],
  "permissions": [
    "http://morningreader.com/",
    "http://*.morningreader.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  }
}
