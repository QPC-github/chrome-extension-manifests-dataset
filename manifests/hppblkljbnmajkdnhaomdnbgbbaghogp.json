{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "wishmindr_icon_16.png",
    "default_title": "WishMindr - Wish List App"
  },
  "name": "WishMindr - Wish List App",
  "description": "Create and share wish lists. Add gifts from any site.",
  "homepage_url": "http://wishmindr.com",
  "icons": {
    "16": "wishmindr_icon_16.png",
    "48": "wishmindr_icon_48.png",
    "128": "wishmindr_icon_128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1",
  "manifest_version": 2
}
