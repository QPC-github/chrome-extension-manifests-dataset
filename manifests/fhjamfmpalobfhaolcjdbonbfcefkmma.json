{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Etsy Search Liker",
  "version": "0.1",
  "author": "Tekly.fr",
  "manifest_version": 2,
  "description": "The Etsy Search Liker extension allows you to like every products on an Etsy search results page in one click.",
  "homepage_url": "https://tekly.fr/",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Etsy Search Liker"
  },
  "permissions": [
    "https://*.etsy.com/*",
    "http://*.etsy.com/*",
    "tabs"
  ]
}
