{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fine Link Selector",
  "version": "0.1.5",
  "description": "__MSG_Manifest_Description__",
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/cs.js"
      ],
      "css": [
        "css/cs.css"
      ],
      "all_frames": true
    }
  ],
  "offline_enabled": true,
  "options_page": "html/opt.html",
  "permissions": [
    "storage"
  ]
}
