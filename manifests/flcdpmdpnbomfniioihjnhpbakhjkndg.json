{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinkedIn CSV Downloader",
  "version": "1.0",
  "description": "Download person search into a csv",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Download CSV"
  },
  "permissions": [
    "declarativeContent"
  ],
  "icons": {
    "64": "icon.png"
  },
  "manifest_version": 2
}
