{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fix tabs in github",
  "description": "This extension make tabs to use just 4 spaces instead of 8 in github.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
