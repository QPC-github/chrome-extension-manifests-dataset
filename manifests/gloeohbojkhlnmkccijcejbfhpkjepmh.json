{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Genie Importer",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "48": "icon48.png"
  },
  "description": "Use this extension to import products from ebay to your Genie application easily",
  "version": "1.0.8",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.ebay.com/*",
        "https://*.ebay.com/*",
        "http://*.ebay.co.uk/*",
        "https://*.ebay.co.uk/*",
        "http://www.genieimporter.com/*"
      ],
      "js": [
        "jquery.min.js",
        "ebay.js",
        "autofillAddress.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "app.js"
    ]
  },
  "web_accessible_resources": [
    "smallWhiteGenie.png",
    "v.png",
    "loading.gif"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://www.ebay.com/",
    "http://www.ebay.co.uk/",
    "http://www.genieimporter.com/*",
    "*://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "http://www.genieimporter.com/*"
    ]
  }
}
