{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "City of Port Orange Quick Links",
  "short_name": "CPO Quick Links",
  "description": "Quickly open useful pages on the City of Port Orange website.",
  "version": "1.3",
  "author": "Corey Kingsbury - City of Port Orange Information Technology Department",
  "browser_action": {
    "default_icon": "btns/btn_citylogo32_reg.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "btns/btn_citylogo48_reg.png",
    "16": "btns/btn_citylogo32_reg.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "*://*.google.com/"
  ]
}
