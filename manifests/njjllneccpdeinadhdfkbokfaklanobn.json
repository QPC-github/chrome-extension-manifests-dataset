{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Simple and Pretty checklist/to-do list",
  "version": "0.0.7",
  "manifest_version": 2,
  "name": "Pretty Todolist",
  "icons": {
    "128": "./favicon1.png"
  },
  "browser_action": {
    "default_popup": "./popup.html",
    "default_icon": "./favicon1.png"
  },
  "background": {
    "scripts": [
      "./background.bundle.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "unlimitedStorage"
  ]
}
