{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kitap Kurdu",
  "description": "Kitap Kurdu Chrome Eklentisini, her yeni sekmede size iyi bir kitap önermek için hayata geçirdik.",
  "version": "1.0.4",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "storage",
    "https://kkx.kitapyurdu.com/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
