{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Roblox Stream Browser",
  "description": "Browse and watch Roblox livestreams.",
  "version": "0.5.2",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "permissions": [
    "https://api.twitch.tv/kraken/",
    "https://www.googleapis.com/youtube/v3/"
  ]
}
