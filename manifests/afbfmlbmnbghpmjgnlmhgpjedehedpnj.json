{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FirePop!",
  "version": "0.4.1",
  "description": "__MSG_extensionDescription__",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "overlay.js"
      ]
    }
  ],
  "icons": {
    "48": "icons/favicon.png",
    "96": "icons/favicon_big.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "nativeMessaging",
    "storage"
  ],
  "key": "",
  "default_locale": "en"
}
