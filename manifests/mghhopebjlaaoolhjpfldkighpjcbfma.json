{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DaUeb",
  "version": "2.1",
  "manifest_version": 2,
  "description": "Feed das postagens mais interessantes da internet.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "imagens/icone_extensao16.png",
    "default_title": "Clique para ver postagens n√£o lidas",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "script1.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons": {
    "16": "imagens/icone_extensao16.png",
    "48": "imagens/icone_extensao48.png",
    "128": "imagens/icone_extensao128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
