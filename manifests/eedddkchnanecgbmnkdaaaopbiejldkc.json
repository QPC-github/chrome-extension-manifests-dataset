{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js",
      "ga.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "matches": [
        "*://vk.com/*",
        "*://*.vk.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Добавляет кнопку просмотра статистики на страницы сообществ, считает CPM на странице поиска сообществ для размещения рекламы..",
  "homepage_url": "https://goo.gl/xeMkw1",
  "icons": {
    "128": "icons/128x128.png",
    "16": "icons/16x16.png",
    "19": "icons/19x19.png",
    "38": "icons/38x38.png",
    "48": "icons/48x48.png"
  },
  "manifest_version": 2,
  "name": "SMM помощник для vk.com",
  "permissions": [
    "tabs"
  ],
  "short_name": "SMM helper",
  "version": "1.3.4",
  "web_accessible_resources": [
    "*.js"
  ]
}
