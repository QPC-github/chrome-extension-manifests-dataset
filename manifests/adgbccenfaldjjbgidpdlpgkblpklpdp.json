{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bootstrap Geoifier",
  "description": "This extension converts all default bootstrap websites to use the much improved Bootstrap Geo.",
  "version": "1.0",
  "icons": {
    "48": "geoicon48.png",
    "128": "geoicon128.png"
  },
  "web_accessible_resources": [
    "popup.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
