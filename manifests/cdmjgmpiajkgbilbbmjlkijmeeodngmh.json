{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Recent Pages",
  "version": "0.0.0.1",
  "description": "Reads your history, and shows the pages you opened.",
  "permissions": [
    "history",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "history.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
