{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "short_name": "__MSG_name__",
  "description": "__MSG_desc__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "/newtab.html"
  },
  "icons": {
    "128": "/img/128.png",
    "64": "/img/64.png",
    "32": "/img/32.png"
  },
  "browser_action": {
    "default_icon": "/img/128.png",
    "default_title": "__MSG_name__"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage",
    "topSites",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "1.0.0"
}
