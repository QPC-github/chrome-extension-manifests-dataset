{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "eMarks",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Extension for eMarks - Online Bookmark Manager",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html",
    "default_title": "eMarks"
  },
  "permissions": [
    "tabs"
  ]
}
