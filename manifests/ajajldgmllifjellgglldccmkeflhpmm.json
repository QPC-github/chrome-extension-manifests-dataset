{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alphaa AI",
  "description": "This extension is used to support Alphaa.ai, Your decision platform",
  "version": "0.3.0",
  "manifest_version": 2,
  "homepage_url": "https://secure.alphaa.ai/",
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.alphaa.ai/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs",
    "<all_urls>"
  ]
}
