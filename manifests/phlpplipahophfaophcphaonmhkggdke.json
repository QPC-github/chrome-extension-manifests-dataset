{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Niffler",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Niffler",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/niffler16.png",
    "48": "img/niffler48.png",
    "128": "img/niffler128.png"
  }
}
