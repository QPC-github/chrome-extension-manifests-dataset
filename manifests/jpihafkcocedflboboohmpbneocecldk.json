{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BreakLink",
  "version": "1.1",
  "manifest_version": 2,
  "description": "BreakLink removes links you don't like",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_title": "htire",
    "default_icon": "icon16.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
