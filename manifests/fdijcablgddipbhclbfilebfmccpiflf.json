{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "dailyhack.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/crack.js"
      ],
      "matches": [
        "*://*.dailymotion.com/*",
        "*://*.dailymotion.com/*"
      ]
    }
  ],
  "description": "Permet de télécharger vos vidéos sur Dailymotion",
  "icons": {
    "128": "dailyhack.png",
    "16": "dailyhack.png",
    "48": "dailyhack.png"
  },
  "manifest_version": 2,
  "name": "DailyHack",
  "author": "Dave Hill",
  "homepage_url": "http://dyrk.org",
  "permissions": [
    "*://*.dailymotion.com/*",
    "*://*.dailymotion.com/*"
  ],
  "version": "1.0"
}
