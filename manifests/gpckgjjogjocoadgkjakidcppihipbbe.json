{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Readditing",
  "description": "Redirects reddit.com requests to readditing.com",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://reddit.com/*",
    "*://www.reddit.com/*",
    "webRequestBlocking"
  ],
  "icons": {
    "16": "readditing16.png",
    "48": "readditing48.png",
    "128": "readditing128.png"
  }
}
