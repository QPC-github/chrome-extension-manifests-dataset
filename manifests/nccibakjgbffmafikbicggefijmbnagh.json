{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LocalWire",
  "description": "Hyperlocal news platform.",
  "version": "1",
  "author": "Neel Ratan & Shubham Choudhary",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Get update"
  },
  "chrome_url_overrides": {},
  "content_scripts": [
    {
      "js": [
        "jquery-3.3.1.min.js",
        "fullpage.min.js",
        "fullpage.extensions.min.js",
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
