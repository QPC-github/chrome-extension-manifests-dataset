{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1",
  "manifest_version": 2,
  "name": "Spin Page In",
  "description": "Get every page a hot off the press.",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "rotate.js"
      ],
      "css": [
        "rotate.css"
      ]
    }
  ],
  "icons": {
    "16": "img/img16.png",
    "48": "img/img48.png",
    "128": "img/img128.png"
  }
}
