{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HoMG Media Hub",
  "version": "1.0.1",
  "default_locale": "en",
  "description": "Keep all of the HoMG media accounts and links in one place. Developed by Joe Moses.",
  "icons": {
    "128": "128_icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
