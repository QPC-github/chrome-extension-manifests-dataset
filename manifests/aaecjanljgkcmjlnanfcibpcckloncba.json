{
  "background": {},
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://www.megacurioso.com.br/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "pt_BR",
  "description": "Receba em seu navegador as curiosidades mais interessantes.",
  "icons": {
    "128": "icon_128.png",
    "600": "icon_600.png",
    "76": "icon_76.png"
  },
  "manifest_version": 2,
  "name": "MegaCurioso Alertas",
  "permissions": [
    "notifications",
    "background",
    "activeTab"
  ],
  "short_name": "MegaCurioso Alertas",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.1"
}
