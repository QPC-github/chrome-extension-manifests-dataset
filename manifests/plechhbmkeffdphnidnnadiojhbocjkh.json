{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ticket ResaleTeam Stubhub",
  "description": "Stubhub Extension tool for TRT users. This extension lets you view sales and listings on Stubhub's event pages easily.",
  "short_name": "Stubhub Counter",
  "version": "1.2.4",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "permissions": [
    "cookies",
    "https://*.stubhub.com/*",
    "storage",
    "activeTab",
    "*://ticketgadgets.com/",
    "*://www.ticketgadgets.com/"
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  }
}
