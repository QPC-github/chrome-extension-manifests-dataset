{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EZProxy",
  "version": "4.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "ezproxy.js"
    ],
    "persistent": false
  },
  "options_page": "ezproxyoptions.html",
  "description": "Toggles addition of EZProxy domain extension and optional prefix to current URL for academic e-journal access.",
  "icons": {
    "16": "oclc16.png",
    "19": "oclc19.png",
    "38": "oclc38.png",
    "48": "oclc48.png",
    "128": "oclc128.png"
  },
  "web_accessible_resources": [
    "oclc16.png",
    "oclc19.png",
    "oclc38.png",
    "oclc48.png",
    "oclc128.png"
  ],
  "browser_action": {
    "default_title": "toggle EZProxy",
    "default_icon": {
      "19": "oclc19.png",
      "38": "oclc38.png"
    }
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
