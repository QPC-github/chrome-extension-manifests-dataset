{
  "background": {
    "scripts": [
      "js/config.js",
      "js/fn.js",
      "js/storage-local.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon.png",
      "38": "icon/icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "Procrastination Patrol"
  },
  "description": "A website tracker that tracks time spent on websites, and allows you to get reminders when you spend excessive time.",
  "icons": {
    "128": "icon/icon.png",
    "16": "icon/icon.png",
    "32": "icon/icon.png",
    "48": "icon/icon.png"
  },
  "manifest_version": 2,
  "name": "Procrastination Patrol",
  "offline_enabled": true,
  "optional_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "idle",
    "storage",
    "unlimitedStorage",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.0.2"
}
