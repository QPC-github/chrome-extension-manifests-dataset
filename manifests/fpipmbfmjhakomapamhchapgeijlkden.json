{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Slingshot Screen Sharing",
  "version": "1.11",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.slingshot.co.th/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
