{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Monhyip Toolbar."
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "description": "Toolbar - Monhyip.net - Мониторинг HYIP и autosurf проектов",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Monhyip Toolbar",
  "permissions": [
    "tabs",
    "http://monhyip.net/*",
    "http://www.monhyip.net/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.4"
}
