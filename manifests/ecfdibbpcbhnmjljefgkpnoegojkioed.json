{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LivingSocial",
  "description": "Living Social Deals App. Find and share the best things to do in your city. Unique and diverse offerings every day.",
  "version": "2.0",
  "browser_action": {
    "default_title": "LivingSocial",
    "default_icon": "img/icon32.png",
    "default_popup": "panel.html"
  },
  "icons": {
    "128": "img/icon128.png"
  },
  "permissions": [
    "*://*.livingsocial.com/"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/main.js"
    ]
  },
  "manifest_version": 2
}
