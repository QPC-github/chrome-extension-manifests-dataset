{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Процессор печати (chrome extension)",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Отправляет пакет выбранных документов на процессор печати.",
  "icons": {
    "48": "images/icon.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://html-tests/*",
        "*://docsbox.s7.ru/*",
        "*://docgeneration.s7.ru/*",
        "*://docsbox-test01.s7.ru/*",
        "*://docsbox-test.s7.ru/*",
        "*://*/api/PrintService/*"
      ],
      "js": [
        "scripts/content.js"
      ]
    }
  ]
}
