{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Elance proposal highlighting",
  "description": "Make your proposal highlighted for better viewing in projects lists.",
  "version": "0.2",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
