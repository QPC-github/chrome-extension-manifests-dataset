{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Uneddit",
  "version": "0.1",
  "description": "Show deleted and original unedited comments on Reddit",
  "permissions": [
    "https://www.reddit.com/r/*/comments/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "60": "icons/icon_60.png"
    }
  },
  "background": {
    "scripts": [
      "un_eddit.js"
    ]
  }
}
