{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Color My Css",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "name": "Color My Css",
    "default_icon": "images/icon48.png"
  },
  "manifest_version": 2
}
