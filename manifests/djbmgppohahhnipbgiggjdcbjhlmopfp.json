{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TTC Enlister",
  "description": "Proprietary Tool for The Talent Company, LLC.",
  "short_name": "TTC Enlister",
  "version": "1.5",
  "permissions": [
    "cookies",
    "alarms",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": "img/fevicon.png",
    "default_popup": "popup.html"
  }
}
