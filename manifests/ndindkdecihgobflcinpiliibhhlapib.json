{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "include_globs": [
        "http://www.delta.com/air-shopping/findFlights.action*",
        "https://www.delta.com/air-shopping/findFlights.action*"
      ],
      "js": [
        "DLChart.user.js"
      ],
      "matches": [
        "http://www.delta.com/air-shopping/findFlights.action*",
        "https://www.delta.com/air-shopping/findFlights.action*"
