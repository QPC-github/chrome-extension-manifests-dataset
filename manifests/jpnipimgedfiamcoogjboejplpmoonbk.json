{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Get Paper",
  "version": "0.1",
  "description": "Open URL of a research paper and click on the button to access paper. Asks to login to your UD Google Account.",
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_title": "Get Paper",
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
