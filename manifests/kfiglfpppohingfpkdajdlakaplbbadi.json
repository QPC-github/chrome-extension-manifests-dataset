{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "404ACG助手",
  "description": "专为404ACG盘开发,加快他的下载速度以及下载兼容性,保证以最快速度下载文件,并配以各种ACG资源UP主实用功能",
  "version": "1.5",
  "browser_action": {
    "default_title": "404ACG助手",
    "default_icon": {
      "19": "./icons/icon19.png",
      "38": "./icons/icon38.png"
    },
    "default_popup": "./html/popup.html"
  },
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webRequestBlocking",
    "contextMenus",
    "webRequest",
    "<all_urls>",
    "notifications"
  ],
  "manifest_version": 2,
  "homepage_url": "http://www.404acg.com/"
}
