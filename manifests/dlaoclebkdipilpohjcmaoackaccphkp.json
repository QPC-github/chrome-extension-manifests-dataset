{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Web Ninja",
  "description": "This extension will allow the user to grab elements on any site",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "grabninja.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "MAke this page editable!"
  },
  "permissions": [
    "activeTab"
  ]
}
