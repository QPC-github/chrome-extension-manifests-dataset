{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vergelijken XL",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Online aankopen en altijd extra korting ontvangen! Wij geven 70% van onze commissie aan de gebruiker!",
  "homepage_url": "https://www.vergelijkenxl.nl/",
  "icons": {
    "16": "icons/icon48.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Vergelijken XL",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "notifications"
  ]
}
