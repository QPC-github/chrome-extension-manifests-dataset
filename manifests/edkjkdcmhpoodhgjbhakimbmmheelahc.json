{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No More Hahas",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Replaces hahahas with lols :]",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "nomorehahas.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "No More Hahas: Replacing hahahas with lols :]",
    "default_icon": "icon.png"
  }
}
