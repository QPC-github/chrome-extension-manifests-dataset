{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fix Jalopnik",
  "description": "Fixes user interface annoyances with the Jalopnik Web site.",
  "version": "2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "fix_jalopnik.user.js"
      ],
      "matches": [
        "http://jalopnik.com/*"
      ]
    }
  ]
}
