{
  "browser_action": {
    "default_icon": "aahighlighter_icon_16.png",
    "default_title": "",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "jquery-1.8.3.js",
        "page.js"
      ]
    }
  ],
  "description": "AA Highlighter - 5 Simple Highlighters",
  "icons": {
    "128": "aahighlighter_icon_128.png",
    "16": "aahighlighter_icon_16.png"
  },
  "name": "AA Highlighter",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0.2",
  "web_accessible_resources": [
    "jquery-1.8.3.js",
    "popup2.js",
    "page.js"
  ],
  "manifest_version": 2
}
