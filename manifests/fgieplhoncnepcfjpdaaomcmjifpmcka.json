{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoAddS",
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
