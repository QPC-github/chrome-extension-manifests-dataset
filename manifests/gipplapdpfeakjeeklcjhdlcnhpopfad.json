{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "investo_content.js"
      ],
      "matches": [
        "http://www.investopedia.com/simulator/*",
        "http://www.barchart.com/*",
        "http://thestockmarketwatch.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Hack the Investopedia page to show todays history",
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "name": "investo hack",
  "optional_permissions": [
    "contentSettings",
    "storage"
  ],
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "cookies",
    "http://*/",
    "https://*/",
    "webRequest",
    "clipboardWrite"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.9"
}
