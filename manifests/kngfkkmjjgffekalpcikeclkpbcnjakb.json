{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "sc2tv free smiles",
  "version": "0.5",
  "manifest_version": 2,
  "author": "rkuzovlev",
  "externally_connectable": {
    "matches": [
      "https://*.rsf.com/*"
    ]
  },
  "icons": {
    "48": "images/peka48.png",
    "128": "images/peka128.png"
  },
  "permissions": [
    "http://chat.sc2tv.ru/*",
    "https://chat.sc2tv.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://chat.sc2tv.ru/*",
        "https://chat.sc2tv.ru/*"
      ],
      "js": [
        "content-scripts.js"
      ],
      "all_frames": true
    }
  ]
}
