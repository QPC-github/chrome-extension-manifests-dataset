{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notedown",
  "version": "0.3",
  "description": "Awesome markdown-powered note-editing that syncs across your devices",
  "permissions": [
    "storage",
    "alwaysOnTopWindows"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "128": "img/icon128.png"
  }
}
