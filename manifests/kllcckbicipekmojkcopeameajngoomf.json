{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extensions Manager",
  "manifest_version": 2,
  "version": "1.0.3.2",
  "permissions": [
    "management",
    "contextMenus",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon48.png",
      "38": "icon48.png"
    },
    "default_title": "Extensions Manager",
    "default_popup": "popup.html"
  }
}
