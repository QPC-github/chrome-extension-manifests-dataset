{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ClutchCart",
  "description": "ClutchCart is your own personal shopper/tailor.",
  "version": "0.0.4",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "storage",
    "https://clutch-cart.com/",
    "cookies"
  ]
}
