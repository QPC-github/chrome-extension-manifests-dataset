{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Curious Earth Daily",
  "description": "Stay up to speed with other topical environmental stories of the day that didn't make it into your daily Curious Earth email.",
  "version": "0.0.0.1",
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "scripts": [
      "vendor/jQuery/jquery-3.1.1.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [],
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  }
}
