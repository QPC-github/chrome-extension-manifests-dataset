{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "no-feed-linkedin.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ],
  "description": "Remove the feed from LinkedIn for a less distracting experience",
  "icons": {
    "16": "img/nofeed-16.png",
    "128": "img/nofeed-128.png",
    "512": "img/nofeed-512.png"
  },
  "manifest_version": 2,
  "name": "No Feed LinkedIn",
  "version": "0.0.1"
}
