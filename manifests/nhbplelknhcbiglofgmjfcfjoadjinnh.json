{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bunqFixer",
  "version": "1.0.1",
  "description": "Because it's hard to type bunq...",
  "icons": {
    "128": "bunqFixer.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "jquery.ba-replacetext.min.js",
        "bunqFixer.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
