{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "55PBX Web Softphone para PABX Virtual(Lite)",
  "short_name": "55PBX Webphone",
  "description": "Extens√£o do Webphone da plataforma de telefonia 55PBX (softphone web) para Google Chrome",
  "version": "1.9.4.6",
  "author": "55TELECOM Ltda.",
  "homepage_url": "https://www.55pbx.com",
  "background": {
    "persistent": true,
    "scripts": [
      "popup.js",
      "jquery.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "/assets/55_logo_16.png"
  },
  "icons": {
    "16": "/assets/55_logo_16.png",
    "48": "/assets/55_logo_48.png",
    "128": "/assets/55_logo_128.png"
  },
  "permissions": [
    "notifications",
    "tabs",
    "activeTab",
    "webNavigation",
    "webRequest",
    "https://ajax.googleapis.com/",
    "https://fone.55pbx.com/",
    "https://fone.localhost:8500/",
    "http://*/",
    "https://*/",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "web_accessible_resources": [
    "content.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/",
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "jquery.min.js",
        "phoneLib.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
