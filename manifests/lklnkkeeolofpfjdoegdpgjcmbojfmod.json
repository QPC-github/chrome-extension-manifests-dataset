{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Plugin inserts images instead of text tex formulas in vk",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://vk.com/*"
  ],
  "name": "VK tex to image",
  "version": "0.3",
  "manifest_version": 2
}
