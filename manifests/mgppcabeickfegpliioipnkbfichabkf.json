{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SR Case Changer",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "This extension was created for case conversion",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "SR Case Changer",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
