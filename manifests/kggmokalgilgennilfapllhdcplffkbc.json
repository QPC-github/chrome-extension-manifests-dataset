{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Emo-sion [Emoji Extension]",
  "short_name": "Emo-sion",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Simple, Lite, one Click Emoji extension.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Insert Emoji",
    "default_popup": "src/bg/background.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "cookies",
    "tabs",
    "activeTab"
  ]
}
