{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "minimum_chrome_version": "40",
  "name": "HMH Player",
  "icons": {
    "128": "icons/icon_hmh_1_128.png",
    "16": "icons/icon_hmh_1_16.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/lib/q/q.js",
        "background.js"
      ]
    }
  },
  "version": "3.1.2",
  "manifest_version": 2,
  "webview": {
    "partitions": [
      {
        "accessible_resources": [
          "embed/**/*"
        ],
        "name": "*"
      }
    ]
  },
  "permissions": [
    "webview",
    {
      "fileSystem": [
        "write"
      ]
    },
    "unlimitedStorage",
    "storage",
    "*://*.hrw.com/*",
    "*://*.thinkcentral.com/*",
    "*://*.hmhco.com/*",
    "*://*.google.com/*",
    "*://*.bing.com/*",
    "*://*.brightcove.com/*",
    "*://*.brightcovecdn.com/*",
    "*://*.boltdns.net/*",
    "*://*.llnwd.net/*",
    "*://*.omtrdc.net/*",
    "*://*.akamaihd.net/*",
    "*://content.link-systems.com/*",
    "*://127.0.0.1/*",
    "*://www.google-analytics.com/",
    "*://*.learnosity.com/*",
    "*://www.google-analytics.com/",
    "*://www.googletagmanager.com/*"
  ],
  "sockets": {
    "tcpServer": {
      "listen": "127.0.0.1:*"
    }
  },
  "description": ""
}
