{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cookie Collector",
  "version": "1.1",
  "description": "Auto Collect Cookie from Chrome for other use",
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "cookie.png",
    "48": "cookie.png",
    "128": "cookie.png"
  },
  "browser_action": {
    "default_icon": "cookie.png",
    "default_title": "Cookie Collector",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-1.4.2.js",
      "background.js"
    ]
  },
  "manifest_version": 2
}
