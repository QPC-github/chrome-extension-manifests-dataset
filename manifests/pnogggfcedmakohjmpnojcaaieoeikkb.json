{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IKawai",
  "description": "Lets your I become kawai! For my love, Yoyo!",
  "version": "1.8",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "ikawai.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "kawai_25.png",
    "kawai_20.png",
    "kawai_17.png",
    "kawai_15.png",
    "kawai_8.png",
    "kawai_5.png",
    "test.html"
  ]
}
