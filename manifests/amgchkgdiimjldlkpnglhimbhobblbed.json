{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Globb Search Engine",
  "description": "Search through web content",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://beta.globb.es/Content/images/ico/favicon.ico",
      "instant_url": "http://beta.globb.es/search/?s={searchTerms}",
      "is_default": true,
      "keyword": "Globb",
      "name": "Globb",
      "search_url": "http://beta.globb.es/search/?s={searchTerms}",
      "suggest_url": "http://beta.globb.es/search/?s={searchTerms}"
    }
  },
  "browser_action": {
    "default_icon": "icon_128.png"
  }
}
