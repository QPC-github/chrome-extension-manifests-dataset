{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube comments skin",
  "description": "Provides a familiar skin you are able to view within the YouTube comment section.",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
