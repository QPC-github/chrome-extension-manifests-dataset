{
  "author": "OEP Asia - Online Education Partnership",
  "background": {
    "persistent": false,
    "scripts": [
      "background-config.js",
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://lesson.oepasia.com/*"
      ]
    }
  ],
  "description": "This Google Chrome extension brings screen sharing to OEP Asia",
  "homepage_url": "http://www.oepasia.com",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Screensharing for OEP Asia",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.6",
  "web_accessible_resources": [
    "icon.png"
  ]
}
