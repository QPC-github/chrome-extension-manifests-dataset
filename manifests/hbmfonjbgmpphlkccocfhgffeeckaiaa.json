{
  "content_scripts": [
    {
      "css": [
        "powertoys.css"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "powertoys.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "This tool adds extra functionality to the JAMIS e-timecard system.",
  "manifest_version": 2,
  "name": "JAMIS Timecard Powertoys",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1.2",
  "web_accessible_resources": [
    "font/fontawesome-webfont.eot",
    "font/fontawesome-webfont.svg",
    "font/fontawesome-webfont.ttf",
    "font/fontawesome-webfont.woff",
    "font-awesome.min.css"
  ],
  "permissions": [
    "storage"
  ]
}
