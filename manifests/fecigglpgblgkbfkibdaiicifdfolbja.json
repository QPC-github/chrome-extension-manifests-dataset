{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/128x128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "bootstrap.min.css"
      ],
      "js": [
        "jquery.js",
        "content-instagram.js"
      ],
      "matches": [
        "*://www.instagram.com/*",
        "*://instagram.com/*"
      ]
    }
  ],
  "description": "Automatic Instagram Liker Extension by GramAssist",
  "icons": {
    "128": "img/128x128.png",
    "16": "img/16x16.png",
    "48": "img/48x48.png"
  },
  "manifest_version": 2,
  "name": "GramAssist AutoLiker",
  "offline_enabled": true,
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/",
    "storage",
    "*://www.instagram.com/*"
  ],
  "version": "1.0"
}
