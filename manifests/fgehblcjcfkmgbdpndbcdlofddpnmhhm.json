{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_pluginName__",
  "version": "1.2",
  "manifest_version": 2,
  "sandbox": {
    "pages": [
      "window.html"
    ]
  },
  "permissions": [
    "*://*/*",
    "storage",
    "clipboardWrite",
    "clipboardRead"
  ],
  "description": "__MSG_pluginDesc__",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "default_locale": "en_US",
  "app": {
    "background": {
      "scripts": [
        "background.js",
        "build.js"
      ]
    }
  }
}
