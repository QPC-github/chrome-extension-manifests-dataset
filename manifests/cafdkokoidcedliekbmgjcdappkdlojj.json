{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube Comment Hide / Toggle",
  "description": "Hides and toggles youtube comments",
  "version": "1.1",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch*"
      ],
      "js": [
        "toggle.js"
      ]
    }
  ]
}
