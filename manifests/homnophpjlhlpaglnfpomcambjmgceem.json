{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AwayTab",
  "description": "Discover a new place to travel to in every new tab you open. Daydream with beautiful images and the cheapest flights from your area.",
  "version": "1.0.5",
  "browser_action": {
    "default_icon": {
      "128": "AwayTabPadding128.png"
    }
  },
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "geolocation",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
