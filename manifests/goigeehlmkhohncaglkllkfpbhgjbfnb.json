{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SNOW | Shrink URL",
  "description": "This extension allows users to shorten the URL and protect it with a password",
  "version": "1.2",
  "browser_action": {
    "default_icon": {
      "19": "snow19.png",
      "38": "snow38.png"
    },
    "default_popup": "snow.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "snow16.png",
    "48": "snow48.png",
    "128": "snow128.png"
  }
}
