{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "description": "Video Ad Killer for YouKu / Tudou / Ku6",
  "icons": {
    "128": "icon128.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "AdKiller-XenMe",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.12.21.0",
  "web_accessible_resources": [
    "*.swf"
  ]
}
