{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "whois信息查询",
  "description": "查询域名whois信息，注册信息，whois反查,历史whois等",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "whois.png",
    "default_title": "域名whois查询",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus"
  ],
  "icons": {
    "16": "whois.png",
    "48": "whois.png",
    "128": "whois.png"
  }
}
