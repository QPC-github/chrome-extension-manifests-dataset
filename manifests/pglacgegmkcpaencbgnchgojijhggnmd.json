{
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Ctrl+Shift+O",
        "linux": "Ctrl+Shift+O",
        "mac": "Command+Shift+O",
        "windows": "Ctrl+Shift+O"
      }
    }
  },
  "description": "Özet Geçer misin sevgili kardeşim? Artık clickbait başlıklara son! Tıkla indir, clickbait'i bitir! ",
  "icons": {
    "128": "icons/icon.png",
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png"
  },
  "manifest_version": 2,
  "name": "Özet Geçer",
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "short_name": "ÖG!",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2019.7.31",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.hurriyet.com.tr/*",
        "*://www.karar.com/*",
        "*://www.milliyet.com.tr/*",
        "*://www.sozcu.com.tr/*",
        "*://www.sabah.com.tr/*",
        "*://www.haberturk.com/*",
        "*://*.ensonhaber.com/*",
        "*://*.yenisafak.com/*",
        "*://www.haberler.com/*",
        "*://*.haber7.com/*",
        "*://www.superhaber.tv/*"
      ],
      "js": [
        "/static/js/popper.min.js",
        "/static/js/tippy.js",
        "contentScript.js"
      ],
      "css": [
        "static/css/tippyTheme.css"
      ]
    }
  ]
}
