{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SearchBar Direction",
  "short_name": "SearchBar Direction",
  "description": "SearchBar Direction",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "SearchBar Direction",
    "default_popup": "app/show.html"
  },
  "chrome_settings_overrides": {
    "homepage": "http://searchbar.online/search.php",
    "startup_pages": [
      "http://searchbar.online/"
    ],
    "search_provider": {
      "is_default": true,
      "encoding": "UTF-8",
      "keyword": "SearchBar Direction Search",
      "name": "SearchBar Direction Search",
      "favicon_url": "http://searchbar.online/",
      "search_url": "http://searchbar.online?query={searchTerms}",
      "suggest_url": "http://searchbar.online?prefix={searchTerms}",
      "search_url_post_params": "testparam=extension"
    }
  },
  "icons": {
    "16": "img/showdirection16-999.png",
    "48": "img/showdirection48-999.png",
    "128": "img/showdirection128-999.png"
  }
}
