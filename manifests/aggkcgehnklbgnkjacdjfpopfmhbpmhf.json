{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "sf_icon_16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "main.js",
        "sfCore.ui.js",
        "background.js",
        "sfCore-latest.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons": {
    "128": "sf_icon_128.png",
    "16": "sf_icon_16.png",
    "32": "sf_icon_32.png"
  },
  "name": "siteface bookmarks",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*.siteface.net/",
    "http://*/*",
    "activeTab"
  ],
  "version": "2.0",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
