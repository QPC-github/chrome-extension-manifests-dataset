{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/lib/jQuery.js",
      "js/classes/Config.js",
      "js/classes/Client.js",
      "js/classes/Basic.js",
      "js/classes/ace.js",
      "js/onStart.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon16.png"
  },
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "16": "img/icon16.png"
  },
  "manifest_version": 2,
  "name": "ACE Bonus-Navigator",
  "version": "1.1.0",
  "description": "Bis zu 15% Cashback kassieren und mit Gutscheinen nochmal extra sparen!",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "storage",
    "webRequest",
    "background"
  ],
  "web_accessible_resources": [
    "img/error.png",
    "img/icon.png",
    "img/tb_logo.png",
    "img/ace_header_logo.jpg",
    "img/visual_ace.jpg",
    "img/Oneworld_logo.jpg",
    "img/green-check.jpg",
    "img/chevron-left.svg",
    "css/Marker.css"
  ]
}
