{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "OPUS Kitchen Monitor",
  "description": "Chrome Kitchen Monitor App",
  "manifest_version": 2,
  "version": "19.0",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "http://*/"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
