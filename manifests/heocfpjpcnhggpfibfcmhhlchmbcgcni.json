{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Freedawn Library Extension",
  "description": "This extension integrates with your Freedawn account to allow you to save pages with notes to your library",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Freedawn Extension"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus",
    "https://ajax.googleapis.com/",
    "https://www.freedawn.com/app/chromeext.php",
    "https://www.freedawn.com/app/logincheck.php",
    "https://www.freedawn.com/app/ext-image.php"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
