{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Do my NYSC Driver Safety Course",
  "description": "Allows you to do your Driver Safety Course while multi-tasking.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Do My Driver Safety Course"
  },
  "permissions": [
    "https://course.uceusa.com/courses/study.aspx",
    "tabs",
    "activeTab"
  ]
}
