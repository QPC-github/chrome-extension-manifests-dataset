{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cat Feed",
  "version": "1.3",
  "description": "Instead of friends, why not cats?",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery.min.js",
        "cats.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "images/icon-nyan16.png",
    "48": "images/icon-nyan48.png",
    "128": "images/icon-nyan128.png"
  },
  "manifest_version": 2
}
