{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CloudCodes For Business (EU)",
  "version": "1.0.11",
  "description": "CloudCodes For Business extension provided by cloudcodes software to enforces DLP and access control policies.",
  "background": {
    "persistent": true,
    "page": "TestDOM.html"
  },
  "icons": {
    "50": "/images/cloudcodesssoone.png"
  },
  "browser_action": {
    "default_icon": {
      "50": "/images/cloudcodesssoone.png"
    },
    "default_title": "CloudCodes For Business"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/public/user/dashboard*",
        "https://*/public/user/dashboard*",
        "http://*/AuthenticateUser*",
        "https://*/AuthenticateUser*",
        "http://*/jsp/public/olddashboard.jsp",
        "http://*/jsp/public/olddashboard.jsp",
        "http://*/GetLoginPage*",
        "https://*/GetLoginPage*",
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/content.js",
        "js/clipboardEvent.js",
        "js/print.js",
        "js/cloud_print.js",
        "js/printJQuery.js",
        "js/crypt.js",
        "js/mode-ecb.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "idle",
    "storage",
    "https://*.cloudcodestest.in/*",
    "http://*.cloudcodestest.in/*",
    "https://*.federateone.com/*",
    "http://*.federateone.com/*",
    "https://*.ssooneqa.com/*",
    "http://*.ssooneqa.com/*",
    "https://*.vault-one.com/*",
    "http://*.vault-one.com/*",
    "https://*.cloudcodes-eu.com/*",
    "http://*.cloudcodes-eu.com/*",
    "https://*.ssoone.com/*",
    "http://*.ssoone.com/*",
    "http://*.cfb.custom/*",
    "https://*.cfb.custom/*",
    "*://*.appspot.com/",
    "*://*.google.com/",
    "clipboardWrite",
    "clipboardRead",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "enterprise.deviceAttributes",
    "identity",
    "identity.email",
    "alarms",
    "browsingData"
  ],
  "web_accessible_resources": [
    "manifest.json"
  ],
  "commands": {
    "cloud_print": {
      "suggested_key": {
        "default": "Ctrl+Shift+4",
        "windows": "Ctrl+Shift+4",
        "chromeos": "Ctrl+Shift+4"
      },
      "description": "Google Cloud Print",
      "global": true
    }
  }
}
