{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL Shortener - Short for People",
  "description": "One-Click short URL generation.",
  "short_name": "S4P Generator",
  "version": "1.11",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png",
    "128": "img/128.png",
    "256": "img/256.png",
    "512": "img/512.png"
  },
  "browser_action": {
    "default_icon": "img/512.png",
    "default_popup": "generate.html"
  },
  "permissions": [
    "http://s4p.us/",
    "tabs"
  ]
}
