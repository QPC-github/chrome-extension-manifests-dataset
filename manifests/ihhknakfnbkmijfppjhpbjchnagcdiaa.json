{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DCmagaz - одежда для мужчин и женщин",
  "description": "Закладка которая поможет вам в один клик перейти на удобный поисковик вещей в интернет магазинах.",
  "version": "2.2",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js.js"
    ]
  },
  "browser_action": {
    "default_title": "DCmagaz",
    "default_icon": "icon32.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://www.dcmagaz.ru/"
  ]
}
