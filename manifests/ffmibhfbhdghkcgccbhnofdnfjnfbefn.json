{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pyrocynical Fix",
  "description": "Replaces all instances of \"Pyrocynical\" with Leafy Clone.",
  "version": "1.0.3",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "bg.js"
      ]
    }
  ]
}
