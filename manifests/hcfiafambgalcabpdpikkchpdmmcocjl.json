{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Unofficial Gimme Bar Extension",
  "version": "0.13",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Unofficial Gimme Bar Extension",
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "description": "Gimme! button for Google Chrome Extensions",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}
