{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rádio Rede",
  "version": "1.2",
  "description": "Rádio Rede - Chrome",
  "content_scripts": [
    {
      "matches": [
        "http://facebook.com/*",
        "https://facebook.com/*",
        "http://www.facebook.com/*",
        "https://www.facebook.com/*",
        "http://twitter.com/*",
        "https://twitter.com/*",
        "http://www.twitter.com/*",
        "https://www.twitter.com/*",
        "http://plus.google.com/*",
        "https://plus.google.com/*",
        "http://instagram.com/*",
        "https://instagram.com/*",
        "http://www.instagram.com/*",
        "https://www.instagram.com/*"
      ],
      "js": [
        "magic.js"
      ],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_icon": "icone.png"
  },
  "icons": {
    "16": "icone16.png",
    "48": "icone48.png",
    "128": "icone128.png"
  },
  "manifest_version": 2
}
