{
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon16.ico",
    "default_popup": "stockPrices.html"
  },
  "description": "This application displays the latest stock prices",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.ico"
  },
  "name": "Check Stock Prices",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://www.finance.google.com/",
    "http://download.finance.yahoo.com/d/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0"
}
