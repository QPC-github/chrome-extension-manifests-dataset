{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TSheets Time Tracking",
  "version": "1.0.53",
  "manifest_version": 2,
  "offline_enabled": true,
  "description": "TSheets Time Tracking is built for busy workdays.",
  "app": {
    "background": {
      "scripts": [
        "assets/tsheets-chrome-app-1.0.53.js",
        "assets/background.js"
      ]
    }
  },
  "permissions": [
    "identity",
    "https://www.google-analytics.com/",
    "storage",
    "unlimitedStorage",
    "https://rest.tsheets.com/api/v1/",
    "https://rest.tsheets.com/",
    "idle"
  ],
  "icons": {
    "128": "assets/icons/logo128.png"
  }
}
