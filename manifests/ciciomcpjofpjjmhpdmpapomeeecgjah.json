{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gamekult no-queue",
  "version": "1.0",
  "description": "Disable autoqueue and autoplay",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://*.gamekult.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
