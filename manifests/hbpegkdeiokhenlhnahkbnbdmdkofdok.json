{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Kapook! Alert",
  "version": "0.4.1",
  "description": "รับข่าวสารรวดเร็วทันใจ จากเว็บไซต์ kapook.com",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*.kapook.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "prototype.js",
      "background.js"
    ]
  },
  "manifest_version": 2
}
