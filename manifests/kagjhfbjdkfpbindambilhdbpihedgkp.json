{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK.Suggests",
  "version": "2.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "Уведомления о предложенных новостях",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "https://*.vk.com/*",
    "https://vk.com/*",
    "https://*.vk.me/*",
    "notifications",
    "alarms"
  ],
  "background": {
    "scripts": [
      "jquery-1.7.2.min.js",
      "jquery.playSound.js",
      "vk_lib.js",
      "data_model.js",
      "Helper.js",
      "controller.js"
    ]
  },
  "minimum_chrome_version": "28"
}
