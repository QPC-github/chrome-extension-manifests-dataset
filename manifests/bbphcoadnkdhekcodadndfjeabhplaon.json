{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Let's Hyde Resort & Villas",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Great Deals & Updates",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "lets hyde logo dark 128x128.png",
    "default_title": "Great Deals & Updates",
    "default_popup": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "lets hyde logo dark 128x128.png"
  }
}
