{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Quick Dirs",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Allows users to get directions to an address from various stored addresses.",
  "options_page": "options.html",
  "icons": {
    "48": "icon48.png",
    "128": "icon48.png"
  },
  "browser_action": {
    "default_icon": "menu_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://maps.google.com/*",
    "https://maps.googleapis.com/*"
  ]
}
