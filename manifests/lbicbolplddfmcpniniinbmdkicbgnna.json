{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Calculus Center",
  "description": "Master the art of solving derivatives and integrals, and learn more about calculus methods and practices.",
  "version": "1.0",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "<all_urls>"
  ]
}
