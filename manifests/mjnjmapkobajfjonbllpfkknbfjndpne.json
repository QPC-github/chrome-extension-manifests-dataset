{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Ivan Nedzvetskiy",
  "content_scripts": [
    {
      "js": [
        "core.js"
      ],
      "matches": [
        "http://www.vk.com/*",
        "https://www.vk.com/*",
        "http://vk.com/*",
        "https://vk.com/*",
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "default_locale": "ru",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "permissions": [
    "tabs",
    "http://www.vk.com/*",
    "https://www.vk.com/*"
  ],
  "version": "1.0.8",
  "web_accessible_resources": [
    "sorter.js",
    "img/icon.svg",
    "/img/icon.svg"
  ]
}
