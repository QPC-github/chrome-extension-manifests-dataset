{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nájsť & Zobraziť Film",
  "description": "Zobrazí informácie o filme.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "Icon-128.png",
    "default_popup": "movie.html",
    "default_title": "Zobraziť informácie o Filme"
  },
  "icons": {
    "128": "Icon-128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
  ]
}
