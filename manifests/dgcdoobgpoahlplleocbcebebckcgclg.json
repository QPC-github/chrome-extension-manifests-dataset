{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Amazon Trap Blocker",
  "description": "Amazonのバーに表示される住所と名前を非表示にします。",
  "version": "1.11",
  "icons": {
    "16": "images/icon/icon_trap_16.png",
    "48": "images/icon/icon_trap_48.png",
    "128": "images/icon/icon_trap_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/*"
      ],
      "css": [
        "css/contents_style.css"
      ]
    }
  ],
  "permissions": [
    "https://www.amazon.co.jp/*"
  ]
}
