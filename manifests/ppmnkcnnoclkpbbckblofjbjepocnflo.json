{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "a161803398",
  "name": "Ultra Button",
  "version": "1.0.7",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_extension_description__",
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "__MSG_extension_title__",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "32": "img/icon.png",
    "64": "img/icon.png"
  },
  "background": {
    "scripts": [
      "js/sync.js",
      "js/background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "tabs",
    "history",
    "downloads",
    "downloads.open",
    "downloads.shelf",
    "management",
    "storage",
    "notifications",
    "sessions"
  ]
}
