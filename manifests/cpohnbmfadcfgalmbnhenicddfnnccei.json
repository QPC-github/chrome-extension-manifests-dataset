{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Domain Defender (TM)",
  "short_name": "AI6 Website Security Tester",
  "description": "Made by ai6net to check blacklisted sites \n https://ai6.net",
  "version": "1.0.1.8",
  "icons": {
    "19": "img/19x19.png",
    "48": "img/48x48.png",
    "100": "img/100x100.png",
    "128": "img/128x128.png"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "<all_urls>",
    "storage",
    "activeTab"
  ],
  "web_accessible_resources": [
    "img/AMBER_250x278.png",
    "img/DEFENDER_LOGO_RED_63x69.png"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/19x19.png",
    "default_popup": "popup.html"
  }
}
