{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ad scripts",
  "version": "2.0",
  "icons": {
    "16": "scriptt.png",
    "128": "scriptt.png"
  },
  "browser_action": {
    "default_icon": "scriptt.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "nouse.js"
    ]
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
