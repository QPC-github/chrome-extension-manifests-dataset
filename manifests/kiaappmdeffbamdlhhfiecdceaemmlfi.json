{
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "zigzag-highway-js/bg.js"
    ]
  },
  "version": "0.0.1",
  "icons": {
    "48": "images/icon-small.png",
    "128": "images/zigzag-highway-128.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "images/icon-small.png",
    "default_popup": "zigzag-highway.html"
  },
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "permissions": [
    "tabs",
    "topSites"
  ]
}
