{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ASUS Download Master Chrome Extension",
  "description": "Simplifies adding torrent, ed2k, ftp files to ASUS Download Master queue",
  "version": "0.0.5",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": {},
  "icons": {
    "64": "icon.png"
  }
}
