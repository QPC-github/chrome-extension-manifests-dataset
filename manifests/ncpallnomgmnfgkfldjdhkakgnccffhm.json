{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "N1ntendo mute a member",
  "description": "Klaar met posts van mensen die enkel kunnen zaniken? Hier is je oplossing.",
  "version": "1.4",
  "author": "Volleybaas",
  "browser_action": {
    "default_icon": "mute.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://www.n1ntendo.nl/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.n1ntendo.nl/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
