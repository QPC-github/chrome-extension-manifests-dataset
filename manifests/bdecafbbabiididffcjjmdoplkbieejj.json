{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Big Facebook Chat",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Enlarges the size of the chat window for Facebook messages.",
  "content_scripts": [
    {
      "matches": [
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ],
      "js": [
        "./bfc.js"
      ],
      "run_at": "document_end"
    }
  ]
}
