{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": " বাংলা বাকাডেমি",
  "description": "এখানে বাংলা বাকাডেমি সাইট থেকে অপ্রয়োজনীয় শব্দ ও তার ব্যাখ্যা সরবরাহ করা হয়।",
  "version": "1.0.0",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "browser_action": {
    "default_icon": "logo16.png"
  },
  "chrome_url_overrides": {
    "newtab": "newpage.html"
  },
  "permissions": [
    "https://*.bakademia.com/*",
    "notifications"
  ]
}
