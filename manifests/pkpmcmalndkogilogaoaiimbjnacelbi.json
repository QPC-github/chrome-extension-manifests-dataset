{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_popup": "html/worldStudyIndex.html",
    "default_title": ""
  },
  "permissions": [
    "storage",
    "webRequestBlocking",
    "tabs",
    "webRequest",
    "<all_urls>",
    "*://*.educationnews.world/*",
    "cookies"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "2.4.8",
  "manifest_version": 2,
  "short_name": "UniNews",
  "name": "Uni World News",
  "background": {
    "scripts": [
      "js/axios.js",
      "js/getEducationNews.js",
      "js/jquery-2.7.2.js"
    ]
  }
}
