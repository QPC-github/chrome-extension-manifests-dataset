{
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "content_scripts": [
    {
      "css": [
        "flipper.css"
      ],
      "js": [
        "VideoFlipper.js"
      ],
      "matches": [
        "*://*.youtube.com/watch*"
      ]
    }
  ],
  "description": "This extension gives a option on youtube videos to mirror the video",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "YouTube Video Flipper",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
