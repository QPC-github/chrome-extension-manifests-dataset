{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "16": "icon.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Squnit Test"
  },
  "manifest_version": 2,
  "name": "The Squint Test",
  "description": "Do a simple squint test on the page to ensure the primary elements have focus",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0"
}
