{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tech in Asia Premium Reader",
  "version": "1.1.0",
  "description": "Allow anyone to read Tech in Asia Premium Content",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.techinasia.com/*"
      ],
      "css": [
        "contentScript.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
