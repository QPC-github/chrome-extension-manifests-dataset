{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Isaura Chrome extension",
  "description": "This extension allows the user to request a cross domain server!",
  "version": "1.83",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "script/jquery3.2.1.min.js",
      "script/utils/uc.js",
      "script/utils/api/api.js",
      "script/utils/helpers/helpers.js",
      "script/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script/jquery3.2.1.min.js",
        "script/utils/uc.js",
        "script/utils/api/api.js",
        "script/utils/helpers/helpers.js",
        "script/moment.js",
        "script/browser/browser.start.js"
      ],
      "css": [
        "css/style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "http://localhost:3000/*",
      "*://*.es6.vn/*",
      "*://google.com/*",
      "*://google.com.vn/*",
      "*://facebook.com/*"
    ]
  }
}
