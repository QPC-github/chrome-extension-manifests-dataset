{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Domlia SEO Analysis",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Domlia SEO Analysis Chrome extension lets you easily analyze websites.",
  "browser_action": {
    "default_icon": "domlia.png"
  },
  "background": {
    "scripts": [
      "domlia.js"
    ]
  },
  "permissions": [
    "http://www.domlia.com/"
  ],
  "icons": {
    "128": "domlia128.png",
    "64": "domlia64.png",
    "32": "domlia32.png",
    "16": "domlia16.png"
  }
}
