{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Talentful - Offer Manager",
  "version": "1.0",
  "description": "Open the offer manager page in a new tab",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2,
  "icons": {
    "128": "images/tf-logo-128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/tf-logo-128.png"
    },
    "default_title": "Talentful Offer Manager"
  }
}
