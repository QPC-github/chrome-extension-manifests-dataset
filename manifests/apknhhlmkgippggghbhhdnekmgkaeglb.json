{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Voldemort Replacer",
  "description": "This extension replaces the word Voldemort in websites with one of 3 random values.",
  "version": "1.2",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "voldemortReplacer.js"
      ],
      "run_at": "document_end"
    }
  ]
}
