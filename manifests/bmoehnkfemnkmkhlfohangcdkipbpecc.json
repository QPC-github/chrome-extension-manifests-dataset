{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Light Shopping",
  "description": "Все любимые магазины в одном расширении",
  "version": "1.4",
  "author": "Ali.Guru",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "http://ali.guru/s",
  "icons": {
    "128": "icon/128.png",
    "64": "icon/64.png",
    "48": "icon/48.png",
    "19": "icon/19.png",
    "16": "icon/16.png"
  },
  "browser_action": {
    "default_icon": "icon/16.png",
    "default_popup": "html/popup.html",
    "default_title": "Light Shopping"
  }
}
