{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Get QR code from Url",
  "description": "This extension shows a QR code from the url of the current page",
  "version": "1.5",
  "browser_action": {
    "default_icon": "qurl/images/icon_qurl.png",
    "default_popup": "qurl/qurl_popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
