{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Teritree",
  "description": "Create collections of products you like and save them to www.teritree.com to share, write about, or buy later.",
  "version": "1.2",
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": [
      "contextMenuCode.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "Logo20.png",
    "48": "Logo48.png",
    "128": "Logo128.png"
  },
  "browser_action": {
    "default_icon": "Logo20.png"
  },
  "manifest_version": 2
}
