{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZIP Extractor",
  "version": "2.9",
  "manifest_version": 2,
  "description": "Open ZIP files on your computer and Google Drive",
  "container": "GOOGLE_DRIVE",
  "api_console_project_id": "824911851129",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/zip",
          "application/x-zip",
          "application/x-zip-compressed",
          "application/x-rar-compressed",
          "application/rar",
          "application/x-rar",
          "application/vnd.google.drive.ext-type.zip"
        ],
        "href": "https://zipextractor.app/",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "16": "zip-extractor-icon-16.png",
    "24": "zip-extractor-icon-24.png",
    "32": "zip-extractor-icon-32.png",
    "48": "zip-extractor-icon-48.png",
    "64": "zip-extractor-icon-64.png",
    "128": "zip-extractor-icon-128.png",
    "256": "zip-extractor-icon-256.png"
  },
  "app": {
    "launch": {
      "web_url": "https://zipextractor.app/"
    }
  }
}
