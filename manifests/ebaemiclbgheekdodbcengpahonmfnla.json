{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "Papaly",
  "version": "1.0.1.5",
  "default_locale": "en",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "app": {
    "urls": [
      "*://papaly.com/",
      "*://www.papaly.com/"
    ],
    "launch": {
      "web_url": "https://papaly.com/?source=app-chrome"
    }
  },
  "icons": {
    "16": "default-16.png",
    "48": "default-48.png",
    "128": "default-128.png"
  },
  "permissions": [
    "background",
    "clipboardRead",
    "clipboardWrite",
    "geolocation",
    "unlimitedStorage",
    "notifications"
  ]
}
