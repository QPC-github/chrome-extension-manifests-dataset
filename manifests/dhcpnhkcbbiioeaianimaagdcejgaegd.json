{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ADA Visit Counter",
  "version": "1.2",
  "description": "Count your visit to website",
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
