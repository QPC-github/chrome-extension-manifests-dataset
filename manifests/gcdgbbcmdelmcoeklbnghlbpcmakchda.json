{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Network Requests Monitor",
  "version": "3.2",
  "description": "Network Requests Monitor for Developers",
  "devtools_page": "networkrequests.html",
  "manifest_version": 2,
  "web_accessible_resources": [
    "*"
  ],
  "permissions": [
    "tabs",
    "clipboardWrite",
    "http://*/*",
    "https://*/*",
    "webRequest"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
