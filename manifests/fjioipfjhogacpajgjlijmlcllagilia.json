{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tuzag extension",
  "description": "Tuzag extension allows you view and update your Tuzag's profile.",
  "version": "0.1",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Tuzag extension"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://tuzagtag.com/"
  ]
}
