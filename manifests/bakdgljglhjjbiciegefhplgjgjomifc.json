{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "fredev Development Inspector",
  "version": "1.8.619",
  "description": "Troubleshoot your website today and get recommendations for improving your websites performance and your search engine optimisation!",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Let's analyze your website!"
  },
  "icons": {
    "16": "img/inactive.png",
    "32": "img/icon-32x32.png",
    "48": "img/icon-48x48.png",
    "128": "img/icon-128x128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "webRequest"
  ],
  "web_accessible_resources": [
    "js/*.*",
    "img/*.*",
    "img/ico/*.*",
    "img/badge/*.*"
  ]
}
