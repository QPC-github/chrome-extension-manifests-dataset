{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Enable Copy",
  "version": "0.0002",
  "manifest_version": 2,
  "description": "Allow selecting, copying, pasting, edit pages.",
  "icons": {
    "19": "icon19.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "enable.js"
  ],
  "browser_action": {
    "default_icon": "icon19-disable.png",
    "default_title": "Enable Copy"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
