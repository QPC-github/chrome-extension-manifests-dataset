{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Final Grade Calculator",
  "description": "Find out what you need on your final exam!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
