{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RAHS Screenshot Extension",
  "version": "1.3",
  "description": "Take a screenshot",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Take a screen shot!"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}
