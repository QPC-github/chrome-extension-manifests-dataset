{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ariba DocumentID Webjumper",
  "short_name": "jumpToDocumentId",
  "author": "Renars Rozitis",
  "version": "1.2",
  "description": "Easily jump to an existing document on the current realm by the document's ID",
  "icons": {
    "128": "document.png",
    "64": "document.png",
    "48": "document.png",
    "32": "document.png",
    "24": "document.png",
    "16": "document.png"
  },
  "background": {
    "persisten": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "32": "document.png"
    },
    "default_title": "Ariba DocumentID Webjumper"
  },
  "omnibox": {
    "keyword": "docid"
  }
}
