{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hipchat History Cleaner",
  "description": "This extension allows to remove history for a room page in HipChat",
  "version": "1.0",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.hipchat.com/history/room/*"
      ],
      "js": [
        "jquery.js",
        "addkey.js"
      ]
    }
  ]
}
