{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SkySplore Search Engine",
  "description": "Adds a search engine that searches skysplore.com",
  "version": "1.0",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SkySplore",
      "search_url": "https://www.skysplore.com/search?q={searchTerms}",
      "keyword": "disc",
      "favicon_url": "https://www.skysplore.com/assets/img/favicon.ico",
      "is_default": true,
      "encoding": "UTF-8"
    }
  }
}
