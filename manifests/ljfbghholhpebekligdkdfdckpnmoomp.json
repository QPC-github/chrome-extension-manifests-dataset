{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrafficLab ImageGrab",
  "description": "Grab images off the web, and manipulate them using trafficlab tools.",
  "version": "1.0",
  "background": {
    "scripts": [
      "imageGrab.js"
    ]
  },
  "icons": {
    "16": "potion_green.png",
    "48": "potion_green48.png",
    "128": "potion_green48.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
