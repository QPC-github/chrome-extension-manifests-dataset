{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark",
  "version": "2.2",
  "description": "Get All Sanmina Application By Plant And Projects",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "32": "img/icon32.png",
      "64": "img/icon64.png",
      "128": "img/icon128.png"
    }
  },
  "web_accessible_resources": [
    "img/icon64.png"
  ],
  "icons": {
    "32": "img/icon32.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}
