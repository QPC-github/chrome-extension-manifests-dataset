{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Your There",
  "version": "1.0",
  "description": "A plugin to annoy the grammar snob in all of us.",
  "content_scripts": [
    {
      "js": [
        "index.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "default-icon.png",
    "default_title": "Your There"
  },
  "manifest_version": 2,
  "offline_enabled": true,
  "short_name": "Your There"
}
