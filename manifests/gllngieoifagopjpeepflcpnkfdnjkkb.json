{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebChronicler",
  "version": "1.09",
  "description": "Search and organize your entire browsing history",
  "permissions": [
    "https://api.webchronicler.io/*",
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js",
      "js/jquery.min.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/home.png",
      "32": "images/home.png",
      "48": "images/home.png",
      "128": "images/home.png"
    }
  },
  "icons": {
    "16": "images/home.png",
    "32": "images/home.png",
    "48": "images/home.png",
    "128": "images/home.png"
  },
  "manifest_version": 2
}
