{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EA analyzer",
  "version": "1.02",
  "description": "EA analyzer aims at easying the debug of web pages containing Eulerian JS snippets.",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": true
  }
}
