{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "MySYS Tools"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "inject.css"
      ],
      "js": [
        "0-jquery-3.4.1.min.js",
        "inject.js"
      ],
      "matches": [
        "*://*.mysys.com/*",
        "*://*.yelmer.net/*",
        "http://127.0.0.1:15820/*",
        "file:///C:/*",
        "*://*.amazon.com/*",
        "*://*.amazon.ca/*",
        "*://*.amazon.co.uk/*",
        "*://*.amazon.de/*",
        "*://*.amazon.es/*",
        "*://*.amazon.fr/*",
        "*://*.amazon.it/*",
        "*://*.ebay.com/*",
        "*://*.ebay.ca/*",
        "*://*.ebay.co.uk/*",
        "*://*.ebay.de/*",
        "*://*.ebay.it/*",
        "*://*.ebay.fr/*",
        "*://*.ebay.es/*",
        "*://*.ebay.nl/*",
        "*://*.ebay.be/*",
        "*://*.ebay.ie/*",
        "*://*.ebay.at/*",
        "*://*.ebay.ch/*",
        "*://*.ebay.com.au/*"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "MySYS",
  "short_name": "MySYS Tools",
  "description": "MySYS Tools for Amazon & eBay",
  "homepage_url": "https://www.mysys.com/",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "storage",
    "*://*.mysys.com/*",
    "*://*.yelmer.net/*",
    "http://127.0.0.1:15820/*",
    "file:///C:/*",
    "*://*.amazon.com/*",
    "*://*.amazon.ca/*",
    "*://*.amazon.co.uk/*",
    "*://*.amazon.de/*",
    "*://*.amazon.es/*",
    "*://*.amazon.fr/*",
    "*://*.amazon.it/*",
    "*://*.ebay.com/*",
    "*://*.ebay.ca/*",
    "*://*.ebay.co.uk/*",
    "*://*.ebay.de/*",
    "*://*.ebay.it/*",
    "*://*.ebay.fr/*",
    "*://*.ebay.es/*",
    "*://*.ebay.nl/*",
    "*://*.ebay.be/*",
    "*://*.ebay.ie/*",
    "*://*.ebay.at/*",
    "*://*.ebay.ch/*",
    "*://*.ebay.com.au/*"
  ],
  "version": "1.1.7"
}
