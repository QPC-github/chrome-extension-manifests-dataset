{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kanoot",
  "description": "Kahoot gets nooted",
  "version": "2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://play.kahoot.it/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "activeTab",
    "storage"
  ]
}
