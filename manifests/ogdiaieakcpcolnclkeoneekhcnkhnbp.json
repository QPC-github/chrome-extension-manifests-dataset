{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "LJMine",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "LiveJournal style changer: applies your defined style to other LJ posts from different users",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
