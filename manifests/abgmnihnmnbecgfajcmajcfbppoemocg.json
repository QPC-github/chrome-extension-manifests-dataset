{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cookie Monster",
  "description": "Show your site cookies",
  "version": "1.2",
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_title": "Cookie Monster",
    "default_popup": "popup.html",
    "default_icon": "cookie-monster-logo.png"
  }
}
