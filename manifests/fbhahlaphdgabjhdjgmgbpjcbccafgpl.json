{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ninetech ShortURL",
  "description": "This extension helps you convert long URL's into a short Ninetech URL.",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "16": "icon.png"
  }
}
