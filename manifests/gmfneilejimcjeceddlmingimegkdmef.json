{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Adwaita Scrollbars",
  "description": "Scrollbars to integrate with the GNOME desktop environment.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "files/scrollbars.css"
      ]
    }
  ]
}
