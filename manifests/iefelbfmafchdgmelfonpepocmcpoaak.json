{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "O'Round",
  "manifest_version": 2,
  "version": "2.5",
  "description": "O'Round is a super easy recruitment Software with a built-in sourcing tool for social media platforms.",
  "browser_action": {
    "default_title": "O'Round",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
