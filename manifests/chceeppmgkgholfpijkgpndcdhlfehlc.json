{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "HouseHusher",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Quiet annoying Realtor speak by converting all their speak to lower case!",
  "content_scripts": [
    {
      "matches": [
        "http://redfin.com/*",
        "http://www.redfin.com/*",
        "http://zillow.com/*",
        "http://www.zillow.com/*"
      ],
      "css": [
        "husher.css"
      ],
      "js": [
        "husher.js"
      ]
    }
  ]
}
