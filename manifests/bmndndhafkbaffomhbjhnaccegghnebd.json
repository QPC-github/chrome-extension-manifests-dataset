{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shopography",
  "description": "Shopography is a social network for products. Tell your friends about stuff you bought or want and see what your friends are into.",
  "version": "1.1.2",
  "icons": {
    "16": "shopo_logo_16.png",
    "32": "shopo_logo_32.png",
    "48": "shopo_logo_48.png",
    "64": "shopo_logo_64.png",
    "128": "shopo_logo_128.png"
  },
  "browser_action": {
    "default_icon": "shopo_logo_32.png",
    "default_title": "Shopography"
  },
  "background": {
    "scripts": [
      "jquery-1.7.1.min.js",
      "jquery.json-2.3.min.js",
      "jquery.url.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
