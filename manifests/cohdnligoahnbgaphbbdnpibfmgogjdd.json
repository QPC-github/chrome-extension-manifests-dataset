{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Get my IP (by Yogev Nisim)",
  "description": "This extension adds an ip adress easy discover",
  "version": "1.2.6",
  "icons": {
    "128": "mail_128x128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
