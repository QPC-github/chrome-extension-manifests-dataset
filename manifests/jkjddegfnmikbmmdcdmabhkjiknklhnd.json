{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Umbraco Debug",
  "version": "1.0",
  "manifest_version": 2,
  "description": "View the stack trace information for the current page. For debugging Umbraco applications",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "umbraco-debug.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-48.png"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
