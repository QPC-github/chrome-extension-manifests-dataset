{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vik",
  "short_name": "Vik",
  "description": "Manage your traffic throw SocksProxy",
  "version": "2.6",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "When On show Country",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://torvik.me/",
    "tabs",
    "background",
    "proxy",
    "management",
    "storage"
  ],
  "icons": {
    "16": "116.png",
    "48": "148.png",
    "128": "1128.png"
  }
}
