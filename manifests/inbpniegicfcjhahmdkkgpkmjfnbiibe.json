{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Addicting & Free Online Games",
  "version": "1.0.6",
  "description": "Collection of addicting, fun and free mini-games to play online in your browser.",
  "browser_action": {
    "default_title": "Mini Games",
    "default_popup": "/html/popup.html"
  },
  "web_accessible_resources": [
    "images/*.png",
    "images/*.jpg",
    "images/*.svg",
    "sound/*.mp3"
  ],
  "content_scripts": [],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://apis.google.com; object-src 'self'"
}
