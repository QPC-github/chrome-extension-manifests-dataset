{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Plug.dj - Auto woot",
  "description": "Auto clicks Woot every minute.",
  "manifest_version": 2,
  "version": "1.1",
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "content_script.js"
      ],
      "matches": [
        "*://*.plug.dj/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://*.plug.dj/*"
  ]
}
