{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "osu!ext",
  "version": "1.0",
  "description": "osu! large batch downloader. Recover played songs or download all that appears in search.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "storage",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "500": "assets/icons/icon.png"
  }
}
