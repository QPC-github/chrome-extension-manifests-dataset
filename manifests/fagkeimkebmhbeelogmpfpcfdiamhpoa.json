{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Well Spent â€” See prices in hours",
  "version": "2.4",
  "description": "Replace all prices with time based on your hourly wage.",
  "author": "Greg Greiner",
  "icons": {
    "16": "icon.png",
    "48": "icon@2x.png",
    "128": "icon@3x.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon@2x.png",
      "128": "icon@3x.png"
    },
    "default_title": "Time Well Spent"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "short_name": "Time Well Spent",
  "manifest_version": 2
}
