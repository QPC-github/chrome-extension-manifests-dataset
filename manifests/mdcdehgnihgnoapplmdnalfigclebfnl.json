{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VirtualBrest Cleaner",
  "version": "1.2",
  "permissions": [
    "activeTab",
    "http://virtualbrest.by/*",
    "https://virtualbrest.by/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://virtualbrest.by/*",
        "https://virtualbrest.by/*",
        "http://brest.name/*",
        "https://brest.name/*"
      ],
      "js": [
        "my.js"
      ],
      "css": [
        "my.css"
      ]
    }
  ]
}
