{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Letterboxd + The Pirate Bay links",
  "version": "1.0.0.4",
  "manifest_version": 2,
  "description": "Adds a 'Find on The Pirate Bay'-option to film functions on letterboxd.com",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://letterboxd.com/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "tbp-icon.png"
  ]
}
