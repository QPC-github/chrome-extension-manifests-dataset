{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zarin",
  "description": "Get the latest rates Gold, Coin, Currency, Oil, Metals and Iran Economic News.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
