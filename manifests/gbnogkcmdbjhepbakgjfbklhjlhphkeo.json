{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Multipath Downloader",
  "short_name": "다중 경로 다운로더",
  "description": "__MSG_appDesc__",
  "default_locale": "ko",
  "version": "1.0.9",
  "options_page": "options.html",
  "homepage_url": "http://kalize.iptime.org:8099/gexShare/",
  "externally_connectable": {
    "matches": [
      "http://kalize.iptime.org/*",
      "https://kalize.iptime.org/*",
      "http://localhost/*",
      "http://localhost:8888/*"
    ]
  },
  "permissions": [
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "manager.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "data/icon128.png"
  }
}
