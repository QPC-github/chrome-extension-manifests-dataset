{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clipmate media files saver",
  "description": "Saving Vkontakte media clips easily. Free one click file saving utility.",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "loader.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "web_accessible_resources": [
    "/main.js"
  ]
}
