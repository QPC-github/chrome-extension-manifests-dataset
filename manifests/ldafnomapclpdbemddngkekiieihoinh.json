{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kloud 12 Lecture Capture",
  "description": "An extension to allow you to record your screen as part of Kloud 12's lecture capture.",
  "version": "0.1.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://www.etherwyse.dev/*",
      "*://lecture.kloud-12.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
