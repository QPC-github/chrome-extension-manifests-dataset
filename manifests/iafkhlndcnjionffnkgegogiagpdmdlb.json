{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NFL New York Jets New Tab",
  "version": "3.10.3.1",
  "description": "The official new tab page for the New York Jets! Stay up on the latest news videos and highlights from the J-E-T-S JETS JETS JETS!",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "NFL New York Jets New Tab",
    "default_icon": {
      "19": "icon19.png"
    }
  },
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage",
    "unlimitedStorage",
    "system.cpu"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://pages.brandthunder.com/*/welcome*"
      ],
      "js": [
        "themeinstall.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/amazon-icon.png",
    "images/ebay-icon.png",
    "images/facebook-icon.png",
    "images/flickr-icon.png",
    "images/foursquare-icon.png",
    "images/games-icon.png",
    "images/googleplus-icon.png",
    "images/groupon-icon.png",
    "images/linkedin-icon.png",
    "images/music-icon.png",
    "images/myspace-icon.png",
    "images/twitter-icon.png",
    "images/youtube-icon.png",
    "images/pinterest-icon.png"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
