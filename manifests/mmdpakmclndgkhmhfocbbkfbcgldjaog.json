{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*.pdf+html"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "converted_from_user_script": true,
  "description": "Removes HTML frames when viewing PDFs of articles",
  "name": "PDF full screener",
  "version": "2.0"
}
