{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CRM Download Repair",
  "short_name": "CRM Download Repair",
  "version": "2.2",
  "description": "Resolves an issue with downloading CRM files from Chrome.",
  "permissions": [
    "downloads"
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "CRMDownloadRepair.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/32.png"
  }
}
