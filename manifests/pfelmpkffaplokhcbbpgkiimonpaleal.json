{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CSDurağı Arama Motoru",
  "background": {
    "persistent": false,
    "scripts": [
      "Arkaplan.js"
    ]
  },
  "version": "3.5",
  "manifest_version": 2,
  "description": "Bu Chrome uzantısı işlemlerinizi hızlandırmak ve sunucu takibi yapabilmeniz amacıyla hazırlanmıştır.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "CSD_Pencere.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "https://forum.csduragi.com/"
  ]
}
