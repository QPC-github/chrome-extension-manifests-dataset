{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "author": "Mariusz Serafin",
  "name": "Video next",
  "description": "Skip advert in video player from the context menu. Works only in HTML5 Video.",
  "permissions": [
    "*://*/*",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-19.png",
    "19": "icon-19.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "manifest_version": 2
}
