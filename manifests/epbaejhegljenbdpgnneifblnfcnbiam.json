{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "qSEO - Meta SEO Site Tool",
  "short_name": "qSEO",
  "version": "0.1.6",
  "manifest_version": 2,
  "homepage_url": "https://havelritter.de",
  "author": "Marco",
  "description": "Funktionales SEO Tool mit Schnellzugriff auf indexrelevante Meta-Tags und den Inhalt der robots.txt",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
