{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "https://www.twitch.tv/*"
      ],
      "js": [
        "script.user.js"
      ],
      "matches": [
        "https://www.twitch.tv/*"
      ],
