{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": " New VK без рекламы",
  "version": "0.1.0",
  "description": "Снимает рекламу в левом меню, только для новой версии ВКонтакте",
  "author": "nFox Inc",
  "short_name": "New VK без рекламы",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "New VK без рекламы"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://new.vk.com/*"
      ]
    }
  ]
}
