{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "StandaloneMusic",
  "version": "0.3.1",
  "manifest_version": 2,
  "description": "Removes the Google toolbar from Google Music",
  "homepage_url": "https://github.com/blainemotsinger/StandaloneMusic",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-2.0.3.min.js",
        "js/background.js"
      ],
      "matches": [
        "https://play.google.com/music/*"
      ]
    }
  ]
}
