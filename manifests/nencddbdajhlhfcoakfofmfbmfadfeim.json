{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Memrise Dark Theme",
  "version": "1.4",
  "description": "A night theme for the memrise website.",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.memrise.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon_1.png"
  }
}
