{
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Urban Abodes Craigslist Posting"
  },
  "content_scripts": [
    {
      "js": [
        "js/jqueryui-min.js",
        "js/blockui.js",
        "lib/dataplace.js"
      ],
      "matches": [
        "*://*.urbanabodes.com/agent/generate/*",
        "*://*.urbanabodes.com/app_dev.php/agent/generate/*",
        "*://*.post.craigslist.org/*",
        "*://*.urbanabodeschicago.isrv.tech/agent/generate/*",
        "*://*.urbanabodeschicago.isrv.tech/app_dev.php/agent/generate/*",
        "*://*.ua-dev.xoho.tools/app_dev.php/agent/generate/*"
      ]
    }
  ],
  "description": "Extension that allows for an effortless and rapid method to post apartments to Craigslist.",
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Urban Abodes Craigslist Posting",
  "permissions": [
    "tabs",
    "webRequest",
    "storage"
  ],
  "short_name": "Urban Abodes Craigslist Posting",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.5.6"
}
