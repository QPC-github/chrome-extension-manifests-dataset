{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "base.js"
      ],
      "matches": [
        "https://www.lectio.dk/lectio/*",
        "http://www.lectio.dk/lectio/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "login.js"
      ],
      "matches": [
        "https://www.lectio.dk/lectio/*/login.aspx*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "bonus.js"
      ],
      "matches": [
        "https://www.lectio.dk/lectio/*/SkemaNy.aspx*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Automatisk login p√• Lectio!",
  "icons": {
    "128": "icon/128.png",
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png"
  },
  "name": "Lectio Auto-Login",
  "version": "2.4",
  "manifest_version": 2
}
