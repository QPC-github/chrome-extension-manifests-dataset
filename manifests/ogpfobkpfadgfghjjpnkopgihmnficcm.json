{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stock-up",
  "description": "An extension to open multiple tabs for stock querying.",
  "version": "1.0",
  "permissions": [
    "http://*/",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}
