{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Paulinator",
  "short_name": "Pauline Hanson eraser",
  "version": "1.0",
  "permissions": [
    "http://*/*"
  ],
  "description": "Keep your blood pressure down by replacing Pauline Hanson's head with a kitten",
  "icons": {
    "128": "icons/Icon-128.png",
    "64": "icons/Icon-64.png"
  }
}
