{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Handy by Flora",
  "short_name": "Handy",
  "description": "Add Videos to Handy Instantly",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://www.handybyflora.com/",
    "http://www.handybyflora.com/"
  ],
  "icons": {
    "16": "extensionIcon_16x16.png",
    "48": "extensionIcon_48x48.png",
    "128": "extensionIcon_128x128.png"
  },
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Add To Handy",
    "default_icon": {
      "19": "extensionIcon_16x16.png",
      "38": "extensionIcon_48x48.png"
    }
  },
  "web_accessible_resources": [
    "extension.js",
    "extension.js.map"
  ],
  "manifest_version": 2
}
