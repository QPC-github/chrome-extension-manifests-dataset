{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DestinyMapper",
  "description": "A responsive layout chrome extension for Desinty",
  "version": "1.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "short_name": "DM",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon38.png"
    }
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "storage",
    "*://*.bungie.net/*",
    "cookies"
  ]
}
