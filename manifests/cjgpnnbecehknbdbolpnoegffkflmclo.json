{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "name": "Rose Gold Wallpapers FullHD New Tab",
  "description": "Get  HD Wallpapers of Rose Gold for new tab",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/scripts/third-party-libs/JQuery.min.js",
      "/scripts/background/patching/array.js",
      "/scripts/background/configure/search-engines.js",
      "/scripts/background/configure/setters.js",
      "/scripts/background/help/prefs-sys.js",
      "/scripts/background/tutorial/checkTour.js",
      "/scripts/background/help/utils.js",
      "/scripts/background/handlers/common.js",
      "/scripts/background/handlers/setup.js",
      "/scripts/background/handlers/analytics.js",
      "/scripts/background/configure/nevada.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/assets/icons/common/128.png"
    }
  },
  "icons": {
    "128": "/assets/icons/common/128.png"
  },
  "permissions": [
    "*://mail.google.com/*",
    "activeTab",
    "storage",
    "cookies",
    "management",
    "topSites",
    "webNavigation",
    "*://www.google.com/*",
    "webRequestBlocking",
    "webRequest"
  ],
  "web_accessible_resources": [
    "*"
  ],
  "manifest_version": 2
}
