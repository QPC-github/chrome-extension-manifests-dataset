{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FilmAffinity Rank Extension",
  "version": "1.0",
  "description": "FilmAffinity Rank Extension",
  "icons": {
    "16": "ico16.png",
    "48": "ico48.png",
    "128": "ico128.png"
  },
  "browser_action": {
    "default_icon": "ico16.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "https://www.filmaffinity.com/es/topgen*"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ]
}
