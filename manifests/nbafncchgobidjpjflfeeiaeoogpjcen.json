{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Color Mail",
  "description": "An easy tool to help you send colour styled emails to your friends for occasions.",
  "version": "1",
  "manifest_version": 2,
  "app": {
    "urls": [
      "http://www.touchapp.co.uk/iphone/colormail/"
    ],
    "launch": {
      "web_url": "http://www.touchapp.co.uk/iphone/colormail/"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
