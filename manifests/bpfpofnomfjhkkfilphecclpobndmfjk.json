{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RapGeniusSamples",
  "description": "Effortlessly view what samples a song contains.",
  "version": "1.2",
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/*.gif"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://rap.genius.com/*",
        "https://rap.genius.com/*",
        "https://genius.com/*",
        "http://genius.com/*"
      ],
      "js": [
        "jquery.min.js",
        "myscript.js"
      ]
    }
  ],
  "icons": {
    "128": "images/logo.png"
  }
}
