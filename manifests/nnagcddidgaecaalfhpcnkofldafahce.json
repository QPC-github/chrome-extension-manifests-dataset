{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "OgilvyOne - Chicago",
  "background": {
    "scripts": [
      "js/lib/google-analytics-bundle.min.js",
      "js/analytics.min.js",
      "js/jquery.min.js",
      "js/background.min.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "img/zmf-16x16.png",
      "19": "img/zmf-19x19.png",
      "38": "img/zmf-38x38.png",
      "48": "img/zmf-48x48.png",
      "128": "img/zmf-128x128.png"
    },
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/app.min.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Remember the victims and give the killers Zero Minutes of Fame.",
  "icons": {
    "16": "img/zmf-16x16.png",
    "19": "img/zmf-19x19.png",
    "38": "img/zmf-38x38.png",
    "48": "img/zmf-48x48.png",
    "128": "img/zmf-128x128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "name": "Zero Minutes of Fame",
  "short_name": "Zero Minutes",
  "permissions": [
    "alarms",
    "https://www.google-analytics.com/",
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "version": "1.2.1",
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": true
  }
}
