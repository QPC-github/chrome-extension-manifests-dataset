{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Promoted Tweets",
  "version": "1.0.0.1",
  "manifest_version": 2,
  "default_locale": "ja",
  "description": "__MSG_extDescription__",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
