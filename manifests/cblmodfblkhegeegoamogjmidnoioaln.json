{
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/game.png"
  },
  "icons": {
    "128": "icons/128.png",
    "24": "icons/game.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "https://play.gamepix.com/*",
    "https://topaddon.com/*",
    "https://search.yahoo.com/*",
    "https://at.search.yahoo.com/*",
    "https://be.search.yahoo.com/*",
    "https://br.search.yahoo.com/*",
    "https://ca.search.yahoo.com/*",
    "https://ch.search.yahoo.com/*",
    "https://dk.search.yahoo.com/*",
    "https://es.search.yahoo.com/*",
    "https://us.search.yahoo.com/*",
    "https://uk.search.yahoo.com/*",
    "https://de.search.yahoo.com/*",
    "https://fr.search.yahoo.com/*",
    "https://it.search.yahoo.com/*",
    "https://in.search.yahoo.com/*",
    "https://no.search.yahoo.com/*",
    "https://nl.search.yahoo.com/*",
    "https://se.search.yahoo.com/*",
    "https://fi.search.yahoo.com/*",
    "https://www.bing.com/*",
    "https://www.google.com/*",
    "https://www.google.at/*",
    "https://www.google.fr/*",
    "https://www.google.be/*",
    "https://www.google.it/*",
    "https://www.google.dk/*",
    "https://www.google.co.in/*",
    "https://www.google.co.uk/*",
    "https://www.google.pt/*",
    "https://www.google.ca/*",
    "https://www.google.nl/*",
    "https://www.google.no/*",
    "https://www.google.es/*",
    "https://www.google.se/*",
    "https://www.google.de/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "persistent": true,
    "scripts": [
      "js/http.js",
      "js/background.js"
    ]
  }
}
