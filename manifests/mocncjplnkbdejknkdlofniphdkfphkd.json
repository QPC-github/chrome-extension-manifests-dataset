{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GCM Sender",
  "version": "1.1",
  "description": "Send messages using Google Cloud Messaging.",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://android.googleapis.com/gcm/send",
    "storage"
  ]
}
