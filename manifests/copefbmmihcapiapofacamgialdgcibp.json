{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BBL Redirector",
  "description": "Redirect from public to internal",
  "version": "1.4",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://kelsey.app.babelhealth.com/*",
    "*://kelseyinternal.app.babelhealth.com/*",
    "*://hpp.app.babelhealth.com/*",
    "*://hppinternal.app.babelhealth.com/*",
    "*://hppstg.app.babelhealth.com/*",
    "*://hppstginternal.app.babelhealth.com/*",
    "*://hghmrk.app.babelhealth.com/*",
    "*://hghmrkinternal.app.babelhealth.com/*",
    "*://hghsbx.app.babelhealth.com/*",
    "*://hghsbxinternal.app.babelhealth.com/*",
    "*://evhclarion.app.babelhealth.com/*",
    "*://evhclarioninternal.app.babelhealth.com/*",
    "*://evhmorecare.app.babelhealth.com/*",
    "*://evhmorecareinternal.app.babelhealth.com/*",
    "webRequestBlocking"
  ]
}
