{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Anti-track",
  "version": "0.0.1",
  "description": "Anti-track removes cookies",
  "options_page": "html/options.html",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "js/main.js",
      "js/parseUri.js",
      "js/background.js"
    ]
  },
  "icons": {
    "48": "images/bigicon.png"
  },
  "browser_action": {
    "default_title": "Anti-track",
    "default_icon": "images/icon.png"
  },
  "manifest_version": 2
}
