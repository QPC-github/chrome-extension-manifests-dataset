{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "나무위키 도우미 (Namuwiki Helper)",
  "description": "Improve readability of Namuwiki web pages.",
  "version": "2.7",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://namu.wiki/*",
    "tabs",
    "activeTab",
    "webNavigation"
  ]
}
