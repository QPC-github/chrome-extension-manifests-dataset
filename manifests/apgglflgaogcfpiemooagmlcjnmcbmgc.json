{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FastWeather",
  "description": "Pogoda dostosowana do Ciebie! Tylko jedno kliknięcie, szybka personalizacja. Pobierz i spróbuj!",
  "version": "1.1.4",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com  https://maps.googleapis.com;  object-src 'self'",
  "browser_action": {
    "default_popup": "wtka.html",
    "default_icon": "obraz.png",
    "default_title": "Sprawdź pogode!"
  },
  "permissions": [
    "storage",
    "geolocation"
  ]
}
