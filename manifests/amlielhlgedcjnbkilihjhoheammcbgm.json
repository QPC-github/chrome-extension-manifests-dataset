{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BestFreeMaps",
  "version": "2.4",
  "description": "__MSG_desc__",
  "default_locale": "en",
  "homepage_url": "https://www.bestfreemaps.com/",
  "author": "Homepage",
  "browser_action": {
    "default_icon": "images/icons/icon48.png",
    "default_title": "BestFreeMaps"
  },
  "icons": {
    "16": "images/icons/icon16.png",
    "32": "images/icons/icon32.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [],
  "permissions": [
    "topSites"
  ]
}
