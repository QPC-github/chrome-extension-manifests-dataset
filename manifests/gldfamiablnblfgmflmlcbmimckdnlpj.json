{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "4p's URL Expander",
  "description": "Right click a shortened link and expand it",
  "version": "0.0.1",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "popup.js"
    ]
  },
  "manifest_version": 2
}
