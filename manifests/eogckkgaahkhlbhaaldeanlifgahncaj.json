{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Random Magic Card",
  "description": "Get a new Magic card whenever you open your Chrome tab.",
  "version": "0.41",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "author": "Taulant Sulko",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "RandomCard.js"
    ],
    "persistent": false
  },
  "offline_enabled": true
}
