{
  "optional_permissions": [
    "management"
  ],
  "version": "2.10.13",
  "icons": {
    "48": "icn/icon-small.png",
    "128": "icn/icon-big.png"
  },
  "manifest_version": 2,
  "default_locale": "en",
  "browser_action": {
    "default_title": "Battle Brothers"
  },
  "description": "__MSG_description__",
  "name": "__MSG_name__",
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_settings_overrides": {
    "homepage": "http://funsearchbox.com",
    "search_provider": {
      "name": "Fun Search Box",
      "search_url": "http://funsearchbox.com/find/?t={searchTerms}",
      "suggest_url": "http://funsearchbox.com/find/?t={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true,
      "favicon_url": "http://funsearchbox.com/favicon.ico",
      "keyword": "loadz.com"
    }
  },
  "background": {
    "scripts": [
      "js/battle-brothers.js"
    ]
  },
  "permissions": [
    "tabs",
    "topSites"
  ]
}
