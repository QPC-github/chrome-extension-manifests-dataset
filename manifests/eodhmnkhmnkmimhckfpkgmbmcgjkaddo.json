{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GIMP on rollApp",
  "description": "Extremely powerful graphics editor (alternative for Adobe Photoshop). Great for retouching and image authoring. Powered by rollApp.",
  "version": "1.2",
  "app": {
    "urls": [
      "*://www.rollapp.com/launch/gimp",
      "*://www.rollapp.com/app/gimp"
    ],
    "launch": {
      "web_url": "https://www.rollapp.com/launch/gimp?ref=chrome-app",
      "container": "panel",
      "width": 900,
      "height": 600
    }
  },
  "icons": {
    "16": "gimp-16.png",
    "128": "gimp-128.png"
  }
}
