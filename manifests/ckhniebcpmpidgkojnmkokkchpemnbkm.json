{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Chris Kirk",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Replaces the text \"Alphabet\" with \"Google.\" Brought to you by Slate.com.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Alphabet to Google",
  "short_name": "Alpha2Google",
  "version": "1.1"
}
