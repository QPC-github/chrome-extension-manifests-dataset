{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "Ahref Cookie Login",
  "description": "One click login to ahref using cookie in clipboard",
  "homepage_url": "http://ngocamha.com",
  "author": "Ngô Hà",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Copy code to clipboard and click to login"
  },
  "icons": {
    "128": "images/icon.png",
    "16": "images/icon.png",
    "48": "images/icon.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "*://ahrefs.com/*",
    "clipboardWrite",
    "clipboardRead",
    "notifications",
    "declarativeContent",
    "activeTab"
  ],
  "version": "0.0.0.69"
}
