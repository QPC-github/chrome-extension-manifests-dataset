{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouLimit",
  "version": "0.0.1",
  "description": "Limits your YouTube usage by annoying you after being on YouTube for longer than 10 minutes.",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "css": [
        "css/overlay.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/icon48.png"
  }
}
