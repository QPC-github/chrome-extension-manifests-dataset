{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Benjamin Moore Color Home",
  "short_name": "Color Home",
  "version": "1.0.8",
  "chrome_url_overrides": {
    "newtab": "BMChromeExtension.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "permissions": [
    "geolocation",
    "storage"
  ]
}
