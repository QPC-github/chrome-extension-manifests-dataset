{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Non-political goat",
  "description": "Tired of politics yet? The non-political super goat can prevent you viewing very politicized pages",
  "version": "1.0",
  "icons": {
    "16": "logo-16x16.PNG",
    "32": "logo-32x32.PNG",
    "128": "logo-128x128.PNG"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "audio",
    "http://*/",
    "https://*/"
  ]
}
