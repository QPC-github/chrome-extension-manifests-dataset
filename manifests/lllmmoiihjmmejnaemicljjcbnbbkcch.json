{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reports for Trello",
  "description": "Track your Trello usage.",
  "version": "0.0.7",
  "app": {
    "urls": [
      "http://*.reportsfortrello.com/"
