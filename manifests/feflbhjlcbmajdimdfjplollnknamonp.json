{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.1.03",
  "name": "DAN Data Labs",
  "short_name": "DDL CX",
  "description": "",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Get personalized recommendations on budget performances of your adsets in Facebookâ€™s console itself"
  },
  "author": "kedarBramhe",
  "icons": {
    "128": "dan_icon_128.jpg"
  },
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus",
    "cookies",
    "http://business.facebook.com/*",
    "https://business.facebook.com/*",
    "http://www.facebook.com/*",
    "https://www.facebook.com/*",
    "http://dandatalab.com/*",
    "https://dandatalab.com/*"
  ],
  "background": {
    "scripts": [
      "jquery-3.4.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://business.facebook.com/adsmanager*",
        "https://business.facebook.com/adsmanager/manage/adsets?*",
        "https://business.facebook.com/adsmanager/manage/campaigns?*"
      ],
      "css": [
        "bootstrap.min.css",
        "font-awesome.min.css",
        "sweetalert.css"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js",
        "bootstrap.min.js",
        "numeral.min.js",
        "sweetalert.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "FontAwesome.otf",
    "fontawesome-webfont.eot",
    "fontawesome-webfont.svg",
    "fontawesome-webfont.ttf",
    "fontawesome-webfont.woff",
    "fontawesome-webfont.woff2"
  ]
}
