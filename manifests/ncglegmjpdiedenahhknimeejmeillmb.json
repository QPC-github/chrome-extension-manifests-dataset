{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "name": "Chat Darkorbit",
  "description": "Chat for us",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Chat Darkorbit"
  },
  "background": {
    "scripts": [
      "./chat/socket.js",
      "./bg.js"
    ]
  },
  "oauth2": {
    "client_id": "931357964864-98rgagbaic9jqrdjtavl4bisopjfvc4i.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login"
    ]
  },
  "icons": {
    "128": "128.png",
    "16": "128.png",
    "32": "128.png",
    "48": "128.png"
  },
  "offline_enabled": false,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "unlimitedStorage",
    "storage",
    "tabs",
    "notifications",
    "identity",
    "background"
  ]
}
