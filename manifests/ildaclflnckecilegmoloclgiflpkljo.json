{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Election Rigger",
  "description": "Replaces instances of 'election' with 'rigged election' in articles, blogs, op-eds, and more.",
  "version": "1.1",
  "icons": {
    "128": "dtrump_3.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
