{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Send URL To Event",
  "description": "Send current URL to EventGhost",
  "version": "1.1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "rightclick.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
