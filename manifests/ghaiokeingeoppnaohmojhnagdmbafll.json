{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Merchant Assistant",
  "short_name": "MAI-MA",
  "manifest_version": 2,
  "version": "2020.01.17",
  "description": "Product Performance Data on your Marketplace Page!",
  "icons": {
    "16": "MAI-16.png",
    "48": "MAI-48.png",
    "128": "MAI-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "web_accessible_resources": [
    "*.html"
  ]
}
