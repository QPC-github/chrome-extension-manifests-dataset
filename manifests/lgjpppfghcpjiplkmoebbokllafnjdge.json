{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Link Highlighter - H&M Only",
  "version": "1.0.9",
  "manifest_version": 2,
  "description": "Highlights any link on icon click or Alt+H",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "commands": {
    "execute": {
      "suggested_key": {
        "default": "Alt+H"
      },
      "description": "Highlight Links"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
