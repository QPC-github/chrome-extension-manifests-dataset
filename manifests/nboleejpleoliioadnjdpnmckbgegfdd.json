{
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Shows your internet ip address and give you general details about an IP address.",
  "icons": {
    "128": "ico128.png",
    "16": "ico16.png",
    "48": "ico48.png"
  },
  "manifest_version": 2,
  "name": "IP-TRACKER LOOKUP",
  "permissions": [
    "contextMenus"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "web_accessible_resources": [
    "icon_19.png",
    "icon_48.png",
    "icon_128.png"
  ]
}
