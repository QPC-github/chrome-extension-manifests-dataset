{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Etsy Search Analytics Saver",
  "short_name": "ESA Saver",
  "version": "1.5",
  "description": "Tool for Etsy Search Analytics. Export search queries and word statistics to files for Excel and word processors.",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.etsy.com/your/shops/me/search-analytics*",
        "file:///Z:/Etsy*",
        "file:///D:/*"
      ],
      "js": [
        "jquery-1.12.4.js",
        "background.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "/injected.js",
    "/style.css"
  ]
}
