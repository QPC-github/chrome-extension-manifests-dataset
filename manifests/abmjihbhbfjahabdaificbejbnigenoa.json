{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Untrackd",
  "version": "0.0.15",
  "description": "Prevent sites you visit and others from tracking what you search for",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "webRequest",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "assets/icon48.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
