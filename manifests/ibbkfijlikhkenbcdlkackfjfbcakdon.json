{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Week",
  "description": "Show week number in toolbar.",
  "version": "2.2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "weeks/0.png"
  },
  "background": {
    "scripts": [
      "background.min.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
