{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Color:Oclock",
  "description": "Displays time and the corresponding hex as background with food and weather suggestions.",
  "version": "1.7",
  "chrome_url_overrides": {
    "newtab": "background.html"
  },
  "permissions": [
    "<all_urls>",
    "chrome://favicon/",
    "management",
    "geolocation",
    "storage"
  ]
}
