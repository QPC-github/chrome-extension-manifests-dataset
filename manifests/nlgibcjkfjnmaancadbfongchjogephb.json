{
  "background": {
    "page": "tweetjukebox.html"
  },
  "content_scripts": [
    {
      "js": [
        "tweetJukeboxjq.js",
        "tweetJukeboxHover.js",
        "tweetHover.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Browser Media Post.",
  "icons": {
    "128": "tjbIcon_128.png",
    "16": "tjbIcon_16.png",
    "32": "tjbIcon_32.png",
    "64": "tjbIcon_64.png"
  },
  "manifest_version": 2,
  "name": "Social Jukebox Loader",
  "page_action": {
    "default_icon": "tjbIcon_32.png"
  },
  "permissions": [
    "https://www.socialjukebox.com/",
    "contextMenus",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "8"
}
