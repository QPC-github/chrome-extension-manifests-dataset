{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MDJ Tools pour LGeL",
  "version": "3.1.5",
  "content_scripts": [
    {
      "matches": [
        "https://www.loups-garous-en-ligne.com/jeu/*"
      ],
      "js": [
        "brdrf.js",
        "jquery-3.3.1.min.js"
      ]
    }
  ],
  "icons": {
    "48": "icons/DE48.png",
    "96": "icons/DE96.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/DE96.png",
      "32": "icons/DE96.png"
    },
    "default_title": "MDJ Tools",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ]
}
