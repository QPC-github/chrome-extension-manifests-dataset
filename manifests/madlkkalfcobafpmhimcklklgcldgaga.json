{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ShortCuts",
  "description": "An extension that allows you to create a shortcut to several URLs.",
  "version": "2.1.0",
  "browser_action": {
    "default_icon": "16.png",
    "default_popup": "chromecut.html"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "background": {
    "run_at": "document_start",
    "scripts": [
      "background.js"
    ]
  }
}
