{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "ZebraTester dropdown to control recording of browser sessions. May change proxy settings, browser cookies and/or cache.",
  "manifest_version": 2,
  "name": "Apica Script Recorder",
  "version": "1.0.7",
  "homepage_url": "http://www.zebratester.com/",
  "icons": {
    "48": "static_contents/PrxRecExt/PrxRecExt.png",
    "96": "static_contents/PrxRecExt/PrxRecExt@2x.png",
    "128": "static_contents/PrxRecExt/PrxRecExt_128.png"
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs",
    "cookies",
    "browsingData",
    "proxy"
  ],
  "browser_action": {
    "default_icon": {
      "16": "static_contents/PrxRecExt1_black_16/PrxRecExt1_black_16.png",
      "32": "static_contents/PrxRecExt1_black_16/PrxRecExt1_black_16@2x.png",
      "64": "static_contents/PrxRecExt1_black_16/PrxRecExt1_black_16@3x.png"
    },
    "default_title": "Apica Script Recorder",
    "default_popup": "popup/choose_action.html",
    "theme_icons": [
      {
        "light": "static_contents/PrxRecExt1_white_16/PrxRecExt1_white_16.png",
        "dark": "static_contents/PrxRecExt1_black_16/PrxRecExt1_black_16.png",
        "size": 16
      },
      {
        "light": "static_contents/PrxRecExt1_white_16/PrxRecExt1_white_16@2x.png",
        "dark": "static_contents/PrxRecExt1_black_16/PrxRecExt1_black_16@2x.png",
        "size": 32
      },
      {
        "light": "static_contents/PrxRecExt1_white_16/PrxRecExt1_white_16@3x.png",
        "dark": "static_contents/PrxRecExt1_black_16/PrxRecExt1_black_16@3x.png",
        "size": 64
      }
    ]
  },
  "web_accessible_resources": [
    "help/start.html"
  ]
}
