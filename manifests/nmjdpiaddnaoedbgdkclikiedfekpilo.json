{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TrumpIt",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "TrumpIt your Trumps.",
  "homepage_url": "https://gituhub.com/dizzygillespie/trumpet",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/jquery-3.1.0.min.js",
        "src/rules.js",
        "src/inject.js"
      ],
      "css": [
        "css/trumpet.css"
      ]
    }
  ]
}
