{
  "browser_action": {
    "default_icon": "images/logotype.png",
    "default_popup": "index.html",
    "default_title": "Todo list manager"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Lite todo list manager.",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "name": "Todo list manager",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "<all_urls>"
  ],
  "short_name": "Todo list manager",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5"
}
