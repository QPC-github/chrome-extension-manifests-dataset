{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Instant YouTube Video Search",
  "version": "1.0.1",
  "description": "A YouTube extension button for your instant video search.",
  "author": "bitBack",
  "icons": {
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_title": "Instant YouTube Video Search",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.googleapis.com/youtube/v3/search",
    "tabs"
  ]
}
