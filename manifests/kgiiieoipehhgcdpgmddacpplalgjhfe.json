{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cink New Tab",
  "author": "Cink",
  "version": "3.2.2",
  "description": "Cink New Tab 帮助您快捷开启访问，提供常用的热门、搜索、视频、新闻、游戏等网站标签；一键打开浏览器功能管理；自定义添加收藏网址。",
  "icons": {
    "16": "ICON/16.png",
    "48": "ICON/48.png",
    "64": "ICON/96.png",
    "128": "ICON/128.png"
  },
  "browser_action": {
    "default_icon": "ICON/16.png",
    "default_popup": "HTML/Popup.html",
    "default_title": "Cink New Tab"
  },
  "background": {
    "page": "HTML/E/02-BG.html"
  },
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "cookies"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
