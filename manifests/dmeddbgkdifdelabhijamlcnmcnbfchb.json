{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hesab.az by GoldenPay",
  "short_name": "Hesab.az",
  "description": "Hesab.az – Azərbaycanın ən böyük onlayn xidmətlər portalı",
  "version": "1.0.2",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "homepage_url": "http://hesab.az",
  "background": {
    "page": "bg.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "256": "images/256.png",
    "128": "images/128.png",
    "64": "images/64.png",
    "48": "images/48.png",
    "32": "images/32.png",
    "16": "images/16.png"
  },
  "permissions": [
    "*://*.hesab.az/",
    "*://*.goldenpay.az/",
    "webRequest",
    "webRequestBlocking"
  ]
}
