{
  "author": "Novodvorski Studio",
  "background": {
    "scripts": [
      "js/ga.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "html/popup.html",
    "default_title": "__MSG_appButtonDesc__"
  },
  "default_locale": "uk",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "storage",
    "cookies",
    "webNavigation",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.9",
  "web_accessible_resources": [
    "images/*",
    "css/*"
  ]
}
