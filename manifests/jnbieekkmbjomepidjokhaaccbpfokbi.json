{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "USA Breaking News",
  "version": "1.1.9",
  "description": "Find the latest breaking news and information on the top stories, politics, business and more with \"USA Breaking News\".",
  "manifest_version": 2,
  "homepage_url": "http://USAbreaking.news",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_popup": "html/popup.html",
    "default_title": "US Breaking News"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "cookies",
    "history",
    "notifications",
    "webNavigation",
    "storage",
    "unlimitedStorage"
  ]
}
