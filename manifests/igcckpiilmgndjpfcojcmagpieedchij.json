{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "dアニメ 新規タブツール",
  "description": "動画再生時に同じウィンドウ内で新規タブ再生、さらに複数視聴を可能にします。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "version": "1.3.2",
  "content_scripts": [
    {
      "js": [
        "script/content-script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
