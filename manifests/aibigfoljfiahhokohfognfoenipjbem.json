{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Protogy Assistant",
  "version": "2.4.3",
  "manifest_version": 2,
  "description": "Allows you to capture content as you navigate on the Internet.\nProtogy subscription is required",
  "icons": {
    "16": "./img/asiss16x16.png",
    "48": "./img/asiss48x48.png",
    "128": "./img/asiss128x128.png"
  },
  "web_accessible_resources": [
    "./img/asiss16x16.png",
    "./img/asiss38x38.png",
    "./img/asiss48x48.png"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/Base64.js",
      "js/aes.js",
      "js/AesUtil.js",
      "js/pbkdf2.js",
      "js/jquery-3.1.2.min.js",
      "js/simplePost.js",
      "js/utils.js",
      "js/Collections.js",
      "js/Arrays.js",
      "js/XmlHttpPostRequest.js",
      "js/json2.js",
      "js/date.js",
      "js/time.js",
      "js/TreeNode.js",
      "js/jsuri-1.1.1.min.js",
      "Notifs.js",
      "Ticker.js",
      "MessageType.js",
      "MessageBuffer.js",
      "EventLog.js",
      "ContextMenuManager.js",
      "IconManager.js",
      "ServerQueue.js",
      "transport.js",
      "ServerCheck.js",
      "CrgMgr.js",
      "UsrDefCtxMgr.js",
      "DataView.js",
      "Crawler.js",
      "CrgHandler.js",
      "RdfLinkHandler.js",
      "BGProgressProxy.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/asiss19x19.png",
    "default_name": "Protogy Assistant",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "notifications",
    "contextMenus",
    "tabs",
    "tts",
    "storage",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://www.protogize.com; object-src 'self'"
}
