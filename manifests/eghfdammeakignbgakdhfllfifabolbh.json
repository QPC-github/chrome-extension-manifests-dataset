{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_application_title__",
  "version": "1.0.7",
  "default_locale": "en_US",
  "manifest_version": 2,
  "description": "__MSG_application_description__",
  "container": [
    "GOOGLE_DRIVE"
  ],
  "offline_enabled": true,
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "image/png",
          "application/vnd.google.drive.ext-type.png"
        ],
        "href": "https://math-equation.freebusinessapps.net/tex-editor",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "512": "icon-512.png",
    "256": "icon-256.png",
    "128": "icon-128.png",
    "64": "icon-64.png",
    "48": "icon-48.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "browser_action": {
    "default_icon": {
      "512": "icon-512.png",
      "256": "icon-256.png",
      "128": "icon-128.png",
      "64": "icon-64.png",
      "48": "icon-48.png",
      "32": "icon-32.png",
      "16": "icon-16.png"
    }
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "https://*.freebusinessapps.net/*",
    "notifications"
  ],
  "background": {
    "scripts": [
      "/notify/update-conf.js",
      "/notify/update.min.js",
      "/notify/background.js"
    ]
  }
}
