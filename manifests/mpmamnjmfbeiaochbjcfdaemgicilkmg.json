{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DW",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_title": "ЛЯТО!",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "cookies",
    "*://*.svadba.com/*"
  ],
  "icons": {
    "48": "48.png",
    "128": "128.png"
  }
}
