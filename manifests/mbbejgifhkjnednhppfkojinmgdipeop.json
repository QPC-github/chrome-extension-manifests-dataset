{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "New Tab"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Custom background wallpaper, clock, and more on every new tab.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Speed Dial New Tab Page for Chrome",
  "offline_enabled": true,
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "version": "1.0.3"
}
