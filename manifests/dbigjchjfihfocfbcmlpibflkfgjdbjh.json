{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Brix Identifier",
  "description": "Lists all brix on a page.",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icons/action19x19.png",
      "38": "icons/action38x38.png"
    }
  },
  "web_accessible_resources": [
    "content.css"
  ]
}
