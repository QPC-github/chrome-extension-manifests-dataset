{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "#TrollK Vote",
  "version": "0.3",
  "description": "Publish your #TrollK votes from Twitter.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "trollk-vote.js"
  ]
}
