{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dimples Fingerprint Scanner Plug-in",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Provides the ability to take thumbprints from Chrome browser.",
  "browser_action": {
    "default_icon": "icon-128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.dimplescharms.com/*",
      "*://localhost/*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ]
}
