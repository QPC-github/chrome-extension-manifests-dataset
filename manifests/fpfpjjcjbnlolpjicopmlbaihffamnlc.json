{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GEODI",
  "description": "",
  "version": "1.5",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "geodimenu.js",
      "geodisearch.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "geodicontent.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "geodi48.png",
    "default_popup": "geodipopup.html"
  },
  "icons": {
    "16": "geodi16.png",
    "48": "geodi48.png",
    "128": "geodi128.png"
  },
  "manifest_version": 2,
  "default_locale": "en"
}
