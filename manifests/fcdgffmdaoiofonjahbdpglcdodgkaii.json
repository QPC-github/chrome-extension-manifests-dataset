{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LWL Chrome Monitor",
  "version": "1.2.4",
  "manifest_version": 2,
  "description": "Liquidware Labs Chrome Monitor",
  "background": {
    "scripts": [
      "lwlchrome.js"
    ]
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging",
    "processes",
    "management",
    "idle",
    "tabs"
  ]
}
