{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Punch Box Stats - Events",
  "manifest_version": 2,
  "version": "1.2",
  "description": "Keep up with the latest events from punchboxstats.net",
  "icons": {
    "16": "images/icon.png"
  },
  "homepage_url": "https://www.punchboxstats.net",
  "permissions": [
    "https://punchboxstats.net/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Punch Box Stats Voting Platform",
    "default_popup": "feed.html"
  },
  "web_accessible_resources": [
    "images/image1.png",
    "/js/*"
  ]
}
