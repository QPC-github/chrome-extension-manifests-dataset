{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Custom QuickSearch",
  "description": "allow you to customize 'QuickSearch' context selection menu.",
  "version": "1.50",
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/jquery-2.1.4.min.js",
      "js/firebase.js",
      "js/storage.js",
      "js/js.js"
    ]
  },
  "options_page": "pages/options.html",
  "manifest_version": 2
}
