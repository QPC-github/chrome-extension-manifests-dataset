{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stackoverflow Search",
  "description": "Stackoverflow solutions in google search results",
  "version": "1.01",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.js",
      "scripts.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {
    "512": "sof512.png"
  }
}
