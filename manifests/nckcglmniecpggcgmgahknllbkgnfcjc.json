{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Preciconnect Screen Sharing V-Link",
  "author": "Codixis, Inc.",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for https://v-link.preciconnect.net/. ",
  "homepage_url": "https://v-link.preciconnect.net/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://v-link.preciconnect.net/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
