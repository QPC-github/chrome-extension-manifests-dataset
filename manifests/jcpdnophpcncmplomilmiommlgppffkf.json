{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "W3 Timer",
  "manifest_version": 2,
  "description": "Plugin respons√°vel para controle de hora",
  "version": "1.0",
  "permissions": [
    "unlimited_storage",
    "notifications",
    "contextMenus",
    "cookies",
    "storage",
    "idle",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "pages/login.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com.br/*"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "js/chrome.js",
        "js/login.js"
      ],
      "run_at": "document_end",
      "persistent": false
    }
  ]
}
