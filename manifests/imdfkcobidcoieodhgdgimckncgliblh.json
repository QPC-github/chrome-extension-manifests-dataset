{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "24": "logo.png"
    }
  },
  "description": "New Tab Extension",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "name": "Time to Fest",
  "permissions": [
    "storage",
    "tabs"
  ],
  "version": "1.0.0"
}
