{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Joywok Collaboration Kit",
  "version": "1.1.5",
  "manifest_version": 2,
  "description": "This kit is for Screen Capture, Screen Sharing, Interactive WhiteBoard and so on.",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "64": "icons/icon_64.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "backround-script.js"
    ]
  }
}
