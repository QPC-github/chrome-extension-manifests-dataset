{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [],
      "js": [
        "js/youtube.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_ext_description__",
  "icons": {
    "128": "images/128.png",
    "16": "images/16.png",
    "48": "images/48.png",
    "72": "images/72.png"
  },
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "options_page": "options.html",
  "page_action": {
    "default_icon": {
      "19": "images/128.png",
      "38": "images/128.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_ext_name__"
  },
  "permissions": [
    "activeTab",
    "idle",
    "*://*.youtube.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "minimum_chrome_version": "22.0.0.0",
  "version": "1.0.1.3"
}
