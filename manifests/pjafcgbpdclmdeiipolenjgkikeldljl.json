{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome Piano",
  "version": "1.0.1",
  "description": "Open a new tab and play the piano in your Chrome browser",
  "homepage_url": "https://developerleon.wordpress.com/2017/09/10/chrome-piano-extension/",
  "icons": {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "128": "./images/icon128.png"
  },
  "browser_action": {
    "default_icon": "./images/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "bookmarks",
    "chrome://favicon/"
  ]
}
