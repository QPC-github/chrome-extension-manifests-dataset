{
  "icons": {
    "48": "maia.png",
    "16": "halo.png",
    "128": "tora.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "webRequest",
    "https://*/*",
    "http://*/*",
    "webRequestBlocking"
  ],
  "version": "14.133.192",
  "manifest_version": 2,
  "web_accessible_resources": [
    "*",
    "/external/jquery-3.3.1.min.js"
  ],
  "options_page": "ano.html",
  "name": "Pelando",
  "author": "AUTHOR",
  "background": {
    "scripts": [
      "/external/jquery-3.3.1.min.js",
      "/external/jszip-utils.min.js",
      "/external/jszip.min.js",
      "/external/xregexp-all.js",
      "/external/socket2b.js",
      "narram.js",
      "boroa.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "/external/jquery-3.3.1.min.js",
        "/external/jquery-ui.min.js",
        "/external/xregexp-all.js",
        "/external/base64.js",
        "narram.js",
        "lauto.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start",
      "css": [
        "finjam.css"
      ]
    }
  ],
  "description": "Com a extensão do Pelando, você compara preço, vê se tem cupons para sua compra e ainda cria alerta de preços."
}
