{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Push.Do",
  "version": "0.82",
  "manifest_version": 2,
  "description": "push any msg to any device.",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Push.do",
    "default_popup": "public/index.html"
  },
  "background": {
    "page": "public/background.html"
  },
  "permissions": [
    "http://push.do:8001/",
    "tabs"
  ]
}
