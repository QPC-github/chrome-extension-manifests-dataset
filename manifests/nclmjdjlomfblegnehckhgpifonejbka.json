{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SMARTWeb",
  "description": "Click here to open SMART application window",
  "version": "1.2",
  "browser_action": {
    "default_icon": "image.png",
    "default_title": "Open SMART Extension"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
