{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GauchoToggles",
  "version": "1.1",
  "author": "Issac Holguin",
  "description": "Adds quick Toggles for Grades, Assignments, and Forums for GauchoSpace Classes",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gauchospace.ucsb.edu/courses/my/"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
