{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EZCast",
  "product": "EZCast",
  "description": "EZCast",
  "version": "2.8.0.30",
  "manifest_version": 2,
  "default_locale": "en",
  "author": "ezcast",
  "app": {
    "background": {
      "scripts": [
        "core/bundle.js",
        "core/predefine.js",
        "core/utils.js",
        "core/udpSocket.js",
        "core/tcpSocket.js",
        "core/tcpServer.js",
        "core/httpServer.js",
        "core/ssdp.js",
        "core/jsonHandler.js",
        "core/ezdisplay.js",
        "core/captureScreen.js",
        "core/projector.js",
        "core/remoteControl.js",
        "core/mediaPlaylist.js",
        "core/mediaStreaming.js",
        "core/themeControl.js",
        "core/uuid.js",
        "core/background.js"
      ]
    }
  },
  "sockets": {
    "tcp": {
      "connect": "*:*"
    },
    "udp": {
      "bind": "*",
      "send": "*",
      "joinGroup": "*",
      "multicastMembership": ""
    },
    "tcpServer": {
      "listen": ""
    }
  },
  "icons": {
    "48": "images/EZCast48.png"
  },
  "commands": {
    "toggle_test_mode": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Toggle Test Mode"
    }
  },
  "permissions": [
    "<all_urls>",
    "notifications",
    "clipboardRead",
    "clipboardWrite",
    "browser",
    "desktopCapture",
    "contextMenus",
    "system.network",
    "system.display",
    "geolocation",
    "webview",
    "storage",
    "identity",
    "fileSystem"
  ],
  "oauth2": {
    "client_id": "757399855682-pp7ps1o3lqo3ggekuke95amfi8v47d09.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "openid"
    ]
  },
  "key": ""
}
