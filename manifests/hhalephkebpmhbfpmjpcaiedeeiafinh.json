{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "manifest_version": 2,
  "name": "Musicracy, let's join them!",
  "description": "This extension allows to connect to Musicracy Towns and send music from YouTube.com, dedicated to them who dont have smartphones :(",
  "homepage_url": "http://musicracy.co",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*"
  ],
  "browser_action": {
    "default_title": "Extension Name",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
