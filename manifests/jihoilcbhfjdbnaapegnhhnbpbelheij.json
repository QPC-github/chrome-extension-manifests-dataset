{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Salesforce Search Context Menu",
  "description": "Allows searching for selected text on Salesforce",
  "version": "0.3",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "salesforcesearch.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
