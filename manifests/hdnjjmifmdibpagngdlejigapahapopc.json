{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VidJS",
  "description": "Odkrywaj ciekawszy Internet",
  "version": "0.40",
  "icons": {
    "16": "ico/icon.png",
    "48": "ico/icon.png",
    "128": "ico/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/jquery.js",
        "js/main.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/event.js"
    ],
    "persistent": false
  },
  "permissions": [],
  "browser_action": {}
}
