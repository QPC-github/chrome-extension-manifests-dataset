{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "enotes-pass",
  "version": "0.0.1",
  "description": "Allows a user to view locked eNotes content.",
  "icons": {
    "256": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "injector.js"
      ],
      "matches": [
        "*://*.enotes.com/*",
        "*://*.freebooksummary.com/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
