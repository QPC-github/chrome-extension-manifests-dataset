{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sinônimos para Bolsonaro",
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "description": "Muda o texto 'Bolsonaro' para coisas do tipo 'Bolosonaro' ou 'o dêmonio em pessoa'",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
