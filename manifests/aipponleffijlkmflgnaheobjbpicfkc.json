{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Соционика VK",
  "short_name": "sociovk",
  "description": "Отображение социотипа на сайте vk.com",
  "version": "3.8",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "64": "64.png",
    "124": "124.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*",
        "*://new.vk.com/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "/jquery.js",
    "/sociovk.js",
    "sociovk.herokuapp.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "SocioVK",
    "default_icon": "48.png",
    "default_popup": "popup.html"
  }
}
