{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "Quicklead"
  },
  "content_scripts": [
    {
      "js": [
        "files/jquery.min.js",
        "content.js"
      ],
      "matches": [
        "*://*.facebook.com/*",
        "*://*.linkedin.com/*"
      ]
    }
  ],
  "description": "Quick and easy way to capture leads from Facebook or LinkedIn",
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png"
  },
  "manifest_version": 2,
  "name": "Top ProducerÂ® QuickLEAD Clipper",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*.facebook.com/",
    "https://*.facebook.com/",
    "https://*.topproduceronline.com/",
    "http://*.topproduceronline.com/"
  ],
  "version": "0.3"
}
