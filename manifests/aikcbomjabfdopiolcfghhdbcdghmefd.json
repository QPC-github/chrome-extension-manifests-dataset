{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EPOCH: New Age Marketers",
  "description": "EPOCH: New Age Marketers",
  "browser_action": {
    "default_icon": {
      "16": "img/fbicon19.png",
      "48": "img/fbicon38.png",
      "128": "img/fbicon38.png"
    },
    "default_title": "EPOCH: New Age Marketers",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "img/fbicon38.png"
  },
  "permissions": [
    "unlimitedStorage",
    "contextMenus",
    "cookies",
    "tabs",
    "notifications",
    "activeTab",
    "storage",
    "declarativeContent",
    "https://www.epoch-agency.com/extension/*"
  ],
  "web_accessible_resources": [
    "css/style.css"
  ],
  "background": {
    "scripts": [
      "/bootstrap/jquery-3.3.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/*"
      ],
      "js": [
        "/bootstrap/jquery-3.3.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "version": "1.5"
}
