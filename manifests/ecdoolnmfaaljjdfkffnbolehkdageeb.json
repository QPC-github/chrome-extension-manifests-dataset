{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Custom Scroll Bar",
  "description": "Custom Scroll Bar",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "<all_urls>"
  ],
  "version": "1.0"
}
