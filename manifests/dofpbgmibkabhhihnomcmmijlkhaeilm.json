{
  "name": "SafeSearch",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "incognito": "split",
  "description": "Search through filtered search results.",
  "homepage_url": "https://www.safesearch.net/",
  "version": "2.8",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://www.safesearch.net/favicon.ico",
      "is_default": true,
      "keyword": "SafeSearch",
      "name": "SafeSearch",
      "search_url": "https://www.safesearch.net/?q={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications"
  ]
}
