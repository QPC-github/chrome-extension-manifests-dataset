{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QR Code from the URL",
  "short_name": "QR Code URL",
  "description": "Create a QR code from the URL by one click.",
  "version": "1.0.7",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "offline_enabled": true,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
