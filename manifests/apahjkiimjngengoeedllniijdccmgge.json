{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zitaat Delivery extension",
  "description": "Easy integration with Zitaat Delivery Platform",
  "version": "2.0.7",
  "background": {
    "persistent": false,
    "scripts": [
      "config.js",
      "merchant.js",
      "armadaAjax.js",
      "do.js"
    ]
  },
  "web_accessible_resources": [
    "modal.html",
    "modalInline.html",
    "overlayLoadingArmada.html",
    "armada_small_white.png",
    "icon128.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://wla.talabat.com/"
      ],
      "css": [
        "modal.css",
        "talabatInject.css"
      ],
      "js": [
        "extensionMessaging.js",
        "armadaAjax.js",
        "overlayLoadingArmada.js",
        "modal.js",
        "fixArea.js",
        "talabatInject.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.trycarriage.com/restaurants/"
      ],
      "css": [
        "modal.css"
      ],
      "js": [
        "extensionMessaging.js",
        "armadaAjax.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.trycarriage.com/orders/*"
      ],
      "css": [
        "modal.css",
        "carriageOrderInject.css"
      ],
      "js": [
        "extensionMessaging.js",
        "armadaAjax.js",
        "overlayLoadingArmada.js",
        "modal.js",
        "fixArea.js",
        "carriageOrderInject.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://trycarriage.com/orders/*"
      ],
      "css": [
        "modal.css",
        "carriageOrderInject.css"
      ],
      "js": [
        "extensionMessaging.js",
        "armadaAjax.js",
        "overlayLoadingArmada.js",
        "modal.js",
        "fixArea.js",
        "carriageOrderInject.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://admin.zyda.com/*/orders*"
      ],
      "css": [
        "modal.css",
        "zydaInject.css"
      ],
      "js": [
        "extensionMessaging.js",
        "armadaAjax.js",
        "overlayLoadingArmada.js",
        "modal.js",
        "fixArea.js",
        "zydaInject.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://zitaat.com/delivery_api",
    "https://merchant.armadadelivery.com/",
    "activeTab",
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "icon128.png",
    "12": "icon128.png",
    "48": "icon128.png"
  }
}
