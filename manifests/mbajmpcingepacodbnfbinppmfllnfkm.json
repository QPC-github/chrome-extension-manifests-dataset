{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Voyager",
  "description": "This extension allows quick access to everything.",
  "version": "1.0",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "runner.js"
    ]
  },
  "omnibox": {
    "keyword": "!"
  },
  "icons": {
    "32": "xcfm.png",
    "128": "xcfm.png"
  }
}
