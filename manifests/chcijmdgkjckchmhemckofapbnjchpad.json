{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "heedu.info",
  "name": "FlatPhpMyAdmin",
  "manifest_version": 2,
  "version": "0.9",
  "homepage_url": "https://heedu.info/FlatPhpMyAdmin",
  "incognito": "spanning",
  "description": "Extensi√≥n que permite cambiar la apariencia de phpMyAdmin sobreponiendo una hoja de estilos CSS del lado del navegador.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "img/icon_16.PNG",
    "32": "img/icon_32.PNG",
    "48": "img/icon_48.PNG",
    "64": "img/icon_64.PNG",
    "128": "img/icon_128.PNG"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "16": "img/icon_16.PNG",
      "32": "img/icon_32.PNG",
      "48": "img/icon_48.PNG",
      "64": "img/icon_64.PNG",
      "128": "img/icon_128.PNG"
    },
    "default_title": "FlatPHPMyAdmin"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "style.css",
    "FlatPHPMyAdmin.ttf"
  ]
}
