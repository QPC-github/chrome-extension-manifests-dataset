{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Scrollbar",
  "version": "2.0.0.3",
  "version_name": "0.0.0.1",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "/icon/icon_128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "/css/scrollbar.css"
      ],
      "run_at": "document_start"
    }
  ]
}
