{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "daemon.js"
    ]
  },
  "description": "This extension makes sure there will be only one tab open for the specified/configured domain URL.",
  "manifest_version": 2,
  "name": "One Tab Only",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "options_page": "options_page.html",
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "0.0.1"
}
