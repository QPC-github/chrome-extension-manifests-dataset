{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iScreen",
  "short_name": "iScreen",
  "version": "0.19.12.12",
  "version_name": "Beta 0.19.12.12",
  "description": "Personalized your tab experience featuring quotes, images, and lot more to come.",
  "icons": {
    "16": "appAssets/images/iScreen16.png",
    "48": "appAssets/images/iScreen48.png",
    "128": "appAssets/images/iScreen128.png"
  },
  "browser_action": {
    "default_icon": "appAssets/images/iScreen16.png"
  },
  "author": "MeshWorld - iScreen",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "offline_enabled": true,
  "permissions": [
    "storage",
    "unlimitedStorage",
    "topSites",
    "history",
    "bookmarks",
    "https://iscreen.meshworld.in/"
  ]
}
