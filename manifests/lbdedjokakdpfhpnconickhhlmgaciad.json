{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "lib/jquery/jquery-3.3.1.min.js",
      "js/urlconfig.js",
      "js/utils.js",
      "js/bg/yulp-storage.js",
      "js/bg/auth.js",
      "js/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Yulp",
    "default_popup": "view/popup.html",
    "default_icon": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "32": "img/icon-32.png",
      "48": "img/icon-64.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.yelp.com/*",
        "https://www.yelp.es/*"
      ],
      "js": [
        "js/urlconfig.js",
        "lib/jquery/jquery-3.3.1.min.js",
        "js/bg/yulp-storage.js",
        "js/content-script/start.js"
      ]
    },
    {
      "matches": [
        "https://www.yulp.app/*"
      ],
      "js": [
        "lib/jquery/jquery-3.3.1.min.js",
        "js/content-script/yulp-home.js"
      ]
    }
  ],
  "content_security_policy": "default-src 'self' https://www.yulp.app/ https://yulp.app/",
  "description": "Yulp RC1 - Your YelpÂ® users bulk messaging assistant",
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-64.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2,
  "name": "Yulp RC1",
  "permissions": [
    "storage",
    "identity",
    "notifications",
    "https://www.yelp.com/*",
    "https://www.yelp.es/*",
    "https://www.yulp.app/*",
    "https://yulp.app/*"
  ],
  "version": "1.3.6",
  "web_accessible_resources": [
    "img/yulp_logo.png",
    "img/yulp_white.png",
    "mp3/audio1.mp3",
    "mp3/audio2.mp3",
    "mp3/audio3.mp3",
    "img/800px-Sad_Puppy_in_Kitchen.jpg",
    "img/error.png",
    "img/animation/Success.png",
    "img/animation/Canvas.png",
    "img/animation/Acquire.png",
    "view/session-list.html"
  ]
}
