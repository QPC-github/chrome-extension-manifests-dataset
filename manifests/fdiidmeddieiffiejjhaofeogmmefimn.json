{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bernd!",
  "version": "1.0.0",
  "description": "",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "96": "img/icon_96.png",
    "128": "img/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
