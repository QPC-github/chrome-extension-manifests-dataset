{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PZextension",
  "description": "twitch.tv/danpz",
  "version": "2.7",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "image/DanpzLogo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "image/DanpzLogo128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/app.js"
    ]
  }
}
