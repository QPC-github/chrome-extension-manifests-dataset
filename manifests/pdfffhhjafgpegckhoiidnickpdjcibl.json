{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube to Trailforks",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Help you post youtube videos to Trailforks",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ]
}
