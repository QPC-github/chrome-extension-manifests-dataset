{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dopa Block",
  "description": "A chrome extension that prevents facebook notifications from activating your brain's happiness receptors.",
  "version": "1.0",
  "icons": {
    "512": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ],
      "css": [
        "modifier.css"
      ],
      "run_at": "document_start"
    }
  ]
}
