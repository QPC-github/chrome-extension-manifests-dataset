{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "cosplayers archive extension",
  "description": "コスプレイヤーズアーカイブを観やすく！",
  "version": "0.1.4",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.cosp.jp/*"
      ],
      "css": [
        "contents_style.css"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
