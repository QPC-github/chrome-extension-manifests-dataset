{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "jquery.min.js",
      "alerts.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://realitygaming.fr/*",
        "https://realitygaming.fr/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "description": "Vos alertes et conversations en temps r√©el sur votre navigateur.",
  "manifest_version": 2,
  "name": "RealityGaming",
  "permissions": [
    "http://realitygaming.fr/",
    "https://realitygaming.fr/",
    "activeTab",
    "tabs",
    "storage",
    "alarms",
    "notifications"
  ],
  "version": "1.0.0"
}
