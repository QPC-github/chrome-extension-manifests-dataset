{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SenecaAnswer",
  "version": "1.0",
  "description": "Gives the answer to seneca assignments",
  "content_scripts": [
    {
      "matches": [
        "https://app.senecalearning.com/classroom/course/*"
      ],
      "js": [
        "JQuery.js",
        "index.js"
      ],
      "css": [
        "ReplaceCSS.scss"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
