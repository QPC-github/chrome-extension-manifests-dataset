{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SO Chat Easy Input Tools",
  "description": "Adds keyboard shortcuts to the SE Chats",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://chat.stackoverflow.com/*",
        "http://chat.stackexchange.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
