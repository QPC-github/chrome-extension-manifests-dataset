{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "rMilos",
  "version": "1.0",
  "description": "Troque todas as imagens de um site por fotos do Ricardo Milos.",
  "icons": {
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.js",
        "rMilos.js"
      ]
    }
  ]
}
