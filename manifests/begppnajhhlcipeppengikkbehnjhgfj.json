{
  "background": {
    "scripts": [
      "jquery.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "name": "Wibki - Unlock the Web",
  "manifest_version": 2,
  "version": "1.4.2",
  "description": "Wibki provides you with an easy way to save, share and discover your Web. Creating a new browsing experience.",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "https://www.wibki.com/"
  ]
}
