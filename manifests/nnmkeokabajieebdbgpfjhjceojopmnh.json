{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Podsluchane.pl",
  "short_name": "Podsluchane.pl",
  "description": "Najnowsze odcinki podcast√≥w Podsluchane.pl w naszej aplikacji Chrome",
  "version": "1.0.3",
  "permissions": [
    "storage",
    "notifications",
    "alarms",
    "https://www.podsluchane.pl/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "icons/*.png",
    "images/*.png"
  ]
}
