{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Giver",
  "description": "Give Items!",
  "version": "0.1.2",
  "browser_action": {
    "default_icon": "16X16.png",
    "default_popup": "index.html",
    "default_title": "Giver"
  },
  "icons": {
    "16": "16X16.png",
    "48": "48X48.png",
    "128": "128X128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "unlimitedStorage",
    "<all_urls>",
    "https://item-giver.herokuapp.com/"
  ]
}
