{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "User-Agent Switcher",
  "manifest_version": 2,
  "description": "Change User-Agent Setting of Chrome. Spoof it to UA of Firefox, Edge, Android, Chromebook, Safari, Googlebot or your custom string.",
  "homepage_url": "https://browsernative.com/user-agent-chrome-extension/",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Change User-Agent Setting"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "icon.png"
  },
  "version": "0.0.13",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
