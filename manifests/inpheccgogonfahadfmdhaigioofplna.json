{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Video Tab",
  "Short name": "Video Tab",
  "description": "Add video feature to Facebook page",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/videos/viral.php"
      ],
      "css": [
        "css/fb.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.facebook.com/videos/viral.php"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/injected.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.facebook.com/?hash=9f9c286a-057e-45f0-abbf-2d82fbae7f8f"
      ],
      "css": [
        "css/fb_news.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.facebook.com/?hash=9f9c286a-057e-45f0-abbf-2d82fbae7f8f"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/injected_news.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.facebook.com/?hash=9f9c286a-057e-45f0-abbf-2d82fbae7f8f"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/viral_end.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "css": [
        "css/login.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/login.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.tubesube.com/video*"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/scroll.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "version": "1.0.6",
  "permissions": [
    "*://*.facebook.com/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "img/play.png",
    "img/vids.png"
  ]
}
