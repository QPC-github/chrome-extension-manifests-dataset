{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timely Calendar",
  "version": "0.2.11",
  "description": "Import events to your Timely Calendar.",
  "permissions": [
    "storage",
    "webNavigation",
    "https://*.time.ly/",
    "https://www.facebook.com/",
    "https://web.facebook.com/",
    "https://www.eventbrite.com/",
    "https://www.eventbrite.com.ar/",
    "https://www.eventbrite.com.br/",
    "https://www.eventbrite.es/",
    "https://www.eventbrite.pt/",
    "https://www.eventbrite.ca/",
    "https://www.eventbrite.co.uk/",
    "https://www.eventbrite.de/",
    "https://www.eventbrite.fr/"
  ],
  "background": {
    "scripts": [
      "moment.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://*.time.ly; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Timely"
  },
  "icons": {
    "16": "timely16.png",
    "48": "timely48.png",
    "128": "timely128.png"
  },
  "manifest_version": 2
}
