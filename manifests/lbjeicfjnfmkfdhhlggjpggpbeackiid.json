{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fix Twitter",
  "version": "0.32",
  "description": "Swaps right and left columns over so tweets are on the left /by @andydavies",
  "icons": {
    "128": "fixtwitter.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "fixtwitter.css"
      ]
    }
  ]
}
