    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "browser_action": {
    "default_icon": "sbo-putih-16.png",
    "default_title": "SBO - TEXT SPINTAX",
    "default_popup": "chromiflash.html"
  },
  "icons": {
    "128": "sbo-putih-128.png"
  }
}
