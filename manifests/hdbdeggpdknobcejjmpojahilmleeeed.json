{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "New Tabs + Quizlet Flashcards",
  "description": "A Quizlet flashcard in every new tab.",
  "version": "0.3.6",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
