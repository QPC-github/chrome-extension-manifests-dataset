{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Txt Mockup",
  "description": "Makes all text on the current page editable",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "pencil.png",
    "default_title": "Make this page editable"
  }
}
