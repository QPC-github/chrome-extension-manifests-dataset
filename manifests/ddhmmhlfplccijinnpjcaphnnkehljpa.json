{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smart EMS Native Extension",
  "short_name": "Smart EMS Extension",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "Smart EMS Native Extension - Native Message Broker",
  "author": "Alaa Salem",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "*://ems:*/ems/*",
      "*://emsweb:*/ems/*",
      "*://emsapp:*/ems/*",
      "*://localhost:*/ems/*",
      "*://127.0.0.1:*/ems/*"
    ]
  }
}
