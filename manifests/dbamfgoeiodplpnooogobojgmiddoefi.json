{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NetPlus",
  "version": "0.13",
  "description": "넷플릭스에 유용한 기능 몇가지를 추가합니다.",
  "content_scripts": [
    {
      "js": [
        "content-watch.js"
      ],
      "matches": [
        "https://www.netflix.com/watch/*"
      ]
    },
    {
      "js": [
        "content-genre.js"
      ],
      "matches": [
        "https://www.netflix.com/browse/*"
      ],
      "css": [
        "style-genre.css"
      ]
    }
  ]
}
