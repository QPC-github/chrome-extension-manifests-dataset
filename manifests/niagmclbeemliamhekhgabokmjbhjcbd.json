{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tech Khan",
  "short_name": "Tech Khan",
  "version": "1.1",
  "description": "Tech Khan updates you with Tech News and Tech Hacks",
  "homepage_url": "https://www.techkhan.com.np",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "global.js",
      "js/jquery.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "icon48.png"
  ]
}
