{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nargi Tracker!",
  "author": "Maywtf",
  "version": "1.1",
  "description": "Wtyczka wyświetlająca powiadomienia o streamie Nargi! ",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/dziabacz_icon16.png",
      "32": "images/dziabacz_icon32.png",
      "48": "images/dziabacz_icon48.png",
      "128": "images/dziabacz_icon128.png"
    }
  },
  "icons": {
    "16": "images/dziabacz_icon16.png",
    "32": "images/dziabacz_icon32.png",
    "48": "images/dziabacz_icon48.png",
    "128": "images/dziabacz_icon128.png"
  },
  "manifest_version": 2
}
