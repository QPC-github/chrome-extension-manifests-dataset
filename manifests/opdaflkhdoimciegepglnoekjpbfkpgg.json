{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Url Mover",
  "description": "This extension allows you to redirect urls to other locations.",
  "version": "1.0",
  "options_page": "options.html",
  "icons": {
    "16": "bulldozer16.png",
    "48": "bulldozer48.png",
    "128": "bulldozer128.png"
  },
  "web_accessible_resources": [
    "bulldozer48.png"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "storage",
    "notifications"
  ]
}
