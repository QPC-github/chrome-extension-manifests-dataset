{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "__MSG_chrome_extension_name__",
  "short_name": "__MSG_chrome_extension_short_name__",
  "description": "__MSG_chrome_extension_description__",
  "default_locale": "en",
  "icons": {
    "128": "128x128.png",
    "64": "64x64.png",
    "48": "48x48.png",
    "19": "19x19.png",
    "16": "16x16.png"
  },
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.7.5",
  "omnibox": {
    "keyword": "aliexpress"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.aliexpress.com/*",
        "*://*.google.com/*",
        "*://*.google.ru/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "jquery.min.js",
    "jquery.min.map",
    "content.js",
    "popup.js"
  ]
}
