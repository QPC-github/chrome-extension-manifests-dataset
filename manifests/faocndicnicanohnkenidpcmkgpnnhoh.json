{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Capitalised Video Remover",
  "version": "0.0.3",
  "description": "Removes videos from Youtube that are written in all caps.",
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/",
        "https://*.youtube.com/"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
