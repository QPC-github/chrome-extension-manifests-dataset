{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HotLog",
  "description": "Grep like utility for browser",
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "HotLogLoader.js"
    ]
  },
  "browser_action": {
    "default_title": "HotLog",
    "default_icon": "utilities-log-viewer.png"
  },
  "icons": {
    "16": "utilities-log-viewer.png",
    "48": "utilities-log-viewer.png",
    "128": "utilities-log-viewer.png"
  },
  "manifest_version": 2
}
