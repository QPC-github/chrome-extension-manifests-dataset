{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gainsight PX",
  "version": "0.0.26",
  "description": "Injects the Gainsight PX tag",
  "permissions": [
    "activeTab",
    "webNavigation",
    "declarativeContent",
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "util.js",
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/images/px-16.png",
      "32": "assets/images/px-32.png",
      "48": "assets/images/px-48.png",
      "128": "assets/images/px-128.png"
    }
  },
  "icons": {
    "16": "assets/images/px-16.png",
    "32": "assets/images/px-32.png",
    "48": "assets/images/px-48.png",
    "128": "assets/images/px-128.png"
  },
  "manifest_version": 2
}
