{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy brezy test",
  "description": "Программа для людей н",
  "version": "0.0.1",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content-script.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Открыть",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
