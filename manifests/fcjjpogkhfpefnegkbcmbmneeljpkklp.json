{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto Reload Aipo Webmail",
  "default_locale": "en",
  "version": "1.0.6",
  "description": "__MSG_description__",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "myscript.js"
      ]
    }
  ],
  "icons": {
    "16": "reload-2398777_19.png",
    "48": "reload-2398777_48.png",
    "128": "reload-2398777_128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
