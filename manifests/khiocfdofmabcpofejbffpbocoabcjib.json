{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "short_name": "RecentTabs",
  "version": "2.2.2",
  "manifest_version": 2,
  "default_locale": "en",
  "minimum_chrome_version": "67",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "assets/installChecker.png"
  ],
  "permissions": [
    "tabs",
    "sessions",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Z"
      }
    }
  },
  "browser_action": {
    "default_title": "__MSG_extName__"
  }
}
