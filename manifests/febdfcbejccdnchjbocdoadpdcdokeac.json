{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pump",
  "description": "Use Pump instantly to save it and make it accessiblefrom any device be it a tablet, smartphone or laptop.",
  "Short name": "pump app",
  "version": "1.5.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "picker.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/ext*"
      ],
      "css": [
        "mystyles.css"
      ],
      "js": [
        "jquery.js",
        "myScript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
