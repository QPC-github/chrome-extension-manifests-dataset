{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "唯品会自动抢单器",
  "version": "1.42",
  "description": "全自动对售完商品跟踪抢单,抢到自动放到购物车. ",
  "permissions": [
    "tabs",
    "*://*/*",
    "webRequest",
    "cookies",
    "history"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "唯品会自动抢单器"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
