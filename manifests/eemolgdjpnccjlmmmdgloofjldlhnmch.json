{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FearlessQ Live Extension",
  "version": "1.2",
  "description": "FearlessQ Live Extension",
  "icons": {
    "128": "images/logo_on.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./utils/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/logo_off.png",
      "20": "images/logo_on.png"
    }
  }
}
