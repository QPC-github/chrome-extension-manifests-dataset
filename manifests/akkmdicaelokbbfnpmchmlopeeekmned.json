  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "http://*/"
  ],
  "name": "Deepika Padukone",
  "description": "Deepika Padukone",
  "icons": {
    "128": "icon.png"
  },
  "version": "1.3",
