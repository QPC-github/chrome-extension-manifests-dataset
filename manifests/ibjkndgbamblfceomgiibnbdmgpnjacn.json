{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL Rabbit - Hop Over URL Fragments",
  "short_name": "URL Rabbit",
  "description": "Used to take the current Tab's URL and break into components of the URL structure",
  "version": "1.0",
  "author": "Ziv Levy",
  "browser_action": {
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png"
    },
    "default_popup": "popup.html",
    "default_title": "URL Rabbit"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  }
}
