{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "コンテキストメニューサーチ",
  "version": "0.0.1",
  "description": "コンテキストメニューから選択したテキストを検索",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "option.html",
  "icons": {
    "16": "icon-bitty.png",
    "48": "icon-small.png",
    "128": "icon-large.png"
  },
  "default_locale": "ja"
}
