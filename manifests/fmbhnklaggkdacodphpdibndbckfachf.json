{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Blocks advertisers on mobile.twitter.com",
  "manifest_version": 2,
  "name": "Ad Blocker for Twitter",
  "version": "1.1",
  "homepage_url": "https://ash.ms/",
  "icons": {
    "48": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://mobile.twitter.com/*"
      ],
      "js": [
        "mobile.js"
      ]
    }
  ]
}
