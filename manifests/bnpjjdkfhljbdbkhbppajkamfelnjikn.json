{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "infra@smartklick.com",
  "name": "Verikick",
  "short_name": "Verikick",
  "minimum_chrome_version": "34",
  "homepage_url": "https://www.veriklick.com",
  "description": "Veriklick, Screen sharing extension",
  "logos": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "version": "1.0.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "run_at": "document_end",
    "matches": [
      "https://www.veriklick.com/*",
      "https://www.smartklick.com/*",
      "https://veriklick.ngrok.io/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "logo-16.png"
  ],
  "accepts_tls_channel_id": false
}
