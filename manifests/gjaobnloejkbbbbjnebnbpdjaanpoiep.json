{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EDX Part Search",
  "version": "1.0.0",
  "description": "Searches http://shop.edxelectronics.com for the highlighted part number",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "http://shop.edxelectronics.com/*"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "imageinfo-16.png",
    "48": "imageinfo-48.png",
    "128": "imageinfo-128.png"
  },
  "manifest_version": 2
}
