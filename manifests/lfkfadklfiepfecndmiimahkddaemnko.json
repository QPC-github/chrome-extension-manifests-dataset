{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google+のベルを表示",
  "description": "Google+のベルを表示します。",
  "author": "がんもさん",
  "version": "1.0.2",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://plus.google.com/*"
  ],
  "short_name": "Google+のベル",
  "content_scripts": [
    {
      "matches": [
        "https://plus.google.com/*"
      ],
      "js": [
        "bell.js"
      ]
    }
  ]
}
