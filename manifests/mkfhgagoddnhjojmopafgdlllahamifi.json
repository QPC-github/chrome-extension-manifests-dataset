{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Jira Extension"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/content.css",
        "style.css",
        "css/buttons.css"
      ],
      "include_globs": [
        "*://mail.google.*/*"
      ],
      "js": [
        "resources/jquery-3.3.1/jquery-3.3.1.min.js",
        "js/api.js",
        "js/inpage.js",
        "js/menus.js",
        "js/content.js",
        "js/background.js",
        "lib/q.js",
        "js/inboxsdk.js",
        "js/app.js"
      ],
      "matches": [
        "*://*/*",
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "matches": [
        "*://drive.google.com/*"
      ],
      "js": [
        "resources/jquery-3.3.1/jquery-3.3.1.min.js",
        "js/gtj.google.drive.js"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "matches": [
        "*://groups.google.com/*"
      ],
      "js": [
        "resources/jquery-3.3.1/jquery-3.3.1.min.js",
        "js/gtj.menu.helpers.js",
        "js/gtj.google.groups.js"
      ],
      "css": [
        "css/calendarMenu.css"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "matches": [
        "*://docs.google.com/*"
      ],
      "js": [
        "resources/jquery-3.3.1/jquery-3.3.1.min.js",
        "js/gtj.menu.helpers.js",
        "js/gtj.google.docs.js"
      ],
      "css": [
        "css/calendarMenu.css"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://infosysta-apps.com:444/ https://apis.google.com https://apis.google.com/js/client.js https://ajax.googleapis.com https://designshack.net/favicon.ico https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js https://aui-cdn.atlassian.com https://netdna.bootstrapcdn.com https://tarruda.github.com  https://cdnjs.cloudflare.com https://ydn-src-2.storage.googleapis.com  https://www.google.com/ 'unsafe-eval'; object-src 'self' ",
  "default_locale": "en",
  "description": "The ultimate extension that connects Jira Extension in order to manage your Jira issues, bugs or tasks directly from Google Apps.",
  "icons": {
    "128": "images/155x155.png",
    "48": "images/48x48.png"
  },
  "manifest_version": 2,
  "name": "Jira",
  "short_name": "GTJ",
  "permissions": [
    "contextMenus",
    "tabs",
    "identity",
    "storage",
    "https://*.google.com/*"
  ],
  "oauth2": {
    "client_id": "768976677270-higgtvslsjcamcqurr6fhdsmvt5a4ggs.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.readonly",
      "https://www.googleapis.com/auth/calendar.events.readonly"
    ]
  },
  "version": "3.8.2.0",
  "web_accessible_resources": [
    "GTJPages/oauth.html",
    "GTJPages/jira.addComment.html",
    "GTJPages/createIssue.html",
    "GTJPages/issueSearch.html",
    "GTJPages/setup.html",
    "GTJPages/login.html",
    "GTJPages/settings.html",
    "GTJPages/loginServer.html",
    "GTJPages/accounts.html",
    "GTJPages/about.html",
    "GTJPages/logWork.html",
    "images/48x48.png",
    "images/gjtcolored_21x21.png",
    "images/CreateFromEmailHD24x24.png",
    "images/comments.png",
    "images/16x16.png",
    "images/ajax-loader.gif",
    "images/blank.png",
    "js/inpage.js",
    "js/menus.js",
    "js/createIssue.js",
    "js/jira.addComment.js",
    "js/gmail.js",
    "icons/gtj.svg",
    "_locales/en/messages.json",
    "_locales/ar/messages.json",
    "_locales/fr/messages.json",
    "_locales/de/messages.json",
    "_locales/es/messages.json",
    "images/grey_24x24.png",
    "icons/loading.gif",
    "images/16x16.png",
    "*://*/*"
  ]
}
