{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Custom Newtab Page",
  "description": "Override the new tab page with a Custom one",
  "version": "1.3",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "Files/icon16.png",
    "48": "Files/icon48.png",
    "128": "Files/icon128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
