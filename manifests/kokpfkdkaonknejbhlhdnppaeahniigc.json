{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jifler",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Convert Jifler in your default home page for fast search",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "omnibox": {
    "keyword": "OMNIBOX-KEYWORD"
  },
  "web_accessible_resources": [
    "images/icon-48.png"
  ],
  "chrome_url_overrides": {
    "newtab": "page.html"
  },
  "chrome_settings_overrides": {
    "homepage": "https://www.jifler.com",
    "startup_pages": [
      "https://www.jifler.com"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon-16.png"
  }
}
