{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "128.ico"
  },
  "name": "Infinite Scroll Blocker",
  "version": "1.0",
  "description": "Stop unintentional scrolling on all websites.",
  "manifest_version": 2,
  "web_accessible_resources": [
    "inject.js"
  ]
}
