{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Github Tasker",
  "version": "1.0.3",
  "description": "Show all task from comments in fixed Taskbar window on bottom of page",
  "manifest_version": 2,
  "homepage_url": "https://github.com/MilanPala/GithubTasker",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/issues/*",
        "https://github.com/*/*/pull/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery.cookie.js",
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
