{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lyrics extractor",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "./icon.png"
  },
  "description": "This extension extracts lyrics from few webpages.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "index.js"
      ],
      "css": [
        "index.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
