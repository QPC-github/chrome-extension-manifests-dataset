{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "commons.js",
      "background.js",
      "newAnalytics.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "assets/img/logo/96.png"
    },
    "default_popup": "popup.html",
    "default_title": "AliExpert for AliExpress"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "commons.js",
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://mc.yandex.ru/ https://cdn.firebase.com https://*.firebaseio.com; object-src 'self';connect-src *; img-src * 'self' data:; media-src *;",
  "default_locale": "ru",
  "description": "__MSG_extDesc__",
  "icons": {
    "128": "assets/img/logo/96.png",
    "16": "assets/img/logo/96.png",
    "48": "assets/img/logo/96.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "options_page": "settings.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage",
    "unlimitedStorage",
    "gcm"
  ],
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "short_name": "__MSG_extSname__",
  "version": "1.9.40",
  "web_accessible_resources": [
    "assets/*/*",
    "_locales/*/*",
    "settings.html",
    "auth.html"
  ]
}
