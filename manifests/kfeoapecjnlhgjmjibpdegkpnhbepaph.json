{
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "style.css"
      ],
      "js": [
        "hdpiano.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ]
    }
  ],
  "description": "Speed controls for HDpiano piano lesson videos.",
  "browser_action": {
    "default_icon": {
      "19": "hdpiano19.png",
      "38": "hdpiano38.png"
    }
  },
  "icons": {
    "128": "hdpiano128.png",
    "48": "hdpiano48.png",
    "17": "icon.png"
  },
  "manifest_version": 2,
  "name": "HDpiano Speed Control",
  "permissions": [
    "activeTab"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
