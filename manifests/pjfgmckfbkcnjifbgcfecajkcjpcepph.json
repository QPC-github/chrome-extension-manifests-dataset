{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Site checker",
  "manifest_version": 2,
  "name": "SiteChecker",
  "version": "1.0.3",
  "homepage_url": "https://sitechecker.club",
  "icons": {
    "64": "icons/test-64.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "background",
    "storage",
    "webNavigation"
  ],
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/test-32.png",
    "default_title": "SiteChecker",
    "default_popup": "popup.html"
  }
}
