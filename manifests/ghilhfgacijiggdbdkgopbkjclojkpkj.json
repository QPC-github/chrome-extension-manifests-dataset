{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "二维码生成工具(w3cways QR Code Generator)",
  "description": "二维码生成工具(w3cways QR Code Generator, www.w3cways.com",
  "version": "1.0.7",
  "browser_action": {
    "default_icon": "qrcode.png",
    "default_title": "w3cways_qrCode",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "qrcode.png",
    "64": "qrcode.png",
    "16": "qrcode.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "file:///*/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "http://*/*",
    "file:///*/*"
  ]
}
