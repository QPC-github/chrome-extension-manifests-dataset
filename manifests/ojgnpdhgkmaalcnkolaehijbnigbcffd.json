{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Y+",
  "description": "Youtube speed up +",
  "short_name": "Youtube speed +",
  "version": "1.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "img/19.png",
      "38": "img/38.png"
    }
  },
  "icons": {
    "16": "img/16.png",
    "19": "img/19.png",
    "38": "img/38.png",
    "64": "img/64.png",
    "128": "img/128.png"
  }
}
