{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "auto-focus",
  "version": "1.0.0",
  "description": "An extension to auto-focus on inputs",
  "author": [
    {
      "name": "Jean Sidharta Rocha Guzman",
      "email": "jeansidharta@gmail.com",
      "github": "github.com/jeansidharta"
    }
  ],
  "icons": {
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "injected.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
