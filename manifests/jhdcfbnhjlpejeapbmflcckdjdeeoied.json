{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "16": "img/01d-16px.png",
      "32": "img/01d-32px.png"
    },
    "default_title": "WeatherEasy",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "libs/jquery-3.2.1.min.js",
      "bg.js",
      "settings.js",
      "weather.js"
    ]
  },
  "description": "",
  "icons": {
    "128": "img/01d-192px.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self'",
  "manifest_version": 2,
  "name": "WeatherEasy",
  "permissions": [
    "storage",
    "alarms",
    "http://weathereasy.com/*"
  ],
  "version": "1.0.3"
}
