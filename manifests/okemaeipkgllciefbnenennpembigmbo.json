{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Webbie",
  "description": "Track your time on websites for Chrome.",
  "version": "1.4",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "backgroundPage.js"
    ],
    "presistent": true
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "logo.png",
    "file.png"
  ]
}
