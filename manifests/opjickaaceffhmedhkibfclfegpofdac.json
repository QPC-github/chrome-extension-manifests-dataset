{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Shuffle",
  "description": "Join in the tab merry go round.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "tab-shuffle": {
      "suggested_key": {
        "default": "Ctrl+Shift+S"
      },
      "description": "Tab Shuffle"
    }
  }
}
