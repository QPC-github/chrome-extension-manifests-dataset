{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LoLEventVods - hide related",
  "description": "Hides related videos on LoLEventVods videos",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": [
    "identity",
    "tabs",
    "*://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
