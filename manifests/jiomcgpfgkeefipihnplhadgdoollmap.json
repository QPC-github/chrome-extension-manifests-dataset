{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.30.0",
  "default_locale": "en",
  "name": "__MSG_name__",
  "short_name": "unseenforfacebook",
  "description": "__MSG_description__",
  "browser_action": {
    "default_title": "Unseen for Facebook",
    "default_popup": "pages/popup.html",
    "default_icon": {
      "16": "images/unseen16.png",
      "32": "images/unseen32.png",
      "48": "images/unseen48.png",
      "128": "images/unseen128.png"
    }
  },
  "background": {
    "scripts": [
      "pages/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "ws://*/*",
    "wss://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "16": "images/unseen16.png",
    "32": "images/unseen32.png",
    "48": "images/unseen48.png",
    "128": "images/unseen128.png"
  },
  "minimum_chrome_version": "60",
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://static.trackized.com; object-src 'self'"
}
