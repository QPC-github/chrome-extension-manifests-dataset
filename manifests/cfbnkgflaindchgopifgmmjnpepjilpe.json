{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Shop2Fundraise",
  "version": "2.12",
  "manifest_version": 2,
  "description": "Fundraise with your favourite shops for your favourite chairties",
  "background_page": "global.html",
  "homepage_url": "http://www.shop2fundraise.co.uk",
  "permissions": [
    "tabs",
    "http://www.shop2fundraise.co.uk/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": "grey.png"
  }
}
