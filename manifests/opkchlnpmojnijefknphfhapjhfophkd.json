{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Securify",
  "version": "1.0.1",
  "description": "Securify gives you extra security on websites. (This extension is in a public beta phase)",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "securify_core.js"
      ]
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
