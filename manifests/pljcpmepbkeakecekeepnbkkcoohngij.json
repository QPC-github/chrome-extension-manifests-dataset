{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TimePilot",
  "description": "Toggle the time tracker when you start and stop working!",
  "version": "1.1.8",
  "permissions": [
    "http://timepilot.nl/"
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
