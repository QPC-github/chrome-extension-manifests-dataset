{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyVRSpot Screen Capture",
  "author": "MyVRSpot, LLC.",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "MyVRSpot screen capture extension",
  "homepage_url": "https://www.myvrspot.com/",
  "background": {
    "scripts": [
      "pipe_sc.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.myvrspot.com/*",
      "https://*.arkansas.gov/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ],
  "icons": {
    "48": "pipe-icon.png"
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}
