{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No More Likes",
  "version": "1.1",
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/style.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "manifest_version": 2
}
