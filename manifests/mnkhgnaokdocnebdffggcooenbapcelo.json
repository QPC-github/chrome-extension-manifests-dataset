{
  "author": "Voipswitch.com",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://qloudpbx.quadcom.ch/*",
        "https://qloudpbx.quadcom.ch/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "A screen capture extension for QuadCom QloudPBX UFC Platform.",
  "homepage_url": "https://www.quadcom.ch/",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "62",
  "name": "QloudPBX Screen sharing",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
