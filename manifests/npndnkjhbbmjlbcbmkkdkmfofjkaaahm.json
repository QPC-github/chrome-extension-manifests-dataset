{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IncrediMail",
  "version": "1.0.8",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "listener.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "notifications"
  ]
}
