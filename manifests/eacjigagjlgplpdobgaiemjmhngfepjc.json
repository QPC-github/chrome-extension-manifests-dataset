{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab",
  "description": "Simple New Tab",
  "version": "1.0.3",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "64": "myicon.png"
  },
  "browser_action": {
    "default_icon": "myicon.png",
    "default_title": "New Tab"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
