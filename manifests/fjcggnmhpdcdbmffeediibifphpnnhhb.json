{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kudos Extension",
  "version": "4.5",
  "manifest_version": 2,
  "description": "Extension to send Kudos",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "kudos.html"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "38": "icon38.png",
    "32": "icon32.png",
    "19": "icon19.png",
    "16": "icon16.png"
  },
  "permissions": [
    "http://api.flickr.com/"
  ]
}
