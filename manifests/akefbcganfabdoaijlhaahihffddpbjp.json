{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "d3/leprosorium service pack 3",
  "version": "3.0.65.38",
  "description": "Модульный сервис пак для сайтов d3.ru/leprosorium.ru",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.d3.ru/*",
        "*://*.dirty.ru/*",
        "*://*.leprosorium.ru/*",
        "*://*.leprosorium.com/*"
      ],
      "js": [
        "d3.loader.user.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "128": "i128.png"
  },
  "web_accessible_resources": [
    "d3.user.js"
  ]
}
