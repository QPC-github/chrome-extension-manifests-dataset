{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "replacement.js"
      ],
      "matches": [
        "http://www.tumblr.com/*"
      ]
    }
  ],
  "description": "Change Tumblr's header into a fixed element!",
  "manifest_version": 2,
  "name": "TumblrFloater",
  "permissions": [
    "http://www.tumblr.com/"
  ],
  "version": "1.3.1"
}
