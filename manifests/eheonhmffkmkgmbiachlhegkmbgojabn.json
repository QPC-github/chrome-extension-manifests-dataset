{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extensionName__",
  "short_name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "3.2.4.801",
  "permissions": [
    "contextMenus"
  ],
  "minimum_chrome_version": "41.0.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
