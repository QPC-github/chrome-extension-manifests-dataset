{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YMPU",
  "version": "0.2.3",
  "manifest_version": 2,
  "description": "Post photos and urls.",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.gif",
    "default_popup": "menu.html",
    "default_title": "YM"
  },
  "icons": {
    "16": "icon_16.gif",
    "48": "icon.gif"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "http://*/*",
    "http://*/",
    "https://*/"
  ]
}
