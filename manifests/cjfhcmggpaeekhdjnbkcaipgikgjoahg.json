{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Host Checker",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Host Checker",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_title": "Host Checker",
    "default_popup": "popup.html",
    "js": [
      "jquery-3.2.1.min.js",
      "popup.js"
    ]
  }
}
