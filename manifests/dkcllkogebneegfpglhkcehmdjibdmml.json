{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Private Search",
  "short_name": "privatesearch",
  "description": "Find information without trails.",
  "version": "1.0",
  "permissions": [
    "history",
    "tabs"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "PrivateSearch",
      "keyword": "private",
      "search_url": "https://privatesearch.biz/?q={searchTerms}",
      "favicon_url": "https://privatesearch.biz/i/logo.png",
      "suggest_url": "https://privatesearch.biz/autocomplete.php?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon32.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "*"
  ],
  "browser_action": {}
}
