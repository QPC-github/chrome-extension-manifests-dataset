{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "16": "images/ico.png",
      "24": "images/ico.png",
      "32": "images/ico.png"
    },
    "default_popup": "popup.html",
    "default_title": "查看天气"
  },
  "description": "查看最近的天气情况",
  "icons": {
    "128": "images/ico.png",
    "16": "images/ico.png",
    "48": "images/ico.png"
  },
  "manifest_version": 2,
  "name": "Weather",
  "permissions": [
    "http://int.dpool.sina.com.cn/iplookup/iplookup.php?format=json",
    "https://free-api.heweather.com/v5/weather?city="
  ],
  "version": "1.2.0"
}
