{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kanser Belirtileri, Tanı ve Tedavisi",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Prof. Dr. Mustafa Özdoğan tıbbi onkoloji uzmanı - Kanser tanı ve güncel kanser tedavisi, sağlıklı yaşam ve kanser haberleri",
  "homepage_url": "https://www.drozdogan.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "https://www.drozdogan.com"
  ]
}
