{
  "content_scripts": [
    {
      "css": [
        "icons/style.css"
      ],
      "js": [
        "jquery.min.js",
        "popup.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Add Emojis To Your Tweets.",
  "homepage_url": "http://A7med.me/",
  "short_name": "TEmoji",
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "Emoji for Twitter",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Emoji for Twitter",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.2",
  "web_accessible_resources": [
    "icons/*"
  ]
}
