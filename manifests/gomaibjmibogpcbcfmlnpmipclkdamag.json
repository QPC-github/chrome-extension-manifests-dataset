{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "chatwork一括既読ボタン",
  "description": "1クリックでchatworkの未読スレッドを既読にするボタンです。ピンで留めていないスレッドが対象です。",
  "version": "1.3",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "name": "既読化！"
  },
  "manifest_version": 2
}
