{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lost Time",
  "version": "7.0",
  "icons": {
    "128": "images/128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background2.js"
    ]
  },
  "permissions": [
    "http://losttime.su/*"
  ],
  "browser_action": {
    "default_title": "LostTime",
    "default_icon": "images/128x128.png",
    "default_popup": "popup.html"
  }
}
