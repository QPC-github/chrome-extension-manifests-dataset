{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wireframer",
  "description": "This extension helps you create simply Wireframes direct on browser",
  "version": "1.0.7.7",
  "author": "Emre Sakarya",
  "homepage_url": "http://www.softberry.de",
  "icons": {
    "16": "./icon-16x16.png",
    "32": "./icon-32x32.png",
    "48": "./icon-48x48.png",
    "64": "./icon-64x64.png",
    "128": "./icon-128x128.png"
  },
  "browser_action": {
    "default_icon": "./icon-16x16.png",
    "default_title": "Show Toolbox"
  },
  "background": {
    "scripts": [
      "./chrome.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
