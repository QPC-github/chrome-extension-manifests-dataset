{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NoSig (robertsspaceindustries.com)",
  "short_name": "NoSig (RSI)",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Hides forum signatures. RSI forums only (yet!)",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "*://forums.robertsspaceindustries.com/*"
  ]
}
