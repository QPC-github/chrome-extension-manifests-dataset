{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pan for Video",
  "version": "1.13.10",
  "author": "@nulltypo",
  "manifest_version": 2,
  "description": "ライブ配信の音声をLRに振り分けるやつ",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "http://live2.nicovideo.jp/*",
        "https://live2.nicovideo.jp/*",
        "https://www.openrec.tv/live/*",
        "https://www.showroom-live.com/*",
        "https://www.mirrativ.com/*",
        "https://twitcasting.tv/*",
        "https://www.twitch.tv/*",
        "https://whowatch.tv/*",
        "https://17.live/*",
        "https://reality.wrightflyer.net/*",
        "https://abema.tv/*",
        "https://freshlive.tv/*",
        "https://www.mildom.com/*",
        "https://*.bilibili.com/*"
      ],
      "js": [
        "jquery.min.js",
        "set.js",
        "sites/youtube.js",
        "sites/nicolive.js",
        "sites/openrec.js",
        "sites/showroom.js",
        "sites/mirrativ.js",
        "sites/twicas.js",
        "sites/twitch.js",
        "sites/whowatch.js",
        "sites/ichinana.js",
        "sites/reality.js",
        "sites/abema.js",
        "sites/freshlive.js",
        "sites/mildom.js",
        "sites/bilibili.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "https://www.youtube.com/*",
    "https://www.openrec.tv/live/*",
    "https://www.showroom-live.com/*",
    "https://www.mirrativ.com/*",
    "https://twitcasting.tv/*",
    "https://www.twitch.tv/*",
    "https://whowatch.tv/*",
    "https://17.live/*",
    "https://reality.wrightflyer.net/*",
    "https://abema.tv/*",
    "https://freshlive.tv/*",
    "https://*.mildom.com/*",
    "https://*.bilibili.com/*"
  ]
}
