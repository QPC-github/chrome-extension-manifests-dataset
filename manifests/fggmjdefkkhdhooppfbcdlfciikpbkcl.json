{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FootballDates",
  "version": "1.4.8.2",
  "manifest_version": 2,
  "author": "hegispok",
  "description": "Â¡Sigue a tus equipos favoritos y no te pierdas ni un solo partido!",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "*://www.google.com/*"
  ],
  "icons": {
    "128": "images/Logos/logo128.png",
    "48": "images/Logos/logo48.png",
    "16": "images/Logos/logo16.png"
  },
  "browser_action": {
    "default_icon": "images/Logos/logo48.png",
