{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wiztute Screen Sharing",
  "author": "Wiztute, Inc.",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome Extension is for Wiztute Screen Sharing",
  "homepage_url": "http://tokbox.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:4000/*",
      "http://localhost:8888/*",
      "https://www.wiztute.com/*",
      "https://www.wiztute.in/*",
      "https://www.wiztute.com/*",
      "https://student.wiztute.com/*",
      "https://tutors.wiztute.com/*",
      "https://wiztute.com/*",
      "https://wiztute.in/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
