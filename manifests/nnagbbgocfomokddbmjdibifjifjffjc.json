{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "DomainTyper - Domain Search Extension",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Search for your Domain as fast as you can type. Instant Domain Name Search.",
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs"
  ]
}
