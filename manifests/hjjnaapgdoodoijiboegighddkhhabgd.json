{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gifty-Fifty.pl",
  "description": "Gifty-Fifty.pl - Otrzymaj zwrot pieniÄ™dzy za zakupy online",
  "version": "1.1.4",
  "icons": {
    "128": "/img/128.png",
    "48": "/img/48.png",
    "16": "/img/16.png"
  },
  "browser_action": {
    "default_icon": "/img/48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "/libs/jquery.min.js",
      "/libs/psl.min.js",
      "/config.js",
      "/js/api.js",
      "/js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "templates/*",
    "libs/*"
  ]
}
