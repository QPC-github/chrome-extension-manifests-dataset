{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Emoji Insert",
  "description": "Insert text Emoji's with the right-click context menu",
  "version": "1.4",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "EmojiContextMenu.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
