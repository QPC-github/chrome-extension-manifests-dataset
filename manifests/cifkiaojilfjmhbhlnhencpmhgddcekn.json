{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SwitchTab",
  "version": "0.1.0",
  "description": "switch tabs with ease",
  "author": "Gilad Gray https://github.com/giladgray",
  "manifest_version": 2,
  "icons": {
    "48": "assets/tab-icon48.png",
    "128": "assets/tab-icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/tab-icon19.png",
    "default_popup": "popup.html",
    "default_title": "SwitchTab"
  },
  "permissions": [
    "http://code.jquery.com/",
    "tabs",
    "http://*/*"
  ]
}
