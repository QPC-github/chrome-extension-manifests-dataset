{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Stickers in Facebook",
  "description": "Hides stickers in Facebook messages.",
  "version": "1.0",
  "author": "Danny van Kooten",
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "manifest_version": 2
}
