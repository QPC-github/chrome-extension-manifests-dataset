{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kidy: Safe search engine for kids",
  "version": "1.11",
  "description": "An intelligent and safe search aimed at children. Our search results blocks explicit contents and emphasize on kid-related webpages",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_settings_overrides": {
    "homepage": "https://kidy.co",
    "search_provider": {
      "name": "name.kidy.co",
      "keyword": "keyword.kidy",
      "search_url": "https://www.kidy.co/search?q={searchTerms}",
      "favicon_url": "https://www.kidy.co/favicon.ico",
      "instant_url": "https://www.kidy.co/search?q={searchTerms}",
      "image_url": "https://www.kidy.co/pictures?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "https://kidy.co"
    ]
  }
}
