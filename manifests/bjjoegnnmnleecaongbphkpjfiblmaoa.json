{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Light IP Locator",
  "manifest_version": 2,
  "version": "0.2",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "alarms",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/world_16.png",
    "default_title": "Light IP Locator",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery-2.0.2.min.js",
      "js/eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "images/world_128.png",
    "64": "images/world_64.png",
    "48": "images/world_48.png",
    "32": "images/world_32.png",
    "16": "images/world_16.png"
  }
}
