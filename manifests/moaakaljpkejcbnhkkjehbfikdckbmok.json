{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gnaar Clicker plug-in",
  "version": "1.0.10",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "tabs",
    "http://gnaar.lokilab.ru/*"
  ],
  "browser_action": {
    "default_title": "Play Gnaar Clicker",
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  }
}
