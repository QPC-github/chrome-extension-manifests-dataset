{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "SAP Note Finder - Launchpad Edition",
  "manifest_version": 2,
  "version": "2",
  "description": "SAP Notes Finder - Launchpad Edition",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png"
  }
}
