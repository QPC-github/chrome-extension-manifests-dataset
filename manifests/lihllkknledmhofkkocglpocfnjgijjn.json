{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MGoBlog Enhancer",
  "version": "2.5",
  "description": "Fixes and customizes our favorite Michigan sports blog",
  "permissions": [
    "storage",
    "activeTab",
    "webRequest",
    "*://*.mgoblog.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.mgoblog.com/*",
        "*://*.collegefootballdata.com/*"
      ],
      "css": [
        "mgl-styles.css"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "background.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
