{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Apps4.Pro PowerBI Connector-Microsoft Planner",
  "short_name": "Apps4.Pro PowerBI Connector-Microsoft Planner",
  "version": "1.2.0.7",
  "background": {
    "page": "index.html#/background",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Apps4.Pro Power BI Connector for Microsoft Planner"
  },
  "description": "Syncs Microsoft Planner Plans and Tasks to Microsoft Power BI. Create and share dashboards with workspaces in Power BI.",
  "icons": {
    "128": "assets/powerbi-sync-128.png",
    "19": "assets/powerbi-sync-19.png",
    "48": "assets/powerbi-sync-48.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "*://*.tasks.office.com/",
    "https://login.windows.net/*"
  ],
  "externally_connectable": {
    "matches": []
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com; object-src 'self'",
  "web_accessible_resources": [
    "index.html"
  ]
}
