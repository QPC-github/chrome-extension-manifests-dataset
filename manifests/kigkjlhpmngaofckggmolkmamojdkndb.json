{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_popup": "index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://finesearching.com/favicon.ico",
      "is_default": true,
      "keyword": "Music",
      "name": "Gif Seeker",
      "search_url": "https://finesearching.com/srch/?keyword={searchTerms}"
    }
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "Extension help you to find any gif images",
  "homepage_url": "https://media-seeker.com",
  "icons": {
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "Gif Seeker",
  "omnibox": {
    "keyword": "g"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "storage",
    "tabs",
    "https://*/*",
    "topSites",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "Gif Seeker",
  "version": "2.1"
}
