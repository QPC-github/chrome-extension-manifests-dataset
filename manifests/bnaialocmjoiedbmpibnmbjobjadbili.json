{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-2.1.4.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "stats.png"
  },
  "description": "Shows servers status",
  "icons": {
    "128": "icon-128.png"
  },
  "name": "Brain/Out status checker",
  "permissions": [
    "https://game-live.brainout.org/"
  ],
  "version": "1.0.6",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
