{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Agile Extension Argentina",
  "short_name": "Agile Extension Argentina",
  "version": "1.0.0.5",
  "icons": {
    "128": "icon_128.png"
  },
  "description": "Agile Extension Argentina",
  "permissions": [
    "nativeMessaging",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "minimum_chrome_version": "26"
}
