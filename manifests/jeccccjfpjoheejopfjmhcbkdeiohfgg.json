{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Right Click Bookmark",
  "description": "Bookmark a link by right-clicking on it",
  "version": "1.0",
  "minimum_chrome_version": "6",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "bookmarks"
  ],
  "icons": {
    "16": "icon_16x16.png",
    "32": "icon_32x32.png",
    "128": "icon_128x128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "bg.js"
    ]
  }
}
