{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OPIS AtTask",
  "description": "AtTask helper",
  "version": "1.2.3",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://opis.attask-ondemand.com/*"
      ],
      "js": [
        "jquery.js",
        "myAtTaskScript.js"
      ]
    }
  ],
  "permissions": [
    "https://opis.attask-ondemand.com/*",
    "tabs",
    "storage"
  ]
}
