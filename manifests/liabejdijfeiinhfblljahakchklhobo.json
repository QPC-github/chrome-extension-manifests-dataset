{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ObamaFilter",
  "version": "1.0",
  "description": "Remove all references to Barack Obama from WhiteHouse.gov and Wikipedia!",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.whitehouse.gov/*",
        "*://*.wikipedia.org/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ]
}
