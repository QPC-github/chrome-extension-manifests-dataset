{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Waatcher Buy Target Button",
  "short_name": "Buy Target",
  "description": "Set your target price and we will place the order when the price drops below your set price",
  "version": "1.0.6",
  "icons": {
    "16": "w.png",
    "48": "w.png",
    "128": "w.png"
  },
  "browser_action": {
    "default_icon": "w.png",
    "default_popup": "popup.html",
    "default_title": "Buy Target"
  },
  "permissions": [
    "activeTab"
  ]
}
