{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Human Friendly",
  "description": "Design thinking with every new tab",
  "version": "1.0",
  "browser_action": {
    "default_icon": "off.png"
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "icons": {
    "16": "off.png",
    "48": "off.png",
    "128": "off.png"
  },
  "permissions": [
    "tabs"
  ]
}
