{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The tiny URL shortener",
  "short_name": "tiny-shortener",
  "version": "2.0.1",
  "author": "@marcjoha",
  "description": "A minimalistic Chrome extension for shortening URLs",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "identity",
    "storage",
    "https://bitly.com/oauth/authorize",
    "https://*.chromiumapp.org/*"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "key": ""
}
