{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Havanızı Değiştiren Sekmeler",
  "short_name": "Baymak ",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "version": "1.9.12",
  "manifest_version": 2,
  "description": "Günün her saati havanızı değiştirecek fotoğrafların olduğu Chrome eklentisini indirin.",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "geolocation",
    "https://www.promoqube.com/*",
    "https://www.googleapis.com/"
  ],
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "web_accessible_resources": [
    "128x128.png"
  ],
  "browser_action": {
    "default_icon": "48x48.png",
    "default_title": "Hoş geldiniz",
    "default_popup": "popup.html"
  }
}
