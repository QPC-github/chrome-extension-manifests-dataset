{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AliExpress Locale Helper - AliExpressWiki.com",
  "description": "The Ultimate Solution to Change AliExpress Website Language back to English and any Other Language You Feel Comfortable with.",
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "version": "0.0.0.1",
  "manifest_version": 2,
  "page_action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "css/locale16.png",
      "32": "css/locale32.png",
      "48": "css/locale48.png",
      "128": "css/locale128.png"
    }
  },
  "icons": {
    "16": "css/locale16.png",
    "32": "css/locale32.png",
    "48": "css/locale48.png",
    "128": "css/locale128.png"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
