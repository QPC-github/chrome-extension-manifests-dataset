{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random new tab page",
  "description": "Override the new tab page with some random pages, for users who want a set of new pages",
  "version": "2.0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "rand.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icon-16x16.png",
    "48": "images/icon-48x48.png",
    "120": "images/icon-120x120.png"
  },
  "manifest_version": 2
}
