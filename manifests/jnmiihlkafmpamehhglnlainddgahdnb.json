{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CQ2 Helper",
  "version": "0.2",
  "manifest_version": 2,
  "description": "An extension that adds some handy functionality to Castle Quest.",
  "content_scripts": [
    {
      "matches": [
        "http://cq2.anion.be/index.php?page=travel*",
        "http://cq2.anion.be/index.php?page=kingdom&action=removegembag*",
        "http://cq2.anion.be/index.php?page=kingdom&action=insertgembag*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ]
    }
  ],
  "permissions": [
    "http://cq2.anion.be/*"
  ]
}
