{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Thrasher Chrome Extension",
  "version": "1.0",
  "description": "Dann Chrome Description",
  "background": {
    "scripts": [
      "VideoListApp/templates/videolistapp/background.js"
    ],
    "persistent": false
  },
  "content-scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery3.3.1.js"
      ]
    }
  ],
  "manifest_version": 2,
  "incognito": "split",
  "browser_action": {
    "default_popup": "VideoListApp/templates/videolistapp/index.html",
    "default_icon": "VideoListApp/templates/videolistapp/Fav_64.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "cookies",
    "http://localhost/"
  ],
  "icons": {
    "128": "VideoListApp/templates/videolistapp/Fav_64.png"
  }
}
