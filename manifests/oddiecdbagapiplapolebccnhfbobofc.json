{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Links Exists",
  "description": "My first Chrome App.",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "core/axios.min.js",
      "core/main.js"
    ]
  },
  "permissions": [
    "tabs"
