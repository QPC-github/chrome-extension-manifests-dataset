{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Freadly",
  "description": "Freadly: how to optimize the feedly experience for Google Reader users. Feedly like Google Reader",
  "version": "1.0.1",
  "permissions": [
    "http://www.feedly.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.feedly.com/*"
      ],
      "js": [
        "readly.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
