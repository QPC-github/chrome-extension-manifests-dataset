{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Klix.ba",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Klix.ba je najposjećeniji i najutjecajniji informativni portal u Bosni i Hercegovini. Saznaj više.",
  "background": {
    "scripts": [
      "jquery.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "icon.png"
  }
}
