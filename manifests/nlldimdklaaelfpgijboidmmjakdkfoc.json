{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "すぐにカート Y!ショッピング",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Y!ショッピングで自動的にカートに入れるの近くへ移動します",
  "browser_action": {
    "default_icon": "icon_48.png"
  },
  "icons": {
    "48": "icon_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://store.shopping.yahoo.co.jp/*"
      ],
      "js": [
        "jump2iteminfo.js"
      ]
    }
  ]
}
