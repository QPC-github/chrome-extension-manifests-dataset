{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Booksync for Google Chromeâ„¢",
  "short_name": "Booksync",
  "version": "0.6.2",
  "description": "Integrate Booksync into your favorite browser!",
  "author": "Philipp A. Mohrenweiser <phiamo@googlemail.com>",
  "homepage_url": "https://chrome.google.com/webstore/detail/booksync-for-google-chrom/hfgcgkhnoneacpdpdkebnnlhndgdnmda",
  "manifest_version": 2,
  "icons": {
    "128": "images/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon128.png",
      "38": "images/icon128.png"
    },
    "default_title": "Booksync",
    "default_popup": "page.html"
  },
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "storage",
    "activeTab",
    "tabs",
    "identity",
    "notifications",
    "https://www.booksync.net/*oauth/v2/*",
    "https://www.booksync.net/*api/v1/*",
    "http://localhost:8000/*oauth/v2/*",
    "http://localhost:8000/*api/v1/*"
  ],
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "windows": "Ctrl+B",
        "linux": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
