{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gbrella email downloader",
  "short_name": "Gbrella",
  "version": "2.0.12",
  "minimum_chrome_version": "29",
  "description": "Download your Gmail",
  "permissions": [
    "storage",
    "unlimitedStorage",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "500874550349-1ge9vd8ivhcs430g9bi2rfca2ikb17r7.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly"
    ]
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "16": "gbrella-16-icon.png",
    "128": "gbrella-128-icon.png"
  }
}
