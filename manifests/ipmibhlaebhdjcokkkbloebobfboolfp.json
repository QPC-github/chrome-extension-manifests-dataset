{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Start page Customizer",
  "short_name": "Start page",
  "description": "Customize your Start Page as you wish.",
  "version": "1.1",
  "permissions": [],
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  }
}
