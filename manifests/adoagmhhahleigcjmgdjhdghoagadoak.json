  "permissions": [
    "tabs",
    "background",
    "http://*.google.com/",
    "https://*.google.com/",
    "https://www.googleapis.com/"
  ],
  "homepage_url": "http://nixongalario.com/naturalsounds",
  "offline_enabled": true
}
