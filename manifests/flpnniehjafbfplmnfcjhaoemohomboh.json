{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VK Frod Blocker",
  "version": "1.8",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://vk.com/*",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Make this page red"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "description": "Another blocking app.",
  "short_name": "Another  blocking app."
}
