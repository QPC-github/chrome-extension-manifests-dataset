{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NoSticky",
  "manifest_version": 2,
  "version": "1.1.1",
  "description": "NoSticky",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "nosticky.css"
      ],
      "matches": [
        "*://*.reddit.com/*"
      ],
      "js": [
        "js/nosticky.js"
      ]
    }
  ]
}
