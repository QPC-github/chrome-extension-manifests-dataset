{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Inetmafia.ru",
  "description": "Монитор лобби Inetmafia.ru",
  "version": "1.1.7",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "favicon.png",
      "38": "favicon.png"
    },
    "default_title": "Inetmafia",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "page": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "http://inetmafia.ru/*",
    "https://inetmafia.ru/*",
    "https://ssl.google-analytics.com/*"
  ],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}
