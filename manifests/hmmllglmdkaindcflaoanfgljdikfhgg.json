{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BJJ Technique Extractor",
  "version": "1.2",
  "description": "Save instagram videos to learn technique",
  "permissions": [
    "tabs",
    "https://www.instagram.com/*",
    "contextMenus",
    "identity"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "oauth2": {
    "client_id": "868405149302-s2amse4vdcj9bqduhjnn1gthiob1phe6.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/bjj_scruber_favicon.png",
      "32": "images/bjj_scruber_favicon32.png",
      "48": "images/bjj_scruber_favicon48.png",
      "128": "images/bjj_scruber_favicon128.png"
    }
  },
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://ajax.googleapis.com https://cdnjs.cloudflare.com; object-src 'self'",
  "icons": {
    "16": "images/bjj_scruber_favicon.png",
    "32": "images/bjj_scruber_favicon32.png",
    "48": "images/bjj_scruber_favicon48.png",
    "128": "images/bjj_scruber_favicon128.png"
  },
  "manifest_version": 2
}
