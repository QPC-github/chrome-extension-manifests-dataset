{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Craigslist in-ad arrows picture switcher",
  "version": "5",
  "manifest_version": 2,
  "description": "adds a few keyboard arrow shortcuts to craigslist",
  "content_scripts": [
    {
      "matches": [
        "http://*.craigslist.org/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "arrows.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "http://*.craigslist.org/*"
  ]
}
