{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TimeConvo",
  "description": "Convert Epoch time to human readable time",
  "version": "1.1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "clipboardRead"
  ]
}
