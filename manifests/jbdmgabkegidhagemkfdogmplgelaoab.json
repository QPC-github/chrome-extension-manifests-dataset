{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CardCounter For Trello with Totalizer",
  "version": "2.84",
  "description": "Card counter for Trello. Show card counters and the total cards in your board!",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "counter.js"
      ],
      "css": [
        "cardstyle.css"
      ],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
