{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.1.5",
  "manifest_version": 2,
  "default_locale": "ru",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://fs.to/*",
