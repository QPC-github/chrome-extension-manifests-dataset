{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_manifestBrowserActionDefaultTitle__"
  },
  "content_scripts": [
    {
      "js": [
        "content3rdParty.js"
      ],
      "matches": [
        "https://www.amazon.com/*"
      ]
    },
    {
      "js": [
        "contentGrabr.js"
      ],
      "matches": [
        "https://grabr.io/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_manifestDescription__",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_manifestName__",
  "permissions": [
    "activeTab"
  ],
  "version": "4.0.33",
  "web_accessible_resources": [
    "index.js",
    "index.css",
    "assets/*"
  ]
}
