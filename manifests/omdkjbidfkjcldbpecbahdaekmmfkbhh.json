{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pulsee",
  "description": "Email tracking application",
  "version": "1.3.45",
  "browser_action": {
    "default_icon": "images/chrome-icon.png",
    "default_popup": "index.html",
    "default_title": "Pulsee"
  },
  "icons": {
    "16": "images/chrome-icon16.png",
    "48": "images/chrome-icon48.png",
    "128": "images/chrome-icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "http://mail.google.com/*"
      ],
      "js": [
        "js/content.js"
      ],
      "css": []
    },
    {
      "matches": [
        "https://pulsee.email/*",
        "http://pulsee.email/*",
        "https://www.pulsee.email/*",
        "http://www.pulsee.email/*",
        "https://account.pulsee.email/*",
        "http://account.pulsee.email/*",
        "https://pp-www.pulsee.email/*",
        "http://pp-www.pulsee.email/*",
        "https://pp-account.pulsee.email/*",
        "http://pp-account.pulsee.email/*",
        "https://stagingaccountpulseeemail.canarium.ph/*",
        "http://stagingaccountpulseeemail.canarium.ph/*",
        "https://stagingwwwpulseeemail.canarium.ph/*",
        "http://stagingwwwpulseeemail.canarium.ph/*"
      ],
      "js": [
        "js/pulsee_content.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/jquery-3.2.1.min.js",
      "js/notifications.js"
    ]
  },
  "web_accessible_resources": [
    "js/jquery-3.2.1.min.js",
    "js/datepicker.min.js",
    "js/dompurify.min.js",
    "js/gmail.min.js",
    "js/jquery.custom.plugins.js",
    "js/moment.min.js",
    "js/platform.min.js",
    "js/qtip.min.js",
    "js/pulsee.plugins.min.js",
    "js/main.js",
    "js/compose.js",
    "css/pulsee.plugins.css",
    "css/style.css",
    "images/email-icon.png",
    "images/logo-blue.png"
  ],
  "permissions": [
    "activeTab",
    "notifications",
    "storage",
    "<all_urls>"
  ]
}
