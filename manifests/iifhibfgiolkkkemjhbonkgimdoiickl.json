{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Oh-o2 Sign-in Simplify",
  "version": "0.1",
  "manifest_version": 2,
  "description": "おーめいじのログイン画面をいきなりビッグに出す!",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://oh-o2.meiji.ac.jp/portal*",
        "https://sso.meiji.ac.jp/rp*"
      ],
      "js": [
        "redirect.js"
      ]
    }
  ]
}
