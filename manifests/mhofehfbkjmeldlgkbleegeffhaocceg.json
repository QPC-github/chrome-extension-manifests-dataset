{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "43",
  "name": "Keyboard Control for Chromeâ„¢",
  "description": "Keyboard shortcuts for navigation and control. Vim like bindings.",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "js/contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    "iframe.html",
    "img/picon.png"
  ],
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "bookmarks",
    "history",
    "pageCapture",
    "clipboardWrite",
    "clipboardRead",
    "downloads",
    "sessions",
    "management",
    "<all_urls>"
  ],
  "options_page": "options.html",
  "content_security_policy": "script-src 'self'; connect-src *; img-src *; frame-src *; object-src 'self'",
  "version": "9.4"
}
