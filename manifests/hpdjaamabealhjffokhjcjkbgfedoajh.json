{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Lovey 收藏工具",
  "version": "5.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "contextMenus"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Lovey",
    "default_icon": "browser_action.png"
  }
}
