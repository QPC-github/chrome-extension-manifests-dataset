{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GRID Backpack",
  "description": "Save pictures and text to your GRID Backpack.",
  "version": "1.0.6",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "Save to GRID"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  }
}
