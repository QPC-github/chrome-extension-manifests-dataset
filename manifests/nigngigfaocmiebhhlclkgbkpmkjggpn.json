{
  "background": {
    "scripts": [
      "bgrnd.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ],
      "all_frames": true,
      "js": [
        "cntnt.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "ReadMyLanguage"
  },
  "description": "Transliterate South Asian Scripts",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Transliterate South Asian Scripts",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.4.1"
}
