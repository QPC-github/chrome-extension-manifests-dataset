{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FleekFramework Cookie Tool",
  "short_name": "FleekCookie",
  "version": "0.0.1",
  "description": "FleekFramework.com",
  "permissions": [
    "cookies",
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "fleek16.png",
    "48": "fleek48.png",
    "128": "fleek128.png"
  },
  "manifest_version": 2
}
