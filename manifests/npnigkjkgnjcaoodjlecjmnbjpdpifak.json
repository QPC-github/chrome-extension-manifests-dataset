{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shakedwn",
  "version": "0.14",
  "manifest_version": 2,
  "description": "Opens Shakedwn at 3.30pm",
  "icons": {
    "128": "IconMainSmall.png"
  },
  "background": {
    "scripts": [
      "jquery-3.1.1.min.js",
      "shakeEvent.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Shakedwn",
    "default_icon": "IconMainSmall.png",
    "default_popup": "shakePopup.html"
  },
  "permissions": [
    "alarms",
    "notifications",
    "background",
    "tabs",
    "storage"
  ]
}
