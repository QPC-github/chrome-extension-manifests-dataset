{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "100 Get Out",
  "description": "Gives Get Out the Rotten Tomatoes Score it Deserves",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.rottentomatoes.com/*",
        "https://www.rottentomatoes.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
