{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Block the Ad-blocker blocker",
  "version": "0.92",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "show_page.js"
      ],
      "matches": [
        "http://indiatoday.intoday.in/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "jquery.min.js",
        "show_toi_page.js"
      ],
      "matches": [
        "http://timesofindia.indiatimes.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "jquery.min.js",
        "show_ht_page.js"
      ],
      "matches": [
        "http://www.hindustantimes.com/*"
      ],
      "run_at": "document_idle"
    }
  ]
}
