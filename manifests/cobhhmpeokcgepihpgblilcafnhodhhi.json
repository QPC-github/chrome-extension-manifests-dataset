{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon16x16.png",
    "default_popup": "popup.html"
  },
  "description": "Build the highest tower you could, dropping blocks on top of each others. The laws of nature can't be broken so watchout!",
  "icons": {
    "128": "icon128x128.png",
    "16": "icon16x16.png",
    "48": "icon48x48.png"
  },
  "manifest_version": 2,
  "name": "Tower Challenge",
  "permissions": [
    "activeTab"
  ],
  "short_name": "Tower Challenge",
  "version": "1.5"
}
