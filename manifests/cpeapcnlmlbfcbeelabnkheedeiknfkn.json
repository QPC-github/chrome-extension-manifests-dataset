{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spacequota",
  "version": "1.1.3",
  "description": "Displays remaining quota for inhabitants of St Peter Wohnheim",
  "default_locale": "en",
  "icons": {
    "16": "LogoSpace_16.png",
    "48": "LogoSpace_48.png",
    "128": "LogoSpace_128.png"
  },
  "browser_action": {
    "default_icon": "LogoSpace_16.png",
    "default_popup": "quota.html"
  },
  "background": {
    "page": "quota.html",
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "http://www.st-peter.stw.uni-erlangen.de/"
  ]
}
