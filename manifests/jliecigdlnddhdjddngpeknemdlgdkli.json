{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Minetrack Chrome",
  "version": "1.2",
  "description": "A Chrome extension for quick views of Minetrack's server listings!",
  "icons": {
    "128": "icons/compass.png"
  },
  "browser_action": {
    "default_icon": "icons/compass.png",
    "default_title": "Minetrack Chrome",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "http://minetrack.me/status.json",
    "http://minetrack.me",
    "http://minetrack.hcherndon.net/versions.json",
    "http://twitter.com/hcherndon",
    "http://twitter.com/CryptkeeperDev",
    "https://chrome.google.com/webstore/detail/minetrack-chrome/jliecigdlnddhdjddngpeknemdlgdkli"
  ]
}
