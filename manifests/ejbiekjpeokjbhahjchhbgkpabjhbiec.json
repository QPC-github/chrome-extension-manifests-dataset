{
  "author": "Raymond Anthony",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "IE Sucks so bad, its actually fun to watch its failures! IE style info bar glows when theres IE handicap code in a page. ad-free!",
  "externally_connectable": {
    "matches": [
      "http://*.rayanthony.io/*"
    ]
  },
  "homepage_url": "http://rayanthony.io/extensions/IE-Sucks/index.html",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "24": "./img/ie-sucks_24x24.png",
      "32": "./img/ie-sucks_32x32.png",
      "64": "./img/ie-sucks_64x64.png"
    },
    "default_action": "infobar.html",
    "default_popup": "popup.html",
    "default_title": "IE Sucks",
    "externally_connectable": {
      "matches": [
        "*.png",
        ".html",
        ".js",
        ".css",
        ".json"
      ]
    },
    "permissions": [
      "webview",
      "infobars",
      "infobar.js",
      "<all_urls>",
      "//platform.twitter.com",
      "//s3-us-west-2.amazonaws.com/webmx-cdn-static/",
      "//webmx.me/assets/",
      "//rayanthony.io/extensions/",
      "//secure-redirection.s3-website-us-west-1.amazonaws.com",
      "//s3-website-us-west-1.amazonaws.com/secure-redirection",
      "https://*/*",
      "infobar",
      "history",
      "tabs"
    ],
    "web_accessible_resources": [
      "/js/*.js",
      "/*.html",
      "manifest.json",
      "infobar.html,",
      "infobar.js",
      "background.js",
      "style.css"
    ]
  },
  "icons": {
    "16": "/img/ie-sucks_16x16.png",
    "18": "/img/ie-sucks_18x18.png",
    "22": "/img/ie-sucks_22x22.png",
    "24": "/img/ie-sucks_24x24.png",
    "32": "/img/ie-sucks_32x32.png",
    "48": "/img/ie-sucks_48x48.png",
    "64": "/img/ie-sucks_64x64.png",
    "96": "/img/ie-sucks_96x96.png",
    "128": "/img/ie-sucks_128x128.png",
    "192": "/img/ie-sucks_192x192.png",
    "256": "/img/ie-sucks_256x256.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "name": "IE Sucks",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "infobars"
  ],
  "short_name": "IE Sucks",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.4"
}
