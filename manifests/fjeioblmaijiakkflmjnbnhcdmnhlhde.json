{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube popup video",
  "short_name": "Youtube popup",
  "description": "Cмотреть youtube видео поверх всех окон!",
  "version": "2.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
