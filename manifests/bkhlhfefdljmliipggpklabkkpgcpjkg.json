{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Web English",
  "version": "3.0",
  "manifest_version": 2,
  "description": "Web English",
  "icons": {
    "16": "web16.png",
    "48": "web48.png",
    "128": "web128.png"
  },
  "browser_action": {
    "default_icon": "web96.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.debrasoft.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.debrasoft.com/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}
