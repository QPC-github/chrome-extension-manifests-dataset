{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TestExtension",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Test Extension to open one link on different screens.",
  "browser_action": {
    "default_icon": {},
    "default_title": "Test Extension"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icons/icon.48.png",
    "128": "icons/icon.128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}
