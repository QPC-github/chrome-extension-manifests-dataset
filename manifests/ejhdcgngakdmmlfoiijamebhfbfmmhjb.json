{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ContextTab",
  "description": "Switch tabs from context menu, for when tabs are obscured",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "ContextTab.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
