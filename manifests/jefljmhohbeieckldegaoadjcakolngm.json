{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Инвента крипто-плагин",
  "short_name": "Inventa crypto",
  "version": "1.0.8",
  "manifest_version": 2,
  "description": "Inventa crypto extension",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "content_script_extension_probe.js"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "*://inventa.su/*",
    "*://crm.inventa.su/*",
    "*://rik.local/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://inventa.su/*",
      "*://crm.inventa.su/*",
      "*://rik.local/*"
    ]
  }
}
