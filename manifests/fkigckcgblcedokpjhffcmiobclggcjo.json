{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OdeCloud extension",
  "version": "0.3.3",
  "description": "One-Click NetSuite Support",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "*://*.odecloud.com/*",
    "*://*.netsuite.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/odecloud-v4.png",
      "32": "images/odecloud-v4.png",
      "48": "images/odecloud-v4.png",
      "128": "images/odecloud-v4.png"
    }
  },
  "icons": {
    "16": "images/odecloud-v4.png",
    "32": "images/odecloud-v4.png",
    "48": "images/odecloud-v4.png",
    "128": "images/odecloud-v4.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
