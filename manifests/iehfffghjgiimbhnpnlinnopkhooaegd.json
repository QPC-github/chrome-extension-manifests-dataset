{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "48x48.png",
    "default_popup": "popup.html",
    "default_title": "Учим английский"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "script_in_content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Учим иностранный язык быстро и легко.",
  "icons": {
    "128": "128x128.png",
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Учим иностранный язык",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "activeTab"
  ],
  "short_name": "Найди пару!",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1"
}
