{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Compartir escritorio - Comunitat Valenciana",
  "description": "Te permite compartir tu escritorio con los demas.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "valencia-logo-16x16.png",
    "48": "valencia-logo-48x48.png",
    "128": "valencia-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://comunitatvalenciana.eternity.online/*"
    ]
  }
}
