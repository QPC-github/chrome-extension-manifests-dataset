{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Player everywhere",
  "version": "0.0.4",
  "description": "Adiciona um media player para todos os arquivos de midia",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "16.png",
    "64": "64.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css",
        "tingle.min.css"
      ],
      "js": [
        "content.js",
        "tingle.min.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
