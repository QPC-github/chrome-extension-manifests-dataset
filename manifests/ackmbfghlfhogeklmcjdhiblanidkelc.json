{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SKULK",
  "description": "skulk",
  "version": "1.7",
  "background": {
    "scripts": [
      "skulk.js"
    ]
  },
  "permissions": [
    "http://www.thebestmog.com/",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
