{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Manager",
  "short_name": "Bookmarker",
  "description": "Bookmark manager",
  "version": "9.1.0",
  "icons": {
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html",
    "default_title": "Bookmark"
  },
  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "<all_urls>",
    "storage"
  ]
}
