{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Groupon coupon",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/button.png"
  },
  "web_accessible_resources": [
    "res/*"
  ],
  "description": "Расширение для быстрого просмотра акций сайта групон. Экономьте время вместе с нами!",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://skiduha.ru/",
  "version": "1.0.10",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/button.png",
    "default_title": "Groupon"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webNavigation",
    "notifications",
    "cookies",
    "webRequest"
  ]
}
