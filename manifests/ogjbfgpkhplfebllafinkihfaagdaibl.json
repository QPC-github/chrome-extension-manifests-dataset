{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Topaz Chrome SigCapture SDK background ext",
  "version": "1.8",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "externally_connectable": {
    "matches": [
      "http://esignemcee.net/ChSigCapSDK/*",
      "https://esignemcee.net/ChSigCapSDK/*",
      "http://www.topazsystems.com/ChromeSD/SigUtil/*"
    ],
    "accepts_tls_channel_id": false
  },
  "permissions": [
    "nativeMessaging"
  ]
}
