{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CoolnCooler - Selected Deals On Every New Tab",
  "description": "CoolnCooler - deliver the best shopping deals on every new tab!",
  "version": "2.6",
  "author": "Thanh Dang",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "CoolnCooler - Selected Deals On Every New Tab!"
  },
  "icons": {
    "16": "res/images/logo/icon16.png",
    "32": "res/images/logo/icon32.png",
    "48": "res/images/logo/icon48.png",
    "96": "res/images/logo/icon96.png",
    "112": "res/images/logo/icon112.png",
    "128": "res/images/logo/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
