{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Root",
  "description": "A better 'new tab' for Chrome",
  "version": "2.0.0",
  "version_name": "2.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "32": "Root-32.png",
    "64": "Root-64.png",
    "128": "Root-128.png",
    "256": "Root-256.png"
  },
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "storage",
    "system.cpu",
    "system.memory",
    "topSites",
    "downloads"
  ]
}
