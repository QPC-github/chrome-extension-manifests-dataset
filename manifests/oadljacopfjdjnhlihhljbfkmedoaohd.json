{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ITHS Screen Share",
  "author": "iths.online",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for https://iths.online/. This is only for screen share.",
  "homepage_url": "https://iths.online/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.iths.online/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
