{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon.png"
    },
    "default_title": "malter"
  },
  "description": "Use markdown to edit and create notes in Evernote",
  "icons": {
    "128": "img/icon.png"
  },
  "manifest_version": 2,
  "name": "malter",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "cookies",
    "https://www.evernote.com/*"
  ],
  "version": "0.9.3"
}
