{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TRAVELDoodle - URLAUBSPLANUNG 2.0",
  "version": "1.1.24",
  "manifest_version": 2,
  "description": "Ihr Browserplugin f√ºr die Reiseplanung",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "*://*.der.lc/user/*",
    "*://*.der-reiseplaner.eu/user/*",
    "http://www.der-reiseplaner.eu/",
    "http://www.traveldoodle.de/",
    "https://www.traveldoodle.de/"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/jquery.min.js",
        "scripts/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "document.html",
    "default_title": "TRAVELDoodle",
    "default_popup": "document.html"
  }
}
