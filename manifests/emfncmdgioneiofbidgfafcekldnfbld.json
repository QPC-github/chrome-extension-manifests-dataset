{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Smuuvr",
  "version": "1.12.3",
  "description": "Smuuvr - For a Smoother UX",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js",
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "content.js",
    "style.css",
    "fonts/*",
    "*.css"
  ],
  "options_ui": {
    "page": "/options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "images/smuuvr_16.png",
    "32": "images/smuuvr_32.png",
    "48": "images/smuuvr_48.png",
    "128": "images/smuuvr_128.png"
  }
}
