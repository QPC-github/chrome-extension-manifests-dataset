{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Notify Me",
  "version": "1.3",
  "description": "Shows desktop notification by making api calls.",
  "icons": {
    "16": "Bitbucket-16.png",
    "48": "Bitbucket-48.png",
    "128": "Bitbucket-120.png"
  },
  "permissions": [
    "notifications",
    "https://*/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "Bitbucket-48.png"
  ]
}
