{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "comjoo screen sharing",
  "description": "A simple extension that allows you to stream your desktop into meetings with comjoo MEET.",
  "version": "0.1.10",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "meet-16x16.png",
    "48": "meet-48x48.png",
    "128": "meet-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet-dev.comjoo.net/*",
      "*://meet.comjoo.com/*",
      "*://meet1.comjoo.com/*"
    ]
  }
}
