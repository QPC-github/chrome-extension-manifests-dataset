{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TAF",
  "description": "Twitter Archive Filter",
  "version": "0.3",
  "content_scripts": [
    {
      "js": [
        "jQuery.min.js",
        "main.js"
      ],
      "css": [
        "taf.css"
      ],
      "matches": [
        "file:///*",
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
