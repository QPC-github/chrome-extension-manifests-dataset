{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Load makorrishon",
  "description": "Adds a makorrishon shortcut to the browser.",
  "version": "1.4",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "open makorrishon web page",
    "default_icon": "print_16x16.png"
  },
  "commands": {
    "open": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "open a link"
    }
  },
  "manifest_version": 2
}
