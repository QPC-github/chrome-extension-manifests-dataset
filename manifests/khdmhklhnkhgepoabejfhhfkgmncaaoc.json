{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "options_page": "options.html",
  "web_accessible_resources": [
    "options.js"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "DIY Bookmarket Manager"
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "name": "DIY Bookmarket Manager",
  "short_name": "DIY Bookmarkets",
  "description": "Do It Yourself Bookmarket Manager",
  "permissions": [
    "management",
    "tabs",
    "storage",
    "activeTab"
  ],
  "version": "1.3"
}
