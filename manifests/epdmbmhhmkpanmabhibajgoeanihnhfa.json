{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Qworum for Google Chromeâ„¢",
  "version": "1.0.2.9",
  "description": "Advanced browser capabilities for application modularization and integration.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_title": "Qworum service call",
    "default_icon": "icons/icon16.png"
  },
  "background": {
    "scripts": [
      "lib/date.js",
      "lib/domain_name.js",
      "lib/drm.js",
      "lib/drm_cache_entry.js",
      "lib/drm_traffic_regulator.js",
      "lib/enterprise.js",
      "lib/enterprise_cache_entry.js",
      "lib/environment.js",
      "lib/interpreter.js",
      "lib/ip_address.js",
      "lib/message.js",
      "lib/uri.js",
      "lib/xml.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "hide.css"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://*.qworum.com/*"
  ]
}
