{
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/ic_mic_gray_36dp.png",
    "name": "Click to start / stop recording"
  },
  "content_scripts": [
    {
      "css": [
        "css/page.css"
      ],
      "js": [
        "js/cs/page.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+9",
        "mac": "Command+Shift+9",
        "chromeos": "Ctrl+Shift+9",
        "linux": "Ctrl+Shift+9"
      }
    },
    "lang1_ks": {
      "suggested_key": {
        "windows": "Ctrl+1",
        "mac": "Command+Shift+1",
        "chromeos": "Ctrl+1",
        "linux": "Ctrl+1"
      },
      "description": "Language 1 Shortcut"
    },
    "lang2_ks": {
      "suggested_key": {
        "windows": "Ctrl+2",
        "mac": "Command+Shift+2",
        "chromeos": "Ctrl+2",
        "linux": "Ctrl+2"
      },
      "description": "Language 2 Shortcut"
    },
    "lang3_ks": {
      "suggested_key": {
        "windows": "Ctrl+3",
        "mac": "Command+Shift+7",
        "chromeos": "Ctrl+3",
        "linux": "Ctrl+3"
      },
      "description": "Language 3 Shortcut"
    }
  },
  "default_locale": "en",
  "description": "__MSG_desc__",
  "icons": {
    "144": "images/icon144.png",
    "128": "images/icon128.png",
    "48": "images/icon48.png"
  },
  "externally_connectable": {
    "matches": [
      "https://*.dictanote.co/*",
      "http://localhost:8000/*"
    ]
  },
  "manifest_version": 2,
  "key": "",
  "name": "__MSG_name__",
  "options_page": "settings.html",
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "background",
    "storage",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "short_name": "__MSG_shortName__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.9",
  "web_accessible_resources": [
    "images/*.png",
    "style/*.css",
    "settings.html",
    "welcome.html"
  ]
}
