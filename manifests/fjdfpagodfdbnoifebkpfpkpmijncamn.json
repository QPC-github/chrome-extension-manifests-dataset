{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Site Analyzer",
  "description": "Analyze SEO factors of website pages, get a report, export your data",
  "version": "0.0.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "static/img/128.png"
  },
  "icons": {
    "16": "static/img/16.png",
    "48": "static/img/128.png",
    "128": "static/img/128.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "activeTab"
  ]
}
