{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "脉可寻-邮件追踪",
  "description": "智能追踪发出邮件的打开状态，打开次数，打开位置等信息。支持网易126邮箱，163邮箱，QQ邮箱等。",
  "version": "1.0.2",
  "icons": {
    "16": "16.png",
    "48": "48.png"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "notifications",
    "storage",
    "*://*.maketion.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-1.7.1.min.js",
        "content_script.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "jquery-1.7.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "脉可寻名片跟踪",
    "default_icon": "16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
