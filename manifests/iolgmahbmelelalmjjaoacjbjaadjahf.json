{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My1stDummyExt",
  "description": "This extension is my first dummy ext",
  "version": "0.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
