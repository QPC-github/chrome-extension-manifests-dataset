{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon_19.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_ext_description__",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.1"
}
