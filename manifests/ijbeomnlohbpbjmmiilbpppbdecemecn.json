{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ayco Meeting Screen Share",
  "description": "Extensión para la compartición de pantalla en Ayco Meeting",
  "version": "0.1.8",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "ayco-logo-16x16.png",
    "48": "ayco-logo-48x48.png",
    "128": "ayco-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meeting.ayco.net/*",
      "*://conferencias.gestores.net/*",
      "*://conferencias.cntravel.es/*",
      "*://conferencias.activahogar.com/*",
      "*://conferences.plymouthrubber.com/*",
      "*://meetings.ayco.net/*"
    ]
  }
}
