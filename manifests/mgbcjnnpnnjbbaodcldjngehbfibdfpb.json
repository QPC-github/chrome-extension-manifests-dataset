{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AuctionStealer Snipe Tool ",
  "short_name": "Snipe Tool!",
  "version": "2018.5.11.1",
  "manifest_version": 2,
  "description": "Places a Snipe button on eBay auction pages. When clicked, the item will be added to our website to be bid on in the final seconds.",
  "homepage_url": "https://www.auctionstealer.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ebay.com/*",
        "*://*.ebay.ca/*",
        "*://*.ebay.co.uk/*",
        "*://*.ebay.de/*",
        "*://*.ebay.it/*",
        "*://*.ebay.ie/*",
        "*://*.ebay.fr/*",
        "*://*.ebay.be/*",
        "*://*.ebay.be/*",
        "*://*.ebay.at/*",
        "*://*.ebay.nl/*",
        "*://*.ebay.es/*",
        "*://*.ebay.ch/*",
        "*://*.ebay.com.au/*",
        "*://*.ebay.com.hk/*",
        "*://*.ebay.in/*",
        "*://*.ebay.com.my/*",
        "*://*.ebay.ph/*",
        "*://*.ebay.com.sg/*"
      ],
      "css": [
        "src/inject.css"
      ],
      "js": [
        "src/inject.js"
      ]
    }
  ]
}
