{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "SqueezeControl",
  "version": "1.2",
  "options_page": "options.html",
  "background": "background.js",
  "manifest_version": 2,
  "description": "An extension to control your squeezebox player from Google Chrome.",
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png",
    "128": "inconlarge.png"
  }
}
