{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Doge",
  "description": "5% chance of doge every time you load page",
  "version": "1.1",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "animate.css",
        "doge.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
