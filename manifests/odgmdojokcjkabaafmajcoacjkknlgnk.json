{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DBpia",
  "description": "DBpia 크롬 확장프로그램으로 참고문헌 관리를 더욱 편리하게",
  "version": "1.1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>"
  ]
}
