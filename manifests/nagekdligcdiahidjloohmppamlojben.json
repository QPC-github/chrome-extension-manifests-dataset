{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fbDarkColor",
  "version": "1.4",
  "description": "Simple dark theme for facebook made as student project. Collects data about percentage of black color on screen.",
  "icons": {
    "128": "icon128.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "scripts": [
      "eventPage.js",
      "jquery-3.3.1.min.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "content.js",
        "jquery-3.3.1.min.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "page_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Tamni fejs!"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "declarativeContent",
    "storage",
    "https://www.facebook.com/*",
    "https://www.jelenacuka.com/coloranalysis/*",
    "http://www.jelenacuka.com/coloranalysis/*",
    "notifications"
  ]
}
