{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Witcher 3 Game HD Wallpapers New Tab",
  "description": "Get newtab background theme with HD wallpapers for every fan of The Witcher 3: Wild Hunt Game",
  "browser_action": {
    "default_icon": "assets/img/icone/128.png",
    "default_title": "The Witcher 3: Wild Hunt Game HD Wallpapers New Tab "
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://wallpapers-chrome.com/favicon.ico",
      "is_default": true,
      "keyword": "wallpapers-chrome.com",
      "name": "wallpapers-chrome.com",
      "search_url": "http://wallpapers-chrome.com/search?q={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "assets/img/icone/128.png"
  },
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "chrome://favicon/*",
    "management",
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "tabCapture",
    "contextMenus",
    "webRequest"
  ],
  "version": "17.19.12",
  "web_accessible_resources": [
    "newtab.html"
  ]
}
