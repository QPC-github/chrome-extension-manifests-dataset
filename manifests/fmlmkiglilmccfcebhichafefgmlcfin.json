{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "19x19.png",
    "default_popup": "popup.html",
    "default_title": "My Aliexpress HELP"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "tabs.js",
        "content.js",
        "lang.js"
      ],
      "matches": [
        "*://*.aliexpress.com/*",
        "*://*.vk.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "description": "My_Aliexpress_HELP",
  "icons": {
    "128": "128x128.png",
    "16": "16x16.png",
    "48": "48x48.png"
  },
  "manifest_version": 2,
  "name": "My_Aliexpress_HELP",
  "permissions": [
    "clipboardWrite",
    "clipboardRead",
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "cookies",
    "browsingData",
    "webNavigation",
    "webRequest",
    "notifications",
    "storage"
  ],
  "short_name": "My_Aliexpress_HELP",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.512",
  "web_accessible_resources": [
    "jquery.min.js",
    "content.js",
    "tabs.js",
    "fb.css",
    "lang.js"
  ]
}
