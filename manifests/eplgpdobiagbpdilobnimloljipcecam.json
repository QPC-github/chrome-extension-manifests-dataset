{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Top10Google",
  "description": "Open first 10 results of your google search automatically",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "addListerners.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.google.ca/*"
      ],
      "js": [
        "jquery-3.2.1.min.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*",
    "https://ajax.googleapis.com/"
  ]
}
