{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Live Feed of Earth from ISS",
  "short_name": "Live Feed of Earth from ISS",
  "description": "NASA Video : Earth From Space Real Footage - Video From The International Space Station ISS",
  "version": "1.0",
  "minimum_chrome_version": "38",
  "offline_enabled": false,
  "permissions": [
    "webview",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "assets/16.png",
    "32": "assets/32.png",
    "64": "assets/64.png",
    "128": "assets/128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
