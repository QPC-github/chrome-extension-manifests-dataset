{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kami - PDF and Document Annotation",
  "short_name": "Kami",
  "description": "Best PDF and Document Annotation and Markup Tool. Works with Google Drive and Google Classroom",
  "version": "2.7",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "notifications"
  ],
  "app": {
    "urls": [
      "https://web.kamihq.com/web/viewer.html"
    ],
    "launch": {
      "web_url": "https://web.kamihq.com/web/viewer.html"
    }
  },
  "offline_enabled": true,
  "container": [
    "GOOGLE_DRIVE",
    "DOMAIN_INSTALLABLE"
  ],
  "api_console_project_id": "185741998891",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/pdf",
          "application/vnd.google.drive.ext-type.pdf"
        ],
        "href": "https://web.kamihq.com/web/viewer.html",
        "title": "Open",
        "disposition": "window"
      }
    ]
  }
}
