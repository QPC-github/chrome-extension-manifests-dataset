{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tab Search",
  "description": "Type 'tab' in Omnibox. You can search tabs.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "tab"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
