{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "intoProxy",
  "version": "2.0",
  "description": "intoProxy Chrome Extension",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "ip16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "ip16.png",
    "32": "ip32.png",
    "48": "ip48.png",
    "128": "ip128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "proxy",
    "storage"
  ],
  "manifest_version": 2
}
