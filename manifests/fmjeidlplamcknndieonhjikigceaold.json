{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JoeMonster.org Szaffa",
  "version": "0.3.9",
  "description": "Wysy≈Çanie obrazka do Szaffy",
  "background": {
    "scripts": [
      "jquery.js",
      "joe.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "JoeMonster Szaffa"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "icon.png",
    "24": "joe48.png",
    "48": "joe48.png",
    "128": "joe128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://joemonster.org/*"
      ],
      "js": [
        "jquery.js",
        "pimpMyJoe.js"
      ]
    }
  ]
}
