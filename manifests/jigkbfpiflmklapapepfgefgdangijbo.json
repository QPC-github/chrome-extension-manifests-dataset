{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Track this!",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "An extention to easily track your packages!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ]
}
