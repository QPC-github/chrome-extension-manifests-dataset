{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YT Suggestion Hider",
  "description": "Hides the annoying suggested video popups at the end of a YouTube video",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
