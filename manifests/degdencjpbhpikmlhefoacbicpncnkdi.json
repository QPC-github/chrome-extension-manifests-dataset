{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ProHance Extension",
  "description": "This extension captures active tab's url.",
  "version": "3.0",
  "permissions": [
    "tabs",
    "alarms",
    "activeTab",
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "Test.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "24": "icons/PH24X24.png"
    }
  },
  "manifest_version": 2
}
