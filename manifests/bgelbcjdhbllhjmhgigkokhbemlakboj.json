{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Biroliro da Havan",
  "version": "0.1",
  "description": "Muda o nome do Véio da Havan para 'Vèio da Havan' e Bolsonaro para 'Biroliro'",
  "icons": {
    "16": "images/veio-16.png",
    "48": "images/veio-48.png",
    "128": "images/veio-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
