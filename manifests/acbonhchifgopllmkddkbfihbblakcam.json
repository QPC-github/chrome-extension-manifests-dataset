{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Overflow It!",
  "version": "1.0",
  "description": "So those bastards blocked the body scroll? No problem, click the button or add blacklists",
  "manifest_version": 2,
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
