{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Art - Cultivate your Culture",
  "version": "2.0.3",
  "author": "Barthelemy de Mazenod",
  "icons": {
    "16": "./assets/images/tab-icon.png",
    "48": "./assets/images/tab-icon.png",
    "128": "./assets/images/tab-icon.png"
  },
  "browser_action": {
    "default_icon": "./assets/images/tab-icon.png",
    "default_title": "Tab Art"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
