{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Media tab selector",
  "description": "This extension lets you quickly navigate to the tab that has media playing",
  "version": "0.2",
  "minimum_chrome_version": "45",
  "icons": {
    "38": "images/computer_38.png",
    "128": "images/computer_128.png"
  },
  "browser_action": {
    "default_title": "Mute audio"
  },
  "background": {
    "scripts": [
      "js/chrome-promise.js",
      "js/eventPage.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}
