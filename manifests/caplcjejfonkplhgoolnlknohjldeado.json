{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chronological Timeline for Twitter",
  "version": "0.0.0.8",
  "description": "Prevents Twitter from automatically switching your timeline from the 'Latest Tweets' mode back to the 'Home' mode.",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "webNavigation",
    "http://twitter.com/home",
    "https://twitter.com/home",
    "https://mobile.twitter.com/home",
    "http://mobile.twitter.com/home",
    "https://m.twitter.com/home",
    "http://m.twitter.com/home"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "manifest_version": 2
}
