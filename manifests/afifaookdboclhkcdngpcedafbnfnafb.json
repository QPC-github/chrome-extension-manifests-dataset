{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "3.9.3",
  "description": "__MSG_appDescription__",
  "author": {
    "name": "www.vod.at.dz",
    "birth": 2017
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.vod.at.dz:*/*.jsp*"
    ]
  }
}
