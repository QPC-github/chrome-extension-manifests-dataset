{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "google.js"
      ],
      "matches": [
        "*://www.google.com/",
        "*://www.google.fr/",
        "*://www.google.be/",
        "*://www.google.ch/",
        "*://www.google.us/",
        "*://www.google.uk/",
        "*://www.google.es/",
        "*://www.google.com/*hp*",
        "*://www.google.fr/*hp*",
        "*://www.google.be/*hp*",
        "*://www.google.ch/*hp*",
        "*://www.google.us/*hp*",
        "*://www.google.uk/*hp*",
        "*://www.google.es/*hp*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Remplacez les liens inutiles du bas de la page d'accueil de google par un Kamoulox, c'est bien plus fun !",
  "homepage_url": "http://twitter.com/MrPointVirgule",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Google Kamoulox",
  "version": "3.0"
}
