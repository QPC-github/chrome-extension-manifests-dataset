{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fidget Spinner - Chrome Edition",
  "short name": "Chrome Spinner",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "jqueryKeyframes.js",
        "fidget.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "fidgetSpinner.png",
    "woosh.mp3"
  ],
  "browser_action": {
    "default_icon": "fidgetSpinner.png"
  }
}
