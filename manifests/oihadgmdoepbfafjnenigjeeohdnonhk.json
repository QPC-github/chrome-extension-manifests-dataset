{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Fullscreen fixer.",
  "manifest_version": 2,
  "name": "Fullscreen Fixer",
  "permissions": [
    "contentSettings",
    "tabs"
  ],
  "web_accessible_resources": [
    "icon16.png"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Allow fullscreen",
    "default_popup": "popup.html"
  },
  "version": "0.31"
}
