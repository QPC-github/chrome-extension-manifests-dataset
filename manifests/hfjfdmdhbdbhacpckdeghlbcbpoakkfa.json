{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VinesauceAlert",
  "author": "Norix Wolfe",
  "options_page": "options.html",
  "version": "0.1.9.2",
  "description": "This official app notifies you when anyone on the Vinesauce Team goes online on Twitch!",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "VinesauceAlert",
    "default_icon": "icon_128.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "run.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "*://static-cdn.jtvnw.net/*",
    "*://*.vineshroom.net/*"
  ],
  "web_accessible_resources": [
    "icon_128.png"
  ]
}
