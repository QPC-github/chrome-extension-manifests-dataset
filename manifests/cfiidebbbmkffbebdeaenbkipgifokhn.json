{
  "author": "KodingKamp",
  "background": {
    "persistent": true,
    "scripts": [
      "NuDash.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "NuDash.html"
  },
  "description": "Replaces New Tab page with a page with quicklinks, search bar, date & time, and random quotes. Icons made by Abdulrahim Bajaber.",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "32": "32.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://clients1.google.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "name": "NuTab Dashboard",
  "short_name": "NuDash",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6"
}
