{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "description": "This extension will enable and fix common issues for Flash when using Time4Learning.",
  "externally_connectable": {
    "matches": [
      "*://*.time4learning.com/*",
      "*://*.time4writing.com/*",
      "*://*.thelearningodyssey.com/*"
    ]
  },
  "icons": {
    "128": "t4l-chrome-icon-128x128.png"
  },
  "manifest_version": 2,
  "name": "Time4Learning",
  "permissions": [
    "tabs",
    "storage",
    "contentSettings",
    "activeTab",
    "http://*.thelearningodyssey.com/*",
    "https://*.thelearningodyssey.com/*",
    "https://*.time4learning.com/*",
    "https://*.time4writing.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0"
}
