{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Temp Extension 3",
  "description": "Allows to extract links from a page",
  "version": "0.0.0.1",
  "page_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Temp Extension 3",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*",
        "http://localhost/*"
      ],
      "js": [
        "le_cs.js",
        "jquery.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "cookies",
    "tabs",
    "http://localhost/*",
    "https://*/*",
    "http://*/*"
  ]
}
