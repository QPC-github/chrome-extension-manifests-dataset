{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bouncy",
  "short_name": "Bouncy",
  "version": "1.0",
  "description": "Send the current URL to another web service.",
  "icons": {
    "16": "icon-16.png",
    "19": "icon-19.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "contextMenu.js"
    ]
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "storage",
    "contextMenus",
    "notifications"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
