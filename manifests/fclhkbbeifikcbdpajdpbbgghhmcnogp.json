{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "core/controller/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Portal Planet",
    "default_icon": "icon.png",
    "default_popup": "core/view/portalplanet.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "News feed for Portal Planet",
  "icons": {
    "16": "icon.png",
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Portal Planet News",
  "permissions": [
    "http://*.yahooapis.com/*"
  ],
  "version": "0.94"
}
