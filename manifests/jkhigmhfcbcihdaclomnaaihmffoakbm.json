{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dia Zue√±o",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A program that allows you to unplug from all the information you're constantly being bombarded with and let your mind flow freely.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
