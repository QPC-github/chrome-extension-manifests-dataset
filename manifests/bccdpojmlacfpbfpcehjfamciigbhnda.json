{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GoT AntiSpoiler for vk.com",
  "short_name": "gotAS",
  "description": "Блокировщик спойлеров сериала «Игра престолов» в личных сообщениях ВКонтакте",
  "version": "0.2",
  "icons": {
    "16": "icons/icon-on.png",
    "32": "icons/icon-on.png",
    "48": "icons/icon-on.png",
    "128": "icons/icon-on.png"
  },
  "permissions": [
    "*://vk.com/*",
    "activeTab"
  ],
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
