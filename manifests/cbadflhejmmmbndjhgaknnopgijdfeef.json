{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Magnify Image",
  "description": "Hover to zoom in on an image.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "extension.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-2.0.3.min.map"
  ]
}
