{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sí sí",
  "description": "This extension changes all appearances of 'si' by 'SI' and 'se' by 'SE'",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "sisi.js"
      ]
    }
  ],
  "permissions": [],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
