{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Social Search",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Add a 'Social' tab to searches on Google.com",
  "icons": {
    "128": "icon_128.jpg",
    "48": "icon_48.jpg",
    "16": "icon_16.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "socialsearch.js"
      ]
    }
  ]
}
