{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OpenTabs",
  "version": "2.41",
  "description": "\n Making an Impact Has Never Been Easier ",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "external.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs"
  ]
}
