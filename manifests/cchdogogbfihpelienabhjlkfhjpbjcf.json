{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NTA Click to Dial",
  "version": "1.3",
  "icons": {
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "mutation-summary.js",
        "jquery-3.1.0.min.js",
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file://*/*",
    "storage",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_title": ""
  },
  "background": {
    "scripts": [
      "jquery-3.1.0.min.js",
      "bg.js"
    ]
  },
  "web_accessible_resources": [
    "images/*.png",
    "options.html"
  ]
}
