{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hoverlytics",
  "short_name": "Hoverlytics",
  "version": "0.9.1",
  "manifest_version": 2,
  "description": "Hoverlytics Â· Hover-over Google Analytics In Your Site",
  "homepage_url": "http://www.burntcaramel.com/hoverlytics/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "Hoverlytics"
  },
  "permissions": [
    "chrome://favicon/",
    "activeTab",
    "contextMenus",
    "cookies",
    "storage",
    "http://*.burntcaramel.com/",
    "https://*.burntcaramel.com/",
    "http://*.hoverlytics.com/",
    "https://*.hoverlytics.com/"
  ]
}
