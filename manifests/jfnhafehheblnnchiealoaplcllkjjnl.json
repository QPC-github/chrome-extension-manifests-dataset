{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "135 List",
  "description": "View and modify your 135 List.",
  "version": "1.4",
  "icons": {
    "48": "icon48.png",
    "96": "icon.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "135 List"
  },
  "permissions": [
    "contextMenus",
    "http://www.135list.com/"
  ]
}
