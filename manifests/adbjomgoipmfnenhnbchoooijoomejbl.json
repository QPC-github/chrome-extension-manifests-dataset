{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "A1電商好幫手",
  "version": "1.1.6",
  "description": "簡單幾個步驟，賣家就能迅速回覆評價。",
  "permissions": [
    "cookies",
    "https://developer.chrome.com/*",
    "https://a1cloud.digiwin.com/*",
    "http://a1cloud.digiwin.com/*",
    "https://a1web.azurewebsites.net/*",
    "http://a1web.azurewebsites.net/*",
    "http://localhost:4200/*",
    "https://a1cloudwebapi01.azurewebsites.net/*",
    "https://A1cdnpoint.azureedge.net/*",
    "https://a1trial.digiwin.com/*",
    "https://seller.shopee.tw/*",
    "https://ssl.google-analytics.com/*",
    "https://maxcdn.bootstrapcdn.com/*",
    "accessibilityFeatures.read",
    "accessibilityFeatures.modify",
    "storage",
    "tabs"
  ],
  "background": {
    "page": "index.html#/event-page",
    "persistent": true
  },
  "browser_action": {
    "default_title": "A1電商好幫手",
    "default_popup": "index.html#/popup"
  },
  "options_page": "index.html#/main",
  "icons": {
    "128": "assets/icons/icon_128x128-3.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com ; object-src 'self'"
}
