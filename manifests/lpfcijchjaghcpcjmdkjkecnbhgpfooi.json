{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "El reves de Vilas",
  "version": "1.5",
  "manifest_version": 2,
  "description": "El tenista argentino Guillermo Vilas fue el mejor jugador de 1977, salvo para la ATP. La extensión cambia el Nº 1 del ranking.",
  "options_ui": {
    "chrome_style": true,
    "page": "settings.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/inject.min.js"
      ],
      "matches": [
        "https://www.tennis-x.com/*",
        "https://www.cbssports.com/*",
        "https://en.wikipedia.org/*",
        "https://uk.reuters.com/*"
      ],
      "exclude_matches": [
        "*://developer.mozilla.org/*",
        "*://twitter.com/intent/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "GuilleVillas",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.tennis-x.com/*",
    "https://www.cbssports.com/*",
    "https://en.wikipedia.org/*",
    "https://uk.reuters.com/*",
    "storage"
  ]
}
