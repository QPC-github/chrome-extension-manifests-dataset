{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Winithemes Cash Back",
  "description": "Winithemes Cash Back - the cash saver for premium themes and templates. Get 10% cash back from the total amount of your purchases.",
  "version": "1.1.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "xtend.js"
      ],
      "run_at": "document_start"
    }
  ]
}
