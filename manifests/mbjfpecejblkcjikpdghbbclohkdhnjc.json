{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Subreddit on Title",
  "version": "1.0.0",
  "description": "Shows the subreddit name in the title of links. RES-compatible.",
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*",
        "*://reddit.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://*.reddit.com/*",
    "*://reddit.com/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
