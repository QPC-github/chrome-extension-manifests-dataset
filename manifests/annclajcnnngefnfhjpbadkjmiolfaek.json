{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stripe",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Post your stripe status, music, photos and more in just a click of a button.",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "Stripe",
