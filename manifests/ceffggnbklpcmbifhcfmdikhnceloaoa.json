{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.3",
  "name": "Imieniny. Życzenia na każdą okazję.",
  "short_name": "Imieniny i życzenia.",
  "description": "Czyje dziś imieniny? Jakie są dziś święta i komu można czegoś dobrego życzyć? Solenizanci zapamiętają że Ty pamiętałeś o nich :)",
  "author": "Ciapek.pl",
  "homepage_url": "http://www.zyczenia.ciapek.pl/google_chrome_extension-imieniny/",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "google_chrome_extension_file-imieniny.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "http://www.zyczenia.ciapek.pl/",
    "https://www.google-analytics.com/"
  ]
}
