{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Free One Touch VPN",
  "short_name": "proxy-spider-vpn",
  "description": "Free One Touch VPN",
  "author": "Proxy-Spider.com",
  "version": "1.0.5",
  "manifest_version": 2,
  "default_locale": "en",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "activeTab",
    "https://proxy-spider.com/*",
    "http://ip-api.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png"
    },
    "default_popup": "data/panel/index.html"
  },
  "background": {
    "scripts": [
      "common.js"
    ],
    "persistent": true
  },
  "homepage_url": "https://proxy-spider.com/private-proxy-chrome-extension",
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  }
}
