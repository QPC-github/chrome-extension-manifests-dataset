{
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://search.moccini.com/favicon.ico",
      "is_default": true,
      "keyword": "search.moccini.com",
      "name": "Web",
      "search_url": "http://search.moccini.com/?isds=1&publisher=defaultmoccini&barcodeid=50120999&pdid=APPVR&st=ds&q={searchTerms}"
    }
  },
  "background": {
    "scripts": [
      "redirect.js"
    ]
  },
  "description": "This extension is a part of Moccini Codeset package for Chrome browser and is responsible for search settings.",
  "icons": {
    "16": "PublisherImages/Mocc16.png",
    "48": "PublisherImages/Mocc48.png",
    "64": "PublisherImages/Mocc64.png",
    "128": "PublisherImages/Mocc128.png"
  },
  "manifest_version": 2,
  "name": "Moccini DS",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
