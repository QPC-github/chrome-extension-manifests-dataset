{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Guizmus' Hordes script",
  "description": "",
  "version": "1.3.4",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "unlimitedStorage"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.hordes.fr/*"
      ],
      "js": [
        "sab_hordes.user.js"
      ],
      "run_at": "document_end"
    }
  ]
}
