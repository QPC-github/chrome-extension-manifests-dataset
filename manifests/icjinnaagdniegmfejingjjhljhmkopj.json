{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Inkscape on rollApp",
  "description": "Powerful vector graphics editor featuring transparency, gradients, node editing, pattern fills, and more. Powered by rollApp.",
  "version": "1.2",
  "app": {
    "urls": [
      "*://www.rollapp.com/launch/inkscape",
      "*://www.rollapp.com/app/inkscape"
    ],
    "launch": {
      "web_url": "https://www.rollapp.com/launch/inkscape?ref=chrome-app",
      "container": "panel",
      "width": 900,
      "height": 600
    }
  },
  "icons": {
    "16": "inkscape-16.png",
    "128": "inkscape-128.png"
  }
}
