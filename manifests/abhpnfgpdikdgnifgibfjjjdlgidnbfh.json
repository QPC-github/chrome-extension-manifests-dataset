{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "dev_connected.png",
    "default_title": "DroidWireless",
    "default_popup": "popup.html"
  },
  "description": "Manage your android phone from Browser",
  "name": "DroidWireless",
  "permissions": [
    "alarms",
    "webNavigation",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.1.0",
  "manifest_version": 2
}
