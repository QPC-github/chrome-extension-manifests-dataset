{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "whoz.png",
    "default_popup": "index.html#/popup",
    "default_title": "Whoz"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Capturez des profils LinkedIn pour alimenter votre CVthèque et bénéficiez de notre analyse de profil et détection des compétences.",
  "icons": {
    "48": "whoz.png"
  },
  "manifest_version": 2,
  "name": "Whoz",
  "options_page": "index.html#/options",
  "permissions": [
    "activeTab",
    "identity",
    "storage",
    "tabs",
    "https://*.linkedin.com/",
    "https://*.whoz.com/"
  ],
  "version": "2.0.5"
}
