{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TwitterDemote",
  "description": "Simply and easily removes any promoted element on Twitter",
  "author": "Guichi",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "*://*.twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "css": [
        "inject.css"
      ]
    }
  ]
}
