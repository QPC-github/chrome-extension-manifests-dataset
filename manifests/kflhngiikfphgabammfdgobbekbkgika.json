{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitterぼかし＋",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "DMで投稿された画像/動画＆ツイートされた画像/動画をぼかします",
  "web_accessible_resources": [
    "twitter_mosaic.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "twitter_mosaic.css"
      ]
    }
  ],
  "icons": {
    "128": "128.png"
  }
}
