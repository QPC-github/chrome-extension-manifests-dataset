{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Epsilon",
  "description": "A Google Chrome Extension to enhance student productivity.",
  "version": "1",
  "author": "Jeremy Nobody",
  "browser_action": {
    "default_icon": "epsilon.png",
    "default_title": "Study hard"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "epsilon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
