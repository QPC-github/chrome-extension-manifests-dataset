{
  "manifest_version": 2,
  "name": "Diario de Avisos",
  "version": "0.1",
  "description": "Las noticias al momento con Diario de Avisos, Decano de la Prensa de Canarias",
  "icons": {
    "16": "images/icon.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "homepage_url": "http://www.diariodeavisos.com/",
  "browser_action": {
    "default_icon": "images/48.png",
    "default_title": "Diario de Avisos",
    "default_popup": "da.html"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
