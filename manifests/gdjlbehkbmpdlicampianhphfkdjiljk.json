{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Caithe Support Add-on",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Caithe.com Support Plugin!",
  "page_action": {
    "default_icon": "icon128.png",
    "default_title": "Thanks for supporting me!"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.amazon.com/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
