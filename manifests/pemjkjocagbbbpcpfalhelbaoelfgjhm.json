{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Worksheet Wizard",
  "description": "Premier Worksheet Wizard",
  "version": "1.2",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
