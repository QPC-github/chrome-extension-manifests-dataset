{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Bandcamp Fanpage Albumcolors",
  "description": "Adds Albumcolors to Bandcamp.com Fanpages",
  "manifest_version": 2,
  "version": "1.0.0",
  "permissions": [
    "http://*.bcbits.com/*"
  ],
  "minimum_chrome_version": "22",
  "content_scripts": [
    {
      "matches": [
        "http://bandcamp.com/*"
      ],
      "js": [
        "lib/jquery.js",
        "app.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "lib/albumcolors.js",
      "background.js"
    ],
    "persistent": false
  }
}
