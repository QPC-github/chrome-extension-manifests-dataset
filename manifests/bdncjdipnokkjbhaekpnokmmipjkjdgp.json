{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Newton",
  "version": "1.1",
  "description": "A little inspiration, a little fun, on every new tab.",
  "homepage_url": "https://twitter.com/k3sava",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
