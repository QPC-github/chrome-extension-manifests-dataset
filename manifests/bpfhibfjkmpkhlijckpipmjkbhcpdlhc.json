{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WP Contentio",
  "manifest_version": 2,
  "short_name": "wpcontentio",
  "description": "3-Step Tool Generates Google-Friendly High Authority Content For Your Website In Less Than 60-Seconds.",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_19-1486839855.png"
  },
  "icons": {
    "128": "128px_19-1486839861.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
