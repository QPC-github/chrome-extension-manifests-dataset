{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoDzADB",
  "version": "1.0",
  "description": "GoDzM4TT3O's Ad Blocker",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "blocked_domains.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
