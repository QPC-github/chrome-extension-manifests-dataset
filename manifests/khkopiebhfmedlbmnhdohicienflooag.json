{
  "name": "GWars.Ru: уведомления о событиях в игре",
  "short_name": "GWars.ru",
  "description": "Уведомления о событиях в игре: о новой почте, начале боя, получении посылки, событии на ферме, изменении здоровья персонажа",
  "browser_action": {
    "default_icon": {
      "16": "icon16x16.png",
      "32": "icon32x32.png",
      "48": "icon48x48.png",
      "64": "icon64x64.png",
      "80": "icon80x80.png",
      "144": "icon.png",
      "128": "icon128x128.png"
    },
    "default_title": "GWars.ru",
    "default_popup": "gw.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-latest.js",
      "bg.js"
    ]
  },
  "permissions": [
    "notifications",
    "alarms",
    "http://www.gwars.ru/",
    "https://www.gwars.ru/"
  ],
  "web_accessible_resources": [
    "icon16x16.png",
    "icon128x128.png",
    "icon.png",
    "icon48x48.png",
    "icon32x32.png",
    "icon64x64.png",
    "icon80x80.png"
  ],
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self' https://www.google.com",
  "version": "3.14159.32"
}
