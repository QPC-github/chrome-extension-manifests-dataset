{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Xone",
  "permissions": [
    "contextMenus"
  ],
  "version": "2",
  "version_name": "alpha 2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js",
      "contexts.js"
    ]
  }
}
