{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fuzzypop",
  "short_name": "fuzzypop",
  "description": "A lightweight CRM.",
  "version": "0.0.3",
  "icons": {
    "19": "icon.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "fuzzypop.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
