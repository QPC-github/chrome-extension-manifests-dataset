{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "version": "4.0.16",
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "38": "icon/38.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "strage.js",
        "main.js"
      ]
    }
  ]
}
