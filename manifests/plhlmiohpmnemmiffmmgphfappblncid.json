{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Star Wars Spoilers",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
