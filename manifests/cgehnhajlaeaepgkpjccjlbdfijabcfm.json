{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My Top Twenty",
  "version": "1.0",
  "description": "My Top Twenty New Tab Page",
  "short_name": "My Top Twenty New Tab Page",
  "permissions": [
    "topSites",
    "http://*/",
    "https://*/"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com ; object-src 'self'"
}
