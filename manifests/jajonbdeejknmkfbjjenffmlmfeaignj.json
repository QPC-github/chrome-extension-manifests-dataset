{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wikipedia Search by PiQue",
  "description": "Search Wikipedia",
  "version": "0.2.40",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Search Wikipedia",
    "default_icon": "wikipuzzle16.png",
    "default_popup": "search.html"
  },
  "icons": {
    "16": "wikipuzzle16.png",
    "128": "wikipuzzle128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://wikipedia.org/*"
  ],
  "options_page": "options.html"
}
