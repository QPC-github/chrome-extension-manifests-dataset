{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inversifier",
  "description": "Make web content look normal again when inverting screen colors.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Invert colors."
  },
  "manifest_version": 2
}
