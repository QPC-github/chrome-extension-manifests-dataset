{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Desbrunalizador",
  "description": "Desbrunaliza as not√≠cias.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "name": "Desbrunalizador",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
