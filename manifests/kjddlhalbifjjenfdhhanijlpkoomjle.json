{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OMG Sales Assessments",
  "description": "Send links to candidates for employment and view assessment results while accessing your Applicant Tracking Systems.",
  "version": "1.0.0",
  "icons": {
    "128": "OMG128.png",
    "48": "OMG48.png",
    "16": "OMG16.png"
  },
  "browser_action": {
    "default_icon": "OMG16.png"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab"
  ],
  "optional_permissions": [
    "tabs",
    "*://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "page": "eventPage.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://info.objectivemanagement.com/CECheck.aspx"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "contentScript.js"
      ]
    }
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Runs"
    }
  }
}
