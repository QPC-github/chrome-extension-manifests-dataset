{
  "background": {
    "script": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "boton-de-reproduccion.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Opens hello.html",
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  },
  "content_scripts": [
    {
      "js": [
        "options.js",
        "popup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "extension use atacado",
  "key": "",
  "manifest_version": 2,
  "name": "adsExtensionsAtacado",
  "options_page": "options.html",
  "permissions": [
    "background",
    "https://www.atacadogames.com/*",
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.4"
}
