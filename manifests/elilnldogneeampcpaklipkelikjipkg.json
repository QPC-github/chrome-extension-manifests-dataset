{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HS.fi autologin",
  "version": "0.0.1",
  "description": "Automatically log in to hs.fi using aalto.fi-sso",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.hs.fi/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}
