{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/jquery.js",
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "淘宝优惠券助手",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/home.css"
      ],
      "js": [
        "js/jquery.js",
        "js/show_win.js",
        "js/taobao.js"
      ],
      "matches": [
        "*://*.taobao.com/*"
      ]
    },
    {
      "css": [
        "css/home.css"
      ],
      "js": [
        "js/jquery.js",
        "js/show_win.js",
        "js/tmall.js"
      ],
      "matches": [
        "*://*.tmall.com/*",
        "*://*.tmall.hk/*"
      ]
    },
    {
      "css": [
        "css/jd.css"
      ],
      "js": [
        "js/jquery.js",
        "js/jd.js"
      ],
      "matches": [
        "*://*.jd.com/*",
        "*://*.jd.hk/*"
      ]
    },
    {
      "js": [
        "js/mogujie.js"
      ],
      "matches": [
        "*://*.mogu.com/*"
      ]
    },
    {
      "js": [
        "js/kaola.js"
      ],
      "matches": [
        "*://*.kaola.com/*"
      ]
    }
  ],
  "description": "搜券助手专为用户查找淘宝、天猫商品内部优惠券，商品店铺优惠券信息，让您花更少的钱购买同样的商品",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "搜券助手",
  "permissions": [
    "https://www.360soquan.com/",
    "https://cart.taobao.com/",
    "http://api.yqfei365.com/",
    "https://cd.jd.com/"
  ],
  "version": "5.9",
  "web_accessible_resources": [
    "images/*"
  ]
}
