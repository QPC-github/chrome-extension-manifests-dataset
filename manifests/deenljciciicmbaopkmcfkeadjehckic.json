{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Leonardo Ciaccio",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "object-src 'self'; script-src 'self' https://ssl.google-analytics.com; connect-src *",
  "default_locale": "en",
  "description": "__MSG_mex01__",
  "homepage_url": "https://www.facebook.com/ILoveAmazonExtension/",
  "icons": {
    "128": "img/ILA.128.png",
    "16": "img/ILA.16.png",
    "24": "img/ILA.24.png",
    "256": "img/ILA.256.png",
    "48": "img/ILA.48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34.0.0.0",
  "name": "I Love Amazon",
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "*://*/*"
  ],
  "version": "1.1.6",
  "web_accessible_resources": [
    "console.html"
  ]
}
