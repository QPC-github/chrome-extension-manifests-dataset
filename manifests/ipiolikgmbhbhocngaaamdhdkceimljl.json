{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "comment.html",
    "default_title": "Send page comment"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Page Commenter is the easy way to manage comments on a web page with your clients or team mates",
  "name": "Ester Liquori Design - Page Commenter",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "<all_urls>",
    "storage"
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.4.1"
}
