{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.7",
  "manifest_version": 2,
  "name": "Mute Inactive Tabs",
  "short_name": "Mute Inactive Tabs",
  "description": "An extension that mutes sound from tabs except the current tab that's playing sound.",
  "author": "Jyothi Babu Araja",
  "homepage_url": "https://github.com/arajajyothibabu/mute-inactive-tabs",
  "minimum_chrome_version": "45",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "default_title": "Unmute Inactive Tabs"
  },
  "permissions": [
    "tabs",
    "management",
    "http://*/*",
    "https://*/*"
  ]
}
