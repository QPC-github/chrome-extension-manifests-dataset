{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nonstop for Google™ Search",
  "version": "1.2.1",
  "description": "Nonstop for Google™ Search",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "script/jquery.js",
        "script/content.js"
      ],
      "matches": [
        "http://*.google.com/*",
        "https://*.google.com/*",
        "http://*.google.com.hk/*",
        "https://*.google.com.hk/*"
      ]
    }
  ]
}
