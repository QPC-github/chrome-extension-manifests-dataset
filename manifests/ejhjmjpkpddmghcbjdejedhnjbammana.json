{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "This changes the Google+ stream image to small size and more.",
  "name": "PetitPhoto+",
  "version": "3.0.7",
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://plus.google.com/*"
      ],
      "css": [
        "gcview.css"
      ],
      "js": [
        "jquery.min.js",
        "gcview.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
