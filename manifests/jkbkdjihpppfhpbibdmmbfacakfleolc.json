{
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "Управляй контактом, как Бог!"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Управляйте контактом, как Пикабу, с помощью A,D,Z,C,W,S",
  "background": {
    "scripts": [
      "jquery.js"
    ]
  },
  "name": "Управляй VK, как Pikabu",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "favicon.ico",
    "16": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "popup.js",
        "jquery.js"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "1.1.6"
}
