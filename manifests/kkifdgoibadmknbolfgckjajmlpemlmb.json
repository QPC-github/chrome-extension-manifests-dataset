{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "낙도 핫키",
  "description": "낙도 구글검색 단축키. Shift+Alt+[G(구글분석),D(다음분석)], N(검색결과열기), M(닫기)]",
  "version": "1.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon_big.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/jquery.js",
        "js/content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "commands": {
    "parse_google": {
      "suggested_key": {
        "default": "Alt+Shift+G",
        "mac": "Command+Shift+G"
      },
      "description": "Google Page Parsing"
    },
    "parse_daum": {
      "suggested_key": {
        "default": "Alt+Shift+D",
        "mac": "Command+Shift+D"
      },
      "description": "Daum Page Parsing"
    },
    "next-tab": {
      "suggested_key": {
        "default": "Alt+Shift+N",
        "mac": "Command+Shift+N"
      },
      "description": "Next Google Search Result"
    },
    "close-tab": {
      "suggested_key": {
        "default": "Alt+Shift+M",
        "mac": "Command+Shift+M"
      },
      "description": "Close Tab"
    }
  }
}
