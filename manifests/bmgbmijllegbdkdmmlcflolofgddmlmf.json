{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ActiveReload for SiteSpect",
  "description": "Clears All Browser Cache and cookies for active domain but keeping SSOE and then reloads the active tab",
  "version": "1.0",
  "browser_action": {
    "default_icon": "reload.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "browsingData"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "reload_16.png",
    "48": "reload_48.png",
    "128": "reload_128.png"
  }
}
