{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "http://247taobao.com/",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Đặt hàng taobao, tmall, 1688...",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "247taobao.js"
      ],
      "matches": [
        "https://*.tmall.com/*",
        "https://*.taobao.com/*",
        "https://*.1688.com/*"
      ]
    }
  ],
  "permissions": [
    "https://*.tmall.com/*",
    "https://*.taobao.com/*",
    "https://*.1688.com/*"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png"
  }
}
