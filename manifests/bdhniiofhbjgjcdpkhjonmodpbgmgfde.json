{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Livejournal Narrow",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Simple user-script which makes livejournal.com a better place",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "http://*.livejournal.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
