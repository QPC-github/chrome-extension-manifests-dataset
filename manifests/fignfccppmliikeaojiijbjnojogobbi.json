{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Boruto's Dad",
  "version": "1.0.1",
  "description": "Replaces all instances of \"Naruto\" with \"Boruto's Dad\".",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
