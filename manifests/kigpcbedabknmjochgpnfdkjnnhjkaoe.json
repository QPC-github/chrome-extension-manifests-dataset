{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pumm - 펌",
  "description": "Pumm에 링크를 바로 올릴 수 있는 확장 프로그램입니다.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "Pumm - 펌"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
