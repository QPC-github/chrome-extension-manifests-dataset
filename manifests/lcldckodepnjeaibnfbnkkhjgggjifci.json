{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IPWhois Lookup",
  "version": "1.0",
  "description": "The IPWhois Lookup addon will allow users to lookup information on any IP Address from around the world.",
  "icons": {
    "48": "icon.png",
    "128": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "IPWhois Lookup",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
