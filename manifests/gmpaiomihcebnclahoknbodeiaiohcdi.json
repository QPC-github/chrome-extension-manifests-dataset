{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HP Smart Print",
  "version": "2.7.0.238",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "smartprintlogo_16.png",
    "48": "smartprintlogo_48.png",
    "128": "smartprintlogo_128.png"
  },
  "default_locale": "en_US",
  "browser_action": {
    "default_icon": "Smartprint_larger.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "options_page": "options.html",
  "web_accessible_resources": [
    "images/blue-glossy-dot.png",
    "images/close_highlight.gif",
    "images/close_normal.gif",
    "images/delete-icon.png",
    "images/discussion.gif",
    "images/help.gif",
    "images/hpsp_icons_sprite.png",
    "images/hpsp_quit_active.png",
    "images/hpsp_quit_hover.png",
    "images/hpsp_quit_normal.png",
    "images/hpsp_toolbar.png",
    "images/logo.gif",
    "images/plus-icon.png",
    "images/remove-icon.png",
    "images/SmartPrintLogo48x48.png",
    "images/undoSelection.gif",
    "css/options.css",
    "configure.js",
    "dialog.css",
    "dialog_rtl.css",
    "easyXDM.js",
    "espresso.js",
    "hplogo_16.jpg",
    "hplogo_48.jpg",
    "hplogo_128.jpg",
    "hpprintonly.css",
    "hptextview.css",
    "hptextview_print.css",
    "hptextview_chrome_print.css",
    "hpweb2print.css",
    "json2.js",
    "localizedStrings.js",
    "resource.css",
    "Smartprint_larger.png"
  ]
}
