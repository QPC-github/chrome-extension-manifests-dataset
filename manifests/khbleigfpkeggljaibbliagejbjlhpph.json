{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Character Count",
  "short_name": "cc",
  "manifest_version": 2,
  "version": "1",
  "description": "Count characters of sentences by inputting 'cc [sentences]' on omnibox.",
  "omnibox": {
    "keyword": "cc"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "notifications",
    "tabs"
  ]
}
