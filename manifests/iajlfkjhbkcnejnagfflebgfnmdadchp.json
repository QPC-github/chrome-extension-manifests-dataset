{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No Moments",
  "description": "hides the 'moments' tab on twitter dot com",
  "version": "1.1",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "nomom.css"
      ]
    }
  ]
}
