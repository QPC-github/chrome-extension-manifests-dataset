{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Better Bookmarks",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Search your bookmarks from your browser's search box.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "16-icon.png",
    "32": "32-icon.png",
    "48": "48-icon.png",
    "128": "128-icon.png"
  },
  "omnibox": {
    "keyword": "bb"
  },
  "permissions": [
    "bookmarks",
    "*://*/*",
    "tabs"
  ]
}
