{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hacker News: Links Only",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Removes everything from Hacker News except the article links.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "css": [
        "ap.css"
      ],
      "js": [
        "javascript.js"
      ]
    }
  ]
}
