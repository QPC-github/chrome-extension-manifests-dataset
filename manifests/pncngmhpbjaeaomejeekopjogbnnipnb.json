{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Track Back",
  "version": "1.1",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Track Back",
    "default_icon": "icon.png"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "permissions": [
    "history",
    "https://maps.googleapis.com/*",
    "*://api.statdns.com/",
    "*://www.telize.com/",
    "geolocation",
    "https://maps.google.com/*",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
