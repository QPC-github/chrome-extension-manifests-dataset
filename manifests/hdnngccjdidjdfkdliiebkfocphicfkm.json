{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AT Internet In-Page Analytics",
  "short_name": "In-Page Analytics",
  "version": "1.0.57",
  "author": "AT Internet",
  "homepage_url": "http://www.atinternet.com",
  "description": "An AT Internet In-Page Analytics tool",
  "devtools_page": "chrome/devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/at-16.png",
    "32": "images/at-32.png",
    "48": "images/at-48.png",
    "64": "images/at-64.png",
    "128": "images/at-128.png"
  },
  "background": {
    "scripts": [
      "chrome/background.js"
    ]
  },
  "permissions": [
    "https://apirest.atinternet-solutions/*",
    "https://*/chrome/getInfos.js",
    "http://*/chrome/getInfos.js",
    "storage",
    "cookies"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "images/at-16.png",
      "32": "images/at-32.png",
      "48": "images/at-48.png",
      "64": "images/at-64.png",
      "128": "images/at-128.png"
    },
    "default_title": "AT Internet In-Page Analytics",
    "default_popup": "index.html"
  }
}
