{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TF - preproduction",
  "version": "1.2.10",
  "description": "Tongue Map is a tool that lets user bookmark useful travel materials like blogs, articles and related websites on personal map",
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab",
    "history",
    "chrome://favicon/",
    "storage",
    "cookies",
    "*://*.google.com/*",
    "http://api.tonguefriend.com/",
    "http://api.tonguefriend.com/userc/login/",
    "http://tonguefriend.com/",
    "https://maps.googleapis.com/",
    "http://tongue.jobforseeker.com/"
  ],
  "browser_action": {
    "default_title": "Tongue Map - Your Travel Bookmarks",
    "default_icon": "icon.png",
    "default_popup": "facebook-login.html"
  },
  "icons": {
    "64": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}
