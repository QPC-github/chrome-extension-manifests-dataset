{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MSGDRK",
  "version": "0.7",
  "description": "messenger.com darkmode",
  "author": "wojexe",
  "content_scripts": [
    {
      "matches": [
        "https://messenger.com/*",
        "https://www.messenger.com/*"
      ],
      "all_frames": true,
      "css": [
        "darkmode.css"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "images/msgdrk16.png",
      "32": "images/msgdrk32.png",
      "48": "images/msgdrk48.png",
      "128": "images/msgdrk128.png"
    },
    "default_title": "MSGDRK - messenger.com darkmode"
  },
  "icons": {
    "16": "images/msgdrk16.png",
    "32": "images/msgdrk32.png",
    "48": "images/msgdrk48.png",
    "128": "images/msgdrk128.png"
  },
  "manifest_version": 2
}
