{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "theSHEDteam",
  "description": "Shows current team members who are streaming",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "management",
    "activeTab",
    "storage"
  ]
}
