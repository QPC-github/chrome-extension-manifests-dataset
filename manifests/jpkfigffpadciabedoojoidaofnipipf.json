{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab search",
  "version": "0.0.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "ts"
  },
  "description": "To use, type a search term into the extension popup. or to get all tabs click Find tabs",
  "permissions": [
    "storage",
    "tabs",
    "history",
    "bookmarks"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "name": "Tab search",
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "search tabs",
    "default_popup": "popup.html"
  }
}
