{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TwitterRssGetter",
  "description": "Test chrome",
  "version": "1.15",
  "content_security_policy": "default-src 'self'; img-src 'self' http://* https://* data:; style-src 'self' 'unsafe-inline'; connect-src 'self' http://* https://*;",
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "js/popup.js",
        "js/background.js",
        "js/jquery-min.js",
        "js/clibs.js"
      ]
    }
  ],
  "permissions": [
    "bookmarks",
    "cookies",
    "tabs",
    "unlimitedStorage",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "TwitterRssGetter",
    "default_icon": "image/icon_disabled.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon_48.png",
    "128": "image/icon_128.png"
  },
  "background": {
    "page": "background.html"
  }
}
