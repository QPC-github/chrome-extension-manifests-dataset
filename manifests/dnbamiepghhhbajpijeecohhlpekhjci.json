{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Woolah POD",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "ext-icon.png"
  },
  "icons": {
    "16": "ext16.png",
    "48": "ext48.png",
    "128": "ext128.png"
  }
}
