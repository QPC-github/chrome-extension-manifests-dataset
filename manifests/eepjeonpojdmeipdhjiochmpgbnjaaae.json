{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/logo16.png",
    "default_popup": "gizbot.html",
    "default_title": "Gizbot"
  },
  "description": "Gadgets News, Mobile prices, Mobile prices",
  "icons": {
    "128": "images/logo128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "name": "Gizbot",
  "permissions": [
    "idle",
    "tabs",
    "storage",
    "cookies",
    "notifications",
    "alarms",
    "*://*.oneindia.com/*",
    "http://rlinks.one.in/*",
    "http://www.oiscripts.com/*",
    "*://*.gizbot.com/*"
  ],
  "version": "0.4",
  "web_accessible_resources": [
    "images/logo128.png",
    "fonts/*.*"
  ],
  "short_name": "Gizbot"
}
