{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Amy Porterfield",
  "manifest_version": 2,
  "short_name": "amyporterfield",
  "description": "A Chrome Extension of amyporterfield.com",
  "version": "1",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_1-1486043644.png"
  },
  "icons": {
    "128": "128px_1-1486043651.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
