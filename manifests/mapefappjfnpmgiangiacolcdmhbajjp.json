{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Ctrl+Q",
        "linux": "Ctrl+Q",
        "mac": "Command+Q",
        "windows": "Ctrl+Q"
      }
    }
  },
  "description": "An extension that helps you switch from http to https or vice versa.",
  "icons": {
    "16": "icon.png",
    "48": "ssl.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "SSL Toggle",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "*://*/*",
    "background"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
