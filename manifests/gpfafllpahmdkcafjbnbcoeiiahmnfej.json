{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Like Safari Page",
  "short_name": "Like Safari Page",
  "version": "0.4",
  "description": "Override start page by bookmarks",
  "permissions": [
    "bookmarks",
    "history",
    "tabs",
    "chrome://favicon/"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src chrome://favicon;"
}
