{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Media Shelf",
  "description": "Save it now.  Enjoy it later.  Share with friends.",
  "version": "0.0.1.4",
  "web_accessible_resources": [
    "*.jpg",
    "*.JPG"
  ],
  "background": {
    "scripts": [
      "jquery-2.2.3.min.js",
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "media_shelf2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.3.min.js"
      ]
    }
  ]
}
