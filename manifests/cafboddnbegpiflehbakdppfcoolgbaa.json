{
  "background": {
    "persistent": true,
    "scripts": [
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "src/content.css"
      ],
      "js": [
        "src/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Share your screen and more on Airwave",
  "homepage_url": "https://airwave.io",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Airwave",
  "short_name": "Airwave",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Airwave Browser Extension"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "desktopCapture",
    "geolocation",
    "notifications",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.3"
}
