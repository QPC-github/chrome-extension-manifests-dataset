{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "CHERRAD (www.cherrad.net)",
  "manifest_version": 2,
  "short_name": "MANGANIME",
  "name": "__MSG_appName__",
  "version": "0.11",
  "version_name": "0.11 Beta",
  "description": "__MSG_appDesc__",
  "homepage_url": "http://radio.manganime.net",
  "default_locale": "en",
  "icons": {
    "16": "img/icon_x16.png",
    "19": "img/icon_x19.png",
    "32": "img/icon_x32.png",
    "38": "img/icon_x38.png",
    "48": "img/icon_x48.png",
    "128": "img/icon_x128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon_x16.png",
      "19": "img/icon_x19.png",
      "32": "img/icon_x32.png",
      "38": "img/icon_x38.png",
      "48": "img/icon_x48.png",
      "128": "img/icon_x128.png"
    },
    "default_title": "__MSG_appName__",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "notifications"
  ],
  "web_accessible_resources": [
    "img/*.png",
    "css/*",
    "fonts/*",
    "js/docProtection.js",
    "js/background.js",
    "js/popup.js"
  ]
}
