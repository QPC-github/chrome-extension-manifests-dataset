{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Импорт в Invite",
  "description": "Публикует событие из ВК на сайте Invite",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "19": "invite_logo_square19.png",
      "38": "invite_logo_square38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "icons": {
    "16": "invite_logo_square16.png",
    "48": "invite_logo_square48.png",
    "128": "invite_logo_square128.png"
  }
}
