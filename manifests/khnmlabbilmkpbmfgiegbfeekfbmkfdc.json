{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "antumid_48x48.png",
    "default_popup": "antumid.html",
    "default_title": "Scan QR-CODE and generate a password for every website."
  },
  "content_scripts": [
    {
      "js": [
        "antumid.js"
      ],
      "css": [
        "antumid.css"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "DigiPassword Pro License for the Digi-ID/AntumID APP",
  "icons": {
    "128": "antumid_128x128.png"
  },
  "manifest_version": 2,
  "name": "Digi-ID/AntumID DigiPassword Pro License",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "storage"
  ],
  "short_name": "Digi-ID/AntumID DigiPassword Pro License",
  "version": "1.0.4",
  "web_accessible_resources": [
    "Digi-password-icon48x48.png",
    "antumidOverlay.html"
  ]
}
