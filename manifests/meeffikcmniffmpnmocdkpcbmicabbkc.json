{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YTubeTab",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Multiple You Tube Videos In a Single Tab",
  "background": {
    "persistent": false,
    "scripts": [
      "js/jquery-1.11.2.min.js",
      "js/context.js",
      "js/main.js",
      "js/action.js"
    ]
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "contextMenus",
    "storage",
    "<all_urls>"
  ]
}
