{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "新浪微博强制方形头像",
  "version": "0.0.1",
  "description": "新浪微博圆形头像变为方形头像",
  "icons": {
    "16": "16icon.png",
    "48": "48icon.png",
    "128": "128icon.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*.weibo.com/*"
      ],
      "js": [
        "scripts/weiboAvatar.js"
      ]
    }
  ]
}
