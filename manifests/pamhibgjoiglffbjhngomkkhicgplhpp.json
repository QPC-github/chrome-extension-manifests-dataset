{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "POINTCreation DeezerFullPage",
  "version": "1.5",
  "description": "Permet d'avoir Deezer en fullpage",
  "permissions": [
    "declarativeContent",
    "https://www.deezer.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.deezer.com/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
