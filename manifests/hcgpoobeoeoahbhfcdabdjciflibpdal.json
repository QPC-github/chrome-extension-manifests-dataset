{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ShowSubjectGmail",
  "version": "1.0",
  "description": "Always show the subject field when replying in Gmail!",
  "content_scripts": [
    {
      "css": [
        "fix.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "*://*/*"
  ]
}
