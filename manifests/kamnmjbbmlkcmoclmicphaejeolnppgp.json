{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Passport Seva App",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get Your Passport Quickly with Passport Seva App At Your Home",
  "homepage_url": "https://www.apkbooth.com/passport-seva-at-your-home/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "ttsEngine",
    "notifications"
  ]
}
