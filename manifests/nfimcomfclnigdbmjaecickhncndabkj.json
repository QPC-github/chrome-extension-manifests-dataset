{
  "author": "Philipp Zeitschel",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*.keenmeet.me/*"
      ]
    }
  ],
  "description": "This extension is developed for https://keenmeet.net/. It doesn't do anything except capturing content of your screen.",
  "homepage_url": "https://www.keenmeet.net/",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "keenMeet Screen Capturing",
  "version": "1.4",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
