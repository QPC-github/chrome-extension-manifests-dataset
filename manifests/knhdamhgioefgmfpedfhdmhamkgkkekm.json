{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Info Retainer",
  "description": "Remember things for a longer time",
  "version": "1.0",
  "browser_action": {
    "default_popup": "infoRetainer.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "storage",
    "notifications",
    "https://ajax.googleapis.com/"
  ]
}
