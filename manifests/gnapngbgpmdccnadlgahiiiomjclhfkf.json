{
  "manifest_version": 2,
  "version": "2.2.12",
  "chrome_url_overrides": {
    "newtab": "pages/index.html"
  },
  "browser_action": {
    "default_icon": "/images/img/icone/128.png",
    "default_title": "Full HD NewTab"
  },
  "description": "Wallpapers Themes HD Pictures - Created for fans of Bayern FC",
  "icons": {
    "128": "/images/img/icone/128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "chrome-themes-hd",
      "name": "chrome-themes-hd",
      "favicon_url": "http://chrome-themes-hd.com/favicon.ico",
      "search_url": "http://chrome-themes-hd.com/search?q={searchTerms}",
      "is_default": true,
      "encoding": "UTF-8"
    }
  },
  "name": "Bayern FC Wallpapers Themes HD",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "contextMenus",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "topSites"
  ],
  "short_name": "Full HD NewTab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "pages/index.html"
  ]
}
