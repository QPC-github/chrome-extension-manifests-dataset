{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Redirector",
  "version": "0.1",
  "description": "Navigate to various subreddits conveniently.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "omnibox": {
    "keyword": "r"
  },
  "minimum_chrome_version": "9",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
