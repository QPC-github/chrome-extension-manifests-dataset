{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "We Tranzlate FREE",
  "version": "1.4.5",
  "description": "We Tranzlate FREE",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "We Tranzlate FREE Translation Tool",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "19": "images/icon_19.png",
      "38": "images/icon_38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/content.min.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "19": "images/icon_19.png",
    "38": "images/icon_38.png",
    "128": "images/icon_128.png"
  },
  "homepage_url": "http://wetranzlate.com/",
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/logo_small.png"
  ]
}
