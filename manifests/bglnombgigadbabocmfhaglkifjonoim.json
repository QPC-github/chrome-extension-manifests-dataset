{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "embed.js"
      ],
      "matches": [
        "http://www.tabletguide.nl/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Via deze Chrome extensie ontvang je een pushbericht bij belangrijk tabletnieuws.",
  "icons": {
    "128": "icons/128x128.png",
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "80": "icons/80x80.png",
    "96": "icons/96x96.png"
  },
  "manifest_version": 2,
  "name": "TabletGuide pushberichten",
  "permissions": [
    "http://www.tabletguide.nl/*",
    "notifications",
    "gcm",
    "storage",
    "alarms",
    "https://secure.tabletguide.nl/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
