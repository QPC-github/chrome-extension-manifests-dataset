{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Censorship Checker",
  "short_name": "Censorship",
  "version": "0.3.7",
  "manifest_version": 2,
  "description": "Visually inspect censored HTML that Wisdom will mask for user privacy.",
  "homepage_url": "https://getwisdom.io",
  "minimum_chrome_version": "56",
  "incognito": "split",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "64": "images/icon_64.png",
    "128": "images/icon_128.png",
    "256": "images/icon_256.png"
  },
  "browser_action": {
    "default_title": "Check Censorship",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "64": "images/icon_64.png",
      "128": "images/icon_128.png",
      "256": "images/icon_256.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
