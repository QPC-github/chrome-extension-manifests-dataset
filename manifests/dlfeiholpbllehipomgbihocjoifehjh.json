{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tstudy.ebanshu.driver",
  "version": "1.1.1",
  "description": "T-study & eBanshu driver",
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.ebanshu.com/site_media/html/chrome_extension.html",
        "http://*/room/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "declarativeContent",
    "nativeMessaging"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
