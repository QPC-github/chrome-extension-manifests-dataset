{
  "manifest_version": 2,
  "version": "1",
  "name": "Ja_Queen",
  "permissions": [
    "http://*.google.com/",
    "http://*.com/",
    "http://*.co.uk/",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "content_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "JacHec"
  }
}
