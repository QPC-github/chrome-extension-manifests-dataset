{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Panasonic ScreenSharing Extension",
  "author": "Smit Patel",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is used for OpenTok ScreenSharing feature",
  "homepage_url": "https://on4care.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
