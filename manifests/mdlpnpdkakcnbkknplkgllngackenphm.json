{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Menetext",
  "version": "1.2",
  "description": "Menetext é a melhor e mais rápida forma para utilização dos já conhecidos menes de texto.",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Menetext",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "clipboardRead"
  ]
}
