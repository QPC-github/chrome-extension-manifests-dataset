{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "B",
  "short_name": "B",
  "version": "0.0.1",
  "author": [
    "Brandon Richardson"
  ],
  "manifest_version": 2,
  "description": "Replace all your B's with the B emoji. Because yolo.",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "b.png",
    "16": "b.png",
    "48": "b.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": false,
      "run_at": "document_end",
      "js": [
        "content.js"
      ]
    }
  ],
  "offline_enabled": true
}
