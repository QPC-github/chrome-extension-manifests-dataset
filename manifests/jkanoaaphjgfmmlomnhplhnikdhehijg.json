{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fetch!",
  "description": "Fetch a dog, anytime, anywhere!",
  "version": "0.0.0.3",
  "browser_action": {
    "default_icon": "content/dog-icon.png",
    "default_popup": "popup.html",
    "default_title": "Fetch!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
