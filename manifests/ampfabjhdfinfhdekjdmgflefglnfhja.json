{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.7",
  "description": "__MSG_extDescription__",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "init.js"
    ]
  },
  "options_page": "option.html",
  "default_locale": "en",
  "page_action": {
    "default_popup": "frame.html",
    "default_icon": "icon-glay.png",
    "default_title": "Show tweets"
  },
  "permissions": [
    "tabs",
    "https://twitter.com/",
    "https://bookmark.hatenaapis.com/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://widgets.pinterest.com/ https://vk.com/; object-src 'self'",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
