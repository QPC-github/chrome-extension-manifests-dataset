{
  "author": "WebGameAPP",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "default_locale": "en",
  "version": "7",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "__MSG_appName__",
    "default_popup": "gametab.html",
    "default_icon": {
      "128": "128.png",
      "16": "16.png",
      "24": "24.png",
      "48": "48.png"
    }
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "tabs",
    "https://*.webgameapp.com/*"
  ],
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "24": "24.png",
    "48": "48.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
