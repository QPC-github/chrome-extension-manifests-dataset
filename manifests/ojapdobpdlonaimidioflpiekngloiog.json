{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kommenter.hu",
  "description": "Kommenter.hu chrome kiegészítő",
  "version": "1.0",
  "browser_action": {
    "default_icon": "kommenter.png",
    "default_title": "Megtekintés a Kommenter.hu-n"
  },
  "permissions": [
    "activeTab",
    "http://*.kommenter.hu/",
    "https://*.kommenter.hu/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "icons": {
    "128": "kommenter.png"
  }
}
