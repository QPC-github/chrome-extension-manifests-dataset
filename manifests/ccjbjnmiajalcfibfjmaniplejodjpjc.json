{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mystique",
  "description": "css randomizer",
  "version": "2.2",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js",
      "jquery-ui.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "css"
  },
  "manifest_version": 2
}
