{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spoiler-free Starcraft",
  "description": "This extension makes sure there are no spoilers on GOMTV.net",
  "version": "1.0.4",
  "content_scripts": [
    {
      "matches": [
        "http://www.gomtv.net/*",
        "http://gomtv.net/*"
      ],
      "run_at": "document_start",
      "js": [
        "jquery-1.9.0.min.js",
        "GOMTV.js"
      ]
    }
  ]
}
