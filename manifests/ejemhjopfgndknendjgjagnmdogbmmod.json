{
  "author": "Kicaken",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "ico18.png",
    "default_popup": "popup.html",
    "default_title": ""
  },
  "description": "Notifies you when UKOGMonkey are starts streaming. Also a YouTube notification from new vids!",
  "icons": {
    "128": "ico128.png",
    "16": "ico16.png",
    "48": "ico48.png"
  },
  "manifest_version": 2,
  "name": "UKOGMonkey Notification",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "management",
    "webRequest",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
