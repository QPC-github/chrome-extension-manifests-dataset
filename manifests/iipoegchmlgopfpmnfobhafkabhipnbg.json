{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "request.js"
    ]
  },
  "description": "Adds -site:pinterest to any google search",
  "icons": {
    "48": "UnPin-48x48.png",
    "128": "UnPin-128x128.png"
  },
  "manifest_version": 2,
  "name": "UnPintrested",
  "short_name": "UnPin",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.google.*/*"
  ],
  "version": "0.02",
  "homepage_url": "http://swyx.io",
  "author": "swyx"
}
