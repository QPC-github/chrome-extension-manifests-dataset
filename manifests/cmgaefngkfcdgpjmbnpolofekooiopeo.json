{
  "background": {
    "persistent": true,
    "scripts": [
      "src/options/lib/store.js",
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_title": "__MSG_browserActionTitle__",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "http://learnerdesign.com/feed-roger-a-simple-google-chrome-extension.html",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "19": "img/icon19.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "permissions": [
    "tabs",
    "downloads",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1"
}
