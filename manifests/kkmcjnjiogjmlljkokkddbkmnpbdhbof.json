{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Miniplayer",
  "description": "Watch youtube videos while scrolling",
  "version": "0.0.2",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch?v=*"
      ],
      "js": [
        "jquery.js",
        "youtubevideo.js"
      ]
    }
  ]
}
