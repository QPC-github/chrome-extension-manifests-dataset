{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Revoke Article 50 - Petition Counter",
  "version": "0.0.0.3",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "alarms"
  ],
  "background": {
    "page": "index.html#/event-page",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Revoke Article 50!",
    "default_popup": "index.html#/popup",
    "icons": {
      "19": "assets/19.jpg",
      "38": "assets/38.jpg"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "assets/EU-16.png",
    "32": "assets/EU-32.png",
    "24": "assets/EU-24.png",
    "128": "assets/EU-128.png"
  },
  "web_accessible_resources": [
    "*.map",
    "webpack://*"
  ]
}
