{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "A new front page for Quora.",
  "manifest_version": 2,
  "name": "Qobalt",
  "version": "2.0",
  "homepage_url": "http://www.ominax.com/qobalt",
  "icons": {
    "16": "images/qobalt_16.png",
    "19": "images/qobalt_19.jpg",
    "32": "images/qobalt_32.jpg",
    "48": "images/qobalt_48.png",
    "64": "images/qobalt_64.png"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "web_accessible_resources": [
    "images/*.png",
    "images/*.jpg"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/qobalt_16.png",
      "19": "images/qobalt_19.jpg"
    },
    "default_title": "Qobalt"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "tabHide"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.quora.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
