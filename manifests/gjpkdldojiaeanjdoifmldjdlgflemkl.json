{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unaddict",
  "version": "1.0",
  "description": "Stop web addiction!",
  "permissions": [
    "declarativeContent",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "130": "image/unaddicticon.png"
  },
  "browser_action": {
    "default_icon": "image/unaddicticon.png",
    "default_popup": "popup.html"
  }
}
