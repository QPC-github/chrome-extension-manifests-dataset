{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yeditor",
  "description": "Yeditor is front-end editor, now you can editing every web page on your browser",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "background": {
    "scripts": [
      "js/bg/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "css/style.min.css",
    "css/*",
    "css/fonts/"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
