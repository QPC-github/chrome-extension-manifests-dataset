{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Microminimus Fullsize Viewer",
  "description": "View the Fullsize images in a microminimus gallery",
  "version": "1.01",
  "icons": {
    "16": "thumb16.png",
    "48": "thumb48.png",
    "128": "thumb128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.microminimus.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
