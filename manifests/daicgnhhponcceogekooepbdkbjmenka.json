{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "tHe aRK",
  "version": "1.0",
  "description": "Get the state of choice",
  "permissions": [
    "activeTab",
    "storage",
    "declarativeContent"
  ],
  "browser_action": {
    "default_popup": "choice.html",
    "default_icon": "icon.png",
    "default_title": "tHe aRK"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.12.3.js",
        "click.js"
      ]
    }
  ]
}
