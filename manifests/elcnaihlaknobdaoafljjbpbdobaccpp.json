{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ytloop",
  "version": "1.0.2",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch*"
      ],
      "js": [
        "ytloop.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
