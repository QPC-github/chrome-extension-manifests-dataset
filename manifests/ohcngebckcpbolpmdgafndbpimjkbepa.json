{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "History Crawler",
  "version": "0.1",
  "description": "An extension that creates a monthly history summary of domains visited.",
  "manifest_version": 2,
  "permissions": [
    "identity"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}
