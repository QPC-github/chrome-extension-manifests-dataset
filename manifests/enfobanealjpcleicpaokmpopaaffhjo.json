{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Placebeard.it extension",
  "description": "Replaces all images with pictures from placebeard.it",
  "version": "0.3",
  "icons": {
    "16": "icon_16.jpg",
    "48": "icon_48.jpg",
    "128": "icon_128.jpg"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "magic.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
