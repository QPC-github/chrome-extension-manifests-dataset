{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "F*ck you, NSA",
  "description": "This extension adds 'F*ck you, NSA' to all Like-buttons.",
  "version": "1.0",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*",
        "*://facebook.com/*"
      ],
      "js": [
        "jquery.js",
        "firewall.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
