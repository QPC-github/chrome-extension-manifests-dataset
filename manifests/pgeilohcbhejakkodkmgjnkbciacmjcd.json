{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pear Password Plugin",
  "description": "This extension will encrypt/decrypt passwords",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "pwd-popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://password.pear-trade.com/*",
    "https://fonts.googleapis.com/*"
  ]
}
