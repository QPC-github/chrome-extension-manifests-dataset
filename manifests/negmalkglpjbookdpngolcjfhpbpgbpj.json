{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Share on Inbound.org",
  "description": "This extension allows you to share interesting articles on Inbound.org",
  "version": "1.0.1",
  "icons": {
    "128": "inbound_128x128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "http://inbound.org/guidelines",
    "http://inbound.org/articles/share"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://api.twitter.com https://si0.twimg.com/; object-src 'self'",
  "browser_action": {
    "default_title": "Share on Inbound.org",
    "default_icon": "inbound_16x16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
