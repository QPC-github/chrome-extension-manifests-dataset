{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rotting Tabs",
  "manifest_version": 2,
  "version": "1",
  "description": "The circle of life",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "http://*/",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
