{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Block HLTV Forum",
  "version": "1.1",
  "description": "Blocks HLTV forum headlines on the hltv.org mainpage!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "https://www.hltv.org/*"
      ]
    }
  ],
  "icons": {
    "16": "blocksign16.png",
    "48": "blocksign48.png",
    "128": "blocksign.png"
  }
}
