{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Sanctarius",
  "background": {
    "scripts": [
      "youtube.js",
      "parameters.js",
      "background.js",
      "chrome.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_off.png",
    "default_title": "JunpeiFrance - Offline",
    "default_popup": "popup.html"
  },
  "description": "Ne rate plus aucun stream de JunpeiFrance ! !",
  "icons": {
    "256": "icons/logo256.png",
    "128": "icons/logo128.png",
    "32": "icons/logo32.png",
    "64": "icons/logo64.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2,
  "name": "JunpeiFrance Live Extension",
  "permissions": [
    "https://api.twitch.tv/",
    "https://www.googleapis.com/",
    "notifications",
    "storage"
  ],
  "version": "1.1"
}
