{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_puzzleitName__",
  "description": "__MSG_puzzleitDescription__",
  "version": "0.1.3.112",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content/start.js",
        "content/puzzleit.js",
        "content/tooltip.js",
        "content/MovPiece.js"
      ],
      "css": [
        "css/css.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "128": "images/Puzzle-Logo.png"
  },
  "web_accessible_resources": [
    "images/Puzzle-Logo.png",
    "images/close-button.png",
    "images/maximin.png",
    "images/SM.png",
    "images/indications.jpg",
    "images/fondo-div-puzzle.png"
  ],
  "default_locale": "es_419"
}
