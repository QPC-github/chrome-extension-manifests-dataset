{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Thirsty Companies",
  "version": "1.0",
  "description": "Replaces the text 'wants to' with 'is thirsty to'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
