{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MakeTime - повышение продуктивности",
  "description": "Инструмент для повышения продуктивности через тайм-менеджмент. Запланируйте все необходимое и фокусируйтесь над выполнением. ",
  "version": "2.2.12",
  "permissions": [],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/img/management_16.png",
      "24": "assets/img/management_24.png"
    }
  },
  "icons": {
    "16": "assets/img/management_16.png",
    "48": "assets/img/management_48.png",
    "128": "assets/img/management_128.png"
  },
  "web_accessible_resources": [
    "*.ttf",
    "*.wofl"
  ],
  "content_security_policy": "script-src 'self' 'sha256-ujiO3DbDAK4oMbY5Skm7Wf5cBMLVGlOP5p9W5qXsx78=' https://www.googletagmanager.com/ https://www.googleadservices.com/ https://googleads.g.doubleclick.net;  object-src 'self'"
}
