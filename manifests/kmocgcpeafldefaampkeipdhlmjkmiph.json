{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1.0",
  "manifest_version": 2,
  "name": "__MSG_title__",
  "description": "__MSG_description__",
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "logo-18.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "*://epicriver.atlassian.net/*"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/jquery-1.7.2.min.js",
        "js/jquery.ajaxLoader.js",
        "js/content.js"
      ],
      "css": [
        "css/jquery.ajaxLoader.css"
      ],
      "matches": [
        "*://epicriver.atlassian.net/browse/*"
      ],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_icon": "logo-18.png",
    "default_title": "Edit configuration"
  },
  "options_page": "options.html"
}
