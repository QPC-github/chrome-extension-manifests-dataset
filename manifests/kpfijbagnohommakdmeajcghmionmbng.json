{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "aiStream extension",
  "description": "Добавить RSS в айСтрим",
  "manifest_version": 2,
  "version": "0.22",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "aiStream_16.png",
    "48": "aiStream_48.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "page": "background.html"
  }
}
