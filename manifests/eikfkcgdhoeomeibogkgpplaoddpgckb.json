{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CleanyTab",
  "description": "Get a nice and clean page while opening a new tab! Welcome to CleanyTab.",
  "version": "1.1.2",
  "incognito": "split",
  "options_page": "settings.html",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
