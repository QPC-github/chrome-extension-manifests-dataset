{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "minimum_chrome_version": "24.0.1307.0",
  "default_locale": "en_US",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "i128x128.png",
    "16": "i16x16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "1.2",
  "permissions": [
    "https://spinbackup.com/*",
    "webview"
  ],
  "webview": {
    "partitions": [
      {
        "name": "static",
        "accessible_resources": [
          "/data/login.html",
          "/data/css/sb.css",
          "/logo.png",
          "/data/css/fonts/*",
          "https://spinbackup.com/*"
        ]
      }
    ]
  }
}
