{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SFProxy",
  "description": "__MSG_extDesc__",
  "author": "StableFlow",
  "version": "0.0.3",
  "homepage_url": "http://www.stableflow.com",
  "default_locale": "en_US",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
