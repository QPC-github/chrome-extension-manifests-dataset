{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Paperwise Connectwise Service Board Refresh",
  "version": "0.5",
  "description": "This is an autorefresh extension for connectwise",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
