{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Org62 EzLink v4",
  "description": "Org62 EzLink v4",
  "version": "4.0.7",
  "permissions": [
    "contextMenus",
    "storage",
    "bookmarks"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Switch new tab mode: Active or Background"
  }
}
