{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "To-Do",
  "version": "1.0.4",
  "icons": {
    "16": "assets/icons/16.png",
    "32": "assets/icons/32.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "optional_permissions": [
    "downloads"
  ],
  "permissions": [
    "storage"
  ]
}
