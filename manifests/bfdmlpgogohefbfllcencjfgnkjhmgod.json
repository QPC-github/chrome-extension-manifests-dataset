{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Edutags",
  "description": "This extension helps to bookmark web pages to the Social Bookmarking Service Edutags.",
  "version": "1.0.1",
  "icons": {
    "16": "edutags_16.png",
    "32": "edutags_32.png",
    "48": "edutags_48.png",
    "128": "edutags_128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "bookmarklet_load.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "edutags.png",
    "default_title": "Edutags"
  }
}
