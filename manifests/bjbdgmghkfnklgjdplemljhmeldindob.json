{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Virool VideoSpy For YouTube",
  "short_name": "Virool VideoSpy",
  "version": "0.3.18",
  "manifest_version": 2,
  "description": "Viroolâ€™s VideoSpy displays a convenient scorecard of key engagement metrics for each YouTube video.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "styles/fonts/*",
    "images/*.png",
    "manifest.json"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/cs.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "*://apis.google.com/*",
    "*://www.youtube.com/*",
    "*://assets-host.s3.amazonaws.com/*"
  ]
}
