{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Eventer",
  "description": "Never miss an Event.  Let us know what you like, and we'll notify you of the events.",
  "version": "1.2",
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
