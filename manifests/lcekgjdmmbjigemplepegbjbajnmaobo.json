{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "You What?",
  "version": "1.2.4",
  "description": "Watch randomly selected videos from YouTubeâ„¢",
  "short_name": "YouWhat?",
  "minimum_chrome_version": "57",
  "browser_action": {
    "default_title": "You What?",
    "default_popup": "cookieWarning.html"
  },
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "incognito": "split",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://www.youtube.com; object-src 'self'",
  "manifest_version": 2
}
