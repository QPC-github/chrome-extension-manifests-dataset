{
  "name": "Rocket Beans Supporter",
  "version": "1.0.0",
  "description": "__MSG_extensionDescription__",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "controller/rbamazoner.user.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "view/ico_16.png",
    "default_popup": "view/popup.html"
  },
  "icons": {
    "16": "view/ico_16.png",
    "48": "view/ico_48.png",
    "128": "view/ico_128.png"
  },
  "default_locale": "de",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
