{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "网页"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/bootstrap.min.js",
        "js/jquery_form.min.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "挖掘网页中的遗珠",
  "homepage_url": "http://xingkoong.com/page",
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "网页",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.8"
}
