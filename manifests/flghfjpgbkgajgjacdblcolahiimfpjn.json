{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Timeline",
  "short_name": "Timeline",
  "description": "This allows you to browse the wikipedia world history timelines.",
  "version": "0.1.2",
  "icons": {
    "128": "timeline-128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "timeline-icon.png",
    "default_title": "Timeline",
    "default_popup": "popup.html"
  }
}
