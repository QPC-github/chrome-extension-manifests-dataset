{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Speedeo",
  "description": "Gracias a mi el tiempo es oro",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "64": "icon.png"
  },
  "version": "1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "activeTab"
  ]
}
