{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Outsons",
  "version": "1.0",
  "description": "Your daily dose of curated Photography, Music, Fashion & Editorial Inspiration.",
  "browser_action": {
    "default_title": "outsons.com"
  },
  "permissions": [
    "https://outsons.com/"
  ],
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
