{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "StackOverflow Right-Click Search",
  "version": "1.8",
  "manifest_version": 2,
  "description": "Search StackOverflow using right-click context menu on selected text",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ]
}
