{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dribbble Plus",
  "description": "Make Dribbble better.",
  "version": "0.1.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.dribbble.com/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "scripts.js"
      ]
    }
  ]
}
