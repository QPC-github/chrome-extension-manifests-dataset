{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ProProfs Poll Maker",
  "description": "Simple and easy way to create online polls",
  "version": "8.9",
  "app": {
    "urls": [
      "http://www.proprofs.com/polls/"
    ],
    "launch": {
      "web_url": "http://www.proprofs.com/polls/create-a-poll.php"
    }
  },
  "icons": {
    "128": "Icon_P_128x128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
