{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "kanye_facebook_filter.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Remove Kanye West from Facebook",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Remove Kanye West from Facebook",
  "short_name": "Remove Kanye West from Facebook",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3"
}
