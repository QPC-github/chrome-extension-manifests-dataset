{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rozetka",
  "version": "0.4",
  "content_scripts": [
    {
      "matches": [
        "http://rozetka.com.ua/*",
        "https://rozetka.com.ua/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "content.css"
  ],
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  }
}
