{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "InlineEvals",
  "short_name": "InlineEvals",
  "version": "2.0.0",
  "description": "Gain insight on prior years' course evaluations as you're searching for classes.",
  "icons": {
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "static/popup.html"
  },
  "author": "Jeremiah Clothier & Isaac Lance",
  "content_scripts": [
    {
      "css": [
        "static/css/styles.css"
      ],
      "js": [
        "static/js/classes.js"
      ],
      "matches": [
        "http://classes.uoregon.edu/pls/prod/*"
      ]
    },
    {
      "css": [
        "static/css/styles.css"
      ],
      "js": [
        "static/js/duckweb.js"
      ],
      "matches": [
        "https://duckweb.uoregon.edu/pls/prod/bwskfcls.P_GetCrse_Advanced*"
      ]
    }
  ],
  "web_accessible_resources": [
    "CSV/*.csv",
    "vals.txt"
  ],
  "permissions": [
    "storage",
    "tabs"
  ]
}
