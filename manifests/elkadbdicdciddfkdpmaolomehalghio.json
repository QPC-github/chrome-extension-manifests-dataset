{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Frame by Frame for YouTube™",
  "version": "1.2.0",
  "description": "Watch YouTube™ videos frame by frame",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "author": "Calum Eadie, regularWand and Tom D. Chambers",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch*",
        "https://www.youtube.com/watch*"
      ],
      "css": [
        "css/youtube-controls.css"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "frame-by-frame.js",
    "font/*"
  ]
}
