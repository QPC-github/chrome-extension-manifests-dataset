{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JSON.format",
  "short_name": "JSON.format",
  "description": "Format JSON data automatically",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
