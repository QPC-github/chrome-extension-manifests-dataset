{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Raito to Light",
  "version": "1.0",
  "description": "Replaces the word Raito with Light on all websites",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
