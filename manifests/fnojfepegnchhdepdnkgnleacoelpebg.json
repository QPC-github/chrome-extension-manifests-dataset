{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "name": "Voice Search It",
  "description": "Voice Search using speaking.",
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://*.google.com https://*.google-analytics.com https://*.doubleclick.net; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "img/ico_19.png"
  },
  "icons": {
    "48": "img/ico_48.png"
  }
}
