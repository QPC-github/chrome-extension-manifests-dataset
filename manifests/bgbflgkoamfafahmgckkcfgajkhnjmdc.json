{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SimpleCache",
  "short_name": "SimpleCache",
  "description": "Adds a 'Clear Cache' button to the right click context menu",
  "version": "1.1",
  "background": {
    "scripts": [
      "cache.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "contextMenus",
    "browsingData",
    "tabs",
    "cookies",
    "<all_urls>"
  ]
}
