{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Flickr Photo Search",
  "short_name": "Flk search",
  "description": "This extension searches Fickr for photos tagged with one or more keywords provided by the user.",
  "version": "1.0",
  "permissions": [
    "https://secure.flickr.com/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  }
}
