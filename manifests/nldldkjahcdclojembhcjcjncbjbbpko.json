{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "general.html"
  },
  "content_security_policy": "script-src 'unsafe-eval' 'self' https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_extdesc__",
  "icons": {
    "128": "icon/icon-128.png",
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extname__",
  "offline_enabled": true,
  "page_action": {
    "default_title": "New Tab Weather"
  },
  "permissions": [
    "*://*/*",
    "background",
    "geolocation",
    "management",
    "topSites",
    "bookmarks",
    "unlimitedStorage",
    "topSites",
    "identity",
    "http://*/",
    "https://*/",
    "chrome://favicon/",
    "history",
    "alarms",
    "notifications",
    "tabs",
    "storage",
    "activeTab",
    "declarativeContent",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies"
  ],
  "version": "3.3.1"
}
