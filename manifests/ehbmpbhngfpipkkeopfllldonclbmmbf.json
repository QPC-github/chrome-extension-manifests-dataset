{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Green Leaf",
  "description": "This extension tells you when a course is available at free cost across all MOOCs",
  "version": "1.4",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "tabs",
    "http://www.edu-box.appspot.com/*"
  ],
  "browser_action": {
    "default_icon": "./images/logo_16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "./js/vendor/jquery-3.2.0.min.js",
      "./js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "./images/logo_16.png",
    "48": "./images/logo_48.png",
    "128": "./images/logo.png"
  }
}
