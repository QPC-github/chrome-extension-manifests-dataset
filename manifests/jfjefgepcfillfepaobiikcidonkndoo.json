{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Duplicate Tab Killer!",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This is an extension for removing the duplicate tabs!",
  "permissions": [
    "tabs"
  ],
  "commands": {
    "record-tab-names": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "remove duplicated tabs"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
