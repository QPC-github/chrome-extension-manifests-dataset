{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SDCCU Improved",
  "description": "Adds color to your SDCCU bank account",
  "version": "1.01",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://internetbranch.sdccu.com/SDCCU/Accounts/Activity.aspx*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "main.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
