{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Memoryzer.com",
  "version": "0.0.16",
  "manifest_version": 2,
  "description": "Save your data, save your time.",
  "homepage_url": "http://memoryzer.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "icons/*.png"
  ],
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Memoryzer.com",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "geolocation",
    "tabs",
    "http://memoryzer.com/",
    "https://memoryzer.com/",
    "http://localhost:3000/*"
  ]
}
