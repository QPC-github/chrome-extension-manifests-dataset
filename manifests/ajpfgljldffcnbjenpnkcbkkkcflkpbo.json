{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vookmark",
  "short_name": "Vookmark",
  "description": "With Vookmark quickly bookmark videos across the web and mobile and watch it later on your browser, Apple TV, iOS or Android devices",
  "version": "18.5.25.1252",
  "icons": {
    "19": "images/icon19.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon@2x.png",
      "128": "images/icon@2x.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "manifest.json",
    "images/*.*",
    "fonts/*.*"
  ],
  "background": {
    "scripts": [
      "js/background.bundle.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "pageCapture",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "optional_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/content.bundle.js"
      ]
    }
  ]
}
