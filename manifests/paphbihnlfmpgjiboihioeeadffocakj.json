{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Redirects to TMIA when a valid sku is found.",
  "manifest_version": 2,
  "name": "BuscarTmia",
  "version": "2.7.1",
  "icons": {
    "48": "icons/search-48.png",
    "32": "icons/search-32.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/search-32.png",
    "default_title": "Search",
    "default_popup": "popup/show.html"
  }
}
