{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JunkFill",
  "version": "2.2.1",
  "manifest_version": 2,
  "description": "Fill out web forms instantly with junk or custom data",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "48": "jf48.png",
    "128": "jf128.png"
  },
  "browser_action": {
    "default_icon": "jf19.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
