{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Holder",
  "version": "1.2.0",
  "description": "a list where to keep your tabs instead of keeping them open.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "dashboard.html"
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "*://*/*",
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
