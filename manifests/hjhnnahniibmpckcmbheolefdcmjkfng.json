{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CRUD Generator for PHP MySQL (PDO)",
  "description": "A made-easy offline PHP and MySQL Syntax Generator for Web developers.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon.png"
  },
  "options_page": "index.html",
  "offline_enabled": true,
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
