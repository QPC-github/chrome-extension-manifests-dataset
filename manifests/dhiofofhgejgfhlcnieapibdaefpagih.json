{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Harvard Q Guide Companion",
  "description": "Opens the Harvard Q Guide in a popup window. Allows users to search for courses by name, catalog number, and URL code.",
  "background": {
    "scripts": [
      "background.js",
      "Qnumbers.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_popup": "qguide.html",
    "default_title": "Harvard Q Guide Companion"
  },
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "version": "1.1",
  "permissions": [
    "tabs"
  ]
}
