{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "name": "ScholarCite",
  "description": "An extension for referencing websites.",
  "homepage_url": "https://teamtangrams.com",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.3",
  "manifest_version": 2
}
