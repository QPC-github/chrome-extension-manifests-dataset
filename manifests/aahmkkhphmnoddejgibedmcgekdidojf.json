{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TTP",
  "description": "Shows minutes to pray time. While the Gah time it shows 10 minuts TTP (time to pray)",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "suncalc.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_name": "Page time to pray",
    "default_title": "Minutes to pray time.",
    "default_icon": "print_16x16.png"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "manifest_version": 2
}
