{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "White Website",
  "description": "Blocks all not relevant websites",
  "version": "1.3",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "manifest_version": 2
}
