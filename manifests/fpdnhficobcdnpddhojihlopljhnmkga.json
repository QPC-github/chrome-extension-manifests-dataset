{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tiny Notes",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A tiny app for write quickly",
  "icons": {
    "128": "assets/img/icons/icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "assets/img/icons/icon-128.png"
    },
    "default_popup": "popup.html"
  }
}
