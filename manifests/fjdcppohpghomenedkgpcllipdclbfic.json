{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StartupWeekly",
  "description": "Sugira tópicos para o StartupWeekly usando este plugin",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Sugira tópicos para StartupWeekly!",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
