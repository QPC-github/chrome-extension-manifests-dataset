{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "issue marker",
  "description": "Easily access tickets for issue management tools such as JIRA and Redmine, and various articles.",
  "version": "1.2.2",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "issue marker",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon/19.png",
      "38": "icon/38.png"
    }
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "default_locale": "en",
  "manifest_version": 2
}
