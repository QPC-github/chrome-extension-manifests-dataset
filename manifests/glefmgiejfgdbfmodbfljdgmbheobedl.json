{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Git Tab",
  "description": "Override the new tab page with a custom one",
  "version": "1.0.3",
  "incognito": "split",
  "short_name": "Git Tab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Add Shortcuts"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/icon16.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
