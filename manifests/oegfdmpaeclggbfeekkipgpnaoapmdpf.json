{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Source Hint Filter",
  "version": "0.0.0.6",
  "description": "Notifies you that this web page has found family members that may be missing from your family tree.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "19": "related-19.png"
    },
    "default_title": "Looking for Ancestors to Add!"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "https://familysearch.org/search/linker*"
  ],
  "icons": {
    "48": "related-48.png",
    "128": "related-128.png"
  },
  "manifest_version": 2
}
