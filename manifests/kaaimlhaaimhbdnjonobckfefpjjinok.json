{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "jsontots",
  "description": "jsontots",
  "version": "1.1",
  "browser_action": {
    "default_icon": "img/icon-128.png"
  },
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "page": "index.html"
  },
  "permissions": [
    "contextMenus",
    "management",
    "tabs",
    "storage",
    "https://github.com/*"
  ],
  "web_accessible_resources": [
    "index.html"
  ]
}
