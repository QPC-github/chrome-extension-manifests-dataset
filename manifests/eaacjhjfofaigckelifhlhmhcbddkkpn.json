{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save button for elCollect",
  "description": "Save images from site into your collections on elCollect.com",
  "version": "1.2.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "elcollect-vue-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "*://*/*",
    "contextMenus",
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "activeTab"
  ]
}
