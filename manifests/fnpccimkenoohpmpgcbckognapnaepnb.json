{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "To Do Check!",
  "version": "2.3",
  "manifest_version": 2,
  "description": "Simple app to manage your tasks directly in the browser popup window.",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ]
}
