{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LynxieLive",
  "short_name": "LynxieLive!",
  "version": "1.2.4",
  "description": "LynxieLive monitoruje najlepsze rysiowe streamy na Twitchu. Zareaguje jeśli tylko pojawi się stream!",
  "permissions": [
    "alarms",
    "storage"
  ],
  "homepage_url": "https://www.twitch.tv/marykateanlive",
  "browser_action": {
    "default_title": "LynxieLive!",
    "default_icon": {
      "16": "images/live-off-16.png",
      "32": "images/live-off-32.png",
      "48": "images/live-off-48.png",
      "128": "images/live-off-128.png"
    }
  },
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "images/lynxie-live-16.png",
    "32": "images/lynxie-live-32.png",
    "48": "images/lynxie-live-48.png",
    "128": "images/lynxie-live-128.png"
  }
}
