{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "명언봇 & 시간봇",
  "version": "3.0",
  "manifest_version": 2,
  "description": "새탭을 열때 마다 명언 & 시간을 보여줍니다.",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "myPage.html"
  },
  "icons": {
    "16": "16.png",
    "128": "128.png"
  }
}
