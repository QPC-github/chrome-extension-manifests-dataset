{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WbSrch Analytics",
  "version": "1.0",
  "author": "Jason Champion",
  "description": "The WbSrch Analytics domain analytics plugin.",
  "icons": {
    "48": "images/WbSrchLogo_48px.png",
    "96": "images/WbSrchLogo_96px.png"
  },
  "permissions": [
    "<all_urls>",
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "wbsrch_analytics.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "images/WbSrchLogo_19px.png",
      "38": "images/WbSrchLogo_38px.png"
    },
    "default_title": "No Site Loaded"
  }
}
