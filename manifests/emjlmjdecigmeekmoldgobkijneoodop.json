{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CopyText",
  "version": "1.1",
  "description": "CopyText",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery-3.1.0.min.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
