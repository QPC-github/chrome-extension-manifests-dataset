{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AtlasParts",
  "description": "Компания Атлас - запчасти и комплектующие для автомобилей производства США",
  "version": "1.0.6",
  "short_name": "AtlasParts UA",
  "icons": {
    "16": "/img/icon-atlas_16.png",
    "128": "/img/icon-atlas_128.png",
    "256": "/img/icon-atlas_256.png"
  },
  "browser_action": {
    "default_title": "AtlasParts",
    "default_icon": {
      "128": "/img/icon-atlas_128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*.atlasparts.com.ua/"
  ]
}
