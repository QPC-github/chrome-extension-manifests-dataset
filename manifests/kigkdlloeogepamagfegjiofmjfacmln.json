{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Events binding check for accessibility",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "This extension allow the developer to check if there are any event attached on element that ar not real action element",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "shame.js"
  ],
  "permissions": [
    "activeTab",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    }
  }
}
