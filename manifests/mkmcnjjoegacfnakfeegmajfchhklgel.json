{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "esmm-16.png",
    "32": "esmm-32.png",
    "48": "esmm-48.png",
    "128": "esmm-128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://esmm.ru/*"
    ]
  },
  "description": "ESMM.RU - сервис поиска и отбора постов по заданным параметрам для их автоматического размещения в сообществах ВКонтакте",
  "manifest_version": 2,
  "homepage_url": "http://esmm.ru",
  "name": "ESMM.RU",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.3"
}
