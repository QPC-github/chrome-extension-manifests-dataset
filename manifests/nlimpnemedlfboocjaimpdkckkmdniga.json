{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "model-kartei.de Notifier",
  "short_name": "MK Notifier",
  "version": "5.0.0.0",
  "icons": {
    "16": "assets/images/icon-default_16px.png",
    "48": "assets/images/icon-default_48px.png",
    "64": "assets/images/icon-default_64px.png",
    "128": "assets/images/icon-default_128px.png"
  },
  "browser_action": {
    "default_icon": "assets/images/icon-default_128px.png",
    "default_popup": "notifier.html"
  },
  "background": {
    "scripts": [
      "assets/js/jquery-3.2.1.min.js",
      "assets/js/background.js"
    ]
  },
  "permissions": [
    "notifications",
    "https://api.model-kartei.de/"
  ]
}
