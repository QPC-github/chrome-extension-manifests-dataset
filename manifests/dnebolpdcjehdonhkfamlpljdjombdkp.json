{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SA Bookmark Opener",
  "description": "Opens all last read links on your SomethingAwful.com bookmarks page.",
  "version": "1.0",
  "short_name": "SA Bookmark Opener",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://forums.somethingawful.com/usercp.php"
  ],
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
