{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.9",
  "version_name": "0.1.9 beta",
  "manifest_version": 2,
  "name": "First Impression",
  "description": "A game that shows you six images, among which three of a certain profession. Can you guess right?",
  "homepage_url": "https://github.com/openstate/labourgames-firstimpression",
  "icons": {
    "48": "icons/page-48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "icons/page-32.png",
      "48": "icons/page-48.png"
    },
    "default_title": "Play First Impression"
  },
  "permissions": [
    "https://www.linkedin.com/",
    "https://media.licdn.com/",
    "cookies"
  ],
  "author": "Open State Foundation",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "minimum_chrome_version": "57"
}
