{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/icons/icon128.png"
  },
  "content_scripts": [
    {
      "css": [
        "css/flairs.css",
        "css/settings.css"
      ],
      "js": [
        "js/jquery-3.1.1.min.js",
        "js/jquery-extensions.js",
        "js/css.js",
        "js/settings.js",
        "js/data.js",
        "js/enhance.js",
        "js/highlight.js",
        "js/notify.js",
        "js/autoload.js",
        "js/shubblerbot.js",
        "js/main.js"
      ],
      "matches": [
        "https://*.reddit.com/r/globaloffensivetrade*",
        "https://*.reddit.com/r/GlobalOffensiveTrade*"
      ]
    }
  ],
  "description": "This extension enhances the experience on the /r/GlobalOffensiveTrade subreddit.",
  "icons": {
    "16": "img/icons/icon16.png",
    "32": "img/icons/icon32.png",
    "48": "img/icons/icon48.png",
    "96": "img/icons/icon96.png",
    "128": "img/icons/icon128.png"
  },
  "manifest_version": 2,
  "short_name": "/r/GOTrade enhancer",
  "name": "/r/GlobalOffensiveTrade enhancer",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "https://steamrep.com/*",
    "https://api.steampowered.com/"
  ],
  "version": "1.1.0.1",
  "version_name": "1.1 release 1 Hotfix",
  "web_accessible_resources": [
    "img/*",
    "html/*",
    "js/*"
  ]
}
