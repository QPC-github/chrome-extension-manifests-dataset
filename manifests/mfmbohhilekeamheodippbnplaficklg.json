{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Drive File List creator",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Google Drive File List",
    "default_popup": "menu.html"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2,
  "name": "Google Drive File List Printer",
  "short_name": "DriveFileList",
  "version": "1.2.1",
  "offline_enabled": true
}
