{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TBA Search",
  "version": "1.1",
  "description": "Search TBA from selection",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "128": "128.png",
      "192": "192.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "128": "128.png",
    "192": "192.png"
  },
  "manifest_version": 2
}
