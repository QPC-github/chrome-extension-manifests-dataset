{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Browser Notes",
  "description": "Persistant notepad in your browser",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "BrowserNotes"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
