{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "js/script.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "description": "Instant movie quotes directly on your new tab.",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "32": "images/icon-32.png"
  },
  "manifest_version": 2,
  "name": "Kwote",
  "version": "0.2"
}
