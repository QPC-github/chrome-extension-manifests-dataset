{
  "content_scripts": [
    {
      "js": [
        "NouAd.js"
      ],
      "matches": [
        "http://www.youtube.com/watch?v=*",
        "https://www.youtube.com/watch?v=*",
        "http://youtube.com/watch?v=*",
        "https://youtube.com/watch?v=*"
      ]
    }
  ],
  "description": "Enjoy youtube videos without preroll ads",
  "icons": {
    "128": "images/128x128.jpg",
    "19": "images/19x19.jpg",
    "48": "images/48x48.jpg",
    "64": "images/64x64.jpg"
  },
  "manifest_version": 2,
  "name": "NouAd",
  "page_action": {
    "default_icon": "images/19x19.jpg",
    "default_title": "NouAd"
  },
  "permissions": [
    "tabs",
    "http://www.youtube.com/watch?v=*",
    "https://www.youtube.com/watch?v=*",
    "http://youtube.com/watch?v=*",
    "https://youtube.com/watch?v=*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.3"
}
