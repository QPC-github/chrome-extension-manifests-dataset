{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Assistant Shopping Futureo",
  "description": "Achat après achat préparez votre retraite sans effort !",
  "version": "1.0.10",
  "permissions": [
    "tabs",
    "notifications",
    "cookies",
    "idle",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "Futureo/Toolbar/Icons/Futureo-24x24.png",
    "default_popup": "Futureo/Toolbar/Pages/Popup.html"
  },
  "icons": {
    "16": "Futureo/Toolbar/Icons/Futureo-16x16.png",
    "24": "Futureo/Toolbar/Icons/Futureo-24x24.png",
    "32": "Futureo/Toolbar/Icons/Futureo-32x32.png",
    "48": "Futureo/Toolbar/Icons/Futureo-48x48.png"
  },
  "background": {
    "page": "Futureo/Toolbar/Pages/Background.html"
  }
}
