{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Super Opinionated Screenshot",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Takes screenshots at opinionated sizes.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
