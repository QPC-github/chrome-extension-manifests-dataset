{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "せどりランキングリサーチ by せどりRank完全版",
  "short_name": "せどりランキングリサーチ by せどりRank完全版",
  "version": "4.2.0",
  "manifest_version": 2,
  "description": "Amazonの各商品ページにランキング、価格、出品者数の変動グラフとせどりRank完全版のリンクを追加します。出品制限の確認、各出品者の在庫数リサーチも可能。",
  "web_accessible_resources": [
    "img/logo.svg",
    "options.html"
  ],
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/*",
        "https://www.amazon.co.jp/*",
        "https://sellercentral-japan.amazon.com/inventory/*",
        "https://sellercentral-japan.amazon.com/hz/*",
        "https://sellercentral-japan.amazon.com/fba/*",
        "https://sellercentral.amazon.co.jp/*",
        "https://www.amazon.com/*",
        "https://www.amazon.co.uk/*",
        "https://www.amazon.ca/*",
        "https://www.amazon.it/*",
        "https://www.amazon.de/*",
        "https://www.amazon.es/*",
        "https://www.amazon.fr/*",
        "https://www.amazon.in/*",
        "https://www.amazon.cn/*",
        "https://www.amazon.com.br/*",
        "https://www.amazon.com.mx/*"
      ],
      "js": [
        "js/add_contents.js",
        "js/add_link.js",
        "js/add_restriction.js",
        "js/add_stock.js",
        "js/jquery.min.js",
        "js/script.js"
      ],
      "css": [
        "css/style.css"
      ],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
