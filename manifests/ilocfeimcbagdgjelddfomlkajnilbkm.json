{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aliexpress shortcut",
  "version": "1.0",
  "description": "Get fast and easy access to your favourite online store.",
  "browser_action": {
    "default_title": "Aliexpress shortcut"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
