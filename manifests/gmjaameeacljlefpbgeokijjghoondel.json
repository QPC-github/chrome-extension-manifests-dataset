{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "newTab",
  "version": "1.1",
  "description": "Redirects new tabs in chrome to a specified page",
  "author": "Keith Wong",
  "browser_action": {
    "default_icon": {
      "16": "images/badge16.png",
      "32": "images/badge32.png",
      "64": "images/badge64.png"
    },
    "default_popup": "popup.html",
    "default_title": "Set your homepage"
  },
  "icons": {
    "48": "images/badge48.png",
    "128": "images/badge128.png"
  },
  "homepage_url": "http://keithwong.info",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "homepage.html"
  }
}
