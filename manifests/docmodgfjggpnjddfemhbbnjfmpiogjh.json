{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iOrder",
  "version": "1.6",
  "description": "An innovative and efficient way to manage your online purchases.",
  "browser_action": {
    "default_title": "iOrder",
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "storage",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
