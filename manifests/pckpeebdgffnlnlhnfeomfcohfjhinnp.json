{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Probably Yours",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Adds a hint about the origin of products for sale on Craigslist.org",
  "content_scripts": [
    {
      "matches": [
        "http://*.craigslist.org/*"
      ],
      "js": [
        "jquery.js",
        "app.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*.craigslist.org"
  ]
}
