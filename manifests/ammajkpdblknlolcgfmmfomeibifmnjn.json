{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Share.to",
  "permissions": [
    "desktopCapture",
    "*://*.share.to/",
    "*://*.shareto.dev/",
    "*://ushare.to/*",
    "*://localhost/*",
    "*://dreamshareto.com/*",
    "*://*.ipdream.co.jp/*",
    "*://*.shareto.nxera.net/*",
    "*://*.goldip.tech/*",
    "*://*.dcj.ip-dream.jp/*",
    "*://*.share-stg.e-kakushin.com/*",
    "*://*.share.company.com/*",
    "*://*.jshare.jpbm.jp/*",
    "*://*.ip-dream.jp/*",
    "*://dc.core.co.jp/*",
    "*://*.teamtalk.to/*",
    "tabs",
    "geolocation",
    "notifications",
    "storage",
    "unlimitedStorage",
    "webRequest"
  ],
  "short_name": "Share.to",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2.2",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension for Share.to",
  "externally_connectable": {
    "matches": [
      "*://*.share.to/*",
      "*://*.ushare.to/*",
      "*://*.shareto.dev/*",
      "*://*.ushare.to/*",
      "*://ushare.to/*",
      "*://localhost/*",
      "https://media1.share.to:3004/*",
      "http://media1.share.to:3004/*",
      "*://*.programming.com/*",
      "*://*.prg.am/*",
      "*://*.ipdream.co.jp/*",
      "*://*.shareto.nxera.net/*",
      "*://*.goldip.tech/*",
      "*://*.dcj.ip-dream.jp/*",
      "*://*.share-stg.e-kakushin.com/*",
      "*://*.share.company.com/*",
      "*://*.jshare.jpbm.jp/*",
      "*://*.ip-dream.jp/*",
      "*://*.dc.core.co.jp/*",
      "*://*.teamtalk.to/*"
    ]
  },
  "icons": {
    "128": "shareto_icon_128.png",
    "16": "shareto_icon_16.png",
    "48": "shareto_icon_48.png"
  },
  "browser_action": {
    "default_icon": "shareto_icon_16.png"
  }
}
