{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Berned Out",
  "version": "1.0.0",
  "author": "Joshua Turner",
  "homepage_url": "http://joshuaturner.co",
  "description": "Okay, we get it. You like Bernie.",
  "content_scripts": [
    {
      "matches": [
        "https://*.reddit.com/*"
      ],
      "run_at": "document_start",
      "css": [
        "style.css"
      ],
      "js": [
        "/lib/jquery.min.js",
        "main.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    "/img/on.png",
    "/img/off.png"
  ]
}
