{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Timely Bookmarks",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Get the right bookmarks at the right time",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "resources/icons/timely-16.png",
    "default_title": "Timely Bookmarks",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "resources/icons/timely-16.png",
    "48": "resources/icons/timely-48.png",
    "128": "resources/icons/timely-128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "identity",
    "storage",
    "tabs"
  ],
  "oauth2": {
    "client_id": "531018824200-fr6jrregjdojnd12npb0nqg6tql2cth3.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
