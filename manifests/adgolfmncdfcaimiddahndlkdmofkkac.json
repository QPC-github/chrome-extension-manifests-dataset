{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Real Full",
  "version": "1.0",
  "description": "Real fullscreen for flash elements.",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "arrow_expand.png",
    "default_title": "Fullscreenify!"
  }
}
