{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Splatoon Map Info Extension",
  "description": "Condensed map rotation information for Splatoon.",
  "version": "1.17",
  "author": "analblaze",
  "short_name": "Splatoon Extension",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png",
      "128": "icon128.png",
      "64": "icon64.png",
      "48": "icon48.png",
      "32": "icon32.png",
      "16": "icon16.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png",
    "19": "icon19.png",
    "38": "icon38.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
