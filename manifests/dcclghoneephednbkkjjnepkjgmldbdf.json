{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rubber New Tab",
  "version": "2.1",
  "description": "A rubbery simple start page inspired by Knew Tab and Humble New Tab Page",
  "homepage_url": "https://github.com/sharat87/rubber-tab",
  "icons": {
    "128": "icons/icon-128.png",
    "48": "icons/icon-48.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "offline_enabled": true,
  "permissions": [
    "management",
    "bookmarks",
    "geolocation",
    "topSites",
    "tabs",
    "cookies",
    "<all_urls>"
  ],
  "manifest_version": 2
}
