{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Netsuite GoGet Record by ID",
  "version": "1.0.0.2",
  "description": "Open/View netsuite record with internal id",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/N.png",
      "32": "images/N.png",
      "48": "images/N.png",
      "128": "images/N.png"
    }
  },
  "icons": {
    "16": "images/N.png",
    "32": "images/N.png",
    "48": "images/N.png",
    "128": "images/N.png"
  },
  "manifest_version": 2
}
