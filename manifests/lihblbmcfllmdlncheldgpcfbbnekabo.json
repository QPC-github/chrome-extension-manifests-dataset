{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LFWidthChecker",
  "description": "Analyze the webpage DOM comparing the widths with the widths from raw CSS, to find nested html elements larger than parent nodes",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://*/*",
    "webNavigation"
  ],
  "devtools_page": "dev_tools.html",
  "options_page": "panel.html",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html"
  }
}
