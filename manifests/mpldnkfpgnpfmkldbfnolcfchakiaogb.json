{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sips Horn",
  "description": "Puts the horn back in Sips",
  "version": "2",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "jquery-1.12.4.min.js",
        "script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "foghorn.mp3",
    "chiptune.mp3"
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
