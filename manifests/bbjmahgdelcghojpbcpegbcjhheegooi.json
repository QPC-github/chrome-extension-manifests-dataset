{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "short_name": "StreamCalc",
  "name": "Streaming Calculator",
  "description": "Streaming media calculator for internet radio or tv stations.",
  "version": "1.1",
  "homepage_url": "http://streamcalc.casterclub.com/",
  "icons": {
    "16": "images/blue_calculator-16x16.png",
    "32": "images/blue_calculator-32x32.png",
    "48": "images/blue_calculator-48x48.png",
    "64": "images/blue_calculator-64x64.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_title": "StreamCalc",
    "default_icon": "images/blue_calculator-80x80.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webNavigation",
    "webRequest",
    "contentSettings",
    "http://*/",
    "https://*/",
    "https://*.google.com/*",
    "http://*.google.com/*",
    "https://*.googleapis.com/*",
    "http://*.googleapis.com/*"
  ]
}
