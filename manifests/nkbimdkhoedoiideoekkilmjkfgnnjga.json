{
  "background": {
    "scripts": [
      "js/background.js",
      "js/jquery.min.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon@16.png",
      "32": "img/icon@32.png"
    },
    "default_title": "MyFavoriteRecipes New Tab"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://myfavoriterecipes.co/favicon.png",
      "image_url": "http://myfavoriterecipes.co/search-redirect.php?keyword={searchTerms}&type=images",
      "is_default": true,
      "keyword": "Recipes",
      "name": "MyFavoriteRecipes New Tab",
      "search_url": "http://myfavoriterecipes.co/search-redirect.php?keyword={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "description": "This extension provide you with your favorite food and recipe features and also allows you to conduct everyday web searches.",
  "icons": {
    "48": "img/icon@48.png",
    "96": "img/icon@96.png"
  },
  "content_security_policy": "script-src 'self' https://searchfuelsuggest.seccint.com/ ; object-src 'self'",
  "key": "",
  "manifest_version": 2,
  "name": "MyFavoriteRecipes New Tab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2",
  "web_accessible_resources": [
    "css/auto-complete.css"
  ]
}
