{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rapid Invoice free online billing & invoicing",
  "description": "Manage your articles,addresses,warehouses & invoices anywhere. Available in English,Deutsch,Español,Françoise,Italiano,Portoghese",
  "version": "1.3",
  "default_locale": "en",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "urls": [
      "http://www.rapid-invoice.com/Default.aspx?pLang=S_DE",
      "http://www.rapid-invoice.biz/Default.aspx?pLang=S_DE",
      "http://www.rapid-invoice.us/Default.aspx?pLang=S_DE",
      "http://www.rapid-invoice.eu/Default.aspx?pLang=S_DE",
      "http://www.rapid-invoice.net/Default.aspx?pLang=S_DE",
      "http://www.rapid-invoice.info/Default.aspx?pLang=S_DE"
    ],
    "launch": {
      "web_url": "http://app.rapid-invoice.biz/Pages/Login/UserLogin.aspx?pLang=S_DE"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
