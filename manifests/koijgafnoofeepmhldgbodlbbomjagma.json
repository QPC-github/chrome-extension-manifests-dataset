{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery/jquery-2.2.3.min.js",
      "jquery/jquery-ui.min.js",
      "jQRangeSlider-5.7.2/lib/jquery.mousewheel.min.js",
      "jQRangeSlider-5.7.2/jQAllRangeSliders-min.js",
      "jquery-toggles-master/toggles.min.js",
      "tabSelect/jquery.tabselect-0.2.js",
      "criterias.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "criterias.html",
    "default_title": "__MSG_appName__"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "fr",
  "description": "__MSG_appDesc__",
  "icons": {
    "48": "Icon-48.png",
    "64": "Icon-64.png",
    "128": "Icon-128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "1.6",
  "web_accessible_resources": [
    "Icon-28.png"
  ]
}
