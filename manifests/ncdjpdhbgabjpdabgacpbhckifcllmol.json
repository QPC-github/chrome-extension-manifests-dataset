{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CaretTab v3 Beta",
  "short_name": "CaretTab",
  "description": "A beautifully simplistic New Tab page that includes multiple clocks, date, search, quick links, and more! Fully customizable!",
  "version": "3.0.0.5",
  "version_name": "3.0.0 Beta 5",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
