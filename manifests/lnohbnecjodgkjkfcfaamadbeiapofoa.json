{
  "browser_action": {
    "deafaul_icon": "img/icon.png",
    "defalut_title": "Warlord Script"
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "xml2js.js",
        "md5.js",
        "script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "description": "Расширение для ВКонтакте, которое позволяет смотреть характеристики игроков игры «WARLORD».",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2,
  "name": "Warlord Script",
  "permissions": [
    "*://api.vk.com/*",
    "*://*.geronimo.su/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4.5"
}
