{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Not Today!",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Not Today! boosts your productivity and web safety by letting you specify sites to block.",
  "options_page": "settings.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "NotToday_16.png",
    "48": "NotToday_48.png",
    "128": "NotToday_128.png"
  },
  "permissions": [
    "storage"
  ],
  "homepage_url": "http://www.fongsoft.com"
}
