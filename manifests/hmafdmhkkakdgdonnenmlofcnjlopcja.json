{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Back/Forward Scroll",
  "description": "This extension lets horizontal scrolling go back and forward",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.3.min.js",
        "jquery.mousewheel.js",
        "backforward.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
