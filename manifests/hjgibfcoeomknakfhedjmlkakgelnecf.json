{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "0.11.11",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icons/icon16.png",
    "128": "assets/icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "utility.js",
      "notifications.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "*://*/*",
    "contextMenus",
    "downloads",
    "management",
    "notifications",
    "storage",
    "tabs"
  ]
}
