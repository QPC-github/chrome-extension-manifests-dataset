{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BeazeNet OFFLINE",
  "description": "BeazeNet Offline Point of Sale Terminal",
  "version": "2.3",
  "manifest_version": 2,
  "permissions": [
    "https://localhost:8443/*",
    "https://*.beaze.net/*"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "content_security_policy": "default-src unsafe-eval",
  "icons": {
    "16": "cloud.ico",
    "128": "cloud.ico"
  }
}
