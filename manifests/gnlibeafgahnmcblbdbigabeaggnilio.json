{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "While1 Voice Notifier",
  "short_name": "While1 Voice NT",
  "description": "This extension shows a caller info popup for the current call",
  "version": "2.1.2",
  "background": {
    "scripts": [
      "javascript/autobahn.min.js",
      "javascript/jquery.min.js",
      "javascript/scripts.js",
      "javascript/init-background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
