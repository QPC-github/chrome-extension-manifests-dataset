{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Share via Email",
  "description": "Send a link to the current page using your default email client.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Share via Email"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
