{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Storage",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Stores data in chrome sync",
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
