  "name": "Mobile.de Ad Filter",
  "description": "Select adds which you no longer want to see. Use it to hide ads that don't meet your requirements.",
  "version": "1.1",
  "icons": {
    "128": "mob_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://suchen.mobile.de/fahrzeuge/search.html*"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
