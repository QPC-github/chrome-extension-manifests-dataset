{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PSG Actus: live actualités du PSG",
  "description": "Toute l'actualité en live du PSG, club de foot Paris Saint Germain",
  "background": {
    "page": "background.html"
  },
  "default_locale": "fr",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "*://*/*"
  ],
  "version": "1.1"
}
