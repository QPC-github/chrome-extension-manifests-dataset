{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nervedful",
  "version": "1.1.3",
  "manifest_version": 2,
  "description": "Rozszerzenie do przeglądarki Google Chrome informujące o nowych filmach Nerveda",
  "homepage_url": "http://wruczek.top",
  "icons": {
    "16": "assets/img/icon-16x16.png",
    "32": "assets/img/icon-32x32.png",
    "64": "assets/img/icon-64x64.png",
    "128": "assets/img/icon-128x128.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "https://*.ytimg.com/"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/img/icon-16x16.png",
      "32": "assets/img/icon-32x32.png",
      "64": "assets/img/icon-64x64.png",
      "128": "assets/img/icon-128x128.png"
    },
    "default_title": "Nervedful - opcje rozszerzenia",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "page": "background/background.html"
  }
}
