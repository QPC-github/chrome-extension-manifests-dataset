{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Clean up games.on.net Forums",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Pew pew that crazy games.on.net Forums header",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://games.on.net/forums*"
      ],
      "css": [
        "games-on-net-cleanup.css"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "games-on-net-cleanup.js"
      ]
    }
  ],
  "permissions": [
    "*://games.on.net/forums*"
  ]
}
