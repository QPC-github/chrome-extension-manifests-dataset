{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Daily Read",
  "description": "This extension pulls up the websites you read daily with two clicks.",
  "version": "1.1",
  "icons": {
    "128": "NewsPaper_128x128.png"
  },
  "browser_action": {
    "default_popup": "DailyRead.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
