{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Quick QR",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Create a QR-Code from every site on the web, for instance for your mobile-phone.",
  "icons": {
    "128": "images/icon_128.png"
  },
  "options_page": "html/settings.html",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Quick QR",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
