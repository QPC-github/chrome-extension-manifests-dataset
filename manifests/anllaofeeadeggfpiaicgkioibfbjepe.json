{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tumblr Timestamps",
  "version": "1.0.7",
  "manifest_version": 2,
  "description": "Puts timestamps in footers of posts in tumblr dashboard",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.tumblr.com/*"
      ],
      "js": [
        "jquery-1.8.3.min.js",
        "content.js"
      ]
    }
  ]
}
