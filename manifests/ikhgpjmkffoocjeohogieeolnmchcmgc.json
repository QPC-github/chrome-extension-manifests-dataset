{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.js",
        "contentScript.js"
      ],
      "matches": [
        "*://www.reddit.com/*"
      ]
    }
  ],
  "description": "Converts Imgur URLs To Filmot URLs",
  "key": "",
  "manifest_version": 2,
  "name": "ImgurAtWork",
  "permissions": [
    "*://www.reddit.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4.1",
  "web_accessible_resources": [
    "contentScript.js",
    "jquery.js",
    "jquery-1.10.2.min.map"
  ]
}
