{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icons/favicon.png",
    "default_title": "Nhập hàng trung quốc với 1688viet.com"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "include/styles.css",
        "include/menu.css"
      ],
      "js": [
        "include/jquery-2.2.1.min.js",
        "include/content.js"
      ],
      "matches": [
        "http://*.1688.com/*",
        "http://*.taobao.com/*",
        "http://*.tmall.com/*",
        "https://*.1688.com/*",
        "https://*.taobao.com/*",
        "https://*.tmall.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Mua hàng tại Taobao,Tmall, 1688 cùng 1688viet.com",
  "icons": {
    "512": "icons/favicon.png"
  },
  "manifest_version": 2,
  "name": "1688viet",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "6.9"
}
