{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TemplerFX",
  "version": "2.1.0.4",
  "description": "TemplerFX extension",
  "permissions": [
    "tabs",
    "webNavigation",
    "contextMenus",
    "http://templerfx.com/*"
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_title": "TemplerFX",
    "default_icon": "images/128.png",
    "default_popup": "popup.html"
  }
}
