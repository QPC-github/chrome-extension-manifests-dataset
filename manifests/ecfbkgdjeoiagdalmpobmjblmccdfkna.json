{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "News Scrolls",
  "description": "Trusted source of fastest news on the earth",
  "version": "0.0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "unlimitedStorage"
  ],
  "icons": {
    "128": "icon.png"
  }
}
