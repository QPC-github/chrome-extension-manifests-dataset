{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trello RTL",
  "version": "0.1.0",
  "description": "Make trello.com RTL",
  "content_scripts": [
    {
      "js": [
        "js/disable-original-styles.js"
      ],
      "css": [
        "css/rtl.css"
      ],
      "matches": [
        "https://trello.com/*"
      ]
    }
  ],
  "icons": {
    "128": "img/icons/128.png"
  }
}
