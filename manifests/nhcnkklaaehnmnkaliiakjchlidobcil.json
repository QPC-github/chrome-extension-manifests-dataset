{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.1",
  "manifest_version": 2,
  "name": "Policy Control",
  "short_name": "policy-control-wext",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contentSettings"
  ],
  "homepage_url": "https://mybrowseraddon.com/policy-control-wext.html",
  "description": "Gain full control over all content settings (cookie, images, javascript, plugins, etc).",
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    },
    "default_popup": "data/popup/popup.html"
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
