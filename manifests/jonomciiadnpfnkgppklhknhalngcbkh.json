{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FTFN (Fixed That For News)",
  "short_name": "FTFN",
  "version": "0.1.3",
  "description": "De-euphemizes references to torture",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
