{
  "author": "FJ, Inc.",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*.financialjuice.com/*"
      ]
    }
  ],
  "description": "This Chrome extension is developed primary for Https://www.financialjuice.com to do screen sharing",
  "homepage_url": "Https://www.financialjuice.com/",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Fj Screen ",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.5",
  "web_accessible_resources": [
    "icon.png",
    "logo16.png"
  ]
}
