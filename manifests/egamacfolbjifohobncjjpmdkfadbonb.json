{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Denizan",
  "icons": {
    "128": "128x128.png"
  },
  "description": "a Traffic Exchange System",
  "version": "5.0.2",
  "browser_action": {
    "default_icon": "19x19.png",
    "default_title": "Quick access to browsing"
  },
  "background": {
    "scripts": [
      "jquery-1.6.2.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "inject.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "webRequest",
    "storage",
    "cookies",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
