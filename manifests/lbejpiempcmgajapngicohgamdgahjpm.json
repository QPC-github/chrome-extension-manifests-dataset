{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Type Helper plus",
  "version": "0.1.3",
  "description": "Auto-Complete with Shift Key + Any Alphabet",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "typesupport.html",
    "default_icon": "icons/icon16.png"
  },
  "content_scripts": [
    {
      "js": [
        "extension.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ]
}
