{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fox News",
  "version": "2.4.4",
  "description": "The official Fox News extension for Google Chrome lets you follow our most popular news before anyone else.",
  "icons": {
    "16": "images/fn-logo-16x16.png",
    "48": "images/fn-logo-48x48.png",
    "128": "images/fn-logo-128x128.png"
  },
  "browser_action": {
    "default_icon": "images/fn-logo-16x16.png",
    "default_title": "Fox News",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'  https://api.foxnews.com; object-src 'self'"
}
