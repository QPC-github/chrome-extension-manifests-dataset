{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fullscreen Pixlr",
  "version": "0.1.7",
  "description": "By Aaron Saporito",
  "content_scripts": [
    {
      "css": [
        "full.css"
      ],
      "matches": [
        "*://pixlr.com/editor/"
      ]
    }
  ],
  "web_accessible_resources": [
    "full.css"
  ],
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
