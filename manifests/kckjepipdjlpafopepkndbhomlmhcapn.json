{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Поиск адреса",
  "version": "1.1.0",
  "description": "Данный сервис разбирает, анализирует, исправляет неполные и/или неправильно написанные почтовые адреса.",
  "icons": {
    "16": "logo_sh16.png",
    "48": "logo_sh.png",
    "128": "logo128.png"
  },
  "permissions": [
    "http://www.iqdq.ru/"
  ],
  "browser_action": {
    "dafault_title": "IQDQ",
    "default_icon": "logo_sh.png",
    "default_popup": "popup.html"
  }
}
