{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dólar Turismo Hoje",
  "description": "Veja a cotação do Dólar Turismo através do DolarHoje.com",
  "version": "0.0.1",
  "background": {
    "page": "app.html"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Dólar Turismo Hoje"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "http://DolarHoje.com/",
    "http://api.DolarHoje.com/",
    "https://DolarHoje.com/"
  ],
  "omnibox": {
    "keyword": "dolarturismohoje"
  },
  "content_scripts": [
    {
      "matches": [
        "https://dolarhoje.com/dolar-turismo/"
      ],
      "js": [
        "has_extension.js"
      ]
    }
  ]
}
