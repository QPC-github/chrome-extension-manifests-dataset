{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PinIt",
  "description": "PinIt adds a keyboard shortcut to quickly pin/unpin your tabs.",
  "version": "1.9",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
