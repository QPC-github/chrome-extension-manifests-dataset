{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "3.1.2",
  "description": "__MSG_desc__",
  "default_locale": "ru",
  "browser_action": {
    "default_title": "__MSG_title__",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/ok_128.png",
    "16": "img/ok_16.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "storage",
    "tabs",
    "<all_urls>",
    "management",
    "cookies"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'"
}
