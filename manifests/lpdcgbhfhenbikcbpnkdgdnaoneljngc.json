{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BackupAccount.com",
  "description": "Manage Your Users",
  "version": "1.2",
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "icons": {
    "128": "ba.png",
    "16": "ba.png",
    "32": "ba.png",
    "64": "ba.png"
  },
  "browser_action": {
    "default_icon": "ba.png",
    "default_title": "BackupAccount",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://secure.backupaccount.com; object-src 'self' https://secure.backupaccount.com; content-src https://secure.backupaccount.com",
  "omnibox": {
    "keyword": "ba"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "background",
    "tabs",
    "http://secure.backupaccount.com/"
  ]
}
