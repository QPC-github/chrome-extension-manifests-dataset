{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "While1 Voice Notifier Standalone App",
  "short_name": "While1 Voice NT AP",
  "description": "This extension shows a caller info popup for the current call, which can be opened in other browsers too",
  "version": "0.1.1",
  "app": {
    "background": {
      "scripts": [
        "javascript/init-background.js"
      ]
    }
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
