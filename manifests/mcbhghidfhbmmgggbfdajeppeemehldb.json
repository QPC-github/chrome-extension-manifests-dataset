{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fercard",
  "description": "Front-End Repo Card",
  "version": "1.0.3",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "notifications",
    "idle",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "./html/home.html"
  }
}
