{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Kiwi IRC",
  "description": "A web based IRC client",
  "version": "0.5.4",
  "manifest_version": 2,
  "app": {
    "urls": [
      "*://kiwiirc.com/",
      "*://www.kiwiirc.com/"
    ],
    "launch": {
      "web_url": "https://kiwiirc.com/client/"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
