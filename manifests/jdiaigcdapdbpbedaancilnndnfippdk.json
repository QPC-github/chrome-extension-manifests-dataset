{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FullTube",
  "version": "1.0",
  "description": "Button to make the current youtube video go fullscreen in your window.",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Youtube",
    "default_icon": "icon.png"
  },
  "icons": {
    "256": "icon.png"
  },
  "background": {
    "scripts": [
      "fulltube.js"
    ]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
