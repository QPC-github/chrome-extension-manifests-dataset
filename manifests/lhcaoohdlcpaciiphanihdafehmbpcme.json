{
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "web_accessible_resources": [
    "newtab.js"
  ],
  "description": "New Tab page with Wallpaper/Color, Safari-like Bookmarks, Rain and Full Moon.",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "name": "Walled New Tab mod",
  "options_page": "options.html",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "bookmarks",
    "chrome://favicon/",
    "topSites"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
