{
  "manifest_version": 2,
  "name": "VKTinyReposts",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.3.0.4",
  "description": "Оптимизатор ленты VK. Сворачивает репосты сообщений из групп и пабликов в короткие заголовки.",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://vk.com/*",
        "https://vk.com/*",
        "http://new.vk.com/*",
        "https://new.vk.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "key": ""
}
