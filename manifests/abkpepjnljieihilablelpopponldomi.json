{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Partage Bureau Fiducom",
  "description": "Partage Bureau Fiducom",
  "version": "0.3.0",
  "minimum_chrome_version": "50",
  "icons": {
    "16": "fiducial-logo-16x16.png",
    "48": "fiducial-logo-48x48.png",
    "128": "fiducial-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://fiducom.fiducial.fr/*",
      "*://fiducomtest.fiducial.fr/*",
      "*://fiducom-ext.fiducial.fr/*",
      "*://fiducomtest-ext.fiducial.fr/*",
      "*://jitsi.arthur.test/*",
      "*://lxlyofdc30.fiducial.dom/*"
    ]
  }
}
