{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Widen Git",
  "description": "This extension widens the boxes in GIT through changing the CSS",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "WIDEN!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "gitstyle.css"
  ],
  "manifest_version": 2
}
