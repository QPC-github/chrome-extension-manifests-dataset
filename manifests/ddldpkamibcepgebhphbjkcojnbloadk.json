{
  "name": "Simple Newtab - HD Wallpaper",
  "short_name": "simple new tab",
  "description": "Simple newtab is an amazing product, it brings you great HD images weekly, it's simple and clear.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "assets/icons/icon-128.png",
    "32": "assets/icons/icon-32.png",
    "16": "assets/icons/icon-16.png",
    "48": "assets/icons/icon-48.png"
  },
  "offline_enabled": true,
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "sessions",
    "background",
    "geolocation",
    "management",
    "bookmarks",
    "unlimitedStorage",
    "topSites",
    "identity",
    "http://*/",
    "https://*/",
    "chrome://favicon/",
    "history",
    "alarms",
    "notifications",
    "tabs",
    "storage",
    "activeTab",
    "declarativeContent",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies"
  ],
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.12"
}
