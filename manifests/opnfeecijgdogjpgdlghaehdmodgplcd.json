{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Simple Twitter",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Simple Twitter is a Google Chrome extension which hides some of Twitter's more attention provoking features.",
  "icons": {
    "128": "content/images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "content/css/content.css"
      ]
    }
  ]
}
