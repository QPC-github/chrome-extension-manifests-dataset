{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Screenshot Search",
  "short_name": "Screenshot Search",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Ahmed Dousa - goldendousa@gmail.com",
  "description": "This extension will help you to search your screenshots, easily with just few steps.",
  "version": "1.0",
  "permissions": [
    "http://urgueo.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
