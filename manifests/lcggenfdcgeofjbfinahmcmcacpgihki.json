{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Transfer",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Create a QR Code out of this page for transferring to another device/computer",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
