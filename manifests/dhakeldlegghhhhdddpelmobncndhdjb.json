{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "habbit",
  "short_name": "habbit",
  "description": "Remove addictiveness of reddit",
  "version": "1.0",
  "permissions": [
    "*://*.reddit.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "matches": [
        "*://*.reddit.com/*"
      ]
    }
  ]
}
