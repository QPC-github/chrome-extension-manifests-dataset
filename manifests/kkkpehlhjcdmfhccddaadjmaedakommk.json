{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "edwisor screen sharing",
  "description": "screen sharing extension.",
  "version": "0.1.2",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://127.0.0.1/*",
      "*://*.live.edwisor.com/*",
      "*://*.edwisor.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
