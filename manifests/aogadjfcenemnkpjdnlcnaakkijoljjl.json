{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inspirational Quotes & More.",
  "version": "0.0.2",
  "description": "A New Tab Extension to get your daily dose of inspiration. Also lets you organize and manage the way you explore the internet",
  "manifest_version": 2,
  "permissions": [],
  "web_accessible_resources": [
    "images/*.png"
  ],
  "browser_action": {
    "default_title": "New Tab",
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Search",
      "keyword": "findat",
      "search_url": "https://findat.net/",
      "favicon_url": "https://findat.net/static/img/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "content_security_policy": "script-src 'self' https://z-na.amazon-adsystem.com https://aax-us-east.amazon-adsystem.com; object-src 'self'",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
