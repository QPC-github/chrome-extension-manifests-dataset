{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Gmail zed",
  "version": "0.2",
  "manifest_version": 2,
  "description": "An elegant, grayish Gmail skin",
  "icons": {
    "16": "gmailzed-icon-16.png",
    "48": "gmailzed-icon-48.png",
    "128": "gmailzed-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "css": [
        "gmailzed.css"
      ]
    }
  ]
}
