{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Talkz",
  "short_name": "Talkz",
  "version": "0.3",
  "minimum_chrome_version": "23",
  "permissions": [
    "http://*.talkz.com/",
    "https://*.talkz.com/",
    "https://www.google-analytics.com/",
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "img/talkz-logo-16.png",
    "48": "img/talkz-logo-48.png",
    "128": "img/talkz-logo-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/ga/google-analytics-bundle.js",
        "js/utils/base64.js",
        "js/utils/utils.js",
        "js/utils/notification-event.js",
        "js/views/view-manager.js",
        "js/views/notification-manager.js",
        "js/talkz/sticker-api/user.js",
        "js/talkz/sticker-api/sticker.js",
        "js/talkz/sticker-api/api.js",
        "js/talkz/sticker-api/session.js",
        "js/background.js"
      ]
    }
  }
}
