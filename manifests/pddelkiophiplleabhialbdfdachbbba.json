{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "# - Domain Hashlink",
  "description": "To use, type '#' and a search term/space into the Omnibox.",
  "version": "1.0.3",
  "icons": {
    "128": "favicon.ico"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://resolution.domainhashlink.com/*"
  ],
  "options_page": "options.html",
  "omnibox": {
    "keyword": "#"
  },
  "browser_action": {
    "default_icon": "favicon.ico"
  },
  "content_security_policy": "default-src 'none'; script-src 'self'; connect-src http://resolution.domainhashlink.com;",
  "minimum_chrome_version": "9",
  "manifest_version": 2
}
