{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clean Up JIRA",
  "description": "Developed by 배강민",
  "version": "0.0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "cleanUpJira_19_icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "cleanUpJira_16.png",
    "48": "cleanUpJira_48.png",
    "128": "cleanUpJira_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
