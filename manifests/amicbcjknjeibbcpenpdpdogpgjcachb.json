{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Add a little class to the Record Pub.",
  "name": "Record Times",
  "permissions": [
    "http://www.recordpub.com/*"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.recordpub.com/*"
      ],
      "css": [
        "recordtimes.css"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "0.6.2"
}
