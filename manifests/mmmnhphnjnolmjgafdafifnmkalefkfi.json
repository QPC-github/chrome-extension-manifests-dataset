{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lookout Lockout",
  "description": "This extension prevents procrastination",
  "version": "1.0",
  "icons": {
    "128": "lock128.png"
  },
  "browser_action": {
    "default_icon": "lock128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "alarms",
    "tabs",
    "storage",
    "cookies",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
