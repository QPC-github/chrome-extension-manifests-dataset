{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quotes From Dal",
  "version": "0.0.2",
  "description": "Random quotes from the @QuotesFromDal twitter page",
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Random quotes",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://quotesfromdal.herokuapp.com/"
  ]
}
