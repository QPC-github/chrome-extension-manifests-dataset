{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sai Tab Chrome Extension",
  "version": "1.11",
  "description": "Sai Ram!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "short_name": "Sai Tab",
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/sai-logo-16.png",
      "32": "assets/sai-logo-32.png",
      "48": "assets/sai-logo-64.png",
      "128": "assets/sai-logo-128.png"
    }
  },
  "icons": {
    "16": "assets/sai-logo-16.png",
    "32": "assets/sai-logo-32.png",
    "48": "assets/sai-logo-64.png",
    "128": "assets/sai-logo-128.png"
  },
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "options.html"
  }
}
