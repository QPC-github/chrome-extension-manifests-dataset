{
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js",
      "meogo.js"
    ]
  },
  "browser_action": {
    "default_title": "__MSG_title__"
  },
  "default_locale": "ru",
  "description": "__MSG_desc__",
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_name__",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "<all_urls>",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
