{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SQLizer",
  "manifest_version": 2,
  "version": "1.2.1",
  "description": "Query every HTML table with SQL.",
  "author": "Johannes Schildgen",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
