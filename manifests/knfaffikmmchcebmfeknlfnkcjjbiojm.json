{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MarksBlank",
  "description": "新页签显示书签",
  "version": "2.0.0",
  "permissions": [
    "bookmarks",
    "history"
  ],
  "icons": {
    "16": "pics/icon16.png",
    "48": "pics/icon48.png",
    "128": "pics/icon128.png"
  },
  "browser_action": {
    "default_title": "MarksBlank"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "NewBlank_MP.html"
  }
}
