{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "persistent": true,
    "scripts": [
      "background.js",
      "download.js",
      "sha256-hashworker.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/download18.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_browser_action_title__"
  },
  "options_page": "options.html",
  "default_locale": "en",
  "description": "__MSG_chrome_extension_description__",
  "icons": {
    "128": "images/download128.png",
    "64": "images/download64.png",
    "18": "images/download18.png"
  },
  "manifest_version": 2,
  "name": "__MSG_chrome_extension_name__",
  "permissions": [
    "downloads",
    "http://*/*",
    "https://*/*",
    "notifications",
    "tabs",
    "management"
  ],
  "short_name": "Check Download",
  "version": "0.0.2"
}
