{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Plugd: Sneaker Comparisons & Deals",
  "short_name": "Plugd",
  "version": "0.4.1",
  "description": "Compare prices between StockX, GOAT, Stadium Goods, and more",
  "key": "",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "./background.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://connect.facebook.net; object-src 'self'",
  "externally_connectable": {
    "matches": [
      "*://extension.getplugd.com/*"
    ]
  },
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation"
  ]
}
