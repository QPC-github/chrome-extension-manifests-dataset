{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tank Riders",
  "version": "1.0.3",
  "icons": {
    "16": "icon_16_rounded.png",
    "48": "icon_48_rounded.png",
    "128": "icon_128_rounded.png"
  },
  "minimum_chrome_version": "25",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "unlimitedStorage",
    {
      "socket": [
        "tcp-connect"
      ]
    },
    "fullscreen"
  ]
}
