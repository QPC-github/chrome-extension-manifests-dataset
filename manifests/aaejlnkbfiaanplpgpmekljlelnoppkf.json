{
  "browser_action": {
    "default_icon": "logo_icon_small.png"
  },
  "content_scripts": [
    {
      "css": [
        "twitter-rssd.css"
      ],
      "js": [
        "jquery.js",
        "twitter-rssd.js"
      ],
      "matches": [
        "*://twitter.com/*"
      ]
    },
    {
      "css": [],
      "js": [
        "jquery.js",
        "jquery.hotkeys.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "Add Tweets ",
  "icons": {
    "128": "logo_icon_128.png",
    "16": "logo_icon_small.png",
    "48": "logo_icon_large.png"
  },
  "name": "تأملات قرآنية",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "http://twilb.net/",
    "tabs",
    "<all_urls>"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.1.5"
}
