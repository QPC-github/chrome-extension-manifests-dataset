{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Megustatube",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "The extension removes video and banner ads on youtube.",
  "homepage_url": "http://megustatube.org",
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "css": [
        "remove.css"
      ]
    }
  ]
}
