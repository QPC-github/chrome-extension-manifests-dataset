{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clever Ads Keyword Planner",
  "description": "Generate and choose the perfect Google Keywords for your Google Ads campaigns with Clever Ads Google Keyword Tool Extension",
  "version": "1.2.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png",
    "512": "icon_512.png"
  },
  "browser_action": {
    "default_icon": "icon_512.png",
    "default_popup": "index.html",
    "default title": "Clever Ads Keyword Planner"
  },
  "permissions": [
    "tabs",
    "https://keyword-ideas.cleverecommerce.com/api/v1/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
