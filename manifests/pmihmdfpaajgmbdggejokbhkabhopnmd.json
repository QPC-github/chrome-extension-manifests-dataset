{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "0.5",
  "default_locale": "zh_TW",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://www.google.com.tw/"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Txt Formatter"
  },
  "manifest_version": 2
}
