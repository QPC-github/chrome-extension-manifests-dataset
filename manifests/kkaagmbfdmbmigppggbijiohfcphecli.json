{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LoginTC (Legacy)",
  "description": "__MSG_app_description__",
  "version": "1.2.1",
  "icons": {
    "16": "assets/img/icon-16.png",
    "128": "assets/img/icon-128.png",
    "256": "assets/img/icon-256.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "system.network",
    "identity",
    "identity.email",
    "http://*/",
    "https://*/"
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "minimum_chrome_version": "37",
  "default_locale": "en"
}
