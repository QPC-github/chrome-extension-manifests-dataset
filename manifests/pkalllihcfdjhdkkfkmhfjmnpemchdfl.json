{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sitios Frecuentes Zoppas Industries Mexico",
  "version": "2.48",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images2/img16.png",
      "32": "images2/img32.png",
      "48": "images2/img48.png",
      "128": "images2/img128.png"
    }
  },
  "icons": {
    "16": "images2/img16.png",
    "32": "images2/img32.png",
    "48": "images2/img48.png",
    "128": "images2/img128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "topSites"
  ]
}
