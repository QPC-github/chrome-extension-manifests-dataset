{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tab search",
  "description": "navigate your tabs by searching the title in the new tab",
  "version": "0.2",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
