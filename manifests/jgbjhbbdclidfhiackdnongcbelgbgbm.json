{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_plugin_name__",
  "version": "1.0",
  "manifest_version": 2,
  "description": "__MSG_plugin_desc__",
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png",
    "128": "/img/icon128.png"
  },
  "default_locale": "lv",
  "browser_action": {
    "default_icon": "/img/icon-action.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "/js/jquery.js",
      "/js/const.js",
      "/js/listener.js"
    ]
  },
  "permissions": [
    "tabs",
    "notifications",
    "cookies",
    "background",
    "*://*.flirts.lv/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.flirts.lv/*"
      ],
      "js": [
        "/js/jquery.js",
        "/js/pagehandler.js"
      ]
    }
  ]
}
