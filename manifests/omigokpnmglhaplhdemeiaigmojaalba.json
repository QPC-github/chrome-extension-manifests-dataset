{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Text replacer",
  "description": "You can change any text in any website you want!",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "yair.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "yair.js"
      ]
    }
  ]
}
