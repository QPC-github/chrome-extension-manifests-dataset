{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HowlsOfOutrage",
  "version": "4.2",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "show MetaFilter favoriters on hover",
  "content_scripts": [
    {
      "matches": [
        "http://*.metafilter.com/*"
      ],
      "js": [
        "HowlsOfOutrage.js"
      ]
    }
  ],
  "permissions": [
    "http://*.metafilter.com/*",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
