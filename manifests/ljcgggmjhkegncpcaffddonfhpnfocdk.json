{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks",
  "version": "1.3",
  "description": "Opens Bookmarks tab in single click from new tab and context menu. Options available to remove it from context menu",
  "icons": {
    "16": "img/icon16.png",
    "19": "img/icon19.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "app": {
    "launch": {
      "local_path": "main.htm"
    }
  },
  "background": {
    "page": "background.htm"
  },
  "options_page": "options.htm",
  "permissions": [
    "contextMenus",
    "unlimitedStorage"
  ],
  "manifest_version": 2,
  "homepage_url": "http://tejji.com/"
}
