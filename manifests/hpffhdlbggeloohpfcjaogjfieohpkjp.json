{
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "browser_action": {
    "default_icon": "64.png",
    "default_popup": "popuplinks.html"
  },
  "name": "Extension GameHerTV",
  "version": "1.1",
  "description": "Extension indiquant quand la GameHerTV est en live !",
  "icons": {
    "64": "64.png"
  },
  "manifest_version": 2,
  "permissions": [
    "https://api.twitch.tv/",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
