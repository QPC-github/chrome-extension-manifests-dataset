{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Moritz Sturm",
  "background": {
    "persistent": true,
    "scripts": [
      "assets/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "assets/css/style.css"
      ],
      "js": [
        "assets/js/inject.js"
      ],
      "matches": [
        "https://www.instagram.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Manage bookmarked/saved Instagramâ„¢ posts",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Unsaver for Instagram",
  "version": "3.0.7",
  "web_accessible_resources": [
    "*.js"
  ]
}
