{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Insightly FREE Upgrade Bar Disabler",
  "description": "This extension will disable the upgrade bar in Insightly CRM when on FREE account",
  "version": "1.0",
  "icons": {
    "32": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.insightly.com/*"
      ],
      "js": [
        "disable.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
