{
  "name": "Plus Helper",
  "description": "Connect with your WhatsApp accounts",
  "version": "1.3",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging"
  ],
  "content_scripts": [
    {
      "js": [
        "script.js",
        "back.js"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "pluslogo.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "f1.js"
  ]
}
