{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cedar Hill ISD Sign Out",
  "description": "This extension will sign out students form the internet at Cedar Hill ISD.",
  "version": "1.0",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_title": "Cedar Hill ISD Sign Out",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css",
    "tabs",
    "http://lsaccess.me/logout",
    "http://lsaccess.me/login"
  ]
}
