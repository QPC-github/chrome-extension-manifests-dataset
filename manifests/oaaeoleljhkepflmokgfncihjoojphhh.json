{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CRM Rename Extension",
  "version": "0.1",
  "permissions": [
    "downloads"
  ],
  "description": "CRM Rename Extension",
  "content_security_policy": "script-src 'self'; default-src 'self'",
  "icons": {
    "16": "writing.png",
    "128": "writing128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
