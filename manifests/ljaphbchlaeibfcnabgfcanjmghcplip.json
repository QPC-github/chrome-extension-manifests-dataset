{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Weitsicht",
  "description": "We are the future - connectivity in our veins. We are weitsicht",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "There's a 'G' in this URL!",
    "default_popup": "popup.html",
    "scripts": [
      "jquery.js",
      "popup.js"
    ]
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent"
  ]
}
