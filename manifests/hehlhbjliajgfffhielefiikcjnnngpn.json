{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CardinalURL",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Stanfordizes URLs which end in a '/' as if you were on campus!",
  "icons": {
    "16": "red_s_16.gif",
    "48": "red_s_48.gif",
    "128": "red_s_128.gif"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "cardinalURL.js"
    ]
  }
}
