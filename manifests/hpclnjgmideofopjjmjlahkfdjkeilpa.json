{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mHealth Search",
  "description": "Chrome Extension to search health topics powered by MedlinePlus.",
  "icons": {
    "128": "searchicon128.png"
  },
  "browser_action": {
    "default_title": "mHealth Search",
    "default_popup": "popup.html",
    "default_icon": "searchicon16.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://query.yahooapis.com; object-src 'self';",
  "version": "1.4"
}
