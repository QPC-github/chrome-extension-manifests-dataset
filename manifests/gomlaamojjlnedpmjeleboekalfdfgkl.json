{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Make Spurs S**t Again!"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Add this app to your Chrome browser and replace all instances of \"Spurs\" with \"S*t.\"",
  "manifest_version": 2,
  "name": "S**tinator",
  "permissions": [
    "http://*.google.com/",
    "http://*.com/",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.1"
}
