{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "苹果米饭",
  "description": "苹果米饭网商品发布的即时通知小助手",
  "version": "1.2",
  "icons": {
    "128": "icon/logo128.png",
    "48": "icon/logo48.png",
    "16": "icon/logo16.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://www.applerice.com/"
  ],
  "browser_action": {
    "default_icon": "icon/logo19.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  }
}
