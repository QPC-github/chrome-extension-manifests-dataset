{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QrURL",
  "version": "0.4.3",
  "description": "create a qrcode from an url",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon-128.png",
    "popup": "popup.html"
  },
  "homepage_url": "http://qrurl.it",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
