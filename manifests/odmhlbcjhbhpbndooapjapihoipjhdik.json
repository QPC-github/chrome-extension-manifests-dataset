{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TooButtons",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Transform every link in a button",
  "icons": {
    "48": "button48.png",
    "128": "button128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "js": [
        "button.js"
      ],
      "all_frames": true
    }
  ]
}
