{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pierre Nelwan",
  "description": "Check me out. I'm Pierre Nelwan",
  "version": "2.69",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://secure.flickr.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "content.js",
        "snow.js"
      ]
    }
  ]
}
