{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TD Video meeting",
  "version": "1.10",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.teledirectasia.com/*",
      "*://*.teledirectgroup.com/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "management"
  ],
  "web_accessible_resources": [
    "icons/icon96.png"
  ]
}
