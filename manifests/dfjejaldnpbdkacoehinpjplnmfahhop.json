{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Local Media Player",
  "version": "4.3",
  "description": "Local media player lets you play your favorite media (be it video, music or audio) files without any third party software.",
  "icons": {
    "16": "icon.jpg",
    "48": "icon.jpg",
    "128": "icon.jpg"
  },
  "homepage_url": "https://mediaplayer.chromecrxstore.com/",
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_title": "Local Media Player"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "is_default": true,
      "name": "Local Media Player",
      "keyword": "Search Local Media Player",
      "encoding": "UTF-8",
      "favicon_url": "https://searchsmart.chromecrxstore.com/favicon.ico",
      "search_url": "https://searchsmart.chromecrxstore.com/?q={searchTerms}",
      "suggest_url": "https://suggest.chromecrxstore.com?q={searchTerms}"
    }
  }
}
