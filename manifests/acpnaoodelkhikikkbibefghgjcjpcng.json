{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Battalion/Division Staff Helper",
  "description": "Helper extension for TG Battalion/Division Staff.",
  "version": "0.2.4",
  "page_action": {
    "default_icon": {
      "19": "images/16px.png",
      "38": "images/48px.png"
    },
    "default_title": "TG Battalion Helper",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.tacticalgaming.net/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "http://www.tacticalgaming.net/*"
  ],
  "icons": {
    "16": "images/16px.png",
    "48": "images/48px.png"
  }
}
