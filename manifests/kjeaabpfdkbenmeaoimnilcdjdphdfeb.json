{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Maxcoin Lookup",
  "description": "This Google Chrome extension allows you to view bitcoin and altcoin addresses, transactions (txid), and blocks (block hashes).",
  "version": "0.2.3",
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "libs/underscore-min.js",
      "libs/contextManagement.js",
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html"
}
