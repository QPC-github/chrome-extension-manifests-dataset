{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Copyto / Clever bookmarking",
  "manifest_version": 2,
  "version": "1.4.1",
  "background": {
    "scripts": [
      "js/webtoolkit.base64.js",
      "js/background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "Icon-19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  }
}
