{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TeamLogger",
  "short_name": "TeamLogger",
  "version": "1.86",
  "manifest_version": 2,
  "minimum_chrome_version": "71",
  "description": "Team time tracking software with automatic screenshots.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "desktopCapture",
    "notifications",
    "storage",
    "idle",
    "unlimitedStorage"
  ]
}
