{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snooze times Google Inbox",
  "description": "More snooze times for Inbox by Gmail",
  "version": "1.0",
  "minimum_chrome_version": "27.0",
  "content_scripts": [
    {
      "matches": [
        "https://inbox.google.com/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "underscore.js",
        "moment.js",
        "jquery.js",
        "jquery.simulate.js",
        "arrive.js",
        "cl.js",
        "main.js"
      ],
      "run_at": "document_end"
    }
  ]
}
