{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Brian Tracy",
  "description": "Start living a defined life by setting a goal with new tab that Get you inspired and motivated daily with Brian Tracy Quote",
  "version": "1.0.0.2",
  "icons": {
    "16": "images/icon.png",
    "64": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "short_name": "Brian Tracy",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
