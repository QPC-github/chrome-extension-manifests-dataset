{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Membeam Answer Hack",
  "version": "0.2",
  "manifest_version": 2,
  "description": "A simple extension that passes your vocab tests.\nCoded By @RunConnor\nhttps://connorlapping.com",
  "homepage_url": "https://connorlapping.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Membeam Answer Hack"
  },
  "permissions": [
    "https://membean.com/*",
    "http://membean.com/*",
    "tabs"
  ]
}
