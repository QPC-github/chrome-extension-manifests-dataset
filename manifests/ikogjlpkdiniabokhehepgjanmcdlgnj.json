{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-1.3.2.min.js",
        "scrollsync.js",
        "dragscrollable.js",
        "myscript.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "Extended Image Viewer",
  "description": "It is an extender for built-in image viewer. Opera like image viewer.",
  "version": "0.0.7"
}
