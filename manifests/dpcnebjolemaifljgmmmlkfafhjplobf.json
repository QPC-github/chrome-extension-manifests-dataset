{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dhruv Dashboard",
  "version": "2",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "background": {
    "page": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "management",
    "bookmarks",
    "background",
    "geolocation",
    "<all_urls>"
  ]
}
