{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "default_locale": "ru",
  "description": "Отслеживание списка ваших посылок с Aliexpress, eBay, Amazon по TrackID (уникальному номеру).",
  "icons": {
    "128": "icon_128.png"
  },
  "name": "TrackIDbot - Отслеживание посылок",
  "short_name": "TrackIDbot - Отслеживание посылок",
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://tagiltsev.ru/"
  ],
  "version": "1.0.4",
  "manifest_version": 2
}
