{
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "luctho.js"
      ],
      "matches": [
        "https://*.tmall.com/*",
        "https://*.taobao.com/*",
        "https://*.1688.com/*",
        "https://*.hm.com/*",
        "https://*.hm.cn/*",
        "https://*.zara.cn/*",
        "https://*.zara.com/*",
        "https://*.forever21.cn/*",
        "http://*.forever21.cn/*",
        "https://*.mango.com/*",
        "https://shop.mango.com/*",
        "https://*.pullandbear.cn/*",
        "https://*.bershka.cn/*"
      ]
    }
  ],
  "description": "Đặt hàng taobao, tmall, hm, mango... trên Chrome & Cờ rôm+.",
  "manifest_version": 2,
  "name": "luctho.com",
  "permissions": [
    "https://*.tmall.com/*",
    "https://*.taobao.com/*",
    "https://*.1688.com/*",
    "https://*.hm.com/*",
    "https://*.hm.cn/*",
    "https://*.zara.cn/*",
    "https://*.zara.com/*",
    "https://*.forever21.cn/*",
    "http://*.forever21.cn/*",
    "https://*.mango.com/*",
    "https://shop.mango.com/*",
    "https://*.pullandbear.cn/*",
    "https://*.bershka.cn/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
