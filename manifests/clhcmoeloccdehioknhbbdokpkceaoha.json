{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Latinify",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Converts Cyrillic to Latin on any website!",
  "browser_action": {
    "default_icon": "latinify128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
