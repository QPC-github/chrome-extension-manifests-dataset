{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Any Clean",
  "description": "Clean browsing history from any set of dates not just Chrome's defaults",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "vac-icon.gif"
  },
  "permissions": [
    "history",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "vac-icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
