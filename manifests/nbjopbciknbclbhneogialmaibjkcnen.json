{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Kittenify",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Because the world needs more kittens.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Enable the Kitty",
    "default_icon": "img/icon.jpeg"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
