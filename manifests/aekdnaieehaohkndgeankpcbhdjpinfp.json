{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zip Opener for Google Chromeâ„¢",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "View & extract any zip file now from your browser.",
  "container": [
    "GOOGLE_DRIVE"
  ],
  "icons": {
    "128": "logo-zip-128.png",
    "48": "logo-zip-48.png",
    "16": "logo-zip-16.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "logo-zip-16.png",
      "32": "logo-zip-32.png"
    }
  },
  "background": {
    "scripts": [
      "launch.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/ https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "https://api.zipfiletab.com/*"
  ]
}
