{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fixfeel",
  "version": "0.474",
  "description": "An Fixfeel extension for Chrome",
  "manifest_version": 2,
  "icons": {
    "16": "app_icon/16.png",
    "48": "app_icon/48.png",
    "128": "app_icon/128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "jquery.js",
      "script.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://fixfeel.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon/icon19.png",
      "38": "icon/icon38.png"
    },
    "default_title": "Fixfeel",
    "default_popup": "popup.htm"
  },
  "options_page": "options.htm"
}
