{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blood Brazy",
  "version": "0.23",
  "description": "turn bhrome brazy - by fred and louis!",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "bandana-16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
