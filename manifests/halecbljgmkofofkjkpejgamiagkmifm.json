{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bannon, Grand Wizard of the White House",
  "description": "Replaces 'Bannon' with 'Bannon, the anti-semitic white nationalist Grand Wizard of the White House'",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
