{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "oscraper",
  "description": "Scrape Your Organic Search Engine Results",
  "version": "0.5.0",
  "manifest_version": 2,
  "homepage_url": "http://www.oscraper.com/",
  "background": {
    "scripts": [
      "lib/jquery-2.0.3.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "*://*/*",
    "storage",
    "tabs"
  ],
  "icons": {
    "128": "logo.png",
    "16": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Scrape Your Organic Search Engine Results",
    "default_popup": "popup.html"
  }
}
