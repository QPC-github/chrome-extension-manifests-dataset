{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Daum 책 검색",
  "description": "Daum 책을 쉽게 검색하게 하기 위한 플러그인",
  "version": "1.2",
  "permissions": [
    "tabs",
    "http://book.daum.net/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
