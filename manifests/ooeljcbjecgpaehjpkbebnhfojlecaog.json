{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Neil's ruruonline poster",
  "description": "Made by Neil from ruruonline.com (deleted site)",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "version": "1.9",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "toolbar_dropdown.html"
  }
}
