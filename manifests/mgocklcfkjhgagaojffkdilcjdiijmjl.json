{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tradesk",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Track cryptocurrency prices in your new tab",
  "homepage_url": "https://tradesk.co",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {},
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "topSites",
    "chrome://favicon/"
  ]
}
