{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wiki/Imgur Savior",
  "version": "0.1",
  "description": "Automatically changes imgur and wikipedia page url to make those site visible",
  "manifest_version": 2,
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
