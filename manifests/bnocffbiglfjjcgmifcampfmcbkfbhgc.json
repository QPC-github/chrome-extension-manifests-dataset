{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Switchmark",
  "version": "2.1",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "background": {
    "scripts": [
      "common.js",
      "switchmark.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
