{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Chrome Console Cleaner",
  "short_name": "QCCC",
  "description": "__MSG_description__",
  "version": "0.2",
  "default_locale": "en",
  "author": "felix seeberg",
  "homepage_url": "http://lypo.de",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "commands": {
    "clearConsole": {
      "description": "__MSG_commandClearConsole__",
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+X"
      }
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "ccc.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon128.png"
  }
}
