{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FiscoClic exporta CFDI's a Excel",
  "description": "Obtiene un resumen detallado de los CFDI de la p√°gina del SAT para exportarlos a Excel",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Exporta tus CFDI del SAT a Excel"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistence": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "<all_urls>"
  ]
}
