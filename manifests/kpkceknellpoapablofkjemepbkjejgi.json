{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ChromeCalmer",
  "version": "0.8",
  "manifest_version": 2,
  "description": "Desaturate web page colors for people with photosensitivity.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
