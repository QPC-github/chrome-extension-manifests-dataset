{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Paste It",
  "description": "Paste text from a toolbar button, context menu or keyboard shortcut",
  "version": "4.0.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
    "scripts": [
      "bg_script_pasteit.js"
    ],
    "persistent": true
  },
  "minimum_chrome_version": "8.0.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": "paste_icon_19.png"
  },
  "commands": {
    "keyboard-paste": {
      "suggested_key": {
        "default": "Alt+V"
      },
      "description": "Paste Primary Text"
    },
    "alt-keyboard-paste": {
      "suggested_key": {
        "default": "Alt+Shift+V"
      },
      "description": "Paste Alternate Text 1"
    },
    "alt-keyboard-paste2": {
      "suggested_key": {
        "default": "Alt+B"
      },
      "description": "Paste Alternate Text 2"
    },
    "alt-keyboard-paste3": {
      "suggested_key": {
        "default": "Alt+Shift+B"
      },
      "description": "Paste Alternate Text 3"
    }
  },
  "icons": {
    "16": "paste_icon_16.png",
    "48": "paste_icon_48.png",
    "128": "paste_icon_128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ]
}
