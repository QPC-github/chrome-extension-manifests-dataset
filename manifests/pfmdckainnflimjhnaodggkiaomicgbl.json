{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Mabashi",
  "background": {
    "scripts": [
      "background.js",
      "popup/popup.js",
      "popup/js/jquery.js"
    ]
  },
  "description": "Soyez au courant quand votre streameur préféré commence à streamer!",
  "icons": {
    "128": "images/off/iconoff128.png",
    "48": "images/off/iconoff48.png",
    "32": "images/off/iconoff32.png",
    "16": "images/off/iconoff16.png"
  },
  "browser_action": {
    "default_icon": "images/off/iconoff16.png",
    "default_popup": "popup/popup.html",
    "default_title": "CartWick"
  },
  "options_page": "options/options.html",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "manifest_version": 2,
  "name": "CartWick Live Extension",
  "permissions": [
    "https://api.twitch.tv/",
    "notifications",
    "storage",
    "tabs"
  ],
  "version": "2.3"
}
