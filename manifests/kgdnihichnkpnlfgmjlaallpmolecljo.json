{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CrickScore",
  "description": "An extension to show latest Cricket Score",
  "version": "1.0",
  "icons": {
    "16": "crick.png",
    "48": "crick.png",
    "128": "crick.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "<all_urls>",
    "contextMenus",
    "clipboardRead",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Print this page",
    "default_popup": "popup.html",
    "default_icon": "crick.png"
  }
}
