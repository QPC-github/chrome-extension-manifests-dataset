{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "homepage_url": "http://christian-neubauer.de/#gimprove",
  "name": "Gambio Improve",
  "description": "Make attribute tables sortable.",
  "version": "0.1.2",
  "permissions": [
    "activeTab",
    "http://www.haarteile-zweithaar.de/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.haarteile-zweithaar.de/*"
      ],
      "css": [
        "zhaar.css"
      ],
      "js": [
        "jqueryz.js",
        "zhaar.js"
      ]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "19": "images/19.png",
    "22": "images/22.png",
    "38": "images/38.png",
    "96": "images/96.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "19": "images/19.png",
      "22": "images/22.png",
      "38": "images/38.png"
    }
  }
}
