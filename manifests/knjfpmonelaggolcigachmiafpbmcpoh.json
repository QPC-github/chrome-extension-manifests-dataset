{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/icon@16x16.png",
    "default_popup": "popup.html",
    "default_title": "FullDomain"
  },
  "description": "An domain tool can let you see all the domain you've visited.",
  "icons": {
    "128": "img/icon@128x128.png",
    "16": "img/icon@16x16.png",
    "48": "img/icon@48x48.png"
  },
  "manifest_version": 2,
  "name": "FullDomain",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0"
}
