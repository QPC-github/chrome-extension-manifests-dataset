{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Squeaky Clean Bum'ole",
  "description": "Just a couple of hairy interns trying to bribe their boss with a narcissistic chrome extension - Enjoy! ",
  "version": "1.0",
  "icons": {
    "128": "images/icon-128.png"
