{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "AppleRanks",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "A Chrome extension for retrieving & showing Apple's *-store ranks instantly.",
  "browser_action": {
    "default_icon": "imgs/icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "imgs/icon16.png",
    "48": "imgs/icon48.png",
    "128": "imgs/icon128.png"
  },
  "permissions": [
    "http://itunes.apple.com/",
    "tabs"
  ]
}
