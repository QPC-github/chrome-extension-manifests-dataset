{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VRPlayground Supporter",
  "version": "0.1.8",
  "minimum_chrome_version": "51",
  "manifest_version": 2,
  "author": "Digitmedia e.K.",
  "description": "Unterst√ºtze VRPlayground",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "images/icon-32.png"
    },
    "default_popup": "popup.html",
    "default_title": "VRPlayground"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "*://*.amazon.de/*",
    "*://*.ebay.de/*"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  }
}
