{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ad remover for Outlook Beta",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Remove ads from Outlook Beta.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
