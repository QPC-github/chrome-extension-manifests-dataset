{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BlackLister Dropshipping Extension",
  "description": "Ebay Lister Tool for Dropshipping and arbitrage Chrome Extension",
  "version": "0.0.4",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_title": "BlackLister Dropshipping Extension"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*"
  ]
}
