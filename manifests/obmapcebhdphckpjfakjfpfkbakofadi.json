{
  "author": "AquSag Technologies ",
  "background": {
    "persistent": false,
    "scripts": [
      "background/background.common.js",
      "background/background.js",
      "background/background.desktopCapture.js",
      "background/background.tabCapture.js",
      "background/background.badgeText.js",
      "background/background.getUserMedia.js",
      "background/background.messaging.js",
      "background/background.players.js",
      "RecordRTC/MediaStreamRecorder.js",
      "RecordRTC/MultiStreamRecorder.js",
      "RecordRTC/DiskStorage.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/main-icon.png",
    "default_popup": "dropdown.html",
    "default_title": "Record Your Screen, Tab or Camera"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "description": "Record screen activity, camera, mic, tab, speakers in 4K HD video format (using RealTime Chat)",
  "homepage_url": "https://partyprep.in",
  "icons": {
    "128": "images/main-icon.png",
    "16": "images/main-icon.png",
    "22": "images/main-icon.png",
    "32": "images/main-icon.png",
    "48": "images/main-icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "name": "Realtime Chat",
  "oauth2": {
    "client_id": "41556190767-44cbqasurg3dhfr9hmj1v90q5v8r9v46.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/youtube.upload",
      "https://www.googleapis.com/auth/youtube"
    ]
  },
  "options_ui": {
    "chrome_style": false,
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "desktopCapture",
    "storage",
    "tabs",
    "tabCapture",
    "activeTab",
    "identity",
    "cookies",
    "sessions",
    "https://www.googleapis.com/*",
    "https://localhost/*",
    "http://localhost/*",
    "http://localhost/chat-server/*",
    "https://localhost/chat-server*",
    "https://partyprep.in/*",
    "https://partyprep.in/*",
    "https://partyprep.in/application/agent/*",
    "https://partyprep.in/application/agent/*"
  ],
  "short_name": "Chat",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "11.6",
  "web_accessible_resources": [
    "images/main-icon.png"
  ]
}
