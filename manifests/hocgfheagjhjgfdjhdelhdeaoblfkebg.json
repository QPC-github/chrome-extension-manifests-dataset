{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Graph adjuster (for sageOne Accounting)",
  "version": "0.0.0.1",
  "description": "Adjusts sageOne Accounting dashboard graph widgets so you can view more information",
  "permissions": [
    "tabs",
    "https://accounting.sageone.co.za/Dashboard/Home.aspx"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Adjust sageOne Accounting dashboard widgets"
  },
  "icons": {
    "128": "appendedapp.png"
  },
  "manifest_version": 2
}
