{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ShowBox - Movie and TV Shows",
  "version": "0.1",
  "manifest_version": 2,
  "description": "ShowBox - Watch free HD movies & TV for free. You can even download the movies and watch offline.",
  "homepage_url": "https://uptofast.com/android/showbox",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "https://uptofast.com/android/showbox",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
