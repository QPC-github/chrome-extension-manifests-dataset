{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple New Tab",
  "description": "Simple New Tab",
  "version": "1.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "history",
    "bookmarks",
    "contextMenus",
    "management",
    "chrome://favicon/*"
  ],
  "web_accessible_resources": []
}
