{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "hashdd",
  "version": "0.0.1",
  "description": "Computes the cryptographic hash of your downloads and checks them against the hashdd.com database.",
  "icons": {
    "16": "favicon.png",
    "48": "apple-touch-icon-48.png",
    "128": "apple-touch-icon-128.png"
  },
  "author": "hashdd",
  "browser_action": {
    "default_title": "Check downloads",
    "default_popup": "popup.html"
  },
  "permissions": [
    "downloads",
    "downloads.open",
    "https://api.hashdd.com/"
  ]
}
