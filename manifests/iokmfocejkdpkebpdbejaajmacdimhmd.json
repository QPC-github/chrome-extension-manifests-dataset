{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hatena Keyword Remover",
  "version": "1.4",
  "description": "Removes Hatena Keyword Links and search keyword highlight",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*"
  ]
}
