{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "pages": [
        "html/embed.html"
      ],
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "description": "A native 7 Cups standalone app for your Chrome Browser.",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png"
  },
  "manifest_version": 2,
  "name": "7 Cups",
  "permissions": [
    "webview",
    "http://*/",
    "https://*/"
  ],
  "version": "0.2"
}
