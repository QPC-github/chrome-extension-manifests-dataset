{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WordPress Clicky",
  "description": "This extension will attach '/wp-admin' to your URL address.",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "img/wordpress.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
