{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "The Deja chrome extension for recording streams",
  "version": "2.1.0",
  "name": "Deja Extension",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "deja.png"
  },
  "manifest_version": 2,
  "permissions": [
    "gcm",
    "storage",
    "cookies",
    "tabs",
    "activeTab",
    "webRequest",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
