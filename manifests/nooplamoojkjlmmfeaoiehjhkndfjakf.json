{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitter CSS Fix",
  "description": "pushes down page content so the \"new tweet\" popup doesn't cover the compose field.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "css": [
        "twitterfix.css"
      ]
    }
  ],
  "permissions": [
    "*://*.twitter.com/*"
  ]
}
