{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZIP Extractor For Google Drive",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Decompress ZIP files from local or Google Drive and can save to Google Drive",
  "container": [
    "DOMAIN_INSTALLABLE",
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "1024501591538",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/zip",
          "application/x-zip",
          "application/x-zip-compressed",
          "application/vnd.google.drive.ext-type.zip"
        ],
        "href": "http://1bestlinks.net/zip-extractor/index.html",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "24": "icon-24.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "app": {
    "launch": {
      "web_url": "http://1bestlinks.net/zip-extractor/index.html"
    }
  }
}
