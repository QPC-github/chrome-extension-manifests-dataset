{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ControCheck SQ",
  "description": "This extension displays controversial topics similar to a search query.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": "iconC.png",
    "default_popup": "popup.html",
    "default_title": "Click here for controversies!"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
