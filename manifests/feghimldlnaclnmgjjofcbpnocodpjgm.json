{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Restless Rocks",
  "version": "1.5",
  "description": "A blog that will describe the day to day activities of a professional stone sculptor, living and working in a remote wooded area",
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Restless Rocks"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.js"
    ]
  }
}
