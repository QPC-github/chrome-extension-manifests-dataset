{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EZMEME",
  "description": "Post that clever meme as soon as it comes to mind. Leaving Reddit to do that is for suckers.",
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.jpg"
  ],
  "permissions": [
    "http://upboat.me/*"
  ]
}
