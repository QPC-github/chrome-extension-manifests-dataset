{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BGO Notifier",
  "description": "Monitors Boardgaming Online for new games",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon_inactive16.png"
  },
  "background": {
    "scripts": [
      "getPage.js"
    ]
  },
  "permissions": [
    "http://boardgaming-online.com/",
    "notifications"
  ]
}
