{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Newsglot",
  "description": "Import any URL into Newsglot to get instant translations, collect words and phrases to practice later, and more.",
  "version": "1",
  "author": "Newsglot",
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon-32x32.png",
    "128": "favicon-128x128.png"
  },
  "browser_action": {
    "default_title": "Import into Newsglot"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
