{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AlternateImgurAtWork",
  "version": "1.1.4",
  "manifest_version": 2,
  "description": "Converts Imgur URLs To Kageurufu URLs",
  "content_scripts": [
    {
      "matches": [
        "*://www.reddit.com/*"
      ],
      "js": [
        "jquery.js",
        "contentScript.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "contentScript.js",
    "jquery.js",
    "jquery.min.map"
  ],
  "permissions": [
    "*://www.reddit.com/*"
  ]
}
