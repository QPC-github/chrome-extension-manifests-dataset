{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Comic Sans Remover",
  "description": "Comic Sans Remover will replace Comic Sans from any web page with a more readable font.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_19.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ]
    }
  ]
}
