{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Randomizer By @Ninouzeer",
  "description": "Un randomizer qui va être amélioré",
  "version": "0.5",
  "permissions": [],
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "index.html",
    "default_title": "Randomizer By @Ninouzeer"
  },
  "icons": {
    "128": "img/logo_128.png",
    "16": "img/logo_16.png",
    "24": "img/logo_24.png",
    "32": "img/logo_32.png",
    "48": "img/logo_48.png"
  },
  "background": {
    "scripts": [
      "data.js",
      "random.js",
      "popup.js"
    ]
  }
}
