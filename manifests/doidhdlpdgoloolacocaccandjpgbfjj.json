{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.1.4",
  "version_name": "0.9.4",
  "name": "Nihi-lists",
  "description": "A nihilistic new tab replacement.",
  "chrome_url_overrides": {
    "newtab": "nihilists.html"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "storage",
    "http://www.nihi-lists.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
