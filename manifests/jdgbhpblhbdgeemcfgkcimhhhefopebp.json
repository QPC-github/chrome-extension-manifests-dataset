{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "12星座占い",
  "version": "1.0.1",
  "description": "人気サイトの［12星座占い〜今日の運勢〜］を、手軽なポップアップ形式で！あなたの毎日をもっとハッピーに！",
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "32": "32.png",
    "16": "16.png"
  },
  "homepage_url": "http://www.dailyfortune.jp/horoscope/",
  "browser_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_popup": "popup.html",
    "default_title": "12星座占い"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "http://*.dailyfortune.jp/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
