{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reviewer Recommender",
  "description": "Reviewer Recommender",
  "version": "3.0.10",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Reviewer Recommender",
    "default_icon": "resource/icon.png"
  },
  "web_accessible_resources": [
    "resource/*"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "resource/icon.png",
    "48": "resource/icon.png",
    "128": "resource/icon.png"
  },
  "manifest_version": 2
}
