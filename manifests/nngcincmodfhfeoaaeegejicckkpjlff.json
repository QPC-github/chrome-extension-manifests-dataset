{
  "author": [
    {
      "name": "Dep. de Tecnologia da Informação",
      "email": "suporte@grupotgm.com.br"
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Compartilhamento de tela na videoconferência do Grupo TGM.",
  "externally_connectable": {
    "matches": [
      "https://video.grupotgm.com.br/*"
    ]
  },
  "icons": {
    "128": "jtisi-logo-128x128.png",
    "16": "jtisi-logo-16x16.png",
    "48": "jtisi-logo-48x48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Desktop Share TGM",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
