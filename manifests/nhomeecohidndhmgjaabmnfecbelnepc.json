{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "olym-ssl-sca",
  "version": "1.0.0",
  "author": "olym",
  "description": "Secure Cloud Access",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "深圳奥联信息技术安全有限公司云安全接入"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "tabs",
    "https://192.168.136.82/*",
    "http://192.168.136.82/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  }
}
