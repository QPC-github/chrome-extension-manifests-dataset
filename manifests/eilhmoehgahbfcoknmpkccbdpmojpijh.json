{
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "券多多"
  },
  "content_scripts": [
    {
      "js": [
        "js/lib/jquery.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "html/background.html"
  },
  "description": "券多多——优惠券领取助手，是中国领先的购物助手工具，提供最新优惠券一键领取功能、在线提醒功能，付款之前券多多！",
  "icons": {
    "128": "img/icon-128.png"
  },
  "name": "券多多",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "unlimitedStorage",
    "storage",
    "notifications"
  ],
  "version": "1.0.2",
  "update_url": "http://clients2.google.com/service/update2/crx"
}
