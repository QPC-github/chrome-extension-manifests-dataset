{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ask Wikipedia",
  "description": "Quench your thirst for knowledge with a Chrome extension that enables you to instantaneously Wiki-Search anything you don't know",
  "version": "1.0.2",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "/js/eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
