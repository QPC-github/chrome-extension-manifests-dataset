{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "Multi-threaded Download Manager",
  "short_name": "multithreaded-download-manager",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "web_accessible_resources": [
    "data/UI/icons/loader.gif"
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "nativeMessaging"
  ],
  "homepage_url": "http://mybrowseraddon.com/multithreaded-download-manager.html",
  "description": "A lite and multi-threaded download utility with aria2 engine.",
  "browser_action": {
    "default_title": "Multi-threaded Download Manager",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
