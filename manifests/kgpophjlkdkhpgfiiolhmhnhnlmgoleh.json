{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Como ToolBox",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "A handy toolbox for como developers",
  "homepage_url": "https://jira.como-services.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Como toolbox",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "activeTab",
    "<all_urls>"
  ]
}
