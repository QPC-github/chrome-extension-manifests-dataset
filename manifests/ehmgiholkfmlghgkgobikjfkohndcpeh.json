{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aria2 chrome extension",
  "description": "Aria2 right click extension",
  "version": "1.02",
  "browser_action": {
    "default_icon": "web/grey.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "activeTab"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}
