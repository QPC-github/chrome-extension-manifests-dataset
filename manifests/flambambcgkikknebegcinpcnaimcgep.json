{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Time-Tracker",
  "description": "Keep track of the time you spend/waste on websites",
  "version": "0.11",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-2.png",
    "default_popup": "popup.html",
    "default_title": "Click to track your time!"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
