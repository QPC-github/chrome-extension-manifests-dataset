        "vendor/templates-new.js",
        "js/content-new.js",
        "vendor/progressbar.min.js"
      ],
      "css": [
        "vendor/select2-3.5.3/select2.css",
        "vendor/alertify/alertify.css",
        "vendor/alertify/default.min.css",
        "css/circle.css",
        "css/css/app.css",
