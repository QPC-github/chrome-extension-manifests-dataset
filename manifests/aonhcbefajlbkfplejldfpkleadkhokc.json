    "images/cross.svg",
    "images/bell.svg",
    "images/bell-h.svg",
    "images/loading.svg",
    "images/loading-h.svg",
    "images/select2-spinner.gif",
    "images/select2.png",
    "images/face1.svg",
    "images/face2.svg",
