{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social Signals Information",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Social Signals Information",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/jquery-ui-1.9.1.custom.css",
        "css/aams_menu.css"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js",
      "js/jquery-1.8.2.min.js"
    ]
  },
  "icons": {
    "16": "fb_16.png",
    "48": "fb_48.png",
    "128": "fb_128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "fb_48.png"
  ]
}
