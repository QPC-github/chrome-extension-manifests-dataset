{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Justtagit.io",
  "short_name": "JustTagIt",
  "version": "0.6.3",
  "manifest_version": 2,
  "description": "JustTagIt Chrome Extension brings you the capability on all websites to see the meaning behind registered hashtags.",
  "homepage_url": "https://justtagit.io",
  "icons": {
    "16": "app/icons/icon_2019_16.png",
    "19": "app/icons/icon_2019_19.png",
    "48": "app/icons/icon_2019_48.png",
    "128": "app/icons/icon_2019_128.png"
  },
  "background": {
    "page": "app/app.html#background",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "app/icons/icon_2019_16.png",
      "19": "app/icons/icon_2019_19.png",
      "48": "app/icons/icon_2019_48.png",
      "128": "app/icons/icon_2019_128.png"
    },
    "default_popup": "app/app.html#popup",
    "default_title": "JustTagIt"
  },
  "options_ui": {
    "page": "app/app.html#options",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
