{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "World Wide Web Wader",
  "description": "W4 : a easy way to wade into a web site",
  "version": "1.0",
  "homepage_url": "http://www.fonteny.org/wader/",
  "icons": {
    "16": "w4-16.png",
    "48": "w4-48.png",
    "128": "w4-128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Press to display the next page"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "navigation.js"
    ],
    "persistent": false
  }
}
