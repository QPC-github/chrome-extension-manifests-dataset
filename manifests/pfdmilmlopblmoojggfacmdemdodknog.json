{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GCUF News Caster",
  "short_name": "GCUF News",
  "description": "Keep yourself up to date with the latest information from http://www.gcuf.edu.pk",
  "version": "0.1",
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.gcuf.edu.pk/*"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "homepage_url": "http://insert_web_site_here.com/",
  "browser_action": {
    "default_title": "GCUF News Caster",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": false
  }
}
