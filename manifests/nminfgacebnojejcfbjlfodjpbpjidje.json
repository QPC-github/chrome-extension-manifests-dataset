{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Netflix Arrow Sqroll",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "q.png",
    "default_title": "Arrow Sqroll",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
