{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Initial Black",
  "version": "1.0",
  "description": "This extension makes the initial background of a webpage black.",
  "icons": {
    "128": "icon128.png"
  },
  "author": "S.P. Hoang",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ],
      "all_frames": false,
      "run_at": "document_start"
    }
  ]
}
