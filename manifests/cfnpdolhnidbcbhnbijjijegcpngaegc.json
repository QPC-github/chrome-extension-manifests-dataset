{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hide the Hider",
  "description": "This extension allows reading sanluisobispo.com",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.sanluisobispo.com/*"
      ],
      "js": [
        "unhide.js"
      ]
    }
  ]
}
