{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Self Meet - Compartir Escritorio",
  "description": "Una extensi√≥n para la plataforma de e-learning SELF que permite compartir tu escritorio desde Chrome durante una clase en directo.",
  "version": "0.1.8",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "self-logo-16x16.png",
    "48": "self-logo-48x48.png",
    "128": "self-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.plataformaself.com/*"
    ]
  }
}
