{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Exchange Rate Monitor",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Watch the exchange rate on the go.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://www.xiaomingtu.com/"
  ]
}
