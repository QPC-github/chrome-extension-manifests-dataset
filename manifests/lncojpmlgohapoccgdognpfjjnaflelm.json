{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CurseForge Download Replacer",
  "version": "1.0.2",
  "description": "Replaces download links from CurseForge.",
  "content_scripts": [
    {
      "matches": [
        "https://www.curseforge.com/minecraft/modpacks/*"
      ],
      "js": [
        "replacer.js"
      ]
    }
  ],
  "icons": {
    "512": "img/logo.png"
  }
}
