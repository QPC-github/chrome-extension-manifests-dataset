{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Level365 Click-to-Call",
  "version": "1.2.0",
  "description": "A Google Chromeâ„¢ extension that provides Level365 UC integration.",
  "icons": {
    "128": "assets/128.png",
    "16": "assets/16.png",
    "32": "assets/32.png",
    "48": "assets/48.png"
  },
  "browser_action": {
    "default_icon": "assets/32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "c2c.css"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "storage"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "pencil-15.png"
  ]
}
