{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Node Js Debugger",
  "short_name": "Debugger",
  "description": "For Node JS App",
  "version": "1.0.0",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "images/logo.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/logo.png",
      "38": "images/logo.png"
    },
    "default_title": "Node Js Debugger",
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "commands",
    "contentSettings",
    "contextMenus",
    "cookies",
    "debugger",
    "declarativeContent",
    "history",
    "management",
    "notifications",
    "pageCapture",
    "proxy",
    "tabCapture",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
