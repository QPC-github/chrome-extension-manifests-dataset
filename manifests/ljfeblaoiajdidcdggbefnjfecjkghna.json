{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VipeCloud Sharing",
  "description": "VipeCloud is the easiest way to post and schedule content to social.",
  "version": "2.0.0",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html",
    "default_title": "Share this page via VipeCloud"
  },
  "icons": {
    "16": "favicon.png",
    "48": "VipeCloudSquareLogo.png",
    "128": "VipeCloudSquareLogo.png"
  },
  "permissions": [
    "activeTab",
    "https://v.vipecloud.com/"
  ],
  "author": "Vipe Inc",
  "homepage_url": "http://www.vipecloud.com",
  "short_name": "VipeCloud"
}
