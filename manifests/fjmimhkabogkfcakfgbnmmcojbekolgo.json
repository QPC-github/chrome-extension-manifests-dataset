{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Monster Klase",
  "description": "Monster Klase tries to make some pages usable despite all the sadistic efforts of some page developers.",
  "version": "1.5",
  "content_scripts": [
    {
      "matches": [
        "*://*.e-klase.lv/lv/"
      ],
      "css": [
        "galvena.css"
      ],
      "js": [
        "galvena.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://chrome.google.com/webstore/detail/monster-klase/fjmimhkabogkfcakfgbnmmcojbekolgo"
}
