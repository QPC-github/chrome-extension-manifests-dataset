{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Todoist",
  "short_name": "better-todoist",
  "description": "Quickly create tasks in Todoist from the current web page.",
  "version": "1.1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "128": "logo-128.png"
  },
  "permissions": [
    "*://api.todoist.com/rest/v1/*",
    "tabs",
    "storage",
    "contextMenus",
    "notifications"
  ]
}
