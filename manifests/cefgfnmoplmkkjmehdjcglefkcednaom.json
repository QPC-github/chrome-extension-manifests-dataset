{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "短网址",
  "description": "能够自动将普通网址转换为短网址，方便进行分享。并可选择开启ref.so短网址赚钱功能，通过分享短网址为您带来收益。",
  "icons": {
    "128": "images/icon1.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/icon3.png",
    "default_title": "短网址",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "notifications",
    "cookies",
    "contextMenus",
    "http://*/*",
    "clipboardRead",
    "clipboardWrite"
  ],
  "web_accessible_resources": [
    "popup.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "version": "1.6"
}
