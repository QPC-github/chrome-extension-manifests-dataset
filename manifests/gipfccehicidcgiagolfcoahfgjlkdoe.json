{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabToTxt",
  "description": "Copie o link das abas abertas para texto simples, para compartilhar links de ",
  "version": "1.6",
  "browser_action": {
    "default_title": "TabToTxt",
    "default_popup": "popup.html",
    "default_icon": "icons/32/txt.png"
  },
  "icons": {
    "16": "icons/16/txt.png",
    "32": "icons/32/txt.png",
    "64": "icons/64/txt.png",
    "128": "icons/128/txt.png"
  },
  "web_accessible_resources": [
    "icons/16/txt.png"
  ],
  "permissions": [
    "tabs"
  ]
}
