{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "ddd"
  },
  "name": "DUDEN",
  "description": "This extension finds word in german Duden dictionary.",
  "version": "1.1",
  "icons": {
    "32": "duden.png"
  },
  "permissions": [
    "http://www.duden.de/",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}
