{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UHC Comment Hider",
  "version": "1.1",
  "author": "notanimposter",
  "description": "Hides UHC comments and provides a button to toggle them",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
