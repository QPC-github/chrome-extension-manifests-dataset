{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ConvertColor",
  "description": "Convert CSS Colors from and to HEX, RGB, HSL and NAME",
  "version": "0.9.2",
  "browser_action": {
    "default_title": "ConvertColor",
    "default_icon": "cc-icon19.png",
    "default_popup": "convertcolor.html"
  },
  "icons": {
    "16": "cc-icon16.png",
    "48": "cc-icon48.png",
    "128": "cc-icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
