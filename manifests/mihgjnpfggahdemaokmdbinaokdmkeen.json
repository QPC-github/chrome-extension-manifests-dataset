{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Feedly Keys",
  "description": "Feedly keyboard shortcuts for Google Reader users",
  "icons": {
    "128": "images/icon_128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "21",
  "content_scripts": [
    {
      "js": [
        "js/key.js"
      ],
      "matches": [
        "http://www.feedly.com/*",
        "http://cloud.feedly.com/*"
      ]
    }
  ],
  "version": "0.0.1",
  "permissions": [
    "http://www.feedly.com/*",
    "http://cloud.feedly.com/*",
    "storage"
  ]
}
