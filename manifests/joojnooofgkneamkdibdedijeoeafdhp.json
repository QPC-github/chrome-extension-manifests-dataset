{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Admsys - Emitir Notas",
  "description": "Essa extens√£o permite a leitura do certificado digital direto do seu computador",
  "version": "1.6",
  "icons": {
    "16": "logo-contentsys.png",
    "48": "logo-contentsys.png",
    "128": "logo-contentsys.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>",
    "nativeMessaging"
  ]
}
