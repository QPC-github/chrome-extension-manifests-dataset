{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "background-search",
  "version": "0.3",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "img/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/src/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus"
  ]
}
