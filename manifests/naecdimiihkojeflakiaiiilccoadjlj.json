{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BodBot - Personal Trainer & Workout Reminder",
  "short_name": "BodBot",
  "version": "1.5",
  "description": "Personalized workouts plus reminders to keep you on track",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "test.html"
  },
  "chrome_url_overrides": {
    "newtab": "new_tab_override.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "homepage_url": "http://www.bodbot.com",
  "permissions": [
    "activeTab",
    "tabs",
    "http://www.bodbot.com/",
    "http://*.bodbot.com/",
    "alarms",
    "notifications",
    "storage"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
