{
  "browser_action": {
    "default_icon": "img/litecoin.png",
    "default_popup": "html/popup.html",
    "default_title": "CrypDough LTC"
  },
  "web_accessible_resources": [
    "img/error.png",
    "img/info.png",
    "img/warning.png",
    "img/litecoin.png",
    "img/ltc-error.png",
    "img/ltc-info.png",
    "img/ltc-warning.png",
    "img/ltc-wallet.png",
    "img/ltc-news.png"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "CrypDough LTC Mining Pool Monitoring Extension",
  "background": {
    "page": "html/background.html"
  },
  "name": "CrypDough.com",
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "background",
    "tabs",
    "https://*.crypdough.com/"
  ],
  "options_page": "html/options.html",
  "icons": {
    "128": "img/litecoin.png",
    "32": "img/litecoin.png",
    "64": "img/litecoin.png",
    "16": "img/litecoin.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.crypdough.com; object-src 'self' https://www.crypdough.com; content-src https://www.crypdough.com",
  "version": "1.2.1"
}
