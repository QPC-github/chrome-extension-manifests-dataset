{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Incogneo",
  "version": "1.2",
  "description": "You've gone incogneo",
  "manifest_version": 2,
  "incognito": "split",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://newtab/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
