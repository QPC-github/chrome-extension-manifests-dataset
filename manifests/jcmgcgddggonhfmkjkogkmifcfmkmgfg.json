{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Render Selection",
  "permissions": [
    "contextMenus",
    "tabs",
    "clipboardRead",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.5",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "icon.png"
  },
  "manifest_version": 2
}
