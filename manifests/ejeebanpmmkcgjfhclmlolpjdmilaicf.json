{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sueca Online",
  "manifest_version": 2,
  "description": "Joga Sueca Online com os teus familiares e amigos!",
  "version": "0.6",
  "browser_action": {
    "default_icon": "images/icon_48.png",
    "default_popup": "popup.html",
    "default_title": "Sueca Online"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "webRequest",
    "*://*.suecaonline.net/"
  ],
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "omnibox": {
    "keyword": "sueca online"
  }
}
