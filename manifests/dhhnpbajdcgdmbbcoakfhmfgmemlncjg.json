{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "jQuery Audit",
  "short_name": "jQuery Audit",
  "version": "1.1.1",
  "description": "Audit jQuery's delegated events, internal data, and more.",
  "icons": {
    "128": "icon128.png"
  },
  "author": "Andres Suarez <zertosh@gmail.com>",
  "homepage_url": "https://github.com/zertosh/jquery-audit",
  "devtools_page": "devtoolsBackground.html",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
