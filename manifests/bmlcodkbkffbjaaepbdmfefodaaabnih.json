{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GiveBack Chrome Extension",
  "description": "This extension will allow you to easily use GiveBack Search engine",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://giveback.world",
    "search_provider": {
      "name": "GiveBack Search Engine",
      "keyword": "giveback.world",
      "search_url": "http://giveback.world/websearch;q={searchTerms};offset=0;addon=opensearch",
      "favicon_url": "http://giveback.world/favicon.ico",
      "suggest_url": "http://giveback.world/suggest;websearch;q={searchTerms};offset=0;addon=opensearch",
      "instant_url": "http://giveback.world/instant;websearch;q={searchTerms};offset=0;addon=opensearch",
      "image_url": "http://giveback.world/images;q={searchTerms};offset:0;addon=opensearch",
      "search_url_post_params": "search_lang=__MSG_url_domain__",
      "suggest_url_post_params": "suggest_lang=__MSG_url_domain__",
      "instant_url_post_params": "instant_lang=__MSG_url_domain__",
      "image_url_post_params": "image_lang=__MSG_url_domain__",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://giveback.world/"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
