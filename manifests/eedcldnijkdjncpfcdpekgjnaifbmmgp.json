{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Purdue Dining Tracker",
  "version": "1.0.2",
  "description": "Easily find your personal favorites meals at Purdue Dining Courts.",
  "icons": {
    "128": "images/logo.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "clipboardWrite",
    "https://api.hfs.purdue.edu/menus/v2/locations/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
