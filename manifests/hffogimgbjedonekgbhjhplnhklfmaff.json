{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "19": "imgs/icon19.png",
      "38": "imgs/icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Grooveshark Remote"
  },
  "description": "Control your Grooveshark playback from any window.",
  "icons": {
    "16": "imgs/icon16.png",
    "48": "imgs/icon48.png",
    "128": "imgs/icon128.png"
  },
  "manifest_version": 2,
  "name": "Grooveshark Remote",
  "permissions": [
    "tabs",
    "http://grooveshark.com/*",
    "https://grooveshark.com/*"
  ],
  "version": "1.0"
}
