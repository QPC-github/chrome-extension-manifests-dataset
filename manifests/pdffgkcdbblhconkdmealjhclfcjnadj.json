{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unvis it",
  "description": "Right click shortcut to use Unvis.it",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "unvis.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png"
  }
}
