{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "A theme devoted to the most incredible series of 2016. Stranger Things! Monday mornings are for coffee and contemplation. Enjoy :-)",
  "name": "Stranger Things Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_title": "Stranger Things!"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Stranger Things.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
