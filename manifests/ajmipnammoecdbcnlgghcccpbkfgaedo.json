{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "Direction_Horz.png"
  },
  "name": "Divvi",
  "description": "Browse multiple sites at the same time with divvi!",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
