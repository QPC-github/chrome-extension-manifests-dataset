{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NMR plugin",
  "version": "1.4",
  "permissions": [
    "https://tomtop/"
  ],
  "description": "NMR plugin",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
