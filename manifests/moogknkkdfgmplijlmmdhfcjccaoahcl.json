{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Instashare",
  "description": "Instantly Share with Friends!",
  "version": "0.0.1.140",
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "Share this tab",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "ui/icon.png",
    "48": "ui/icon.png",
    "128": "ui/icon.png"
  }
}
