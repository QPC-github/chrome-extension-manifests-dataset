{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Java.net URL rewrite",
  "description": "Java.net to Github.io rewrite",
  "version": "1.2",
  "background": {
    "scripts": [
      "rewrite.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.java.net/"
  ]
}
