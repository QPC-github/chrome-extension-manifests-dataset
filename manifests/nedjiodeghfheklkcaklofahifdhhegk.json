{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hacker News Feed",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Get the latest from Hacker News",
  "icons": {
    "16": "kdt.png",
    "48": "kdt.png",
    "128": "kdt.png"
  },
  "permissions": [
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "kdt.png",
    "default_title": "HN Extension",
    "default_popup": "index.html"
  }
}
