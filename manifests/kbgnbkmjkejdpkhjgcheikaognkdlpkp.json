{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tumbyTagSearcher",
  "version": "1.0.3",
  "description": "Enjoy Tumblr's beloved \"old school\" Tracked Tag Search results format alongside the new Search--with a single click",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "Do the tumbyTagSearch"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
