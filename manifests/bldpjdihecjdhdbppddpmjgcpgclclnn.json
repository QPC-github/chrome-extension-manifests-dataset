{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "manifest_version": 2,
  "name": "Petrolicious Typography",
  "content_scripts": [
    {
      "matches": [
        "http://www.petrolicious.com/*"
      ],
      "css": [
        "petrolicious.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "petrolicious.css"
  ]
}
