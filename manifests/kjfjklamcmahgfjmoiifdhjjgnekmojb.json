{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AppSumo (Beta) ",
  "short_name": "AppSumo (Beta) ",
  "description": "A simple extension for AppSumo fanatics to track deals and easily find Roadmaps and Changelogs for all their favorite AppSumo deals.",
  "version": "1.0.2",
  "background": {
    "persistent": false,
    "scripts": [
      "js/analytics.js",
      "js/jquery-2.0.3.min.js",
      "js/OneSignal.js",
      "js/background.js",
      "js/jquery-3.4.1.min.js"
    ]
  },
  "permissions": [
    "storage",
    "https://appsumo.com/api/deals",
    "gcm",
    "notifications",
    "identity"
  ],
  "icons": {
    "16": "img/as-favicon-16.png",
    "48": "img/as-favicon-48.png",
    "128": "img/as-favicon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/as-favicon-48.png",
      "38": "img/as-favicon-48.png"
    },
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com; object-src 'self'",
  "web_accessible_resources": [
    "img/as-favicon-128.png"
  ]
}
