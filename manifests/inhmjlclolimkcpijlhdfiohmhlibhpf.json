{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Velocímetro Proteste",
  "description": "Teste a velocidade da sua conexão de internet e veja se sua velocidade está abaixo da contratada",
  "version": "1.0.5",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_title": "Velocímetro Proteste - Testar minha velocidade"
  },
  "permissions": [
    "notifications",
    "storage",
    "alarms"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon.png"
  }
}
