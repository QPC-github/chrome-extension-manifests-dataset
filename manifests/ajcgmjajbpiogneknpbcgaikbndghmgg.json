{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.min.js",
        "hodor.js"
      ],
      "matches": [
        "https://apis.google.com/*",
        "https://plus.googleapis.com/*",
        "https://www.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Hodors your HodorTube by adding Hodors instead of the comments.",
  "homepage_url": "http://httpster.io",
  "icons": {
    "128": "hodor128.png",
    "48": "hodor48.png"
  },
  "manifest_version": 2,
  "name": "HodorTube",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
