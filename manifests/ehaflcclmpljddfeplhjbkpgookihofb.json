{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome Lockdown",
  "description": "One Click Privacy. New: Log out of all of your accounts just by putting your computer to sleep!",
  "version": "0.28",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Chrome Lockdown"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "cookies",
    "<all_urls>",
    "idle",
    "identity",
    "privacy",
    "history"
  ],
  "oauth2": {
    "client_id": "750162415744-3cm5o9ggtqcmu0tpqoav67ffd2k9jej6.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "key": ""
}
