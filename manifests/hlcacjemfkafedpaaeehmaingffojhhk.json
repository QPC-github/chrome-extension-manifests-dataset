{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "InNote",
  "short_name": "InNote",
  "version": "0.1.0.0",
  "description": "Keep Notes on your LinkedIn Contacts!",
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/img16.png",
      "32": "images/img32.png"
    }
  },
  "icons": {
    "16": "images/img16.png",
    "32": "images/img32.png"
  }
}
