{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.htm"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "BIT助手"
  },
  "content_scripts": [
    {
      "css": [
        "insert.css"
      ],
      "js": [
        "jsInSite.js"
      ],
      "matches": [
        "http://10.5.2.80/*"
      ]
    }
  ],
  "description": "One extension a day, keep IE6 away.",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "BIT助手",
  "permissions": [
    "tabs",
    "notifications",
    "cookies",
    "http://10.5.2.80/*",
    "http://jwc.bit.edu.cn/*",
    "https://10.5.2.80/*",
    "https://jwc.bit.edu.cn/*"
  ],
  "version": "0.16",
  "web_accessible_resources": [
    "applyCSS/jwcmain.css",
    "applyCSS/jwcwin8.css",
    "img/icon.png"
  ]
}
