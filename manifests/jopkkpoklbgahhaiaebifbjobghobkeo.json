{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Taiwan Typhoon Holidays",
  "description": "A typhoon is coming soon ? Check if your city got some holidays.",
  "version": "1.15",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.dgpa.gov.tw/ndse.html"
  ]
}
