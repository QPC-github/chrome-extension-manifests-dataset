{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": " Trump The Talking Yam",
  "version": "1.0.0.1",
  "description": "Changes text that says Trump to nicknames such as The Talking Yam, The Angry Cheeto, and many more.",
  "icons": {
    "16": "images/trump16.png",
    "48": "images/trump48.png",
    "128": "images/trump128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
