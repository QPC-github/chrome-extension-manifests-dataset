{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "128": "/img/128.png",
    "64": "/img/64.png",
    "32": "/img/32.png"
  },
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "/pages/newtab.html"
  },
  "browser_action": {
    "default_icon": "/img/128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://catsnewtab.com/favicon.ico",
      "is_default": true,
      "keyword": "Default",
      "name": "__MSG_extName__",
      "search_url": "https://catsnewtab.com/search/?q={searchTerms}"
    }
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "history",
    "unlimitedStorage",
    "chrome://favicon/",
    "contextMenus",
    "storage",
    "topSites",
    "activeTab",
    "*://catsnewtab.com/*",
    "*://mini.s-shot.ru/*"
  ],
  "content_security_policy": "script-src 'self' https://query.yahooapis.com; object-src 'self'",
  "version": "1.1.0"
}
