{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Fav QR Code Generator",
  "version": "1.2",
  "manifest_version": 2,
  "description": "QR Code Generator, FavQR.com.",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
