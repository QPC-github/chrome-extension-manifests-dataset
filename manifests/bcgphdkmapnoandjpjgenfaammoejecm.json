{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Suodatin negatiivisille uutisille",
  "short_name": "Valon vartija",
  "version": "0.0.7",
  "description": "Lisää Iltasanomien ja Iltalehden sivuille huomaamattoman negatiivisten uutisten suodattimen ",
  "icons": {
    "16": "icon16.png",
    "48": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.is.fi/*"
      ]
    },
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.iltalehti.fi/*"
      ]
    }
  ]
}
