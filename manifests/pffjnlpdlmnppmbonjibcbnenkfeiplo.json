{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "AOTG.com Browser Extension",
  "version": "1.2",
  "manifest_version": 2,
  "description": "AOTG.com Auto Submission Script",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://ajax.googleapis.com/",
    "bookmarks"
  ],
  "icons": {
    "16": "icon_16x16.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  }
}
