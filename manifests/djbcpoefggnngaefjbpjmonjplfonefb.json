{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SSPS api",
  "description": "Personalization of the SSPS api",
  "version": "1.3",
  "permissions": [
    "storage"
  ],
  "page_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://ssps.cz/api/messages"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
