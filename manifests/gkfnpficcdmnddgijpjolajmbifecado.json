{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Huya Web Checker",
  "description": "Huya Web Checker",
  "version": "1.1.1.0",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://*/*",
    "downloads",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Huya Web Checker",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
