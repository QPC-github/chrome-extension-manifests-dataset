{
  "app": {
    "background": {
      "scripts": [
        "min.js"
      ]
    }
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Binary file analyzer for Chrome",
  "name": "HexReader",
  "permissions": [
    "fileSystem",
    "contextMenus",
    "clipboardWrite",
    "storage"
  ],
  "minimum_chrome_version": "23",
  "icons": {
    "128": "icon.png",
    "16": "icon_small.png"
  },
  "offline_enabled": true,
  "manifest_version": 2,
  "version": "0.5.24"
}
