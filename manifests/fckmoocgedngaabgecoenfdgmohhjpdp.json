{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "default_locale": "ru",
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "96": "img/icon96.png",
      "128": "img/icon128.png"
    },
    "default_title": "__MSG_button_title__"
  },
  "description": "__MSG_app_description__",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "96": "img/icon96.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_app_name__",
  "permissions": [
    "tabs",
    "*://radio.yandex.ru/*"
  ],
  "version": "1.0.0"
}
