{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "That's Not Real",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "ALERT!",
    "default_icon": "warning.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ]
}
