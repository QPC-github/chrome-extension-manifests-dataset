{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitch Contrast",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Change Twitch chat contrast",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Twitch Contrast"
  },
  "permissions": [
    "https://www.twitch.tv/*",
    "http://www.twitch.tv/*",
    "tabs"
  ]
}
