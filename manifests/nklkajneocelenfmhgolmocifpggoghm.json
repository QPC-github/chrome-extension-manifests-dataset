{
  "manifest_version": 2,
  "name": "Own cursor",
  "options_page": "nastroyka.html",
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.7",
  "version_name": "1.7",
  "icons": {
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/icon128.png",
      "256": "icons/icon256.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/listofcollection.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery.js",
        "js/owncursor.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "description": "Own cursor Colorful cursors make your browser Colorful"
}
