{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube redirect tracking remover",
  "description": "Removes YouTube redirect tracking links from the description of YouTube videos.",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "App.js"
      ]
    }
  ]
}
