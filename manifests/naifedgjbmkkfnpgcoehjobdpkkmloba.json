{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zanita - Home Design and Decor News",
  "short_name": "Zanita",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "This awesome extension was created for the Zanita.info. With this extension, you can news up to date on Home Design and Decoration.",
  "homepage_url": "http://www.zanita.info",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
