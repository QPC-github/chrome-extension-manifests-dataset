{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Samurai Fruit",
  "short_name": "Play Samurai Fruit game",
  "description": "Use your samurai skills together with your katana to slash fruit to pieces as they fly up in the air.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "samuraifruit128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://ajax.googleapis.com/"
  ]
}
