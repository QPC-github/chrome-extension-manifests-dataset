{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Admicro check SSP",
  "description": "Tìm kiếm check banner mã code trùng",
  "version": "2.0",
  "icons": {
    "48": "icon.png",
    "64": "icon.png",
    "128": "icon.png",
    "256": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Admicro check SSP",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "*"
  ]
}
