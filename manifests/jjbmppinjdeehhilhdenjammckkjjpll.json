{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notifier for HackerOne",
  "version": "1.0.3",
  "description": "Instant notification from HackerOne",
  "icons": {
    "49": "./images/icon49.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "notifications",
    "https://hackerone.com/notifications.json",
    "storage"
  ]
}
