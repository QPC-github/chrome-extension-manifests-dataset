{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "트뮈터",
  "version": "1.0.2",
  "description": "트뮈터믜 짐그러문 돔그라미를 네모로 바꿉니다.",
  "author": "koreapyj",
  "short_name": "TwitterSquares",
  "content_scripts": [
    {
      "js": [
        "twitter-squares.user.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "permissions": [
    "https://twitter.com/*"
  ]
}
