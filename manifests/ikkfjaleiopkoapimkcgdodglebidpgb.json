{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Populr",
  "description": "Easily create & share pages on the web",
  "version": "1.4",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "urls": [
      "*://populr.me/",
      "*://www.populr.me/"
    ],
    "launch": {
      "web_url": "https://populr.me"
    },
    "permissions": [
      "unlimitedStorage",
      "notifications"
    ]
  }
}
