{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Lets you share your desktop with your FreeConference participants.",
  "externally_connectable": {
    "matches": [
      "*://*.beta.freeconference.com/*",
      "*://*.freeconference.com/*",
      "*://*.freeconference.co.uk/*"
    ]
  },
  "icons": {
    "128": "puffin-128x128.png",
    "48": "puffin-48x48.png",
    "16": "puffin-16x16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "FreeConference Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "version": "0.1.4.4"
}
