{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Copy Cookie",
  "version": "1.0.4",
  "description": "An extension to copy cookies across the domains specifically localhost.",
  "permissions": [
    "cookies",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "author": "Avaneesh Tripathi"
}
