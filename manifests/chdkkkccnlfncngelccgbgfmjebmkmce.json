{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ARC Toolkit",
  "version": "3.3.2.0",
  "version_name": "3.3.2.0",
  "description": "Accessibility testing tools from The Paciello Group",
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "ARC_16px.png",
    "default_popup": "popup.html",
    "default_title": "ARC Toolkit"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "ARC_16px.png",
    "48": "ARC_48px.png",
    "128": "ARC_128px.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "file:///"
  ],
  "short_name": "ARC Toolkit",
  "manifest_version": 2,
  "default_locale": "en"
}
