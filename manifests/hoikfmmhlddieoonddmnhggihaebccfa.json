{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Laravel Documentation Expander",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Expands all menu items in the Laravel documentation that are collapsed by default and removes ad",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://laravel.com/docs/*"
      ],
      "js": [
        "./inject.js"
      ]
    }
  ]
}
