{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ČSFD předpověď",
  "version": "0.0.5",
  "description": "Přepovídání hodnocení filmů na ČSFD",
  "permissions": [
    "storage",
    "http://www.csfd.cz/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.csfd.cz/film/*"
      ],
      "js": [
        "node_modules/string-similarity/node_modules/lodash/dist/lodash.min.js",
        "compare-strings.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/popup-icon-19.png",
      "38": "images/popup-icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "author": "Filip Chalupa <chalupa.filip@gmail.com>"
}
