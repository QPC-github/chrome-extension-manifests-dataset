{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gindex",
  "author": "Guillermo Reyes",
  "version": "1.0",
  "description": "Muestra el número de páginas indexadas en Google",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/get_started128.png",
    "default_title": "See indexing"
  },
  "homepage_url": "https://apostilleint.com/",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
