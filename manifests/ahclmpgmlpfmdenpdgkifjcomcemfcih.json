{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WatchDog",
  "version": "1.12",
  "description": "WatchDog is the perfect productivity tool for all dog-lovers (and everyone else!)",
  "author": "James Park",
  "permissions": [
    "storage",
    "tabs",
    "background",
    "activeTab",
    "audio",
    "management"
  ],
  "background": {
    "scripts": [
      "background.js",
      "user-data.js",
      "jquery-3.3.1.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "WatchDog128.png",
    "512": "WatchDog512.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
