{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "256": "volume256.png",
      "128": "volume128.png",
      "48": "volume48.png",
      "16": "volume16.png"
    },
    "default_popup": "popup.html",
    "default_title": "Open Audio Splitter and Booster"
  },
  "manifest_version": 2,
  "name": "Audio Splitter and Booster",
  "short_name": "Audio Splitter",
  "author": "Kulwinderjit Singh and Manpreet Kaur",
  "description": "This can be used to play audio/video from two tabs side by side or on two monitors and their audio in left and right speakers.",
  "permissions": [
    "tabCapture",
    "activeTab",
    "storage"
  ],
  "icons": {
    "256": "volume256.png",
    "128": "volume128.png",
    "48": "volume48.png",
    "16": "volume16.png"
  },
  "version": "0.0.2"
}
