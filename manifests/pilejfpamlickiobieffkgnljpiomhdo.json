{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "256": "icon256.png"
    },
    "default_popup": "popup.html",
    "default_title": "Organize your workspaces"
  },
  "description": "Groups tabs together in workspaces to be loaded in bulk",
  "manifest_version": 2,
  "name": "Workspaces",
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "https://ajax.googleapis.com/"
  ],
  "version": "1.0"
}
