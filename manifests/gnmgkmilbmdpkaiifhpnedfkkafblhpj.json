{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LeetCode Patch",
  "short_name": "Patch",
  "version": "4.1",
  "description": "Helps Hide Leetcode's Difficulty, Acceptance, and Last Executed Input.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problemset/*",
        "https://leetcode.com/problems/*",
        "https://leetcode.com/tag/*",
        "https://leetcode.com/company/*",
        "https://leetcode.com/contest/*"
      ],
      "js": [
        "js/jquery.js",
        "js/common.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "js": [
        "js/content-script.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://leetcode.com/problemset/*",
        "https://leetcode.com/tag/*",
        "https://leetcode.com/company/*"
      ],
      "js": [
        "js/problemset-script.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://leetcode.com/contest/*/problems/*"
      ],
      "js": [
        "js/contest-script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo_v8.0.png",
      "32": "logo_v8.0.png",
      "48": "logo_v8.0.png",
      "128": "logo_v8.0.png"
    }
  },
  "icons": {
    "16": "logo_v8.0.png",
    "32": "logo_v8.0.png",
    "48": "logo_v8.0.png",
    "128": "logo_v8.0.png"
  },
  "manifest_version": 2
}
