{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video LR mirror",
  "description": "video left right mirror",
  "default_locale": "ko",
  "version": "1.1",
  "icons": {
    "512": "lr512.png",
    "256": "lr256.png",
    "128": "lr128.png",
    "64": "lr64.png"
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": "lr512.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "MacCtrl+Shift+X",
        "linux": "Ctrl+Shift+X"
      },
      "description": "video left right mirror"
    }
  }
}
