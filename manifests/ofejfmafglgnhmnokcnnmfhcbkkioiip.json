{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "bitfinex32.png",
    "default_popup": "popup.html"
  },
  "description": "Realtime bitcoin prices from Bitfinex in USD and EUR",
  "icons": {
    "128": "bitfinex128.png"
  },
  "manifest_version": 2,
  "name": "Bitfinex Ticker",
  "permissions": [
    "https://api.bitfinex.com/",
    "http://www.forex.com/",
    "https://www.galmarley.com/"
  ],
  "version": "1.0.1"
}
