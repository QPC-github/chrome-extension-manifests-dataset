{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ExtensionHunt",
  "short_name": "Enhancing Web Experiences",
  "version": "0.0.2",
  "description": "ExtensionHunt",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "sample-19.png"
  },
  "icons": {
    "16": "sample-16.png",
    "48": "sample-48.png",
    "128": "sample-128.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "onload.js"
    ]
  }
}
