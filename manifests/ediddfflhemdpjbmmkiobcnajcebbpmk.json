{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TwitMute",
  "description": "Remove Everything,You Call Noise!",
  "manifest_version": 2,
  "permissions": [
    "https://twitter.com/*",
    "tabs",
    "storage",
    "activeTab"
  ],
  "version": "1.0.4",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "ui.js",
        "functions.js",
        "jquery.bpopup.min.js",
        "script.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icons/128.png",
    "64": "icons/64.png",
    "48": "icons/48.png",
    "20": "icons/20.png",
    "16": "icons/16.png"
  },
  "web_accessible_resources": [
    "/img/keyword.png",
    "/img/hash.png",
    "/img/profile.png",
    "/img/popup.png",
    "/img/mutelink.png",
    "/img/mention.png",
    "/img/clear.png",
    "/img/on.png",
    "/img/confirm.png",
    "/img/search.png",
    "/img/bpopup.png",
    "/img/my_avatar.png",
    "/img/jquery.png",
    "/img/reset.png",
    "/img/off.png"
  ]
}
