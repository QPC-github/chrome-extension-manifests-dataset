{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kasikorn Bank starter and warning remover",
  "description": "Start Kasikorn bank login page with out annoying noisy warning for trojan spyware from fraudelent sms and email.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "kasikorn.js"
    ]
  },
  "icons": {
    "48": "48x48.png"
  },
  "browser_action": {
    "default_icon": "48x48.png",
    "default_title": "KasikornBank start"
  },
  "manifest_version": 2,
  "version": "0.1"
}
