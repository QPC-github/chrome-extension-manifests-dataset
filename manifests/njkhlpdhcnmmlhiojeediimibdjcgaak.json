{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ISBN Converter",
  "description": "Convert 13 Digit ISBN to 10 Digit and vice versa",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
