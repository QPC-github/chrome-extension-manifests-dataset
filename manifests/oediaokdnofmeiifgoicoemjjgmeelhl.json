{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MS: NO FILTER",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "MS: No Filter â€“ a browser technology that turns MS information online into real talk.",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/48.png",
    "default_title": "MS: No Filter",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "jquery.qtip.js",
        "content.js",
        "find-and-replace.js",
        "ms-no-filter.js"
      ],
      "css": [
        "jquery.qtip.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "content.js",
    "jquery.qtip.js",
    "find-and-replace.js",
    "ms-no-filter.js",
    "jquery.qtip.css",
    "images/32.png",
    "msnf-logo.jpg"
  ]
}
