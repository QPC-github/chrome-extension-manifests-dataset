{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Janus",
  "short_name": "Janus",
  "description": "Change sled dog",
  "version": "10.2.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "def128.png",
    "default_title": "Country Here",
    "default_popup": ""
  },
  "permissions": [
    "http://thejanus.org/",
    "tabs",
    "background",
    "proxy",
    "management",
    "storage"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
