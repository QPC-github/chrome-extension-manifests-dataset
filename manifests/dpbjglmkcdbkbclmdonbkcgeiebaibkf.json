{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Five9 Agent Desktop Toolkit",
  "description": "Five9 Agent Desktop Toolkit.",
  "version": "0.3",
  "options_page": "options.html",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "http://localhost/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Five9 Agent Desktop Toolkit"
  },
  "web_accessible_resources": [
    "five9.js",
    "bridgeNotRunning.html"
  ]
}
