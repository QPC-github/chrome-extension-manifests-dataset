{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "/img/icon-32.png",
    "default_title": "Ads Earnings Checker",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' chrome-extension-resource: https://ajax.googleapis.com https://ssl.google-analytics.com; object-src 'self';",
  "description": "Shows earnings from the Ad networks.",
  "icons": {
    "16": "/img/icon-16.png",
    "24": "/img/icon-24.png",
    "32": "/img/icon-32.png",
    "48": "/img/icon-48.png",
    "64": "/img/icon-64.png",
    "128": "/img/icon-128.png",
    "256": "/img/icon-256.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "4.0.302.2",
  "name": "Ads Earnings Checker",
  "short_name": "Ads Earnings",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
