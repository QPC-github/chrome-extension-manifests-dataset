{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ireeed",
  "version": "1.4.3",
  "manifest_version": 2,
  "description": "Ireeed Recommendation",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.jpeg"
  },
  "browser_action": {
    "default_icon": "icon128.jpeg",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery.js",
        "my.js"
      ]
    }
  ]
}
