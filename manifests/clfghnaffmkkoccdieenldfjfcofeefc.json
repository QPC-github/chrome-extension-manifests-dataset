{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Moyesbuster",
  "description": "Replace mentions of David Moyes with...Nick Cage",
  "version": "0.1",
  "permissions": [
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
