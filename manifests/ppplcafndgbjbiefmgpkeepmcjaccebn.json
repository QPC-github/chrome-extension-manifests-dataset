{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "<body class='domain'>",
  "version": "1.5",
  "description": "Assigns the body element a class value of the website's domain.",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "bodyclassdomain.js"
      ]
    }
  ]
}
