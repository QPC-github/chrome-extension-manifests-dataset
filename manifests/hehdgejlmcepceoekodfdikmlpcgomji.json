{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SIP Phone Extension",
  "description": "This is Sip based soft Phone which have click to call, Caller information with name , call logs integration with CRM, ERP.",
  "short_name": "Click to call phone",
  "version": "0.3",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "audioCapture",
    "storage"
  ],
  "icons": {
    "16": "img/icon-phone.png",
    "128": "img/icon-phone.png"
  }
}
