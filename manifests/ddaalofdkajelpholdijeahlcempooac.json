{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "XML-RPC Tester!",
  "description": "This extension helps you test xml rpc services!",
  "version": "0.8",
  "background": {
    "page": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "http://*/"
  ],
  "browser_action": {
    "default_icon": "img/icon-16.png",
    "default_title": "XML-RPC Tester!"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
