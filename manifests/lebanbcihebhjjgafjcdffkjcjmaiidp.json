{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Mohamed ELbahja",
  "homepage_url": "https://elbahja.me",
  "version": "1.4.1",
  "minimum_chrome_version": "63",
  "manifest_version": 2,
  "name": "KO! for Facebook",
  "short_name": "KO!",
  "description": "KO! for Facebook helps you to block tracking and collecting your data on websites you visit outside of facebook.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "KickFacebookAss.js"
    ]
  },
  "browser_action": {
    "default_popup": "show.html"
  },
  "icons": {
    "16": "assets/icons/16.png",
    "32": "assets/icons/32.png",
    "48": "assets/icons/48.png",
    "120": "assets/icons/120.png",
    "128": "assets/icons/128.png"
  }
}
