{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Frugal Testing Recorder",
  "short_name": "Frugal Testing",
  "version": "1.1.5",
  "description": "Frugal Testing's chrome extension for recording browser traffic into a jmx script",
  "icons": {
    "128": "logo.png",
    "48": "logo.png",
    "16": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "browsingData",
    "webRequest",
    "<all_urls>",
    "tabs",
    "notifications"
  ]
}
