{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Classmiles Screenshare",
  "description": "Screensharing Extension for Classmiles",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "desktopCapture",
    "https://*/*",
    "tabs"
  ]
}
