{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EmailSherlock",
  "version": "1.2",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "description": "Free Email Search on EmailSherlock.com",
  "homepage_url": "http://www.emailsherlock.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "eSherlock.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "imgs/top_left.png",
    "imgs/top_right.png",
    "imgs/bottom_left.png",
    "imgs/bottom_right.png"
  ]
}
