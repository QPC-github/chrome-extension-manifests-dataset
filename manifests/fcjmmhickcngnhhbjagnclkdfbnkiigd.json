{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IRMA Auto Login",
  "short_name": "IAL",
  "version": "2.2",
  "manifest_version": 2,
  "description": "It Automates the process of Login. The icon on the top right of ur browser can be used for Login into Internet",
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon.png"
  },
  "background": {
    "scripts": [
      "lib/jquery-1.8.3.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon_grey.png",
    "default_title": "Cyberoam"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "notifications",
    "contextMenus",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ]
}
