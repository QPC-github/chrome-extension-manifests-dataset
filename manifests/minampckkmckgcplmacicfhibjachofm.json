{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.5",
  "name": "Кофе Твик",
  "description": "Оповещение о смене статуса работы мобильной кофейни Кофе Твик",
  "permissions": [
    "http://vk.com/*",
    "https://vk.com/*",
    "notifications",
    "storage"
  ],
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png",
    "19": "images/19.png"
  },
  "background": {
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "browser_action": {
    "default_title": "Узнать статус",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
