{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iAC",
  "version": "4.0.1",
  "description": "To provide quick access to localized *.atlassian.com",
  "icons": {
    "16": "image/icon_16.png",
    "48": "image/icon_48.png",
    "128": "image/icon_128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
