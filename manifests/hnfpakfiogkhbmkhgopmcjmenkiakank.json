{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Split Me",
  "description": "Split view for 4 different pages on the same tab",
  "version": "0.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
