{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Δ.everywhere()",
  "version": "0.3.1",
  "manifest_version": 2,
  "description": "Wrap Δ.js in every web page and do cool things with it",
  "homepage_url": "http://anythingjs.io",
  "icons": {
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/8",
    "https://*/*",
    "https://rawgit.com/Rabrennie/anything.js/master/dist/anything.min.js"
  ]
}
