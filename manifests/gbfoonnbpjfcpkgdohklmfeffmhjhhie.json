{
  "browser_action": {
    "default_icon": "images/icon_disabled.png",
    "default_popup": "popup.html",
    "name": "StreamIn"
  },
  "web_accessible_resources": [
    "images/icon_16.png"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "StreamIn enables you to receive real time messages and notifications from different applications. Register at http://streamin.io",
  "background": {
    "scripts": [
      "scripts/jquery-1.9.1.js",
      "scripts/streamin.js"
    ],
    "persistent": true
  },
  "name": "StreamIn",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "notifications",
    "http://app.streamin.io/api/*"
  ],
  "icons": {
    "128": "images/icon128.png"
  },
  "manifest_version": 2,
  "version": "1.7"
}
