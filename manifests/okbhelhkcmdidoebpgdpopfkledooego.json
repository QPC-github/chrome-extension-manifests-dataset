{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CPHE - Riverside Brookfield High School",
  "version": "15",
  "manifest_version": 2,
  "description": "Chrome PhoneHome Enterprise",
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery.steps.js",
      "background.js",
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CPH",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "background",
    "identity"
  ],
  "icons": {
    "128": "icon.png"
  },
  "accId": "26",
  "accCode": "26-320f2",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "web_accessible_resources": [
    "jquery.min.js",
    "jquery.steps.js",
    "popup.js",
    "googleplus.png"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.gdizlocal2.com/*",
      "*://*.gdiz.org/*",
      "*://*.pcphonehome.com/*"
    ]
  }
}
