{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ABBR!",
  "description": "This extension changes the title of abbr tags with the content.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "abbr.png",
    "name": "Test"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "32": "abbr.png",
    "128": "abbr_128.png"
  }
}
