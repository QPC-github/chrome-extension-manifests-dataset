{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.2",
  "description": "A theme solely devoted to the smartest and the most loved enemy of Conan, KAITO KID! Enjoy :-)",
  "name": "Magic-Kaito Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_title": "The Phantom Thief!"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Magic-Kaito.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
