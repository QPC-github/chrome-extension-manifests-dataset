{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK Search",
  "description": "Search in VK from Context Menu",
  "version": "0.8",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "search.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
