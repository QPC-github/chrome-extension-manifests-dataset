{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flashie",
  "description": "Use new tab page to learn something useful!",
  "version": "1.4",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "flashie.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "options_page": "manager.html",
  "permissions": [
    "storage"
  ]
}
