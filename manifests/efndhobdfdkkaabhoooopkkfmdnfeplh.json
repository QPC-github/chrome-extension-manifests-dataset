{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bundle Catcher",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "description": "Get the PWA/Adaptive bundle you are viewing on mobile/tablet sites",
  "minimum_chrome_version": "45.0",
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "panel.js"
    ],
    "persistent": true
  },
  "browser_action": {}
}
