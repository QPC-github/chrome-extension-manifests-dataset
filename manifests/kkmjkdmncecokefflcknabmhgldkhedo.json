{
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "64": "icons/icon64.png"
    },
    "default_title": "Clocky",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "description": "Clocky help you count how productive you are excuting different tasks",
  "name": "Clocky",
  "background": {
    "scripts": [
      "scripts/time.js",
      "uid.js",
      "scripts/promised.js",
      "background.js"
    ],
    "persistent": true
  },
  "version": "9.5.13",
  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png"
  },
  "content_security_policy": "script-src blob: filesystem: chrome-extension-resource: 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webNavigation",
    "<all_urls>",
    "alarms",
    "webRequest",
    "notifications",
    "storage",
    "tabs",
    "webRequestBlocking",
    "background"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "web_accessible_resources": [],
  "content_scripts": []
}
