{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PonyThread Notifications",
  "version": "0.0.1.3",
  "description": "It displays notifications from PonyThread, what more is there to explain?",
  "icons": {
    "16": "img/icons/icon16.png",
    "19": "img/icons/icon19.png",
    "32": "img/icons/icon32.png",
    "38": "img/icons/icon38.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png",
    "192": "img/icons/icon192.png",
    "256": "img/icons/icon256.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icons/icon16.png",
      "19": "img/icons/icon19.png",
      "32": "img/icons/icon32.png",
      "38": "img/icons/icon38.png",
      "48": "img/icons/icon48.png",
      "128": "img/icons/icon128.png",
      "192": "img/icons/icon192.png",
      "256": "img/icons/icon256.png"
    },
    "default_popup": "popup.html"
  },
  "author": "1RV34",
  "homepage_url": "http://www.ponythread.com/",
  "incognito": "spanning",
  "options_page": "options.html",
  "permissions": [
    "http://www.ponythread.com/",
    "https://www.ponythread.com/",
    "cookies"
  ]
}
