{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Notable Extension",
  "version": "3.0.6",
  "manifest_version": 2,
  "description": "Notable lets you take any screenshot, sketch or wireframe and exchange notes on specific details with your team. Made by ZURB.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "activeTab"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
