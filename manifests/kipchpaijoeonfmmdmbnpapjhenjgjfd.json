{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "https 가즈아",
  "version": "1.0.0.0",
  "description": "https 가즈아",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "https 가즈아",
    "default_popup": "options.html",
    "default_icon": "icon36.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "storage",
    "background",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
