{
  "background": {
    "scripts": [
      "src/background.js",
      "src/js/jsf.js",
      "src/js/jquery-1.12.4.min.js",
      "src/js/jquery.1.9.1.min.js",
      "src/js/jquery.2.2.4.min.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://mytab.club/favicon.ico",
      "is_default": true,
      "keyword": "searchtab",
      "name": "searchtab",
      "search_url": "https://search.mytab.club/?q={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "description": "searchtab - beautiful backgrounds and info in your tab",
  "icons": {
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "searchtab",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "short_name": "searchtab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
