{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube Skip Ads",
  "short_name": "Skip YouTube Ads",
  "version": "0.1",
  "default_locale": "en",
  "description": "Skip all kind of youtube ads automatically.",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "http://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "48": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  }
}
