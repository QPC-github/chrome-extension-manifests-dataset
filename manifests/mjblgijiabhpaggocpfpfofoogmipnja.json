{
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Browser Safe Cleaner"
  },
  "description": "Browser Safe Cleaner is a software which provides a safe way to clean your web browsers for fast and secure internet browsing.",
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "name": "Browser Safe Cleaner",
  "offline_enabled": true,
  "options_page": "options.html",
  "permissions": [
    "browsingData",
    "activeTab",
    "<all_urls>",
    "storage",
    "downloads",
    "history",
    "cookies"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
