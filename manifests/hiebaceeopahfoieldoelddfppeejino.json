{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StudyPhim.vn Free",
  "description": "Addon 'VIP for studyphim.vn' có chèn thêm Content làm chậm tốc độ Internet. Addon này chỉ có mục đích duy nhất là xem Free",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.studyphim.vn/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
