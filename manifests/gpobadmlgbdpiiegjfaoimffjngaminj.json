{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Futhead FIFA 17 Ultimate Team Search",
  "version": "17.0.2",
  "manifest_version": 2,
  "description": "FIFA 17 Ultimate Team Player Search for Futhead.com",
  "permissions": [
    "http://www.futhead.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Futhead 17",
    "default_icon": "icon.png",
    "default_popup": "futhead.html"
  },
  "web_accessible_resources": [
    "images/fh-red.png",
    "images/search-bg.png",
    "images/cancel.png",
    "images/loader.gif"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Toggle feature foo"
    }
  }
}
