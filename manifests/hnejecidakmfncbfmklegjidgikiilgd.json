{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Home Button",
  "description": "Go to home page on current website quickly",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "48.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
