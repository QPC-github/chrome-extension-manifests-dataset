{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "file_handlers": {
    "NES ROM image": {
      "title": "YAME",
      "types": [
        "application/x-nes-rom"
      ],
      "extensions": [
        "nes"
      ]
    },
    "GAMEBOY ROM image": {
      "title": "YAME",
      "types": [
        "application/x-gb-rom"
      ],
      "extensions": [
        "gb",
        "gbc"
      ]
    },
    "SNES ROM image": {
      "title": "YAME",
      "types": [
        "application/x-snes-rom"
      ],
      "extensions": [
        "sfc",
        "smc",
        "fig",
        "spc"
      ]
    },
    "PC-Engine ROM image": {
      "title": "YAME",
      "types": [
        "application/x-pce-rom"
      ],
      "extensions": [
        "pce",
        "huc",
        "rom"
      ]
    }
  },
  "manifest_version": 2,
  "name": "YAME",
  "version": "0.38.10.3",
  "permissions": [
    "fileSystem",
    "unlimited_storage",
    "contextMenus",
    "storage",
    "fullscreen"
  ]
}
