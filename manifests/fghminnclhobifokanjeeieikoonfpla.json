{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unicircles",
  "description": "Share an article on Unicircles - www.unicircles.com",
  "version": "2.4",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon48x48.png",
    "128": "images/icon128x128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
