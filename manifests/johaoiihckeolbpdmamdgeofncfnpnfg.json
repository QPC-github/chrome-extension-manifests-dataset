{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Imagelicious Image Uploader",
  "description": "Adds a quick and simple upload menu for uploading images to Imagelicious.co",
  "version": "1.0",
  "permissions": [
    "unlimitedStorage",
    "storage",
    "contextMenus",
    "http://api.imagelicious.co/checkLogin.php",
    "http://api.imagelicious.co/fileUpload.php",
    "http://api.imagelicious.co/saveImage.php"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  }
}
