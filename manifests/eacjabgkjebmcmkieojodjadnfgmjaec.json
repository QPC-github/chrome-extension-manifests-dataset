{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CheckEm",
  "version": "0.1.0",
  "description": "For checking twitter Dub GETs.",
  "icons": {
    "16": "dubzGet16.ico",
    "48": "dubzGet48.ico",
    "128": "dubzGet128.ico"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.1.0.min.js",
        "content.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
