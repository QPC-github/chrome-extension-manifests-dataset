{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple XKCD",
  "description": "Quickly grabs XKCD of the day and displays it! :)",
  "version": "1.0.8",
  "permissions": [
    "https://xkcd.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  }
}
