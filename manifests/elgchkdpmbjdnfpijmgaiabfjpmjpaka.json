{
  "author": "Satrang Technologies",
  "background": {
    "scripts": [
      "back.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/external-link-2x.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.11.1.min.js",
        "background.js"
      ],
      "matches": [
        "https://*.salesforce.com/*",
        "http://*.salesforce.com/*"
      ]
    }
  ],
  "description": "This extension displays logged in Org/User name, current time and Logout, Dev Console, My Settings and My Profile as separate links.",
  "homepage_url": "http://www.satrangtech.com/",
  "icons": {
    "16": "img/external-link-2x.png",
    "24": "img/external-link-3x.png",
    "32": "img/external-link-4x.png",
    "48": "img/external-link-6x.png",
    "64": "img/external-link-8x.png",
    "8": "img/external-link.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Enhance Salesforce Header",
  "permissions": [
    "*://*.salesforce.com/*",
    "storage"
  ],
  "short_name": "Org/User Name, Current Time, Logout, Dev Console, My Settings and My Profile",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.5",
  "web_accessible_resources": [
    "background.js"
  ]
}
