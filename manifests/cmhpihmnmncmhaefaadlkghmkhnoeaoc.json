{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Poking",
  "author": "Jennifer Apacible",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Automate poking.",
  "content_scripts": [
    {
      "matches": [
        "http://*.facebook.com/pokes*",
        "https://*.facebook.com/pokes*"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
