{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "显式允许来自Origin的请求 / Modify cros option response into  `Access-Control-Allow-Origin: origin-host`",
  "version": "1.0.7",
  "name": "cros-anywhere",
  "author": "姚泽源",
  "homepage_url": "https://www.yaozeyuan.online/",
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "cros-anywhere",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "on.png",
    "off.png"
  ]
}
