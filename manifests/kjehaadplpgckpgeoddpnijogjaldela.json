{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Převod měn",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Převod měn, který pracuje s aktuálním kurzovním lístkem České národní banky.",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Převod měn",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://www.cnb.cz/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.cnb.cz; object-src 'self'"
}
