{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Baconitiate Beta v0.2",
  "short_name": "Baconitiate",
  "author": "A™B™C™",
  "description": "Is Art With Bacon Still Art?",
  "version": "0.2",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "p5.min.js",
        "p5.dom.min.js",
        "app.min.js"
      ]
    }
  ]
}
