{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PlebTV",
  "description": "Free Twitch Sub Emotes for those who dont want to give him money.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*"
      ],
      "js": [
        "lolalpha1.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "PlebTV",
    "default_icon": {
      "128": "gusta.png"
    }
  },
  "permissions": [
    "tabs",
    "https://www.twitch.tv/*"
  ]
}
