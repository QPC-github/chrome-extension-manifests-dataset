{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appName__",
  "default_locale": "en",
  "version": "1.0",
  "icons": {
    "128": "logo_himoinsa.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://spares.himoinsa.com/*",
      "*://power-support.himoinsa.com/*"
    ]
  },
  "permissions": [
    "contentSettings",
    "activeTab"
  ]
}
