{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My Hero Academia Wallpaper - New Tab Theme",
  "version": "1.0",
  "manifest_version": 2,
  "description": "My Hero Academia Wallpaper New Tab - Install and get a better looking Chrome with My Hero Academia theme extension",
  "icons": {
    "16": "icons/logo1.png",
    "48": "icons/logo2.png",
    "128": "icons/logo3.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "NeoSearch",
      "keyword": "keyword.myheroacademia.co",
      "search_url": "https://search.azulessolution.com/search?sfid=526987&pid=16&q={searchTerms}",
      "favicon_url": "https://azulessolution.com/wp-content/uploads/2019/01/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
