{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Cooki Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Cooki Extension",
  "background": {
    "page": "cooki.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "logo_icon_19.png",
      "38": "logo_icon_38.png"
    }
  },
  "icons": {
    "128": "logo_icon_128.png",
    "16": "logo_icon_small.png",
    "48": "logo_icon_large.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ]
}
