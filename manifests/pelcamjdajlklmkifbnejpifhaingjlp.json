{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome IG Profile Picture Enlarger",
  "short_name": "Big Form of IG Profile Picture Enlarger",
  "manifest_version": 2,
  "version": "1.8",
  "background": {
    "scripts": [
      "enlarger.js"
    ]
  },
  "icons": {
    "128": "instagram-ext.png"
  },
  "browser_action": {
    "default_icon": "instagram-ext.png"
  },
  "permissions": [
    "tabs",
    "https://www.instagram.com/"
  ],
  "web_accessible_resources": [
    "instagram-ext.png"
  ]
}
