{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Adblock Minus",
  "version": "1.0.7",
  "manifest_version": 2,
  "minimum_chrome_version": "50",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "background": {
    "scripts": [
      "vendor.js",
      "app.js"
    ]
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "homepage_url": "https://github.com/xiaody/adblock-minus"
}
