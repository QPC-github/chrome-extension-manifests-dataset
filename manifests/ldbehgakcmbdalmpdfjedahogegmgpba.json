{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My CookBook",
  "version": "1.0",
  "description": "Import your recipes",
  "permissions": [
    "activeTab",
    "storage",
    "cookies"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/mycookbook-16.png",
      "32": "images/mycookbook-32.png",
      "48": "images/mycookbook-48.png",
      "128": "images/mycookbook-128.png"
    }
  },
  "icons": {
    "16": "images/mycookbook-16.png",
    "32": "images/mycookbook-32.png",
    "48": "images/mycookbook-48.png",
    "128": "images/mycookbook-128.png"
  },
  "manifest_version": 2,
  "default_locale": "en"
}
