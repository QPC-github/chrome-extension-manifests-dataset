{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Keep Me Signed Out",
  "description": "KMSO forces you to stay signed out of websites, but leaves behind your bookmarks, history, and downloads.",
  "version": "1.2",
  "icons": {
    "128": "128x128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Keep Me Signed Out"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "browsingData",
    "https://ajax.googleapis.com/"
  ]
}
