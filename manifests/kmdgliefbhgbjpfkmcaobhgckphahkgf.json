{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Remove Tumblr Radar",
  "version": "2",
  "manifest_version": 2,
  "description": "Removes tumblr radar from dashboard.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.tumblr.com/dashboard"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
