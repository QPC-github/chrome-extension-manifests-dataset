{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweetee",
  "version": "1.0",
  "description": "A stylesheet for a read-only Twitter experience.",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_48.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*"
      ],
      "css": [
        "tweetee.css"
      ],
      "run_at": "document_end"
    }
  ]
}
