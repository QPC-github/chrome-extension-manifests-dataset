{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "start.js"
      ],
      "matches": [
        "http://www.volvooceanrace.com/static/tracker/*",
        "https://www.volvooceanrace.com/static/tracker/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "VoRProMode.user.js"
  ],
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "page": "bg.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "converted_from_user_script": true,
  "description": "Switch VoR Tracker to Pro !",
  "name": "VoR Pro Mode",
  "version": "1.0.6"
}
