{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "index.html"
  },
  "browser_action": {
    "default_icon": "icons/16x16.png",
    "default_popup": "popup.html",
    "default_title": "Чистилка"
  },
  "description": "Чистилка",
  "icons": {
    "128": "icons/128x128.png",
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png"
  },
  "manifest_version": 2,
  "name": "Чистилка",
  "permissions": [
    "management",
    "storage",
    "http://localhost/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost; object-src 'self'",
  "version": "1.2"
}
