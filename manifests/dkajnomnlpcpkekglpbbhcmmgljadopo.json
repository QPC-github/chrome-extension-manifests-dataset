{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Giphy Tabs",
  "short_name": "Giphy Tabs",
  "version": "1.0",
  "description": "Giphy Tabs",
  "homepage_url": "http://giphy.com",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "/img/giphy_icon_16.png",
    "48": "/img/giphy_icon_48.png",
    "128": "/img/giphy_icon_128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://suggestqueries.google.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}
