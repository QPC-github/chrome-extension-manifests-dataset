  "manifest_version": 2,
  "name": "pouet enhancement suite",
  "description": "enhances pouet.net",
  "version": "0.2.5",
  "permissions": [
    "https://demozoo.org/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.pouet.net/*",
        "http://www.pouet.net/*"
      ],
      "css": [
        "pouet.css",
        "pesicons.css"
      ],
      "js": [
        "pouet.js"
      ]
    }
  ]
}
