{
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Leo Chino"
  },
  "description": "Leo Chino parses, adds pinyin, meaning, and pronunciation to Chinese words. It is based on http://mand.jon-long.ca/web",
  "manifest_version": 2,
  "name": "Leo Chino",
  "permissions": [
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.0.0.2",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "icon-128x128.png",
    "16": "icon-16x16.png",
    "48": "icon-48x48.png"
  }
}
