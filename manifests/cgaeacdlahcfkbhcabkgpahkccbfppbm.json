{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "j-play",
  "version": "1.2.9",
  "description": "Interact with a Jeopardy! game on the J-Archive website.",
  "content_scripts": [
    {
      "js": [
        "third-party/jquery-2.0.3.min.js",
        "contentscript.js"
      ],
      "css": [
        "stylecontent.css"
      ],
      "matches": [
        "http://www.j-archive.com/showgame*"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "icons": {
    "128": "images/128x128.png"
  }
}
