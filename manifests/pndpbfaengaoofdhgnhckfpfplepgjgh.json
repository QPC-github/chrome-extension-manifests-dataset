{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Moon Mode",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "content.js"
      ]
    }
  ],
  "description": "Removes the sell button from a range of cryptocurrency exchanges, so that you don't accidently sell.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
