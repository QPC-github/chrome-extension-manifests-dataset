{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Headphone",
  "short_name": "headphone",
  "author": "Sonu Kumar Gupta",
  "description": "To cover all type of Headphone and used to reduce Error",
  "version": "0.01",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": []
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "receiver.html"
  },
  "options_ui": {
    "chrome_style": true
  },
  "permissions": [
    "desktopCapture",
    "notifications",
    "tabs",
    "tabCapture",
    "activeTab",
    "storage",
    "identity",
    "unlimitedStorage",
    "downloads"
  ]
}
