{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.12",
  "name": "Chomper",
  "short_name": "Chomper",
  "description": "Chomps through clickbait.",
  "icons": {
    "128": "app/images/chomper_128.png"
  },
  "key": "",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "app/scripts/bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "contextMenus",
    "background",
    "tabs",
    "activeTab"
  ]
}
