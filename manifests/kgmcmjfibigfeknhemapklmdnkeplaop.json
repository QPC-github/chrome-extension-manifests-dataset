{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HiVA26 - Behind The Tricks",
  "short_name": "HiVA26",
  "description": "Official Chrome Extension of Hiva26.com",
  "version": "2.0.1",
  "browser_action": {
    "default_popup": "view/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },
  "icons": {
    "32": "image/icon.png",
    "64": "image/icon.png",
    "128": "image/icon.png"
  },
  "permissions": [
    "http://www.hiva26.com/",
    "notifications",
    "storage",
    "tabs"
  ]
}
