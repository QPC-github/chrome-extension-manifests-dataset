{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto-Filler",
  "description": "This extension will auto-fill the inputs for uploading video files",
  "version": "1.2",
  "icons": {
    "48": "myIcon48.png"
  },
  "browser_action": {
    "default_icon": "myIcon.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "auto.js"
    ],
    "persistent": false
  }
}
