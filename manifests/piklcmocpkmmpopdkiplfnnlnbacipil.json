{
  "manifest_version": 2,
  "name": "Teclado Virtual",
  "description": "Teclado Virtual",
  "version": "1.2",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png",
    "650": "icon_650.png",
    "50": "icon_50.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-2.1.3.min.js",
        "jquery.mobile.custom.min.js",
        "script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "keyboard.html",
    "style.css",
    "images/*"
  ]
}
