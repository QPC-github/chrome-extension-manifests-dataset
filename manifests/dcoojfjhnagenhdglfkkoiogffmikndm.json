{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.8.3.min.js",
        "content.js"
      ],
      "matches": [
        "http://www.reddit.com/*"
      ]
    }
  ],
  "description": "Hide Multireddit Bar",
  "manifest_version": 2,
  "name": "Hide Multireddit Bar",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.4"
}
