{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon_32.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/jquery.min.js",
        "js/webtoolkit-sha1.js",
        "js/content.js"
      ],
      "matches": [
        "*://*.telexfree.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This is the best extension for Telexfree. Validade your Telexfree Ads directly from your back office.",
  "icons": {
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2,
  "name": "TelexExpress - Plugin for Telexfree",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs",
    "webRequest",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.1",
  "web_accessible_resources": [
    "images/icon_128.png",
    "icon_128.png",
    "js/jquery-ui.js",
    "js/content.js",
    "js/inject.js",
    "js/jquery-2.0.3.min.map",
    "js/jquery.min.map",
    "js/jquery-1.9.1.min.js",
    "js/bootstrap.min.js",
    "js/main.js"
  ]
}
