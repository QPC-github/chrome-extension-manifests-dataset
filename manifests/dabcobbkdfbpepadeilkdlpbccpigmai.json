{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Todo List",
  "description": "This extension helps you get stuff done",
  "version": "3.4",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Get stuff done"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "icons": {
    "16": "todo.png",
    "32": "todo32.png",
    "64": "todo64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "geolocation"
  ]
}
