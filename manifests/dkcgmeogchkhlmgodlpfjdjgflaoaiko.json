{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My Ads Words",
  "version": "0.0.1.1",
  "description": "Cung cáº¥p Ads words....",
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Ads Words",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
