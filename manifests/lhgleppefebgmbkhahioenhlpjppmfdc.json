{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Lowest Price",
  "version": "1.1",
  "description": "Get the lowest price for products on the Web!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "The Lowest Price",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
