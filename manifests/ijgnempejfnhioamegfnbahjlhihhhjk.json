{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wunjun Notification",
  "icons": {
    "16": "images/drawable-ldpi/ic_launcher.png",
    "48": "images/drawable-mdpi/ic_launcher.png",
    "128": "images/drawable-hdpi/ic_launcher.png"
  },
  "version": "1.2",
  "description": "wunjun.com (วันจันทร์ดอทคอม)สำหรับใช้งาน wunjun.com ได้รวดเร็ว ทันทีทุกที่ทุกเวลา ด้วยระบบ real-time notIfication แจ้งเตือนทันที",
  "author": "wunjun.com",
  "browser_action": {
    "default_title": "Wunjun Notification",
    "default_icon": "images/drawable-hdpi/ic_launcher.png",
    "default_popup": "html/window.html"
  },
  "background": {
    "scripts": [
      "js/config.js",
      "js/function.min.js",
      "js/sockjs.js",
      "js/noti.min.js",
      "eventPage.min.js"
    ],
    "persistent": false
  },
  "permissions": [
    "notifications",
    "http://*.wunjun.com/",
    "cookies"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/drawable-ldpi/ic_launcher.png"
  ]
}
