{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "81.3.gif",
    "default_popup": "popup.html",
    "default_title": "Расширения для игры Mist от Школы Добра"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "https://world.mist-game.ru/"
      ]
    }
  ],
  "description": "Расширения для игры Mist от Школы Добра!",
  "icons": {
    "128": "128.jpg",
    "16": "16.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "name": "Mist-game - расширения от Школы Добра",
  "permissions": [
    "https://world.mist-game.ru/",
    "storage"
  ],
  "version": "1.1.7",
  "web_accessible_resources": [
    "roznica.js",
    "memorys.js"
  ]
}
