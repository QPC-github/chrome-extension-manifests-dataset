{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "name": "The Education District",
  "short_name": "The Education District",
  "version": "0.0.0.22",
  "manifest_version": 2,
  "description": "The Education District",
  "background": {
    "scripts": [
      "js/veport.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.public.theeducationdistrict.com/*",
        "https://*.public.theeducationdistrict.com/*",
        "https://*.cms.theeducationdistrict.com/*",
        "file://*/*"
      ],
      "js": [
        "js/vecontent.js"
      ],
      "run_at": "document_end"
    }
  ]
}
