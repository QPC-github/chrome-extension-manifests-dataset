{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IMDB ERASER",
  "version": "1.0.0",
  "description": "IMDB hides the flyer image and user ratings",
  "manifest_version": 2,
  "icons": {
    "16": "imdb-16.png",
    "48": "imdb-48.png",
    "128": "imdb-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.imdb.com/*"
      ],
      "all_frames": true,
      "js": [
        "imdb.user.js"
      ]
    }
  ]
}
