{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PS All Permissions Whitelist Test",
  "description": "This extension verifies Whitelisting of Extensions in Public session. Has Notifications,tabs(optional) and Bluetooth.",
  "version": "2.1.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "bluetooth": {
    "uuids": [
      "180D",
      "1809",
      "180F"
    ],
    "low_energy": true
  },
  "permissions": [
    "alarms",
    "audioCapture",
    "bookmarks",
    "browser",
    "certificateProvider",
    "clipboardWrite",
    "desktopCapture",
    "enterprise.deviceAttributes",
    "enterprise.platformKeys",
    "idle",
    "management",
    "mdns",
    "notifications",
    "platformKeys",
    "proxy",
    "runtime",
    "serial",
    "socket",
    "storage",
    "tabCapture",
    "tts",
    "unlimitedStorage",
    "usb",
    "usbDevices",
    "videoCapture",
    "vpnProvider",
    "wallpaper",
    "webRequest",
    "webRequestBlocking",
    "webview"
  ],
  "optional_permissions": [
    "tabs",
    "http://www.google.com/",
    "bookmarks"
  ],
  "display_in_launcher": true
}
