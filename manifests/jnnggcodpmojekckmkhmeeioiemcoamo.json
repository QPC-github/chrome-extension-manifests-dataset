{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "author": "Samir <contact@tabuladmcc.com>",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
