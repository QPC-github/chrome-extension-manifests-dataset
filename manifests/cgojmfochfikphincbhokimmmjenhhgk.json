{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Whoer VPN",
  "description": "__MSG_extensionDescription__",
  "version": "1.6",
  "short_name": "Your VPN for browser",
  "default_locale": "en",
  "permissions": [
    "storage",
    "privacy",
    "proxy",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "16": "off.png",
    "48": "off@2.png",
    "128": "off@2.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "off.png"
  },
  "background": {
    "scripts": [
      "static/js/background.js"
    ]
  }
}
