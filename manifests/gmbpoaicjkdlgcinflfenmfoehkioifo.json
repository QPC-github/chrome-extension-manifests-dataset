{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "Purdue Career Account Auto Login",
  "description": "This extension simply presses every login button automatically, saving you lots of trouble",
  "version": "0.1.2",
  "icons": {
    "436": "icons/purdue.png"
  },
  "browser_action": {
    "default_icon": "popup/purdue.png",
    "default_popup": "popup/popup.html",
    "default_title": "purdueAutologin"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.purdue.edu/apps/account/cas/login*",
        "https://mycourses.purdue.edu/*"
      ],
      "include_globs": [
        "https://www.purdue.edu/apps/account/cas/login*",
        "https://mycourses.purdue.edu/",
        "https://mycourses.purdue.edu/webapps/login/*",
        "https://mycourses.purdue.edu/?ticket=*"
      ],
      "run_at": "document_end",
      "js": [
        "jquery-3.1.0.min.js",
        "cracker.js"
      ]
    }
  ],
  "author": "Stool",
  "short_name": "purdueAutologin"
}
