{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tabmaster",
  "description": "Tame your tabs.",
  "short_name": "Tame your tabs.",
  "manifest_version": 2,
  "version": "1.88",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_title": "Tabmaster",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
