{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Quake Radar",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Check for earthquakes near you",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Earthquake?",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://earthquake.usgs.gov/",
    "geolocation",
    "notifications"
  ]
}
