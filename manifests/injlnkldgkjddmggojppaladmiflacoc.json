{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.4",
  "default_locale": "en",
  "author": "Kesantielu Dasefern",
  "homepage_url": "https://kesantielu.dasefern.com",
  "minimum_chrome_version": "49",
  "icons": {
    "16": "kitapdl-16.png",
    "48": "kitapdl-48.png",
    "128": "kitapdl-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "kitapdl-16.png"
    }
  },
  "background": {
    "scripts": [
      "lib/truncate.js",
      "events.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*.kitap.kz/*",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.kitap.kz/*"
      ],
      "js": [
        "main.js"
      ],
      "css": [
        "main.css"
      ]
    }
  ],
  "web_accessible_resources": []
}
