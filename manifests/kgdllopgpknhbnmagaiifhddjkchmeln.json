{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Switcher for Lastpass",
  "description": "Quickly enable/disable the Lastpass Plugin.",
  "version": "1.1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "key.png",
    "32": "key@2x.png",
    "48": "key-48.png",
    "64": "key-64.png",
    "128": "key-128.png"
  },
  "browser_action": {
    "name": "Click to change the icon's color"
  },
  "permissions": [
    "management",
    "notifications"
  ],
  "manifest_version": 2
}
