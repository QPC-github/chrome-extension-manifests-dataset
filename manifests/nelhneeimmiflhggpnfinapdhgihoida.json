{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open Original Images from Kinja Posts",
  "version": "1.6",
  "manifest_version": 2,
  "description": "Opens photos posted to Gawker sites in a new tab at their original size",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "newtab.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}
