{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrapNaija",
  "description": "TrapNaija Serves You With Latest African Songs, Videos, Global News & Entertainment Gist",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "https://trapnaija.com/wp-json/wp/v2/posts",
    "http://trapnaija.com/*",
    "https://trapnaija.com/*",
    "activeTab"
  ]
}
