{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smart A Lot",
  "version": "0.993",
  "manifest_version": 2,
  "description": "Estensione per la comunit√† virtuale Extremelot. Gioca non farti giocare dal gioco.",
  "homepage_url": "http://www.danielebaschieri.eu/smartalot/index.htm",
  "browser_action": {
    "default_icon": "logo48.png",
    "default_popup": "dialog.html"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.extremelot.leonardo.it/*"
      ],
      "js": [
        "script.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "storage"
  ],
  "web_accessible_resources": [
    "super_css.js",
    "jquery-1.10.2.js",
    "jquery-ui-1.10.4.custom.min.js",
    "prova.js"
  ]
}
