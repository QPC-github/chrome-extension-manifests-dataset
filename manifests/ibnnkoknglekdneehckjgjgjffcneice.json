{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scam Stomper",
  "version": "1.0.0.0",
  "manifest_version": 2,
  "description": "Community Based Scam Identification Tool",
  "content_scripts": [
    {
      "matches": [
        "https://*.craigslist.org/*",
        "https://*.craigslist.ca/*"
      ],
      "js": [
        "jquery.js",
        "core-min.js",
        "md5-min.js",
        "lib-typedarrays-min.js",
        "hmac-sha256.min.js",
        "enc-base64.min.js",
        "crypto-js.min.js",
        "scam_stomper.js"
      ]
    }
  ],
  "permissions": [
    "https://*.craigslist.org/*",
    "tabs"
  ]
}
