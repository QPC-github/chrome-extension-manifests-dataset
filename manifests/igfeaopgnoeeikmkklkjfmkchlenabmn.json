{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Florida Uncensorizer",
  "description": "Remind yourself about stuff Gov. Rick Scott would prefer you forget wherever the word 'Florida' appears on the web.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "icons": {
    "16": "icon_016.png",
    "48": "icon_048.png",
    "128": "icon_128.png"
  }
}
