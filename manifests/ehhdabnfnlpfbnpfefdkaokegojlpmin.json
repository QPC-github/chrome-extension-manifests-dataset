{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Likey",
  "description": "Quickly see which Facebook posts you've liked, which ones you haven't, and which ones are ads.",
  "version": "1.1",
  "short_name": "See which Facebook posts you've already liked and which ones you haven't",
  "homepage_url": "http://tech-in-check.blogspot.com/",
  "icons": {
    "16": "likey16.png",
    "48": "likey48.png",
    "128": "likey128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "likey19.png",
      "38": "likey38.png"
    },
    "default_title": "Likey"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
