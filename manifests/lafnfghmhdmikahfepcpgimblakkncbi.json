{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Your stickers",
  "description": "Ваши любимые изображения всегда под рукой",
  "version": "0.45",
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "icon/48.png",
    "128": "icon/128.png",
    "256": "icon/256.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Your stickers",
    "default_icon": "icon/48.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
