{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Adysis AdReporter",
  "description": "Adysis advertisements reporting tool.",
  "homepage_url": "http://adysis.com",
  "version": "0.3",
  "icons": {
    "16": "/icons/icon_16.png",
    "48": "/icons/icon_48.png",
    "128": "/icons/icon_128.png"
  },
  "browser_action": {
    "default_popup": "/popup/popup.html",
    "default_icon": {
      "19": "/icons/action_19_off.png",
      "38": "/icons/action_38_off.png"
    }
  },
  "background": {
    "scripts": [
      "/eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}
