{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "multitube",
  "description": "Stay tuned. Stay productive.",
  "version": "2.0.5",
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "128": "icons/icon-128.png",
    "16": "icons/icon-16.png",
    "256": "icons/icon-256.png",
    "32": "icons/icon-32.png",
    "512": "icons/icon-512.png",
    "64": "icons/icon-64.png"
  },
  "permissions": [
    "webview",
    "alwaysOnTopWindows",
    "storage",
    "<all_urls>",
    "fullscreen",
    "identity"
  ],
  "oauth2": {
    "client_id": "915668084552-47382501gkr4c0v0kqhsoa4rrhqo4ssr.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
