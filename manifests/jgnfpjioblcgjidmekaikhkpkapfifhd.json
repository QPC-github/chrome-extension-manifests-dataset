{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google AMP Test API Extension",
  "short_name": "Google AMP Test API Extension",
  "description": "Google AMP Test -Is your AMP page valid?",
  "version": "1.1",
  "author": "Eliyas Naswale",
  "version_name": "Production v1.1",
  "icons": {
    "128": "Images/icon_128.png",
    "48": "Images/icon_48.png",
    "16": "Images/icon_16.png"
  },
  "browser_action": {
    "default_icon": "Images/icon_128.png",
    "default_popup": "popup.html",
    "default_title": "Google AMP Test API Extension"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
