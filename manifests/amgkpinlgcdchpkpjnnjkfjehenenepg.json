{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Delads.TV",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Extension to add videos to Delads.TV",
  "homepage_url": "http://www.delads.tv",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": {
      "19": "icons/icon.gif"
    },
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "http://www.youtube.com/*",
    "tabs"
  ]
}
