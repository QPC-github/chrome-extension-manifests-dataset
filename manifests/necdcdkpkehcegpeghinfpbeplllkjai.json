{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pass! Secure Password Generator 1.0",
  "description": "This extension will add secure password generator to Google Chrome",
  "version": "1.0",
  "short_name": "P! Sec Pwd",
  "background": {
    "page": "Pass!.html"
  },
  "browser_action": {
    "default_icon": "Pass!128.png",
    "default_popup": "Pass!.html"
  },
  "permissions": [
    "notifications",
    "storage"
  ]
}
