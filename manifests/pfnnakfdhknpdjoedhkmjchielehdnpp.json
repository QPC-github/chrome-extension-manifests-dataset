{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Concourse 360 Beta",
  "version": "0.20",
  "description": "Concourse 360's Extension for automatically filling out BPOs",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Concourse 360",
    "default_popup": "pop.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "128icon.png"
  }
}
