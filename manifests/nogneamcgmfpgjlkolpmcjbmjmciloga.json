{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Presse gratuite",
  "description": "Supprime les limites de lecture sur certains sites de presse",
  "version": "1.2",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "backgroundscript.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "logo-160.png"
  }
}
