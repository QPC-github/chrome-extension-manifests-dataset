{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MathJax for Slack",
  "description": "This extension adds MathJax functionality for Slack",
  "version": "1",
  "web_accessible_resources": [],
  "content_scripts": [
    {
      "matches": [
        "*://*.slack.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "32": "slack.png",
    "64": "slack.png",
    "128": "slack.png"
  },
  "browser_action": {
    "default_icon": "slack.png"
  }
}
