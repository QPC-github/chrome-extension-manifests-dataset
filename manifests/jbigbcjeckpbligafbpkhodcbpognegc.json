{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MTV’s Jerzdays",
  "short_name": "Jerzdays",
  "description": "Jersey Shore Family Vacation is BACK with new episodes on MTV every Thursday… we mean every Jerzday!",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
