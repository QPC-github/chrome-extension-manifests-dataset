{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "jwt Clipboard Paster",
  "short_name": "jwt Paster",
  "version": "0.2",
  "description": "Opens jwt.io with the content of the clipboard in the token box!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "128": "images/jwtPaster.128.png",
      "48": "images/jwtPaster.48.png",
      "32": "images/jwtPaster.32.png",
      "16": "images/jwtPaster.16.png"
    }
  },
  "permissions": [
    "clipboardRead",
    "tabs"
  ],
  "manifest_version": 2,
  "icons": {
    "128": "images/jwtPaster.128.png",
    "48": "images/jwtPaster.48.png",
    "32": "images/jwtPaster.32.png",
    "16": "images/jwtPaster.16.png"
  }
}
