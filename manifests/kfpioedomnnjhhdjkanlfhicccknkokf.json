{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "BGG quick record information button",
  "version": "0.1.5",
  "description": "This module provides a quick registration button to record item information quickly.",
  "manifest_version": 2,
  "permissions": [
    "http://boardgamegeek.com/boardgame/*",
    "http://www.boardgamegeek.com/boardgame/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.boardgamegeek.com/boardgame/*",
        "http://boardgamegeek.com/boardgame/*"
      ],
      "css": [
        "css/bootstrap.min.css",
        "css/content.css"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/bootstrap.js",
        "js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/plus48x48.png"
  ]
}
