{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ElliotForWater",
  "description": "Il motore di ricerca che salva il pianeta",
  "icons": {
    "16": "goccia16.png",
    "48": "goccia48.png",
    "128": "icon128.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.elliotforwater.com",
    "search_provider": {
      "name": "name.__MSG_url_domain__",
      "keyword": "keyword.__MSG_url_domain__",
      "search_url": "http://www.elliotforwater.com/search?q={searchTerms}",
      "favicon_url": "http://www.elliotforwater.com/favicon.ico",
      "image_url": "http://www.elliotforwater.com/search?q={searchTerms}&type=image",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://www.elliotforwater.com"
    ]
  },
  "version": "1.0.0.0",
  "minimum_chrome_version": "9",
  "manifest_version": 2
}
