{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "A simple extension that allows you to stream your desktop into meetings with Jitsi Meet and Jitsi Videobridge.",
  "externally_connectable": {
    "matches": [
      "*://webmeet.kreonet.net/*",
      "*://webmeet.kreonet.net/*/*",
      "*://webmeet.kreonet.net/*/*/*"
    ]
  },
  "icons": {
    "128": "jtisi-logo-128x128.png",
    "16": "jtisi-logo-16x16.png",
    "48": "jtisi-logo-48x48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Jitsi Desktop Streamer",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.5.3"
}
