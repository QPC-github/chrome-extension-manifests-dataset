{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GUC Thesis Helper",
  "version": "1.1.2",
  "description": "Get some help dealing with the worst possible prioritization system",
  "manifest_version": 2,
  "permissions": [
    "http://student.guc.edu.eg/External/Thesis/ChooseThesis.aspx"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://student.guc.edu.eg/External/Thesis/ChooseThesis.aspx"
      ],
      "js": [
        "src/js/vendor/sortable.js",
        "src/js/constants.js",
        "src/js/dom-manipulation.js",
        "src/js/main.js",
        "src/js/request-formatter.js",
        "src/js/inject.js",
        "src/js/spinner.js"
      ],
      "css": [
        "src/css/vendor/sortable.css",
        "src/css/spinner.css",
        "src/css/button.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}
