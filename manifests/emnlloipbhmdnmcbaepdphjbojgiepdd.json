{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dribbble GIF thumbnails",
  "description": "This extension replaces gif thumbnails with the original gifs",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://dribbble.com/*"
      ],
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
