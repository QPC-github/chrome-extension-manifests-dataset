{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tabFund.me ",
  "version": "0.1.7",
  "manifest_version": 2,
  "description": "Each time you open new tab you donate money to your chosen community.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_48.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
