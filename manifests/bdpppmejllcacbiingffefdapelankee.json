{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PACifier",
  "description": "Allows easy configuration of your proxy settings on a per-site basis",
  "version": "1.1",
  "permissions": [
    "tabs",
    "proxy"
  ],
  "background": {
    "scripts": [
      "event_page.js"
    ]
  },
  "page_action": {
    "default_icon": "pacifier-off.png",
    "default_title": "PACifier"
  },
  "icons": {
    "128": "pacifier-on.png"
  },
  "manifest_version": 2
}
