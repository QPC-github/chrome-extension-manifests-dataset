{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Claudio Sender",
  "version": "1.3",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "tabs",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "/js/jquery.js",
      "/js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.htm"
  },
  "web_accessible_resources": [
    "/*"
  ]
}
