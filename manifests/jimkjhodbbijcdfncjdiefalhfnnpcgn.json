{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "eco1",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "ニコニコ動画の動画ページURL末尾に『?eco=1』を追加して読み込みなおします。",
  "icons": {
    "16": "./img/eco1_16.png",
    "48": "./img/ECO1_48.png",
    "128": "./img/ECO1_128.png"
  },
  "browser_action": {
    "default_icon": "./img/eco1_16.png",
    "default_title": "eco1"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://www.nicovideo.jp/watch/*"
  ]
}
