{
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_title": "违章查询",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "jQuery.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "zh_CN",
