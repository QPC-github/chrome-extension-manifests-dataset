{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Roboboogie's Optimizely Events Tracker",
  "version": "0.0.1.1",
  "description": "Roboboogie's Optimizely Events Tracker",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Roboboogie's Optimizely Events Tracker"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
