{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "圖書館多重帳戶登入",
  "version": "3.84",
  "manifest_version": 2,
  "description": "如果你有多張台灣主要城市的圖書館證時，本程式可以幫助你登入所有的帳戶，檢視全部的借書狀況。",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "圖書館多重帳戶登入",
    "default_popup": "baloon.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
