{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BuzzSumo",
  "version": "1.3.6",
  "manifest_version": 2,
  "description": "Get share counts & more from BuzzSumo",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "20": "images/icon-20.png",
      "40": "images/icon-40.png"
    },
    "default_title": "BuzzSumo",
    "default_popup": "popup.html"
  },
  "externally_connectable": {
    "matches": []
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://app.buzzsumo.com/"
  ],
  "content_security_policy": "script-src 'self' https://*.buzzsumo.com; object-src 'self'",
  "web_accessible_resources": [
    "http://fonts.googleapis.com/"
  ]
}
