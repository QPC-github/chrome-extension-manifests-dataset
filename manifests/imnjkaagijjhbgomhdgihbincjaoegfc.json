{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bungadoo",
  "version": "17.0.3",
  "description": "Productivity  Extension",
  "icons": {
    "48": "images/icon-new.png",
    "96": "images/icon-new.png"
  },
  "chrome_url_overrides": {
    "newtab": "html/index.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_area": "navbar",
    "default_icon": "images/icon-new.png",
    "default_title": "__MSG_settings_title__"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Shift+Alt+B"
      }
    }
  },
  "omnibox": {
    "keyword": "newtab"
  },
  "permissions": [
    "cookies",
    "history",
    "storage",
    "tabs"
  ],
  "optional_permissions": [
    "https://bungadoo.com/*"
  ],
  "default_locale": "en",
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}
