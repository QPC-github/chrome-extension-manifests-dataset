{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gram Rank",
  "version": "4.6.2",
  "description": "An extension to find and download top content off the gram",
  "permissions": [
    "activeTab",
    "storage",
    "downloads",
    "https://eumetric.com/*",
    "https://www.instagram.com/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "licencing.js",
      "jquery-3.3.1.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/GramRank128.png"
    }
  },
  "icons": {
    "128": "images/GramRank128.png"
  },
  "web_accessible_resources": [
    "images/download.png"
  ],
  "manifest_version": 2
}
