{
  "background": {
    "persistent": true,
    "scripts": [
      "js/vendor/angular.1.0.8.min.js",
      "js/controllers/tickerCtrl.js"
    ]
  },
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "38": "icons/38.png",
    "64": "icons/64.png"
  },
  "browser_action": {
    "default_icon": "icons/64.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Worldcoin price ticker.",
  "manifest_version": 2,
  "name": "Worldcoin Ticker",
  "permissions": [
    "http://api.btc38.com/v1/ticker.php?c=wdc&mk_type=btc",
    "https://blockchain.info/ro/ticker",
    "http://blockchain.info/ro/ticker"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.5.5"
}
