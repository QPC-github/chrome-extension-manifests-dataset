{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MarkUp Page Creator",
  "short_name": "MarkUp",
  "description": "This chrome extension allows you to generate MarkUp Pages straight from your browser",
  "version": "1.0.11",
  "minimum_chrome_version": "10.0",
  "devtools_page": "devtools.html",
  "permissions": [
    "<all_urls>",
    "debugger",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "be-logo-16.png",
    "32": "be-logo-32.png",
    "48": "be-logo-48.png",
    "128": "be-logo-128.png"
  },
  "manifest_version": 2
}
