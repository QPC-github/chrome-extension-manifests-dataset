{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GMPE - Sistema Gestão e CRM para empresas",
  "short_name": "GMPE - Sistema Gestão",
  "version": "1.0.8",
  "author": "Youzoom Soluções Web",
  "minimum_chrome_version": "23",
  "description": "GMPE é um sistema de gestão de empresas totalmente online e integrado. Tenha um CRM completo para sua empresa.",
  "homepage_url": "https://gmpe.com.br",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "cookies",
    "notifications",
    "storage",
    "https://gmpe.com.br/"
  ],
  "browser_action": {
    "default_icon": "19.png",
    "default_title": "Abrir GMPE"
  },
  "icons": {
    "128": "128.png",
    "19": "19.png"
  }
}
