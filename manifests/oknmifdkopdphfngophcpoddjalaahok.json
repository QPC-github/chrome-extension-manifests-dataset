{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "dmm-cookie-neatrabbit",
  "description": "DMM 쿠키 조작기 by NeatRabbit.",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.dmm.com/",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "img/icon_128x128.png"
  },
  "web_accessible_resources": [
    "img/icon_128x128.png"
  ]
}
