{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Earthworm Ronnie",
  "description": "A World Country Quiz",
  "version": "0.1.4",
  "manifest_version": 2,
  "default_locale": "en",
  "permissions": [
    "app.runtime",
    "app.window",
    "storage",
    "system.storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "scripts/background.js"
      ]
    }
  }
}
