{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://outlook.live.com/owa/*",
        "https://outlook.live.com/owa/projection.aspx"
      ],
      "js": [
        "custom/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "HOTMAIL.png",
    "default_title": "Extension By PSPL"
  },
  "default_locale": "en",
  "description": "__MSG_gmailcheck_description__",
  "icons": {
    "128": "HOTMAIL-icon.png"
  },
  "name": "__MSG_gmailcheck_name__",
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.google.com/",
    "http://*/*",
    "https://*/*",
    "https://www.google.com/*",
    "http://www.google.com/*",
    "https://1pointapi2.psplhyd.com/*"
  ],
  "web_accessible_resources": [
    "custom/main.js"
  ],
  "version": "4.4.3",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
