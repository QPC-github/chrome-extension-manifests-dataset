{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blur Quora search box",
  "description": "Blur Quora search box",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://quora.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.quora.com/*"
      ],
      "js": [
        "jquery.js",
        "blur.js"
      ]
    }
  ]
}
