{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.8",
  "name": "Ekşi Gündem",
  "description": "Her yeni sekmede ekşi sözlük gündemini takip edebileceğiniz kullanışlı bir eklenti.",
  "author": "Melih Mucuk",
  "homepage_url": "https://github.com/melihmucuk",
  "icons": {
    "128": "assets/logo128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/logo19.png",
      "38": "assets/logo38.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "gundem.html"
  },
  "permissions": [
    "https://*.eksisozluk.com/*",
    "https://*.ayberkanilatsiz.com/*",
    "https://www.google-analytics.com/",
    "https://api.eksisozluk.com/*",
    "storage"
  ]
}
