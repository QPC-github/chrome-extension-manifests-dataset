{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "URL2QR",
  "description": "Show the QR code of the current tab's url.",
  "version": "1.0.0",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "96": "icon_96.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Show QR code",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
