{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BookDepository.cheap",
  "description": "This extension helps you save money on BookDepository.com by finding the best price from multiple countries.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "author": "Benjamin Tan Wei Hao",
  "homepage_url": "http://www.bookdepository.cheap",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "content.js"
      ]
    }
  ]
}
