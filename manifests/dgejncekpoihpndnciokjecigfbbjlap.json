{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Open",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Quickly open popular websites with the click of a button.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "running.png",
    "default_title": "Quick Open"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "90": "bing.png",
    "128": "running.png",
    "60": "google logo.png"
  }
}
