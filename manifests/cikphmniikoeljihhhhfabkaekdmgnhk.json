{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery-1.10.2.js",
        "js/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Remplace toutes les images par une image de Rascar Capac.",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "RascarCapac",
  "version": "0.1.1",
  "web_accessible_resources": [
    "js/jquery.js",
    "img/*"
  ],
  "permissions": [
    "activeTab"
  ]
}
