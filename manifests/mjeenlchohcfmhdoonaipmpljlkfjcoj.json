{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ROBOhawk Verifier DOM",
  "manifest_version": 2,
  "version": "1.6",
  "description": "Manipulate the DOM when the page is done loading",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.2.1.min.js",
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
