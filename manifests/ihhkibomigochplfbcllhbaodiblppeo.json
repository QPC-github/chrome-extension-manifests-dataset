{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CreativeBoots",
  "description": "This extension is for the CB family so they can contact CB easily",
  "version": "1.0",
  "browser_action": {
    "default_title": "CreativeBoots Help Me",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*"
  ]
}
