{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Website Dimmer",
  "description": "NIGHT MODE FOR ALL WEBSITES. It darkens backgrounds of html based webpages.",
  "version": "0.660",
  "author": "no9org@gmail.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*/*"
      ],
      "js": [
        "WDScript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "file:///*/*"
  ],
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Website Dimmer"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "96": "icon96.png",
    "128": "icon128.png"
  }
}
