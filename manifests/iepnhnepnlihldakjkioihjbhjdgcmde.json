{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "New On Centre Version 3.3"
  },
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/jquery.js",
        "js/script.js",
        "js/jquery.ba-resize.js"
      ],
      "matches": [
        "http://www.google.com/streetview/businesses/publish/pano_moderator*",
        "https://www.google.com/streetview/businesses/publish/pano_moderator*"
      ]
    }
  ],
  "description": "Google Trusted Photographer Moderation Tool Kit. A transition alignment ruler plus grid overlay.",
  "homepage_url": "http://www.aroundyourbusiness.co.uk/on-centre-plus",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "New On Centre",
  "options_page": "options.html",
  "permissions": [
    "http://www.google.com/streetview/businesses/publish/pano_moderator*",
    "https://www.google.com/streetview/businesses/publish/pano_moderator*",
    "storage"
  ],
  "version": "3.3"
}
