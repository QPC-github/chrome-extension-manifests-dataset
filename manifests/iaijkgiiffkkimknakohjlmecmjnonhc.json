{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MROSupply Search",
  "version": "1.0.1",
  "description": "This extension allows you to search selected text on MROSupply website.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
