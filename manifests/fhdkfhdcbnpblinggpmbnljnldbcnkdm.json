{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "name": "Закладки : a.te.ua",
  "version": "1.0.42",
  "description": "Стартова сторінка закладок. Закладки для Хром. ",
  "permissions": [
    "http://a.te.ua/*",
    "tabs",
    "activeTab",
    "bookmarks"
  ],
  "page_action": {
    "default_icon": {
      "19": "images/za19.png"
    },
    "default_title": "Додати в закладки.",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
