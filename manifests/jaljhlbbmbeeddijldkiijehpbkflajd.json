{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "32": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "exclude_globs": [
        ""
      ],
      "include_globs": [
        "*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://secure.globiflow.com/*",
        "*://secure.globiflow.com/*"
      ]
    }
  ],
  "description": "Globiflow Night Mode",
  "name": "Globiflow Night Mode",
  "short_name": "Globiflow Night Mode",
  "version": "1.1"
}
