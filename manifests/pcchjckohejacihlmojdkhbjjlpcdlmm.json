{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "~UnderlinedBing",
  "description": "Remove underlines from Bing search URLs",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.bing.com/search*"
      ],
      "js": [
        "removeUnderline.js"
      ]
    }
  ]
}
