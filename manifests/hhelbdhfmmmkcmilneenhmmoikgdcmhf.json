{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Finger circle",
  "manifest_version": 2,
  "version": "1.0.1",
  "short_name": "Finger circle",
  "description": "Prank your friends with this extension!",
  "browser_action": {
    "default_icon": "/images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "fingerprank.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*.jpg"
  ]
}
