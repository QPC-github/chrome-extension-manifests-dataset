{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Current",
  "version": "1.1.6",
  "description": "Bring a microdose of mindfulness into the flow of your work day with every new tab",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {},
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
