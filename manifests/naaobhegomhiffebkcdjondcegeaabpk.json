{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Id Finder Free",
  "short_name": "IFF",
  "version": "0.3",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/*",
        "*://*.smmfok.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
