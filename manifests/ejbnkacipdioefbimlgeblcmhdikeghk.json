{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "On The Bench",
  "short_name": "On The Bench",
  "description": "On The Bench is a platform that provides you with the easy access to the best HubSpot talent and solutions.",
  "version": "1.3.2",
  "manifest_version": 2,
  "author": "On The Bench",
  "permissions": [
    "tabs",
    "https://onthebench.io/api/get-recipes"
  ],
  "icons": {
    "16": "assets/images/otb-favicon.png",
    "48": "assets/images/otb-favicon.png",
    "128": "assets/images/otb-favicon.png"
  },
  "browser_action": {
    "default_icon": "assets/images/otb-favicon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "assets/js/background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
