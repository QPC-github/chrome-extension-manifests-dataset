{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Isradown",
  "description": "This extension sets your homepage to Isradown",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "management"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ],
  "chrome_settings_overrides": {
    "homepage": "http://isradown.info/",
    "startup_pages": [
      "http://isradown.info/"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://isradown.info/*"
    ]
  }
}
