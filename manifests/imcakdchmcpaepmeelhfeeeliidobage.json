{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "48": "block.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "name": "Link disliker",
  "description": "Dislike stupid links",
  "version": "1.2",
  "author": "kaleem ullah khan",
  "permissions": [
    "storage",
    "activeTab",
    "webNavigation",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  }
}
