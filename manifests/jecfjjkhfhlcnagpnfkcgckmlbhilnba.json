{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick View docs",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Quick view MS Office file (doc, excel, powerpoint). Need to install extension Office Editing for Docs, Sheets & Slides",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "downloads",
    "downloads.open"
  ]
}
