{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fan-Stats-y",
  "description": "Use this extension on nfl.com to pick your optimal fantasy lineup.",
  "version": "1.1.1",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "js_functions/*"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/Fan-stats-y_Logo_16x16.png",
      "32": "images/Fan-stats-y_Logo_32x32.png",
      "48": "images/Fan-stats-y_Logo_48x48.png",
      "128": "images/Fan-stats-y_Logo_128x128.png"
    }
  }
}
