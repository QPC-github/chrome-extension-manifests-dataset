{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Catalystify Libguides Books",
  "description": "Allows Bridge librarians to input a URL and then copy resulting code to the clipboard.",
  "author": "Iris Jastram",
  "version": "1.2",
  "icons": {
    "22": "penguiknight_icon.png",
    "16": "popout_icon_white.png",
    "17": "popout_icon_black.png"
  },
  "web_accessible_resources": [
    "popup.html",
    "popup2.html",
    "popup.css",
    "popup.js",
    "popout_icon_black.png",
    "popout_icon_white.png",
    "penguiknight_icon.png"
  ],
  "browser_action": {
    "default_title": "Catalystify Libguides Books",
    "default_icon": "penguiknight_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
