{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NewTab Dota2",
  "version": "3.0.0.1",
  "icons": {
    "128": "images/logo-128.png",
    "16": "images/logo-16.png"
  },
  "description": "Thay thế NewTab nhàm chán bằng Video Trailer bạn muốn !!",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    "videos/*.mp4"
  ],
  "permissions": [
    "activeTab"
  ]
}
