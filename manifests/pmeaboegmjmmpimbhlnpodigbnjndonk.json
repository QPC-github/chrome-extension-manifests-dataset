{
  "name": "Simple History Cleaner",
  "short_name": "SH Cleaner",
  "description": "Clear browser history. Simple. Automatically. Constantly.",
  "version": "0.12",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "/i/16.png",
      "32": "/i/on32.png"
    }
  },
  "icons": {
    "16": "/i/16.png",
    "32": "/i/on32.png",
    "48": "/i/48.png",
    "128": "/i/128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "browsingData"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
