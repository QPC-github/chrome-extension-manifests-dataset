{
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.09",
  "name": "PinRSS Feed Finder",
  "description": "Pinrss Feed Finder helps to discover RSS Feeds youâ€™ll like around the web easily.",
  "homepage_url": "https://www.pinrss.com",
  "icons": {
    "128": "m/icon128.png",
    "48": "m/icon48.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "j/find_feed.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "popup.html",
    "pinrss.html"
  ],
  "browser_action": {
    "default_icon": "m/icon.png",
    "default_title": "PinRSS Feed Finder"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "tabs",
    "cookies",
    "history",
    "storage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "*://*/*"
  ],
  "short_name": "PinRSS",
  "author": "pinrss.com"
}
