{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "xkcd Reader",
  "description": "Read the last xkcd comic.",
  "version": "0.1",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://www.xkcd.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
