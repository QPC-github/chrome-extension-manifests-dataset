{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sorensen tobakk reveal images",
  "version": "0.4.0",
  "description": "Reveals images at Sorensen tobakk website",
  "content_scripts": [
    {
      "css": [
        "style-override.css"
      ],
      "matches": [
        "http://www.msorensen.no/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "sigarcom.js"
      ],
      "matches": [
        "*://*.sigar.com/*",
        "*://sigar.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
