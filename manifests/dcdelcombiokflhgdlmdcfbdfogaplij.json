{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "count.js"
      ],
      "matches": [
        "https://plus.google.com/*"
      ]
    }
  ],
  "description": "Adds notification count to the document title in the page tab.",
  "manifest_version": 2,
  "name": "G+ Count in Title",
  "version": "1.3"
}
