{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Assemblée nationale",
  "version": "0.0.4",
  "description": "Texte en cours d'examen à l'Assemblée nationale.",
  "icons": {
    "48": "icons/logo_an_square_48.png",
    "128": "icons/logo_an_square_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/logo_an_square_128.png",
    "default_title": "Assemblée nationale",
    "default_popup": "popup/display_elements.html",
    "browser_style": true
  },
  "permissions": [
    "alarms",
    "http://eliasse.assemblee-nationale.fr/"
  ]
}
