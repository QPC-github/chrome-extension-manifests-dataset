{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wire Screen Sharing",
  "author": "SciMedMedia",
  "version": "3.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension for Wire Screen Sharing. It doesn't do anything except capture content of your screen.",
  "homepage_url": "http://tokbox.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.smm.nyc/*",
      "http://localhost:4764/*",
      "https://yuriy.blueprint.com/*",
      "https://admin.gobluemeeting.com/*",
      "*://*.gobluemeeting.com/*",
      "*://blueprint.scimeddev.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
