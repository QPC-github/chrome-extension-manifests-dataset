{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Kitten Placer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Replaces all images with ones from placekitten.com",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "kitten-placer.js"
      ]
    }
  ]
}
