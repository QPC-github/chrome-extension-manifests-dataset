{
  "name": "Tabs manager by domain",
  "description": "Tool for mamaging tabs in browser",
  "version": "0.2.9",
  "manifest_version": 2,
  "icons": {
    "48": "icons/internet-48.png",
    "128": "icons/internet-128.png"
  },
  "browser_action": {
    "default_title": "Tabs manager by domain",
    "default_icon": "icons/internet-32.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "history"
  ],
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx"
}
