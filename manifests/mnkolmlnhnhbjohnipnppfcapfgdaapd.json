{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Snack",
  "version": "1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle feature foo"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Q",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    },
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "windows": "Alt+Shift+P",
        "mac": "Alt+Shift+P"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://snack-start.com/*"
      ],
      "js": [
        "content.js",
        "Jquery.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "popup.js",
      "Jquery.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus"
  ]
}
