{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DOM Monster",
  "description": "DOM Monster is a bookmarklet that will analyze the DOM & other features of the page you're on, and give you its bill of health.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
