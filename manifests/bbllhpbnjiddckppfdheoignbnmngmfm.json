{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CEDAR OnDemand",
  "description": "CEDAR OnDemand: a browser extension to generate ontology-based scientific metadata",
  "version": "2.1",
  "permissions": [
    "activeTab",
    "https://bioportal.bioontology.org/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CEDAR OnDemand"
  },
  "web_accessible_resources": [
    "selectize.default.css",
    "tingle.css",
    "style.css",
    "crossdomain_autocomplete.js",
    "cod_complete.js"
  ],
  "manifest_version": 2
}
