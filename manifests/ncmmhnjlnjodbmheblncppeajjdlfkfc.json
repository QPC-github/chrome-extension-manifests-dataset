{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Flashlight",
  "description": "A free flashlight for your screen",
  "version": "0.0.0.5",
  "offline_enabled": false,
  "app": {
    "urls": [
      "http://freeflashlight.org/"
    ],
    "launch": {
      "web_url": "http://freeflashlight.org/"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
