{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vaux Tab",
  "short_name": "Vaux Tab",
  "description": "Vaux Tab Manager and Window Organizer",
  "version": "1.9.4.11",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://cdn.ravenjs.com; object-src 'self'",
  "icons": {
    "16": "vaux_logo.png",
    "38": "vaux_logo.png",
    "48": "vaux_logo.png",
    "96": "vaux_logo.png",
    "128": "vaux_logo.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "unlimitedStorage",
    "clipboardWrite"
  ],
  "background": {
    "page": "bg_empty.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "View windows",
    "default_popup": "window_manager.html",
    "default_icon": {
      "16": "vaux_128.png",
      "38": "vaux_128.png",
      "48": "vaux_128.png",
      "96": "vaux_128.png",
      "128": "vaux_128.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "manifest_version": 2
}
