{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quản lý đào tạo PTIT",
  "short_name": "QLDT PTIT",
  "description": "Tính năng nâng cao cho hệ thống QLDT PTIT. Cung cấp bởi Tôi Yêu PTIT.",
  "version": "1.0.7",
  "content_scripts": [
    {
      "matches": [
        "http://qldt.ptit.edu.vn/*"
      ],
      "js": [
        "jquery.js",
        "ptitvl.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://qldt.ptit.edu.vn/Default.aspx?page=lichthi*"
      ],
      "js": [
        "jquery.js",
        "tiennv.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://qldt.ptit.edu.vn/Default.aspx?page=xemhocphi*"
      ],
      "js": [
        "jquery.js",
        "hocphi.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://qldt.ptit.edu.vn/Default.aspx?page=dkmonhoc*",
        "http://qldt.ptit.edu.vn/default.aspx?page=dkmonhoc*"
      ],
      "js": [
        "jquery.js",
        "auto_reg.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://qldt.ptit.edu.vn/Default.aspx?page=xemdiemthi*",
        "http://qldt.ptit.edu.vn/default.aspx?page=xemdiemthi*"
      ],
      "js": [
        "jquery.js",
        "diemthi.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "storage",
    "http://qldt.ptit.edu.vn/*",
    "http://*.toiyeuptit.com/*",
    "http://*.tiennv.com/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "jquery.js",
      "back_bg.js"
    ]
  },
  "icons": {
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "options.html?type=chrome_style",
    "chrome_style": true
  }
}
