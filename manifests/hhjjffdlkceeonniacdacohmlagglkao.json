{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump to Harambe",
  "description": "Replaces all instances of Donald Trump / Trump with His name, the legend Harambe.",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
