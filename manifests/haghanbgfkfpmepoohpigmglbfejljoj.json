{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CircuitLab",
  "version": "0.0.0.8",
  "manifest_version": 2,
  "description": "Schematic editor and circuit simulator.",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "urls": [
      "https://*.circuitlab.com/chromeapp/",
      "https://*.circuitlab.com/editor/"
    ],
    "launch": {
      "web_url": "https://www.circuitlab.com/chromeapp/launcher/"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ]
}
