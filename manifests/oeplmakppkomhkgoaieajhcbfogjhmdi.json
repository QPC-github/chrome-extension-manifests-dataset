{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "noAutoScroll.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Stops auto scroll (aka smooth scroll) from activating on middle mouse click.",
  "icons": {
    "128": "128x128.png",
    "16": "16x16.png",
    "48": "48x48.png"
  },
  "manifest_version": 2,
  "name": "No Auto Scroll",
  "version": "1.0"
}
