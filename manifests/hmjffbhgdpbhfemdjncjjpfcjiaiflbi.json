{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Instant Page",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Çağdaş Dağ",
  "description": "instant.page Chrome Extension",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
