{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OA Date Converter",
  "short_name": "OADateConverter",
  "description": "Markit OA Date Converter",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "assets/icon-ao.png",
    "128": "assets/icon-ao.png"
  },
  "browser_action": {
    "default_icon": "assets/icon-ao.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
