{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "content/style.css"
      ],
      "js": [
        "content/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "default_locale": "zh_CN",
  "description": "帮助您在开发者工具面板里，轻松地提取，编辑和分析XPath",
  "devtools_page": "devtools.html",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/16.png",
      "38": "icons/48.png"
    }
  },
  "manifest_version": 2,
  "name": "XPath查询工具",
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "enable.js"
  ],
  "version": "1.1.3"
}
