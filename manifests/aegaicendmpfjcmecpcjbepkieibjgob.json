{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Include Visual",
  "description": "Extensão auxiliar para baixa visão",
  "version": "5.2.1",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Leia bem com Include Visual",
    "default_icon": "icon-ext.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "256": "icon-ext.png"
  }
}
