{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Service posting messages",
  "version": "1.1.4",
  "description": "Автоматическая рассылка сообщений для сайта знакомств VictoriyaClub.com",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://victoriyaclub.com/newchat/"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "https://delivery.h5-game.ru/"
  ],
  "web_accessible_resources": [
    "app.js"
  ]
}
