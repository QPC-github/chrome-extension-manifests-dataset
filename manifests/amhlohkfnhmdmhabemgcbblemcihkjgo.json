{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "whoischeck.js"
    ]
  },
  "name": "Whois One-Click WHOIS search",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Perform WHOIS lookups directly from the navigation bar on any domain that you are currently visiting.",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
