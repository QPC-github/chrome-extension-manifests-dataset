{
  "background": {
    "scripts": [
      "js/utils/tld.js",
      "js/common.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png",
    "128": "img/128.png",
    "256": "img/256.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "short_name": "__MSG_appShortName__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2"
}
