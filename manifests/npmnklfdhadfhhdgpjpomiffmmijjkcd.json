{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IP",
  "description": "Displays public IP",
  "version": "1.0.1",
  "permissions": [
    "http://jsonip.appspot.com/"
  ],
  "web_accessible_resources": [
    "css/index.css",
    "lib/jquery-1.9.1.min.js",
    "js/ip_info_extractor.js"
  ],
  "content_security_policy": "script-src 'self' https://smart-ip.net; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/index.html"
  }
}
