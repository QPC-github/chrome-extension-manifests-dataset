{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ali Search",
  "description": "Search Ali on the go!",
  "version": "1.0",
  "icons": {
    "128": "ali96.png",
    "48": "ali48.png",
    "24": "ali24.png"
  },
  "browser_action": {
    "default_icon": "ali24.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
