{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SustituCAT",
  "version": "1.5",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "¡Elimina el independentismo catalán de tu navegador!",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "stfu.js"
      ],
      "run_at": "document_end"
    }
  ]
}
