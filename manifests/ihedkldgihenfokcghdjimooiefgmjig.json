{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Favicon Fixer for Gmail",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Replace Gmail (or Inbox) favicon with favicon from mail domain.",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "fix-icon.js"
      ]
    }
  ]
}
