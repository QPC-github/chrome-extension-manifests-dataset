{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "twitthat",
  "description": "Share webpage to twitter easily.",
  "version": "1.3",
  "permissions": [
    "http://twitthat.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
