{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "findol - beautiful & efficient New Tab",
  "short_name": "findol",
  "version": "1.0.6",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "cookies",
    "downloads"
  ],
  "browser_action": {
    "default_title": "ADD page to findol",
    "default_icon": "/icons/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "nfind_newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2,
  "default_locale": "en"
}
