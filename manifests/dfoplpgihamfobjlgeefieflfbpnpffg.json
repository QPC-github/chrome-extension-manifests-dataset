{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Who.is Lookup",
  "version": "1.0",
  "description": "Domain data at your fingertips. Provided by Name.com. Coded by @StayUber.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Who.is Lookup"
  }
}
