{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open SAP Note",
  "description": "Easily open SAP Notes from within Chrome Context Menu",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon-large.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-bitty.png",
    "128": "icon-large.png"
  },
  "manifest_version": 2
}
