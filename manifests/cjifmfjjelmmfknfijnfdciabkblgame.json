{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Image Scaling",
  "short_name": "BIS",
  "description": "Improves the image scaling of Chrome, providing crisper and clearer resized images on pages.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "incognito": "spanning",
  "manifest_version": 2
}
