{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Games Boom",
  "version": "1.3",
  "description": "Search extension for gobebaba.com.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://gobebaba.com/favicon.ico",
      "is_default": true,
      "keyword": "gobebaba.com",
      "name": "Games Boom",
      "search_url": "https://gobebaba.com/search?q={searchTerms}",
      "suggest_url": "https://gobebaba.com/suggest?q={searchTerms}"
    }
  }
}
