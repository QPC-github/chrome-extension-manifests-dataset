{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "Icon1080.png",
    "default_popup": "popup.html"
  },
  "description": "Change Google search results to show only selected country's results.",
  "icons": {
    "16": "Icon16.png"
  },
  "manifest_version": 2,
  "name": "Google Search by Country",
  "permissions": [
    "activeTab"
  ],
  "short_name": "Google Search by Country",
  "version": "2.1"
}
