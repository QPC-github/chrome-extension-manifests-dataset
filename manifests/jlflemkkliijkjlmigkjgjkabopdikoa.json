{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LeadGenych",
  "description": "Плагин для работы с сайтом https://leadgenych.pro/",
  "version": "1.0.2",
  "icons": {
    "128": "ico.png"
  },
  "browser_action": {
    "default_icon": "ico.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "unlimitedStorage",
    "tabs",
    "webRequest",
    "proxy",
    "webRequestBlocking",
    "https://leadgenych.pro/",
    "*://vk.com/",
    "*://*.vk.com/"
  ]
}
