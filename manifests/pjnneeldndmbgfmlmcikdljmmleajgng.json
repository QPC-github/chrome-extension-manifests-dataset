{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.2",
  "name": "Ticker Tool",
  "description": "Type 'tt' plus a stock ticker in the omnibox.",
  "omnibox": {
    "keyword": "tt"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon_16x16.png",
    "32": "icon_32x32.png",
    "48": "icon_48x48.png",
    "64": "icon_64x64.png"
  }
}
