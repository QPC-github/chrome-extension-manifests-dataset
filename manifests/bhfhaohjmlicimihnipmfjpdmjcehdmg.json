{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CX Bookmark Helper",
  "description": "This extension is meant to improve upon the built-in bookmark management system in Chrome",
  "version": "1.0.0.5",
  "browser_action": {
    "default_icon": "icon24.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "bookmarks",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
