{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CexBrainRest",
  "description": "Don't lose focus for more efficient work. Create your own break period and start using it.",
  "version": "1.1",
  "offline_enabled": true,
  "browser_action": {
    "default_title": "Brain Rest",
    "default_icon": "images/16.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "notifications",
    "background"
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "64": "images/64.png"
  }
}
