{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "randmizr",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "randmizr is an Urucas chrome extension for generating random urls, great for avoiding server cache",
  "homepage_url": "https://github.com/Urucas/randmizr",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "tabs"
  ]
}
