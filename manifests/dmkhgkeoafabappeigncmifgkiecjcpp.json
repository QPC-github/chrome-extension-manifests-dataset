{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cookies List",
  "version": "1.1",
  "description": "List and look for cookies",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "cookies.png",
    "48": "cookies.png",
    "128": "cookies.png"
  },
  "browser_action": {
    "default_icon": "cookies.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
