{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Lonely Fisherman",
  "version": "1.1",
  "description": "I'd rather be fishing.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./jquery-3.4.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "./stylesheet.css"
      ],
      "js": [
        "./jquery-3.4.1.min.js",
        "./scripts/generateReplacementContent.js",
        "./scripts/model.js",
        "./scripts/controller.js",
        "./scripts/contentReplacement.js",
        "./scripts/gameTextHandler.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "images/*"
  ]
}
