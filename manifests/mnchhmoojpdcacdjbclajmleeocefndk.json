{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Egerin Homepage",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Automatically sets homepage to egerin.com",
  "homepage_url": "http://egerin.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://egerin.com",
    "startup_pages": [
      "http://www.egerin.com"
    ]
  },
  "default_locale": "en"
}
