{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Eruda",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Console for Browsers",
  "homepage_url": "https://github.com/liriliri/eruda",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Eruda"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
