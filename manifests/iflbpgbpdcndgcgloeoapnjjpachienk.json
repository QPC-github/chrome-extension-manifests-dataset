{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GloScreenTags",
  "version": "1.1",
  "description": "Bookmark any area of a web page and automatically add the link to a comment in your ticket.",
  "manifest_version": 2,
  "devtools_page": "devtools.html",
  "icons": {
    "16": "GloScreenTags16.png",
    "48": "GloScreenTags48.png",
    "128": "GloScreenTags128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage"
  ],
  "web_accessible_resources": [
    "GloScreenTags48.png"
  ]
}
