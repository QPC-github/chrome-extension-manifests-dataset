{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ed+ screen sharing",
  "description": "Enable screen sharing for ed+ video conference.",
  "version": "1.0.1",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "edp16.png",
    "48": "edp48.png",
    "128": "edp128.png"
  },
  "background": {
    "scripts": [
      "u.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.edplus.biz/*"
    ]
  }
}
