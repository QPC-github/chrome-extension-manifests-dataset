{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Loldogs Tab",
  "description": "New Loldog every time you open a new tab!",
  "version": "1.1.12",
  "homepage_url": "http://www.loldogstab.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "topSites",
    "chrome://favicon/",
    "http://be.loldogstab.com/*",
    "https://*/*",
    "http://*/*",
    "management",
    "storage",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ]
}
