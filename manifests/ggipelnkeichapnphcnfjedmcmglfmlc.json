{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Make Reddit great again",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injects a single CSS rule that hides posts from /r/The_Donald",
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/r/all*"
      ],
      "css": [
        "inject.css"
      ]
    }
  ]
}
