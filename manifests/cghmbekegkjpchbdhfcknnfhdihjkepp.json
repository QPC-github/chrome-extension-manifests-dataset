{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Food Roulette",
  "description": "This Executes a random food location search based on filters given by the user",
  "version": "1.0",
  "browser_action": {
    "default_icon": "Menu-Logo.png",
    "default_popup": "FoodRoulettte.html"
  },
  "permissions": [
    "http://*/"
  ],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}
