{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK hidden",
  "version": "0.1",
  "description": "Разработчик: Блещеев Артур",
  "permissions": [
    "tabs",
    "*://vk.com/*"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "jquery.min.js",
        "bg.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
