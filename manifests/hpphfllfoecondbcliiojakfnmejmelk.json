{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MonkeyWish",
  "description": "Use this extension to add an item from any website to your MonkeyWish list.",
  "version": "2.5",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery-ui.min.js",
      "extension.js"
    ],
    "css": [
      "bootstrap.min.css",
      "slick.css",
      "slick-theme.css",
      "main.css"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Add to MonkeyWish List"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
