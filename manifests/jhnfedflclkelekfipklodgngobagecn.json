{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VidSpree Grabber by SnapBatch",
  "description": "This extension helps VidSpree users to easily get YouTube videos into their albums, never leave YouTube again.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery-1.9.1.min.js"
      ]
    }
  ],
  "permissions": [
    "http://api.vidspree.com/",
    "https://vidspree.com/",
    "tabs",
    "cookies"
  ]
}
