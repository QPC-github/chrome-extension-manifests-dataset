{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spamurai",
  "version": "1.1",
  "description": "Blocks static google iframe ads.",
  "icons": {
    "48": "icons/ic_48.png",
    "96": "icons/ic_96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "spamurai.js"
      ]
    }
  ]
}
