{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "New Tab"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Customize your new tab page with awesome new features.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Momentum Tab - #1 New Tab Customizer",
  "offline_enabled": true,
  "permissions": [
    "identity",
    "storage",
    "unlimitedStorage"
  ],
  "short_name": "Tab",
  "version": "3.1.1"
}
