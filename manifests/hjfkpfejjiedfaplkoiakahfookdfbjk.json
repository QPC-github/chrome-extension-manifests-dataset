{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HoloSpace - JanusVR Rooms in Chrome",
  "short_name": "HoloSpace",
  "version": "0.0.2",
  "description": "Preview 3D website built for JanusVR without leaving your browser",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "http://*/"
  ],
  "requirements": {
    "3D": {
      "features": [
        "webgl"
      ]
    }
  }
}
