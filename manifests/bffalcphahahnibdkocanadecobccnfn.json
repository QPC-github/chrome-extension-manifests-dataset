{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FYI Extension",
  "description": "Add links to your FYI Smartlists or your Board with this extension.",
  "version": "0.0.3.7",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "*://*.fyi.to/",
    "https://fyi.to/",
    "storage"
  ]
}
