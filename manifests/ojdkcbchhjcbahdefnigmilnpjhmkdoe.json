{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EisenVault Plugin for Gmail",
  "version": "1.7",
  "description": "Save your email & attachments from Gmail to EisenVault DMS or Alfresco 5.2 (and greater).",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "static/images/logo.png",
    "48": "static/images/logo.png",
    "128": "static/images/logo.png"
  },
  "browser_action": {
    "default_title": "Save to EisenVault",
    "default_icon": "static/images/logo.png",
    "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://*.googleapis.com; object-src 'self'"
}
