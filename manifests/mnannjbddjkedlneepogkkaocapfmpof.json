{
  "background": {
    "scripts": [
      "snt.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "snt.html"
  },
  "description": "SocialDownloadr web search",
  "manifest_version": 2,
  "name": "SocialDownloadr New Tab",
  "options_page": "options.html",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "tabs"
  ],
  "version": "1.1.4",
  "homepage_url": "http://socialdownloadr.com",
  "icons": {
    "128": "icon.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://search.socialdownloadr.com/?channel=ntt&src=hp",
    "search_provider": {
      "name": "socialdownloadr Search",
      "keyword": "socialdownloadr",
      "search_url": "http://search.socialdownloadr.com/search.php?channel=ntt&q={searchTerms}",
      "favicon_url": "http://search.socialdownloadr.com/favicon.php?channel=ntt&src=ds",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://search.socialdownloadr.com/?channel=ntt&src=hp"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "SocialDownloadr"
  }
}
