{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Save",
  "manifest_version": 2,
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "nosave.js"
      ],
      "runat": "document_start",
      "all_frames": true
    }
  ]
}
