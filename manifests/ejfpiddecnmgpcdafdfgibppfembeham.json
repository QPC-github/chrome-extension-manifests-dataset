{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AspenUX",
  "version": "1.2.1",
  "description": "Slide-based editor for HTML5 interactive contents",
  "container": "GOOGLE_DRIVE",
  "api_console_project_id": "319445210353",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/vnd.google.drive.ext-type.apn",
          "application/vnd.google-apps.drive-sdk.319445210353",
          "application/octet-stream"
        ],
        "href": "http://ux.aspenux.com/",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "16": "ux_16.png",
    "32": "ux_32.png",
    "48": "ux_48.png",
    "128": "ux_128.png",
    "256": "ux_256.png"
  },
  "app": {
    "launch": {
      "web_url": "http://ux.aspenux.com"
    }
  }
}
