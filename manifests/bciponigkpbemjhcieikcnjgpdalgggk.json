      "*://118.31.187.206/*",
      "*://127.0.0.1/*",
      "*://192.168.16.59/*",
      "*://192.168.16.53/*",
      "*://114.215.199.163/*",
      "*://121.43.109.238/*",
      "*://121.43.173.249/*",
      "*://112.124.117.58/*",
      "*://mcc.ikandy.cn/*",
      "*://s.mcc.ikandy.cn/*",
      "*://codepen.io/*",
      "*://s.codepen.io/*",
      "*://dev.ikandy.cn/*",
      "*://yzh.ikandy.cn/*",
