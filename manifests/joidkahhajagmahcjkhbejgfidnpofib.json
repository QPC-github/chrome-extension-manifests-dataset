{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Go4Go Downloader",
  "version": "0.97",
  "description": "Allow to download any game at Go4Go website",
  "permissions": [
    "activeTab",
    "https://www.go4go.net/go/games/sgfview/*",
    "http://www.go4go.net/go/games/sgfview/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    },
    "default_title": "Go4Go Downloader"
  },
  "icons": {
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
