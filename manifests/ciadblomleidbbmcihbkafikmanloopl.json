{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "script/jquery.js",
      "script/ready.js",
      "script/popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "graph/icon_16.png",
    "default_popup": "page/popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Türkiye'nin haber sitesi Haber3com farkıyla son dakika haberlerine ulaşın.",
  "homepage_url": "http://www.haber3.com/",
  "icons": {
    "128": "graph/icon_128.png",
    "16": "graph/icon_16.png",
    "48": "graph/icon_48.png"
  },
  "manifest_version": 2,
  "name": "Haber3.com",
  "options_page": "page/options.html",
  "permissions": [
    "*://*.haber3.com/",
    "notifications"
  ],
  "version": "4.2.1"
}
