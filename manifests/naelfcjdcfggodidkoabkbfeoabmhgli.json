{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Anti White",
  "version": "1.0.2",
  "description": "Dark mode mainly for Android documentation website!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "action_ui.html",
    "default_title": "Go dark",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon64.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
