{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SNATCHR",
  "description": "This extension shows a styleguide for the current page",
  "version": "2",
  "browser_action": {
    "default_icon": "logo-favicon.png",
    "default_popup": "index.html",
    "default_title": "SNATCHR"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "web_accessible_resources": [
    "herewego2.js",
    "views/content.ejs"
  ]
}
