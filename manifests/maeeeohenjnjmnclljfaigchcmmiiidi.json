{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shopping.FM",
  "short_name": "Shopping",
  "version": "0.0.0.8",
  "description": "Compare prices from hundreds of stores with one quick search",
  "homepage_url": "https://www.shopping.fm",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' object-src 'self'",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
    },
    "default_title": "Shopping.FM"
  }
}
