{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Coming Up On Giant Bomb",
  "description": "Shows upcoming Giant Bomb videos and live streams. Also shows when Giant Bomb is live.",
  "version": "1.0.0",
  "icons": {
    "16": "icon/16-p.png",
    "48": "icon/48-p.png",
    "128": "icon/128-p.png"
  },
  "browser_action": {
    "default_icon": "icon/128-p.png",
    "default_title": "GB Upcoming",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/live-check.js"
    ]
  }
}
