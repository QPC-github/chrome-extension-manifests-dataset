{
  "background": {
    "persistent": true,
    "scripts": [
      "mainScript.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Simple Site Cleaner"
  },
  "content_scripts": [
    {
      "js": [
        "content_handler.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Clean up the site stored data",
  "homepage_url": "https://chrome.google.com/webstore/detail/simple-site-cleaner/ndembdeooagnameaaopcnjgghmieddke",
  "icons": {
    "128": "icon_128.png",
    "16": "icon.png",
    "48": "icon_48.png"
  },
  "incognito": "spanning",
  "key": "",
  "manifest_version": 2,
  "name": "Simple Site Cleaner",
  "offline_enabled": true,
  "options_page": "settings.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "cookies",
    "storage",
    "browsingData",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.2"
}
