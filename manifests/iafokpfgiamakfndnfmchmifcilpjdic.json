{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CleanFeed",
  "version": "2",
  "manifest_version": 2,
  "description": "Block spam generated by pesky Facebook apps.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*"
      ],
      "js": [
        "clean.js"
      ]
    }
  ]
}
