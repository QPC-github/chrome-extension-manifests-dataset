{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Choice Language Pro-Life",
  "description": "Replaces 'anti-choice' and 'anti-abortion' with 'pro-life'",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
