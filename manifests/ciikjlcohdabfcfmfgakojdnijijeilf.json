{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Feynman",
  "short_name": "Feynman",
  "version": "1.1",
  "description": "Feynman technique in a chrome extension. This extension will help you keep time for studying/focus.",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "alarms",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/feynman_16.png",
      "32": "images/feynman_32.png",
      "48": "images/feynman_48.png",
      "128": "images/feynman_128.png"
    }
  },
  "icons": {
    "16": "images/feynman_16.png",
    "32": "images/feynman_32.png",
    "48": "images/feynman_48.png",
    "128": "images/feynman_128.png"
  },
  "manifest_version": 2
}
