{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Apply CSS and JS to the vote application on cyberlearn.hes-so.ch to improve presentation for projector",
  "manifest_version": 2,
  "author": "Media Engineering Institute",
  "name": "cyberlearn-vote-beamer-mode",
  "version": "1.7",
  "homepage_url": "https://github.com/MediaComem/cyberlearn-vote-beamer-mode",
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "32": "icons/off32.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "*://cyberlearn.hes-so.ch/mod/evoting/*"
  ]
}
