{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nervatura Touch",
  "version": "2.4.2",
  "minimum_chrome_version": "29",
  "description": "Business Management Application",
  "icons": {
    "128": "img/ntura_128_leaf.png",
    "48": "img/ntura_48_leaf.png",
    "16": "img/ntura_16_leaf.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/ntura_19_leaf.png",
      "38": "img/ntura_38_leaf.png"
    },
    "default_title": "Nervatura Touch"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-v0VHf5zy8r/6c+4Mly7xAW4Yr/t5oRyM6X0HBJA//Hw='; object-src 'self'",
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "notifications"
  ]
}
