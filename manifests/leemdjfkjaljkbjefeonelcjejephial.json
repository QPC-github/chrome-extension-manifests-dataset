{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ZTE Chrome Plugin Helper For Summit OTT",
  "version": "2.6",
  "description": "Chrome Native Messaging for VisualOn Plugin Player",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.summitotteas.tk/*.jsp?*"
    ]
  }
}
