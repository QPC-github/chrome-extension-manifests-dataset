{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "This extension allows TM1 Web access to the clipboard.",
  "manifest_version": 2,
  "name": "TM1 Web Clipboard Extension",
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "version": "1.0"
}
