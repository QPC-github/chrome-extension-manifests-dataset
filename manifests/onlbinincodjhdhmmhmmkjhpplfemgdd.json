{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "1.0.2",
  "version_name": "v1.0.2",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "permissions": [
    "browsingData",
    "bookmarks",
    "system.cpu",
    "system.memory",
    "tabs",
    "webNavigation",
    "contextMenus",
    "management",
    "*://*.google.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_icon": "icon/48.png",
    "default_popup": "popup/index.html",
    "default_title": "Add page to MNT2"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
