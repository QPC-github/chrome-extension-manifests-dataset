{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Мой поиск",
  "short_name": "Мой поиск",
  "version": "2.0",
  "author": "Дмитрий Пройдак",
  "description": "Удобный поиск в Яндексе",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Rндекс",
      "keyword": "Rндекс_search",
      "search_url": "http://ya.tupoisk.ru/?text={searchTerms}",
      "favicon_url": "http://ya.tupoisk.ru/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "browser_action": {
    "default_title": "Мой поиск",
    "default_icon": "img/avatar.png"
  },
  "icons": {
    "128": "img/avatar.png"
  }
}
