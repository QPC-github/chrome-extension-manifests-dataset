{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "asset/main.js"
    ]
  },
  "browser_action": {
    "default_icon": "asset/icons/icon16.png",
    "default_popup": "popup.html"
  },
  "description": "Analyze Current URL with SmallToolStation Tools",
  "icons": {
    "128": "asset/icons/icon128.png",
    "16": "asset/icons/icon16.png",
    "48": "asset/icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "SmallToolStation Tools for Chrome!",
  "short_name": "Seo tools",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "version": "1.0"
}
