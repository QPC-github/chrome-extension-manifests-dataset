{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Disappointed Queen",
  "version": "1.0",
  "description": "A picture of a disappointed Queen Elizabeth will pop up every 30 seconds. That is all.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "bg.html",
    "css": [
      "styles.cs"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "queen.jpg",
    "overlay.png"
  ],
  "manifest_version": 2
}
