{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BJ Screen Sharing",
  "author": "Bluejay Mobile Health, Inc.",
  "version": "6",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Required for screen capture",
  "homepage_url": "https://meeting.bluejayhealth.com",
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.bluejayhealth.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
