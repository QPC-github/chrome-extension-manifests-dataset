{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IO Campaign Checker",
  "version": "1.0",
  "description": "InteractiveOffers checker to verify if campaigns are well configured",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/io16.png",
      "32": "images/io32.png",
      "48": "images/io48.png",
      "128": "images/io128.png"
    }
  },
  "icons": {
    "16": "images/io16.png",
    "32": "images/io32.png",
    "48": "images/io48.png",
    "128": "images/io128.png"
  },
  "manifest_version": 2
}
