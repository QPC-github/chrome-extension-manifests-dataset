{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scrapp Bookmarks",
  "version": "1.0",
  "description": "Your bookmarks everywhere",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "32": "icon-only-32.png",
    "192": "icon-192.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-only-32.png"
  },
  "manifest_version": 2
}
