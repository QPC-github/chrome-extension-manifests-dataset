    "*://www.spreadshirt.se/*",
    "*://www.spreadshirt.at/*",
    "*://www.spreadshirt.net/*",
    "*://www.spreadshirt.com.au/*",
    "*://*.productor.io/*",
    "activeTab",
    "storage",
    "background",
    "downloads"
  ],
  "web_accessible_resources": [
    "assets/*.*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
