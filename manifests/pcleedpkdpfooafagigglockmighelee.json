{
  "description": "Copy & Paste, Excel Sheet (.xlsx) import and export, Trademark Check and many more!",
  "version": "3.3.7",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Productor for Spreadshirt",
  "short_name": "PRDSP",
  "author": "Thimo Grauerholz",
  "browser_action": {
    "default_icon": "assets/icons/icon128.png",
    "default_title": "Productor for Spreadshirt"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://partner.spreadshirt.com/*",
        "*://partner.spreadshirt.ca/*",
        "*://partner.spreadshirt.de/*",
        "*://partner.spreadshirt.fr/*",
        "*://partner.spreadshirt.co.uk/*",
        "*://partner.spreadshirt.be/*",
        "*://partner.spreadshirt.dk/*",
        "*://partner.spreadshirt.es/*",
        "*://partner.spreadshirt.ie/*",
        "*://partner.spreadshirt.it/*",
        "*://partner.spreadshirt.fr/*",
        "*://partner.spreadshirt.nl/*",
        "*://partner.spreadshirt.no/*",
        "*://partner.spreadshirt.pl/*",
        "*://partner.spreadshirt.ch/*",
        "*://partner.spreadshirt.fi/*",
        "*://partner.spreadshirt.se/*",
        "*://partner.spreadshirt.at/*",
        "*://partner.spreadshirt.net/*",
        "*://partner.spreadshirt.com.au/*",
        "*://www.spreadshirt.com/*",
        "*://www.spreadshirt.ca/*",
        "*://www.spreadshirt.de/*",
        "*://www.spreadshirt.co.uk/*",
        "*://www.spreadshirt.be/*",
        "*://www.spreadshirt.dk/*",
        "*://www.spreadshirt.es/*",
        "*://www.spreadshirt.ie/*",
        "*://www.spreadshirt.it/*",
        "*://www.spreadshirt.nl/*",
        "*://www.spreadshirt.no/*",
        "*://www.spreadshirt.pl/*",
        "*://www.spreadshirt.ch/*",
        "*://www.spreadshirt.fi/*",
        "*://www.spreadshirt.se/*",
        "*://www.spreadshirt.at/*",
        "*://www.spreadshirt.fr/*",
        "*://www.spreadshirt.net/*",
        "*://www.spreadshirt.com.au/*"
      ],
      "all_frames": true,
      "js": [
        "content.bundle.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://partner.spreadshirt.com/*",
    "*://partner.spreadshirt.ca/*",
    "*://partner.spreadshirt.de/*",
    "*://partner.spreadshirt.fr/*",
    "*://partner.spreadshirt.co.uk/*",
    "*://partner.spreadshirt.be/*",
