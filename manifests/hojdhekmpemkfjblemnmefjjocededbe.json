{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Moxtra Web Clipper",
  "version": "1.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "12",
  "description": "__MSG_ExtensionDescription__",
  "icons": {
    "128": "xe.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "images/icon_16x16.png",
    "default_title": "__MSG_ExtensionTitle__",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "unlimitedStorage",
    "cookies",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*",
    "http://localhost:8090/user",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "images/icon_16x16.png",
    "images/logo.png",
    "images/moxtralogo.svg",
    "images/moxtralogo@2x.svg",
    "images/logo@2x.png"
  ]
}
