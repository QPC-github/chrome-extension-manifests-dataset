{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto Open Links",
  "version": "1.1",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*"
      ],
      "js": [
        "auto_open_links.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "auto_open_links.js"
  ]
}
