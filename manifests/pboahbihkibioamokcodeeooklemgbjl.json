{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Get papers via the OSU proxy",
  "manifest_version": 2,
  "name": "OSU-proxy",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/page-16.png",
      "32": "icons/page-32.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
