{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.6",
  "name": "Shop Search",
  "description": "Search different shops for previously selected text.",
  "permissions": [
    "contextMenus"
  ],
  "web_accessible_resources": [
    "settings.js",
    "cmsearch/sitesearch.js",
    "cmsearch/options.js",
    "cmsearch/bootstrap.min.css"
  ],
  "background": {
    "scripts": [
      "settings.js",
      "cmsearch/sitesearch.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "shopsearch-16.png",
    "48": "shopsearch-48.png",
    "128": "shopsearch-128.png"
  }
}
