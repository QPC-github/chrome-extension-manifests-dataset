{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "[CandrabeQx] AyoDance Emoticons on Facebook",
  "version": "2.2.39",
  "manifest_version": 2,
  "description": "Shows someone's profile picture if his/her name is tagged on a post/comment. You'll also get Audition AyoDance emoticons displayed.",
  "homepage_url": "https://www.facebook.com/notes/candra/emoticon-ayodance-di-facebook-client-side/10150730939436343",
  "icons": {
    "16": "icon set/16 32bit.png",
    "48": "icon set/48 32bit.png",
    "128": "icon set/128 32bit.png"
  },
  "permissions": [
    "http://www.facebook.com/*",
    "https://www.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "css": [
        "css.css"
      ],
      "js": [
        "www.js",
        "main_vars.js",
        "main_func.js"
      ],
      "run_at": "document_end"
    }
  ]
}
