{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Close Sharpen on Zendesk",
  "description": "This extension closes the Sharpen popup on page load.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.zendesk.com/*",
        "https://*.zendesk.com/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}
