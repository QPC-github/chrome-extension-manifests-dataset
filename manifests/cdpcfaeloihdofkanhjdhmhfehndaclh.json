{
  "background": {
    "persistent": false,
    "scripts": [
      "imanager.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css.css"
      ],
      "js": [
        "jquery.min.js",
        "analytics.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Share link to google plus",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "share link to google plus",
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
