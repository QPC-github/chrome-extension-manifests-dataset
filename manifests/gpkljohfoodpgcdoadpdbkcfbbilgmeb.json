{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Xclusiveloaded",
  "description": "The fastest way to get notifications for the latest news and song updates",
  "background": {
    "page": "background.html"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "https://xclusiveloaded.com/*"
  ],
  "version": "2.0"
}
