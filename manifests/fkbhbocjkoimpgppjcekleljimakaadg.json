{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoCogs",
  "description": "Cost of goods Calculator for Oberlo + Aliexpress",
  "version": "0.0.3",
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "*://*.aliexpress.com/*",
    "*://*.oberlo.com/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/libs/jquery.min.js",
      "js/libs/moment.js",
      "js/Settings.js",
      "js/lh.js",
      "js/MessageHelper.js"
    ]
  }
}
