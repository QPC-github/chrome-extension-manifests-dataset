{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Real Time Reporting for Google Analytics",
  "default_locale": "en",
  "version": "1.0.9",
  "description": "An extension to watch real time report of Google Analytics",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com; object-src 'self'; font-src https://fonts.gstatic.com http://fonts.gstatic.com",
  "oauth2": {
    "client_id": "367229178017-4d8es9fl6uvk3khqckb52jqk0isuqqtb.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/analytics.readonly"
    ]
  },
  "permissions": [
    "identity",
    "idle",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/image38.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/event.js"
    ]
  },
  "icons": {
    "128": "images/image38.png"
  }
}
