{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Browserstack Loader",
  "version": "2.5",
  "description": "Test pages on the fly with Browserstack.",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
