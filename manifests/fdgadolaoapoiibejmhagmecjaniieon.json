{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Terrific Shopper-B",
  "description": "Demonstrates usage and features of the Shopping New Tab",
  "version": "6.0.7",
  "manifest_version": 2,
  "permissions": [
    "history",
    "topSites",
    "cookies",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Open Terrific Shoppers  New Tab"
  },
  "icons": {
    "128": "img/logo_128x.png",
    "16": "img/logo_16x.png",
    "48": "img/logo_48x.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://trends.mobitech-search.xyz; object-src 'self'"
}
