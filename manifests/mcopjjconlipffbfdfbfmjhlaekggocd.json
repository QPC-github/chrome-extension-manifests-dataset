{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sillow",
  "version": "0.1.0",
  "description": "Embed Google Street View on Zillow details pages",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "*://www.zillow.com/homedetails/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.zillow.com/homedetails/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "inject.js"
  ],
  "manifest_version": 2
}
