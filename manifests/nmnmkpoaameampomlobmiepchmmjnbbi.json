{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Share my Tabs",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Share tabs to your friends !",
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "main.html",
    "default_title": "Share my tabs"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://sharemytabs.com/"
  ],
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "img/icon48.png"
  ]
}
