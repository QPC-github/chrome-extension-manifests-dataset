{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.4.1.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "https://www.shop-alert.nl/"
  ],
  "description": "Shopalert is een prijsvergelijker die altijd de goedkoopste prijs vindt. Met Shopalert vergelijken we 50 miljoen producten.",
  "name": "Shop Alert | realtime prijsvergelijking",
  "short_name": "prijsvergelijker, vindt actiecodes",
  "version": "1.4.1",
  "web_accessible_resources": [
    "modal.html",
    "shop_alert_pig.png"
  ]
}
