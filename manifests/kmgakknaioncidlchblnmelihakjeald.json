{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_title": ""
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Календарь Mail.Ru",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "manifest_version": 2,
  "name": "Календарь Mail.Ru",
  "permissions": [
    "tabs",
    "http://*.mail.ru/",
    "https://*.mail.ru/"
  ],
  "version": "0.4"
}
