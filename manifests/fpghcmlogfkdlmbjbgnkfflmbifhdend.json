{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reklamsız İzle",
  "description": "Dizi ve film sitelerindeki reklamları kaldırır",
  "version": "1.10.0",
  "icons": {
    "16": "assets/icon/16.png",
    "48": "assets/icon/48.png",
    "128": "assets/icon/128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "name": "Reklamsız İzle"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://unutulmazfilmler.co/*"
      ],
      "css": [
        "site/unutulmazfilmler/style.css"
      ],
      "js": [
        "site/unutulmazfilmler/script.js"
      ]
    },
    {
      "matches": [
        "*://filmakinesi.net/*"
      ],
      "css": [
        "site/filmakinesi/style.css"
      ],
      "js": [
        "site/filmakinesi/script.js"
      ]
    },
    {
      "matches": [
        "*://dizipub.net/*"
      ],
      "css": [
        "site/dizipub/style.css"
      ],
      "js": [
        "site/dizipub/script.js"
      ]
    },
    {
      "matches": [
        "*://dizilab.pw/*"
      ],
      "css": [
        "site/dizilab/style.css"
      ],
      "js": [
        "site/dizilab/script.js"
      ],
      "_cookie": true
    },
    {
      "all_frames": true,
      "matches": [
        "http://dizipas.net/*",
        "http://dizipas.org/*"
      ],
      "css": [
        "site/dizipas/style.css"
      ],
      "js": [
        "site/dizipas/script.js"
      ],
      "_cookie": true
    },
    {
      "matches": [
        "*://diziay.me/*"
      ],
      "css": [
        "site/diziay/style.css"
      ],
      "js": [
        "site/diziay/script.js"
      ],
      "_cookie": true
    },
    {
      "matches": [
        "*://www.dizibox.pw/*"
      ],
      "css": [
        "site/dizibox/style.css"
      ],
      "js": [
        "site/dizibox/script.js"
      ]
    },
    {
      "matches": [
        "*://dizist.net/*"
      ],
      "css": [
        "site/dizipub/style.css"
      ],
      "js": [
        "site/dizipub/script.js"
      ]
    },
    {
      "all_frames": true,
      "matches": [
        "*://dizimag.pw/*"
      ],
      "css": [
        "site/dizimag/style.css"
      ],
      "js": [
        "assets/js/jquery.min.js",
        "assets/js/arrive.min.js",
        "site/dizimag/script.js"
      ],
      "_cookie": true
    },
    {
      "all_frames": true,
      "matches": [
        "*://vidmoly.me/*",
        "*://vidmoly.to/*"
      ],
      "js": [
        "assets/js/arrive.min.js",
        "site/vidmoly/script.js"
      ]
    },
    {
      "matches": [
        "http://www.ilkfullfilmizle.com/*"
      ],
      "css": [
        "site/ilkfullfilmizle/style.css"
      ],
      "js": [
        "site/ilkfullfilmizle/script.js"
      ]
    },
    {
      "matches": [
        "http://www.inndirmedenfilmizle.com/*"
      ],
      "css": [
        "site/inndirmedenfilmizle/style.css"
      ],
      "js": [
        "site/inndirmedenfilmizle/script.js"
      ]
    },
    {
      "matches": [
        "http://www.altyazilifilmizle.org/*"
      ],
      "css": [
        "site/altyazilifilmizle/style.css"
      ],
      "_cookie": true
    },
    {
      "matches": [
        "*://www.fullhdfilmizlesene.org/*"
      ],
      "css": [
        "site/fullhdfilmizlesene/style.css"
      ],
      "js": [
        "assets/js/arrive.min.js",
        "site/fullhdfilmizlesene/script.js"
      ]
    },
    {
      "matches": [
        "*://720p-izle.com/*"
      ],
      "css": [
        "site/720pizle/style.css"
      ],
      "js": [
        "site/720pizle/script.js"
      ]
    },
    {
      "matches": [
        "http://www.filmifullizle.org/*"
      ],
      "css": [
        "site/filmifullizle/style.css"
      ],
      "js": [
        "site/filmifullizle/script.js"
      ]
    },
    {
      "matches": [
        "http://jetfilmizle.biz/*"
      ],
      "css": [
        "site/jetfilmizle/style.css"
      ],
      "js": [
        "site/jetfilmizle/script.js"
      ]
    },
    {
      "matches": [
        "http://www.fullfilmizlesin.com/*"
      ],
      "js": [
        "site/fullfilmizlesin/script.js"
      ]
    }
  ]
}
