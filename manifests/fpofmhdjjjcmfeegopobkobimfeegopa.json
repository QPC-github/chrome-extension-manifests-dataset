{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_title": "SSearch",
    "default_popup": "choice.html",
    "default_icon": "icons/ssearch.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "description": "An add-on to Search on multiple search-engines at one request.",
  "manifest_version": 2,
  "name": "SuperSearch",
  "version": "3.0"
}
