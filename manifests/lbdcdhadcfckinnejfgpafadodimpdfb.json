{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DISCO",
  "description": "DISCO 서비스 이용을 위한 확장 프로그램",
  "version": "1.0.3",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon48.png",
    "default_title": "DISCO Extension",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "*://apis.naver.com/*"
  ]
}
