{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search With X3",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Search With X3",
  "icons": {
    "16": "icon_16.png",
    "128": "icon-128-128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "downloads",
    "downloads.open"
  ]
}
