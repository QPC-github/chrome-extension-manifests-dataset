{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "History Clear",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Auto clear browsing history",
  "browser_action": {
    "default_title": "History Clear",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "browsingData",
    "tabs"
  ]
}
