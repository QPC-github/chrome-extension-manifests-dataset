{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Social Analytics",
  "description": "This extension will show the social analytics data based on the url",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost/demos/social-stats/url/stats",
    "http://20170112-codrsource.rhcloud.com/Url/stats"
  ]
}
