{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Дима пишет",
  "version": "1.0.0",
  "description": "Дима пишет",
  "author": "",
  "icons": {
    "128": "/img/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/lib/randomColor.js",
        "/js/cs.js"
      ],
      "css": [
        "/css/cs.css"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "/*"
  ]
}
