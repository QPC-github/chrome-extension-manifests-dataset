{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Business Crowd - Secret Source",
  "short_name": "Business Crowd",
  "description": "The freshest links about business and news, from around the web. A Founders must!",
  "version": "2.0",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
