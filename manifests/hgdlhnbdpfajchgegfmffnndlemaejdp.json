{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitgnostic",
  "version": "0.47",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "http://twitter.com/*",
    "https://twitter.com/*"
  ],
  "web_accessible_resources": [
    "anon-timeline.min.js",
    "anon-timeline.js"
  ],
  "icons": {
    "16": "twitgnostic16.png",
    "48": "twitgnostic48.png",
    "128": "twitgnostic128.png"
  }
}
