{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TJPM25",
  "description": "Show pm2.5 in Tianjin. The badge shows the Nanjing Road",
  "version": "1.0",
  "permissions": [
    "<all_urls>",
    "alarms"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  }
}
