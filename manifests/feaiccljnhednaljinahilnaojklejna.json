{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "StarPound Technologies, Inc.",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "StarPound CORE WebRTC-based Screen Sharing",
  "short_name": "StarPound Screen Sharing",
  "homepage_url": "http://starpoundtech.com/",
  "icons": {
    "48": "128_sp-app-icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "StarPound Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "version": "1.0.0.7",
  "web_accessible_resources": [
    "128_sp-app-icon.png"
  ]
}
