{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "50 tabs: fast preview & selection",
  "short_name": "50 tabs",
  "description": "Quick tab selection. Useful when you have 10 or more open tabs. Press Alt+Shift+0 and enter the index of the tab you need.",
  "version": "3.0",
  "icons": {
    "128": "50t_128_tp.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "50t_128_tp.png"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+0",
        "mac": "Alt+Shift+0"
      },
      "description": "Tab selection"
    }
  }
}
