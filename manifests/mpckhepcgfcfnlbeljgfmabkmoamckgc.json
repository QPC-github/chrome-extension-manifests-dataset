{
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ]
    }
  ],
  "description": "Twitter tools",
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "TwitterPlus",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "author": "Marvin Medeiros",
  "web_accessible_resources": [
    "classic-retweet.js",
    "jquery.js"
  ]
}
