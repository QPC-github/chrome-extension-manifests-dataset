{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sumer",
  "description": "Get a total duration by selecting a region with a list of durations",
  "version": "1.5",
  "browser_action": {
    "default_icon": {
      "38": "icon38.png"
    },
    "default_title": "Calculate total duration"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {}
}
