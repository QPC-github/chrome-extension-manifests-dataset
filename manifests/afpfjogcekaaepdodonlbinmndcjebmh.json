{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dash Links",
  "short_name": "Dash Links",
  "description": "Not really bookmarks...",
  "version": "0.0.13",
  "incognito": "split",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "page.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "app.js"
    ]
  },
  "manifest_version": 2
}
