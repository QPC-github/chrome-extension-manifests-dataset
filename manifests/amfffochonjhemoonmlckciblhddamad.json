{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AMP for Endpoint Search",
  "description": "Pivot to the AMP for Endpoint console from an indicator (SHA256, IP, Domain, Filename)",
  "version": "0.3",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "64": "64x64.png",
    "128": "128x128.png",
    "256": "256x256.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
