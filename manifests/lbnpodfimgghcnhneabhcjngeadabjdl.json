{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CoIndex",
  "description": "Easily manage your Bitcoin addresses from your browser.",
  "version": "1.0.3",
  "permissions": [
    "http://blockexplorer.com/",
    "https://blockchain.info/",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
