{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ToDo",
  "description": "The Simplest ToDo List",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "jquery.js",
    "jquery.cookie.js",
    "jquery.jeditable.js",
    "popup.js",
    "icon.png"
  ]
}
