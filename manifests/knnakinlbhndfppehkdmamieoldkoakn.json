{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cumberbatch",
  "version": "1.1.8",
  "manifest_version": 2,
  "description": "Find out what Benedict Cumberbatch's name could be with a little imagination!",
  "homepage_url": "https://github.com/akhilgupta1093/cumberbatch",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Cumberbatch"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
