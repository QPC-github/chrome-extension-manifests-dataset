{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My GitHub Repos",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "Extension to show my last 5 GitHub repos",
  "icons": {
    "16": "images/octicat.png",
    "128": "images/octicat.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/octicat.png",
      "38": "images/octicat.png"
    },
    "default_title": "My GitHub Repos",
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "GitHub Repos"
  }
}
