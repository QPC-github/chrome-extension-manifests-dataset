{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mocker",
  "description": "GenErAte MocKiNg TeXT",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "mypopup.html"
  },
  "commands": {
    "mock": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "Command+Shift+M"
      },
      "description": "mocks"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/",
    "storage",
    "contextMenus",
    "clipboardWrite"
  ]
}
