{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SoftPlan Old Forum Link",
  "version": "1.0",
  "description": "Takes an old forum link and moves you to the correct forum topic.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "48": "midsize.png",
      "128": "logo.png"
    }
  },
  "background": {
    "scripts": [
      "js/code.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://splash.softplansplsh.org/*"
  ]
}
