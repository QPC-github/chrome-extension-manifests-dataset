{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.1.7",
  "name": "Visualping",
  "manifest_version": 2,
  "description": "Visualping Chrome Extension",
  "browser_action": {
    "default_icon": {
      "76": "img/icon76.png",
      "38": "img/icon38.png",
      "19": "img/icon19.png"
    },
    "default_title": "Visualping",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "32": "img/icon32.png",
    "16": "img/icon16.png"
  },
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "img/*.png"
  ],
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "tabs",
    "notifications",
    "chrome://favicon/"
  ],
  "content_security_policy": "default-src 'self' 'unsafe-eval'; script-src 'self' https://api.amplitude.com https://d24n15hnbwhuhn.cloudfront.net 'unsafe-eval'; connect-src *; style-src * 'unsafe-inline'; img-src 'self' * data: chrome:;"
}
