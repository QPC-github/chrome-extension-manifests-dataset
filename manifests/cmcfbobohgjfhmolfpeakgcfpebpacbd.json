{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GravyAccess - Don't be Blocked!",
  "description": "JavaScript Package Redirector, this will help you use JQuery and other packages where Google severs are blocked.",
  "version": "0.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://ajax.googleapis.com/*"
  ],
  "short_name": "GravyAccess!",
  "background": {
    "scripts": [
      "redirect.js"
    ]
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
