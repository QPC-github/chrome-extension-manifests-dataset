{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MPOS info widget",
  "description": "This extension keeps a track of a single MPOS server and user.",
  "version": "1.0",
  "author": "Aaron Charcoal Styles",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "https://*/index.php?page=api"
  ],
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
