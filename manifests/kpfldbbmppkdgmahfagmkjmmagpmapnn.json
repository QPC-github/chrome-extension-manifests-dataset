{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "fb_main.js",
        "fb_navigation_functions.js"
      ]
    },
    {
      "matches": [
        "*://*.pandora.com/*"
      ],
      "js": [
        "pandora_main.js",
        "pandora_navigation_functions.js"
      ]
    },
    {
      "matches": [
        "*://docs.google.com/*"
      ],
      "js": [
        "gdocs_gsheets_gslides_main.js",
        "gdocs_gsheets_gslides_navigation_functions.js"
      ]
    },
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "gmail_main.js"
      ]
    },
    {
      "matches": [
        "*://*.hulu.com/"
      ],
      "js": [
        "hulu_main.js",
        "hulu_navigation_functions.js"
      ]
    },
    {
      "matches": [
        "*://*.hulu.com/search*"
      ],
      "js": [
        "hulu_search_main.js",
        "hulu_search_navigation_functions.js"
      ]
    },
    {
      "matches": [
        "*://*.netflix.com/*"
      ],
      "js": [
        "netflix_main.js",
        "netflix_navigation_functions.js"
      ]
    },
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "youtube_main.js",
        "youtube_navigation_functions.js"
      ],
      "css": [
        "youtube_css.css"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "closepost.js"
      ]
    }
  ],
  "description": "This Add-on creates extra keyboard shortcuts to various websites for easier use. ",
  "manifest_version": 2,
  "name": "BBradley's Extended Shortcuts",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "version": "2.3"
}
