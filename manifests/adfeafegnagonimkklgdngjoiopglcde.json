{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Experience Project Image Viewer",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Shows photos in stories on the Experience Project uncropped.",
  "icons": {
    "128": "logo.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.experienceproject.com/stories/*"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "ep_image_viewer.js"
      ]
    }
  ]
}
