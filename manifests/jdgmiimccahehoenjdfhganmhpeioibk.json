{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "description": "A simple persian calendar which shows the current day on the extension icon.",
  "icons": {
    "128": "img/icon.png"
  },
  "manifest_version": 2,
  "name": "Simple Persian Calendar",
  "permissions": [
    "http://blacksrc.com/ext/calendar/*"
  ],
  "version": "1.0.1"
}
