{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Retina YT",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Automatically doubles YouTube resolution to 720p for pixel-perfect Retina Display support.",
  "background": {
    "scripts": [
      "onload.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch*",
        "*://*.youtube.dk/watch*",
        "*://*.youtube.de/watch*",
        "*://*.youtube.se/watch*",
        "*://*.youtube.co.uk/watch*",
        "*://*.youtube.it/watch*",
        "*://*.youtube.es/watch*",
        "*://*.youtube.fr/watch*",
        "*://*.youtube.cn/watch*",
        "*://*.youtube.jp/watch*",
        "*://*.youtube.no/watch*",
        "*://*.youtube.fi/watch*",
        "*://*.youtube.is/watch*"
      ],
      "js": [
        "onload.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://*.youtube.com/watch*",
    "*://*.youtube.dk/watch*",
    "*://*.youtube.de/watch*",
    "*://*.youtube.se/watch*",
    "*://*.youtube.co.uk/watch*",
    "*://*.youtube.it/watch*",
    "*://*.youtube.es/watch*",
    "*://*.youtube.fr/watch*",
    "*://*.youtube.cn/watch*",
    "*://*.youtube.jp/watch*",
    "*://*.youtube.no/watch*",
    "*://*.youtube.fi/watch*",
    "*://*.youtube.is/watch*"
  ]
}
