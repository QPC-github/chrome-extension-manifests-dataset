{
  "background": {
    "scripts": [
      "background.js",
      "bk.js"
    ]
  },
  "browser_action": {
    "default_icon": "48.png",
    "default_title": "Switch YouTube mode"
  },
  "description": "Bring the Christmas and New Year enviroment to you",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "name": "Youtube Christmas Mode",
  "permissions": [
    "tabs",
    "<all_urls>",
    "*://*.youtube.com/*",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "Youtube New Year",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.0.6",
  "web_accessible_resources": [
    "bgr.png",
    "lr.png",
    "yt.png",
    "s1.png",
    "s2.png",
    "s3.png"
  ]
}
