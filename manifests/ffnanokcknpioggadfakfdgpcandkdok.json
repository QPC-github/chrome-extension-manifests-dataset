{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Your Stash on the Web",
  "description": "Save info into your stash on the web",
  "browser_action": {
    "default_title": "Stash Web",
    "default_icon": "icon-16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "minimum_chrome_version": "30.0.0.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "identity",
    "notifications",
    "pageCapture",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "overlay_panel.html"
  ],
  "key": "",
  "oauth2": {
    "client_id": "628950343240-q0f1h45cijc5jpfpd1t3832tkqh2shnf.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "manifest_version": 2,
  "version": "0.0.0.9"
}
