{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "好玩",
  "version": "1.2.4.0",
  "manifest_version": 2,
  "description": "好玩",
  "icons": {
    "16": "logos/16.png",
    "48": "logos/48.png",
    "128": "logos/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "management",
    "nativeMessaging"
  ],
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self'  'unsafe-eval'; object-src 'self'"
}
