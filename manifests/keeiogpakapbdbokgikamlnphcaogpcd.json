{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MyInviteAllFB-JustListen",
  "description": "For invite all your friend to event/page etc..",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "inviteALL",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
