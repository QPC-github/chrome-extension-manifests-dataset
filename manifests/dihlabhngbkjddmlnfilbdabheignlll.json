{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Christmas (Traditional) New Tab",
  "version": "3.10.3",
  "description": "A better new tab experience with Christmas (Traditional)",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Christmas (Traditional) New Tab"
  },
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage",
    "unlimitedStorage",
    "system.cpu"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://pages.brandthunder.com/*/welcome*"
      ],
      "js": [
        "themeinstall.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/amazon-icon.png",
    "images/ebay-icon.png",
    "images/facebook-icon.png",
    "images/flickr-icon.png",
    "images/foursquare-icon.png",
    "images/games-icon.png",
    "images/googleplus-icon.png",
    "images/groupon-icon.png",
    "images/linkedin-icon.png",
    "images/music-icon.png",
    "images/myspace-icon.png",
    "images/twitter-icon.png",
    "images/youtube-icon.png",
    "images/pinterest-icon.png"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
