{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "pages": [
        "html/embed.html"
      ],
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "description": "Convert any website or web application to a native Windows/Linux/Mac/Chrome application, and/or Chrome extension.",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png"
  },
  "manifest_version": 2,
  "name": "WebDGap",
  "permissions": [
    "webview",
    "http://*/",
    "https://*/"
  ],
  "version": "1.2.7"
}
