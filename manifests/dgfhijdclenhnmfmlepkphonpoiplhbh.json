{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Social Gones - Actualités Olympique Lyonnais",
  "description": "Votre source d'informations sur l'actualité de l'OL.",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ]
}
