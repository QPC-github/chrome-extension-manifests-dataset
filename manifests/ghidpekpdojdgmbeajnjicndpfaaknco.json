{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CIS Day Cycles",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A browser extension to remind CISHK students/faculty of timetable day cycles",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "CIS Day Cycles"
  },
  "chrome_url_overrides": {
    "newtab": "override.html"
  }
}
