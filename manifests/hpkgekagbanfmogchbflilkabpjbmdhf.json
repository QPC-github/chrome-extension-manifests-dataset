{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "qcode-decoder.min.js",
        "utils.js",
        "content.js"
      ]
    }
  ],
  "name": "__MSG_plugin_name__",
  "default_locale": "en",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "description": "__MSG_plugin_description__",
  "version": "0.0.6",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
