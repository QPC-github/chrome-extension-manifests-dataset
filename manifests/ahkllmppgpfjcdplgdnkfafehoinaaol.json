{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tyda",
  "description": "This extension converts the text of a web page to shakespearian english.",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "Tyda-128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.dev.tyda.io/"
  ]
}
