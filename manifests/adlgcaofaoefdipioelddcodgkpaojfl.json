{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_zeef_extension_name__",
  "description": "__MSG_zeef_extension_description__",
  "options_page": "/options/options.html",
  "version": "2.1.1",
  "background": {
    "page": "background.html",
    "script": "/libs/jquery-1.11.2.js"
  },
  "default_locale": "nl",
  "icons": {
    "128": "images/ZEEF_icon_128.png",
    "19": "images/ZEEF_icon_19.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "contextMenus",
    "https://*.zeef.com/*",
    "chrome://favicon/"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/ZEEF_icon_19.png",
      "38": "images/ZEEF_icon_38@2x.png"
    },
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "images/*",
    "css/*",
    "fonts/*",
    "includes/*"
  ]
}
