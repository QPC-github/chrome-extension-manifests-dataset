{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CoinMarketCap Search Tool",
  "description": "A basic search toolbar for CoinMarketCap.com",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://coinmarketcap.com/api/*"
  ],
  "icons": {
    "128": "icon.png"
  }
}
