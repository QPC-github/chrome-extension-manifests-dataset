{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CSP Disable. DevTool",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Only for Developers. Disable CSP and X-Frame for debug",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ]
}
