{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QLinks",
  "description": "Create a bookmark of the current page into the Qaleido QLinks",
  "version": "1.4",
  "browser_action": {
    "default_icon": {
      "19": "19x19.png",
      "38": "38x38.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "https://tqs.qaleido.com/*",
    "https://dev.qaleido.com/*",
    "https://webmail.qaleido.com/*",
    "https://ssw.solcon.nl/*",
    "storage",
    "notifications",
    "tabs"
  ]
}
