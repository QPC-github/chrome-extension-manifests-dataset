{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Logger",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Logger",
  "icons": {
    "16": "resources/icon_16.png",
    "48": "resources/icon_48.png",
    "128": "resources/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "resources/icon_19.png",
      "38": "resources/icon_38.png"
    }
  },
  "background": {
    "scripts": [
      "jquery.js",
      "model_classes.js",
      "global_constant.js",
      "global_variables.js",
      "sha1.js",
      "WebService.js",
      "uri_parser.js",
      "bg.js"
    ]
  },
  "permissions": [
    "alarms",
    "geolocation",
    "tabs",
    "history",
    "bookmarks",
    "notifications",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage",
    "cookies"
  ],
  "web_accessible_resources": [
    "resources/icon_48.png"
  ]
}
