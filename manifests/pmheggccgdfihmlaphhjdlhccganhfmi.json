{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "More Stars",
  "description": "This extension frees up space on the sidebar of StackExchange chat so that more starred messages can be seen.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://chat.stackexchange.com/rooms/*",
        "http://chat.stackoverflow.com/rooms/*"
      ],
      "js": [
        "morestars.user.js"
      ]
    }
  ]
}
