{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remembrance Relief Plugin",
  "description": "Do you miss him? Do you want more of him? With this plugin, The Most Interesting Man In The World is never more than a click away.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "background"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Remembrance Relief Plugin",
    "default_icon": "icon-off.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "mim.gif"
  ],
  "manifest_version": 2
}
