  "options_page": "options.html",
  "default_locale": "en",
  "permissions": [
    "alarms",
    "storage",
    "tts",
    "idle",
    "contextMenus",
    "notifications",
    "activeTab",
    "gcm",
    "identity",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "background",
    "https://calendar.google.com/"
