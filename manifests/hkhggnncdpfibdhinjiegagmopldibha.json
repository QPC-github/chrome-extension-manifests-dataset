  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://www.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "options.html",
    "popup.html",
    "contribute.html",
    "oauth2callback.html",
    "images/bell-48.png",
    "images/icons/icon-48.png",
    "images/icons/icon-128.png"
