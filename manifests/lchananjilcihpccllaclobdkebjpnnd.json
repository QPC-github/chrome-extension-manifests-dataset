{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QR Url",
  "version": "1.0.1",
  "description": "Use your phone camera to capture current tab URL through QR Code",
  "icons": {
    "16": "images/qrurl16.png",
    "32": "images/qrurl32.png",
    "48": "images/qrurl48.png",
    "128": "images/qrurl128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/qrurl16.png",
      "32": "images/qrurl32.png",
      "48": "images/qrurl48.png",
      "128": "images/qrurl128.png"
    }
  },
  "author": "Edgars S",
  "homepage_url": "https://www.qrurl.app",
  "offline_enabled": true,
  "manifest_version": 2
}
