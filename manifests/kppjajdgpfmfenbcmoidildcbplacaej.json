{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Enlightened",
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "jquery-ui-1.8.21.custom.min.js",
        "pageScan.js",
        "main.js"
      ],
      "matches": [
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "enso-red-brick16x16.png",
    "48": "enso-red-brick48x48.png",
    "128": "enso-red-brick128x128.png"
  },
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "While shopping, discover other sites selling the same item for less!",
  "permissions": [
    "http://*/",
    "tabs"
  ]
}
