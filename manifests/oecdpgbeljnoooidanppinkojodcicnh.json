{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "hide-featured.css"
      ],
      "matches": [
        "http://gumtree.com/*",
        "http://*.gumtree.com/*",
        "http://www.reddit.com/*",
        "http://*.reddit.com/*"
      ]
    }
  ],
  "description": "Hide featured ads that spam the top listings. Currently hides featured ads on Gumtree.",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "manifest_version": 2,
  "name": "Hide Featured",
  "version": "1.0.2",
  "web_accessible_resources": [
    "hide-featured.css"
  ]
}
