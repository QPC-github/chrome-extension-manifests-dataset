{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mindomo - Mind Mapping and Concept Mapping",
  "short_name": "Mindomo",
  "version": "7.6.3",
  "manifest_version": 2,
  "description": "Real-time collaboration on mind maps, concept maps and outlines. Works with Google Drive.",
  "container": [
    "DOMAIN_INSTALLABLE",
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "521918824624",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/xmind",
          "application/vnd-mindmeister",
          "application/x-freemind",
          "text/plain",
          "application/vnd.google.drive.ext-type.mm",
          "application/vnd.google.drive.ext-type.xmind",
          "application/vnd.google.drive.ext-type.mmap",
          "application/vnd.google.drive.ext-type.mom",
          "application/vnd.google.drive.ext-type.opml",
          "application/vnd.google.drive.ext-type.txt"
        ],
        "href": "https://www.mindomo.com/google/drive.htm",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "app": {
    "launch": {
      "web_url": "https://www.mindomo.com/j_spring_gapp_security_check?drive_request=true"
    }
  }
}
