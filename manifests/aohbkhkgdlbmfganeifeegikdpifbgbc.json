{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ChromeCacheKiller",
  "short_name": "CCK",
  "description": "This extension will delete all cache and cookies for the site you are currently on.",
  "version": "1.1",
  "icons": {
    "16": "CacheKiller.png",
    "48": "CacheKiller.png",
    "128": "CacheKiller.png"
  },
  "browser_action": {
    "default_title": "Kill Cache!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}
