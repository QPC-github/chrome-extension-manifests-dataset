{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sitant: Simple Tasks in New Tab",
  "short_name": "Sitant",
  "version": "1.3.1",
  "description": "A minimal, yet incredibly functional new tab replacement",
  "icons": {
    "16": "logo.png",
    "64": "logo.png",
    "128": "logo.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ]
}
