{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Email Finder - AeroLeads",
  "description": "Find email address and phone numbers",
  "version": "5.2.8",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "AeroLeads",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://*.aeroleads.com/*",
    "http://*.aeroleads.com/*",
    "https://*.linkedin.com/*",
    "http://*.linkedin.com/*",
    "https://*.crunchbase.com/*",
    "http://*.crunchbase.com/*",
    "https://*.angel.co/*",
    "http://*.angel.co/*",
    "https://*.xing.com/*",
    "http://*.xing.com/*",
    "tabs",
    "activeTab",
    "storage"
  ]
}
