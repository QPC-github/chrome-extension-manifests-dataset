{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Webinar Ninja",
  "author": "Business Republic",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is for Webinar Ninja, allowing you to share your screen during a webinar. Learn more at http://webinarninja.co",
  "homepage_url": "https://my.webinarninja.co/my/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.webinarninja.co/*"
    ]
  },
  "icons": {
    "16": "WN-Icon-16.png",
    "48": "WN-Icon-48.png",
    "128": "WN-Icon-128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
