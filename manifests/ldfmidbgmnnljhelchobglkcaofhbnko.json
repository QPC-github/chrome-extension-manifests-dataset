{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "form memory",
  "version": "0.1",
  "manifest_version": 2,
  "description": "persistent form input",
  "homepage_url": "https://github.com/quillford/formmemory",
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
