{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Open Tabs with URLs from Clipboard",
  "description": "This extension opens tabs with URLs contained in a clipboard",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "clipboardRead",
    "background",
    "https://ajax.googleapis.com/"
  ]
}
