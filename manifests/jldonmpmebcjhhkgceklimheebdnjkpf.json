{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Saved to Bookmark for Reddit",
  "version": "3.0.5",
  "author": "Christopher Jagoda",
  "permissions": [
    "https://ssl.reddit.com/",
    "https://www.reddit.com/",
    "https://reddit.com/",
    "bookmarks",
    "cookies",
    "storage"
  ],
  "description": "Allows a user to convert their saved Reddit content to browser bookmarks.",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Saved to Bookmark for Reddit",
    "default_popup": "index.html",
    "default_icon": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
