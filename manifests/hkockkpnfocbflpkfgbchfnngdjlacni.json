{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Emoji Replacer",
  "version": "1.0.1",
  "description": "Replace all emoji with the middle finger emoji",
  "manifest_version": 2,
  "icons": {
    "128": "emoji_replacer_icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
