{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Elect Jasper!",
  "description": "Replaces Clinton (Bill or Hillary) with Jasper. So much better.",
  "version": "1.0",
  "author": "eric schrepel and zoe hoeltzel",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
