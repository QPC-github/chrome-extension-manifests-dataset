{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ForgetMeNot - Never forget to CC someone",
  "version": "0.2",
  "description": "This extension will catch potential mistakes when you meant to add someone to an email thread via '+'.",
  "author": "Greg Greiner",
  "icons": {
    "16": "missing16.png",
    "48": "missing48.png",
    "128": "missing128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-1.10.2.min.js",
    "gmail.js",
    "main.js"
  ],
  "short_name": "Missing Emails",
  "manifest_version": 2
}
