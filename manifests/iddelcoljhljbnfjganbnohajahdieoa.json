{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Green Building Banner",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get info about NYC building energy usage while browsing Yelp, Foursquare, Menupages, Foursquare, Property Shark, and more.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://banner.logcheckapp.com/"
  ],
  "background": {
    "scripts": [
      "jquery-1.8.2.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.yelp.com/*"
      ],
      "js": [
        "yelp.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.zillow.com/*"
      ],
      "js": [
        "zillow.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.trulia.com/*"
      ],
      "js": [
        "trulia.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "https://foursquare.com/*"
      ],
      "js": [
        "foursquare.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.propertyshark.com/*"
      ],
      "js": [
        "propertyshark.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://*.streeteasy.com/*"
      ],
      "js": [
        "streeteasy.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://*.citysearch.com/profile/*"
      ],
      "js": [
        "citysearch.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.tripadvisor.com/*"
      ],
      "js": [
        "yelp.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://nymag.com/listings/*"
      ],
      "js": [
        "nymag.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.menupages.com/*"
      ],
      "js": [
        "menupages.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.loopnet.com/Listing/*"
      ],
      "js": [
        "loopnet.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.urbanspoon.com/*"
      ],
      "js": [
        "urbanspoon.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "http://www.zagat.com/r/*"
      ],
      "js": [
        "zagat.js",
        "contentscript.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
}
