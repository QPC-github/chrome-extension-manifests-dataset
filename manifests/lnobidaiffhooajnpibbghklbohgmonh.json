{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No registrarse para Clarín y La Nación",
  "short_name": "NoRegistrarseClarin",
  "description": "Democratizes the access to information, for free",
  "version": "5.3.4",
  "manifest_version": 2,
  "author": "@miltonlaufer",
  "permissions": [],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.clarin.com/*",
        "https://*.lanacion.com.ar/*"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ]
}
