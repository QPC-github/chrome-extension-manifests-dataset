{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "IIT KGP TNP Extension",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Latest TNP Updates",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "TNP Notice Board",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://tp.iitkgp.ernet.in/notice/",
    "tabs"
  ]
}
