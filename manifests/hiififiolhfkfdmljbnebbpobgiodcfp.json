{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Debrand Alt-Right",
  "version": "0.3",
  "description": "Debrand the term 'alt-right', and return it to its white supremacist roots",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "no-kkk-48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "no-kkk-48.png",
    "128": "no-kkk-128.png"
  }
}
