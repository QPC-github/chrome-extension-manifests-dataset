{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Movie lover",
  "description": "Let's you know upcoming movies for the week",
  "version": "0.1.3.3",
  "icons": {
    "16": "icon1_16.png",
    "48": "icon1_48.png",
    "128": "icon1_128.png"
  },
  "browser_action": {
    "default_title": "Click to know Movie updates!",
    "default_icon": "icon1.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
