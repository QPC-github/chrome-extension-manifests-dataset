{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notifications",
  "description": "Notifications App",
  "browser_action": {
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "Notification",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "2.3",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "Persistent": false
  }
}
