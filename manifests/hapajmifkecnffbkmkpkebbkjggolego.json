{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Open In IDE",
  "description": "This extension adds an Open In IDE for Tomcat Exception Reports",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "linkify.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "opener.js"
  ]
}
