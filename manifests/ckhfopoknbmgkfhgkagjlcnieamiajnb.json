{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Scuttle Bookmarker",
  "description": "Add bookmarks to Scuttle / SemanticScuttle",
  "version": "0.0.2",
  "omnibox": {
    "keyword": "scuttle"
  },
  "background": {
    "scripts": [
      "ui/js/jquery-1.7.2.min.js",
      "background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Scuttle Bookmarker",
    "default_icon": "icons/iconGray24.png"
  },
  "offline_enabled": false,
  "manifest_version": 2
}
