{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "sudden-death",
  "short_name": "sudden-death",
  "description": "Sudden Death ASCII Art Generator",
  "version": "1.2",
  "icons": {
    "16": "assets/img/icon_16.png",
    "128": "assets/img/icon_128.png"
  },
  "browser_action": {
    "default_title": "Sudden Death ASCII Art Generator",
    "default_icon": "assets/img/icon_16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://sudden-death.herokuapp.com/",
    "clipboardWrite"
  ]
}
