{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Eleot's Content Editable Text Formatter",
  "version": "1.0.9",
  "manifest_version": 2,
  "description": "Yet another text formatting extension, because the others I tried didn't seem to work satisfactory.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Eleot's Text Formatter"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ]
}
