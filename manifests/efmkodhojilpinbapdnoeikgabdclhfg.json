{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cointrack",
  "description": "Real-time Bitcoin price",
  "version": "0.2",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/chromeicon1.png",
    "48": "images/chromeicon1.png",
    "128": "images/chromeicon1.png"
  },
  "browser_action": {
    "default_title": "Cointrack",
    "default_popup": "popup.html"
  },
  "options_page": "popup.html"
}
