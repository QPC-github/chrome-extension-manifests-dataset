{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Fullframe",
  "description": "This extension puts Youtube videos in full-frame mode.",
  "version": "0.7",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "onClick.js"
    ],
    "persisent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
