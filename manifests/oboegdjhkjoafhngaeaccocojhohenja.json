{
  "author": "Quantum Tech",
  "background": {
    "persistent": false,
    "scripts": [
      "background-config.js",
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://sync.quantumtech.sg/*"
      ]
    }
  ],
  "description": "Google Chrome extension for Sync",
  "homepage_url": "http://www.quantumtech.sg",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Screensharing Extension for Sync",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.10.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
