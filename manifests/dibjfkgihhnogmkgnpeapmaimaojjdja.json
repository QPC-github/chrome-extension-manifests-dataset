{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quip Helper",
  "short_name": "Quip",
  "description": "This extension opens quip, and toggles full-screen editing mode.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "quip_16.png",
    "48": "quip_48.png",
    "128": "quip_128.png"
  },
  "permissions": [
    "tabs",
    "https://quip.com/*"
  ],
  "browser_action": {
    "default_title": "Quip Helper",
    "default_icon": "quip_128.png"
  }
}
