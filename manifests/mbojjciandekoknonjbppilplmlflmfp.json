{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Not Seen",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Write messages in Facebook without being tracked.",
  "browser_action": {
    "default_icon": "spy_active.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png",
    "256": "icon_256.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webNavigation",
    "management",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking"
  ]
}
