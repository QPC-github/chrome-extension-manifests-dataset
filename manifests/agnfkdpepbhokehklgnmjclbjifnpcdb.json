{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Axon Game",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Play flash games, Axon Game",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "share.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
