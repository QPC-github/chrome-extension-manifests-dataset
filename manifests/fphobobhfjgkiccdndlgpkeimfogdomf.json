{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FindLinks",
  "version": "1.4",
  "author": "https://www.google.com/+VittorioMinacori",
  "background": {
    "scripts": [
      "background.js",
      "ga.js"
    ]
  },
  "description": "FindLinks highlights for you all clickable elements and links in the page!",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Find all clickable elements in the page.",
    "default_icon": "findlinks.png"
  },
  "icons": {
    "16": "findlinks.png",
    "48": "icon_48.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
