{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HTML Color Coder",
  "version": "1.0.2",
  "description": "This extension gives HTML color code value for selected color.",
  "browser_action": {
    "default_icon": "19.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "16.png",
    "18": "18.png",
    "19": "19.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  }
}
