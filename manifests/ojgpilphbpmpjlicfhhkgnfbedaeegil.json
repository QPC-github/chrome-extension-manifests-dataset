{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Piktochart",
  "description": "Make information beautiful using Piktochart | Create infographics today",
  "version": "2.0.4",
  "manifest_version": 2,
  "app": {
    "urls": [
      "https://magic.piktochart.com/",
      "http://piktochart.com/"
    ],
    "launch": {
      "web_url": "https://magic.piktochart.com/users/auth/google_oauth2"
    }
  },
  "icons": {
    "128": "piktochart-128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
