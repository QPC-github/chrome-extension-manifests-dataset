{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/index.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/index.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "background",
    "tabs",
    "activeTab",
    "http://*/",
    "https://*/*",
    "*://*/*",
    "storage",
    "unlimitedStorage",
    "alarms",
    "notifications",
    "contextMenus",
    "identity"
  ],
  "icons": {
    "16": "$assets/icon-16.png",
    "32": "$assets/icon-48.png",
    "128": "$assets/icon-128.png"
  },
  "browser_action": {
    "default_title": "fluany",
    "default_popup": "popup/index.html"
  },
  "name": "fluany",
  "version": "1.0.3"
}
