{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "S|pp|S",
  "version": "2.0",
  "description": "Extension for Secure Web Access---Dedicated to Dr.Anjali Sardana",
  "background": {
    "scripts": [
      "sanchig.js"
    ]
  },
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "<all_urls>"
  ],
  "options_page": "ReadMe.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "S|pp|S",
    "default_popup": "sanchig.html"
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "manifest_version": 2
}
