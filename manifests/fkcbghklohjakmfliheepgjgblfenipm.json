{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stylur",
  "version": "0.0.2",
  "description": "Change any website, apply community styles or create your own.",
  "browser_action": {
    "default_icon": "logo128s.png",
    "default_popup": "popup.html",
    "default_title": "Stylur.net extension"
  },
  "background": {
    "scripts": [
      "background.js",
      "helpers.js"
    ]
  },
  "icons": {
    "16": "logo16s.png",
    "48": "logo48s.png",
    "128": "logo128s.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "*://*/*"
  ]
}
