{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "phpMyAdmin Watch",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "2.0",
  "options_page": "options.html",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "knockout-3.3.0.js",
        "jquery.min.js",
        "phpmyadminwatch.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "web_accessible_resources": []
}
