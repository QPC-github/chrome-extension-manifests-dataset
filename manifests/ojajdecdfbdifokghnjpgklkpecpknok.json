{
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_dessan_on.png",
    "default_popup": "index.html",
    "default_title": "L'extension de Dessan"
  },
  "content_security_policy": "script-src 'self' https://www.google.com;object-src 'self'",
  "description": "Notifications et annonces des dernières vidéos et streams de Dessan",
  "icons": {
    "128": "img/icon_dessan_on128.png"
  },
  "manifest_version": 2,
  "name": "L'extension de Dessan",
  "permissions": [
    "tabs",
    "https://api.twitch.tv/",
    "https://www.googleapis.com/",
    "notifications",
    "alarms",
    "storage",
    "webRequest"
  ],
  "short_name": "Dessan",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3.6"
}
