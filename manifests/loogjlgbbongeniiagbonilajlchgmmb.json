{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Xerox Mobile Print Portal",
  "short_name": "Xerox Mobile Print Portal",
  "description": "",
  "version": "1.1.0",
  "minimum_chrome_version": "44",
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "jquery/jquery-1.11.3.js",
        "jquery/angular.min.js",
        "lib/commlib.js",
        "lib/xrxlocale.js",
        "lib/mobileprint.js"
      ]
    }
  },
  "permissions": [
    "printerProvider",
    "app.window.alwaysOnTop",
    "storage",
    "fileSystem",
    "fileSystem.write",
    "contextMenus",
    "webview",
    "https://*/*"
  ],
  "icons": {
    "16": "assets/App_128.png",
    "48": "assets/App_128.png",
    "128": "assets/App_128.png"
  },
  "storage": {
    "managed_schema": "schema.json"
  }
}
