{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bayside Sniper II Chrome Extension",
  "description": "This extension enables users of the Bayside Sniper II program to add eBay auction listings from the viewed listing page.",
  "version": "1.2",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Bayside Sniper",
    "default_icon": "bs2_19.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "bs2_128.png"
  }
}
