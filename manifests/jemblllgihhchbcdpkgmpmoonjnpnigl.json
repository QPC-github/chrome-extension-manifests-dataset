{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wakfupedia++",
  "version": "0.0.0.4",
  "content_scripts": [
    {
      "matches": [
        "https://www.wakfu.com/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "dom-change-listener.js",
        "wildcard-match.js",
        "tooltip-fix.js",
        "filter-fix.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
