{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video Lecture Helper",
  "description": "Video Lecture Helper",
  "version": "1.1",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Video Lecture Helper",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
