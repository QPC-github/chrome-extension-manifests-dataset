{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "'unsafe-eval';script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "name": "HBCU Counter",
  "description": "RETOOL Your School Vote counter for the top HBCU's!",
  "version": "2.0.0",
  "permissions": [
    "storage",
    "http://www.spencersims.com/hbcuhd/general.php",
    "https://ssl.google-analytics.com/ga.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "128": "hbculogo.png"
  }
}
