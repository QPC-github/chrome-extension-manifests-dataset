{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Instant Search",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Speeds up your searches on many websites through the use of custom shortcut words and using context-menu.",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "homepage_url": "http://instantsearchextension.com/",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "config.html",
    "configured.html",
    "options.html",
    "jquery-1.8.1.min.js",
    "sites.js"
  ]
}
