{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Price Sheet Processor",
  "description": "This extension will process Google Shopping results into a printable price sheet!",
  "version": "1.1",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://www.google.com/*",
    "http://www.google.com/*"
  ],
  "web_accessible_resources": [
    "PriceLabel.jpg"
  ]
}
