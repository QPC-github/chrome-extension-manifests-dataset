{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IdFinder",
  "version": "1.8",
  "manifest_version": 2,
  "description": "It's unique chrome extension that get all id in a webpage and show them in a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
