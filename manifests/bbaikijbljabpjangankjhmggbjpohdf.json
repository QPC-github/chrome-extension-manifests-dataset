{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FAST LOGIN - Aged Acc For Sale Store",
  "description": "One click login to facebook using cookie in clipboard",
  "homepage_url": "https://selly.gg/@AgedAccStore",
  "author": "Nguyen Duc Tuan",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "48": "img/icon_48.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/",
    "https://*/",
    "clipboardWrite",
    "clipboardRead",
    "notifications",
    "declarativeContent",
    "activeTab"
  ],
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Login with cookie - Aged account store"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
