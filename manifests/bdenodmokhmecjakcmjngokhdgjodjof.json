{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "shopshopme",
  "manifest_version": 2,
  "short_name": "shopshopmenetwork",
  "description": "Shop Shop Me an online ecommerce store with hundreds of items all linked to top online product stores for easy comparison",
  "version": "1.1",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_1047-1503474070.png"
  },
  "icons": {
    "128": "128px_1047-1503474130.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
