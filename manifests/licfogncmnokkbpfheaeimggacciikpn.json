{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Travelling",
  "version": "1.6",
  "default_locale": "en",
  "icons": {
    "16": "images/Aviasales-Jetradar-Hotellook16.png",
    "48": "images/Aviasales-Jetradar-Hotellook48.png",
    "128": "images/Aviasales-Jetradar-Hotellook128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/Aviasales-Jetradar-Hotellook19.png",
      "38": "images/Aviasales-Jetradar-Hotellook38.png"
    },
    "default_title": "__MSG_name__",
    "default_popup": "forms.html"
  },
  "description": "__MSG_description__",
  "background": {
    "scripts": [
      "script/event.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "geolocation"
  ],
  "content_security_policy": "script-src 'self' https://autocomplete.travelpayouts.com https://engine.hotellook.com; object-src 'self'"
}
