{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remember Me",
  "description": "Plays a sample of \"Remember Me\" by Blue Boy when a \"Remember me\" checkbox on a login form is clicked",
  "version": "0.3",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "content_script.js"
      ]
    }
  ],
  "manifest_version": 2
}
