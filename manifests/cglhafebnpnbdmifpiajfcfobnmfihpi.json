{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "description": "The best news.",
  "manifest_version": 2,
  "name": "News Tab",
  "permissions": [
    "alarms",
    "storage",
    "https://nypost.com/news/feed/",
    "https://nypost.com/sports/feed/",
    "https://nypost.com/business/feed/",
    "https://nypost.com/fashion/feed/",
    "https://nypost.com/tech/feed/",
    "https://thenypost.files.wordpress.com/*",
    "https://nyppagesix.files.wordpress.com/*"
  ],
  "short_name": "News Tab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2.2",
  "version_name": "0.2.2"
}
