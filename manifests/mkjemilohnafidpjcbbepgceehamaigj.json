{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Swtich Tab2Window",
  "description": "Swtich tabs from window to window",
  "version": "1.1",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
