{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One Click Email",
  "short_name": "1Click Email",
  "description": "Create a temporary email in one click.",
  "version": "0.4",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://10minutemail.com/",
    "clipboardWrite"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
