{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "易收藏",
  "description": "过右键菜单添加当前页面或链接来保存到易收藏，以便于页面的浏览收藏。",
  "version": "2.1.4",
  "permissions": [
    "notifications",
    "contextMenus",
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "/js/jquery-1.10.2.min.js",
      "/js/contextMenu.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon128.png"
  }
}
