{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Search Information in Freebase",
  "manifest_version": 2,
  "description": "Opens the selected text as keyword in a popup",
  "version": "0.2",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "rightclick.js"
    ]
  },
  "icons": {
    "128": "logo128.png",
    "48": "logo48.png",
    "19": "logo19.png",
    "16": "logo16.png"
  },
  "browser_action": {
    "default_icon": "logo19.png"
  }
}
