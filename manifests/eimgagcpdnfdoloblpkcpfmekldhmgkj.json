{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ebola: Mass Hysteria",
  "description": "This extension Cures Ebola",
  "version": "1.1",
  "icons": {
    "16": "mass16.jpg",
    "48": "mass48.jpg",
    "128": "mass128.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "jquery-2.1.1.min.js",
        "ebola.js"
      ]
    }
  ]
}
