{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitch extension : Youtube Now Playing",
  "description": "This extension is working with twitch extension Youtube Now Playing",
  "version": "3.1",
  "background": {
    "scripts": [
      "thirdParty/jquery-3.2.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
