{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mail.Ru Регистратор полуавтомат",
  "description": "Помогает регистрировать аккаунты mail.ru",
  "version": "0.0.0.1",
  "permissions": [
    "storage",
    "*://mail.ru/*",
    "*://e.mail.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://mail.ru/*",
        "*://account.mail.ru/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_start"
    }
  ]
}
