{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random Lists",
  "description": "Rastgele müzik, video, gif, film, dizi, oyun öneren bir eklenti aracıdır.",
  "version": "0.0.0.1",
  "author": "Nullovy",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Random Lists",
    "content_scripts": [
      {
        "css": [
          "css/index.css"
        ],
        "js": [
          "js/oneri.js"
        ]
      }
    ]
  },
  "permissions": [
    "https://www.randomlists.com/*",
    "https://www.youtube.com/*"
  ],
  "icons": {
    "128": "icon.png",
    "256": "icon-256.png"
  }
}
