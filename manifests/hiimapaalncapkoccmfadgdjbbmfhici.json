{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RAGE MP master list",
  "version": "1.0.1",
  "description": "Lets you join RAGE MP servers easily.",
  "permissions": [
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/rage_16.png",
      "32": "images/rage_32.png",
      "48": "images/rage_48.png",
      "128": "images/rage_128.png"
    },
    "default_title": "View RAGE MP master list"
  },
  "icons": {
    "16": "images/rage_16.png",
    "32": "images/rage_32.png",
    "48": "images/rage_48.png",
    "128": "images/rage_128.png"
  }
}
