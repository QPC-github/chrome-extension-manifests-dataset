{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RateMyProf Quick Search",
  "short_name": "RMF Quick Search",
  "description": "Quicker and easier way to look up your profs.",
  "version": "0.0.4",
  "minimum_chrome_version": "38",
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "128": "assets/search.png"
  },
  "browser_action": {
    "default_icon": "assets/search.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  }
}
