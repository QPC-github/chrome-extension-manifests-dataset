{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Upload selected image to 3IH.RU",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "3IH.RU Uploader",
  "permissions": [
    "http://3ih.ru/*",
    "contextMenus",
    "clipboardWrite"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
