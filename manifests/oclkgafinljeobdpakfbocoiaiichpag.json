{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "page_action": {
    "default_icon": "16x16-3.png",
    "default_title": "",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "management",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "name": "World time",
  "description": "World time in your browser",
  "default_locale": "en",
  "icons": {
    "16": "16x16-3.png",
    "32": "32x32-3.png",
    "48": "48x48-3.png",
    "128": "128x128-3.png"
  },
  "manifest_version": 2,
  "version": "1.1.4"
}
