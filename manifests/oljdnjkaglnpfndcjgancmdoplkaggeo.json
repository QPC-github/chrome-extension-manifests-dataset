{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Palmer Studio Launcher",
  "description": "Easily launch content and more",
  "version": "0.0.2",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "permissions": [
    "activetab",
    "newtab"
  ]
}
