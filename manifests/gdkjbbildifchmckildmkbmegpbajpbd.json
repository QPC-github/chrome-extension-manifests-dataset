{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Behind the Code - Desafio 2 - FIAP",
  "version": "1.0",
  "description": "Menos cliques por favor...",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "https://discovery-us-east.watsonplatform.net/*"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/jquery-3.4.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-3.4.1.min.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/behind_the_code_16.png",
      "32": "images/behind_the_code_32.png",
      "48": "images/behind_the_code_48.png",
      "128": "images/behind_the_code_128.png"
    }
  },
  "icons": {
    "16": "images/behind_the_code_16.png",
    "32": "images/behind_the_code_32.png",
    "48": "images/behind_the_code_48.png",
    "128": "images/behind_the_code_128.png"
  }
}
