{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iKontacts for WhatsApp",
  "short_name": "iKontacts",
  "version": "1.1",
  "description": "Export unknown numbers from WhatsApp",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "downloads",
    "tabs",
    "*://*.whatsapp.com/*",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "iKontacts"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
