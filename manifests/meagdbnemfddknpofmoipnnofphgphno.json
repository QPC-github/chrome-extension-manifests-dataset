{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_settings_overrides": {
    "search_provider": {
      "favicon_url": "https://myfirsttab.com/favicon.ico",
      "is_default": true,
      "keyword": "Watch Series – MediaTabTV",
      "name": "Watch Series – MediaTabTV",
      "search_url": "https://myfirsttab.com/api/search-redirect?aid=720&sid=710&t={searchTerms}",
      "encoding": "UTF-8"
    }
  },
  "description": "Watch Series - MediaTabTV Search allows you to search for your favourite titles right from the Chrome New Tab and Search Bar",
  "icons": {
    "128": "images/icons/icon128.png",
    "16": "images/icons/icon16.png",
    "32": "images/icons/icon32.png",
    "64": "images/icons/icon64.png"
  },
  "manifest_version": 2,
  "name": "Watch Series – MediaTabTV",
  "omnibox": {
    "keyword": "browse"
  },
  "browser_action": {
    "default_icon": "images/icons/icon32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://*/*"
  ],
  "version": "1.8.7.6",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "http://*.dwatchseries.to/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
