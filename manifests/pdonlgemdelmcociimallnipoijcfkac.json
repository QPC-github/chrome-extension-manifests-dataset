{
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    }
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Send web page to Instapaper using its bookmarklet",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "name": "InstapaperBookmarklet",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "version": "1.2"
}
