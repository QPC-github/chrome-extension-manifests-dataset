{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Hell",
  "description": "__MSG_extDescription__",
  "version": "0.1.2",
  "browser_action": {
    "default_icon": "tabHell.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  "options_page": "options.html",
  "default_locale": "en"
}
