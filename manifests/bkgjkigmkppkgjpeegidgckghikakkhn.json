{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My IP",
  "version": "1.1",
  "description": "Shows instantly your public ip adress",
  "permissions": [
    "notifications"
  ],
  "browser_action": {
    "default_icon": "myip16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "myip16.png",
    "48": "myip48.png",
    "128": "myip128.png"
  }
}
