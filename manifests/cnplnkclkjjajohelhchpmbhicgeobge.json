{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WhatIsLirikPlaying",
  "description": "This extension will notify you when LIRIK changes games.",
  "version": "1.0",
  "icons": {
    "56": "56.png",
    "112": "112.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "112.png"
  },
  "permissions": [
    "storage",
    "notifications"
  ]
}
