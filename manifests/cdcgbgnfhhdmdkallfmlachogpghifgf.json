{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JavaScript Toggle On and Off",
  "description": "__MSG_app_description__",
  "author": "Richard Neomy",
  "version": "0.2.3",
  "manifest_version": 2,
  "default_locale": "en",
  "permissions": [
    "storage",
    "contentSettings",
    "contextMenus",
    "activeTab",
    "notifications"
  ],
  "browser_action": {
    "default_icon": {
      "19": "data/icons/19.png",
      "38": "data/icons/38.png"
    }
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "homepage_url": "https://add0n.com/javascript-toggler.html",
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  }
}
