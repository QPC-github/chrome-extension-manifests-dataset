{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gold Rate in Pakistan Today",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "37.0.0.0",
  "homepage_url": "http://goldratepakistan.today/",
  "description": "Live Gold Rate and Price in Tolla and 10 Grams from GoldRatePakistan.today",
  "browser_action": {
    "default_action": "img/128.png",
    "default_popup": "mypopup.html",
    "default_title": "Gold Rate in Pakistan Today Live & News"
  },
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "http://goldratepakistan.today/*"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "pages": [
      "background.html"
    ]
  }
}
