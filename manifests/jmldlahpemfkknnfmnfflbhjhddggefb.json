{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lowest Priced Book",
  "version": "0.0.0.192",
  "manifest_version": 2,
  "description": "Automatically find the best prices on books as you browse! Search hundreds of online book retailers in a flash",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "background.js",
        "add-content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://fonts.googleapis.com/*; object-src 'self'",
  "web_accessible_resources": [
    "fonts/Roboto-Bold.woff",
    "fonts/Roboto-Italic.woff",
    "fonts/Roboto-Medium.woff",
    "fonts/Roboto-Regular.woff",
    "fonts/RobotoSlab-Bold.woff",
    "images/collapse.svg",
    "images/close.svg",
    "images/amazon.svg",
    "images/indigo.svg",
    "images/barnes-and-noble.svg",
    "images/ebay.svg",
    "images/vital.svg",
    "images/bam.svg",
    "images/abebooks.svg",
    "images/ebooks.png",
    "images/biblio.png",
    "images/ecampus.svg",
    "images/valore.svg",
    "images/betterWorld.png",
    "images/oreilly.png",
    "images/expand.png",
    "images/powell.png"
  ]
}
