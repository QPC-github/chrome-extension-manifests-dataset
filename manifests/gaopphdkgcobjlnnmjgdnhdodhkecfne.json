{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZSC.co.il",
  "description": "ZSC",
  "version": "1.5",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "jquery/jquery.min.js",
      "notifications/notifications.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "activeTab",
    "downloads",
    "http://www.zsc.co.il/index.php/rss/forums/1-zsc-rss/"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options/options.html"
}
