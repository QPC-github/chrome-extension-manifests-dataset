{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MACU",
  "description": "This extension tracs web request.",
  "version": "1.0",
  "permissions": [
    "http://*/",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup1.html",
    "default_icon": "downloads.png"
  }
}
