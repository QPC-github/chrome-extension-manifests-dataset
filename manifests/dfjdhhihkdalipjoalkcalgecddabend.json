{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Blacklist",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Automatically closes blacklisted tabs.",
  "icons": {
    "16": "icons/tbl16.png",
    "48": "icons/tbl48.png",
    "128": "icons/tbl128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "options_page": "html/options.html"
}
