{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yakkyofy",
  "description": "Yakkyofy's Chrome Extension",
  "version": "4.0.6",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "//_page": "background.html",
    "persistent": true
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "notifications",
    "gcm"
  ],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://www.googletagmanager.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "web_accessible_resources": [
    "assets/*.png",
    "assets/*.svg",
    "scripts/*.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.aliexpress.com/item/*",
        "https://*.aliexpress.com/store/product/*",
        "https://*.aliexpress.com/aeglodetailweb/*",
        "https://aliexpress.com/item/*",
        "https://aliexpress.com/store/product/*",
        "https://aliexpress.com/aeglodetailweb/*"
      ],
      "js": [
        "js/aliexpress.js"
      ]
    },
    {
      "matches": [
        "https://alibaba.com/product-detail/*",
        "https://alibaba.com/product/*",
        "https://*.alibaba.com/product-detail/*",
        "https://*.alibaba.com/product/*"
      ],
      "js": [
        "js/alibaba.js"
      ]
    },
    {
      "matches": [
        "https://amazon.co.uk/dp/*",
        "https://amazon.co.uk/*/dp/*",
        "https://amazon.com/dp/*",
        "https://amazon.com/*/dp/*",
        "https://amazon.com.au/dp/*",
        "https://amazon.com.au/*/dp/*",
        "https://amazon.com.br/dp/*",
        "https://amazon.com.br/*/dp/*",
        "https://amazon.com.mx/dp/*",
        "https://amazon.com.mx/*/dp/*",
        "https://amazon.com.tr/dp/*",
        "https://amazon.com.tr/*/dp/*",
        "https://amazon.ae/dp/*",
        "https://amazon.ae/*/dp/*",
        "https://amazon.ca/dp/*",
        "https://amazon.ca/*/dp/*",
        "https://amazon.de/dp/*",
        "https://amazon.de/*/dp/*",
        "https://amazon.es/dp/*",
        "https://amazon.es/*/dp/*",
        "https://amazon.fr/dp/*",
        "https://amazon.fr/*/dp/*",
        "https://amazon.it/dp/*",
        "https://amazon.it/*/dp/*",
        "https://amazon.in/dp/*",
        "https://amazon.in/*/dp/*",
        "https://amazon.nl/dp/*",
        "https://amazon.nl/*/dp/*",
        "https://amazon.sg/dp/*",
        "https://amazon.sg/*/dp/*",
        "https://*.amazon.co.uk/dp/*",
        "https://*.amazon.co.uk/*/dp/*",
        "https://*.amazon.com/dp/*",
        "https://*.amazon.com/*/dp/*",
        "https://*.amazon.com.au/dp/*",
        "https://*.amazon.com.au/*/dp/*",
        "https://*.amazon.com.br/dp/*",
        "https://*.amazon.com.br/*/dp/*",
        "https://*.amazon.com.mx/dp/*",
        "https://*.amazon.com.mx/*/dp/*",
        "https://*.amazon.com.tr/dp/*",
        "https://*.amazon.com.tr/*/dp/*",
        "https://*.amazon.ae/dp/*",
        "https://*.amazon.ae/*/dp/*",
        "https://*.amazon.ca/dp/*",
        "https://*.amazon.ca/*/dp/*",
        "https://*.amazon.de/dp/*",
        "https://*.amazon.de/*/dp/*",
        "https://*.amazon.es/dp/*",
        "https://*.amazon.es/*/dp/*",
        "https://*.amazon.fr/dp/*",
        "https://*.amazon.fr/*/dp/*",
        "https://*.amazon.it/dp/*",
        "https://*.amazon.it/*/dp/*",
        "https://*.amazon.in/dp/*",
        "https://*.amazon.in/*/dp/*",
        "https://*.amazon.nl/dp/*",
        "https://*.amazon.nl/*/dp/*",
        "https://*.amazon.sg/dp/*",
        "https://*.amazon.sg/*/dp/*"
      ],
      "js": [
        "js/amazon.js"
      ]
    },
    {
      "matches": [
        "https://wish.com/*",
        "https://*.wish.com/*"
      ],
      "js": [
        "js/wish.js"
      ]
    },
    {
      "matches": [
        "https://gearbest.com/*",
        "https://*.gearbest.com/*"
      ],
      "js": [
        "js/gearbest.js"
      ]
    },
    {
      "matches": [
        "https://banggood.com/*",
        "https://*.banggood.com/*"
      ],
      "js": [
        "js/banggood.js"
      ]
    }
  ]
}
