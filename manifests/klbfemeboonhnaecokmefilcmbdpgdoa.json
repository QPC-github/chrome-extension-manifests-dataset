{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tedioso.com",
  "description": "Os melhores links da semana",
  "version": "1.0.0.1",
  "browser_action": {
    "default_icon": "tedioso-logo-19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "19": "tedioso-logo-19.png",
    "48": "tedioso-logo-48.png",
    "128": "tedioso-logo-128.png"
  },
  "permissions": [
    "http://chrome-extension.tedioso.com/",
    "background",
    "tabs",
    "contextMenus",
    "clipboardWrite"
  ]
}
