{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qrcode URL",
  "version": "1.0.2",
  "description": "QR CODES CREATION",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "128": "images/icon-128.png"
    }
  },
  "permissions": [
    "activeTab",
    "declarativeContent"
  ]
}
