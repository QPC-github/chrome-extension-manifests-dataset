{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Check youTube links",
  "version": "1.3",
  "author": "ak400400@gmail.com",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.co.il/*",
        "https://www.google.com/*"
      ],
      "js": [
        "jquery-3.0.0.min.js",
        "check-links.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
