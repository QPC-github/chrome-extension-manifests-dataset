{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mnet Assistant",
  "description": "Make voting easier.",
  "version": "8",
  "page_action": {
    "default_icon": "icon_128.png",
    "default_title": "Imevotebot Content Script"
  },
  "background": {
    "scripts": [
      "jquery-2.1.4.min.js",
      "imevotebot_backgroundscript.js"
    ]
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*.interest.me/",
    "https://user.interest.me/",
    "http://*/*/"
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.interest.me/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "jquery.cookie.js",
        "sprintf-0.7-beta1.js",
        "imevotebot_contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://www.google.com https://www.gstatic.com https://apis.google.com; object-src 'self'"
}
