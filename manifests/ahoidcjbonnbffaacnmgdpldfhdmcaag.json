{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BingoNinja",
  "version": "1.3",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.bing.com/*",
        "https://www.graphiq.com/semantics*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "bing.js"
      ]
    }
  ]
}
