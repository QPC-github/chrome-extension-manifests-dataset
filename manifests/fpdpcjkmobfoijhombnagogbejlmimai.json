{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DefaultLinks",
  "short_name": "DefaultLinks",
  "description": "DefaultLinks provides a quick way to refine your search when you donâ€™t find what your looking for. ",
  "version": "2.5.1",
  "permissions": [
    "tabs",
    "<all_urls>",
    "management"
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "checkUninstall.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
