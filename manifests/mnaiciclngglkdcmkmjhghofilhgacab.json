{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "url2qr",
  "description": "This extension converts an url of your current tab to a QR code.",
  "version": "0.2.1",
  "permissions": [
    "activeTab",
    "https://api.weibo.com/"
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
