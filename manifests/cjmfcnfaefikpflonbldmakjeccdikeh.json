{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rob's Chrome Extension",
  "version": "1.0.3",
  "description": "Chrome browser extension to highlight how synchronised browsers could attack corporate environments.",
  "manifest_version": 2,
  "author": "Rob Leadbeater",
  "short_name": "Rob's Chrome Extension",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Rob's Chrome Extension"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
