{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Victory No More",
  "description": "Victory for Satan no more!  Replace \"Mormon\" nickname with the official name: The Church of Jesus Christ of Latter-day Saints.",
  "version": "1.1",
  "icons": {
    "16": "images/vnm_icon16.png",
    "32": "images/vnm_icon32.png",
    "48": "images/vnm_icon48.png",
    "128": "images/vnm_icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
