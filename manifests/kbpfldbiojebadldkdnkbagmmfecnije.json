{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "陆金所新项目提示器",
  "description": "实时通知陆金所投资人新投资项目上线信息，为抢标成功赢得先机！",
  "version": "0.1",
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js",
      "js/jbeep.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*.lufax.com/*"
  ],
  "page_action": {
    "default_icon": "image/coin-16.png",
    "default_title": "陆金所新项目提示器"
  },
  "icons": {
    "16": "image/coin-16.png",
    "48": "image/coin-48.png",
    "128": "image/coin-128.png"
  }
}
