{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sly Dashboard",
  "version": "2.0.2",
  "author": "Matteo Padovan",
  "manifest_version": 2,
  "description": "Do more on new tab with Sly.",
  "icons": {
    "16": "img/logos/scaled16.png",
    "48": "img/logos/scaled48.png",
    "128": "img/logos/scaled128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "topSites",
    "activeTab",
    "geolocation",
    "webNavigation",
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ],
    "persistent": false
  }
}
