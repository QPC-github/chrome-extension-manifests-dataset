{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Googol Modifier",
  "version": "2.0",
  "description": "Change Google's logo to anything you want",
  "icons": {
    "128": "ico/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/c.js"
      ],
      "matches": [
        "*://*.google.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "a.html"
  },
  "permissions": [
    "storage"
  ]
}
