{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "QuikShre",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Creates QR code from URL of current tab.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "quikshre.html"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
