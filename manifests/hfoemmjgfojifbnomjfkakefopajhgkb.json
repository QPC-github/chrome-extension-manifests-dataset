{
  "name": "__MSG_extension_name__",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "__MSG_extension_desc__",
  "homepage_url": "http://www.zappingmag.com/",
  "default_locale": "en",
  "icons": {
    "16": "images/16.png",
    "38": "images/38.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_": {
      "16": "images/16.png",
      "38": "images/38.png"
    },
    "default_title": "__MSG_extension_name__ | __MSG_extension_desc__",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "notifications",
    "tabs",
    "history",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "author": "Bassem Rabia",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
