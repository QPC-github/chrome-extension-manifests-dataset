{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ID9k creator",
  "version": "1.0.5",
  "description": "quickly create an ID9k link to pages.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "page_action": {
    "default_icon": "id9k19.png",
    "default_title": "Create ID9k Link to this page"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "id9k16.png",
    "48": "id9klogo48.png",
    "128": "id9klogo128.png"
  }
}
