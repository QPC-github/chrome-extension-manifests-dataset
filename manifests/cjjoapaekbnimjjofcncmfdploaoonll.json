{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_application_title__",
  "version": "1.0.9",
  "default_locale": "en_US",
  "manifest_version": 2,
  "description": "__MSG_application_description__",
  "container": [
    "GOOGLE_DRIVE"
  ],
  "offline_enabled": true,
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "audio/mpeg3",
          "audio/x-mpeg-3",
          "audio/mp3",
          "audio/mpeg",
          "audio/mp4",
          "audio/mpg",
          "audio/mp4a-latm",
          "audio/ogg",
          "application/ogg",
          "audio/webm",
          "audio/wav",
          "audio/x-wav",
          "audio/wave",
          "audio/x-flv",
          "audio/x-flac",
          "video/x-mpeg",
          "video/webm",
          "video/x-flv",
          "application/vnd.google.drive.ext-type.mpeg3",
          "application/vnd.google.drive.ext-type.mp3",
          "application/vnd.google.drive.ext-type.mpeg",
          "application/vnd.google.drive.ext-type.mp4",
          "application/vnd.google.drive.ext-type.mpg",
          "application/vnd.google.drive.ext-type.ogg",
          "application/vnd.google.drive.ext-type.wav",
          "application/vnd.google.drive.ext-type.wave",
          "application/vnd.google.drive.ext-type.webm"
        ],
        "href": "https://free-music.freebusinessapps.net/music",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "512": "icon-512.png",
    "256": "icon-256.png",
    "128": "icon-128.png",
    "96": "icon-96.png",
    "64": "icon-64.png",
    "48": "icon-48.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "browser_action": {
    "default_icon": {
      "512": "icon-512.png",
      "256": "icon-256.png",
      "128": "icon-128.png",
      "96": "icon-96.png",
      "64": "icon-64.png",
      "48": "icon-48.png",
      "32": "icon-32.png",
      "16": "icon-16.png"
    }
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "https://*.freebusinessapps.net/*"
  ],
  "background": {
    "scripts": [
      "/notify/update-conf.js",
      "/notify/update.min.js",
      "/notify/background.js"
    ]
  }
}
