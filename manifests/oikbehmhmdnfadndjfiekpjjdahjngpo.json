{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Send to My Phone",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Send links to your phone",
  "options_page": "options/options.html",
  "background": {
    "scripts": [
      "js/jquery-1.7.2.min.js",
      "app/app.js"
    ]
  },
  "browser_action": {
    "default_icon": "imgs/icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "http://localhost/*",
    "http://sendtomyphone.no-ip.org/*"
  ]
}
