{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stocks",
  "description": "Stocks",
  "version": "0.0.5",
  "icons": {
    "16": "images/dollar_16.png",
    "32": "images/dollar_32.png",
    "48": "images/dollar_48.png",
    "64": "images/dollar_64.png",
    "128": "images/dollar_128.png"
  },
  "content_security_policy": "script-src 'self' https://s3.tradingview.com; object-src 'self'",
  "permissions": [
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/dollar_16.png",
      "32": "images/dollar_32.png",
      "48": "images/dollar_48.png",
      "64": "images/dollar_64.png",
      "128": "images/dollar_128.png"
    },
    "default_title": "Курсы",
    "default_popup": "popup/popup.html"
  }
}
