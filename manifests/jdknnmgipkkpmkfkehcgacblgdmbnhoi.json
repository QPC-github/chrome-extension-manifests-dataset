{
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "1.0",
  "version_name": "1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "icons/24.png"
  },
  "icons": {
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "browsingData",
    "storage",
    "unlimitedStorage",
    "cookies",
    "https://topaddon.com/*",
    "http://topaddon.com/*",
    "https://www.topaddon.com/*",
    "https://duckduckgo.com/*",
    "https://www.bing.com/*",
    "https://www.yahoo.com/*",
    "https://www.google.com/*",
    "https://www.google.fr/*",
    "https://www.google.co.in/*",
    "https://www.google.pt/*",
    "https://www.google.ca/*",
    "https://www.google.es/*",
    "https://www.google.de/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "persistent": true,
    "scripts": [
      "js/http.js",
      "js/background.js"
    ]
  }
}
