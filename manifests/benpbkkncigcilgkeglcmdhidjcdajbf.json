{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Piezas y recambios de coche Online",
  "version": "1.0",
  "description": "Descubre todas las ofertas en piezas de recambios de nuestra web de Desguaces Melli",
  "icons": {
    "48": "img/48x48.png",
    "16": "img/16X16.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/19X19.png",
    "default_title": "Pulsa para ver mas opciones",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
