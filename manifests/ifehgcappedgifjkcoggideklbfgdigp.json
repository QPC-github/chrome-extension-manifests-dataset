{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SEO Analysis and Suggestions by Seotify.com",
  "version": "3.0",
  "description": "SEO extension that brings TF-IDF, word count and questions you should cover to your browser.",
  "permissions": [
    "activeTab",
    "storage",
    "*://*/*",
    "<all_urls>",
    "https://ajax.googleapis.com/*",
    "https://stackpath.bootstrapcdn.com/*",
    "https://cdnjs.cloudflare.com/*",
    "https://seotify.com/",
    "https://*.seotify.com/v1/",
    "http://*.seotify.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "assets/jquery-3.3.1.min.js",
        "assets/Chart.min.js",
        "wordpress/alert.js",
        "serp/google-serp-content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "wordpress/frame.html",
    "serp/google-frame.html"
  ],
  "background": {
    "scripts": [
      "assets/jquery-3.3.1.min.js",
      "assets/Chart.min.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "128": "images/128.png"
  },
  "browser_action": {},
  "manifest_version": 2
}
