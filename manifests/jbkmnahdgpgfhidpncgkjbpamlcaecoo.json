{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "imesetup.js",
      "imecore.js"
    ]
  },
  "description": "Chrome OS only. ローカルで動く日本語 IME です。かな漢字変換エンジンが WebAssembly で動いています。",
  "homepage_url": "https://plaza.rakuten.co.jp/millimo/",
  "icons": {
    "128": "icon128.png"
  },
  "input_components": [
    {
      "description": "Wasm IME (technical experiment)",
      "id": "wasm_japanese_ime",
      "language": "ja",
      "layouts": [
        "us::eng"
      ],
      "name": "Wasm Japanese IME",
      "type": "ime"
    }
  ],
  "manifest_version": 2,
  "name": "Wasm IME (technical experiment)",
  "short_name": "Wasm IME",
  "permissions": [
    "input"
  ],
  "version": "0.0.0.14",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
