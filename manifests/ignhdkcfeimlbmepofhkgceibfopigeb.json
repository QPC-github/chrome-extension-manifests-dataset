{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jamify",
  "version": "0.2.3",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "main.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Jamify",
    "default_popup": "popup.html"
  }
}
