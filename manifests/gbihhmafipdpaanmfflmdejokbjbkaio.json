{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Anti-Netflix",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Can't control your Netflix Binges? Use the Anti-Netflix extensions to better manage how much time you spend on Netflix",
  "page_action": {
    "default_icon": "netflix.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "management"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "antinetflix.png",
    "48": "antinetflix.png",
    "128": "antinetflix.png"
  }
}
