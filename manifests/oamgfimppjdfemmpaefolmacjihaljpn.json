{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Invert all the colors on the page.",
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "name": "Pages Colors Invert ",
  "page_action": {
    "default_icon": "icons/disabled.png",
    "default_title": "Invert Page Colors"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
