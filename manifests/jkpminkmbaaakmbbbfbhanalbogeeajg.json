{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Noisy tabs",
  "description": "Finds tabs that play sound",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
