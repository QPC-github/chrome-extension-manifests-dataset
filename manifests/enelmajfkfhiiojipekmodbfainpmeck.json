{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Minimal Search Term Extension",
  "version": "1.1",
  "description": "Finds a term and display only text that match specific term.",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "search.png",
    "48": "search.png",
    "128": "search.png"
  },
  "browser_action": {
    "default_popup": "src/popup.html"
  },
  "manifest_version": 2
}
