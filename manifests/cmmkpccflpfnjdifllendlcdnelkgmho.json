{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bonjour Madame",
  "description": "Le plaisir de decouvrir chaque jour un nouveau plaisir",
  "version": "0.9.4",
  "icons": {
    "16": "icon.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://feeds.feedburner.com/*",
    "history",
    "storage",
    "http://www.bonjourmadame.fr/*",
    "http://25.media.tumblr.com/*"
  ],
  "homepage_url": "http://www.bonjourmadame.fr",
  "background": {
    "scripts": [
      "scripts/back.js",
      "scripts/jquery-1.6.1.min.js",
      "scripts/parse.js"
    ]
  },
  "web_accessible_resources": [
    "icon_48.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.bonjourmadame.fr/*"
      ],
      "js": [
        "scripts/jquery-1.6.1.min.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Bonjour Madame",
    "default_popup": "popup _feed.html"
  }
}
