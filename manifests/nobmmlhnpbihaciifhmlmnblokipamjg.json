{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Phrasal Verb Frenzy",
  "version": "1.0.0.5",
  "manifest_version": 2,
  "homepage_url": "https://singleclickapps.com/phrasal-verb-new-tab/",
  "description": "Learn a new english phrasal verb each time you open a new tab.",
  "icons": {
    "16": "24.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "offline_enabled": true,
  "browser_action": {
    "default_icon": "24.png",
    "default_title": "New Tab"
  },
  "incognito": "split",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "settings.html"
}
