{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabjab",
  "version": "0.2.0",
  "description": "Bookmark, tag, and add notes to web pages.",
  "homepage_url": "https://tabjab.com/",
  "icons": {
    "48": "icons/xfer-48.png"
  },
  "permissions": [
    "activeTab",
    "https://tabjab.com/"
  ],
  "content_security_policy": "script-src 'self' https://tabjab.com/; object-src 'self'; unsafe-inline",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/xfer-48.png",
    "default_title": "Bookmark on Tabjab",
    "default_popup": "popup/bookmark.html"
  },
  "applications": {
    "gecko": {
      "id": "{e4074843-a717-43c6-8e72-a5f6a6ccbb03}"
    }
  }
}
