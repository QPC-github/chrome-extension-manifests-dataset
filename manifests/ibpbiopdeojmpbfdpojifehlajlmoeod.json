{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gamepedia SBR",
  "version": "0.1",
  "permissions": [
    "*://*.gamepedia.com/*"
  ],
  "description": "Removes Gamepedia's ad sidebar that hogs up the right side of the screen.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.gamepedia.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
