{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "#FreeBillSimmons",
  "version": "0.0.0.6",
  "description": "Use this chrome plugin to boycott ESPN until Bill Simmons return",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://zencap.co; object-src 'self' https://zencap.co",
  "permissions": [
    "http://zencap.co/*",
    "https://zencap.co/*"
  ],
  "icons": {
    "48": "zencap_logo.png",
    "128": "zencap_logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "jquery-ui.js",
        "bootstrap.js",
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
