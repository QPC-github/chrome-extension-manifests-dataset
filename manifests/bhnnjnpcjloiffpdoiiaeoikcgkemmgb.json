{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better Arbor Accessibility",
  "version": "0.1.2",
  "description": "Accessibility tweaks for Arbor Education MIS",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*.uk.arbor.sc/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "128.png"
  }
}
