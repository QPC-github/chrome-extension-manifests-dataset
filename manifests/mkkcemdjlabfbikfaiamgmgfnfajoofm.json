{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "External Links Remover",
  "version": "1.0",
  "description": "remove the external domain link of the site that you are looking at.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "External Links Remover"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon.png"
  },
  "background": {
    "page": "each.html"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
