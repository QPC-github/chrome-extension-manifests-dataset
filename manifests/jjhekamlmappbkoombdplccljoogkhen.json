{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Grey DeviantArt",
  "version": "1.0",
  "description": "Replaces the green styling on DeviantArt with something more nuetral.",
  "content_scripts": [
    {
      "matches": [
        "http://*.deviantart.com/*"
      ],
      "css": [
        "graystyle.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.deviantart.com/*"
  ],
  "icons": {
    "19": "icon-19.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
