{
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.7.min.js",
        "expandsvc.js",
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Shorten and expand URLs with TinyW.in and upload images and screenshots to Phota.me",
  "icons": {
    "16": "TWurl16.png",
    "32": "TWurl32.png",
    "48": "TWurl48.png",
    "128": "TWurl128.png"
  },
  "name": "TWurl Url Shortener/Expander with QR codes",
  "options_page": "options.html",
  "page_action": {
    "default_icon": "TWurl19.png",
    "default_title": "Shorten URL",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "TWurl48.png"
  ],
  "version": "2.2.0"
}
