{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Painting by Gusky for Google Background",
  "short_name": "Art-By-Gusky",
  "description": "Offbeat artworks to energize your Google Search page",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "main.css"
      ]
    }
  ],
  "page_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html",
    "default_title": "Art by Gusky"
  }
}
