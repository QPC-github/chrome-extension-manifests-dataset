{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "buzzQuoteList.js"
      ],
      "matches": [
        "https://www.buzzfeed.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Retreives all buzzfeed lists and displays in simple fashion",
  "icons": {
    "16": "16.png"
  },
  "manifest_version": 2,
  "name": "Buzzquotes Only",
  "version": "1"
}
