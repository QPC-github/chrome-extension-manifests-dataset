{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Daywatch.io Feed",
  "description": "Access our content easily with this extension for google chrome. Filter by Articles, Highlights of the weeks and Freebies",
  "version": "1.0.2",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "logo-daywatch-48.png",
    "48": "logo-daywatch-48.png",
    "128": "logo-daywatch-128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "permissions": [
    "storage",
    "alarms",
    "https://daywatch.io/"
  ]
}
