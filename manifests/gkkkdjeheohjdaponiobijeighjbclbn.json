{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "wiredwith sitexray",
  "description": "Wiredwith sitexray uncovers technologies used in a website employing detection methods for over 1500+ technologies & notifies trends",
  "version": "1.0.4",
  "short_name": "wiredwith sitexray",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "wiredwith sitexray",
    "default_popup": "pages/sitexray.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "webRequest",
    "<all_urls>"
  ]
}
