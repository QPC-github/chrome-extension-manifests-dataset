{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IEEE Search bar",
  "short_name": "IEEE Search bar",
  "description": "Search bar",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "IEEE Search Bar"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "32": "logo32.png",
    "128": "logo128.png"
  }
}
