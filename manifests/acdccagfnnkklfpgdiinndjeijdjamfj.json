{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QR code generator",
  "version": "0.0.3",
  "manifest_version": 2,
  "author": "Astrit Zeqiri",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "QR code generator",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
