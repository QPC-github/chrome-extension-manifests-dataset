{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Annuaire du Luxembourg Follow.lu",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Trouver rapidement un numéro de téléphone au Luxembourg",
  "browser_action": {
    "default_icon": "icon128x128.png",
    "default_title": "Follow.lu",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16x16.png",
    "48": "icon48x48.png",
    "128": "icon128x128.png"
  },
  "permissions": [
    "notifications",
    "http://apisearch.follow.lu/"
  ]
}
