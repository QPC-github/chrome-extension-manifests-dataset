{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CSS Grady",
  "description": "Generating CSS Gradients was never this easy,autogenerate multibrowser css3 code in a snap",
  "version": "1.5.1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "i_16.png",
    "64": "i_64.png",
    "128": "i_128.png"
  },
  "offline_enabled": true,
  "omnibox": {
    "keyword": "ctw"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "unlimited_storage",
    "notifications"
  ]
}
