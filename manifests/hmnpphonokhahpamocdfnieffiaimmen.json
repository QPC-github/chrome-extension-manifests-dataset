{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ContactOn by Opennetworks",
  "version": "6.3.012",
  "homepage_url": "http://www.opennetworks.com",
  "description": "ContactOn Sign-in",
  "icons": {
    "128": "chromeextensionicon-01.png"
  },
  "permissions": [
    "*://*.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "*.svg"
  ]
}
