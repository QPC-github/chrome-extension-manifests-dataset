{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "E-Leitor",
  "description": "",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.min.js",
        "underscore-min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "logo.png",
    "nexo-logo.png",
    "wideLarge.css",
    "medium.css",
    "mini.css",
    "mini.html",
    "medium.html",
    "wideLarge.html",
    "fonts.css",
    "jquery-3.3.1.min.js"
  ],
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png"
  },
  "permissions": [
    "debugger",
    "storage"
  ]
}
