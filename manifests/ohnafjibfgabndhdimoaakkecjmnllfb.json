{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Substantiator.com",
  "description": "Create and archive documentation for the R&D tax credit",
  "version": "1.0.4",
  "browser_action": {
    "default_icon": "if_tubes_no_shadow_128.png",
    "default_popup": "popup.html",
    "default_title": "Substantiator"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistant": true
  },
  "icons": {
    "16": "16_tubes.png",
    "48": "48_tubes.png",
    "128": "128_tubes.png"
  },
  "homepage_url": "http://substantiator.com",
  "permissions": [
    "identity",
    "identity.email",
    "https://www.rdsubstantiation.com/sub_crud/*.*",
    "https://www.rdsubstantiation.com/sub_crud/register/*",
    "https://www.rdsubstantiation.com/sub_crud/Subit_backend/*",
    "storage",
    "activeTab"
  ],
  "oauth2": {
    "client_id": "67311738279-7h3ai8jau72p8d473m19ld6jgsfu4r91.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
