{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_QUASAR_NAME__",
  "description": "__MSG_QUASAR_DESCRIPTION__",
  "default_locale": "zh_CN",
  "version": "5.0.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "48.png"
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "96": "96.png",
    "128": "128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "storage"
  ]
}
