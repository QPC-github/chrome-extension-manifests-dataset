{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CIS Enhancement Suite",
  "short_name": "CISes",
  "version": "1.0.30",
  "description": "A browser extension that incrementally improves on the University of Utah CIS experience.",
  "permissions": [
    "https://search-production.ratemyprofessors.com/",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://utah.collegescheduler.com/*"
      ],
      "js": [
        "college-scheduler.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
