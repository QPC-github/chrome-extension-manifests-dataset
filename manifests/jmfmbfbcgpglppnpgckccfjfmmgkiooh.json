{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seo Analyzer Beta",
  "description": "Analizza un sito tramite un semplice click",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
