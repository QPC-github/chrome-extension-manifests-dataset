{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BL Tool",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Shows BB IDs",
  "homepage_url": "http://www.bookinglab.co.uk",
  "background": {
    "scripts": [
      "javascripts/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "bookinglab Tool"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "web_accessible_resources": [
    "javascripts/inject.js"
  ],
  "permissions": [
    "https://*.bookingbug.com/*"
  ]
}
