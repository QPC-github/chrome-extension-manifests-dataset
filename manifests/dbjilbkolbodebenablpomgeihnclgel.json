{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "chinese pinyin",
  "description": "Add pinyin to chinese text.",
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
