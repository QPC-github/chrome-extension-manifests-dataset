{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cyber Driver",
  "description": "Don't have a driver's license yet? No worries. You can drive online without worrying about the police!",
  "version": "1.0.0.0",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "permissions": [
    "unlimitedStorage"
  ],
  "offline_enabled": true
}
