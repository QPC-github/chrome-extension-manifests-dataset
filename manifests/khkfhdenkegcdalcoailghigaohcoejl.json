{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MoeMaker",
  "description": "This extension makes politics super moe.",
  "version": "0.1",
  "icons": {
    "16": "moe.png",
    "48": "moe.png",
    "128": "moe.png"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ]
}
