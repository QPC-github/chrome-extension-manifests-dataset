{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Create QRCode, QR Code generator from link",
  "description": "Công cụ tạo mã QR Code từ link website, truy cập nhanh hơn từ điện thoại.",
  "version": "1.3.1",
  "browser_action": {
    "default_icon": "/images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Create QRCode from link - Ver 1.3.1"
  },
  "icons": {
    "16": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
