{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DNS Into",
  "short_name": "DNSINTO",
  "version": "1.0",
  "description": "Check DNS Report ",
  "icons": {
    "48": "img/48.png",
    "16": "img/16.png",
    "128": "img/128.png"
  },
  "browser_action": {
    "default_icon": "img/128.png",
    "default_title": "DNS INTO Report",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
