{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fanat.live",
  "description": "Live results",
  "version": "0.0.4",
  "browser_action": {
    "default_icon": "apple-icon-57x57.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "48": "apple-icon-57x57.png",
    "128": "apple-icon-180x180.png"
  },
  "permissions": [
    "<all_urls>"
  ]
}
