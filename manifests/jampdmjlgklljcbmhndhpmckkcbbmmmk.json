{
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "Search and Find any movie to watch or download online for free",
  "homepage_url": "https://tsearch.club",
  "icons": {
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "Movie Search Engine",
  "omnibox": {
    "keyword": "v"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "storage",
    "tabs",
    "https://*/*",
    "topSites",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "Movie Search Engine",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
