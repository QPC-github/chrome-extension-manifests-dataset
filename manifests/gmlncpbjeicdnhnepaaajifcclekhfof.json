{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "offline_enabled": false,
  "name": "Мои закладки",
  "version": "0.83",
  "description": "Храните все ваши закладки на my-tabs.ru. Быстрый доступ из любого браузера ",
  "omnibox": {
    "keyword": "trye"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "http://my-tabs.ru/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Мои закладки.",
    "default_icon": "img/favicon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
