{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GitHub Search",
  "version": "4.0",
  "description": "Search GitHub repos",
  "homepage_url": "https://github.com/ushis/github-search-chrome-extension",
  "icons": {
    "16": "icons/16.ico",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "omnibox": {
    "keyword": "gh"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "https://api.github.com/"
  ],
  "background": {
    "scripts": [
      "o.js",
      "github-search.js"
    ]
  }
}
