{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BetterTab",
  "description": "A new and improved new tab page. Includes better design and daily news for a better experience.",
  "version": "3.1.4",
  "author": "Aditeya Shukla",
  "browser_action": {
    "default_icon": "tab-icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "tab-icon.png"
  },
  "permissions": [
    "activeTab",
    "identity",
    "identity.email"
  ]
}
