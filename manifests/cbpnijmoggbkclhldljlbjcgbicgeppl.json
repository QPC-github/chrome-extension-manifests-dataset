{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fun gif",
  "description": "Get a fun gif for relaxing",
  "version": "1.2",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "see something"
  },
  "permissions": [
    "activeTab",
    "http://api.giphy.com/"
  ]
}
