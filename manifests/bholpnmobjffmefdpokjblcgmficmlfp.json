{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "E-learning",
  "description": "Day hoc online",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.11.2.min.js",
        "e-learning.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "<all_urls>"
  ]
}
