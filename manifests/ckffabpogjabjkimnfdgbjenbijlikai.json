{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SEOwarpAnalizBookmarklari",
  "version": "1.1.2",
  "description": "SEO Analizi İçin Gerekli Bazı Sitelerin Kısa Yolları",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>",
    "*://*/*"
  ],
  "browser_action": {
    "default_title": "SEOwarp Analiz Bookmarkları",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "popup.js"
      ]
    }
  ]
}
