{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Middlebury Go",
  "version": "1.2.2",
  "manifest_version": 2,
  "description": "A fix to use the Middlebury Go system from the Chrome omni bar. Type go-slash plus a search term into the omnibox.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://*/*",
    "webRequestBlocking"
  ]
}
