{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mPandanda",
  "version": "1.0.4",
  "description": "mPandanda's official plugin, making Flash enabled automatically during gameplay",
  "permissions": [
    "contentSettings",
    "tabs",
    "http://mpandanda.eu/*",
    "https://mpandanda.eu/*"
  ],
  "background": {
    "scripts": [
      "mpd.js"
    ],
    "persistent": false
  },
  "icons": {
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 2
}
