{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wander&Aspire",
  "chrome_url_overrides": {
    "newtab": "wander.html"
  },
  "version": "1.0.5",
  "manifest_version": 2,
  "web_accessible_resources": [
    "logo.png"
  ],
  "permissions": [
    "tabs",
    "notifications",
    "geolocation",
    "*://*.wander.am/*"
  ],
  "optional_permissions": [
    "geolocation"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:3000; object-src 'self'; "
}
