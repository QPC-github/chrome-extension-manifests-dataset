{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Floattube",
  "version": "1.2.6",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "player.js",
        "playlist.js",
        "main.js"
      ]
    }
  },
  "permissions": [
    "webview",
    "alwaysOnTopWindows",
    "fullscreen",
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ]
}
