{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "js/jquery-everywhere.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "js/jquery-everywhere.js",
    "js/jquery.min.js",
    "js/fix-$-problems.js",
    "js/jquery-noConflict.js"
  ],
  "name": "jQuery Everywhere",
  "version": "1.0.7.2",
  "description": "jQuery on every page... jQuery Everywhere. As it has to be.",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "contextMenus",
    "notifications"
  ],
  "homepage_url": "http://twitter.com/jotmaster",
  "offline_enabled": true,
  "icons": {
    "128": "jquery-everywhere.png",
    "112": "jquery-everywhere.png",
    "96": "jquery-everywhere.png",
    "48": "jquery-everywhere.png",
    "32": "jquery-everywhere.png",
    "16": "jquery-everywhere.png"
  }
}
