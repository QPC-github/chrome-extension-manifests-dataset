{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No One Direction on Twitter",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "No longer want One direction to appear on your Twitter-feed? This is made just for you!",
  "permissions": [
    "http://twitter.com/*",
    "https://twitter.com/*",
    "http://*.twitter.com/*",
    "https://*.twitter.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
