{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "metric /r/keto",
  "version": "0.11",
  "description": "Converts imperial flairs to metric flairs on /r/keto, provides tooltips for quick lbs2kg conversion.",
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "jquery-2.0.2.min.js",
        "jquery.qtip.min.js",
        "highlightRegex.min.js",
        "background.js"
      ],
      "css": [
        "jquery.qtip.min.css"
      ],
      "matches": [
        "http://www.reddit.com/r/keto",
        "http://www.reddit.com/r/keto/*",
        "https://www.reddit.com/r/keto",
        "https://www.reddit.com/r/keto/*"
      ]
    }
  ]
}
