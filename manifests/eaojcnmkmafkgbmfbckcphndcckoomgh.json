{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Extension to communicate Vivaldi browser with Qbserve time tracker",
  "manifest_version": 2,
  "name": "Qbserve",
  "version": "1.4",
  "homepage_url": "https://qotoqot.com/qbserve",
  "icons": {
    "32": "icons/qbserve-32.png",
    "48": "icons/qbserve-48.png",
    "64": "icons/qbserve-64.png",
    "96": "icons/qbserve-96.png"
  },
  "applications": {
    "gecko": {
      "id": "qbserve@qotoqot.com"
    }
  },
  "permissions": [
    "tabs",
    "http://localhost:27272/*"
  ],
  "background": {
    "scripts": [
      "qbserve.js"
    ]
  }
}
