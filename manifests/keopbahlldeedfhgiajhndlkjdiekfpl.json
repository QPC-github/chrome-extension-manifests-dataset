{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MomoNote",
  "description": "MomoNote is a personal memo application for the Web and iPhone/iPad.",
  "version": "2.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "app": {
    "urls": [
      "https://www.momonote.com/"
    ],
    "launch": {
      "web_url": "https://www.momonote.com/chrome/"
    }
  }
}
