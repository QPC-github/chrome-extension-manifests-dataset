{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Collab Screen Sharing Plugin",
  "version": "7",
  "description": "Collab Screen Sharing Plugin is a Screen Sharing Plugin runs on WebRTC. It is built and share for Collab platform",
  "author": "Debopam Majilya",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "minimum_chrome_version": "40",
  "homepage_url": "https://indusnet.co.in/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.zipidy.co.uk/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon48.png"
  ]
}
