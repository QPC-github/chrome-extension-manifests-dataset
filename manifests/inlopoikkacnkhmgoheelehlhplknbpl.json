{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EmojiMagic!",
  "description": "Replace text with emojis!",
  "version": "1.0.4",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "EmojiMagic!",
    "default_icon": "img/thumb.png"
  },
  "manifest_version": 2
}
