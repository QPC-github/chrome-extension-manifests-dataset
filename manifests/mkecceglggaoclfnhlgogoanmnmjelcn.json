{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Anilist Liker",
  "version": "1.1",
  "default_locale": "en",
  "description": "Allows users to quickly like posts on Anilist",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
