{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "qt-stock-link-addition",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Qiita:Team でストック一覧をメニューバーに追加する",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "48": "./icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.qiita.com/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
