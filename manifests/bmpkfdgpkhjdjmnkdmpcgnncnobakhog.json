{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WhoisMind",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Whois tool to lookup any Domain name, IP, Whois history, SEO, Nameservers, Emails and much more!",
  "icons": {
    "128": "images/whoismind-icon-128x128.png",
    "16": "images/whoismind-icon-16x16.png",
    "48": "images/whoismind-icon-48x48.png",
    "64": "images/whoismind-icon-64x64.png"
  },
  "page_action": {
    "default_icon": "images/whoismind-icon-16x16.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
