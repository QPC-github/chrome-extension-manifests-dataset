{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Changelly",
  "version": "1.0.1",
  "description": "Altcoins Exchange instantly",
  "icons": {
    "48": "img/logo.png",
    "16": "img/logo.png",
    "128": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_title": "changelly altcoins exchange instantly",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}
