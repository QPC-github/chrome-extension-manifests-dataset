{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_ExtensionName__",
  "description": "__MSG_ExtensionDescription__",
  "author": "gexsi.com",
  "homepage_url": "https://www.gexsi.com",
  "version": "1.0.3",
  "icons": {
    "16": "Icon-16.png",
    "32": "Icon-32.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/main.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "activeTab"
  ],
  "default_locale": "en",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Gexsi",
      "search_url": "https://suggest.gexsi.com/search?rtag=gexsiomn&rsrc=o&q={searchTerms}&{google:originalQueryForSuggestion}{google:prefetchQuery}",
      "favicon_url": "https://gexsi.com/favicon.ico",
      "suggest_url": "https://suggest.gexsi.com/suggest?rtag=gexsiomn&rsrc=po&q={searchTerms}&fmt=cr",
      "keyword": "gexsi",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
