{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Netflix Search",
  "version": "1.2",
  "description": "Takes a url from Trakt.tv and sparses it to a Netflix search with Google.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "48": "midsize.png",
      "128": "logo.png"
    }
  },
  "background": {
    "scripts": [
      "js/code.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://trakt.tv/movie/*",
    "http://www.trakt.tv/movie/*"
  ]
}
