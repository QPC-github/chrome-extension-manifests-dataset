{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit eddit",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Reddit with a couple tiny fixes",
  "icons": {
    "128": "img/128_2.png"
  },
  "browser_action": {
    "default_icon": "img/icon_2.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "open_in_tab": false,
    "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*",
        "https://www.reddit.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "permissions": [
    "storage",
    "http://www.reddit.com/*",
    "https://www.reddit.com/*",
    "http://www.reddit.com/",
    "https://www.reddit.com/"
  ]
}
