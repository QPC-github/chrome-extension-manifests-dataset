{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YuyushikiReplacement",
  "version": "1.0",
  "manifest_version": 2,
  "description": "ページ中の（由(々|由)|忌(々|忌)|ゆゆ）しきをゆゆ式に置換する拡張機能",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "yuyushikireplacement.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
