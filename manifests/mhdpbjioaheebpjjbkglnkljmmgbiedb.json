{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cyberx Password Generator",
  "description": "Easy and fast password generator for 'easy to remember' and secure passwords",
  "icons": {
    "128": "cpg/logo-128.png"
  },
  "version": "2.0.2",
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
  ],
  "browser_action": {
    "default_icon": {
      "19": "cpg/logo-19.png"
    },
    "default_popup": "cpg/index.html",
    "default_title": "Cyberx Password Generator"
  }
}
