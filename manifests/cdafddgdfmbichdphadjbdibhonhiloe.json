{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Pirate Bay Redirection",
  "version": "1.0",
  "manifest_version": 2,
  "description": "rewrites TPB requests with URLs that work for censored UK users.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.thepiratebay.se/*",
    "*://*.thepiratebay.org/*",
    "http://snaek.org/"
  ],
  "background": {
    "scripts": [
      "omg.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
