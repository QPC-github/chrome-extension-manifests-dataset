{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "Emphasize the most important part of the page",
  "version": "0.1",
  "name": "Paint Header",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jq.js",
        "paint.js"
      ]
    }
  ],
  "icons": {
    "16": "resource/icon-16.png",
    "48": "resource/icon-48.png",
    "128": "resource/icon-128.png"
  }
}
