{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One Office",
  "description": "The one stop Extension for all your office needs",
  "version": "1.0.8",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "/background/background.html"
  },
  "browser_action": {
    "default_title": "One Office"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "oneoffice-extension@adtraffic.co"
    }
  },
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "offline_enabled": true,
  "content_security_policy": "script-src 'self' https://apis.google.com https://www.googleapis.com; object-src 'self'"
}
