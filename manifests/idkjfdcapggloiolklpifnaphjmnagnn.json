      "https://*.cmbchina.com/*",
      "http://*.paas.cmbchina.com/*",
      "https://*.paas.cmbchina.com/*",
      "http://*.paasst.cmbchina.com/*",
      "https://*.paasst.cmbchina.com/*",
      "http://*.paasuat.cmbchina.com/*",
