{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hot for lyrics",
  "description": "Redirects to Musixmatch lyrics on click.",
  "version": "1.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Go for the lyrics",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
