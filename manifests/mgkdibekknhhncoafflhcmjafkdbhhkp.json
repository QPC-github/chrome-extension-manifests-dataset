{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "They Need A Designer",
  "short_name": "Bad Site!",
  "version": "1.0.0.2",
  "description": "Submit a site that needs some developer help!",
  "permissions": [
    "identity",
    "identity.email",
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "http://localhost:50847/api/Values?",
    "https://theyneedadesigner.azurewebsites.net/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/grimmace16.png",
      "32": "images/grimmace32.png",
      "48": "images/grimmace48.png",
      "128": "images/grimmace128.png"
    },
    "default_title": "This site needs help!"
  },
  "icons": {
    "16": "images/grimmace16.png",
    "32": "images/grimmace32.png",
    "48": "images/grimmace48.png",
    "128": "images/grimmace128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://theyneedadesigner.azurewebsites.net/; object-src 'self'"
}
