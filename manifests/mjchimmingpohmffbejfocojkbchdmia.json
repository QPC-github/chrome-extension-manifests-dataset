{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FX Check",
  "description": "FX Check extens√£o",
  "version": "1.8",
  "browser_action": {
    "default_icon": "logo-fx-diagnosticate16.png"
  },
  "icons": {
    "16": "logo-fx-diagnosticate16.png",
    "48": "logo-fx-diagnosticate48.png",
    "128": "logo-fx-diagnosticate128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.fxdata.com.br/*",
      "http://localhost:3000/*"
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
