{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tafy",
  "description": "Find best time to leave.",
  "version": "7.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "home.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "notifications",
    "tabs",
    "https://tafy-server.herokuapp.com/*"
  ]
}
