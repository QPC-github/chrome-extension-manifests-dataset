{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Search Time Options",
  "description": "Extension to use Google Search's time options quickly.",
  "version": "1.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "https://www.google.com/search?*",
        "https://www.google.co.jp/search?*"
      ],
      "run_at": "document_start"
    }
  ]
}
