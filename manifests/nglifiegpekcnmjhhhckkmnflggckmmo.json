{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "browser_action": {
    "default_title": "__MSG_deft__",
    "default_icon": "img/close-19.png",
    "default_popup": "popup.html"
  },
  "short_name": "__MSG_short__",
  "icons": {
    "128": "img/icon-128.png",
    "64": "img/icon-64.png",
    "48": "img/icon-48.png",
    "32": "img/icon-32.png",
    "16": "img/icon-16.png"
  },
  "version": "1.3.6",
  "description": "__MSG_desc__",
  "background": {
    "scripts": [
      "js/ga.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "downloads",
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "img/*.png"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
