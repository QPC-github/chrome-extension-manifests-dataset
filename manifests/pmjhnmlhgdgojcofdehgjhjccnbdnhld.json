{
  "background": {
    "scripts": [
      "js/jquery-1.7.2.min.js",
      "js/script/soundmanager2-nodebug-jsmin.js",
      "js/bg.js",
      "js/google-analytics-bundle.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "PROMO RADIO"
  },
  "commands": {
    "vol0": {
      "description": "Sound switch",
      "suggested_key": {
        "default": "Alt+C",
        "mac": "Alt+C"
      }
    }
  },
  "description": "PROMO RADIO - Любимые радиостанции из PROMO DJ!",
  "icons": {
    "128": "ico.png",
    "16": "ico.png",
    "48": "ico.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "25",
  "name": "PROMO RADIO",
  "permissions": [
    "http://*.promodj.com/*",
    "https://www.google-analytics.com/",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.3"
}
