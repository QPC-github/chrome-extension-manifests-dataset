{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "InCheck",
  "description": "Keep track of your weekly habits with In Check, a new tab chrome extension.",
  "version": "1",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "js/vendor/angular.min.js"
    ]
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "manifest_version": 2
}
