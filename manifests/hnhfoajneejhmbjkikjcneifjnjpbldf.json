    "16": "original-ofx-exporter-16.png",
    "48": "original-ofx-exporter-48.png",
    "128": "original-ofx-exporter-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://meu.original.com.br/*"
      ],
      "js": [
        "md5.min.js",
        "jquery-3.0.0.min.js",
        "original-ofx-exporter.js"
      ]
    }
  ]
}
