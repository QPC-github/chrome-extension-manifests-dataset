{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "short_name": "Usersnap",
  "description": "__MSG_appDescription__",
  "version": "2.3.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Usersnap",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "*://*.usersnap.com/*",
    "*://usersnap.com/*",
    "identity"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.usersnap.com/*",
      "*://usersnap.com/*"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+U",
        "mac": "Alt+U",
        "chromeos": "Alt+U",
        "linux": "Alt+U"
      }
    }
  },
  "key": "",
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "applications": {
    "gecko": {
      "id": "jid1-JVFdtm4BbUJ6wQ@jetpack"
    }
  }
}
