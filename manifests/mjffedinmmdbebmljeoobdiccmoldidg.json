{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Polynesian Keyboard",
  "version": "1.0",
  "description": "Type poynesian characters (w/macrons) into web pages",
  "author": "James Pakele",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "keypress-2.0.1.min.js",
        "polynesiankeys.js"
      ],
      "all_frames": true
    }
  ]
}
