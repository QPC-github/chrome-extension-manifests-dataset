{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Follower Prime",
  "version": "1.1.5",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "notifications",
    "<all_urls>",
    "activeTab",
    "webRequest",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "64": "img/icon.png",
    "32": "img/icon.png",
    "16": "img/icon.png",
    "128": "img/icon.png"
  }
}
