{
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_title": "Half-Life 2"
  },
  "version": "9.12.6",
  "chrome_url_overrides": {
    "newtab": "default.html"
  },
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "manifest_version": 2,
  "optional_permissions": [
    "management"
  ],
  "chrome_settings_overrides": {
    "homepage": "http://funsearchbox.com",
    "search_provider": {
      "name": "Fun Search Box",
      "search_url": "http://funsearchbox.com/search/{searchTerms}",
      "suggest_url": "http://funsearchbox.com/suggest/{searchTerms}",
      "encoding": "UTF-8",
      "is_default": true,
      "favicon_url": "http://funsearchbox.com/favicon.ico",
      "keyword": "loadz.com"
    }
  },
  "background": {
    "scripts": [
      "half-life-2-js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "topSites"
  ]
}
