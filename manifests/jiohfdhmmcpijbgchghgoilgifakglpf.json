{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Onliner.by currency converter",
  "version": "1.2",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "background.js"
      ],
      "matches": [
        "*://*.onliner.by/*"
      ]
    }
  ],
  "permissions": [
    "*://*.onliner.by/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "LostTime",
    "default_icon": "icon.png"
  }
}
