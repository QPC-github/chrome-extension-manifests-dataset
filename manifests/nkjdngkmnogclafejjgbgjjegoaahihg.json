{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TextNow",
  "description": "Get your own dedicated phone number to send and receive real unlimited texts, for free.",
  "version": "2.2.2",
  "app": {
    "urls": [
      "http://textnow.com/",
      "https://textnow.com/",
      "http://*.textnow.com/",
      "https://*.textnow.com/"
    ],
    "launch": {
      "web_url": "http://www.textnow.com/"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "notifications"
  ]
}
