{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LinksTamed SEO Tool",
  "short_name": "LinksTamed SEO Tool",
  "version": "1.3.1.2",
  "description": "Расcчитайте внутренний статический вес страниц сайта и проведите аудит многих факторов, связанных со ссылками.",
  "manifest_version": 2,
  "minimum_chrome_version": "68.0",
  "author": "Svyatoslav Alekseev",
  "icons": {
    "48": "/images/48.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "system.memory"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "/images/19.png",
      "38": "/images/38.png"
    },
    "default_title": "LinksTamed SEO Tool - \nрасчёт статического веса и аудит сайта",
    "default_popup": "popup.html"
  }
}
