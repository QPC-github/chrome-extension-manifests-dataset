{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DIGest",
  "description": "Save clips of texts & url to server",
  "author": "Rozzi H",
  "version": "1.0",
  "permissions": [
    "storage",
    "clipboardRead",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "options_page": "options.html",
  "manifest_version": 2
}
