{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "10kvtech Screen Sharing",
  "author": "10kvtech",
  "version": "1.10",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed for 10kvtech WCS Api to enable screen capture.",
  "homepage_url": "http://10kvtech.com",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.i-hydra.org/*",
      "*://*.hitc.local/*",
      "*://localhost/*",
      "*://127.0.0.1/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "logo48.png"
  ]
}
