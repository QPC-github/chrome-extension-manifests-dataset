{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hacker News Rank",
  "description": "Chrome extension that shows how many times a website was upvoted on Hacker News",
  "version": "2.3",
  "author": "Jitbit & Artiom Dashinsky",
  "browser_action": {
    "default_icon": "Icon.png"
  },
  "icons": {
    "16": "Icon.png",
    "48": "Icon@2x.png",
    "128": "Icon128.png"
  },
  "permissions": [
    "tabs",
    "https://hn.algolia.com/"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  }
}
