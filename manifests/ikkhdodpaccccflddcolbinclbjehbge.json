{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pro SP Team - Form Filler",
  "version": "1.5",
  "description": "Form Filler",
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "form-filler-icon.png",
      "32": "form-filler-icon.png",
      "48": "form-filler-icon.png",
      "128": "form-filler-icon.png"
    }
  },
  "icons": {
    "16": "form-filler-icon.png",
    "32": "form-filler-icon.png",
    "48": "form-filler-icon.png",
    "128": "form-filler-icon.png"
  },
  "commands": {
    "fill_form": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "windows": "Ctrl+Space",
        "mac": "Command+Space"
      },
      "description": "Ctrl Space"
    },
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "windows": "Ctrl+Q",
        "mac": "Command+Q"
      }
    }
  },
  "manifest_version": 2
}
