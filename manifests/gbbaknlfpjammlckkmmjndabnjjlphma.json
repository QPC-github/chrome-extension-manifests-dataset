{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "version": "2.6",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/blue-globe-foxy-alt-icon-38.png",
    "128": "images/blue-globe-foxy-alt-icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/jquery-2.2.4.js",
      "scripts/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/blue-globe-foxy-alt-icon-38.png",
      "38": "images/blue-globe-foxy-alt-icon-38.png"
    },
    "default_title": "__MSG_appName__",
    "default_popup": "popup.html"
  },
  "permissions": [
    "proxy",
    "notifications",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
