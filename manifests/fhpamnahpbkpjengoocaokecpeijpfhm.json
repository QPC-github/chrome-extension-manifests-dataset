{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Predikson",
  "description": "This extension enables tracking of mail",
  "version": "1.0.8",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "build/scripts/content.js"
      ],
      "css": [
        "build/styles/prdksn-main.min.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "build/scripts/prdksn-main.js",
    "build/styles/fonts/glyphicons-halflings-regular.eot",
    "build/styles/fonts/glyphicons-halflings-regular.ttf",
    "build/styles/fonts/glyphicons-halflings-regular.woff",
    "build/styles/fonts/glyphicons-halflings-regular.woff2",
    "build/styles/fonts/fontawesome-webfont.ttf",
    "build/styles/fonts/fontawesome-webfont.woff",
    "build/styles/fonts/fontawesome-webfont.woff2",
    "build/scripts/skins/lightgray/content.min.css",
    "build/scripts/skins/lightgray/skin.min.css",
    "build/scripts/skins/lightgray/fonts/tinymce.woff",
    "build/scripts/skins/lightgray/fonts/tinymce.ttf",
    "build/scripts/skins/lightgray/fonts/tinymce-small.woff",
    "build/scripts/skins/lightgray/fonts/tinymce-small.ttf"
  ]
}
