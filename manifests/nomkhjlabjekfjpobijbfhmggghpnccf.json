{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Novaoito e-commerce | Plataforma loja virtual",
  "short_name": "Novaoito e-commerce",
  "description": "Novaoito e-commerce é uma plataforma loja virtual moderna, rápida e robusta. Crie sua loja virtual e comece a vender pela internet.",
  "version": "1.0.6",
  "author": "Youzoom Soluções Web",
  "homepage_url": "https://novaoito.com.br",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Abrir configurações",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "tabs",
    "cookies",
    "nativeMessaging",
    "https://novaoito.com.br/"
  ],
  "externally_connectable": {
    "matches": [
      "http://yzshop/*",
      "https://*.novaoito.com.br/*",
      "https://*.novaoito.app/*",
      "https://bfms.com.br/*",
      "https://corpobelo.com.br/*"
    ]
  }
}
