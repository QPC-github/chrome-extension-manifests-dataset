{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OM New Tab",
  "short_name": "OM New Tab",
  "description": "More than 60 top websites in Favourites,40 Recently visited, 20 Most visited, All Bookmarks in your om new tab.",
  "version": "1.20",
  "incognito": "split",
  "permissions": [
    "history",
    "topSites",
    "bookmarks",
    "<all_urls>",
    "tabs",
    "storage",
    "chrome://favicon/"
  ],
  "web_accessible_resources": [
    "*.png"
  ],
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "browser_action": {
    "default_icon": "om logo final.png",
    "default_popup": "popup.html",
    "default_title": "OM New Tab"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
