{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Tab Cycle",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Press tab to select the top link on a results page",
  "homepage_url": "http://github.com/slifin",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.google.co.uk/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.co.uk/*"
      ],
      "js": [
        "js/tab.js"
      ]
    }
  ]
}
