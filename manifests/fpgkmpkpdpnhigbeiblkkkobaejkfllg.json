{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "short_name": "Switch our Vegan for Bin Cicken",
  "name": "Veganizer",
  "description": "This extension changes the word vegan to bin chicken",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
