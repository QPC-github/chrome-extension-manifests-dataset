{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "src/inject/inject.css"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "matches": [
        "https://www.designernews.co/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "Tired of comments and opinions cluttering Designer News? This extension is for you.",
  "homepage_url": "https://daneden.me/",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Neutral DN",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Neutral DN"
  },
  "permissions": [
    "tabs",
    "https://www.designernews.co/*"
  ],
  "version": "0.0.4"
}
