{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alexie",
  "version": "1.0",
  "description": "Alexie.co is your friend in the know: beauty-obsessed, with a passion for fashion and lust for a stylish life. ",
  "browser_action": {
    "default_title": "alexie.co"
  },
  "permissions": [
    "https://alexie.co/"
  ],
  "icons": {
    "48": "48.png",
    "128": "128.png"
