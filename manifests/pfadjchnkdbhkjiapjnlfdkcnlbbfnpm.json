{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Click to Call for Daktela",
  "version": "1.0",
  "description": "This is lightweight but powerful click to dial extension for omni-channel cloud based Daktela Contact Center",
  "homepage_url": "https://www.daktela.com",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "applications": {
    "gecko": {
      "id": "settings-example@mozilla.org",
      "strict_min_version": "48.0"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ]
}
