{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "tfl tube toolkit",
  "version": "1.3.1",
  "manifest_version": 2,
  "description": "Get the Live Tube Status, and upcoming services times to keep from a long wait on the platform",
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "img/icon-20-20.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "img/icon-20-20.png",
    "48": "img/icon-48-48.png",
    "128": "img/icon-128-128.png"
  },
  "permissions": [
    "notifications",
    "http://cloud.tfl.gov.uk/*"
  ]
}
