{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Motivator",
  "description": "Whiteboard in browser",
  "version": "1.0.4",
  "author": "Anton Krinitsyn",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "Have a good day"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
