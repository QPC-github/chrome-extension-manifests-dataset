{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "name": "Environment Switcher",
  "short_name": "Environment Switcher",
  "description": "This extension allows developers to switch between multiple environments very easily. (Example: DEV, TEST, etc..) You should try it!",
  "options_page": "settings.html",
  "version": "2.3.6",
  "icons": {
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/19x19.png",
      "38": "images/38x38.png"
    },
    "default_title": "EnvironmentSwitcher",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "commands"
  ],
  "background": {
    "scripts": [
      "js/library/require.js",
      "js/app.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "fonts/*.png",
    "images/*.png",
    "js/library/*.*",
    "js/*.*",
    "*.*"
  ]
}
