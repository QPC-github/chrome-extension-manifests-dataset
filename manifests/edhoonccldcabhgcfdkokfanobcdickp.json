{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Return to Infocomplete",
  "version": "0.39",
  "description": "Checks URL and redirects back to infocomplete after a specified period of time and goes back if at other URL instantly.",
  "background": {
    "page": "bg.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "information38.png"
  }
}
