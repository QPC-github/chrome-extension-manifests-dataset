{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump to Duck",
  "version": "1.0",
  "description": "Replaces Trump with Duck",
  "icons": {
    "48": "LargeTrum.png",
    "128": "SmallTrum.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "D2D.js"
      ],
      "run_at": "document_end"
    }
  ]
}
