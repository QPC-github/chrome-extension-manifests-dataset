{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GantzIsPatrick",
  "version": "1.0.0",
  "description": "Replaces Benny Gantz's name and photos with Patrick Star's name and photos from SpongeBob SquarePants.",
  "icons": {
    "16": "patrick.png",
    "48": "patrick.png",
    "128": "patrick.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "webNavigation"
  ]
}
