{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "View This Image",
  "description": "View any image by right clicking on it and selecting 'View This Image'.",
  "version": "1.0",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon/16.png",
    "64": "icon/64.png",
    "128": "icon/128.png",
    "512": "icon/512.png"
  },
  "background": {
    "scripts": [
      "viewthis.js"
    ]
  }
}
