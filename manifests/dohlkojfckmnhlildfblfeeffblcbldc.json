{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Practice League CRM",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This extension is used for Practice League CRM",
  "homepage_url": "https://www.practiceleague.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Practice League CRM",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "clipboardRead",
    "activeTab"
  ]
}
