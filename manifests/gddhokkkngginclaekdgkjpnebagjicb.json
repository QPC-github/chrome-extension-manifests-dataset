{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "rread is a social internet highlighting application.",
  "background": {
    "persistent": true,
    "page": "src/bg/background.html"
  },
  "default_locale": "en",
  "name": "rread",
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "windows"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "16": "icons/icon16.png"
  },
  "homepage_url": "http://rread.com",
  "manifest_version": 2,
  "version": "0.1"
}
