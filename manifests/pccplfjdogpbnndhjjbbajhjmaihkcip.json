{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alderiate Live",
  "description": "Extension navigateur pour le streamer Alderiate",
  "version": "1.0.6",
  "manifest_version": 2,
  "permissions": [
    "notifications",
    "https://*.alderiate.com/"
  ],
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "Alderiate Live",
    "default_icon": {
      "48": "icons/icon_128.png"
    }
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background.js"
    ]
  }
}
