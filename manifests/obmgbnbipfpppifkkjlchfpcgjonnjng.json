{
  "background": {
    "scripts": [
      "lib/jquery-1.4.2.min.js",
      "cookie_handler.js"
    ]
  },
  "browser_action": {
    "default_icon": "broken-cookie128.png",
    "default_title": "Удалить куки для сайта"
  },
  "description": "Удаляет все куки для текущего сайта.",
  "icons": {
    "128": "broken-cookie128.png",
    "48": "broken-cookie48.png"
  },
  "manifest_version": 2,
  "name": "Уничтожитель Cookie",
  "permissions": [
    "cookies",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "CK",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
