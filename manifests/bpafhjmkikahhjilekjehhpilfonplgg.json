{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PSU Authentication",
  "version": "1.9.7",
  "manifest_version": 2,
  "description": "Description ...",
  "icons": {
    "128": "128x128b.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "24x24b.png"
    },
    "default_title": "PSU Authentication tools",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "cookies",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "management"
  ]
}
