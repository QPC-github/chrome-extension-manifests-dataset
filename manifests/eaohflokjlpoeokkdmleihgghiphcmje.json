{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CertEnrollment.ChinaTRC Extension",
  "version": "3.2.0.3",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "http://10.10.20.37/*",
      "https://10.10.20.37/*",
      "http://10.10.10.32/*",
      "https://10.10.10.32/*",
      "http://10.19.1.249/*",
      "https://10.19.1.249/*",
      "http://11.11.10.93/*"
    ]
  }
}
