{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Startboard",
  "description": "Your very own personal page",
  "version": "1.0.4",
  "homepage_url": "http://startboard.io",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "data/widget.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "minimum_chrome_version": "11.0.696.57",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "cookies",
    "http://*/",
    "https://*/"
  ]
}
