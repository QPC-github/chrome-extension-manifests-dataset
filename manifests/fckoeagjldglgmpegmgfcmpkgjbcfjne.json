{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Editify",
  "version": "1.0",
  "description": "Editify is a tool to edit a website temporarily until you refresh it. I know i doesn't make much sense",
  "icons": {
    "16": "favicon16.png",
    "48": "favicon48.png",
    "128": "favicon128.png"
  },
  "browser_action": {
    "name": "Name",
    "default_icon": "favicon128.png"
  },
  "background": {
    "scripts": [
      "Background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
