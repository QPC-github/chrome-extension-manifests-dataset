{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kiva.org UI Improvements",
  "version": "1.9",
  "manifest_version": 2,
  "description": "Modifies Kiva.org to make editing loans easier.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://viva.kiva.org/en/node/*"
      ],
      "js": [
        "modifyKivaUI.js"
      ]
    }
  ]
}
