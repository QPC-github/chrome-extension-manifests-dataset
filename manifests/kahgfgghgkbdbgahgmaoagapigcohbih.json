{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jarvis AI",
  "manifest_version": 2,
  "version": "1.1",
  "icons": {
    "16": "jr.jpg",
    "128": "j.gif"
  },
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "jr.jpg",
    "default_title": "j.gif"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://www.google.com/buzz/api/buzzThis/buzzCounter"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
