{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DeepSea CIC",
  "version": "1.0.2",
  "minimum_chrome_version": "28.0",
  "author": "MG",
  "description": "KanColle analyzer for Chrome",
  "manifest_version": 2,
  "devtools_page": "devpage.html",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*",
    "*://*.kcwiki.moe/*",
    "storage"
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "web_accessible_resources": [
    "icons/48x48.png",
    "icons/128x128/png"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "DeepSea CIC"
  }
}
