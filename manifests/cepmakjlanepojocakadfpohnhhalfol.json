{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Goobric Web App Launcher",
  "description": "This extension launches the Goobric web app, a rubric based assessment tool that works with the Doctopus Add-on for Google Sheets.",
  "version": "2.5",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*",
    "https://script.google.com/*",
    "https://script.googleusercontent.com/",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "goobric-128x128.png"
  }
}
