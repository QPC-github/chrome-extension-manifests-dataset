{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "DataART Project Manager Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension provides ability to edit your timesheets within itself.",
  "browser_action": {
    "default_icon": "Resources/favicon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "Resources/favicon.png",
    "48": "Resources/PM-icon-48.png",
    "128": "Resources/PM-icon-128.png"
  },
  "permissions": [
    "https://newpm.dataart.com/"
  ]
}
