{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iStock UBB Maker",
  "description": "Provides the UBB code to link to images, lightboxes, etc. on the istockphoto.com website",
  "version": "0.0.2",
  "icons": {
    "128": "ubb-icon-1.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.9.0.min.js",
        "theasis_ubb_maker.user.js"
      ],
      "matches": [
        "http://*.istockphoto.com/stock-*",
        "https://*.istockphoto.com/stock-*",
        "http://*.istockphoto.com/photo/*",
        "http://*.istockphoto.com/vector/*",
        "http://*.istockphoto.com/video/*",
        "http://*.istockphoto.com/audio/*",
        "https://*.istockphoto.com/photo/*",
        "https://*.istockphoto.com/vector/*",
        "https://*.istockphoto.com/video/*",
        "https://*.istockphoto.com/audio/*"
      ]
    }
  ]
}
