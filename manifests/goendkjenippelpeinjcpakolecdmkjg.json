{
  "page_action": {
    "default_icon": "main.jpg",
    "default_popup": "dashboard.html",
    "default_title": "Disable FB Suggestions"
  },
  "web_accessible_resources": [
    "main.jpg",
    "maind.jpg"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Cool Extenstion to hide suggessions(Friends/pages) in Facebook",
  "background": {
    "scripts": [
      "load.js"
    ]
  },
  "name": "Hide Facebook Suggestions and Recomendations",
  "permissions": [
    "tabs",
    "webNavigation",
    "https://www.facebook.com/*",
    "http://www.facebook.com/*",
    "notifications"
  ],
  "icons": {
    "48": "main.jpg",
    "128": "main.jpg"
  },
  "manifest_version": 2,
  "version": "2.3"
}
