{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Email Extractor",
  "description": "Extract email addresses from web pages you visit",
  "version": "1.5",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "yellowenvelope.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "webNavigation"
  ]
}
