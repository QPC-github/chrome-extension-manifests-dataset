{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VinCertURLMon",
  "description": "VinCert URL Monitor",
  "version": "1.6",
  "icons": {
    "16": "vcchromeaddon_16.png",
    "128": "vcchromeaddon_128.png"
  },
  "permissions": [
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "vcURLMonBackground.js"
    ]
  }
}
