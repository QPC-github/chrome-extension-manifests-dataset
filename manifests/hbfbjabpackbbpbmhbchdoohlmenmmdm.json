{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Epoch converter",
  "description": "This extension converts epoch time to human readable local time",
  "version": "1.1",
  "author": "AK",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "main.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "epoch-icon.png",
    "48": "epoch-icon.png",
    "128": "epoch-icon.png"
  }
}
