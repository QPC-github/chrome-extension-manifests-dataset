{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YachtBonVivant",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "Replace @JeffersonObama's current avatar with his original Dean Barker avatar.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://twitter.com/*",
    "https://pbs.twimg.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "images/dean_barker_small.jpg",
    "images/dean_barker_big.jpg"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
