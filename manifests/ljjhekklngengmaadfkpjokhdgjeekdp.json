{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GHWebTweaks",
  "description": "GitHub Web Tweaks: auto-selecting indentation settings in web editor, and more features to come!",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/edit/*",
        "https://github.com/*/*/new/*"
      ],
      "js": [
        "ddsel.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}
