{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "focusevt.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Google Chromeでセキュリティソフト「SaAT Netizen」を使用するための拡張機能です。",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "32": "icon.png"
  },
  "manifest_version": 2,
  "name": "SaAT Netizen Chrome用拡張機能",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2"
}
