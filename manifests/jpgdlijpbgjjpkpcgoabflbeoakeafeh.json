{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Coal = Toxic death rock",
  "browser_action": {
    "default_icon": "icon_128.png"
  },
  "description": "Replaces the word 'coal with 'toxic death rock' when browsing the internet.",
  "version": "4.0",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
