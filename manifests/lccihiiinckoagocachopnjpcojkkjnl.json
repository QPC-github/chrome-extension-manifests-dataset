{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SAP Improvement Request Lookup",
  "short_name": "sapInfluence",
  "author": "Renars Rozitis",
  "version": "1.1",
  "description": "Look up an improvement request by its ID in SAP Customer Influence portal",
  "icons": {
    "128": "feedback.png",
    "64": "feedback.png",
    "48": "feedback.png",
    "32": "feedback.png",
    "24": "feedback.png",
    "16": "feedback.png"
  },
  "background": {
    "persisten": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "32": "feedback.png"
    },
    "default_title": "Look up IR in Influence"
  },
  "omnibox": {
    "keyword": "ir"
  }
}
