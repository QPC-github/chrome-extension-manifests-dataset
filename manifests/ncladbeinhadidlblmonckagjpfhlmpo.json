{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Motorcycle Weather",
  "description": "Motorcycle Weather App.",
  "manifest_version": 2,
  "version": "0.6",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "https://api.forecast.io/",
    "https://maps.googleapis.com/maps/api/geocode/json",
    "storage",
    "geolocation"
  ],
  "offline_enabled": false,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
