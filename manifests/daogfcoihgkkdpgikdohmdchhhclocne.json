{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js",
      "js/ga.js"
    ]
  },
  "browser_action": {
    "default_icon": "/i/icon.png",
    "default_popup": "html/dashboard.html",
    "default_title": "G™ Space"
  },
  "options_page": "html/settings.html",
  "default_locale": "en",
  "description": "Configure your own space for Google™ apps and services with shortcuts to show up as buttons in a simple popup, it's simple!",
  "icons": {
    "128": "/i/icon.png",
    "48": "/i/icon.png",
    "24": "/i/icon.png"
  },
  "manifest_version": 2,
  "name": "Google Apps Quick Launcher",
  "content_security_policy": "script-src 'self' https://browserextensions.store; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "short_name": "Google Quick Launcher",
  "version": "1.0.1",
  "web_accessible_resources": []
}
