{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Word Search Puzzle",
  "short_name": "Word Search",
  "description": "This word search puzzle game is one of the best of its kind. Try it and look for hidden words between letters of the alphabet.",
  "version": "1.5.1",
  "app": {
    "launch": {
      "local_path": "app/searchPuzzle.html"
    }
  },
  "icons": {
    "16": "app/icon_16.png",
    "128": "app/icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
