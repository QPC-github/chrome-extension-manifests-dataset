{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TWSeasy",
  "description": "Skips ad-page of TWS and takes you directly to the video hoster.",
  "version": "0.1.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
