{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "من الأشهر اليوم؟",
  "description": "اكثر الشخصيات اهمية فى صحف اليوم، كل ما ذكر فى الصحف عن الشخصيات العامة موثق بالتاريخ و المصدر",
  "version": "1.3.0",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "من الأشهر اليوم؟"
  },
  "permissions": [
    "alarms",
    "https://*.whoispopulartoday.com/*"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
