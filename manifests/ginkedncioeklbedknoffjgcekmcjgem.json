{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Feathr Canary",
  "version": "1.1.2",
  "description": "Check your Feathr Super Pixel implementation on your website.",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
    "default_title": "Feathr Canary",
    "default_popup": "src/index.html"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "debugger",
    "tab",
    "storage",
    "webNavigation"
  ]
}
