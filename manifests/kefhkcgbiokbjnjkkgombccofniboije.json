{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QR二维码生成器",
  "description": "This extension don't know who the author is.",
  "version": "0.2.0",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon@2x.png"
    },
    "default_popup": "popup.html"
  }
}
