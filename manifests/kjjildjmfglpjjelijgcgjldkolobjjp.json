{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Bitcoin's mainpage dashboard with reddit and twitter feeds.",
  "icons": {
    "128": "logo-128.png",
    "16": "logo-16.png",
    "48": "logo-48.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "name": "Mainpage",
  "offline_enabled": true,
  "version": "0.1.2.1",
  "minimum_chrome_version": "18",
  "permissions": [
    "https://blockchain.info/stats?format=json",
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/services/search/news ; object-src 'self'"
}
