{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ytFastForward",
  "description": "Youtube video speed controls on the extension icon. Enables you to fast forward beyond 2x on youtube. Very lightweight!",
  "version": "1.31",
  "permissions": [
    "tabs",
    "http://youtube.com/*",
    "https://youtube.com/*",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*"
  ],
  "browser_action": {
    "default_title": "Set speed",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
