{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Simple chess FEN/PGN viewer with evaluation",
  "manifest_version": 2,
  "name": "SimpleChessBoard",
  "version": "1.6",
  "homepage_url": "https://github.com/hxim/simplechessboard",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon-48.png"
    },
    "default_title": "Simple chess FEN/PGN viewer with evaluation",
    "default_popup": "popup/simplechessboard.html"
  }
}
