{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Add to Wishlist button - WishModo",
  "description": "Adds a button for adding products from any site to your wish list at WishModo",
  "manifest_version": 2,
  "version": "1.2",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "name": "WishModo : Add To Wishlist"
  }
}
