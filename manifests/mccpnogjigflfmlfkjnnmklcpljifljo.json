{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Incognito mode for AspDotNetStorefront",
  "description": "Extension for execution an impersonation process in an incognito mode",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*/order.aspx?*",
        "https://*/*/order.aspx?*",
        "http://*/*/cart.aspx?*",
        "https://*/*/cart.aspx?*",
        "http://*/*/cartImpersonation.aspx?*",
        "https://*/*/cartImpersonation.aspx?*"
      ],
      "js": [
        "myScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "manifest.json"
  ]
}
