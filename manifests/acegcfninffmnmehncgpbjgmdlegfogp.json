{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BingeTrendz",
  "description": "Today's trending TV shows info on your default tab",
  "version": "1.0.0",
  "author": "Parthipan Natkunam",
  "browser_action": {
    "default_title": "BingeTrendz - Find today's trending shows",
    "default_icon": {
      "19": "logos/logo16.png",
      "38": "logos/logo38.png"
    }
  },
  "icons": {
    "16": "logos/logo16.png",
    "48": "logos/logo48.png",
    "128": "logos/logo128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
