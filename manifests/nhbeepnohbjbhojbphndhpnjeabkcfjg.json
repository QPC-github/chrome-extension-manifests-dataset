{
  "short_name": "Scan to Mobile",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6",
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery/jquery.min.js",
      "urlmod/main.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/ico48.png",
    "default_title": "Scan to Mobile",
    "default_popup": "popup.html"
  },
  "description": "Quick Scan get link in mobile",
  "icons": {
    "128": "icons/ico128.png",
    "48": "icons/ico48.png"
  },
  "manifest_version": 2,
  "name": "Scan to Mobile",
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
