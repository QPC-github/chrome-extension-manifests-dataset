{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Missing Type",
  "description": "This extension shows what the world would look like without blood types",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "MissingType.css"
      ],
      "js": [
        "jquery-3.1.0.min.js",
        "MissingType.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
