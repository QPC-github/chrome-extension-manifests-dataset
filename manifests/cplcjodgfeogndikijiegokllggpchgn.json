{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Erinome Godville LS Predefiner",
  "description": "__MSG_appDesc__",
  "author": "__MSG_author__",
  "default_locale": "ru",
  "version": "0.0.0.1",
  "options_page": "options.html",
  "homepage_url": "https://gv.erinome.net",
  "permissions": [
    "storage",
    "http://godville.net/",
    "https://godville.net/",
    "http://godvillegame.com/",
    "https://godvillegame.com/"
  ],
  "content_scripts": [
    {
      "js": [
        "loader.js"
      ],
      "matches": [
        "*://godville.net/superhero*",
        "*://godville.net/duels/log/*",
        "*://godvillegame.com/superhero*",
        "*://godvillegame.com/duels/log/*",
        "*://gdvl.tk/duels/log/*",
        "*://gv.erinome.net/duels/log/*",
        "*://gvg.erinome.net/duels/log/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "web_accessible_resources": []
}
