{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "My BYU-I Login",
  "description": "This a simple app to take you to the login page for mybyui.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "urls": [
      "https://secure.byui.edu/cas/login?service=https://web.byui.edu/Services/Login/?RedirectURL=https%253a%252f%252fmy.byui.edu%252fics"
    ],
    "launch": {
      "web_url": "https://secure.byui.edu/cas/login?service=https://web.byui.edu/Services/Login/?RedirectURL=https%253a%252f%252fmy.byui.edu%252fics"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
