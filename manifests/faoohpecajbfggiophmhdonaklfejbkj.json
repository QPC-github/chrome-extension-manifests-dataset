{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "4PDA comments tracker",
  "version": "1.9",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://4pda.ru/*"
      ],
      "js": [
        "jquery-1.11.3.js",
        "background.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "/injected.js"
  ]
}
