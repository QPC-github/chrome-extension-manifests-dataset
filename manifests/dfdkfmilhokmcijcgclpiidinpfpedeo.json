{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ebot Helper and Cellect",
  "version": "1.2",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>",
    "clipboardWrite",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "ebot-reverser.js",
        "cellect.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "ebot.png"
  },
  "web_accessible_resources": [
    "*.ogg",
    "*.wav",
    "*.js",
    "*.html",
    "*.ttf"
  ]
}
