{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ali Orders History",
  "description": "Display history of orders",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Ali Orders History",
    "default_popup": "search.html"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "*://*.aliexpress.com/"
  ]
}
