{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lizify",
  "version": "1.0",
  "description": "Extension with pics of Liz Warren",
  "manifest_version": 2,
  "icons": {
    "16": "tab_icon.png",
    "48": "tab_icon.png",
    "128": "tab_icon.png"
  },
  "browser_action": {
    "default_icon": "tab_icon.png",
    "default_title": "Liz Warren"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
