{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clever Bookmarks",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "With this extension you can easily save pages to Google Bookmarks.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "tabs",
    "https://www.google.com/bookmarks/",
    "http://tagthe.net/api/*",
    "http://access.alchemyapi.com/*",
    "http://tesuck.eveel.ru/extract.json*"
  ],
  "page_action": {
    "default_name": "Clever Bookmarks",
    "default_icon": "icons/label_passive.png",
    "default_title": "Add page to bookmarks"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
