{
  "background": {
    "persistent": true,
    "scripts": [
      "authorities.js",
      "include.js",
      "encoder.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "injectStyle.css"
      ],
      "js": [
        "authorities.js",
        "inject.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "FishBarrel - a quackery reporting tool",
  "manifest_version": 2,
  "name": "FishBarrel",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications",
    "activeTab"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.1.23",
  "web_accessible_resources": [
    "duck.png"
  ]
}
