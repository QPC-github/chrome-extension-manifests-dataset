{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Chrome Censored",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Censors inappropriate words/terms on the interweb by filling them in with flowery replacements.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "censored.js"
      ],
      "run_at": "document_end"
    }
  ]
}
