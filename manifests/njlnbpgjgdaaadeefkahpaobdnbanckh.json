{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "New writeurl",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Create a new writeurl document.",
  "background": {
    "scripts": [
      "action.js"
    ]
  },
  "icons": {
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "browser_action": {
    "default_title": "New writeurl",
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://www.writeurl.com/"
  ]
}
