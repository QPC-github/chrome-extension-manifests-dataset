{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "diagrams.net Desktop",
  "description": "diagrams.net is a completely free diagram editor",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "13.7.5",
  "offline_enabled": true,
  "icons": {
    "16": "drawlogo16.png",
    "128": "drawlogo128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "file_handlers": {
    "handler_name": {
      "extensions": [
        "xml",
        "html",
        "svg",
        "png",
        "drawio"
      ]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    },
    "storage"
  ]
}
