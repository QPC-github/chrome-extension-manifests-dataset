{
  "content_scripts": [
    {
      "js": [
        "ContentJS.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "目前包含了21种编码,12种解码功能. 还提供了本地html的代码预览功能. 以及各种编码变异等.",
  "devtools_page": "devtools.html",
  "homepage_url": "https://www.xssav.com/",
  "icons": {
    "128": "icon/128px.png",
    "16": "icon/16px.png",
    "48": "icon/48px.png"
  },
  "manifest_version": 2,
  "name": "XSS Console",
  "permissions": [
    "cookies",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.6.6",
  "web_accessible_resources": [
    "icon/icon.png"
  ]
}
