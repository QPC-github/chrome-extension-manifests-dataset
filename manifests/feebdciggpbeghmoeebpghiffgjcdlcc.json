{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "no more auto fb login",
  "version": "0.3",
  "description": "Logs out of fb when you close the tab",
  "permissions": [
    "tabs",
    "cookies",
    "*://*.facebook.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
