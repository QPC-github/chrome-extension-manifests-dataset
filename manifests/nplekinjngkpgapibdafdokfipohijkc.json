{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Oxford English Dictionary",
  "version": "1.0",
  "description": "Look up the selected word in Oxford English Dictionary",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "manifest_version": 2
}
