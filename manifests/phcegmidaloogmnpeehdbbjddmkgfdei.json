{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screen Capturing-DEV",
  "author": "Gaurav Srivastava",
  "version": "4.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This is WebRTC screen capturing extension for Dev",
  "homepage_url": "https://ec2-52-25-206-225.us-west-2.compute.amazonaws.com/MultiTenantUIBase/*",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "matches": [
        "https://ec2-52-25-206-225.us-west-2.compute.amazonaws.com/MultiTenantUIBase/*"
      ]
    }
  ],
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
