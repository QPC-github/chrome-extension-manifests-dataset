{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "my Chaturbate models",
  "version": "2.0.0",
  "description": "Keep your favorite Chaturbate models at the distance of a click.",
  "icons": {
    "16": "includes/imgs/icon-cb-16.png",
    "48": "includes/imgs/icon-cb-48.png",
    "128": "includes/imgs/icon-cb-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "includes/imgs/icon-cb-16.png",
      "24": "includes/imgs/icon-cb-24.png",
      "32": "includes/imgs/icon-cb-32.png"
    },
    "default_title": "my Chaturbate models",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://chaturbate.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
