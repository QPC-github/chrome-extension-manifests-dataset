{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Subreddit Omnibox",
  "description": "Omnibox subreddit autocompletion. Just type 'r ' and explore 1648 of the biggest SFW subreddits.",
  "version": "1.1",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "r"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "data/topSubredditList.js",
      "js/bg.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "static/icon16.png",
    "19": "static/icon19.png",
    "32": "static/icon32.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
