{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nouvola DiveTrace",
  "short_name": "Nouvola",
  "description": "Record all HTTP / HTTPS traffic and create complex real-world tests in mere minutes â€“ without scripts.",
  "version": "1.0.17",
  "browser_action": {
    "default_icon": "assets/images/icon_128.png"
  },
  "background": {
    "scripts": [
      "assets/javascripts/listener.js"
    ]
  },
  "icons": {
    "16": "assets/images/icon_16.png",
    "48": "assets/images/icon_48.png",
    "128": "assets/images/icon_128.png"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "<all_urls>",
    "cookies"
  ]
}
