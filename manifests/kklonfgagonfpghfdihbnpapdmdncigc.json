{
  "background": {
    "persistent": false,
    "scripts": [
      "libs/adblockplus/deps.js",
      "libs/adblockplus/adblockplus.js",
      "components.js",
      "matcher.js",
      "patch.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "skin/16.png",
    "default_popup": "skin/popup.html",
    "default_title": "Flashcontrol Play"
  },
  "content_scripts": [
    {
      "css": [
        "skin/content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_description__",
  "devtools_page": "skin/devtools.html",
  "icons": {
    "128": "skin/128.png",
    "16": "skin/16.png",
    "48": "skin/48.png"
  },
  "manifest_version": 2,
  "name": "Flashcontrol Play",
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.7",
  "web_accessible_resources": [
    "skin/flashlogo.svg",
    "skin/pluginlogo.svg"
  ]
}
