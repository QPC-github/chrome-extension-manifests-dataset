{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "First In Google",
  "description": "Shows the first Google Search result of selected text.",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://ajax.googleapis.com/*",
    "https://ajax.googleapis.com/*",
    "contextMenus",
    "storage"
  ]
}
