{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Number of the Week",
  "short_name": "Number of the Week",
  "description": "An extension for The Number of the Week website. ",
  "author": "SamHDev",
  "version": "1.3",
  "version_name": "Release 1.3",
  "browser_action": {
    "default_icon": "icon_rounded.png"
  },
  "permissions": [
    "nativeMessaging",
    "notifications",
    "tabs",
    "storage",
    "alarms",
    "http://numberoftheweek.co.uk/*",
    "https://numberoftheweek.co.uk/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "512": "icon_square.png"
  }
}
