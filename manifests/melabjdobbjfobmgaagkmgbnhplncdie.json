{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_title": "MultiTool",
    "default_popup": "popup/index.html"
  },
  "icons": {
    "128": "icon-128.png",
    "195": "icon-128.png",
    "48": "icon-48.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "scripts/ext/jquery.js",
        "scripts/content.js",
        "scripts/pushFullScreen.js"
      ],
      "matches": [
        "*://*.khanacademy.org/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "scripts/inject/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.khanacademy.org/*"
    ]
  },
  "description": "Useful tools for use on Khan Academy",
  "key": "",
  "manifest_version": 2,
  "name": "MultiTool for Khan Academy",
  "permissions": [
    "storage",
    "activeTab",
    "notifications",
    "management",
    "http://terminalbit.com/"
  ],
  "short_name": "MultiTool",
  "version": "1.5",
  "version_name": "1.5",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
