{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Heads Up!",
  "description": "This extension replaces links to WowDB with those to Wowhead.",
  "version": "0.1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "replacelinks.js"
      ]
    }
  ]
}
