{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CloudCopyPaste",
  "description": "You can copy text from any device and paste in adive running with same google ID",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "CouldData.html"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "clipboardWrite",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background_test.js"
    ],
    "persistent": true
  },
  "commands": {
    "Shift+c": {
      "suggested_key": {
        "default": "Alt+Shift+C",
        "mac": "Alt+Shift+C",
        "windows": "Alt+Shift+C"
      },
      "description": "Shift+C."
    }
  }
}
