{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "This extension wraps long lines of code on the StackOverflow website and places a â†³ to mark line breaks.",
  "short_name": "CodeWrap",
  "name": "StackOverflow CodeWrap",
  "version": "1.1.0",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "StackOverflow CodeWrap"
  },
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://stackoverflow.com/questions*",
        "http://www.stackoverflow.com/*"
      ],
      "js": [
        "jquery-2.1.4.js",
        "stackoverflow_cs.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.stackoverflow.com/*",
    "storage"
  ],
  "options_page": "options.html"
}
