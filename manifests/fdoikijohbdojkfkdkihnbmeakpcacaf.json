{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "dopice.sk",
  "version": "0.6",
  "description": "Pičný plugin alebo po slovensky vložka. Vďaka tejto vložke viete skracovať linky ak máte chromý prehliadač od gúglu.",
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "img/logo.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
