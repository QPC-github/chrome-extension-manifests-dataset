{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SISpatch",
  "description": "A patch for the Tyler SIS login page that conceals the username field like a password.",
  "version": "2",
  "author": "Nate Kean",
  "permissions": [
    "tabs",
    "*://sdm.sisk12.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://sdm.sisk12.com/*"
      ],
      "js": [
        "patch.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png",
    "1024": "icons/icon1024.png"
  }
}
