{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Z QR Code",
  "description": "生成二维码",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Z QR Code"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [],
      "css": [
        "style.css"
      ]
    }
  ]
}
