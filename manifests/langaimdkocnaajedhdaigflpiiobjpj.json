{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA SecEditCtlAdv.DEMO Extension",
  "version": "3.0.0.8",
  "description": "CFCA Secure Edit Control Extension Chrome driver version",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://cfca.seceditctl.demo/*",
        "https://cfca.seceditctl.demo/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "http://cfca.seceditctl.demo/*",
      "https://cfca.seceditctl.demo/*"
    ]
  }
}
