{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mark Piazza Read",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Mark all Piazza posts as read upon closing the tab.",
  "icons": {
    "48": "src/img/48.png",
    "128": "src/img/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://piazza.com/class/*"
      ],
      "js": [
        "src/js/content.js"
      ]
    }
  ]
}
