{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cera is planet",
  "version": "1.3.1",
  "manifest_version": 2,
  "description": "Cera is planet, Cera is life",
  "options_page": "options.html",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "include_globs": [
        "*"
      ],
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_globs": [
        "https://www.google.*/search*",
        "http://www.google.*/search*",
        "http://www.google.*/#q*",
        "https://www.google.*/#q*",
        "http://maps.google.*/*",
        "https://maps.google.*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "images/cera1.png",
    "images/ceraisplanet.png"
  ]
}
