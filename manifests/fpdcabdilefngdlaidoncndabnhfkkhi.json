{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "roPass",
  "description": "roPass for Chrome",
  "version": "1.0",
  "icons": {
    "16": "ropass16.png",
    "48": "ropass48.png",
    "128": "ropass128.png"
  },
  "homepage_url": "https://ropass.com/",
  "browser_action": {
    "default_icon": "ropassIconlock.png",
    "default_title": "roPass",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://ropass.com/api/"
  ]
}
