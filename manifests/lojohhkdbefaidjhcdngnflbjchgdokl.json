{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Coagmento",
  "manifest_version": 2,
  "description": "Make the most of your Web browsing - individually or collaboratively.",
  "version": "1.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ],
      "css": [
        "sidebar.css"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "jquery-2.0.3.min.map"
  ]
}
