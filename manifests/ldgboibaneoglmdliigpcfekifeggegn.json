{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "dwpHash by dewep.pro",
  "author": "Mikhail Knyazhev <markus621@gmail.com>",
  "version": "1.2.2",
  "description": "Расширение для генерации сложного устойчивого к подбору и взлому пароля на базе простого и запоминающегося пароля.",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "options_page": "html/option.html",
  "permissions": [
    "clipboardWrite",
    "clipboardRead",
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Генератор сложных паролей",
    "default_icon": "img/icon-48.png",
    "default_popup": "html/index.html"
  }
}
