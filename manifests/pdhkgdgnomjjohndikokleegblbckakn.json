{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Likeshuo Extension",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Likeshuo for chrome's extension",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Some extensions required for Likeshuo",
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*",
      "*://*.lks.com/*",
      "*://*.lkstest.com/*",
      "*://*.likeshuo.com/*",
      "*://*.agora.platform.com/*"
    ]
  },
  "web_accessible_resources": [
    "woogeen_icon_128.png"
  ],
  "icons": {
    "128": "woogeen_icon_128.png",
    "16": "woogeen_icon_16.png",
    "48": "woogeen_icon_48.png"
  },
  "browser_action": {
    "default_icon": "woogeen_icon_16.png"
  }
}
