{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pocket open original page",
  "description": "This extension load original page after article view is loaded in new tab",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://getpocket.com/a/read/*"
      ],
      "js": [
        "openoriginal.js"
      ]
    }
  ]
}
