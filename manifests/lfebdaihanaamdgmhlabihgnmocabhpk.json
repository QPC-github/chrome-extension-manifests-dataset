{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RkBlock",
  "description": "Block RK on mlpfictions.com/",
  "version": "1.0.1",
  "icons": {
    "24": "image/icon24.png",
    "32": "image/icon32.png",
    "48": "image/icon48.png",
    "64": "image/icon64.png",
    "128": "image/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mlpfictions.com/*"
      ],
      "js": [
        "content-script/rkblock.js"
      ]
    }
  ],
  "permissions": [
    "https://mlpfictions.com/*"
  ]
}
