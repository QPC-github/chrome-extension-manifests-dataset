{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cyberbass Worker",
  "description": "This extension makes Cyberbass works again in Chrome.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.cyberbass.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
