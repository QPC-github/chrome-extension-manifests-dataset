{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "nCore posters",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Displays movie posters on ncore.cc",
  "homepage_url": "https://www.facebook.com/ncoerposters",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://ncore.cc/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://ncore.cc/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
