{
  "name": "techpp",
  "description": "Chrome extension of TechPP.com",
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_title": "Technology Personalized",
    "default_popup": "techpp.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "0.3"
}
