{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Where's Rashid?",
  "description": "This extension allows you to quickly find out where Rashid is.",
  "version": "1.2.1",
  "browser_action": {
    "default_icon": "icon_small.png",
    "default_popup": "rashid.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "icon_small.png",
    "48": "icon_medium.png"
  }
}
