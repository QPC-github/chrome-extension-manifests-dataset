{
  "background": {
    "scripts": [
      "jquery.js",
      "background.js",
      "popup.html"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Xpress Sign-On"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "signon.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "js": [
        "popup.js",
        "jquery.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "This extension acts as a Form fill and password manager.",
  "icons": {
    "128": "favicon128.PNG",
    "16": "favicon16.PNG",
    "32": "favicon32.PNG",
    "48": "favicon48.PNG"
  },
  "manifest_version": 2,
  "name": "Xpress Sign-On Extension(Apps.ilantus.com)",
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "7.4",
  "web_accessible_resources": [
    "signon.js",
    "popup.html",
    "popup.js",
    "loading.gif",
    "images/content-loader.GIF"
  ]
}
