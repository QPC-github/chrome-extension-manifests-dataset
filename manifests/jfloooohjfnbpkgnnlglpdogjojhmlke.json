{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Allows you to create a separate priority chat window in Picarto that only shows people you specify.",
  "version": "1.0.0",
  "name": "Picarto Priority Chat Queue",
  "background": {
    "scripts": [
      "hot-reload.bundle.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-34.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    "/*.js.map"
  ],
  "short_name": "PicartoChatX",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*.picarto.tv/*"
      ],
      "exclude_matches": [
        "*://*/chatpopout/*"
      ],
      "js": [
        "content-script.bundle.js"
      ],
      "style.css": [
        "content-script-style.css"
      ]
    }
  ]
}
