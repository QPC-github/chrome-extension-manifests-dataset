{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WordCloud Generator",
  "description": "This extension create a worldcloud with the content of the page where you are",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "https://ajax.googleapis.com/",
    "http://www.infovis-wiki.net/"
  ]
}
