{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Neo",
  "name": "Kaotic Spam Filter",
  "short_name": "kaoticfilter",
  "version": "1.1",
  "description": "This extension removes all spam links/videos to externals sites!",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://kaotic.com/*",
        "https://kaotic.com/*",
        "http://www.kaotic.com/*",
        "https://www.kaotic.com/*"
      ],
      "js": [
        "/content_script.js"
      ]
    }
  ]
}
