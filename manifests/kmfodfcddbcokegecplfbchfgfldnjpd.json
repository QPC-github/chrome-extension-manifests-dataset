{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "404 Bookmark Remover",
  "description": "Get a list of your unavailable, old bookmarks and remove them with ease!",
  "version": "2.0.1",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "activeTab",
    "https://*/",
    "http://*/"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/icon128.png"
  ]
}
