{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WEB face",
  "short_name": "WEB face",
  "description": "Modifying page",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": "images/128.png",
    "default_title": "",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "css": [
        "styles/content.css"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/draggable.js",
        "scripts/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs"
  ]
}
