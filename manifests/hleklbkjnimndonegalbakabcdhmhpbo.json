{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "eCann News brings you all of the cannabis industry related news under one roof",
  "name": "eCannabis.com News",
  "version": "5",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ajax.googleapis.com; object-src 'self'",
  "background": {
    "scripts": [
      "Popup_files/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "background",
    "storage",
    "alarms",
    "tabs",
    "notifications",
    "https://*/",
    "http://*/",
    "https://ajax.googleapis.com/",
    "http://ref.li/"
  ],
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  }
}
