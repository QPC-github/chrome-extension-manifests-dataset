{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pienapple Search for Google Chrome",
  "description": "An extension that allows you to quickly bookmark web pages.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "/img/icon.png",
    "default_popup": "index.html",
    "default_title": "Napple this page!"
  },
  "permissions": [
    "activeTab",
    "https://api.pienapple.com/"
  ]
}
