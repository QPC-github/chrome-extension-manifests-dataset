{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ctrl-alt-right-delete",
  "description": "Replace instances of 'Alt-Right' with something a little more accurate",
  "version": "1.2.0",
  "icons": {
    "16": "card-16.png",
    "48": "card-48.png",
    "128": "card-128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "defaults.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}
