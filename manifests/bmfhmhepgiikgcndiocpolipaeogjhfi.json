{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unique Tabs | Manual Edition",
  "description": "Click on the browser icon, to close any duplicates of the current tabs in the same window. Fork of Unique Tabs.",
  "version": "0.8",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Unique Tabs"
  },
  "permissions": [
    "tabs",
    "notifications",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "icon48.png"
  ],
  "manifest_version": 2
}
