{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Watch The Competition",
  "version": "1.0.10",
  "description": "watchthecompetition.com price monitoring plugin",
  "key": "",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "page_action": {
    "default_icon": "images/eye_plus_button_19px.png",
    "default_title": "Watch prices on this page by clicking on them"
  },
  "web_accessible_resources": [
    "images/*",
    "css/*",
    "fonts/*",
    "content-out/*"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "*://watchthecompetition.com/*",
      "*://localhost:*/*"
    ]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
