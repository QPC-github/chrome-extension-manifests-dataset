{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Make Keep gray again",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This extension returns gray background to Google Keep",
  "permissions": [
    "https://keep.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://keep.google.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
