{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/highlight.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "LIGHT",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "libs/jquery.min.js",
        "js/highlight.js",
        "js/content_script.js"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "description": "LIGHT.",
  "manifest_version": 2,
  "name": "LIGHT",
  "short_name": "LIGHT",
  "version": "0.0.5"
}
