{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Writend",
  "version": "0.4.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Writend helps you take quick notes on every new tab without sacrificing your beautiful new tab.",
  "permissions": [
    "tabs",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/writend_128.png"
  },
  "icons": {
    "16": "img/writend_16.png",
    "32": "img/writend_32.png",
    "48": "img/writend_48.png",
    "128": "img/writend_128.png"
  },
  "manifest_version": 2
}
