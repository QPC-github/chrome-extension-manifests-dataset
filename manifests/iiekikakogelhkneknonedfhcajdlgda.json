{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ueno.design",
  "short_name": "ueno.design",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "You need more beautiful things in your life.",
  "author": "Ueno.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://ueno.design/"
    ]
  },
  "permissions": [
    "storage",
    "*://ueno.design/"
  ]
}
