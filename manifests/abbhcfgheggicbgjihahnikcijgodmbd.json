{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seismic for Gmail",
  "version": "1.0.3",
  "key": "",
  "short_name": "Gmail extension for adding documents to email from Seismic.",
  "permissions": [
    "https://mail.google.com/*",
    "tabs",
    "identity",
    "notifications",
    "storage"
  ],
  "icons": {
    "128": "seismic-128.png",
    "48": "seismic-48.png",
    "32": "seismic-32.png",
    "16": "seismic-16.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "seismic-19.png",
      "38": "seismic-38.png"
    },
    "default_title": "Seismic",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://smc-prod-cdn.seismic.com ; object-src 'self'; frame-src 'self' https://smc-prod-cdn.seismic.com https://auth.seismic.com https://auth-qa.seismic.com https://auth-uat.seismic.com https://auth.seismic-dev.com",
  "externally_connectable": {
    "matches": []
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "bootstrap.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*"
  ]
}
