{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DramaQ Ad Bypass",
  "version": "0.1",
  "manifest_version": 2,
  "description": "DramaQ Ad Bypass",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.dailymotion.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "webRequest",
    "*://dramaq.tw/*",
    "*://www.dailymotion.com/*",
    "tabs",
    "webRequestBlocking"
  ]
}
