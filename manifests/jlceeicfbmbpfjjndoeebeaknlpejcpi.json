{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Reddit Spam Notifier",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Checks for spam and reported items and alerts you to new ones in your subs.",
  "content_scripts": [
    {
      "matches": [
        "https://ssl.reddit.com/",
        "http://www.reddit.com/",
        "http://reddit.com/",
        "https://ssl.reddit.com/*",
        "http://www.reddit.com/*",
        "http://reddit.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
