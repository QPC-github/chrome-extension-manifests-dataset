{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trove Words",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "trove_icon.png",
    "default_title": "Search on Trove"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "scripts.js"
    ]
  },
  "icons": {
    "16": "trove16.png",
    "32": "trove32.png"
  }
}
