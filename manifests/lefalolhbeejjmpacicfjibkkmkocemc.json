{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Flexor CTI extension Beta",
  "short_name": "Flexor CTI",
  "version": "3.0.0.27",
  "description": "Flexor CTI extension",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "FlexorCTI.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "permissions": [
      "nativeMessaging"
    ]
  },
  "icons": {
    "48": "Flexor48.png",
    "128": "Flexor128.png"
  },
  "permissions": [
    "nativeMessaging"
  ]
}
