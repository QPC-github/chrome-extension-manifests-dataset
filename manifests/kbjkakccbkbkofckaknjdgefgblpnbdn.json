{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fidonav Tabs",
  "version": "2.1.2",
  "description": "Fidonav provides a easy way to access the internet",
  "icons": {
    "48": "img/abp-48.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "storage",
    "windows",
    "tabs",
    "activeTab",
    "webNavigation",
    "<all_urls>",
    "management"
  ]
}
