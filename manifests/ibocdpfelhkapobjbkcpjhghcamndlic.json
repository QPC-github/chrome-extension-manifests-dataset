{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MY IP(灵巧版)--快速查IP",
  "version": "1.19",
  "description": "MY IP(灵巧版), 为您快速方便地知道自己的IP.",
  "permissions": [
    "tabs",
    "*://*/*",
    "webRequest",
    "cookies",
    "history"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "MY IP(灵巧版), 为您快速方便地知道自己的IP."
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
