{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Worlds Greatest Chrome Plugin of 2017",
  "description": "Make every page the Worlds Greatest of 2017",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "MakeEverythingGreat.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "justice.png",
    "48": "justice.png",
    "128": "justice.png"
  }
}
