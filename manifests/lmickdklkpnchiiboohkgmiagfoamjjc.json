{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cluster",
  "description": "Organize yourself and your entire team",
  "version": "1.0",
  "browser_action": {
    "default_icon": "e-commerce-icon-icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "uncluttered.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}
