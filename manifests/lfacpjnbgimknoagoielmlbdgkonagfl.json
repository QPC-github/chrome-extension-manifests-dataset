{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Datacook",
  "description": "Set your startup page by DataCook Kitchen!",
  "version": "2.2.3",
  "icons": {
    "128": "icons_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "DataCook",
    "default_icon": "icons_128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*",
      "http://datacook.org/",
      "http://datacook.org/*"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
