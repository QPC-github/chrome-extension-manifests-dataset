{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LoadLeak",
  "description": "Lataa Liveleak -videot koneellesi heleposti!",
  "version": "3.3",
  "author": "Kiurumedia Software",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://liveleak.com/*",
    "https://*.liveleak.com/*"
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
