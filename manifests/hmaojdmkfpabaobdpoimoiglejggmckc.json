{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Open Rutor",
  "version": "1.1",
  "description": "Доступ к сайту Rutor",
  "background": {
    "scripts": [
      "rutor.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Доступ к сайту Rutor",
    "default_icon": "icon.png",
    "default_popup": "rutor.html"
  },
  "permissions": [
    "proxy"
  ]
}
