{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "9Gag Simple Extension",
  "version": "1.6",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "description": "A simple extension to get the latest news from 9gag.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "icon_big.png"
  ]
}
