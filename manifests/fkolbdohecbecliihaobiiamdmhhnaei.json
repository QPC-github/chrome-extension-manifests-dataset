{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Strike Out Nofollow Links",
  "version": "1.3",
  "description": "Just strikes out rel= 'nofollow', 'sponsored' and 'ugc' links via CSS3 selectors.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
