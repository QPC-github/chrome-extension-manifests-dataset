{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Diario El Informador",
  "description": "El Diario Online de Venezuela",
  "version": "0.0.8",
  "author": "Frank Barrera - @farkbarn - frankb2320@gmail.com",
  "permissions": [
    "tabs",
    "http://*.elinformador.com.ve/*"
  ],
  "icons": {
    "128": "img/icon_128x128.png",
    "48": "img/icon_48x48.png"
  },
  "background": {
    "scripts": [
      "js/jquery-2.0.0.min.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19x19.png",
      "38": "img/icon_38x38.png"
    },
    "default_popup": "popup.html",
    "default_title": "EL INFORMADOR"
  }
}
