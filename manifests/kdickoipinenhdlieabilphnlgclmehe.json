{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/19.png",
    "default_popup": "popup.html",
    "default_title": "Feed2Mail notifications"
  },
  "content_security_policy": "script-src 'self' https://www.google.com https://ssl.google-analytics.com; object-src 'self'",
  "description": "Get desktop notifications when your Feed2Mail subscriptions are updated.",
  "homepage_url": "http://www.feed2mail.com/",
  "icons": {
    "128": "img/128.png",
    "64": "img/64.png",
    "19": "img/19.png",
    "16": "img/16.png"
  },
  "manifest_version": 2,
  "name": "Feed2Mail notifications",
  "permissions": [
    "notifications",
    "http://*/*",
    "https://*/*",
    "cookies",
    "unlimitedStorage",
    "storage"
  ],
  "version": "1.4"
}
