{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube Pauser",
  "description": "This extension pauses all YouTube videos (from youtube.com) playing on Chrome.",
  "version": "1.0",
  "permissions": [
    "http://*.youtube.com/",
    "https://*.youtube.com/",
    "http://*.youtu.be/",
    "https://*.youtu.be/",
    "background",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "pauser.js"
    ]
  }
}
