{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ivy SKU code activation - by dien.dp@pg.com",
  "version": "1.0",
  "description": "Active SKU code in Ivy webportal",
  "permissions": [
    "tabs",
    "declarativeContent",
    "storage",
    "activeTab",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "help.html",
  "icons": {
    "16": "images/ivy16.png",
    "32": "images/ivy32.png",
    "48": "images/ivy48.png",
    "128": "images/ivy128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/ivy16.png",
      "32": "images/ivy32.png",
      "48": "images/ivy48.png",
      "128": "images/ivy128.png"
    }
  },
  "manifest_version": 2
}
