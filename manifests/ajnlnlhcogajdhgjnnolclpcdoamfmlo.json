{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab Heaven",
  "version": "0.2",
  "icons": {
    "128": "img/icon256.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon256.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
