{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gipper",
  "description": "The way we communicate is evolving.",
  "version": "2.1.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "content.js",
        "initialize.js",
        "show-gipper-inline.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.gif",
    "*.png"
  ]
}
