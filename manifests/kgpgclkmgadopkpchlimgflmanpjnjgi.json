{
  "background": {
    "persistent": true,
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon4175.png",
    "default_popup": "index.html",
    "default_title": "L'extension de Brenell"
  },
  "content_security_policy": "script-src 'self' https://www.google.com;object-src 'self'",
  "description": "Permet d'être alerté de la sortie des dernières vidéos et des streams",
  "icons": {
    "64": "img/icon4175.png"
  },
  "manifest_version": 2,
  "name": "L'extension de Brenell",
  "permissions": [
    "tabs",
    "https://api.twitch.tv/",
    "https://www.googleapis.com/",
    "notifications",
    "alarms",
    "storage",
    "webRequest"
  ],
  "short_name": "Brenell",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.3"
}
