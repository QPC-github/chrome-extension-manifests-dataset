{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Amazon Data Extractor",
  "description": "Saves the product information of currently viewing Amazon product",
  "version": "2.6",
  "browser_action": {
    "default_icon": "new_icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://www.amazon.com/*"
  ]
}
