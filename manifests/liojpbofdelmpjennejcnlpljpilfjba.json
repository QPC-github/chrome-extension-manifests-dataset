{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Like to Communism",
  "version": "0.3",
  "description": "Replaces Twitter’s “Like” button with a “Communism” button, as Twitter’s creators originally intended",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "overrides.css"
      ],
      "js": [
        "overrides.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.png"
  ]
}
