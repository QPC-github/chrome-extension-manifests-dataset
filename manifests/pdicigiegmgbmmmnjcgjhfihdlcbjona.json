{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PHOWERD",
  "description": "Empower your day with photo stories in our new tab page.",
  "author": "Phowerd",
  "homepage_url": "https://phowerd.com/",
  "version": "1.0.3",
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "phowerd.js"
    ],
    "persistnet": false
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "unlimitedStorage",
    "https://phowerd.com/",
    "http://*/",
    "https://*/",
    "https://storage.googleapis.com/*"
  ]
}
