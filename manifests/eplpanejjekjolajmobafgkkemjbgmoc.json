{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dynamix Collaborate",
  "author": "Dynamix Labs, Inc.",
  "version": "2.9",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This extension is developed for Dynamix's Solvr suite of applications. It captures content of your screen for collaboration.",
  "homepage_url": "http://www.dynamixcloud.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.solvrhub.com/"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "puzzle16.png",
      "48": "puzzle48.png"
    },
    "default_title": "Dynamix Collaborate"
  },
  "icons": {
    "16": "puzzle16.png",
    "48": "puzzle48.png",
    "128": "puzzle128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "puzzle.png"
  ]
}
