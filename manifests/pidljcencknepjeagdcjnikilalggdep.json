{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Coupon Automatic",
  "version": "2.0",
  "description": "Save more money with Coupon Automatic (powered by SplatSearch.com)",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "nocouponsfound.html"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "chromesplat.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://www.splatsearch.com/*"
  ],
  "manifest_version": 2
}
