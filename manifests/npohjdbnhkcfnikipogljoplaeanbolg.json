{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Goodratings",
  "version": "2.1.0",
  "description": "Changes the way ratings on Goodreads are displayed.",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.goodreads.com/*"
      ],
      "js": [
        "gr.js"
      ]
    }
  ]
}
