{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lotto Helper Blackcan",
  "short_name": "LH Blackcan",
  "description": "Helps you to pick the best lotto numbers based on the month, day you are born and your favourite color. Weekly advice.",
  "version": "1.0.0.1",
  "permissions": [
    "unlimitedStorage"
  ],
  "icons": {
    "48": "icons/android/48.png",
    "72": "icons/android/72.png",
    "96": "icons/android/96.png",
    "128": "icons/android/128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index_extention.html"
  }
}
