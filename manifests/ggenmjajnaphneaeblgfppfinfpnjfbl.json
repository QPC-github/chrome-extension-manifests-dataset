{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube™ Double Tap Scroller",
  "version": "2.1",
  "description": "Allows you to rewind/fast forward youtube videos by 10 sec as in the YouTube™ Android app.",
  "icons": {
    "48": "ico-48.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "https://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "youtubedtap.js"
      ],
      "css": [
        "ydbt.css"
      ],
      "all_frames": true
    }
  ]
}
