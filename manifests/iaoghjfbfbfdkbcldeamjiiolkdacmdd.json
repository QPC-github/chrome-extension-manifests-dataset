{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://stackoverflow.com/questions/*",
        "*://*.stackexchange.com/questions/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    }
  },
  "manifest_version": 2,
  "name": "stackoverflow-favorite-as-history",
  "permissions": [],
  "version": "0.1.3"
}
