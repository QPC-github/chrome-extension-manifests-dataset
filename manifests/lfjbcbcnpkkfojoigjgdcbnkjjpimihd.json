{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks GDL1",
  "short_name": "Bookmarks GDL1",
  "description": "",
  "version": "0.0.47",
  "manifest_version": 2,
  "minimum_chrome_version": "20",
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src https://apis.google.com 'self'",
  "icons": {
    "32": "assets/icon_32.png",
    "64": "assets/icon_64.png",
    "128": "assets/icon_128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon_32.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://www.tcpiputils.com/",
    "*://www.utlsapi.com/",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
