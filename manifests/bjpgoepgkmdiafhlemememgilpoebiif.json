{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Forget",
  "version": "0.1",
  "description": "Faulty bookmarking.",
  "icons": {
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html",
    "default_title": "Remember for a while"
  },
  "background": {
    "scripts": [
      "moment.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "storage",
    "alarms"
  ]
}
