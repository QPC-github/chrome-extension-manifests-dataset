{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Listio",
  "description": "This extension allows you to create lists of tabs that are easy to manage!",
  "version": "0.0.0.4",
  "browser_action": {
    "default_icon": "listio-logo.png",
    "default_popup": "index.html",
    "default_title": "Listio"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "background"
  ]
}
