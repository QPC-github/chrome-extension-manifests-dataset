{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Brew Videos",
  "version": "1.2",
  "description": "Become part of the brewing process to make video discovery easy.",
  "author": "BlynQ Technologies",
  "permissions": [
    "tabs",
    "http://www.brewtv.in/*",
    "http://127.0.0.1:8000/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
