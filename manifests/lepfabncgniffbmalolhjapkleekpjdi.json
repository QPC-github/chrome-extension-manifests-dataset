{
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "description": "Permet d'être alerté quand Dessan est en stream !",
  "icons": {
    "64": "img/logo_dessan_off.jpg"
  },
  "manifest_version": 2,
  "name": "Dessan stream extension",
  "permissions": [
    "notifications",
    "webRequest",
    "*://*.api.twitch.tv/",
    "alarms",
    "https://www.googleapis.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.4"
}
