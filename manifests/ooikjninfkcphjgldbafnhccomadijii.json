{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "G.Harvard Quick Links",
  "short_name": "G.Harvard Quick Links",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Quickly jump to G.Harvard apps with this small extension.",
  "homepage_url": "http://digitalserials.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "G.Harvard Quick Link",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": []
}
