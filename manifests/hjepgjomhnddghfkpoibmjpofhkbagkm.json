{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "grt memo",
  "version": "0.2.5",
  "description": "simple memopad",
  "permissions": [
    "tabs",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
