{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Hacker News New Tab",
  "manifest_version": 2,
  "version": "2.0",
  "description": "Open Hacker News Links in new tab",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://news.ycombinator.com/*",
        "https://news.ycombinator.com/*",
        "http://hackerne.ws/*",
        "https://hackerne.ws/*"
      ],
      "js": [
        "ext.js"
      ]
    }
  ]
}
