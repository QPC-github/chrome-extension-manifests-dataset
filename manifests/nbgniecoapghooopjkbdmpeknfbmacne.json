{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Photo Calendar Creator Application",
  "description": "An online application where you can create a printable pdf photo calendar",
  "version": "1.1",
  "icons": {
    "128": "sn-logo128.png"
  },
  "app": {
    "urls": [
      "http://www.photocalendarcreator.com/"
    ],
    "launch": {
      "web_url": "http://www.photocalendarcreator.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
