{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random Image Quote",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Random Quote with beautiful background",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Random Quote",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "https://talaikis.com/"
  ]
}
