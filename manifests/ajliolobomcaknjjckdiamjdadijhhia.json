{
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "debut_dark.png"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "transfer page from light theme to dark one and make it more readable when light is off",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "name": "MoonR",
  "incognito": "split",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "R48.png",
    "128": "R128.png",
    "16": "R16.png",
    "256": "R256.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "css": [
        "contentstyle.css"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "2.1"
}
