{
  "author": "ATP Software",
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/jquery.js",
      "scripts/OneSignal.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "images/128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Scan Email"
  },
  "description": "Get all email addresses and phone numbers (Viet Nam format) in a web page.",
  "icons": {
    "128": "images/128.png"
  },
  "manifest_version": 2,
  "name": "Scan Email",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
