{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Who's Domain is this?",
  "description": "Instantly view information about this domain from network solutions",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "commands": {
    "Domain": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Domain"
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1",
  "manifest_version": 2
}
