{
  "background": {
    "scripts": [
      "common.js",
      "md5.js",
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://login.vk.com https://vk.com; object-src 'self'",
  "description": "Позволяет добавить собственные наборы изображений в качестве дополнения к стандартным стикерам ВКонтакте.",
  "externally_connectable": {
    "matches": [
      "*://*.vk.com/*"
    ]
  },
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "manifest_version": 2,
  "name": "Stickers League of Legends",
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*.vk.com/*",
    "*://*.vk.me/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [
    "inject.js"
  ]
}
