{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mormon no More",
  "short_name": "MoNoMo",
  "description": "For those who can't wait for the wicked world to hearken unto the prophet, this extension will stop you from seeing Mormon or LDS",
  "version": "0.2",
  "icons": {
    "16": "lds16.png",
    "48": "lds48.png",
    "128": "lds128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
