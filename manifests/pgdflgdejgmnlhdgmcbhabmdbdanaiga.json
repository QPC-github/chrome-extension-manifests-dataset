{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Printet",
  "description": "Printet allows you to convert web pages to PDF.",
  "version": "1.0.1",
  "icons": {
    "128": "logo128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "notifications",
    "downloads",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Print this page",
    "default_icon": "icon.png"
  },
  "options_page": "options.html"
}
