{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "vConsole Agent",
  "version": "1.0.2",
  "background": {
    "scripts": [
      "reporter.js"
    ],
    "persistent": true
  },
  "permissions": [
    "background",
    "tabs",
    "storage",
    "geolocation",
    "management",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage",
    "https://api.1to1vconsole.com/"
  ]
}
