{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks Omnibox finder",
  "description": "Find your bookmarks directly from omnibox.",
  "manifest_version": 2,
  "version": "1.0.1",
  "omnibox": {
    "keyword": "b"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
