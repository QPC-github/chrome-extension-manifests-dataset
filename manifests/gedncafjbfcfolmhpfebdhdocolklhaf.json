{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "author": [
    {
      "name": "Jonatas Urias B Teixeira",
      "email": "chrome@urias.me"
    }
  ],
  "manifest_version": 2,
  "name": "Quick QR code Generator",
  "description": "Converts the current url to a QR Code in one click. You can also encode a simple text on the go!",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "19.png",
    "default_popup": "p.html"
  },
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "32": "32.png",
    "24": "24.png",
    "16": "16.png"
  }
}
