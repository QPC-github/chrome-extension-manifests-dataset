{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Futbol Akademi",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Bu uygulama, okurların en son Futbol Akademi haberlerini yakından takip edebilmesi için hazırlanmıştır. © Tüm Hakları Saklıdır.",
  "permissions": [
    "tabs",
    "storage",
    "http://*/"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "arkaplan.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "ekran.html"
  }
}
