{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Translate History Fix",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.translate.google.com/*"
      ],
      "js": [
        "gthistory.js"
      ],
      "run_at": "document_start"
    }
  ]
}
