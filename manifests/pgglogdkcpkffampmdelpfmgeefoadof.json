{
  "name": "RYVE DevTools",
  "short_name": "RYVE_DEV",
  "version": "1.2.6",
  "description": "Developer tools for the hybrid headless system RYVE",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "devTools.js"
    ],
    "persistant": false
  },
  "page_action": {
    "default_popup": "devTools.html",
    "default_icon": {
      "16": "assets/images/favicons/favicon-16x16.png",
      "32": "assets/images/favicons/favicon-32x32.png",
      "96": "assets/images/favicons/favicon-96x96.png",
      "128": "assets/images/favicons/favicon-128.png"
    }
  },
  "icons": {
    "16": "assets/images/favicons/favicon-16x16.png",
    "32": "assets/images/favicons/favicon-32x32.png",
    "96": "assets/images/favicons/favicon-96x96.png",
    "128": "assets/images/favicons/favicon-128.png"
  },
  "manifest_version": 2,
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx"
}
