{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LolEsports Match Dropdown Fix",
  "description": "This extension makes the BO3/BO5 selector on lolesports.com show completely",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://na.lolesports.com/*"
