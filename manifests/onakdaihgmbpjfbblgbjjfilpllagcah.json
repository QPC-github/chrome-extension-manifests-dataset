{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Browsing With Bill",
  "short_name": "Bill",
  "description": "Browse a better way.  Browse with Bill.",
  "version": "1.3",
  "background": {
    "scripts": [
      "bground.js",
      "jquery.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
