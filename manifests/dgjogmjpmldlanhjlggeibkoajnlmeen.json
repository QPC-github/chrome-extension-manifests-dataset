  "name": "Twitch Plays Pokemon Chat Fix",
  "description": "Removes seeing other user's commands in chat, so that you can actually see what matters",
  "version": "1.1.16",
  "homepage_url": "http://reditr.com",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://twitch.tv/twitchplayspokemon/*",
        "*://*.twitch.tv/twitchplayspokemon/*"
      ],
      "js": [
        "jQuery.js",
        "script.js"
      ]
    }
  ]
}
