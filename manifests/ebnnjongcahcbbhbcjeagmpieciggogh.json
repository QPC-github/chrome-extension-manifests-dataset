{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No duplicate tabs",
  "short_name": "No duplicate tabs",
  "author": "Arlo Godfrey",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Auto switch to a tab instead of creating a duplicate",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "icons": {
    "48": "no-duplicate-tabs.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "-ms-preload": {
    "backgroundScript": "backgroundScriptsAPIBridge.js",
    "contentScript": "contentScriptsAPIBridge.js"
  }
}
