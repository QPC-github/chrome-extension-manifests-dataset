{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open in GIMP photo editor",
  "description": "Adds context menu items to send image links and image files directly to the GIMP photo editor",
  "author": "Andy Portmen",
  "version": "0.1.4",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "contextMenus",
    "downloads",
    "nativeMessaging"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "offline_enabled": true,
  "homepage_url": "https://add0n.com/open-in.html?from=gimp",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  }
}
