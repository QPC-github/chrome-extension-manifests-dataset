{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "charlesScrapper_Redfin",
  "version": "0.0.2",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com https://script.google.com https://script.googleusercontent.com; object-src 'self'",
  "browser_action": {
    "default_title": "charlesScrapper_Redfin",
    "default_icon": "images.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file:///*/*"
  ]
}
