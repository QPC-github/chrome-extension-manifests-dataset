{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.7.4",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "permissions": [
    "https://gaday.net/",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gaday.net/"
      ],
      "js": [
        "jquery.js",
        "opcje.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  }
}
