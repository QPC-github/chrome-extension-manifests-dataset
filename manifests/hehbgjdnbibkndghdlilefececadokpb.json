{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Theme Infinity New Tab Backgrounds HD",
  "short_name": "chrome-delight",
  "description": "Infinite dashboard wallpapers with weather, search engines, games, and more!",
  "default_locale": "en",
  "manifest_version": 2,
  "version": "1.3.6",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "topSites",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
