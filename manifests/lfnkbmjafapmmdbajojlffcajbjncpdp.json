{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "gitcoingg",
  "description": "Displays the gitcoin.gg widget on twitch.tv",
  "version": "3.0",
  "author": "",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*"
      ],
      "js": [
        "arrive.min.js",
        "chrome.js"
      ]
    }
  ],
  "permissions": [
    "https://www.twitch.tv/*",
    "activeTab"
  ]
}
