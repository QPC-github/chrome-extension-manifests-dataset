{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "description": "Snif.gr - Διαχείριση καταστήματος",
  "name": "Snif Partners",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "contextMenus"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Snif for partners",
    "default_icon": "icon128_box.png",
    "default_popup": "popup.html"
  }
}
