{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Appfolio Note Extendor",
  "version": "1.0.0",
  "description": "Allow Appfolio Notes to support Markdown and HTML",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "content_scripts": [
    {
      "css": [],
      "js": [
        "vendor/showdown.js",
        "content.js"
      ],
      "matches": [
        "https://*.appfolio.com/*"
      ]
    }
  ]
}
