{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AMP Validator - NotifyVisitors",
  "description": "Use NotifyVisitors AMP Validator to check if your webpages are valid AMP pages (accelerated mobile pages) or not.",
  "version": "0.03",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/favicon.png",
    "default_title": "AMP Validator - NotifyVisitors"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "validate.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "storage"
  ]
}
