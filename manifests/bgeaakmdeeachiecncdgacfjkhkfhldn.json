{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Homepaw",
  "version": "0.5",
  "description": "Smart and efficient bookmarks manager",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
