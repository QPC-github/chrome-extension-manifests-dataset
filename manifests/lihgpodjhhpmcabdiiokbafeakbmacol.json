{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Brandon Robbins & David Kent",
  "background": {
    "scripts": [
      "request.js"
    ]
  },
  "description": "Redirects any www.amazon.com or ebay.com page to Davy's affiliate link.",
  "homepage_url": "http://www.omicrononline.com",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "name": "Support David Kent",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://www.amazon.com/*",
    "https://www.amazon.com/*",
    "https://www.ebay.com/*",
    "http://www.ebay.com/*"
  ],
  "version": "2.1"
}
