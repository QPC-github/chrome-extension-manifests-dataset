{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Airsoftinfo.ru",
  "version": "0.1.0.9",
  "description": "Плагин для социальной сети на портале Airsoftinfo.ru",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon_logged_out.png"
  },
  "permissions": [
    "alarms",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  }
}
