{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TXT Clarity",
  "description": "Change the current page background and font color",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Set page color.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
