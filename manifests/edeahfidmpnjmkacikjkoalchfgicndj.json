{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mật Ngữ 12 Chòm Sao",
  "description": "Tiện ích xem bài mới đơn giản và gọn nhẹ. Được xây dựng bởi O.A.P Corporation Media & Technology",
  "version": "1.3",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "feed.html",
    "default_title": "Feed"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
