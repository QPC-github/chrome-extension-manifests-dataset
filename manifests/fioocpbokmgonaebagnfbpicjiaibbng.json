{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Identity Forum Notifier",
  "description": "Adds notifications to sidebar",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://forums.identityrpg.com/*",
        "https://forums.identityrpg.com/*"
      ],
      "js": [
        "jquery-2.1.3.js",
        "background.js"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
