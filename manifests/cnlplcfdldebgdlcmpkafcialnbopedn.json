{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pychat Screensharing Extension",
  "description": "This extension allows to shareScreen during a call for pychat.org.",
  "version": "1.0.1",
  "key": "",
  "author": "deathangel908@gmail.com https://github.com/Deathangel908",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://pychat.org/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
