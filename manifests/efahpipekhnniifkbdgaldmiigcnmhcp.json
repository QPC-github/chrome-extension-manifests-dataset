{
  "background": {
    "page": "page.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/icons/imageinfo-16.png",
    "default_title": "Aliexpress Dropship"
  },
  "content_scripts": [
    {
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/lib.js",
        "src/content/content_wk_api.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css",
        "assets/js/tinymce/skins/lightgray/skin.min.css",
        "assets/js/tinymce/skins/lightgray/content.min.css",
        "assets/css/select2.css",
        "assets/css/jquery.loadingModal.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/tinymce/tinymce.js",
        "assets/js/tinymce/jquery.tinymce.js",
        "assets/js/tinymce/themes/modern/theme.js",
        "assets/js/tinymce/plugins/advlist/plugin.js",
        "assets/js/tinymce/plugins/autolink/plugin.js",
        "assets/js/tinymce/plugins/lists/plugin.js",
        "assets/js/tinymce/plugins/charmap/plugin.js",
        "assets/js/tinymce/plugins/image/plugin.js",
        "assets/js/tinymce/plugins/preview/plugin.js",
        "assets/js/tinymce/plugins/print/plugin.js",
        "assets/js/tinymce/plugins/anchor/plugin.js",
        "assets/js/tinymce/plugins/searchreplace/plugin.js",
        "assets/js/tinymce/plugins/visualblocks/plugin.js",
        "assets/js/tinymce/plugins/code/plugin.js",
        "assets/js/tinymce/plugins/insertdatetime/plugin.js",
        "assets/js/tinymce/plugins/fullscreen/plugin.js",
        "assets/js/tinymce/plugins/media/plugin.js",
        "assets/js/tinymce/plugins/table/plugin.js",
        "assets/js/tinymce/plugins/contextmenu/plugin.js",
        "assets/js/tinymce/plugins/paste/plugin.js",
        "assets/js/tinymce/plugins/link/plugin.js",
        "assets/js/libs/bootstrap.js",
        "assets/js/libs/select2.full.js",
        "assets/js/libs/jquery.loadingModal.js",
        "assets/js/scripts/lib.js",
        "src/content/item.js",
        "assets/js/libs/jquery.xpath.js"
      ],
      "matches": [
        "*://*.aliexpress.com/item/*",
        "*://*.aliexpress.com/store/product/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/lib.js",
        "src/content/shopping_cart.js"
      ],
      "matches": [
        "*://shoppingcart.aliexpress.com/shopcart/shopcartDetail.htm*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/lib.js",
        "src/content/place_order.js"
      ],
      "matches": [
        "*://shoppingcart.aliexpress.com/order/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css",
        "assets/js/tinymce/skins/lightgray/skin.min.css",
        "assets/js/tinymce/skins/lightgray/content.min.css",
        "assets/css/select2.css",
        "assets/css/jquery.loadingModal.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/tinymce/tinymce.js",
        "assets/js/tinymce/jquery.tinymce.js",
        "assets/js/tinymce/themes/modern/theme.js",
        "assets/js/tinymce/plugins/advlist/plugin.js",
        "assets/js/tinymce/plugins/autolink/plugin.js",
        "assets/js/tinymce/plugins/lists/plugin.js",
        "assets/js/tinymce/plugins/charmap/plugin.js",
        "assets/js/tinymce/plugins/image/plugin.js",
        "assets/js/tinymce/plugins/preview/plugin.js",
        "assets/js/tinymce/plugins/print/plugin.js",
        "assets/js/tinymce/plugins/anchor/plugin.js",
        "assets/js/tinymce/plugins/searchreplace/plugin.js",
        "assets/js/tinymce/plugins/visualblocks/plugin.js",
        "assets/js/tinymce/plugins/code/plugin.js",
        "assets/js/tinymce/plugins/insertdatetime/plugin.js",
        "assets/js/tinymce/plugins/fullscreen/plugin.js",
        "assets/js/tinymce/plugins/media/plugin.js",
        "assets/js/tinymce/plugins/table/plugin.js",
        "assets/js/tinymce/plugins/contextmenu/plugin.js",
        "assets/js/tinymce/plugins/paste/plugin.js",
        "assets/js/tinymce/plugins/link/plugin.js",
        "assets/js/libs/bootstrap.js",
        "assets/js/libs/select2.full.js",
        "assets/js/libs/jquery.loadingModal.js",
        "assets/js/scripts/lib.js",
        "src/content/catalog.js"
      ],
      "matches": [
        "*://*.aliexpress.com/af/*",
        "*://*.aliexpress.com/w/*",
        "*://*.aliexpress.com/category/*",
        "*://*.aliexpress.com/wholesale*",
        "*://*.aliexpress.com/premium*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/lib.js",
        "src/content/confirm_order.js"
      ],
      "matches": [
        "*://icashier.alipay.com/payment/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/lib.js",
        "src/content/tracking.js"
      ],
      "matches": [
        "*://track.aliexpress.com/logisticsdetail.htm*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "assets/css/app.css"
      ],
      "js": [
        "assets/js/libs/jquery.js",
        "assets/js/scripts/lib.js",
        "src/content/pay_success.js"
      ],
      "matches": [
        "*://shoppingcart.aliexpress.com/order/payOnlineSuccess.htm*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Extention for Aliexpress Dropship for edrop247 site. Search products on Aliexpress easily!",
  "homepage_url": "https://edrop247.com",
  "icons": {
    "128": "assets/icons/imageinfo-128.png",
    "16": "assets/icons/imageinfo-16.png",
    "48": "assets/icons/imageinfo-48.png"
  },
  "manifest_version": 2,
  "name": "eDrop247 Aliexpress Dropship",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.8",
  "web_accessible_resources": [
    "assets/js/scripts/wk_api.js",
    "assets/images/logo-small.png",
    "assets/images/btn-floating.png",
    "assets/images/icon-import-error.png",
    "assets/images/icon-import-success.png",
    "assets/images/icon-close.png",
    "assets/images/logo.svg",
    "assets/css/bootstrap.css",
    "assets/css/popup.css",
    "*.eot",
    "*.svg",
    "*.ttf",
    "*.woff",
    "*.woff2",
    "*.js",
    "*.css"
  ]
}
