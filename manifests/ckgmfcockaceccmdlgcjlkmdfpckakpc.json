{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "jquery.min.js",
        "floodextension.js"
      ],
      "matches": [
        "https://soundcloud.com/*",
        "https://soundcloud.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "converted_from_user_script": true,
  "description": "Flood Extension for Soundcloud",
  "name": "Flood",
  "version": "0.0.3"
}
