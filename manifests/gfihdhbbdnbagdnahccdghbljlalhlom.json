{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tillit.nu",
  "description": "Gör det enkelt att dela innehåll på tillit.nu från valfri hemsida, Youtube eller Vimeo.",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "/src/popup.html"
  },
  "permissions": [
    "*://google.com/*",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
