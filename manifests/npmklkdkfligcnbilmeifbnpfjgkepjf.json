{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Dark",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/"
      ],
      "js": [
        "js/jquery-3.2.1.js",
        "js/script.js"
      ],
      "css": [
        "style.css"
      ],
      "all_frames": true
    }
  ],
  "description": "Dark theme for YouTube",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
