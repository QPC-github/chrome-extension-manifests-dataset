{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "homepage_url": "https://app.klue.com",
  "name": "Klue Button",
  "short_name": "Klue",
  "manifest_version": 2,
  "version": "0.27.4",
  "author": "Klue Labs Inc.",
  "description": "Collect Klues from around the web.",
  "permissions": [
    "tabs",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "fonts/*",
    "html/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.klue.com/*",
      "https://*.klue.d/*"
    ]
  },
  "background": {
    "scripts": [
      "javascripts/vendor/jquery.js",
      "javascripts/vendor/annotator.js",
      "javascripts/background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "20": "icons/icon48.png",
      "25": "icons/icon48.png",
      "30": "icons/icon48.png",
      "40": "icons/icon48.png"
    },
    "default_title": "Send to Klue"
  }
}
