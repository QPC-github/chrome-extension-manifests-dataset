{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pure Newtab",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A newtab with a pure customizable background.",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "ftp://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "default_locale": "en"
}
