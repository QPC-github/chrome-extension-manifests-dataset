{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "de",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "author": "neo-Design.ch",
  "homepage_url": "http://neo-design.ch/LegaReader/index.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "skin/icons/reader-32.png",
      "50": "skin/icons/reader-50.png",
      "75": "skin/icons/reader-75.png",
      "100": "skin/icons/reader-100.png"
    },
    "default_title": "__MSG_status02__"
  },
  "icons": {
    "32": "skin/icons/reader-32-book.png",
    "48": "skin/icons/reader-48-book.png",
    "75": "skin/icons/reader-75-book.png",
    "96": "skin/icons/reader-96-book.png",
    "128": "skin/icons/reader-128-book.png"
  },
  "permissions": [
    "background",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "version": "1.3.2.1"
}
