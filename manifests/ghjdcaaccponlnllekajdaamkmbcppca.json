{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "change_image.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_48.png",
    "default_title": "Ariston MoodFeed",
    "default_popup": "popup.html"
  },
  "description": "Ariston Moodfeed - Update your mood.",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "48": "img/icon_48.png"
  },
  "manifest_version": 2,
  "name": "Ariston Moodfeed",
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "version": "2.3"
}
