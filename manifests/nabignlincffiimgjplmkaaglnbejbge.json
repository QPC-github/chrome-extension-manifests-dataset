{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_title": "Get Rolling"
  },
  "description": "Now you can just roll up all that boring content",
  "icons": {
    "128": "icon.jpg"
  },
  "name": "Katamari Everywhere",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.4"
}
