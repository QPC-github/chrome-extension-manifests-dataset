{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Biryudumkitap Alıntılar",
  "description": "Her yeni sekmede bir yudum kitap tadın.",
  "version": "1.2",
  "incognito": "split",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "quote.html"
  },
  "browser_action": {
    "default_icon": "img/icon16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "http://extensions.biryudumkitap.com/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
