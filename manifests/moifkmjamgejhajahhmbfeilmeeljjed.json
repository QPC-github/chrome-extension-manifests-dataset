{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "vkreloc (Relocate vk.com to another page)",
  "short_name": "vkreloc",
  "version": "1.32",
  "author": "LanKing",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "https://vk.com/",
    "http://vk.com/",
    "webRequest",
    "webRequestBlocking"
  ]
}
