{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Airpics.net Recent Photos",
  "manifest_version": 2,
  "version": "1.4",
  "description": "See recent photos from Airpics.net.",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Airpics.net Recent Photos"
  },
  "permissions": [
    "http://feeds.feedburner.com/"
  ]
}
