{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "key": "",
  "name": "Overcoat",
  "description": "Overcoat extension",
  "version": "0.2.7",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true,
    "all_frames": false,
    "exclude_matches": [
      "http://*:4000/*",
      "https://overcoat.io/*"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "statics/css/*",
    "statics/fonts/*",
    "statics/images/*",
    "statics/js/iframe/iframe-pkg.js",
    "statics/js/base/app.js",
    "statics/js/base/base-pkg.js",
    "/statics/js/base/vendor.js",
    "/iframe/js/base/base-pkg.js"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}
