{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SemiMark",
  "description": "Ease the pain of bookmarks",
  "author": "semimark.com",
  "version": "0.1.3",
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background_content_script_options_popup.js",
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "web_accessible_resources": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "vendor.js",
        "background_content_script_options_popup.js",
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "bookmarks",
    "contextMenus",
    "chrome://favicon/",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
