{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FTC Helper",
  "description": "Fully Automates Floating Twitch Chat",
  "version": "0.29",
  "browser_action": {
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "64": "iconDisabled.png",
    "16": "icon16.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "bookmarks"
  ]
}
