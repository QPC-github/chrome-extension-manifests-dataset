{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BuzzVote",
  "version": "0.0.1",
  "author": "BuzzVote",
  "homepage_url": "https://buzzvote.com/",
  "description": "BuzzVote URL Recommender",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "64": "assets/icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "URL Popup",
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "incognito": "spanning",
  "offline_enabled": false
}
