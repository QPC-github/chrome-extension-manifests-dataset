{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pikabu AdBlock",
  "description": "Blocks ads' posts on pikabu",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://pikabu.ru/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "pageCapture"
  ]
}
