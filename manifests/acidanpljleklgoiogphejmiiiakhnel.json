{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "QR Code Generator",
    "default_popup": "popup.html"
  },
  "description": "可以將指定的內容轉換成 QR Code 條碼, 預設是將目前瀏覽的網址進行轉換!",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2,
  "name": "QR Code Generator",
  "offline_enabled": true,
  "permissions": [
    "tabs"
  ],
  "short_name": "QR Code Generator",
  "version": "1.0"
}
