{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.4.1",
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "short_name": "bookmarker",
  "default_locale": "en",
  "minimum_chrome_version": "67",
  "author": "Mythical5th.com",
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "32": "icons/icon32.png",
      "38": "icons/icon38.png"
    },
    "default_title": "__MSG_extName__",
    "default_popup": "html/popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Alt+S",
        "linux": "Alt+S",
        "mac": "Command+S",
        "windows": "Alt+S"
      }
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ]
}
