{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pedal Collective Quick Launch",
  "version": "1.35",
  "description": "Pedal Collective Quick Launch Utility",
  "icons": {
    "48": "img/ChromeExtension48.png",
    "128": "img/ChromeExtension128.png",
    "16": "img/ChromeExtension16.png"
  },
  "browser_action": {
    "default_icon": "img/ChromeExtension19.png",
    "default_title": "Quickly Access The Pedal Collective Website",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
