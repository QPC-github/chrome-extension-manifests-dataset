{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon.png",
    "128": "image/icon.png",
    "256": "image/icon.png",
    "512": "image/icon.png"
  },
  "permissions": [
    "topSites",
    "tabs",
    "storage",
    "<all_urls>",
    "chrome://favicon/",
    "chrome://favicon/*"
  ],
  "content_security_policy": "script-src 'self';object-src 'self'",
  "description": "Make your notice in your browser",
  "manifest_version": 2,
  "name": "Easy Note",
  "short_name": "ENote",
  "version": "0.2.2"
}
