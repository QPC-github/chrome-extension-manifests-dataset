{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "XTRABYTES (XBY) Price Ticker",
  "short_name": "XBY Ticker",
  "description": "Displays information about the XTRABYTES (XBY) cryptocurrency using the yobit, c-cex, cryptopia, and coinmarketcap APIs",
  "version": "0.3",
  "browser_action": {
    "default_icon": "img/xby_logo_19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "http://xtrabytes.me/"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.2.1.min.js",
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/xby_logo_16.png",
    "19": "img/xby_logo_19.png",
    "32": "img/xby_logo_32.png",
    "48": "img/xby_logo_48.png",
    "128": "img/xby_logo_128.png"
  }
}
