{
  "content_scripts": [
    {
      "css": [
        "stackCodeExpander.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://stackoverflow.com/*",
        "*://github.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Expand code samples in stackoverflow comments and github.",
  "icons": {
    "128": "logo.png",
    "48": "logo-48.png"
  },
  "manifest_version": 2,
  "name": "stackCodeExpander",
  "permissions": [
    "*://stackoverflow.com/*",
    "*://github.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1.2",
  "web_accessible_resources": [
    "main.js"
  ]
}
