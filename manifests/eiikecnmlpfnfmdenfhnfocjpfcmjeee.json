{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SCAMALERT Checker",
  "description": "This extension will analyze a page using https://scamalert.online",
  "version": "1.0",
  "browser_action": {
    "default_icon": "scamalert.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://scamalert.online/getstats/"
  ],
  "icons": {
    "16": "scamalert.png",
    "48": "scamalert.png",
    "128": "scamalert.png"
  }
}
