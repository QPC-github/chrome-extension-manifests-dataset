{
  "name": "滚蛋吧！淘宝二维码登录",
  "version": "0.0.1",
  "author": "晨冬",
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "description": "企业大了都是这么欺负用户的，明明输入密码很方便，干嘛手机扫，人人手机都像iPhone 7一样快吗？店大欺客，我们自己动手解决这件事，本插件很简单就是屏蔽一个小js而已！",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "icon/icon128.png",
    "48": "icon/icon48.png",
    "16": "icon/icon16.png"
  },
  "browser_action": {
    "default_icon": "icon/icon16.png",
    "default_popup": "popup.html",
    "default_title": "您可以来这里一起吐槽"
  },
  "permissions": [
    "contextMenus",
    "management",
    "bookmarks",
    "*://*/*",
    "tabs",
    "cookies",
    "notifications",
    "webRequestBlocking",
    "webRequest",
    "storage"
  ]
}
