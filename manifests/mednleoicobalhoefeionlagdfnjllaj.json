{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrumpSpeak",
  "short_name": "TrumpSpeak",
  "description": "Whatever you think of him, he's got great rhetoric. Makes your text Trumpier.",
  "author": "Casey Means",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
