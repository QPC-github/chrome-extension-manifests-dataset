{
  "name": "Responsively",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "templates/background.html"
  },
  "description": "Website responsiveness testing for developers and designers",
  "icons": {
    "128": "images/responsively_128.png",
    "16": "images/responsively_16.png",
    "48": "images/responsively_48.png"
  },
  "app": {
    "launch": {
      "local_path": "templates/default.html"
    }
  }
}
