{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "feedly blacker catter theme",
  "version": "3.822",
  "icons": {
    "128": "icon.png"
  },
  "description": "feedly's ui made darker, tighter, and wider. makes the Black Cat theme both blacker and catter.",
  "manifest_version": 2,
  "page_action": {
    "name": "change feedly",
    "icons": "icon.png",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "customStyles.css"
      ],
      "matches": [
        "http://cloud.feedly.com/*",
        "https://cloud.feedly.com/*",
        "http://feedly.com/*",
        "https://feedly.com/*"
      ]
    }
  ]
}
