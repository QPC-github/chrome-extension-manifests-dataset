{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "アイオーンちゃんタイマーmini",
  "short_name": "AionTimer",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "・・・騎士・・・時間・・・・・・",
  "offline_enabled": true,
  "options_page": "options.html",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_title": "アイオーンちゃんタイマー",
    "default_popup": "popup.html",
    "default_icon": "img/icon48.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "notifications",
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "img/*",
    "sound/*",
    "style/*",
    "script/*",
    "chara/*",
    "chara/aeon/*",
    "chara/navi/*",
    "chara/miu/*",
    "chara/nina/*"
  ]
}
