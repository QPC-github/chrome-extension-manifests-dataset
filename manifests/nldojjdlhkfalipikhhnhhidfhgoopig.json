{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Subtly â€“ Subtitles for Netflix",
  "short_name": "Subtly",
  "description": "Subtitles Translator for Popular Video-Streaming Services",
  "version": "1.4.4",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "netflix.js",
    "dictionary.js"
  ],
  "permissions": [
    "storage",
    "https://subtly.media/",
    "https://www.netflix.com/",
    "https://subtly.media/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*",
        "https://subtly.media/*"
      ],
      "js": [
        "loader.js"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "19": "icon-19.png",
      "32": "icon-32.png"
    },
    "default_title": "Subtly",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
