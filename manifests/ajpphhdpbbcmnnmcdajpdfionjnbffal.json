{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NotArmory Shop Extension",
  "version": "1.1",
  "description": "A lightweight create-a-shop tool with easy click-and-paste action directly into your MedianXL trade forum thread!",
  "content_scripts": [
    {
      "matches": [
        "https://tsw.vn.cz/acc/char.php?name=*"
      ],
      "js": [
        "contentCurrent.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "data/SUJsoned.json",
    "data/SUTUAccessoryJsoned.json"
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/icons8-demon-filled-16.png",
    "48": "images/icons8-demon-filled-48.png"
  },
  "manifest_version": 2
}
