{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LineMeUp, Jeu de l'Entraineur Winamax",
  "version": "1.1.2",
  "description": "Plugin LineMeUp, permettant de rentrer facilement ses équipes sauvegardées sur LineMeUp.fr sur le Jeu de l'Entraineur Winamax.",
  "permissions": [
    "storage",
    "declarativeContent",
    "https://www.winamax.fr/"
  ],
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./images/logo/logo-16.png",
      "32": "./images/logo/logo-32.png",
      "48": "./images/logo/logo-48.png",
      "96": "./images/logo/logo-96.png",
      "144": "./images/logo/logo-144.png"
    }
  },
  "icons": {
    "16": "./images/logo/logo-16.png",
    "32": "./images/logo/logo-32.png",
    "48": "./images/logo/logo-48.png",
    "96": "./images/logo/logo-96.png",
    "144": "./images/logo/logo-144.png"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "manifest_version": 2
}
