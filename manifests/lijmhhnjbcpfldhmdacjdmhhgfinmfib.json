{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ニコ生自動次枠移動",
  "version": "0.1.8",
  "manifest_version": 2,
  "description": "自動枠移動だけする．新配信（β）対応（多分）",
  "icons": {
    "128": "img/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://live.nicovideo.jp/*",
        "*://*.nicovideo.jp/*"
      ],
      "js": [
        "lib/jquery.js",
        "js/content.js"
      ],
      "css": [
        "css/css.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
