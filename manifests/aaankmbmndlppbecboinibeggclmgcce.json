{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/styles.css"
      ],
      "js": [
        "lib/priceAdvance_browser_wrapper.js",
        "lib/jquery-1.5.min.js",
        "lib/dropdown.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Automatically get the lowest price on whatever youâ€™re buying",
  "icons": {
    "256": "images/logo-256.png",
    "128": "images/logo-128.png",
    "64": "images/logo-64.png",
    "48": "images/logo-48.png",
    "36": "images/logo-36.png",
    "24": "images/logo.png"
  },
  "web_accessible_resources": [
    "images/logo.png",
    "images/settings.png",
    "css/styles.css",
    "images/close.png",
    "images/background.png",
    "images/down_arrow.png",
    "images/verticle_line.png",
    "images/facebook.png",
    "images/twitter.png",
    "images/amazon.png",
    "images/arrow1.png",
    "images/arrow2.png",
    "images/use_button.png",
    "images/shop_now.png",
    "images/both_arrow.png",
    "images/logo-36.png",
    "images/both_arrow_black.png"
  ],
  "name": "PriceAdvance",
  "options_page": "html/options.html",
  "permissions": [
    "idle",
    "tabs",
    "cookies",
    "http://*/",
    "https://*/",
    "storage",
    "webRequest"
  ],
  "version": "1.1.0",
  "manifest_version": 2
}
