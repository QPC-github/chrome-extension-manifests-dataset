{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AccelerateTab",
  "version": "1.3.1",
  "default_locale": "en",
  "description": "The Ultimate Browser Speed Dial",
  "homepage_url": "http://www.websecuritykeeper.com/",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "management",
    "history",
    "bookmarks",
    "topSites",
    "unlimitedStorage",
    "storage"
  ],
  "web_accessible_resources": [
    "cache/*"
  ],
  "background": {
    "page": "background.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
