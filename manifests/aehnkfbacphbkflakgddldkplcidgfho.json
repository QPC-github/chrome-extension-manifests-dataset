{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popover.html",
    "default_title": "Time2do - приложение для автоматического контроля вашего времени!"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Time2do - приложение для автоматического контроля вашего времени!",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Time2do",
  "offline_enabled": false,
  "permissions": [
    "activeTab",
    "idle",
    "webRequest",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "version": "0.0.4"
}
