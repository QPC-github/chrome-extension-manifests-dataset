{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FinStat Connector",
  "version": "1.2",
  "description": "Informácie o spoločnosti cez IČO alebo názov spoločnosti cez FinStat",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "finstat.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "32": "images/16.png",
      "48": "images/16.png",
      "128": "images/16.png"
    }
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/16.png",
    "48": "images/16.png",
    "128": "images/16.png"
  }
}
