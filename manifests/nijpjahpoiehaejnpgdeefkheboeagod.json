{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extronus News Feed",
  "description": "Check your extronus news feed, easier than ever!",
  "version": "1.0",
  "author": "Alexis Tan",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "js/jquery-1.11.0.min.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "http://minecraft-api.com/",
    "http://feed.extronus.net/"
  ],
  "browser_action": {
    "default_icon": "images/up.png",
    "default_popup": "popup.html"
  }
}
