{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit Login Remover (And Signup Remover)",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Don't want to log in or sign up for Reddit? This extension removes those annoying login and signup walls.",
  "homepage_url": "https://github.com/NedYork/Reddit-Login-Signup-Remover",
  "icons": {
    "16": "icons/RLR16.png",
    "48": "icons/RLR48.png",
    "128": "icons/RLR128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/*"
      ],
      "css": [
        "css/reddit.css"
      ]
    }
  ]
}
