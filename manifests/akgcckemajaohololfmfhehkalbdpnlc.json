{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fishlyst",
  "description": "Add items to wishlist and track the price, buy at right time.",
  "version": "2.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://*/*",
    "http://*/*",
    "cookies"
  ]
}
