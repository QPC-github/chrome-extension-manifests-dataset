{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "交易与信息技术中心技术周刊插件",
  "description": "美团交易与信息技术中心技术周刊插件",
  "version": "2.1.0",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "交易与信息技术中心技术周刊"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "bookmarks",
    "activeTab",
    "*://*/*"
  ]
}
