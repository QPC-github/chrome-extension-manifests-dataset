{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DeFloaty",
  "version": "0.3",
  "description": "Remove those annoying floating bars",
  "content_scripts": [
    {
      "matches": [
        "http://www.slate.com/*"
      ],
      "css": [
        "slate.css"
      ]
    },
    {
      "matches": [
        "http://arstechnica.com/*"
      ],
      "css": [
        "arstechnica.css"
      ]
    },
    {
      "matches": [
        "http://www.techdirt.com/*"
      ],
      "css": [
        "techdirt.css"
      ]
    },
    {
      "matches": [
        "http://www.themarysue.com/*"
      ],
      "css": [
        "themarysue.css"
      ]
    },
    {
      "matches": [
        "http://news.cnet.com/*"
      ],
      "css": [
        "news.cnet.css"
      ]
    }
  ]
}
