{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One Source for Lumesse TalentLink",
  "description": "One Source for Lumesse TalentLink",
  "version": "0.2",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "iconnew128.png",
    "16": "iconnew16.png",
    "48": "iconnew48.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "js": [
        "lightbox.js",
        "js/jquery.min.js",
        "fancybox/jquery.fancybox.js"
      ],
      "css": [
        "lightbox.css",
        "fancybox/jquery.fancybox.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true
    }
  ]
}
