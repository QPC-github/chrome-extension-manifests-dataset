{
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "dinamalaricon.ico",
    "default_popup": "popup.html"
  },
  "description": "Shows front page News feeds from Dinamalar",
  "icons": {
    "48": "logo.png"
  },
  "name": "Dinamalar",
  "permissions": [
    "http://dinamalar.com/",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "version": "1.2"
}
