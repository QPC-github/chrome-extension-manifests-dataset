{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "Nukeviet CMS Open Source",
  "homepage_url": "http://thethao.name.vn",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Nukeviet",
  "offline_enabled": true,
  "page_action": {
    "default_title": "Nukeviet Version Check"
  },
  "version": "4.0.26.213"
}
