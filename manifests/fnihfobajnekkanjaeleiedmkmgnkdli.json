{
  "content_scripts": [
    {
      "css": [
        "css/sanity.css"
      ],
      "matches": [
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Remove a seção de comentários dos maiores portais de notícias do Brasil, porque eu não sou obrigado.",
  "icons": {
    "128": "icons/large.png",
    "16": "icons/small.png",
    "48": "icons/medium.png"
  },
  "manifest_version": 2,
  "name": "sanity",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.1.1"
}
