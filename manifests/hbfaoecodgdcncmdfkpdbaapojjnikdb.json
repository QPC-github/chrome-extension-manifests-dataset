{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VK TUNE",
  "version": "1.0.4",
  "icons": {
    "16": "icon_16x.png",
    "48": "icon_48x.png",
    "128": "icon_128x.png"
  },
  "commands": {
    "PP": {
      "suggested_key": {
        "default": "Alt+Shift+8",
        "windows": "Alt+Shift+8",
        "mac": "MediaPlayPause"
      },
      "description": "Play Pause",
      "global": true
    },
    "next": {
      "suggested_key": {
        "default": "Alt+Shift+9",
        "windows": "Alt+Shift+9",
        "mac": "MediaNextTrack"
      },
      "description": "Next Track",
      "global": true
    },
    "prev": {
      "suggested_key": {
        "default": "Alt+Shift+7",
        "windows": "Alt+Shift+7",
        "mac": "MediaPrevTrack"
      },
      "description": "Prev Track",
      "global": true
    }
  },
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "background",
    "tabs",
    "storage",
    "notifications",
    "https://vk.com/*",
    "http://vk.com/*",
    "https://*.vk.com/*",
    "http://*.vk.com/*",
    "http://*.altox.tk/*"
  ],
  "browser_action": {
    "default_title": "VK TUNE",
    "default_popup": "popup.html"
  }
}
