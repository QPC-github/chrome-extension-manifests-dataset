{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "App Publish Checklist",
  "version": "1.1",
  "description": "Checklist before that crucial PUBLISH button to make sure you dont mess up the release!",
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "myscript.js"
      ],
      "matches": [
        "*://*.play.google.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon128.png"
  }
}
