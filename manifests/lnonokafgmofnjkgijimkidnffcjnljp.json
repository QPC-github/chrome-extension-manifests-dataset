{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "oknoleggioauto.it",
  "description": "Questa estensione cambia lo sfondo di default dell'homepage di google con un background di oknoleggioauto.it",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ],
  "default_locale": "it",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "oknoleggioauto.it cambio sfondo homepage google"
  }
}
