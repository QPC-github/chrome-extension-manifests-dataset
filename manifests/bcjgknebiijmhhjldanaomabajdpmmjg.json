{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free-Discussion",
  "description": "This extension shows if the discussion has started with the topic of the current URL.",
  "version": "1.4",
  "permissions": [
    "tabs",
    "storage",
    "http://www.sk.free-discussion.eu/ajax/ext/*"
  ],
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "browser_action": {
    "default_icon": "logo-128.png",
    "alert_icon": "logo-128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background2.js"
    ]
  }
}
