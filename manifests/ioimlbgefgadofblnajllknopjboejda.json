{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "67",
  "name": "Transpose ▲▼ pitch ▹ speed ▹ loop for videos",
  "short_name": "Transpose ▲▼",
  "version": "3.2.14",
  "description": "Pitch shifter, speed changer and looper for online videos like YouTube. Musician's toolbox for practicing and rehearsing.",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Tune73",
  "homepage_url": "https://transpose.video",
  "optional_permissions": [
    "activeTab",
    "storage",
    "tabCapture",
    "tabs",
    "*://*/*"
  ],
  "oauth2": {
    "client_id": "643097471182-toup5vukk9sqka3rjulmjopknekmgfre.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "controls.html"
  },
  "background": {
    "scripts": [
      "scripts/background-bundle.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com;  object-src 'self'"
}
