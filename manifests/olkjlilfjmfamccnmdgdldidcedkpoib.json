{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dashboard.io",
  "short_name": "Dashboard io",
  "description": "Dashboard io helper.",
  "version": "11.9.4",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com https://platform.twitter.com https://connect.facebook.net https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "images/dashboard.png",
    "48": "images/dashboard.png",
    "128": "images/dashboard.png"
  },
  "permissions": [
    "https://dashboard.io/",
    "https://www.dashboard.io/",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery/jquery.js",
      "backbone/underscore.js",
      "backbone/backbone.js",
      "backbone/models/discussions.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Dashboard.io helper",
    "default_icon": "images/dashboard.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
