{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RollOnBy Social Aggregator for Chromecast",
  "manifest_version": 2,
  "version": "1.2",
  "description": "RollOnBy Social Aggregator for Chromecastï»¿",
  "content_scripts": [
    {
      "matches": [
        "https://rollonby.com/*"
      ],
      "js": [
        "content_local.js"
      ],
      "persistent": false
    }
  ],
  "icons": {
    "128": "images/128.png"
  },
  "browser_action": {
    "default_title": "Rollonby",
    "default_icon": "images/128.png",
    "default_popup": "page.html"
  },
  "permissions": [
    "tabs"
  ]
}
