{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "gw5",
  "description": "Override the new tab page with a barcellona home page",
  "version": "0.9.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "barca.html"
  },
  "icons": {
    "16": "16.gif"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.conduit.com",
    "search_provider": {
      "name": "name.conduit",
      "keyword": "keyword.conduit",
      "search_url": "http://www.search.conduit/s?q={searchTerms}",
      "favicon_url": "http://www.search.conduit/favicon.ico",
      "suggest_url": "http://www.search.conduit/suggest?q={searchTerms}",
      "instant_url": "http://www.search.conduit/instant?q={searchTerms}",
      "image_url": "http://www.search.conduit/image?q={searchTerms}",
      "search_url_post_params": "search_lang=conduit",
      "suggest_url_post_params": "suggest_lang=conduit",
      "instant_url_post_params": "instant_lang=conduit",
      "image_url_post_params": "image_lang=conduit",
      "alternate_urls": [
        "http://www.search.conduit/s?q={searchTerms}",
        "http://www.foo.conduit/s?q={searchTerms}"
      ],
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://www.conduit.com"
    ]
  },
  "manifest_version": 2
}
