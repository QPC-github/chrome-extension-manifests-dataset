{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "nbnm8",
  "icons": {
    "128": "thumbnail.png"
  },
  "browser_action": {
    "default_title": "nbnm8",
    "default_icon": "icon.png"
  },
  "description": "Checks nbn availability so you don't have to.",
  "version": "1.2.0",
  "permissions": [
    "*://www.realestate.com.au/*",
    "*://www.domain.com.au/*",
    "*://www.allhomes.com.au/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.realestate.com.au/*",
        "*://www.domain.com.au/*",
        "*://www.allhomes.com.au/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
