{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Github BFF: Better Fork Finder",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Alerts you when there is a more active fork of the Github repo you are viewing",
  "permissions": [
    "https://github.com/",
    "https://api.github.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "myscript.js"
      ]
    }
  ]
}
