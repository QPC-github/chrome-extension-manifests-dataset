{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CWS Connector for Chrome",
  "short_name": "CWS Connector",
  "version": "1.4",
  "description": "Chrome connector for Cisco Cloud Web Security",
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "proxy",
    "identity",
    "identity.email",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "*://*/*"
  ],
  "storage": {
    "managed_schema": "schema.json"
  }
}
