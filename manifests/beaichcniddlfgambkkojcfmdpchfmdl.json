{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tags4info app integration extension",
  "description": "Adds current page URL and Title to Tags4info app",
  "version": "1.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Add To Tags4info",
    "default_icon": "icon.png"
  }
}
