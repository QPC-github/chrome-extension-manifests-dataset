{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "펨카우터",
  "version": "3.0.0",
  "description": "방대한 양의 Football Manager (FM) 데이터를 이용하여 축구 선수의 장단점을 알아보자",
  "icons": {
    "128": "small-logo.png",
    "48": "small-logo.png",
    "16": "small-logo.png"
  },
  "browser_action": {
    "default_popup": "search_main.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs",
    "contextMenus"
  ]
}
