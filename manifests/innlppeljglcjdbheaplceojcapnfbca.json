{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mahmut VisualOn Chrome Plugin Helper",
  "version": "3.9.1",
  "description": "Mahmut Chrome Native Messaging for VisualOn Plugin Player",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*.html",
      "*://demo.visualon.info:*/*SamplePlayerPPAPI.html"
    ]
  }
}
