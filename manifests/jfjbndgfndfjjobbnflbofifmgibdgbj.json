{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Wunderlist.com Shortcut",
  "version": "0.2",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "description": "Opens up Wunderlist.com webpage.",
  "background": {
    "page": "open_wunderlist.html"
  },
  "browser_action": {
    "default_title": "Wunderlist",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
