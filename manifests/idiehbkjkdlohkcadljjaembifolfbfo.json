{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Template Finder",
  "description": "Find free, downloadable, printable, and customizable templates to help promote, organize, and brand your business.",
  "homepage_url": "http://templatesextension.com/",
  "permissions": [
    "https://www.bex-digital.com/templates/"
  ],
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "16": "images/bd-16.png",
      "32": "images/bd-32.png",
      "48": "images/bd-48.png",
      "128": "images/bd-128.png"
    }
  },
  "icons": {
    "16": "images/bd-16.png",
    "32": "images/bd-32.png",
    "48": "images/bd-48.png",
    "128": "images/bd-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "landing.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self';style-src 'self' 'unsafe-inline' *;media-src *;"
}
