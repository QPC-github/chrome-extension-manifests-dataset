{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Examcookie",
  "version": "1.0.0.5",
  "manifest_version": 2,
  "description": "Extensions used to provide chrome visited urls",
  "icons": {
    "16": "images/icon-16.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "history"
  ]
}
