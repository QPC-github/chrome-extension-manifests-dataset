{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Copy Pasta",
  "short_name": "Copy Pasta",
  "description": "Enhance browser paste functionality. Paste anything anywhere on any webpage.",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_popup": "options/options.html",
    "default_title": "asdasd",
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  }
}
