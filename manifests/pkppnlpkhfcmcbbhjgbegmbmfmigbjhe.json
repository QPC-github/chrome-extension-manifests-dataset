{
  "background": {
    "scripts": [
      "js/chrome.js",
      "js/common.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_name__"
  },
  "content_scripts": [
    {
      "js": [
        "js/contentScript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "Capture visible content / region of a web page, or the whole page. Edit your image / screenshot with our tools and upload",
  "homepage_url": "http://www.ultraimg.com",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon19.png",
    "19": "images/icon19.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png"
  },
  "manifest_version": 2,
  "name": "UltraIMG - Image Hosting Screenshot Capture",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "activeTab",
    "<all_urls>"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "3.2.0"
}
