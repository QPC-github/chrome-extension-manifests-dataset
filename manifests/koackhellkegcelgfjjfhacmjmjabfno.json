{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My IP Plus",
  "description": "Find out your IP address with one click",
  "short_name": "My IP Address",
  "version": "1.3",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "18": "18.png",
    "48": "48.png",
    "64": "64.png"
  },
  "browser_action": {
    "default_icon": "18.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
