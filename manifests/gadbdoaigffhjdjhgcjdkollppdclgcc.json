{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ingia",
  "description": "Enhance your new tab page with typed url history, bookmark search and reddit feeds.",
  "version": "1.0",
  "permissions": [
    "management",
    "bookmarks",
    "history"
  ],
  "background": {
    "scripts": [
      "js/lib/angular.min.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "web_accessible_resources": [
    "bookmarks.html"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
