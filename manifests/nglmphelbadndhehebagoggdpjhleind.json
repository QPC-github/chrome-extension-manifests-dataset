{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Feed Rango for Feed Wrangler",
  "description": "A fresh coat of paint that improves the experience on Feed Wrangler.",
  "version": "0.2",
  "permissions": [
    "http://feedwrangler.net/*",
    "https://feedwrangler.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://feedwrangler.net/*",
        "https://feedwrangler.net/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "128": "Icon-128.png",
    "96": "Icon-96.png",
    "64": "Icon-64.png"
  }
}
