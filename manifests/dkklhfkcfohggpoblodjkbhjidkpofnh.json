{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Loop",
  "short_name": "Loop",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": [
    "https://www.youtube.com/*"
  ]
}
