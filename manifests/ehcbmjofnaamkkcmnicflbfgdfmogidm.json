{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Looong Tweet",
  "description": "Looong Tweet allows you to tweet longer tweets from Twitter",
  "version": "1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://short-tweet.azure-mobile.net/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "script.js",
        "MobileServices.Web-1.2.5.min.js"
      ]
    }
  ]
}
