{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "twitch setting(twutch)",
  "version": "1.0",
  "description": "",
  "browser_action": {
    "default_title": "Timer",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icons/GlitchIcon_Purple_48px.png",
    "128": "icons/GlitchIcon_Purple_128px.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://*.twitch.tv/*"
  ],
  "background": {
    "page": "popup/bpage.html"
  }
}
