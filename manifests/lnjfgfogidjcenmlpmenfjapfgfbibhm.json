{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "God Emperor",
  "description": "Replaces all instances of 'Donald Trump' with 'The God Emperor of Mankind'.",
  "version": "1.1.1.1",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "godemperor.js"
      ]
    }
  ],
  "manifest_version": 2
}
