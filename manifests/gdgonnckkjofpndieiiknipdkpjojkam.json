{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Same Image Tab Opener",
  "description": "Open all link that have same image into new tabs.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "manifest_version": 2
}
