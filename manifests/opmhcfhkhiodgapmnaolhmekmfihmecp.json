{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome New Window Focus",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "The 'Chrome New Window Focus' will try to force focus on any newly created Chrome window.",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
