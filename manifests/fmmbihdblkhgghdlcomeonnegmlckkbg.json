{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo-128.png",
    "default_title": "Qloo It"
  },
  "name": "Qlooer",
  "short_name": "Qlooer",
  "description": "This extension allows the user to Qloo any gift they want.",
  "homepage_url": "http://qlooer.com",
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "permissions": [
    "tabs",
    "http://qlooer.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1.1",
  "manifest_version": 2
}
