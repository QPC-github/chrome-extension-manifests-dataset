{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Manage Virtual Hosts",
  "manifest_version": 2,
  "name": "Virtual Hosts",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "activeTab"
  ],
  "version": "0.0.0.4",
  "author": "10100111001",
  "browser_action": {
    "default_icon": "images/disabled.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "eventPage.bundle.js"
    ],
    "persistent": true
  }
}
