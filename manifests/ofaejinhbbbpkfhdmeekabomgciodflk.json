{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scrubpad",
  "version": "0.1",
  "description": "Scrub away worries with Scrubpad. Clean up the mess way quicker and save your precious time!",
  "content_scripts": [],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_icon": {},
    "default_popup": ""
  },
  "background": {
    "scripts": [
      "js/background-page.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://www.google.co.uk/",
    "https://www.google.co.uk/",
    "http://ipv4.google.com/",
    "https://ipv4.google.com/"
  ],
  "web_accessible_resources": [],
  "manifest_version": 2,
  "author": "Sujit & Vishal"
}
