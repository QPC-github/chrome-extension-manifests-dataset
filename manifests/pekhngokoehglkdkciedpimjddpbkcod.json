{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.3.7",
  "name": "Last Tab Keepalive",
  "description": "Creates a new tab when the last tab is closed.",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "offline_enabled": true,
  "permissions": [
    "background"
  ]
}
