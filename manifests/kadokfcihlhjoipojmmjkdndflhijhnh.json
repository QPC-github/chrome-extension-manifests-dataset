{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "Reverts Google Plus' favicon to the old one.",
  "icons": {
    "128": "icon_128.png"
  },
  "name": "Gminus",
  "version": "2.0.1",
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://plus.google.com/*",
        "http://plus.google.com/*"
      ],
      "js": [
        "gminus.js"
      ]
    }
  ]
}
