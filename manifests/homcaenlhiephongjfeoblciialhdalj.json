{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twii Noti",
  "description": "View your Twii Notifications from your browser!",
  "icons": {
    "128": "assets/img/icon.png"
  },
  "version": "2.0.2",
  "background": {
    "scripts": [
      "assets/js/jquery-2.1.1.min.js",
      "assets/js/control/functions.js",
      "assets/js/control/bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://*.twii.me/",
    "http://twiinoti.twii.ca/*",
    "storage",
    "notifications",
    "tabs",
    "alarms"
  ],
  "web_accessible_resources": [
    "assets/img/icon.png"
  ],
  "author": "Cody L.",
  "browser_action": {
    "default_icon": "assets/img/icon_inactive.19.png",
    "default_popup": "popup.html",
    "default_title": "Twii Noti [0]"
  }
}
