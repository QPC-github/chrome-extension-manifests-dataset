{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Disney Wallpapers & Themes",
  "description": "Random HD wallpapers on your new tab for Walt Disney world",
  "manifest_version": 2,
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "data/newtab/index.html"
  },
  "permissions": [
    "storage"
  ],
  "homepage_url": "https://add0n.com/wallpaper.html?from=di",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
