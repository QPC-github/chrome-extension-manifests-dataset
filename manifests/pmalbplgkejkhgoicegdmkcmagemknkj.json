{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UserStory- Screenshot, RecordVideo, LiveShare",
  "short_name": "UserStory",
  "version": "1.0.33",
  "description": "Take screenshots, record screen videos with javascript logs or screenshare to give feedback or report bugs. Fix Faster. Ship Better.",
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "background": {
    "scripts": [
      "dist/js/libs.min.js",
      "dist/js/src.min.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "images/main-icon.png",
    "default_title": "UserStory"
  },
  "icons": {
    "16": "images/logo/logo16.png",
    "22": "images/logo/logo16.png",
    "32": "images/logo/logo48.png",
    "48": "images/logo/logo48.png",
    "128": "images/logo/logo128.png"
  },
  "permissions": [
    "identity",
    "desktopCapture",
    "storage",
    "cookies",
    "contextMenus",
    "tabs",
    "activeTab",
    "tabCapture",
    "<all_urls>",
    "clipboardWrite",
    "*://*.userstory.io/*",
    "*://*.ec2-13-126-63-102.ap-south-1.compute.amazonaws.com/*",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "declarativeContent",
    "debugger",
    "alarms",
    "notifications",
    "system.storage",
    "system.memory",
    "system.cpu"
  ],
  "optional_permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "images/main-icon.png",
    "html/videoControls.html",
    "html/mainContent.html",
    "html/screenshareControls.html",
    "popup.html",
    "html/login.html",
    "html/screensharingSuccessPopUp.html",
    "html/screensharingFailurePopUp.html",
    "images/logo/*.png",
    "images/*.png",
    "images/*.svg",
    "dist/*.css",
    "dist/themes/default/assets/fonts/icons.woff2",
    "dist/themes/default/assets/fonts/icons.woff",
    "dist/themes/default/assets/fonts/icons.ttf",
    "js/editor_injected_script_screenshot.js",
    "js/editor_injected_script_video.js",
    "js/editor_injected_script_screenshot_redirect.js",
    "js/editor_injected_script_video_redirect.js"
  ],
  "oauth2": {
    "client_id": "492964085325-44vsvkn680rvforgc90se1vtjs7kng8t.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email"
    ]
  }
}
