{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nudge Kick for Chrome",
  "version": "1.1",
  "description": "Nudge Kick blocks URLs until you meet a fitness goal for the day.  Currently works with Fitbit.",
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "third-party/jquery-1.7.min.js",
      "js/background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "128": "images/Nudge-Kick-icon-extenson-128.png",
    "48": "images/Nudge-Kick-icon-extension-48.png",
    "16": "images/Nudge-Kick-icon-extension-16.png"
  },
  "homepage_url": "http://nudgekick.com/",
  "browser_action": {
    "default_icon": "images/Nudge-Kick-icon-extenson-128.png",
    "popup": "popup.html",
    "default_popup": "popup.html"
  }
}
