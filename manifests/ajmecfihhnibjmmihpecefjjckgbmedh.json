{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.icbc.com/*",
        "*://*.icbc.com.cn/*",
        "*://*.dccnet.com.cn/*",
        "*://*.dccnet.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "ICBC new chrome extension.",
  "externally_connectable": {
    "matches": [
      "*://*.icbc.com.cn/*",
      "*://*.icbc.com/*",
      "*://*.dccnet.com.cn/*",
      "*://*.dccnet.com/*"
    ]
  },
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "manifest_version": 2,
  "name": "ICBCNewChromeExtension",
  "page_action": {
    "default_icon": "icon-128.png",
    "default_title": "ICBCNewChromeExtension"
  },
  "web_accessible_resources": [
    "*.png"
  ],
  "permissions": [
    "declarativeContent",
    "nativeMessaging",
    "cookies",
    "tabs",
    "http://*.icbc.com.cn/*",
    "https://*.icbc.com.cn/*",
    "browsingData",
    "history"
  ],
  "version": "1.0.2.1"
}
