{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "420chan+",
  "version": "0.1.6",
  "description": "420chan+",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*420chan.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://420chan.org/*",
        "http://*.420chan.org/*/*"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "css": [
        "content.css"
      ],
      "all_frames": true
    }
  ]
}
