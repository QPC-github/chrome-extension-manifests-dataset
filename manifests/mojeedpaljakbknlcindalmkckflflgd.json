{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "https://meeting.uwex.edu/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Share your screen content while in a WisLine Anytime Video meeting",
  "icons": {
    "128": "images/128.png",
    "16": "images/16.png",
    "48": "images/48.png",
    "64": "images/64.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "WisLine Anytime Video Screen Sharing",
  "options_page": "options.html",
  "permissions": [
    "desktopCapture",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.3"
}
