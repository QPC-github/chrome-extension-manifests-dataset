{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello Compact Mode",
  "description": "https://github.com/pennyapp/trello-compact-mode",
  "version": "0.0.3",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "compact.js"
      ],
      "css": [
        "compact.css"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
