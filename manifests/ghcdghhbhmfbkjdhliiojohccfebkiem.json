{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WikiProfile",
  "version": "1.5",
  "manifest_version": 2,
  "description": "WikiProfile",
  "icons": {
    "128": "big_w_icon.png"
  },
  "permissions": [
    "https://www.google.com/search*"
  ],
  "browser_action": {
    "default_icon": "big_w_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*"
      ],
      "js": [
        "jquery.min.js",
        "myScript.js"
      ],
      "css": [
        "css1.css"
      ]
    }
  ]
}
