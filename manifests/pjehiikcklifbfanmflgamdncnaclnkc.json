{
  "name": "Tamil and English Dictionary",
  "version": "4.0.0.1",
  "description": "Dictionary for Tamil to English and English to Tamil",
  "background": "background.html",
  "browser_action": {
    "default_icon": "Icon1.png",
    "default_title": "On Click Tamil and English Dictionary",
    "popup": "options.html"
  },
  "icons": {
    "128": "icon1.png",
    "16": "icon4.png",
    "32": "icon3.png",
    "48": "icon2.png"
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2
}
