{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CloudGate UNO for Google Chrome",
  "version": "1.2.0",
  "description": "CloudGate UNO for Google Chrome",
  "default_locale": "en",
  "incognito": "split",
  "key": "",
  "icons": {
    "16": "images/cloudgate-48.png",
    "48": "images/cloudgate-48.png",
    "128": "images/cloudgate-128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/",
    "contextMenus",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "css/notification.css"
      ],
      "images": [
        "images/cg-extension.png"
      ],
      "js": [
        "js/lib/utils.js",
        "js/lib/common.js",
        "js/notification.js",
        "js/jquery-3.2.1.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "js/jquery-3.2.1.min.js",
      "js/cgListeners.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/cloudgate-48.png",
      "32": "images/cloudgate-48.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/*.png",
    "images/svg/*.svg",
    "css/notification.css",
    "options.html"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.cloudgate.jp/*",
      "*://*.isr.co.jp/*"
    ]
  }
}
