{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pokémon Quick Search",
  "version": "1.0.1.2",
  "manifest_version": 2,
  "description": "Type a Pokémon's name, select it from the list and click it to display it's detailed info on Serebii.net. All 7 gens listed.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+A",
        "mac": "Alt+A",
        "chromeos": "Alt+A",
        "linux": "Alt+A"
      }
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": []
}
