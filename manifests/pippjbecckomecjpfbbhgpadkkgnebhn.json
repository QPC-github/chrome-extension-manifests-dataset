{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "name": "Longman",
  "version": "1.0000001",
  "icons": {
    "32": "icons/search.png",
    "64": "icons/search64.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "subfunc.js",
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icons/search.png"
    }
  },
  "default_locale": "en"
}
