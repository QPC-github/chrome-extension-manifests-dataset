{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Slick Checkbox for Gmail",
  "version": "0.1",
  "description": "Just hover your cursor to check a checkbox in gmail",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
