{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Joke Extension",
  "version": "1.0",
  "description": "Click to deliver a new joke!",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Joke",
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
