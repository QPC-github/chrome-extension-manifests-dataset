{
  "author": "DLS Internet Services",
  "background": {
    "persistent": false,
    "scripts": [
      "background-config.js",
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://meetingroom.dls.net/*",
        "https://*.dls.net/*"
      ]
    }
  ],
  "description": "This Google Chrome extension brings screen sharing to the DLS Meeting Room",
  "homepage_url": "http://www.dls.net",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Screensharing for the DLS Meeting Room",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.10.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
