{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "quickTabs",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "description": "System quick bookmarks",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "./images/icon16.png",
    "default_title": "quickTabs",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  }
}
