{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "WebGameAPP",
  "name": "Dark Souls HD Wallpapers New Tab",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "default_locale": "en",
  "version": "3.1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "Dark Souls HD Wallpapers New Tab",
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "https://*.webgameapp.com/*",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
