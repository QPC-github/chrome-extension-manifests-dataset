{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CareCloud Helper",
  "version": "1.0.6",
  "description": "CareCloud Google Chrome Helper Extension",
  "permissions": [
    "contentSettings",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "CareCloud Corporation"
  },
  "icons": {
    "16": "images/carecloud16.png",
    "32": "images/carecloud32.png",
    "48": "images/carecloud48.png",
    "128": "images/carecloud128.png"
  },
  "manifest_version": 2
}
