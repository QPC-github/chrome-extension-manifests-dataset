{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Super Tab",
  "version": "1.3",
  "manifest_version": 2,
  "description": "A create your own home screen that is fully customizable with date, time, weather and more.",
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/logo.png",
    "default_title": "Super Tab"
  },
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "<all_urls>"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.min.js",
      "js/background.js"
    ]
  }
}
