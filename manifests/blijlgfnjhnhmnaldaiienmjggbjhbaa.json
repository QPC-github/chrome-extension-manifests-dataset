{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "description": "Equivalent to the Apple Safari picture-in-picture player but for chrome",
  "commands": {
    "next-track": {
      "suggested_key": {
        "default": "MediaNextTrack"
      },
      "description": "Next track",
      "global": true
    },
    "prev-track": {
      "suggested_key": {
        "default": "MediaPrevTrack"
      },
      "description": "Previous track",
      "global": true
    },
    "play-track": {
      "suggested_key": {
        "default": "MediaPlayPause"
      },
      "description": "Play/Pause track",
      "global": true
    },
    "stop-track": {
      "suggested_key": {
        "default": "MediaStop"
      },
      "description": "Stop",
      "global": true
    }
  },
  "externally_connectable": {
    "ids": [
      "cmdehdaenpmcjddcnfejenikokbgghei",
      "eanbfbjgmjajhkkojhpapbinoeaagonn",
      "lfcocapfophldijbogicgmhinmehhfil",
      "nekdikklkpofpjjhkdhggkciicdhpmmk"
    ],
    "matches": [
      "*://www.youtube.com/*",
      "*://www.netflix.com/*"
    ]
  },
  "manifest_version": 2,
  "name": "iPrevYou - YouTubeâ„¢ Player",
  "short_name": "iPrevYou Player",
  "permissions": [
    "*://*/*",
    "<all_urls>",
    "app.window.alwaysOnTop",
    "app.window.fullscreen",
    "app.window.fullscreen.overrideEsc",
    "app.window.shape",
    "background",
    "browser",
