{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Latex for Yahoo Answers",
  "description": "LaTeX formulas in Yahoo Answers.",
  "short_name": "Latex4Yahoo",
  "version": "0.1",
  "homepage_url": "http://www.youmath.it",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.js",
        "app.js"
      ],
      "matches": [
        "http://*.yahoo.com/*",
        "https://*.yahoo.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*.yahoo.com/*",
    "https://*.yahoo.com/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
