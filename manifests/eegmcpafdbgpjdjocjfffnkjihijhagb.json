{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GW Sistemas",
  "author": "Mateus de Paula Veloso",
  "description": "GW Sistemas - Extensão",
  "manifest_version": 2,
  "version": "2.11",
  "browser_action": {
    "default_icon": {
      "16": "icones/16.png",
      "19": "icones/19.png",
      "20": "icones/20.png",
      "32": "icones/32.png",
      "38": "icones/38.png",
      "40": "icones/40.png"
    },
    "default_popup": "index.html",
    "default_title": "GW Sistemas - Extensão"
  },
  "content_scripts": [
    {
      "js": [
        "monitoring-gw.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "128": "icones/128.png",
    "16": "icones/16.png",
    "32": "icones/32.png",
    "48": "icones/48.png",
    "64": "icones/64.png"
  },
  "web_accessible_resources": [
    "manifest.json"
  ]
}
