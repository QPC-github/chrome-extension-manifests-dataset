{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alarm",
  "version": "0.71",
  "manifest_version": 2,
  "description": "This extension help you to remember yours appointments. That is simple. Give us your feedback for further improve.",
  "icons": {
    "128": "icon_128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_title": "Alarm",
    "default_popup": "popup.html"
  }
}
