{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "photobucket embed fix",
  "version": "0.0.3",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.photobucket.com/*",
    "*://photobucket.com/*",
    "*://*.wp.com/*.photobucket.com/*",
    "*://*.wp.com/photobucket.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
