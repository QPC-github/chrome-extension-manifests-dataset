{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "external/parseuri.js",
      "framework/common.js",
      "framework/prefs.js",
      "framework/remoteConfig.js",
      "background.js",
      "chrome_helper.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "docload.js",
        "external/parseuri.js",
        "framework/common.js",
        "injection.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "name": "OkayFreedom",
  "version": "1.6.0",
  "homepage_url": "https://www.okayfreedom.com/",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "icons": {
    "16": "logos/logo16.png",
    "48": "logos/logo48.png",
    "96": "logos/logo96.png",
    "128": "logos/logo128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "web_accessible_resources": [
    "web/ciuvo.js",
    "web/libs/ciuvo-addon-sdk.min.js",
    "web/libs/jquery.min.js",
    "web/leaderboard.html",
    "web/leaderboard.js",
    "web/leaderboard.css",
    "web/text.js",
    "web/textresource.js",
    "web/images/leaderboard_left.png",
    "web/images/logo.png",
    "web/images/logo.png",
    "web/images/en/productlabel.png",
    "web/images/de/productlabel.png",
    "web/images/icon-help.png",
    "web/images/icon-close.png",
    "web/images/icon-buy.png",
    "img/error.png",
    "img/ok_logo_big_none.png",
    "img/rainbow.png",
    "img/ring.png",
    "img/spinner.png"
  ]
}
