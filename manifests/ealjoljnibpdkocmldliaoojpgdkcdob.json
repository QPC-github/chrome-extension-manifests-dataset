{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Slick RSS",
  "version": "2.994",
  "description": "A full featured RSS reader that's fully contained within the browser.",
  "options_page": "options.html",
  "icons": {
    "128": "icon_128.png",
    "16": "transmit.png"
  },
  "browser_action": {
    "default_icon": "transmit.png",
    "default_title": "Slick RSS"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "bookmarks"
  ]
}
