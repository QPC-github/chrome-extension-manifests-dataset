{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "STEP to STL Converter",
  "default_locale": "en",
  "description": "Convert STEP files to STL format",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.0",
  "icons": {
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
