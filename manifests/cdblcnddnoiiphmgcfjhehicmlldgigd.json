{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IMAO",
  "version": "0.0.0.1",
  "description": "Image downloader, click button to download all images on a webpage",
  "permissions": [
    "tabs",
    "declarativeContent",
    "storage",
    "notifications",
    "http://*/",
    "https://*/",
    "downloads"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "resources/images/d1-16.png",
      "32": "resources/images/d1-32.png",
      "64": "resources/images/d1-64.png",
      "128": "resources/images/d1-128.png"
    }
  },
  "icons": {
    "16": "resources/images/d1-16.png",
    "32": "resources/images/d1-32.png",
    "64": "resources/images/d1-64.png",
    "128": "resources/images/d1-128.png"
  },
  "manifest_version": 2
}
