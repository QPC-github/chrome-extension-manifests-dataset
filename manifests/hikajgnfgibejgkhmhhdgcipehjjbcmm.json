{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VerifierScan",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "VerifierScan - a tool for helping users identify untrustworthy websites or fake news.",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "verifierscan_logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "verifierscan_logo.png"
  }
}
