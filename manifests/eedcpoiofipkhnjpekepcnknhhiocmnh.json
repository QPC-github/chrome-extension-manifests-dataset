{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "History",
  "short_name": "History",
  "version": "1.0.0.3",
  "manifest_version": 2,
  "homepage_url": "http://singleclickapps.com/history-app",
  "description": "A shortcut to Chrome history",
  "icons": {
    "16": "icon.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [],
  "offline_enabled": true,
  "incognito": "spanning",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  }
}
