{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Bookmarks",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A better style page for local bookmarks",
  "browser_action": {
    "default_icon": "./lib/icon.png",
    "default_popup": "./jump/jump.html",
    "default_title": "Better Bookmarks"
  },
  "chrome_url_overrides": {
    "newtab": "./main.html"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}
