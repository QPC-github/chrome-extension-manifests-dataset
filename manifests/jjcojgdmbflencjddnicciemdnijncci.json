{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Adwords Change",
  "description": "This extension allows create a comments on a Google Adwords Campains, saving history in a Google Spreadsheet.",
  "version": "1.1",
  "minimum_chrome_version": "29",
  "oauth2": {
    "client_id": "",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "background": {
    "scripts": [
      "select.js"
    ]
  },
  "icons": {
    "16": "icon-sheet-bit.png",
    "48": "icon-sheet-small.png",
    "128": "icon-sheet-large.png"
  },
  "permissions": [
    "identity",
    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "https://accounts.google.com/",
    "contextMenus"
  ]
}
