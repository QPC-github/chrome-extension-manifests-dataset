{
  "background": {
    "persistent": false,
    "scripts": [
      "js/newtab-event.js"
    ]
  },
  "browser_action": {
    "default_title": "Akte Inspirator"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "externally_connectable": {
    "matches": [
      "https://*.genealogiewerkbalk.nl/*"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.genealogiewerkbalk.nl https://ssl.google-analytics.com; object-src 'self'",
  "description": "Vervangt de nieuwe tab met een willekeurige historische akte ter inspiratie met genealogische zoekfuncties.",
  "icons": {
    "512": "img/icon-512.png",
    "128": "img/icon-128.png",
    "48": "img/icon-48.png",
    "16": "img/icon-16.png"
  },
  "manifest_version": 2,
  "name": "Akte Inspirator",
  "offline_enabled": true,
  "permissions": [
    "storage",
    "*://*.genealogiewerkbalk.nl/*"
  ],
  "short_name": "Akte Inspirator",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.22",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
