{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VPN Sumo",
  "version": "0.2.2",
  "description": "Free VPN and Proxy Services free of charge!",
  "browser_action": {
    "default_icon": "icon_dis16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "proxy",
    "alarms",
    "cookies",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "https://vpnsumo.com/",
    "<all_urls>"
  ],
  "manifest_version": 2
}
