{
  "background": {
    "scripts": [
      "/scripts/defaults.js",
      "meogo.js"
    ]
  },
  "browser_action": {
    "default_icon": "/icon_128.png",
    "default_popup": "/views/popup.html"
  },
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "128": "/icon_128.png",
    "16": "/icon_16.png",
    "48": "/icon_48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "name": "__MSG_extName__",
  "options_page": "/views/options.html",
  "permissions": [
    "activeTab",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "http://*.com/*",
    "https://*.com/*",
    "http://*.net/*",
    "https://*.net/*",
    "http://*.ru/*",
    "https://*.ru/*",
    "http://*.org/*",
    "https://*.org/*",
    "http://*.by/*",
    "https://*.by/*",
    "http://*.zone/*",
    "https://*.zone/*",
    "http://*.biz/*",
    "https://*.biz/*",
    "http://*.top/*",
    "https://*.top/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4",
  "web_accessible_resources": [
    "/images/open.png",
    "/images/download.png"
  ]
}
