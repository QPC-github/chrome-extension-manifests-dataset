{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Linkifier for Soundtrack.IO",
  "version": "1.0.6",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://soundtrack.io/*"
      ],
      "js": [
        "linkifier.js"
      ]
    }
  ],
  "permissions": [
    "notifications"
  ]
}
