{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Browser Information",
  "short_name": "Browser Information",
  "version": "1.1",
  "description": "Browser Information",
  "icons": {
    "16": "images/browser-info-16.png",
    "32": "images/browser-info-32.png",
    "48": "images/browser-info-48.png",
    "64": "images/browser-info-64.png",
    "128": "images/browser-info-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/browser-info-19.png",
      "38": "images/browser-info-38.png"
    },
    "default_title": "Browser Information",
    "default_popup": "html/browserinfo.html"
  }
}
