{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QQBookmarks",
  "version": "2",
  "description": "Easy to add a web page as a bookmark to shuqian.qq.com. This is NOT a official extension.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "action_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://shuqian.qq.com/*"
      ],
      "js": [
        "beautify.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
