{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "4D move tool",
  "description": "change style=left: x px; top: y px",
  "version": "1.5",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "128.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "128.png"
  }
}
