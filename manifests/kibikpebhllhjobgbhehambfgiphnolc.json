{
  "manifest_version": 2,
  "name": "Findex",
  "description": "Set Findex as default search engine",
  "permissions": [],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1",
  "browser_action": {
    "default_title": "Findex",
    "default_icon": "img/icon-128.png"
  },
  "icons": {
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "64": "img/icon-64.png",
    "96": "img/icon-96.png",
    "128": "img/icon-128.png",
    "256": "img/icon-256.png"
  },
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Findex",
      "keyword": "Findex",
      "favicon_url": "https://findex.vip/favicon.ico",
      "search_url": "https://search.findex.vip/search?q={searchTerms}",
      "image_url": "",
      "suggest_url": "",
      "instant_url": "",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
