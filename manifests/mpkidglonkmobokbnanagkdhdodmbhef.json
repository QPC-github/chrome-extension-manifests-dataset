{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Text navigator",
  "description": "Tool for visual navigation throw text",
  "version": "1.1",
  "icons": {
    "16": "graph16.png",
    "48": "graph48.png",
    "128": "graph128.png"
  },
  "permissions": [
    "activeTab",
    "https://textnav.app/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "graph16.png",
    "default_title": "Open a long article and press this button"
  },
  "manifest_version": 2
}
