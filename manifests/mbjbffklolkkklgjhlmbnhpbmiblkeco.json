{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One Touch Dialer",
  "description": "Single click access to frequently used websites.",
  "version": "0.0.0.10",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "/onetouchdialer/launcher.html"
  },
  "icons": {
    "16": "/onetouchdialer/otd16.png",
    "48": "/onetouchdialer/otd48.png",
    "128": "/onetouchdialer/otd128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "topSites",
    "clipboardRead",
    "clipboardWrite",
    "unlimitedStorage",
    "*://*/*",
    "bookmarks"
  ]
}
