{
  "manifest_version": 2,
  "name": "Pixabay Tab Background Images",
  "description": "Stunning images from Pixabay.com as backgrounds in your new tabs.",
  "homepage_url": "https://pixabay.com/",
  "version": "1.0.5",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_icon": "icons/icon19.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "events.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
