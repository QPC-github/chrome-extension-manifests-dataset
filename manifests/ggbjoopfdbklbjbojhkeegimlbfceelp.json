{
  "background": {
    "page": "bg.html"
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'; script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "This extension is a part of the Bakinu package for Chrome browser and is responsible for new tab search",
  "icons": {
    "16": "PublisherImages/bakinu16.png",
    "48": "PublisherImages/bakinu48.png",
    "128": "PublisherImages/bakinu128.png"
  },
  "manifest_version": 2,
  "name": "Bakinu NT",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "cookies"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.0"
}
