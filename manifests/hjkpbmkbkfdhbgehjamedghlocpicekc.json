{
  "author": "Rasta - Rhafaman | Rha Marketing",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "css/style.css",
        "js/noty/lib/noty.css",
        "js/noty/lib/themes/sunset.css"
      ],
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/noty/lib/noty.min.js",
        "js/rastabase.js"
      ],
      "matches": [
        "*://*.instagram.com/*",
        "*://*.post.con3ct.com.br/accounts-ext"
      ],
      "persistent": false,
      "run_at": "document_end"
    }
  ],
  "description": "Conectar sua conta em nosso sistema agora está muito fácil com nossa extensão!",
  "externally_connectable": {
    "matches": [
      "https://post.con3ct.com.br/*"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Conector Up",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "webRequest",
    "unlimitedStorage",
    "notifications",
    "storage",
    "clipboardWrite",
    "webRequestBlocking",
    "*://*.instagram.com/*",
    "*://*.instatop.app/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.2"
}
