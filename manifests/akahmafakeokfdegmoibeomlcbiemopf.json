{
  "icons": {
    "128": "128-pic.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "history",
    "tabs",
    "topSites"
  ],
  "background": {
    "scripts": [
      "back_js.js"
    ]
  },
  "browser_action": {
    "default_icon": "128-pic.png",
    "default_title": "Petra Jordan Theme & New Tab",
    "default_popup": "newtab_page.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab_page.html"
  },
  "manifest_version": 2,
  "name": "__MSG_headTitle__",
  "version": "2.0",
  "default_locale": "en",
  "description": "Petra Jordan Themes & New Tab Is A Cool Extension With 4K Wallpapers, Weather, Clock And More Amazing Features."
}
