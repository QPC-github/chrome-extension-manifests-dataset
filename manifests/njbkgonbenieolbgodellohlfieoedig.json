{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Enlyft Sales Intelligence",
  "description": "Enlyft â€“ Quickly qualify any company based on what technologies they use",
  "version": "1.2.4",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://*/"
      ],
      "all_frames": true,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "tldjs.js",
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://app.enlyft.com/*"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon-logo.png",
    "16": "icon-logo-16.png",
    "128": "icon-logo-128.png"
  },
  "permissions": [
    "cookies",
    "*://*.idatalabs.com/*",
    "*://*.enlyft.com/*",
    "*://*.dynamics.com/*",
    "*://*.salesforce.com/*",
    "*://*.force.com/*",
    "*://*.linkedin.com/*",
    "*://*.hubspot.com/*",
    "*://*.pipedrive.com/*",
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://cdn.pendo.io/agent/static/af821ab6-c09d-470d-72e1-a3b66ffca6dd/pendo.js https://data.pendo.io/ https://www.googletagmanager.com; object-src 'self'"
}
