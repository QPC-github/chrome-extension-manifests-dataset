{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "getTitle",
  "description": "Get Web Site's title",
  "version": "1.0.2",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon20.png",
    "default_popup": "popup.html"
  }
}
