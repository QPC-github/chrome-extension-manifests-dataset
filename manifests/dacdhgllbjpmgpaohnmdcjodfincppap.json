{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cookie Jacker",
  "version": "5.0",
  "manifest_version": 2,
  "description": "方便使用盗取来的Cookie",
  "author": "wanghao6677",
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "background"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Cookie Jacker",
    "default_popup": "popup.html"
  }
}
