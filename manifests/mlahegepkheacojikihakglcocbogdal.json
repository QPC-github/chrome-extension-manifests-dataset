{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "News Reader",
  "description": "This extension allows users to read news sites from a popup",
  "version": "0.1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webRequestBlocking",
    "webRequest",
    "<all_urls>"
  ]
}
