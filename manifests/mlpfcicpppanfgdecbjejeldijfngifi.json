{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search Me",
  "default_locale": "en",
  "description": "Search Me brings daily used websites in one place on your new tab.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "style.css"
  ]
}
