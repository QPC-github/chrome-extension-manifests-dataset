{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sunshine Chrome Extension",
  "Short Name": "Sunshine",
  "version": "5.5",
  "description": "Chrome Native Messaging for Sunshine Plugin Player",
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*",
      "*://fidotv.ca/*",
      "*://eclipselab1.vf.rogers.com/*",
      "*://eclipsetv2.vf.rogers.com/*",
      "*://eclipsetv.vf.rogers.com/*",
      "*://eclipsedftv.vf.rogers.com/*",
      "*://eclipsedftv2.vf.rogers.com/*",
      "*://eclipseux.cdn.vf.rogers.com/*",
      "*://24.114.117.75/*",
      "*://202.174.93.105:*/*",
      "*://fidotv.s3-website-us-west-2.amazonaws.com/*",
      "*://eclipsedrtv.vf.rogers.com/*",
      "*://27.50.4.63:*/*",
      "*://rogerslab3.cdn.vf.rogers.com/*",
      "*://rogerstvlab1.cdn.vf.rogers.com/*",
      "*://rogerstvlab2.cdn.vf.rogers.com/*",
      "*://fidotvtest.com/*"
    ]
  }
}
