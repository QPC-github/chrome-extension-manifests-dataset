{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open Multiple URLs",
  "version": "1.0",
  "description": "Extension for Opening Multiple URLs",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "urls16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "urls16.png",
    "128": "urls128.png"
  }
}
