{
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "img/home.png",
    "default_title": "Complete Tab"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "name": "Complete Tab",
  "description": "Complete Tab, a Chrome-based cloud applications platform, for more elegant and easy Chrome usage.",
  "icons": {
    "16": "img/home/home16.png",
    "32": "img/home/home32.png",
    "128": "img/home/home128.png"
  },
  "offline_enabled": true,
  "permissions": [
    "background",
    "geolocation",
    "management",
    "topSites",
    "bookmarks",
    "unlimitedStorage",
    "topSites",
    "identity",
    "http://*/",
    "https://*/",
    "chrome://favicon/",
    "history",
    "alarms",
    "notifications",
    "tabs",
    "storage",
    "activeTab",
    "declarativeContent",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.9.1"
}
