{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name": "TabSqueeze",
  "icons": {
    "16": "squeezed16x16.png",
    "32": "squeezed32x32.png",
    "48": "squeezed48x48.png",
    "64": "squeezed64x64.png",
    "128": "squeezed128x128.png"
  },
  "browser_action": {
    "default_title": "TabSqueeze",
    "default_icon": "squeezed64x64.png"
  },
  "author": "Maxim Topchu <maximtop@gmail.com>",
  "content_scripts": [
    {
      "matches": [
        "http://tabsqueeze.com/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "notifications",
    "storage",
    "contextMenus"
  ],
  "description": "Squeeze your tabs and save or share them anywhere",
  "version": "1.0.0"
}
