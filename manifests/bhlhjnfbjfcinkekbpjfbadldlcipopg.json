{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SaveFromGmail",
  "description": "An extension that enables you to easily save mails from Gmail to your computer",
  "author": "Mark Boom",
  "version": "2.1.1",
  "homepage_url": "https://github.com/mpboom/SaveFromGmail",
  "options_page": "options.html",
  "permissions": [
    "*://mail.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "short_name": "SFG"
}
