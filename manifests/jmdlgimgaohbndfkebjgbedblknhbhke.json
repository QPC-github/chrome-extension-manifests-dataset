{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Same Picture of Charles Barkley",
  "description": "A simple edit of Ben Halpern's hilarious extension.",
  "version": "1.0.1",
