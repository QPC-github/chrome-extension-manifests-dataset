{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "css/bootstrap.css"
      ],
      "js": [
        "js/jquery-3.2.1.min.js",
        "js/bootstrap.js",
        "js/bootbox.min.js",
        "core.js",
        "bots/MartingaleBot.js",
        "bots/PopularBot.js",
        "bots/GreenBot.js",
        "bots/Custom.js"
      ],
      "matches": [
        "*://*.csgoroll.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "AUTOPROFIT - CSGOROLL",
  "description": "AUTOPROFIT bot for CSGOROLL",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "*://*.csgoroll.com/*"
  ]
}
