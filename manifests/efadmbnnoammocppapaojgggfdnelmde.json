{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.3.3",
  "name": "Genie - Extemp, Congress, Debate",
  "manifest_version": 2,
  "key": "",
  "description": "Genie puts Evidence At Your Command for Extemp, Congress, and Debate.",
  "browser_action": {
    "default_title": "Genie - Congress, Debate, Extemp",
    "default_icon": "genie-icon.png",
    "default_popup": "v4/v4-popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+G",
        "mac": "Alt+G"
      }
    }
  },
  "icons": {
    "16": "genie-icon-16.png",
    "48": "genie-icon-48.png",
    "128": "genie-icon-128.png"
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; connect-src http://*/ https://*/"
}
