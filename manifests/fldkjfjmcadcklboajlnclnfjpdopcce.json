{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Switch Direction in edits, useful for mixed RTL and LTR texts.",
  "default_locale": "en",
  "name": "Switch Direction",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "options_page": "options.html",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "manifest_version": 2,
  "version": "0.6"
}
