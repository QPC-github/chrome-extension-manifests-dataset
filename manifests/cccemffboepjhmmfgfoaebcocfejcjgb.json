{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SoyTime",
  "description": "This extension tells you how long your Soylent wait time is going to be.",
  "version": "2.1",
  "permissions": [
    "https://www.corsproxy.com/www.soylent.me",
    "https://www.soylent.me",
    "https://fonts.googleapis.com/css?family=Nunito",
    "https://fonts.googleapis.com/css?family=Electrolize",
    "http://www.thegrimcoder.com",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "soyicon24.png",
    "default_popup": "page.html"
  }
}
