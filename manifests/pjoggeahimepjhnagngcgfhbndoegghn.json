{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lowyat forum accounts switcher",
  "description": "Dupes management without hassel, an extension that let you switch between accounts quickly in lowyat forum.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies",
    "storage"
  ],
  "browser_action": {
    "default_title": "Lowyat Accounts Switcher",
    "default_icon": "icon_small.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  }
}
