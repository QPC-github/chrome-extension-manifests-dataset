{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Data Player",
  "description": "This extension provides access to a page's data layer",
  "version": "0.0.4",
  "devtools_page": "devtools.html",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Data Player"
  },
  "permissions": []
}
