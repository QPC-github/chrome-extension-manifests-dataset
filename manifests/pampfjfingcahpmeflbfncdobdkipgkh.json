{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "habbidplayer.js",
        "executaRadio.js"
      ]
    }
  ],
  "name": "Habbid",
  "description": "A extens√£o de uma das comunidades jovens mais populares da internet. Habbid - Conecte-se a esse mundo (+13).",
  "version": "1.0.2",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Habbid",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "background"
  ]
}
