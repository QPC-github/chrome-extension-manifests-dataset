{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ciclo Menstrual Femenino",
  "version": "0.2",
  "description": "Calcula el ciclo menstrual y los dias fertiles de la mujer. Guarda un historial, planificacion familiar.",
  "icons": {
    "128": "images/128.png",
    "16": "images/icon.png",
    "24": "images/24.png"
  },
  "permissions": [
    "background"
  ],
  "background": {
    "scripts": [
      "scripts/jquery-1.8.2.min.js",
      "scripts/jquery-ui.min.js",
      "scripts/less.min.js",
      "scripts/core.js"
    ],
    "css": [
      "styles/jquery-ui.min.css",
      "styles/style.css"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Ciclo menstrual femenino"
  },
  "web_accessible_resources": [
    "images/128.png"
  ],
  "incognito": "spanning"
}
