{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Information Market Control Panel",
  "version": "5.1",
  "description": "This is the Information Market control panel extension",
  "author": "Arash Molavi Kakhki (http://www.ccs.neu.edu/home/arash/, arash@ccs.neu.edu)",
  "homepage_url": "https://informationmarket.info",
  "short_name": "Information Market",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "storage",
    "tabs",
    "proxy",
    "notifications"
  ],
  "background": {
    "scripts": [
      "jquery-1.9.1.min.js",
      "background.js",
      "popup.js",
      "jquery.validate.min.js"
    ]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "Transactional Privacy control pannel",
    "default_icon": "images/off.png"
  },
  "icons": {
    "48": "images/off_48.png",
    "128": "images/off_128.png"
  },
  "manifest_version": 2
}
