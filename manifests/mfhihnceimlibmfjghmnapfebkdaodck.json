{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DuckDuckRun",
  "description": "Extends the DuckDuckGo search with some shortcuts",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://duckduckgo.com/*"
      ],
      "css": [
        "src/css/style.css"
      ],
      "js": [
        "src/js/script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*.png"
  ]
}
