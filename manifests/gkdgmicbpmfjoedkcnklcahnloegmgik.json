{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "EchoSign User Search",
  "description": "Search the EchoSign system for users from any web page.",
  "version": "1.2",
  "browser_action": {
    "default_icon": "EchoSign.png",
    "name": "Click to search"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "newSearch.js"
    ]
  },
  "icons": {
    "16": "EchoSign.png",
    "48": "EchoSign.png",
    "128": "EchoSign.png"
  }
}
