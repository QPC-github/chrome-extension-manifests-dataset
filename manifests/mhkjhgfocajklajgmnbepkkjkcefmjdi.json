{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Sports.ru",
  "version": "1.1.9",
  "description": "Sports.ru",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://www.sports.ru/*"
  ],
  "icons": {
    "16": "img/ico.png",
    "48": "img/sports_icon48.png",
    "128": "img/sports_icon.png"
  },
  "browser_action": {
    "default_title": "Sports.ru",
    "default_icon": "img/ico.png",
    "default_popup": "popup.html"
  }
}
