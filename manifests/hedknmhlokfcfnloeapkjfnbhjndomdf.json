{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Öffnungszeiten",
  "description": "Alle Öffnungszeiten in Deiner Stadt",
  "version": "1.5",
  "browser_action": {
    "default_icon": "img/offen32x32.png",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "img/offen32x32.png",
    "48": "img/icon-trans.png",
    "128": "img/icon-trans.png"
  },
  "permissions": [
    "geolocation"
  ]
}
