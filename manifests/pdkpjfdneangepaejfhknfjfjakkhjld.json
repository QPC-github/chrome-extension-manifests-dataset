{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NatterBox",
  "version": "0.2.2.1",
  "description": "A box of Natters.",
  "content_scripts": [
    {
      "matches": [
        "https://natter.com/*"
      ],
      "js": [
        "natterbox.script.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/natterbox-16.png",
    "32": "icons/natterbox-32.png",
    "64": "icons/natterbox-64.png",
    "128": "icons/natterbox-128.png"
  },
  "permissions": [
    "storage"
  ]
}
