  "manifest_version": 2,
  "name": "一鍵清快取",
  "description": "When you click this button, will clear this website localStorage and refresh again.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "一鍵快速清除Cookie"
  },
  "offline_enabled": true,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
