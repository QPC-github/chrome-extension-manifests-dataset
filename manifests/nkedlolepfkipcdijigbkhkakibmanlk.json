{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "plugin804 Screen Sharing",
  "author": ",IQ Technologies",
  "version": "2.4",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for https://plugin.love/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://plugin.love/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.tajacreates.com/*",
      "https://*.ourplugin.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
