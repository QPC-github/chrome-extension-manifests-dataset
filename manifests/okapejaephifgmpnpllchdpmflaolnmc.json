{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    },
    "default_popup": "popup.html",
    "default_title": "infodot"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "128.png",
    "256": "256.png",
    "512": "512.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "short_name": "__MSG_shortName__",
  "version": "0.3"
}
