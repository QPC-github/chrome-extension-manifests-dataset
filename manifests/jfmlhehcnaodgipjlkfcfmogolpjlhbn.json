{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "iStock Forum Manager",
  "description": "Put an iSock in it! Minor improvements for the iStock forums. Silence people you don't want to read, highlight those you do.",
  "version": "1.3",
  "icons": {
    "128": "isock-icon-1.png"
  },
  "content_scripts": [
    {
      "js": [
        "put_an_isock_in_it.user.js"
      ],
      "matches": [
        "http://*.istockphoto.com/forum_messages.php*",
        "https://*.istockphoto.com/forum_messages.php*"
      ]
    }
  ]
}
