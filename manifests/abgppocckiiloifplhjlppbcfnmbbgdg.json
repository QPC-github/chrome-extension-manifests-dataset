{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ctrl Data",
  "description": "Ctrl Data helps you understand how much data you give away with every search, and shows you how to minimize your data transactions. ",
  "version": "1.0.3",
  "content_scripts": [
    {
      "js": [
        "js/jquery-1.11.2.min.js",
        "contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "128": "icons/128x128.png",
    "16": "icons/16x16.png",
    "48": "icons/48x48.png"
  },
  "browser_action": {
    "default_icon": "icons/48x48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "http://www.joranbackx.nl/*",
    "https://steffisteffi.nl/*"
  ],
  "web_accessible_resources": [
    "images/logo-ctrl.svg"
  ]
}
