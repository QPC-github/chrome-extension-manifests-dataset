{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "short_name": "Denwa Share",
  "description": "__MSG_extDescription__",
  "version": "1.0.1",
  "default_locale": "en",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "desktopCapture"
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "34"
}
