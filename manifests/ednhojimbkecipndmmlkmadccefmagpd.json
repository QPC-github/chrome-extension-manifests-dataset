{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Boxclock",
  "description": "Based on visual synesthetic experiences, this site is a visual clock that represents the minute, hour, and week of any given date.",
  "version": "1.0",
  "icons": {
    "128": "assets/images/icon128.png",
    "64": "assets/images/icon64.png",
    "48": "assets/images/icon48.png",
    "16": "assets/images/icon16.png"
  },
  "browser_action": {
    "default_title": "Boxclock"
  },
  "chrome_url_overrides": {
    "newtab": "automaton.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ]
}
