{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "getjet",
  "version": "0.4.7",
  "manifest_version": 2,
  "description": "Get Jet: An add-on for simple download from publishers.",
  "homepage_url": "http://getarticle.com",
  "icons": {
    "16": "src/browser_action/images/icon-16.png",
    "32": "src/browser_action/images/icon-32.png",
    "64": "src/browser_action/images/icon-64.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "web_accessible_resources": [
    "src/browser_action/browser_action.html",
    "src/inject/inject.js",
    "/src/browser_action/images/icon-dis-32.png",
    "/src/browser_action/images/icon-anim-32.gif",
    "/src/browser_action/images/sim-loading.gif",
    "/src/browser_action/images/offline_btn.png",
    "/src/browser_action/images/pdf_btn.png",
    "/src/browser_action/images/auth_error@2x.png",
    "/src/browser_action/images/download_btn.png",
    "/src/browser_action/images/sim-loading@2x.png",
    "/src/browser_action/images/icon-64.png",
    "/src/browser_action/images/icon-32.png",
    "/src/browser_action/images/msg.png",
    "/src/browser_action/images/auth_error.png"
  ],
  "browser_action": {
    "default_icon": "src/browser_action/images/icon-dis-32.png",
    "default_title": "GetJet Extension",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "chrome://favicon/",
    "contentSettings",
    "contextMenus",
    "cookies",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/circle-progress.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
