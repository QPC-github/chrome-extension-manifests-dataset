{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spoutbase",
  "description": "Painless point-and-click data extraction and web scraping",
  "author": "spoutbase.com",
  "icons": {
    "16": "spout-black-16.png",
    "32": "spout-black-32.png",
    "48": "spout-black-48.png",
    "64": "spout-black-64.png",
    "128": "spout-black-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://www.spoutbase.com/"
  ],
  "web_accessible_resources": [
    "fonts/LibreFranklin-Regular.ttf",
    "fonts/LibreFranklin-Medium.ttf",
    "fonts/LibreFranklin-Bold.ttf",
    "spout-white-128.png",
    "spout-black-128.png",
    "user.svg"
  ],
  "browser_action": {
    "default_icon": {
      "16": "spout-black-16.png",
      "32": "spout-black-32.png",
      "48": "spout-black-48.png",
      "64": "spout-black-64.png",
      "128": "spout-black-128.png"
    }
  },
  "manifest_version": 2,
  "homepage_url": "https://www.spoutbase.com",
  "version": "1.1.0"
}
