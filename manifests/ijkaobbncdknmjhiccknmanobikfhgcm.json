{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pandora to Playlist Converter",
  "description": "",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "logo.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Pandora to Playlist Converter",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "background",
    "tabs",
    "cookies",
    "http://pandora.com/*",
    "http://yourflavor.me/*",
    "<all_urls>"
  ]
}
