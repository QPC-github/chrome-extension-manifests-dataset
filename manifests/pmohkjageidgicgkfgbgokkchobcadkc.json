{
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "description": "Automatically opens the official Chrome releases blog when Chrome autoupdates itself",
  "manifest_version": 2,
  "name": "Update Notifier",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2",
  "icons": {
    "128": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
