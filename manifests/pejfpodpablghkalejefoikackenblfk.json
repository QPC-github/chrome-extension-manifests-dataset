{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Numerologia Gratis",
  "description": "Numerologia Pitagorica Gratuita",
  "version": "1.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://en.wiktionary.org/; object-src 'self'",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon2.png",
    "16": "icon.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "https://en.wiktionary.org/",
    "http://en.wiktionary.org/",
    "http://numerologiagratis.com.ar/",
    "tabs",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "notifications"
  ]
}
