{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "name": "Screenshot It",
  "description": "Screenshot It",
  "icons": {
    "48": "icon.png"
  },
  "version": "0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
