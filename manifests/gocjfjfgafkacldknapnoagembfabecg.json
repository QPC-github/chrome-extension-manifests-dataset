{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "9gag assistant",
  "description": "This extension will show video durations in post titles and controls volume.",
  "author": "Catfood",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "*://9gag.com/*"
      ],
      "js": [
        "9gag.js"
      ]
    }
  ],
  "icons": {
    "16": "icon_big.png",
    "48": "icon_big.png",
    "128": "icon_big.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
