{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HoofBump!",
  "description": "This extension takes the bro out of the PonyHoof extension.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/",
        "https://www.facebook.com/*"
      ],
      "css": [
        "hoofbump.css"
      ],
      "js": [
        "jquery.js",
        "hoofbump.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.facebook.com/*"
  ]
}
