{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wahl-O-Mehr",
  "description": "Zeigt die Ergebnisse f√ºr alle Parteien gleichzeitig.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.wahl-o-mat.de/*/main_app.php?cb_themen*",
        "https://*.wahl-o-mat.de/*/main_app.php?cb_parteien*"
      ],
      "js": [
        "jquery.min.js",
        "wahlomehr.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
