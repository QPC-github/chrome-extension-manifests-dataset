{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Insperity People Parser",
  "short_name": "Insperity Parser",
  "description": "This extension parses any relavant contact information on the current page into your Insperity Account",
  "version": "2.1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Insperity People Parser"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://insperity.brightmove.com/",
    "https://*.linkedin.com/"
  ]
}
