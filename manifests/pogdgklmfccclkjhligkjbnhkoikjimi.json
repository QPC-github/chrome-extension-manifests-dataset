{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "loggedout.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon.png"
  },
  "name": "TimeLoggr",
  "description": "TimeLoggr is a must have application if you use Assembla, and want to track time spent on tickets.",
  "version": "1.0",
  "manifest_version": 2
}
