{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "上海易连科技有限公司",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "http://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "description": "提供易连科技Web应用扩展功能",
  "manifest_version": 2,
  "name": "易连助手",
  "permissions": [
    "notifications",
    "tabs",
    "desktopCapture"
  ],
  "origins": "<all_urls>",
  "optional_permissions": [
    "tabs",
    "<all_urls>"
  ],
  "short_name": "LinkEasy",
  "version": "1.0.0.1",
  "version_name": "LinkEasy 1.0.0.1"
}
