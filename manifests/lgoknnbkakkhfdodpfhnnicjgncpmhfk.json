{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Darwinism - The Craziest Extension",
  "short_name": "DMJ",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "jquery.fancyboxpop.css"
      ],
      "js": [
        "jquery.js",
        "jquery.fancyboxpop.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "32": "D.png",
    "48": "D.png",
    "128": "D.png"
  },
  "browser_action": {
    "default_icon": "D.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "injector.js",
    "close_icon.png",
    "notify.html",
    "notify.js",
    "light-loader.gif"
  ]
}
