{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL2QR",
  "version": "0.2.0",
  "description": "Generate QR code from URL.",
  "icons": {
    "16": "img/icon32.png",
    "48": "img/icon96.png",
    "128": "img/icon256.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon38.png",
    "default_title": "URL2QR",
    "default_popup": "html/popup.html"
  },
  "offline_enabled": false,
  "author": "1000ch"
}
