{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon/48.png",
    "default_popup": "popup.html",
    "default_title": "SafeZone"
  },
  "content_security_policy": "default-src 'self'",
  "description": "Сообщество SafeZone - всегда под рукой! ",
  "icons": {
    "128": "icon/128.png",
    "16": "icon/16.png",
    "48": "icon/48.png"
  },
  "manifest_version": 2,
  "name": "SafeZone",
  "permissions": [
    "tabs",
    "bookmarks",
    "activeTab",
    "notifications",
    "http://www.safezone.cc/",
    "http://*.google.com/",
    "unlimitedStorage",
    "http://*",
    "http://*/*",
    "https://*",
    "https://*/*",
    "contextMenus",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.3"
}
