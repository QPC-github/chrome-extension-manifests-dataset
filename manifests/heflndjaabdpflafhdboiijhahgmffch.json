{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Site DOM Tree",
  "version": "1.0",
  "description": "Quickly analyze a website's DOM tree",
  "author": "Gregory Dalton",
  "icons": {
    "128": "logo_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo_32.png"
  }
}
