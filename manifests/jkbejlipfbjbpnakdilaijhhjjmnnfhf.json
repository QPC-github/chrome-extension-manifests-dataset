{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Maatwerk Online Datareport",
  "version": "1.0",
  "manifest_version": 2,
  "description": "It collect user internet,browser,screen information.User can copy info on clipboard or email and report issue. It will help to dev.",
  "homepage_url": "http://maatwerkonline.nl",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "icons": {
    "16": "images/logo-16.png",
    "32": "images/logo-32.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "clipboardWrite"
  ]
}
