{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ReturnToSearch",
  "version": "1.0",
  "description": "Returns to most recent search page.",
  "icons": {
    "48": "icons/return-to-search-48.png",
    "96": "icons/return-to-search-96.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "history"
  ],
  "browser_action": {
    "default_icon": "icons/return-to-search-32.png",
    "default_title": "Return to Search",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "returntosearch.js"
    ]
  }
}
