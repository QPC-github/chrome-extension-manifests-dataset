{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Url2QR",
  "description": "Convert current url to qrcode",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "contextMenus",
    "unlimitedStorage",
    "notifications"
  ]
}
