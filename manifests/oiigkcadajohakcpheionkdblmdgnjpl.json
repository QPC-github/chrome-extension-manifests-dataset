{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "HTTP Status code",
  "version": "1.0.3",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "webRequest",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "With this tool, you are able to get oppen a huge database of HTTP Codes and to check the current page! Just with up to 2 clicks.",
  "homepage_url": "https://github.com/kloentje2/HTTP-Status-code",
  "browser_action": {
    "default_title": "HTTP Status codes",
    "default_popup": "popup.html"
  }
}
