{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Traffic",
  "description": "A clearer view of raw network packets.",
  "version": "1.23",
  "author": "abagoforgans",
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html",
    "default_title": "Traffic"
  },
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  }
}
