{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "KidyGo",
  "version": "1.9.0",
  "manifest_version": 2,
  "description": "Extension officielle de KidyGo. Soyez inform√©s des annonces disponibles sur KidyGo lorsque vous naviguez sur d'autres sites !",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_popup": "index_popup.html",
    "default_icon": "favicon.png"
  },
  "web_accessible_resources": [
    "index_popup.html",
    "kidygo_bar.html"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "geolocation"
  ],
  "content_scripts": [
