{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LocaleSearch",
  "description": "Find where you rank for specific keywords in different locations around the world.",
  "version": "1",
  "author": "Jonny Hudson",
  "browser_action": {
    "default_icon": "Logo128px.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "Logo16px.png",
    "48": "Logo48px.png",
    "128": "Logo128px.png"
  },
  "permissions": [
    "activeTab"
  ]
}
