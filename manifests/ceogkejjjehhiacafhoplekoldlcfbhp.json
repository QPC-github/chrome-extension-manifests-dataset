{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Linksmith",
  "version": "0.2.11",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; img-src *; object-src 'self'",
  "description": "Quick and easy access to sites you frequently look up things on.",
  "icons": {
    "16": "images/i16.png",
    "32": "images/i32.png",
    "64": "images/i64.png",
    "128": "images/i128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/i32.png",
    "default_title": "Linksmith",
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "ls"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
