{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "description": "__MSG_ext_desc__",
  "version": "1.0.2",
  "default_locale": "en",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "mmi_icon16.png",
    "48": "mmi_icon48.png",
    "128": "mmi_icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture",
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.moshimoshiconcierge.net/*",
      "https://*.sosuiken.net/*"
    ]
  }
}
