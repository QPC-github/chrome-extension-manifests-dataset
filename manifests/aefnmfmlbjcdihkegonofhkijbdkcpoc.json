{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "BackgroundPage.html"
  },
  "browser_action": {
    "default_icon": "perf-scorecard-19.png",
    "default_popup": "guide.html",
    "default_title": "Performance Scorecard"
  },
  "content_security_policy": "default-src 'self'; connect-src http: https:; script-src 'self'; img-src 'self' data:; style-src 'self' 'unsafe-inline'; frame-src 'self' about: ",
  "default_locale": "en",
  "devtools_page": "devtools-page.html",
  "homepage_url": "http://performancescorecard.net",
  "icons": {
    "128": "perf-scorecard-128.png",
    "16": "perf-scorecard-16.png",
    "32": "perf-scorecard-32.png",
    "48": "perf-scorecard-48.png",
    "64": "perf-scorecard-64.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "19.0",
  "name": "Performance Scorecard",
  "short_name": "PerfScore",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.1",
  "web_accessible_resources": [
    "devtools-page.html",
    "devtools-page.js",
    "pagespeed-devtools-icon.png",
    "PagespeedChromium.html"
  ]
}
