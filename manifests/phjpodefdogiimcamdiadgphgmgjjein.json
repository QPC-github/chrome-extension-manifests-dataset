{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clearly Unread",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Get bigger, clearer unread Gmail badge in your tab",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "js": [
        "replacer.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "favicons/0.png",
    "favicons/1.png",
    "favicons/2.png",
