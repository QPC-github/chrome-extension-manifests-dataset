{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick news",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  }
}
