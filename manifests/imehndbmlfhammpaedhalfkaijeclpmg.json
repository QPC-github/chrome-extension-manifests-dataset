{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://tagonsearch.com/images/search.ico",
      "is_default": true,
      "keyword": "tagonsearch.com",
      "name": "Tagonsearch",
      "search_url": "http://tagonsearch.com/search.php?afid=pub1&aid=AID_TAG&q={searchTerms}",
      "suggest_url": "http://tagonsearch.com/suggest.php?q={searchTerms}"
    },
    "homepage": "http://tagonsearch.com/?pl=htn",
    "startup_pages": [
      "http://tagonsearch.com/"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Tagonsearch",
  "icons": {},
  "manifest_version": 2,
  "name": "Tagonsearch",
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.0.4"
}
