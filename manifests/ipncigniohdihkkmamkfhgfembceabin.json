{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sunda Web",
  "version": "3.1.1.0",
  "author": "Sunda Systems Oy",
  "description": "__MSG_extdesc__",
  "homepage_url": "https://www.sunda.fi/sunda-web.html",
  "default_locale": "en",
  "icons": {
    "16": "icons/web_icon_16.png",
    "32": "icons/web_icon_32.png",
    "48": "icons/web_icon_48.png",
    "128": "icons/web_icon_128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "icons/translate_16.png",
      "20": "icons/translate_20.png",
      "32": "icons/translate_32.png",
      "40": "icons/translate_40.png"
    },
    "default_title": "__MSG_pageaction__"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js",
      "md5.js"
    ]
  }
}
