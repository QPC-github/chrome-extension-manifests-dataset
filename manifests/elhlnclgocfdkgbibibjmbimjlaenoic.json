{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PMR Desktop share",
  "description": "PMR Desktop share with Jitsi Meet and Jitsi Videobridge.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "promm2-transparent-16px.png",
    "48": "promm2-transparent-48px.png",
    "128": "promm2-transparent-128px.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meeting.pmr/*"
    ]
  }
}
