{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Headers of your browser",
  "description": "Shows which headers are sent by your browser",
  "version": "1.5",
  "permissions": [
    "debugger"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Your browser headers"
  },
  "manifest_version": 2
}
