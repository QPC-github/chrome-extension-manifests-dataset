{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PwdFly",
  "short_name": "PwdFly",
  "manifest_version": 2,
  "version": "0.5",
  "description": "Generate Secure Password on the fly.",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/extension.png",
    "default_popup": "pwdfly.html"
  }
}
