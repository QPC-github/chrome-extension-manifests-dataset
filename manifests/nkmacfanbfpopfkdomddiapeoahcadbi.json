{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extension StupidCrew",
  "version": "1.0",
  "description": "Sois avertis quand Ismylife et Djo du StupidCrew sont en Stream sur Twitch",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js",
      "newtab.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
