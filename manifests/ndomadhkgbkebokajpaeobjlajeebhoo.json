{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VisualOn Chrome Plugin Helper IQ-TFC",
  "version": "1.7",
  "description": "Chrome Native Messaging for VisualOn Plugin Player",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*SamplePlayerPPAPI.html",
      "*://demo.visualon.info:*/*SamplePlayerPPAPI.html",
      "*://*.logica.io/*",
      "*://*.logiclogic.jp/*",
      "*://*.star-ch.jp/*",
      "*://ec2-54-250-163-201.ap-northeast-1.compute.amazonaws.com/*"
    ]
  }
}
