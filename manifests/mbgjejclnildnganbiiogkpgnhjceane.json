{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LDS Member Tracker",
  "description": "This advanced version lets you see new members, tag members, filter by tags, add notes, and keep track of Home/Visiting teaching.",
  "version": "2.2",
  "icons": {
    "128": "logo-128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://*.lds.org/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.lds.org/directory/"
      ],
      "css": [
        "member.css"
      ],
      "js": [
        "jquery.js",
        "encryption.js",
        "aes.js",
        "member.js"
      ]
    }
  ]
}
