{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "MyBristol"
  },
  "content_scripts": [
    {
      "js": [
        "popup.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "MyBristol extension",
  "icons": {
    "128": "logo128.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "name": "MyBristol Extension",
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.6"
}
