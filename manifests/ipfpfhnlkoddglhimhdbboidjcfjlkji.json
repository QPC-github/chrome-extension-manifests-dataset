{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Click-free Browsing",
  "description": "Use mouse effortlessly: Follow links without clicking and scroll pages without using the wheel.",
  "version": "2.2.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js",
      "jquery-3.4.1.min.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Click-free Browsing",
    "default_icon": "icons/cfb.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "click_free_browsing.js",
        "jquery-3.4.1.min.js"
      ],
      "run_at": "document_start"
    }
  ],
