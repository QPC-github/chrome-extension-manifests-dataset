{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Innoent CollectIt",
  "description": "This extension will get page as image",
  "version": "2.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "cookies"
  ]
}
