{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "JustPic позволяет напрямую открывать исходное изображение, минуя страницу с назойливой рекламой.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "name": "JustPic",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://fastpic.ru/*",
    "http://*.fastpic.ru/*",
    "http://*.imagebam.com/*",
    "http://imageban.ru/*",
    "http://imageshost.ru/*",
    "http://imgdepo.ru/*",
    "http://radikal.ru/*"
  ],
  "manifest_version": 2,
  "version": "0.0.3"
}
