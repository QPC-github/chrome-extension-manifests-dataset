{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SetCookie",
  "description": "Type 'setcookie' then type keyword of custom host into the Omnibox to set cookies of custom host for localhost.",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon48.png"
  },
  "browser_action": {
    "default_title": "SetCookie",
    "default_icon": "icon16.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "omnibox": {
    "keyword": "setcookie"
  },
  "manifest_version": 2
}
