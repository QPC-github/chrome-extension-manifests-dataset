{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Track Turbo Extension",
  "description": "The chrome extension for Time Track Turbo. A timetrackturbo.com account is required to use this extension.",
  "homepage_url": "http://www.timetrackturbo.com",
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "offline_enabled": false,
  "version": "1.6",
  "permissions": [
    "cookies",
    "https://www.timetrackturbo.com/"
  ],
  "content_security_policy": "script-src 'self' https://www.timetrackturbo.com/; object-src 'self'",
  "browser_action": {
    "default_title": "Track time",
    "default_icon": "images/icon_32.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
