{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto List Search",
  "version": "1.6.4",
  "description": "Automated web searches using several different search engines.",
  "permissions": [
    "storage",
    "background",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/auto_search_logo16.png",
      "32": "images/auto_search_logo32.png",
      "48": "images/auto_search_logo48.png",
      "128": "images/auto_search_logo128.png"
    }
  },
  "icons": {
    "16": "images/auto_search_logo16.png",
    "32": "images/auto_search_logo32.png",
    "48": "images/auto_search_logo48.png",
    "128": "images/auto_search_logo128.png"
  },
  "manifest_version": 2
}
