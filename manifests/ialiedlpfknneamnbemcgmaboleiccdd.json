{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "default_locale": "en",
  "version": "1.9.1",
  "manifest_version": 2,
  "icons": {
    "32": "assets/32.png",
    "64": "assets/64.png",
    "128": "assets/128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "debugger",
    "desktopCapture",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "__MSG_title__",
    "default_popup": "index.html"
  },
  "options_page": "./settings.html",
  "background": {
    "scripts": [
      "./js/background.js"
    ]
  },
  "web_accessible_resources": [
    "assets/*.svg",
    "assets/*.png",
    "assets/*.css",
    "assets/*.otf"
  ],
  "content_scripts": [
    {
      "js": [
        "./js/content-script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
