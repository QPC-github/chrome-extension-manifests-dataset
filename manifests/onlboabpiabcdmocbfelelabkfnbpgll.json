{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Handover Chrome Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Launch websites you visit on your computer on your Android phone or tablet",
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "<all_urls>"
  ]
}
