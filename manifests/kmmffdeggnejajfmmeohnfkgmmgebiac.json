{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dreamweather",
  "version": "1.1.1",
  "description": "A new Page Extension displaying a beautiful picture and the time and weather",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "storage"
  ],
  "author": "Timbo Blaesche"
}
