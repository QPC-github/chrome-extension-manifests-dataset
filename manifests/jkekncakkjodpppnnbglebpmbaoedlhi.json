{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Check Time Left V1",
  "short_name": "TL V1",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Check the Time left of the item displayed in eBay Selling Manager Listings.",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "https://k2b-bulk.ebay.com/*",
    "https://www.ebay.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon_tl_19.png",
    "default_title": "Check Time Left Ver.1.5",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_tl_16.png",
    "48": "icon_tl_48.png",
    "128": "icon_tl_128.png"
  }
}
