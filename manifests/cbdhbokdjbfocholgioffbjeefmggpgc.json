{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Renderosity Site Fixer",
  "version": "1.1",
  "description": "Removed the Opening of new tabs on Renderosity.com",
  "browser_action": {
    "default_icon": "FixRendo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.renderosity.com/*"
      ],
      "js": [
        "jquery.min.js",
        "event.js"
      ]
    }
  ]
}
