{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tooltip Note",
  "version": "0.0.20",
  "manifest_version": 2,
  "description": "Show your webpage in one Image by add Tooltip note wherever you want, then create a full screen-shot.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "notifications"
  ],
  "web_accessible_resources": [
    "images/bubble-sprite.png",
    "qtip/jquery-1.10.2.min.map"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
