{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NBA Timezone Extension",
  "version": "0.1.0",
  "description": "Change the time of games to the users current timezone",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://watch.nba.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  }
}
