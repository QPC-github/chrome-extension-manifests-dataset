{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better forum.autoua.net",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Changes default CSS of the forum.autoua.net",
  "homepage_url": "http://forum.autoua.net",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://forum.autoua.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://forum.autoua.net/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
