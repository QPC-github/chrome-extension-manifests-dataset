{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "songsweeper",
  "version": "1.1",
  "description": "To be used with https://martinbarker.herokuapp.com/songsweeper/",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "https://songsweeper.herokuapp.com/",
    "http://songsweeper.herokuapp.com/",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "static/newIcon.png",
      "32": "static/newIcon.png",
      "48": "static/newIcon.png",
      "128": "static/newIcon.png"
    }
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "icons": {
    "16": "static/newIcon.png",
    "32": "static/newIcon.png",
    "48": "static/newIcon.png",
    "128": "static/newIcon.png"
  }
}
