{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "This extension converts links to the non-mobile version of the site",
  "name": "Unmobilize",
  "icons": {
    "48": "Unmobilizer48.png",
    "128": "Unmobilizer128.png",
    "16": "Unmobilizer16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "version": "1.0"
}
