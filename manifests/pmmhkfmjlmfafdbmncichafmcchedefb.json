{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Googley Eyes",
  "version": "2.0",
  "description": "Web usage app",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "32": "images/eyeslogo32.png",
      "64": "images/eyeslogo64.png",
      "128": "images/eyeslogo128.png"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
