{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Password Genie",
  "version": "1.4.4",
  "manifest_version": 2,
  "description": "Instant and secure password generator, the extension is safe and open source",
  "icons": {
    "16": "images/icon@32w.png",
    "48": "images/icon@48w.png",
    "128": "images/icon@128w.png",
    "512": "images/icon@512w.png",
    "1024": "images/icon@1024w.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [],
  "browser_action": {
    "default_icon": {
      "16": "images/icon@32w.png",
      "48": "images/icon@48w.png"
    },
    "default_title": "The Password Genie",
    "default_popup": "popup.html"
  }
}
