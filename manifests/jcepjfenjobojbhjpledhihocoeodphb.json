{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stock Finder",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Allows user to enter stock ticker and select their favorite finance websites to search the price.",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Stock Finder"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
