{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Glasses",
  "description": "This extension changes colors and fonts with options for those with color viewing deficiencies",
  "version": "1.5",
  "browser_action": {
    "default_icon": "Glasses.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "fontSettings",
    "storage"
  ]
}
