{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ToToDo",
  "version": "1.0",
  "description": "ToDo List,Программа для расспределения Задач",
  "background": {
    "page": "bg_page.html"
  },
  "permissions": [
    "tabs",
    "http://ciframe.com/tol/json.php*"
  ],
  "browser_action": {
    "default_title": "ToToDo",
    "default_icon": "images/ToToDo.png",
    "default_popup": "popup/login.html"
  }
}
