{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Buildout Helper",
  "version": "1.0.16",
  "manifest_version": 2,
  "description": "Adds extra features to your buildout.com experience.",
  "background": {
    "scripts": [
      "chrome.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://buildout.dev/*",
        "*://lo.buildout/*",
        "*://buildout.com/*"
      ],
      "js": [
        "buildout.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://buildout.com/*",
    "*://buildout.dev/*",
    "*://lo.buildout/*",
    "https://post.craigslist.org/*"
  ]
}
