{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CapSumo",
  "short_name": "CapSumo",
  "version": "1.0",
  "description": "CapSumo user can copy the view source of web page.",
  "manifest_version": 2,
  "permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  }
}
