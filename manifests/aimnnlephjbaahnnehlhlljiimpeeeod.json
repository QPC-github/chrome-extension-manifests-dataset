{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SeoChat.Top",
  "description": "Tiện ích hỗ trợ xuất cookie zalo lên SeoChat.Top",
  "version": "1.3",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "logo.png"
    }
  },
  "permissions": [
    "https://*.zaloapp.com/*",
    "https://*.zalo.me/*",
    "https://*.seochat.top/*",
    "https://seochat.top/*",
    "https://.zaloapp.com/*",
    "https://.zalo.me/*",
    "https://.seochat.top/*",
    "https://seochat.top/*",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "background",
    "cookies"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.4.1.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.zaloapp.com/*",
        "https://*.zalo.me/*",
        "https://*.seochat.top/*",
        "https://seochat.top/*",
        "https://.zaloapp.com/*",
        "https://.zalo.me/*",
        "https://.seochat.top/*",
        "https://seochat.top/*"
      ],
      "js": [
        "js/jquery-3.4.1.min.js",
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
