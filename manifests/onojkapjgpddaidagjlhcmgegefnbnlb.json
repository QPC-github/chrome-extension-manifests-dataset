{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Breakout",
  "description": "Breakout in a chrome extension",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "128": "breakout.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "code.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}
