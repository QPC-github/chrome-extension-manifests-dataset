{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CopyWebURL",
  "description": "the excellent tool to copy current web url",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "sample.js",
      "./lib/jquery-3.2.1.min.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "devtools_page": "devtools.html"
}
