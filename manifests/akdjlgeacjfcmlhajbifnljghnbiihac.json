{
  "background": {
    "scripts": [
      "vendor/jquery-3.1.1.min.js",
      "vendor/decode.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "detector.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "SALESOK.ru - Кулинарный новостной портал",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "SALESOK.ru - Кулинарный новостной портал",
  "permissions": [
    "http://*.teasermoney.com/*",
    "http://*.teasermoney.ru/*",
    "http://salesok.ru/*",
    "notifications",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.2"
}
