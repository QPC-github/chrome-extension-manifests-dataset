{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ShowMePassword",
  "homepage_url": "https://github.com/maliming/showmepassword",
  "description": "Quick and easy to view passwords saved by Chrome browser",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
