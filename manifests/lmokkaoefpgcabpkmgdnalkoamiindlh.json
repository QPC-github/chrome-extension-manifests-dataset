{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Alistafixer",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This extension fixes the new A List Apart website design",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.alistapart.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
