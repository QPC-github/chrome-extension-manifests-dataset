{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ThisIP",
  "description": "Gets your public ip address.  Tools for Host, IP, MX, and TXT records.",
  "version": "0.3.0",
  "permissions": [
    "tabs",
    "http://www.thisip.net/",
    "http://thisip.net/"
  ],
  "browser_action": {
    "default_icon": "ip.png",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "ip.png",
    "64": "ip.png",
    "128": "ip.png"
  },
  "omnibox": {
    "keyword": "thisip"
  },
  "manifest_version": 2
}
