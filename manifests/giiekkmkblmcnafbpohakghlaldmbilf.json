{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "tiny link",
  "version": "1.4.0",
  "description": "Easy tool to shorten your URL's",
  "permissions": [
    "activeTab",
    "storage",
    "https://12ab.pw/create",
    "tabs",
    "notifications"
  ],
  "commands": {
    "short_copy": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "windows": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      },
      "description": "short the current URL and copy to clipboard"
    }
  },
  "background": {
    "scripts": [
      "background.js",
      "popup.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo_T_16.png",
      "32": "images/logo_T_32.png",
      "48": "images/logo_T_48.png",
      "128": "images/logo_T_128.png"
    }
  },
  "icons": {
    "16": "images/logo_T_16.png",
    "32": "images/logo_T_32.png",
    "48": "images/logo_T_48.png",
    "128": "images/logo_T_128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
