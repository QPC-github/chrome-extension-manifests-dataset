{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "2Panel",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "injected.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ]
}
