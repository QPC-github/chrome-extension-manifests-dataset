{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "wim",
  "version": "0.2",
  "description": "A chrome extension that will enable us to surf the web, vim style.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_idle",
      "all_frames": true,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "images/w.png"
    }
  }
}
