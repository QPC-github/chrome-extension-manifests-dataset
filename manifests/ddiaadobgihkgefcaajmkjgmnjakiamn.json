{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Digital Keyway Plugin",
  "description": "Chrome extension for Digital Keyway",
  "version": "1.0",
  "version_name": "1.0",
  "homepage_url": "https://www.digitalkeyway.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.digitalkeyway.com/*",
    "tabs"
  ],
  "externally_connectable": {
    "matches": [
      "*://auth.digitalkeyway.com/*"
    ]
  }
}
