{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My Dashboard Speed Dial",
  "description": "Speed Dial changes standard page to the new tab page with your saved bookmarks.",
  "homepage_url": "https://www.mydashboardspeeddial.com/",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "https://www.renaissancebeautyllc.com/mydashboardspeeddial/*"
  ],
  "version": "1.3",
  "browser_action": {
    "default_icon": {
      "16": "images/bd-16.png",
      "32": "images/bd-32.png",
      "48": "images/bd-48.png",
      "128": "images/bd-128.png"
    }
  },
  "icons": {
    "16": "images/bd-16.png",
    "32": "images/bd-32.png",
    "48": "images/bd-48.png",
    "128": "images/bd-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "landing.html"
  },
  "web_accessible_resources": [
    "images/*.*"
  ],
  "content_security_policy": "script-src 'self' https://www.renaissancebeautyllc.com/mydashboardspeeddial/custom.js https://www.renaissancebeautyllc.com/mydashboardspeeddial/background.js; object-src 'self' ;style-src 'self' 'unsafe-inline' *;media-src *;"
}
