{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Triolab.si",
  "name": "Aliexpress - Slovenski iskalnik",
  "short_name": "Ali Iskalnik",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Iščem po slovensko na aliexpress-u.",
  "icons": {
    "32": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.aliexpress.com/*",
        "https://message.aliexpress.com/*",
        "https://trade.aliexpress.com/order_detail.htm?*",
        "https://portals.aliexpress.com/*",
        "http://www.aliexpress.com/*",
        "http://message.aliexpress.com/*",
        "http://trade.aliexpress.com/order_detail.htm?*",
        "http://portals.aliexpress.com/*"
      ],
      "css": [
        "extension.css"
      ],
      "js": [
        "jquery-2.0.0.min.js",
        "helpers.js",
        "linkgenerator.js"
      ]
    }
  ],
  "permissions": [
    "http://*/"
  ],
  "web_accessible_resources": [
    "jquery-2.0.0.min.map",
    "jquery.min.map"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
