{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mutirão Suicida",
  "description": "Extensão para agilizar votações",
  "version": "1.0.7",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://gshow.globo.com/realities/bbb/BBB-16/interatividade/enquetetv/2016/4/4/quem-voce-quer-que-venca-o-bbb16-80d4c0f4-fa13-11e5-9143-9cb654717a46.html"
