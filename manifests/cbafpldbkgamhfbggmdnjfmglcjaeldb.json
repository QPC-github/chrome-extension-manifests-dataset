{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smart Repeat",
  "content_scripts": [
    {
      "js": [
        "smartrepeat.js"
      ],
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ]
    }
  ],
  "description": "Repeat YouTube videos from any point in time",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "version": "1.33.7",
  "manifest_version": 2
}
