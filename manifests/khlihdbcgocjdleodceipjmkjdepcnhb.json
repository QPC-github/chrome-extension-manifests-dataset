{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stacked Chrome Extension",
  "short_name": "Stacked Extension",
  "description": "This extension connects with Stacked App",
  "version": "1.0.0",
  "icons": {
    "16": "images/cookie.png",
    "48": "images/cookie.png",
    "128": "images/cookie.png"
  },
  "permissions": [
    "storage",
    "http://mystackedapp-api-dev.ap-northeast-2.elasticbeanstalk.com/",
    "https://api.mystackedapp.com/"
  ],
  "author": "Hyon Lee",
  "web_accessible_resources": [
    "js/*",
    "css/*",
    "images/*"
  ],
  "browser_action": {
    "default_title": "Stacked",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
