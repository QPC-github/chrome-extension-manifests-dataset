{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "d3.ru user notes",
  "version": "1.3",
  "description": "Удобный просмотр заметок в профилях пользователей на d3.ru.",
  "manifest_version": 2,
  "permissions": [
    "*://d3.ru/user/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://d3.ru/user/*"
      ],
      "js": [
        "notes.js"
      ]
    }
  ]
}
