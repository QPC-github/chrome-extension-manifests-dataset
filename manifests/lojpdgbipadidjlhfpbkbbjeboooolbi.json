{
  "manifest_version": 2,
  "name": "Kkrt Dev Tools",
  "version": "1.1.0",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Tools for Devs",
  "icons": {
    "128": "devTools.png"
  },
  "browser_action": {
    "default_icon": "devTools.png",
    "default_popup": "options.html",
    "default_title": "Kkrt Dev Tools"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/jquery-3.2.1.min.js",
        "lib/faker.min.js",
        "src/utils.js",
        "contentScript.js"
      ],
      "all_frames": true
    }
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "lib/jquery-3.2.1.min.js",
      "lib/faker.min.js",
      "src/utils.js",
      "src/meu_ip/meuIp.js",
      "src/string/string.js",
      "src/guid/guid.js",
      "src/cpf_cnpj/cnpj.js",
      "src/cpf_cnpj/cpf.js",
      "src/rg/checkRG.js",
      "src/titulo_eleitor/checkTituloEleitor.js",
      "src/cnh/checkCNH.js",
      "src/menus.config.js"
    ],
    "persistent": false
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "notifications",
    "contextMenus",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
