{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Print Image Pro",
  "description": "This extension allows you to scale an image to fit the page before printing it.",
  "version": "2.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon-128.png"
  }
}
