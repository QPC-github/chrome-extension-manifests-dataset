{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Editor",
  "version": "2.3.2",
  "permissions": [
    "activeTab",
    "tabs",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "noedit.png"
  },
  "browser_action": {
    "default_title": "Ability to (temporarily) edit any webpage",
    "default_icon": "noedit.png"
  },
  "manifest_version": 2
}
