{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JukeBox",
  "description": "This extension, lets you copy a youtube link for a music playlist and it will create a playlist in your own spotify account",
  "version": "1.1.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "juke.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://accounts.spotify.com/*",
    "https://api.spotify.com/*",
    "identity"
  ]
}
