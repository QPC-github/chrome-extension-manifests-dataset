{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Streak.life",
  "short_name": "StreakLife",
  "description": "Track your streaks with your calendar",
  "version": "0.0.3",
  "homepage_url": "https://streak.life",
  "icons": {
    "16": "icon@16.png",
    "48": "icon@48.png",
    "128": "icon@128.png"
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://storage.googleapis.com; object-src 'self'",
  "oauth2": {
    "client_id": "895943675383-4luqfin2lj0e3n6pbt114o4f70k6n5eh.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "openid",
      "https://www.googleapis.com/auth/calendar"
    ]
  }
}
