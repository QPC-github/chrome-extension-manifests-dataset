{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "wikipedia.png"
  },
  "description": "Redirect all requests from Wikipedia to WikiZero, you know why.",
  "icons": {
    "128": "wikipedia.png",
    "16": "wikipedia.png",
    "48": "wikipedia.png"
  },
  "manifest_version": 2,
  "name": "Wikipedia to WikiZero",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "1.0.1"
}
