{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Everyday German phrases",
  "description": "Learn German phrases on every new tab",
  "version": "0.02",
  "manifest_version": 2,
  "icons": {
    "128": "logo_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "vocab.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "javascript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
