{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Current Tab QR Code",
  "description": "This extension will generate a QR code for the current tab",
  "version": "1.2",
  "short_name": "QR Code",
  "browser_action": {
    "default_icon": "Connect_256.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "Connect_16.png",
    "48": "Connect_48.png",
    "128": "Connect_128.png",
    "256": "Connect_256.png"
  }
}
