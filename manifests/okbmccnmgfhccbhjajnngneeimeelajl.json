{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Omegle Shortcut Messages",
  "version": "0.1",
  "description": "Send messages automatically",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  }
}
