{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "homepage_url": "http://shitokaivenezuela.com",
  "name": "Asociación Shitokai de Venezuela",
  "version": "1.4.8",
  "description": "Aplicación con información para el público en general y acceso privado para senseis registrados...",
  "permissions": [
    "contextMenus",
    "idle",
    "notifications",
    "storage",
    "tabs",
    "management",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": true,
    "background_page": "background.html",
    "scripts": [
      "clickderecho.js",
      "background.js",
      "ventana.js",
      "popup.js",
      "popup2.js",
      "version.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "shitokai19.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "shitokai16.png",
    "48": "shitokai48.png",
    "128": "shitokai128.png"
  },
  "manifest_version": 2,
  "options_page": "opciones.html",
  "web_accessible_resources": [
    "shitokai48.png"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.colonus.com/*"
    ]
  }
}
