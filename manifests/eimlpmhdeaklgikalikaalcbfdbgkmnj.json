{
  "name": "Any Website 1Button App for Chrome",
  "version": "1.3",
  "manifest_version": 2,
  "description": "This is an extension for Chrome to get with 1Button on any website.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
