{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tiger Fitness Chrome Extension",
  "short_name": "TigerFitness",
  "homepage_url": "http://www.tigerfitness.com",
  "author": "Tiger Software - JRN",
  "version": "1.27.1",
  "manifest_version": 2,
  "description": "You're More Than Just a Number Here",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "cookies",
    "notifications",
    "contextMenus",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  }
}
