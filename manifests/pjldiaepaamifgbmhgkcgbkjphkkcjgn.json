{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TextTV.nu - nyheter från SVT Text",
  "description": "Visa nyheter från SVT Text direkt i din webbläsare",
  "version": "0.0.0.3",
  "browser_action": {
    "default_icon": "icons/default_icon.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "https://texttv.nu/",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
