{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Iris",
  "description": "A sleek modern newtab page with draggable tabs that display basic and advanced info.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "assets/icon32.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "system.cpu",
    "system.memory"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
