{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Twitter Handle Linker",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Replace Twitter handles with links in webpages.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "homepage_url": "http://www.keefertaylor.com"
}
