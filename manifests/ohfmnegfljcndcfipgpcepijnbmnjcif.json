{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WoW Item IDs",
  "version": "1.06",
  "description": "Easily get those WoW Item IDs!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://classic.wowhead.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "clipboardWrite",
    "clipboardRead",
    "https://classic.wowhead.com/*"
  ],
  "icons": {
    "16": "idx16.png",
    "32": "idx32.png",
    "48": "idx48.png",
    "128": "idx128.png"
  },
  "browser_action": {
    "default_icon": "idx32.png"
  }
}
