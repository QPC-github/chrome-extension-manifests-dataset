{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Indglass",
  "description": "Access companies's glassdoor site straight from indeed",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "iconGD.jpg"
  ],
  "content_scripts": [
    {
      "css": [
        "myStyles.css"
      ],
      "js": [
        "myscript.js"
      ],
      "matches": [
        "http://www.indeed.com/*",
        "https://www.indeed.com/*"
      ]
    }
  ],
  "permissions": [
    "http://api.glassdoor.com/*",
    "https://api.ipify.org/*"
  ]
}
