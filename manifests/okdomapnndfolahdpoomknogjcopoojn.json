{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Test Site Indicator",
  "version": "1.0",
  "description": "automatically show orange icon on tool-bar when site contains 'staging.' or 'test.' in its name.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_title": "This is test site"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
