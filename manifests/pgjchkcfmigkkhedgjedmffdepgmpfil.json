{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Psykopaint",
  "default_title": "Psykopaint",
  "description": "__MSG_AppDescription__",
  "version": "1.0",
  "default_locale": "en",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png"
  },
  "app": {
    "urls": [
      "http://www.psykopaint.com/index.html",
      "http://www.psykopaint.com/gallery.html",
      "http://www.psykopaint.com/blog/",
      "http://www.psykopaint.com/painter.html"
    ],
    "launch": {
      "web_url": "http://www.psykopaint.com/painter.html"
    }
  },
  "intents": {
    "http://webintents.org/edit": [
      {
        "type": [
          "image/png",
          "image/jpeg"
        ],
        "href": "http://www.psykopaint.com/painter.html",
        "title": "Create amazing paintings from photos"
      }
    ]
  }
}
