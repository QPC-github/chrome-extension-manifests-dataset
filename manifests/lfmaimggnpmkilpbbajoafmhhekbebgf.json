{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Classo ScreenShare",
  "author": "classo.me",
  "version": "2.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for http://tokbox.com/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://www.classo.me/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://www.classo.me/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "classo-ss",
  "web_accessible_resources": [
    "icon.png"
  ]
}
