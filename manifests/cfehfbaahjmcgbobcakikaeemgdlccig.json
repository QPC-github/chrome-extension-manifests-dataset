{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yodiz Search Helper",
  "description": "Search Yodiz using advance search functionality",
  "version": "1.1.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs",
    "https://*/*",
    "background",
    "<all_urls>",
    "downloads"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
