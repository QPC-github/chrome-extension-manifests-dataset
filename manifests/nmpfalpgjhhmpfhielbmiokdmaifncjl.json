{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Go to Tablet",
  "description": "This extension permites send your present url to an android device.",
  "version": "1.1",
  "icons": {
    "128": "icon128.jpg",
    "48": "icon128.jpg"
  },
  "background": {
    "scripts": [
      "event.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.jpg",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ]
}
