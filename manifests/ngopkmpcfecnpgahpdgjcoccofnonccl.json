{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UDP MIDI Communication",
  "description": "POC of MIDI data sent through UDP.",
  "version": "0.2",
  "minimum_chrome_version": "44",
  "icons": {
    "128": "icon.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "sockets": {
    "udp": {
      "send": [
        "*"
      ],
      "bind": [
        "*"
      ]
    }
  }
}
