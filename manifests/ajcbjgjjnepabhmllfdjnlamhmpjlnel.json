{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Refers.To Link Shrinker",
  "description": "Shortens URLs to the clipboard",
  "version": "0.2.7",
  "permissions": [
    "contextMenus",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "event.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "refers.to16.png",
    "48": "refers.to48.png",
    "128": "refers.to128.png"
  }
}
