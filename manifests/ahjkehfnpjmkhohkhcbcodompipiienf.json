{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remote Pin Toggle For Inbox",
  "version": "1.52",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png",
    "48": "icon.png",
    "19": "icon19.png",
    "38": "icon38.png"
  },
  "browser_action": {
    "name": "Toggle 'Em All!",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery.js",
        "clicky-clicky.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "*://*/*"
  ]
}
