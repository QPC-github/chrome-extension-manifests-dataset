{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VIP风暴",
  "version": "1.0",
  "description": "VIP资源数据分享插件，在不泄露用户密码的前提下分享用户可访问的VIP资源。",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "background": {
    "scripts": [
      "app.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "https://*.ximalaya.com/*"
  ]
}
