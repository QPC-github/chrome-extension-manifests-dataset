{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GTM dataLayer Sifter",
  "version": "0.3",
  "description": "A plugin to find the dot notation from an event's element to another element on the page.",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "inserted-script.js"
  ],
  "icons": {
    "16": "icons/GTMsifter-icon-16px.png",
    "19": "icons/GTMsifter-icon-19px.png",
    "38": "icons/GTMsifter icon-noBg-48px-01.png",
    "48": "icons/GTMsifter-icon-48.png",
    "128": "icons/GTMsifter-icon-128px.png"
  },
  "browser_action": {
    "default_icon": "icons/GTMsifter icon-noBg-48px-01.png",
    "default_popup": "popup.html"
  }
}
