{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "ru",
  "name": "__MSG_name__",
  "version": "1.0.3",
  "description": "__MSG_description__",
  "browser_action": {
    "default_icon": {
      "19": "ext/icons/headphones_19.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "ext/icons/kometa_16.png",
    "48": "ext/icons/kometa_48.png",
    "128": "ext/icons/kometa_128.png"
  },
  "background": {
    "page": "ext/background.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://cometa.widget-proto.ru/*",
    "http://*.adfox.ru/*",
    "https://www.google-analytics.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
