{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AnalizSajta Tool",
  "description": "Анализ сайта и проверка позиций.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/16.png",
    "128": "img/128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "activeTab",
    "tabs",
    "cookies"
  ]
}
