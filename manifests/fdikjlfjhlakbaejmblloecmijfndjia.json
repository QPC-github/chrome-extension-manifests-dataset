{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Designer Kit",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "The freshest links about design and interactive, from around the web. A designer's must!",
  "homepage_url": "http://somedomain.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [],
  "optional_permissions": [
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
