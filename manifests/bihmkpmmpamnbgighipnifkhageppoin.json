{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The \"Tech Problems\" Extension",
  "version": "7.0.3",
  "description": "For those times when you don't want your brower to do anything useful",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
