{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Handy utils",
  "description": "Save some time and keep your data to yourself",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "contextMenus",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts": [
      "sample.js"
    ]
  }
}
