{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "youtube.css"
      ],
      "matches": [
        "*://www.youtube.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "16.png",
    "19": "19.png",
    "128": "128.png",
    "48": "48.png"
  },
  "manifest_version": 2,
  "name": "Тема для YouTube - Темный карбон",
  "description": "Для любителей темных тем оформления",
  "permissions": [
    "*://*.youtube.com/*",
    "*://apis.google.com/*",
    "*://plus.googleapis.com/*",
    "storage",
    "*://iliz.net/*"
  ],
  "version": "1.3"
}
