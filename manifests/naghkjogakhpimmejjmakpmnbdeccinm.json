{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmark",
  "description": "Chrome Bookmarks button",
  "version": "1.2.1",
  "browser_action": {
    "default_title": "Open your chrome bookmarks",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo128.png"
  },
  "default_locale": "en",
  "offline_enabled": true
}
