{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "img/off.png",
    "default_title": "OFF"
  },
  "manifest_version": 2,
  "name": "Unblocker",
  "description": "Internet Unblocking Extension.",
  "permissions": [
    "proxy",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/",
    "https://*/"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "*://*.proxydns.co/",
      "*://*.proxydns.co/*",
      "*://*.wipzard.com/*",
      "*://localhost/*",
      "*://localhost:*/*"
    ]
  },
  "version": "3.1.0"
}
