{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Finn.no Property Map Assistant",
  "description": "Marks finn.no property map search results as \"hidden\" or \"saved\".",
  "version": "1.2.2",
  "permissions": [
    "http://kart.finn.no/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://kart.finn.no/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*.png"
  ]
}
