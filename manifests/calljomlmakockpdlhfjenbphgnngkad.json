{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Helpjuice",
  "description": "Helpjuice Chrome extension",
  "version": "0.1",
  "author": "Helpjuice Inc.",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "https://*.helpjuice.com/",
    "storage",
    "tabs"
  ]
}
