{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Puppy Tab",
  "short_name": "Puppy Tab",
  "version": "0.1.16",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Puppy Tab",
    "default_icon": {
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newpage.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
