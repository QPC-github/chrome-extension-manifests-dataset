{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "talkingRaymondSavesTemplates",
  "description": "save templates that you just typed - only for private use by raymond",
  "author": "PJL",
  "version": "2.5",
  "content_scripts": [
    {
      "matches": [
        "https://app1xxxxxxxxx.intellechart.net/*"
      ],
      "css": [
        "bs4.css"
      ],
      "js": [
        "bs4.js",
        "jquery.js",
        "content2.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.js",
      "content2.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "https://savetemplate.herokuapp.com/*",
    "http://127.0.0.1:5000/*",
    "http://0.0.0.0:5000/*",
    "http://192.168.5.3:5000/*"
  ]
}
