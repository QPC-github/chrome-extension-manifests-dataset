{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Rescue Music Videos HD Images Animations",
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png"
    },
    "default_popup": "popup.html",
    "default_title": "Tab Rescue"
  },
  "permissions": [
    "tabs",
    "storage",
    "windows",
    "unlimitedStorage",
    "https://api.bing.com/*",
    "*://tabrescue.com/*"
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "icons": {
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://tabrescue.com 'unsafe-eval'; object-src 'self'",
  "description": "Customize Your New Tabs With Amazing Video Animations And Photos, New Animations Uploaded Weekly.",
  "version": "0.0.27",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Tab Rescue",
      "keyword": "r",
      "search_url": "https://rescuematrix.com/?q={searchTerms}",
      "favicon_url": "https://rescuematrix.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
