{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ecommerce: Promotions & Coupons",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "http://127.0.0.1:8080/",
    "storage"
  ]
}
