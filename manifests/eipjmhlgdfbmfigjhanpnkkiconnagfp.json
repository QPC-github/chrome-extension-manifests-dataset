{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Add to Sadhat",
  "version": "0.7.3",
  "manifest_version": 2,
  "description": "Adds a toolbar button that submits the current page to Sadhat.com",
  "homepage_url": "http://sadhat.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Add to Sadhat"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
