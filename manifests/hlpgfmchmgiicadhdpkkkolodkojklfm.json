{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SNS bot detector",
  "description": "Pronalazaƒç SNS botova na tviteru i fejsbuku.",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "twitter.js"
      ]
    },
    {
      "matches": [
        "https://*.facebook.com/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "facebook.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "jquery-3.4.1.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "stop-vucic.png"
  }
}
