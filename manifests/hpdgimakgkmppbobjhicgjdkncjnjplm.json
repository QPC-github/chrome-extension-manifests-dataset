{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Welcome, who do you support?",
  "version": "1.0.2",
  "description": "Find out what football your favourite NeoGAF members support and judge them accordingly.",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://neogaf.com/*",
        "*://*.neogaf.com/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "event.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
