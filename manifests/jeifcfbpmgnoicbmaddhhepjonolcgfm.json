{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Upload Photos to Cloudinary",
  "version": "1.0",
  "description": "A simple extension that uploads images to your cloudinary account",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-2.1.3.min.js",
      "jsbn.js",
      "rsa.js",
      "encrypt.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Configure me!"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
