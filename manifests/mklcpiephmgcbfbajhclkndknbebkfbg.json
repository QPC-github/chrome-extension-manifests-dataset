{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vehichaul Importer",
  "description": "Import loads into Vehichaul.",
  "version": "1.19.2",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "notifications",
    "*://*.vehichaul.com/"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.centraldispatch.com/protected/dispatch/view*",
        "https://www.carshipio.com/shipper*"
      ],
      "css": [
        "fix.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
