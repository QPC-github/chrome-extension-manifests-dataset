{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WearWhat Fashion Shop",
  "description": "This extension will help you buy fashion items that you see while using the browser",
  "version": "1.8",
  "browser_action": {
    "default_icon": "clothes17.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://www.google-analytics.com/",
    "storage"
  ]
}
