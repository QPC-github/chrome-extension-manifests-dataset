{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "No Radio SNCF",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Ferme l'accès à la radio SNCF",
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "http://www.sncf.com/fr/radio"
      ],
      "js": [
        "radiosncf.user.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.sncf.com/fr/radio"
  ]
}
