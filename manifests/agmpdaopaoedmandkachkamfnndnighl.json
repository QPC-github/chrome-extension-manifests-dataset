{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TaoBaoHelper",
  "version": "0.2",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://item.taobao.com/item.htm*",
        "http://detail.tmall.com/item.htm*"
      ],
      "js": [
        "config/config.js",
        "lib/socket.io.js",
        "lib/jquery-1.7.2.min.js",
        "lib/jquery.dateFormat-1.0.js",
        "lib/underscore-min.js",
        "lib/lib.js",
        "script/helper.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
