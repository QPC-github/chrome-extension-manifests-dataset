{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "20paths",
  "short_name": "20paths",
  "version": "1.0.0.5",
  "description": "20paths - Self Learn at Own Pace",
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/underscore-min.js",
      "js/purl.js",
      "js/log_enteries.js",
      "iframe/js/coalan.async.js",
      "js/common.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "20paths_icon_128.png",
    "default_title": "20paths",
    "default_popup": "sign_in.html"
  },
  "icons": {
    "128": "20paths_icon_128.png",
    "48": "20paths_icon_48.png",
    "16": "20paths_icon_16.png"
  },
  "web_accessible_resources": [
    "**.css",
    "**.html",
    "**.js",
    "**.png",
    "**.gif",
    "**.map",
    "iframe/**.css",
    "iframe/**.html",
    "iframe/**.js",
    "iframe/**.map",
    "iframe/**.png"
  ],
  "permissions": [
    "storage",
    "activeTab",
    "clipboardWrite",
    "notifications",
    "http://*/",
    "https://*/",
    "tabs"
  ],
  "optional_permissions": [],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
