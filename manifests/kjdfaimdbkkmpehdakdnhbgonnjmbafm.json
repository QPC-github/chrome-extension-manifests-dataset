{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rebtel Total",
  "version": "0.2",
  "description": "Rebtel Total",
  "icons": {
    "16": "Icons/Logo16.png",
    "32": "Icons/Logo32.png",
    "48": "Icons/Logo48.png",
    "128": "Icons/Logo128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://my.rebtel.com/en/account/history/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
