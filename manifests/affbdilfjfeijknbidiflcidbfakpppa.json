{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reverse Reddit",
  "description": "When you visit a link which is on reddit front page top 25, you get an alert showing you the Post title and upvote count",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
