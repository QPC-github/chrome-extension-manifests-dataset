{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Slack",
  "short_name": "Slack",
  "description": "Slack is all your team communication in one place, instantly searchable, available wherever you go.",
  "version": "1.0.3",
  "app": {
    "urls": [
      "*://slack.com/",
      "*://*.slack.com/"
    ],
    "launch": {
      "web_url": "https://slack.com/ssb"
    }
  },
  "icons": {
    "128": "app-96_chrome_app.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "clipboardRead",
    "clipboardWrite"
  ],
  "homepage_url": "https://slack.com"
}
