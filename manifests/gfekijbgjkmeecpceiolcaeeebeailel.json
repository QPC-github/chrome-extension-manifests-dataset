{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rdio Play All",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Play all songs from artist. You can play all popular songs from artists.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.rdio.com/*"
      ],
      "js": [
        "jquery.js",
        "rdiolyrics.js"
      ]
    }
  ]
}
