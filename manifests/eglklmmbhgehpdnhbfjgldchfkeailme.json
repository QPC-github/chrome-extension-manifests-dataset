{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Academicos Utils",
  "description": "Ferramentas para facilitar acesso aos academicos",
  "content_scripts": [
    {
      "matches": [
        "https://academicos.ubi.pt/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "version": "2.2",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "permissions": [
    "https://academicos.ubi.pt/*"
  ],
  "web_accessible_resources": [
    "horario.js",
    "counter.js",
    "newlook.js",
    "logo.png"
  ]
}
