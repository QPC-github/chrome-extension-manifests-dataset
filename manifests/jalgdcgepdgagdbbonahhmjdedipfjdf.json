{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Selfy",
  "description": "Force links to open in same tab.",
  "version": "0.2",
  "homepage_url": "http://here.ru",
  "default_locale": "en",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "js/*"
  ],
  "icons": {
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
