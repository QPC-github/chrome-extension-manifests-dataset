{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Resume Playlists",
  "version": "0.1",
  "description": "Resume YouTube playlists where you left off with this simple extension!",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "css": [
        "css/custom.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "YouTube Resume Playlists"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "manifest_version": 2
}
