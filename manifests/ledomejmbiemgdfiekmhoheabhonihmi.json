{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unsecure Login Notifier",
  "description": "Adds an icon to password fields indicating whether HTTPS is being used or not",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "iconTypes.css"
      ],
      "js": [
        "jquery.js",
        "checkPasswords.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/good.png",
    "images/bad.png"
  ]
}
