{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ForcePoint PreConfig",
  "description": "This extension pre-selects default options to search",
  "version": "1.2.6",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Setup"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://admin.websense.net/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "myscript.js"
      ]
    }
  ]
}
