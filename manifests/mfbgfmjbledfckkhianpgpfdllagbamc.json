{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ivy 新标签页优化扩展",
  "description": "在新标签页显示网站快捷方式，以及来自你的社交网络的新资讯。",
  "version": "0.0.2",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "preference.html",
  "background": {
    "scripts": [
      "install.js"
    ]
  },
  "permissions": [
    "storage",
    "https://api.feedbin.com/*"
  ],
  "icons": {
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  }
}
