{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Better Study Tools",
  "description": "This extension helps you to watch any youtube video on better study tools.",
  "version": "1.1",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "besto.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "besto.html"
  }
}
