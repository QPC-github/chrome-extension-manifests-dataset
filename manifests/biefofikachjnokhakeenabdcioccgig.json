{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Classical Music Only",
  "description": "Discover and listen to thousands of classical music works filtered by category, period and composer.",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icons/cmo_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/cmo_16.png",
    "32": "icons/cmo_32.png",
    "96": "icons/cmo_96.png",
    "128": "icons/cmo_128.png"
  },
  "permissions": [
    "https://classicalmusiconly.com/*",
    "cookies",
    "storage"
  ]
}
