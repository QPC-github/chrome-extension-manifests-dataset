{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Krishna PS WebRequest Background Blocking",
  "description": "Testing Webrequests Blocking in PS",
  "version": "1.0.0.8",
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": true
  },
  "permissions": [
    "audioCapture",
    "clipboardWrite",
    "desktopCapture",
    "webRequest",
    "webRequestBlocking",
    "fileSystem",
    {
      "fileSystem": [
        "write"
      ]
    },
    "mediaGalleries",
    {
      "mediaGalleries": [
        "read",
        "allAutoDetected"
      ]
    },
    {
      "usbDevices": [
        {
          "vendorId": 123,
          "productId": 456
        }
      ]
    }
  ],
  "arc_metadata": {
    "apkList": [
      "VMware-Horizon-Client-AndroidOS-ARC-4.3.0-4710774.apk"
    ],
    "disableAutoBackButton": true,
    "enableExternalDirectory": false,
    "enableSynthesizeTouchEventsOnWheel": false,
    "formFactor": "fullscreen",
    "name": "VMware Horizon Client",
    "short_name": "VMware Horizon Client",
    "orientation": "landscape",
    "packageName": "com.vmware.view.client.android",
    "pauseOnBlur": true,
    "resize": "reconfigure",
    "sleepOnBlur": false
  }
}
