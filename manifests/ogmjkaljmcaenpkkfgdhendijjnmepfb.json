  "homepage_url": "http://www.top-page.ru",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "Календарь квартальный",
  "options_page": "options/index.html",
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "notifications",
    "storage"
