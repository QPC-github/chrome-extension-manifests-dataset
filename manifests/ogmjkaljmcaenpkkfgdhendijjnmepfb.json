{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background/index.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Календарь квартальный"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
