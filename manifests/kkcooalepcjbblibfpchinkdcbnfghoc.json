{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Doc Finder for Chrome",
  "description": "Find static resources on a target website.",
  "version": "0.2.0",
  "browser_action": {
    "default_icon": "logo-16.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab"
  ]
}
