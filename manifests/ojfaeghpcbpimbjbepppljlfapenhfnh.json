{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "download to folder",
  "author": "Andrey Vladykin",
  "version": "0.0.3",
  "description": "",
  "icons": {
    "16": "img/icon128.png",
    "48": "img/icon128.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "downloads",
    "history"
  ]
}
