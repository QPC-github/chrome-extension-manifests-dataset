{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Zenwego",
  "version": "1.1",
  "manifest_version": 2,
  "description": "__MSG_manifest_description__",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "__MSG_manifest_default_title__"
  },
  "homepage_url": "http://www.zenwego.com",
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' https://r-static.zenwego.com https://ajax.googleapis.com/; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
