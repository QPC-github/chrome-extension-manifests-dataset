{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "reddit.js"
      ],
      "matches": [
        "*://*.reddit.com/",
        "*://*.reddit.com/hot*",
        "*://*.reddit.com/rising*",
        "*://*.reddit.com/r/popular*",
        "*://*.reddit.com/r/all*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "facebook.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Stop mindless scrolling by killing home feeds.",
  "manifest_version": 2,
  "name": "Kill Social Feeds",
  "version": "1.0.0",
  "author": "Prajna Concepts"
}
