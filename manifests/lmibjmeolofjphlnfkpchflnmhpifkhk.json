{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "feedly+",
  "description": "This extension applies a new style to feedly.com",
  "version": "1.2.6",
  "icons": {
    "16": "feedly+.png",
    "48": "feedly+.png",
    "128": "feedly+128.png"
  },
  "web_accessible_resources": [
    "roboto.woff"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://feedly.com/*"
      ],
      "css": [
        "feedly+.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
