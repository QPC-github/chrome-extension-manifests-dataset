{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "I'm feeling lucky hotkey",
  "version": "1.1",
  "description": "Adds a hotkey to navigate to the first google search result.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "commands": {
    "toggle-feature-go": {
      "suggested_key": {
        "default": "Alt+L",
        "mac": "Alt+L"
      },
      "description": "Go to the first search result"
    }
  },
  "manifest_version": 2
}
