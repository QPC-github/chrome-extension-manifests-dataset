{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Git First",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Takes you to first commit of any GitHub repository",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "browser_action": {
    "default_title": "Git First",
    "default_icon": "/icons/icon128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
