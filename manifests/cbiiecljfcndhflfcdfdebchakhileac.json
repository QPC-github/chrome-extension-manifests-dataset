{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Informatikk innlogging",
  "description": "This extension will help you log in to Informatikk 1/2",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
