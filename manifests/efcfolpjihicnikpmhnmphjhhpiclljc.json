{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Siteimprove Accessibility Checker",
  "version": "126",
  "author": "Siteimprove",
  "description": "Jumpstart Your Web Accessibility Efforts Directly in Google Chrome",
  "homepage_url": "https://siteimprove.com",
  "offline_enabled": true,
  "incognito": "split",
  "default_locale": "en_US",
  "minimum_chrome_version": "53",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "img/symbol@16w.png",
    "19": "img/symbol@19w.png",
    "32": "img/symbol@32w.png",
    "38": "img/symbol@38w.png",
    "48": "img/symbol@48w.png",
    "128": "img/symbol@128w.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": {
      "16": "img/symbol@16w.png",
      "19": "img/symbol@19w.png",
      "32": "img/symbol@32w.png",
      "38": "img/symbol@38w.png",
      "48": "img/symbol@48w.png",
      "128": "img/symbol@128w.png"
    }
  },
  "web_accessible_resources": [
    "css/extension.css",
    "img/icons.svg",
    "img/logo.svg"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
