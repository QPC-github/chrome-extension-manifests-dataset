{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Multilingual Voice Search",
  "version": "1.1",
  "description": "Perform Google voice search in various languages.",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "vs_bg.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "vs_icon.png"
  },
  "icons": {
    "16": "vs_icon.png",
    "32": "vs_icon.png",
    "48": "vs_icon.png",
    "128": "vs_icon.png"
  },
  "manifest_version": 2
}
