{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Undrumpf",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Returning Donald to his rightful glory",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "name": "Undrumpf",
    "icons": [
      "icon.png"
    ],
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
