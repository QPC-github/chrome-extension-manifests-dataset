{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PeakSpan Capital's ADA Finder",
  "short_name": "ADAFinder",
  "version": "0.2.6.5",
  "description": "Automated Company Search in ADA",
  "author": "Vikas Kumar",
  "homepage_url": "https://wopr.peakspan.io",
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*",
        "http://*/",
        "https://*/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Find Company in ADA"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "contextMenus",
    "webRequest",
    "<all_urls>"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "Ctrl+Shift+Space"
      }
    },
    "ada_search_action": {
      "description": "Find Company in various datasources",
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Ctrl+Shift+F"
      }
    },
    "ada_jesus_walks": {
      "description": "Jesus walks on water!",
      "suggested_key": {
        "default": "Ctrl+Shift+6",
        "mac": "Ctrl+Shift+6"
      }
    }
  },
  "manifest_version": 2
}
