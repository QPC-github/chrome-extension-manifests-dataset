{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Magic Mail Extractor & Validator",
  "short_name": "Magic Mail Extractor",
  "description": "This addon will extract emails and clean them - eliminating invalid emails.",
  "version": "1.6.5",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*/*"
      ],
      "js": [
        "content/content.js"
      ]
    }
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "clipboardWrite",
    "downloads"
  ]
}
