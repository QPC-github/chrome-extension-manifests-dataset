{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bucovina Mapa",
  "description": "Program to fill data for registration site",
  "version": "1.40",
  "browser_action": {
    "default_icon": "icons/icon.48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://ghiseulonline.ro/*"
  ],
  "optional_permissions": [
    "tabs",
    "activeTab",
    "http://ghiseulonline.ro/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}
