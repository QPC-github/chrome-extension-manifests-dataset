{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Collector",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Collect all Youtube URLs from current page and create a playlist.",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "init.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
