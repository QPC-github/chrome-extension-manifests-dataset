{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Custom Hotkeys",
  "description": "Allows you to add custom hotkeys to a website.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "options_page": "options.html",
  "icons": {
    "48": "icon_48.png",
    "24": "icon_24.png"
  },
  "permissions": [
    "storage"
  ]
}
