{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RedirectR",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "bg.js",
      "jquery.js"
    ],
    "persistent": true
  },
  "description": "Redirects pages!",
  "icons": {
    "16": "./ico/16.png",
    "48": "./ico/48.png",
    "128": "./ico/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "./ico/16.png",
      "24": "./ico/24.png",
      "48": "./ico/48.png"
    },
    "default_title": "RedirectR",
    "default_popup": "./popup/popup.html"
  },
  "options_page": "/options/options.html",
  "author": "TheKoopaCrafter",
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'"
}
