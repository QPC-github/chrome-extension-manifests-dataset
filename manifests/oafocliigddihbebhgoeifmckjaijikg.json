{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sobek",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Sobek extension for Chrome",
  "browser_action": {
    "default_icon": "sobek_16.png",
    "default_popup": "sobekchrome.html"
  },
  "icons": {
    "16": "sobek_16.png",
    "48": "sobek_48.png",
    "128": "sobek_128.png"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "<all_urls>",
    "clipboardWrite",
    "clipboardRead"
  ]
}
