{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "A browser action to rename a tab title based on the URL",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "KPRenameTitle",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ],
  "icons": {
    "48": "icon-48x48.png",
    "128": "icon-128x128.png",
    "16": "icon-16x16.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "version": "1.0.0.76"
}
