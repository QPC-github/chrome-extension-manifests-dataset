{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Troll Quirk Remover for Homestuck.com",
  "description": "Attempts to remove the Homestuck Troll's leet speak quirks in chat logs.",
  "version": "0.1",
  "icons": {
    "32": "icon32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.homestuck.com/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ]
    }
  ]
}
