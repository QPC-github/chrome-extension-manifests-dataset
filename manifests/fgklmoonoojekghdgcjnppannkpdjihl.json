{
  "background": {
    "scripts": [
      "fifa-fut-chrome.js"
    ]
  },
  "browser_action": {
    "default_title": "TraderFUT"
  },
  "content_scripts": [
    {
      "js": [
        "openDialog.js",
        "jquery.js"
      ],
      "matches": [
        "*://traderfut.com/*"
      ]
    }
  ],
  "description": "Syncs your FIFA 20 FUT Trading with TraderFUT",
  "externally_connectable": {
    "matches": [
      "*://traderfut.com/*"
    ]
  },
  "icons": {
    "128": "logo-128.png",
    "16": "logo-16.png",
    "48": "logo-48.png"
  },
  "manifest_version": 2,
  "name": "TraderFUT Sync",
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*.easports.com/",
    "*://*.ea.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.0.4"
}
