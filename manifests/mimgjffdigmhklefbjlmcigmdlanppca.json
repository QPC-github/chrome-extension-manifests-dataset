{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "short_name": "Dortmund New Tab and Wallpapers",
  "version": "0.6.13",
  "author": "Adir Kol",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "topSites",
    "activeTab",
    "https://*.google.com/",
    "http://*.google.com/",
    "https://*.gstatic.com/",
    "https://*.google-analytics.com",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com/ga.js https://www.google-analytics.com/analytics.js; object-src 'self'",
  "background": {
    "scripts": [
      "resources/js/bgfile.js"
    ]
  },
  "icons": {
    "128": "resources/img/icon128.png",
    "48": "resources/img/icon48.png",
    "32": "resources/img/icon32.png",
    "16": "resources/img/icon16.png"
  },
  "browser_action": {
    "default_icon": "resources/img/icon16.png",
    "default_popup": "popup.html"
  }
}
