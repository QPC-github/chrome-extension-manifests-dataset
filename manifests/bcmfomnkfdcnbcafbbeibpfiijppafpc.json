{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-latest.js",
        "awesome.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "awesome_128.png",
    "64": "awesome_64.png",
    "32": "awesome_32.png"
  },
  "web_accessible_resources": [
    "icanseearainbow.css"
  ],
  "description": "This extension makes everything AWESOME by replacing negativity and swear words with the word 'awesome'.",
  "manifest_version": 2,
  "name": "Everything Is Awesome",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.0.2"
}
