{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "I'm Feeling Lucky",
  "description": "Access i'm feeling lucky from the address bar! Type '\\' and press Tab.",
  "version": "1.1",
  "omnibox": {
    "keyword": "\\"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
