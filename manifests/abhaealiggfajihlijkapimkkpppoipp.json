{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Har scrapper",
  "description": "Triggers Chrome to dump the HAR into the page context",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "page.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "devtools_page": "devtools.html",
  "manifest_version": 2
}
