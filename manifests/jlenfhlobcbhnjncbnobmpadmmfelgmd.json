{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EverSuite Mega Extension",
  "short_name": "EverSuite Mega Extension",
  "version": "1.0.11",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "storage",
    "downloads",
    "cookies",
    "clipboardWrite",
    "notifications",
    "http://files1.restunited.com/*",
    "https://cdnjs.cloudflare.com/*",
    "https://cdn.socket.io/*",
    "https://stackpath.bootstrapcdn.com/*",
    "https://code.jquery.com/*"
  ],
  "web_accessible_resources": [
    "icons/38x38.png"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "EverSuite Mega Extension",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdnjs.cloudflare.com https://cdn.socket.io https://stackpath.bootstrapcdn.com https://code.jquery.com; object-src 'self'"
}
