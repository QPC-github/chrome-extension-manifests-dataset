{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Внутреннее расписание академии Наяновой",
  "description": "Модифицирует запросы к базе расписания для получения данных",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "Внутреннее расписание академии Наяновой"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://wiki.nayanova.edu/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
