{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Collokia Tips TEST",
  "manifest_version": 2,
  "version": "2.11.0",
  "description": "Use machine learning to automatically keep track of your IT team skills set. Collokia is secure and confidential.",
  "homepage_url": "https://www.collokia.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "pages/background/index.html",
    "persistent": true
  },
  "options_page": "pages/options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19-popup-inactive.png",
    "default_title": "Collokia Tips TEST",
    "default_popup": "pages/browser-action/index.html"
  },
  "optional_permissions": [
    "history"
  ],
  "permissions": [
    "idle",
    "notifications",
    "activeTab",
    "tabs",
    "webNavigation",
    "storage",
    "alarms",
    "contextMenus",
    "https://api.collokia.com/*",
    "https://api-test.collokia.com/*",
    "https://app.collokia.com/*",
    "https://app-test.collokia.com/*",
    "https://api-dev.collokia.com/*",
    "https://app-dev.collokia.com/*",
    "https://api-dev.collokia.com:8443/*",
    "https://app-dev.collokia.com:8443/*",
    "http://app-dev.collokia.com:9000/*",
    "https://www.google-analytics.com/",
    "http://*/*",
    "https://*/*"
