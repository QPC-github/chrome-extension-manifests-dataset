  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_globs": [
        "*://accounts.google.com/*",
        "*://mail.google.com/*",
        "*://*.google.com/maps/*",
        "*://*.google.com.??/maps/*",
        "*://*.google.com/calendar/*",
        "*://*.google.com.??/calendar/*",
        "*://*/_/chrome/newtab*"
