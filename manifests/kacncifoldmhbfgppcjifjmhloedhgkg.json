      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_globs": [],
      "include_globs": [
        "https://app.collokia.com/*",
        "https://app-test.collokia.com/*",
        "https://app-dev.collokia.com:8443/*",
        "http://app-dev.collokia.com:9000/*"
      ],
      "js": [
        "build/bootstrap/inject/app-comm.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "build/*.js",
    "build/**/*.js",
    "build/**/*.js.map",
    "jspm_packages/**/*.js",
    "jspm_packages/**/*.css",
