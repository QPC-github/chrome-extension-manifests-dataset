{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Realations",
  "version": "1.0.0",
  "description": "Realations",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage",
    "notifications",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Realations",
    "default_popup": "popup.html",
    "default_icon": "icon-38.png"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png"
  },
  "manifest_version": 2
}
