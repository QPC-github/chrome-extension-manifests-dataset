{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Folha, Estadão e O Globo SEM LIMITES",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Leia quantas reportagens quiser na Folha, Estadão ou no O Globo sem se preocupar em ser bloqueado pelo seu limite de acessos",
  "homepage_url": "https://github.com/joseandro/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "chrome://favicon/",
    "cookies",
    "tabs",
    "<all_urls>"
  ]
}
