{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MaxRoyal",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Go to Current Domain",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://*/"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Redirect Current Domain"
  }
}
