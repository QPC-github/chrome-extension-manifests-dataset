{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2",
  "name": "NOW Proxy",
  "description": "NOW any online and free",
  "author": "L1 TECH",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "declarativeWebRequest",
    "proxy",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "NOW Proxy: Disabled"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
