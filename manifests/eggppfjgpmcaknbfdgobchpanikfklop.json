{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TinyHands.io",
  "version": "1.0.1",
  "description": "Big Wig. Tiny Hands.",
  "icons": {
    "16": "bigwig-16.png",
    "32": "bigwig-32.png",
    "64": "bigwig-64.png",
    "96": "bigwig-96.png",
    "128": "bigwig-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
