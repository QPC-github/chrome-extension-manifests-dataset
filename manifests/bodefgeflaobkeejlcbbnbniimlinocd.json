{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello Card Counter",
  "description": "This extension is a product of my laziness. You don't need to count the cards anymore.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.11.2.min.js",
        "silent.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
