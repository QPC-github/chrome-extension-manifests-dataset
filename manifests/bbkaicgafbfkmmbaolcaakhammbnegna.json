{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Download Images",
  "description": "Displays all webpage images and allows user to download",
  "version": "1.0",
  "manifest_version": 2,
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.jpg"
    }
  },
  "icons": {
    "16": "icon.jpg"
  },
  "permissions": [
    "downloads",
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
