{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Contaker",
  "version": "1.0.1",
  "author": "Contaker",
  "description": "View your contacts in Contaker cloud",
  "permissions": [
    "http://contaker.com/*",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "My Contacts",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "imgs/19.png",
      "38": "imgs/38.png"
    }
  },
  "default_locale": "en",
  "icons": {
    "16": "imgs/16.png",
    "48": "imgs/48.png",
    "128": "imgs/128.png"
  }
}
