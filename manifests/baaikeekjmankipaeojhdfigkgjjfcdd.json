{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cheapveilingen - productveilingen",
  "short_name": "Cheapveilingen",
  "version": "1.0.2",
  "description": "Ontdek de spanning van het bieden en de voldoening van het winnen.",
  "icons": {
    "16": "images/64x64.png",
    "48": "images/64x64.png",
    "128": "images/64x64.png"
  },
  "homepage_url": "http://www.www.cheapveilingen.nl/",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://www.cheapveilingen.nl/*"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Cheapveilingen!",
    "default_popup": "popup.html"
  }
}
