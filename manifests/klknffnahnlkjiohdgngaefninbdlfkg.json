{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "run_at": "document_end",
      "css": [
        "css/clarin.css"
      ],
      "matches": [
        "*://*.clarin.com/*"
      ],
      "js": [
        "js/clarin.js"
      ]
    },
    {
      "run_at": "document_end",
      "css": [
        "css/ln.css"
      ],
      "web_accessible_resources": [
        "css/ln.css",
        "fonts/*.*"
      ],
      "matches": [
        "*://*.lanacion.com.ar/*"
      ],
      "exclude_globs": [
        "*://canchallena.lanacion.com.ar/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/ln.js"
      ],
      "all_frames": true
    },
    {
      "run_at": "document_end",
      "css": [
        "css/cancha-llena.css"
      ],
      "web_accessible_resources": [
        "css/cancha-llena.css"
      ],
      "matches": [
        "*://canchallena.lanacion.com.ar/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/cancha-llena.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "name": "Clarín+Nación - Sin registro ni publicidades",
  "description": "Oculta el formulario de registro obligatorio para leer el contenido.",
  "version": "1.3.8.6",
  "homepage_url": "http://www.juanchorossi.com/evitar-registro-lanacion",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
