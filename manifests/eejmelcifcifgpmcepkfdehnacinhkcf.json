{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "App for Twitter™",
  "permissions": [
    "storage"
  ],
  "short_name": "appfortwitter",
  "background": {
    "page": "lib/chrome/background.html"
  },
  "description": "An easy-to-use and lite App for Twitter",
  "homepage_url": "http://mybrowseraddon.com/twitter-app.html",
  "browser_action": {
    "default_icon": "data/icon16.png",
    "default_title": "App for Twitter™"
  },
  "icons": {
    "16": "data/icon16.png",
    "32": "data/icon32.png",
    "48": "data/icon48.png",
    "64": "data/icon64.png",
    "128": "data/icon128.png"
  }
}
