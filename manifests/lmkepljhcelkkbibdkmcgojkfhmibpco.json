{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "2.4.3",
  "manifest_version": 2,
  "icons": {
    "32": "icons/ic_32.png",
    "48": "icons/ic_48.png",
    "96": "icons/ic_96.png",
    "128": "icons/ic_128.png"
  },
  "browser_action": {
    "default_title": "Who Deleted Me ?"
  },
  "background": {
    "scripts": [
      "js/lib/jquery-3.1.1.min.js",
      "js/background.js",
      "js/messaging.js"
    ]
  },
  "permissions": [
    "https://*.facebook.com/*",
    "storage",
    "cookies",
    "notifications"
  ],
  "externally_connectable": {
    "matches": [
      "http://deleted.miinosoft.com/*"
    ]
  },
  "web_accessible_resources": [
    "icons/*.png"
  ]
}
