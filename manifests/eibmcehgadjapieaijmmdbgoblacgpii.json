{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "MOLUTI",
  "version": "1.4",
  "manifest_version": 2,
  "homepage_url": "http://moluti.com",
  "description": "MOLUTI for Chrome offers personal web analystics in the browser through an explorable visualisation of your browsing history",
  "browser_action": {
    "default_icon": "moluti-icon.png",
    "default_popup": "popup.html",
    "default_title": "MOLUTI"
  },
  "icons": {
    "16": "moluti-icon-16.png",
    "48": "moluti-icon-48.png",
    "128": "moluti-icon-128.png"
  },
  "permissions": [
    "history",
    "http://www.moluti.com/"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com/jsapi; object-src 'self'"
}
