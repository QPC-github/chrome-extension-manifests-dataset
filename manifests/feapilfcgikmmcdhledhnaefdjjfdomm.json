{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DestinyChecklist.net Spreadsheet Manager",
  "short_name": "DCL",
  "version": "1.4.8",
  "manifest_version": 2,
  "description": "Spreadsheet views of gear and PVP info",
  "homepage_url": "http://destinychecklist.net",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://maxcdn.bootstrapcdn.com https://cdnjs.cloudflare.com; object-src 'self'",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "permissions": [
    "storage",
    "*://*.bungie.net/*",
    "*://*.destinychecklist.net/*",
    "*://*.destinychecklist.net/*",
    "storage",
    "cookies",
    "tabs"
  ]
}
