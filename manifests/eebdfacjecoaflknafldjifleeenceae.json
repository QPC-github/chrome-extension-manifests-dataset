{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Desktop-Capturing Azharegypt.net ",
  "author": "Hashem Aly 01141481921",
  "version": "1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Azharegypt.net Desktop-Capturing API to share entire screen.",
  "homepage_url": "https://azharegypt.net/",
  "background": {
    "scripts": [
      "RTCMultiConnection-v1.9.js",
      "desktop-capturing.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/desktopCapture22.png",
    "default_title": "Share Desktop!"
  },
  "icons": {
    "16": "images/desktopCapture16.png",
    "22": "images/desktopCapture22.png",
    "32": "images/desktopCapture32.png",
    "48": "images/desktopCapture48.png",
    "128": "images/desktopCapture128.png"
  },
  "permissions": [
    "desktopCapture",
    "activeTab",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "images/desktopCapture48.png"
  ]
}
