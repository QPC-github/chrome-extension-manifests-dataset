{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Oust-Screen-Share",
  "version": "1.0",
  "author": "Vikrant",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.oustme.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
