{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Have This",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "DOM elements to PNG + instant style guide",
  "homepage_url": "http://iamhavingthis.com",
  "icons": {
    "16": "favicon-16.png",
    "48": "favicon-48.png",
    "128": "favicon-120.png",
    "256": "favicon-256.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Have This - DOM elements to PNG"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "activeTab",
    "storage"
  ]
}
