{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Roblox Assault Team - Group Shout Notifier",
  "description": "This extension is used to inform RAT members whenever a new shout was published.",
  "version": "1.0.1",
  "icons": {
    "128": "rat-logo.png"
  },
  "background": {
    "scripts": [
      "logic.js"
    ]
  },
  "web_accessible_resources": [
    "rat-logo.png"
  ],
  "permissions": [
    "http://*.roblox.com/*",
    "notifications",
    "tabs"
  ]
}
