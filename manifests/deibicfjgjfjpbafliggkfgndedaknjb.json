{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ClickValue's Qubit Chrome extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A Qubit extension to see active experiments and change variations",
  "author": "Jan Schutte",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Qubit",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "web_accessible_resources": [
    "js/readpage.js"
  ],
  "permissions": [
    "cookies",
    "https://*/*",
    "http://*/*",
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "storage"
  ]
}
