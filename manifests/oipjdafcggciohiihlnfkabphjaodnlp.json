{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "fast up-scroller",
  "description": "scroll page easy and fast",
  "version": "2.0",
  "icons": {
    "200": "cst.png"
  },
  "background": {
    "scripts": [
      "my.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "click to unhide fast-scroller",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
