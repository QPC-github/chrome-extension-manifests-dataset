{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "options_page": "options.html",
  "name": "ReNZB",
  "description": "Send nzb files to NZBGet",
  "version": "0.3",
  "permissions": [
    "*://*/",
    "contextMenus",
    "tabs",
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "jquery-2.0.3.min.js",
      "nzbget.js",
      "init.js"
    ]
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
