{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Enlightened Browsing",
  "description": "A calming way to browse the web",
  "version": "1.3",
  "icons": {
    "48": "48-icon.png",
    "128": "128-icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "popup.html"
  }
}
