{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Field Nation WorkSmart Connect",
  "version": "3.3",
  "permissions": [
    "http://parse.fieldnation.com/*",
    "https://parse.fieldnation.com/*",
    "http://app.fieldnation.com/*",
    "https://app.fieldnation.com/*",
    "activeTab",
    "tabs",
    "background",
    "storage",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "fonts/*.woff"
  ],
  "background": {
    "scripts": [
      "background.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_title": "Field Nation WorkSmart Connect",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
