{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Weather Getter",
  "description": "Gets weather data and formats into a column.",
  "version": "2.1",
  "author": "Justin Francisco",
  "browser_action": {
    "default_icon": "Main_Icon-02.png",
    "default_title": "Get that weather!",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "Main_Icon-02.png",
    "48": "Small_Icon-02.png",
    "16": "Small_Icon-04.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "script.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
