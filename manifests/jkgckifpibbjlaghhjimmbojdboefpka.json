{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TeamWave",
  "description": "TeamWave extension to quickly add tasklist, tasks, deals, contacts, request for leave and much more",
  "version": "1.11",
  "icons": {
    "16": "images/favicon.png",
    "48": "images/favicon.png",
    "128": "images/favicon.png"
  },
  "browser_action": {
    "default_icon": "images/favicon.png",
    "default_popup": "popup.html",
    "default_title": "Teamwave"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "cookies",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/socket.io.js",
      "background.js"
    ]
  }
}
