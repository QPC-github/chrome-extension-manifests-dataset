{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ebay Timezone Fixer",
  "version": "1.0",
  "description": "Fix the PDT date shown in My Ebay. The date will be convernted to your local time.",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*.ebay.com/*"
      ]
    }
  ],
  "optional_permissions": [
    "tabs",
    "http://www.google.com/"
  ],
  "manifest_version": 2
}
