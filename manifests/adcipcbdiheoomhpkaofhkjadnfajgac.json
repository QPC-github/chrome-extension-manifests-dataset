{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pixiv Supporter",
  "version": "0.2",
  "description": "help your browsing in pixiv",
  "content_scripts": [
    {
      "matches": [
        "http://www.pixiv.net/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
