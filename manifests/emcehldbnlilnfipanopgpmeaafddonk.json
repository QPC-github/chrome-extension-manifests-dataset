{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "files/jquery.js",
      "files/jquery.ajax-retry.js",
      "ga.js",
      "background.js",
      "files/newtabpromo_bg.js",
      "proxy/Proxy.js",
      "proxy/ProxyInit_chrome.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_appButtonDesc__"
  },
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.rutracker.org/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' blob: 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_ui": {
    "page": "ya.sovetnik/options/options.html"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "version": "1.1.0.44"
}
