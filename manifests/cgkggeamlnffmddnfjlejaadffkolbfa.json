{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PokerCity Forum Theme",
  "description": "This extension contains a theme for the website forum.pokercity.nl.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://forum.pokercity.nl/*"
      ],
      "css": [
        "pokercity.css"
      ],
      "js": [
        "jquery.js",
        "pokercity.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "Images/*.png"
  ]
}
