{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Navigate Me",
  "short_name": "Navigate Me",
  "description": "This extension helps you to save your bookmark, you will be able to access it from anywhere",
  "version": "0.0.8",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Navigate Me"
  },
  "icons": {
    "16": "images/navigate.png",
    "48": "images/navigate.png",
    "128": "images/navigate.png"
  },
  "permissions": [
    "tabs"
  ]
}
