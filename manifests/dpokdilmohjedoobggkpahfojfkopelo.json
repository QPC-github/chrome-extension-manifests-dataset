{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "assets/128.png",
    "default_popup": "app/index.html",
    "default_title": "Randomizer"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Easily randomize a list of text",
  "icons": {
    "128": "assets/128.png",
    "16": "assets/16.png",
    "32": "assets/32.png",
    "64": "assets/64.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "38",
  "name": "Randomizer",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://*/",
    "https://*/"
  ],
  "short_name": "Randomizer",
  "version": "0.0.4"
}
