{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Haiku Improved",
  "description": "Alpha-Version Extension aimed at overhauling the Powerschool Learning (formerly Haiku Learning) website",
  "version": "0.0.94.1",
  "manifest_version": 2,
  "author": "Ethan Printz",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png",
    "1024": "icons/icon1024.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.learning.powerschool.com/u/*/portal*"
      ],
      "css": [
        "css/dashboard.css"
      ],
      "js": [
        "plugins/jquery.js",
        "js/dashboard.js"
      ]
    },
    {
      "matches": [
        "*://*.learning.powerschool.com/do/account/login",
        "*://pacificridge.learning.powerschool.com/do/account/*"
      ],
      "css": [
        "css/login.css"
      ]
    }
  ]
}
