{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LearnPlatform for Students",
  "description": "LearnPlatform for Students provides school administrators an easy way to measure student engagement with EdTech products.",
  "short_name": "LearnPlatform for Students",
  "minimum_chrome_version": "6",
  "icons": {
    "128": "icons/icon.png",
    "48": "icons/icon.png",
    "16": "icons/icon.png"
  },
  "version": "1.15",
  "page_action": {
    "default_icon": {
      "19": "icons/icon.png",
      "38": "icons/icon.png"
    },
    "default_title": "LearnPlatform for Students",
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms",
    "tabs",
    "gcm",
    "notifications",
    "storage",
    "contextMenus",
    "identity",
    "identity.email",
    "https://app.learnplatform.com/*",
    "https://ep.learnplatform.com/*"
  ],
  "background": {
    "scripts": [
      "/background.js"
    ]
  },
  "web_accessible_resources": [
    "/popup/popup.css",
    "/popup/popup.css.map",
    "/popup/popup.js",
    "/popup/popup.js.map"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self' 'unsafe-eval'",
  "offline_enabled": false,
  "manifest_version": 2
}
