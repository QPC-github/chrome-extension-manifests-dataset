{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Homepage love2liveblog.pl",
  "description": "Set homepage on site love2liveblog.pl",
  "version": "0.0.0.10",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "image.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.love2liveblog.pl/",
    "startup_pages": [
      "http://www.love2liveblog.pl/"
    ]
  },
  "icons": {
    "128": "image.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://www.love2liveblog.pl/",
    "http://www.love2liveblog.pl/"
  ],
  "externally_connectable": {
    "matches": [
      "*://love2liveblog.pl/*"
    ]
  }
}
