{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DotNetJump",
  "description": "Search multiple documents for .NET at once. Jump to another .NET document.",
  "version": "0.1",
  "icons": {
    "16": "/static/img/logo.png",
    "48": "/static/img/logo.png",
    "128": "/static/img/logo.png"
  },
  "browser_action": {
    "default_title": "DotNetJump",
    "default_popup": "popup.html",
    "default_icon": "/static/img/logo.png"
  },
  "permissions": [
    "activeTab",
    "https://docs.microsoft.com/*",
    "https://referencesource.microsoft.com/*",
    "https://services.social.microsoft.com/*"
  ]
}
