{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PDP Analysis",
  "description": "Price Trend Analysis",
  "version": "2.0",
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.bundle.js; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Hello!",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
