{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BrowseMyTown Netherlands",
  "description": "Search Towns and Cities",
  "version": "2.0.0.1",
  "browser_action": {
    "default_title": "Visit a Town or City",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://www.browsemytown.com/"
  ]
}
