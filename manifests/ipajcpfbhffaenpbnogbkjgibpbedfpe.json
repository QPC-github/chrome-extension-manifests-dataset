{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PM2.5空气质量指数",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "将PM2.5测量而得的空气质量指数及污染颜色直观的显示在浏览器上。",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "notifications",
    "http://www.mysem.com.cn/*"
  ]
}
