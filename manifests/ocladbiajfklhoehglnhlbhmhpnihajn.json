{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cloudsday",
  "description": "Cloudsday Weather forecast. Provides 10 days weather forecast and 48 hours weather forecast. New page design.",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "app": {
    "urls": [
      "http://cloudsday.com/"
    ],
    "launch": {
      "web_url": "http://cloudsday.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
