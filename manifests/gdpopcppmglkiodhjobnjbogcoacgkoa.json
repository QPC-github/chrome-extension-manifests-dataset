{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Close Left",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Close Left Tabs.",
  "icons": {
    "16": "close_left.png",
    "48": "close_left_48.png",
    "128": "close_left_128.png"
  },
  "browser_action": {
    "default_icon": "close_left.png",
    "default_title": "Close Left"
  },
  "background": {
    "scripts": [
      "close_left.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
