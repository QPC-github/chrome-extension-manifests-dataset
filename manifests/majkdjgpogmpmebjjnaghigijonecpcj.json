{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GetYourCouponCodes",
  "version": "1.0",
  "manifest_version": 2,
  "description": "GetYourCouponCodes will help you find the latest coupon codes online.",
  "homepage_url": "https://www.getyourcouponcodes.com",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "src/browser_action/getyourcouponcodes.html"
  },
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en"
}
