{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wayback Images",
  "version": "1.3",
  "default_locale": "en",
  "description": "__MSG_chrome_extension_description__",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "main.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/",
        "https://*/"
      ],
      "js": [
        "jquery-3.2.1.slim.js"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon_19.png"
    },
    "default_title": ""
  }
}
