{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VipSkidka",
  "short_name": "VipSkidka",
  "description": "Шоппинг клуб VipSkidka",
  "version": "1.1",
  "icons": {
    "16": "vipskidka/img/icon16.png",
    "48": "vipskidka/img/icon48.png",
    "128": "vipskidka/img/icon128.png"
  },
  "background": {
    "scripts": [
      "vipskidka/js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Шоппинг клуб VipSkidka",
    "default_icon": "vipskidka/img/icon.png",
    "default_popup": "vipskidka/popup.html"
  },
  "web_accessible_resources": [
    "vipskidka/js/*",
    "vipskidka/img/*",
    "vipskidka/font/*"
  ],
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "notifications",
    "https://www.skidka.vip/*"
  ]
}
