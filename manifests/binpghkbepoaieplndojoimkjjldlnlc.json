{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chromie app",
  "description": "Make the best out of your chrome device.",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "1.2",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "clipboardWrite"
  ],
  "icons": {
    "16": "chromie.png",
    "128": "chromie.png"
  }
}
