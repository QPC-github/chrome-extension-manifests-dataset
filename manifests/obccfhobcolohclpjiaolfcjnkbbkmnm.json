{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "128": "frenysIcon144x144.png"
  },
  "name": "Frenys: one-click collect",
  "description": "This extension helps you collect things using frenys.",
  "version": "0.2",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
