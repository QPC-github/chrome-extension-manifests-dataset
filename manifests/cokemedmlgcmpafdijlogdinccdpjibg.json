{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouControl - перевірка контрагентів",
  "description": "YouControl - перевірка контрагентів",
  "version": "1.7",
  "icons": {
    "64": "favicon_64.png",
    "32": "favicon_32.png"
  },
  "background": {
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_title": "YouControl - перевірка контрагентів",
    "default_icon": {
      "64": "favicon_64.png",
      "32": "favicon_32.png"
    },
    "default_popup": "popup.html"
  }
}
