{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Talkonly 直播通知",
  "description": "在 Talkonly 節目直播開始時通知使用者",
  "version": "0.3.1",
  "background": {
    "scripts": [
      "global.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "register.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/*",
    "gcm",
    "storage",
    "alarms",
    "notifications"
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
