{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "科学上网",
  "description": "最简单的科学上网工具，内置https 代理/spdy proxy，无障碍访问网站，请先禁用Proxy SwitchySharp等其它代理管理扩展。",
  "version": "0.2.8.3",
  "permissions": [
    "tabs",
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "128": "image/icon-128.png"
  },
  "browser_action": {
    "default_icon": "image/inactive.png",
    "default_popup": "html/popup3.html"
  },
  "options_page": "html/options.html",
  "background": {
    "page": "html/background.html"
  }
}
