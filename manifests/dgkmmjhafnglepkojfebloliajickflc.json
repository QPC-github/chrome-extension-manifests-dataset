{
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/script.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "*://*.themeforest.net/*",
        "*://*.codecanyon.net/*",
        "*://*.videohive.net/*",
        "*://*.audiojungle.net/*",
        "*://*.graphicriver.net/*",
        "*://*.photodune.net/*",
        "*://*.3docean.net/*",
        "*://*.activeden.net/*",
        "*://*.envato.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*",
    "options.html"
  ],
  "browser_action": {
    "name": "Elite Buyer"
  },
  "background": {
    "page": "background.html"
  },
  "description": "Some Cool Features for Regular Buyers of Envato Market",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "Elite Buyer",
  "short_name": "Elite Buyer",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "webRequest",
    "notifications",
    "background"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.1"
}
