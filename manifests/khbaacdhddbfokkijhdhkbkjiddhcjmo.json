{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Consolio",
  "short_name": "Consolio",
  "description": "A cloud console in your Google Chrome browser",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*.consolio.co/*",
    "externally_connectable"
  ]
}
