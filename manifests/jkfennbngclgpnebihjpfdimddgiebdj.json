{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IPaddress.is IP address lookup",
  "description": "Shows your internet ip address and give you general details about an IP address.",
  "version": "1.1",
  "icons": {
    "16": "ico16.png",
    "48": "ico48.png",
    "128": "ico128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "manifest_version": 2
}
