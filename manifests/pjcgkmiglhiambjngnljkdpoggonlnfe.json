{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "*://music.163.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "inject.js"
  ],
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "browser_action": {},
  "manifest_version": 2,
  "permissions": [
    "*://music.163.com/",
    "*://*.music.126.net/",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "name": "__MSG_name__",
  "author": "nondanee",
  "version": "0.0.5"
}
