{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tweet Stories",
  "short_name": "Tweetstories",
  "version": "1.0",
  "author": "Anthony Aubertin @noxdzine",
  "description": "Tweet DN stories quickly!",
  "content_scripts": [
    {
