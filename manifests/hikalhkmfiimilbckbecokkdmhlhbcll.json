{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SharePad - A Simple Notepad ",
  "description": "A Simple notepad extension that saves what you jot down and syncs with your Google account!",
  "version": "1.0.9",
  "offline_enabled": true,
  "icons": {
    "19": "imgs/sharepad19.png",
    "48": "imgs/sharepad48.png",
    "128": "imgs/sharepad128.png"
  },
  "browser_action": {
    "default_icon": "imgs/sharepad128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
