{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pulp",
  "version": "3.8",
  "description": "Your daily inspiration",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "identity",
    "storage",
    "https://www.rt.com/rss/",
    "http://www.aljazeera.com/xml/rss/all.xml",
    "http://www.aljazeera.com/assets/images/AljazeeraLogo.jpg",
    "http://feeds.feedburner.com/TechCrunch/",
    "https://s0.wp.com/wp-content/themes/vip/techcrunch-2013/assets/images/logo.svg"
  ],
  "content_security_policy": "script-src 'unsafe-eval' https://api.unsplash.com/"
}
