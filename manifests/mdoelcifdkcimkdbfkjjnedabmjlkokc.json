{
  "name": "Video Control ( new gamma )",
  "version": "3.9.9",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "__MSG_chrome_extension_description__",
  "default_locale": "en",
  "manifest_version": 2,
  "icons": {
    "128": "24.png",
    "16": "24.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "/css/style_background.css",
        "/js/jquery-ui-1.11.4/jquery-ui.css"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.js",
        "storage.js",
        "fullscreen.js",
        "arrive.js",
        "background.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "*://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "tab.js"
    ]
  },
  "web_accessible_resources": [
    "/*",
    "*://*"
  ],
  "browser_action": {
    "default_title": "__MSG_browser_action_title__",
    "default_icon": "24.png",
    "default_popup": "/help.html"
  },
  "content_security_policy": "object-src 'self' https://www.youtube.com/iframe_api https://ad.admitad.com https://money.yandex.ru https://api.vk.com/ ; script-src 'self' 'unsafe-eval' https://s.ytimg.com/yts/jsbin/www-widgetapi-vflxuRgW5/www-widgetapi.js https://ipinfo.io https://ad.admitad.com "
}
