{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "buttonBot",
  "description": "let me press it for you",
  "version": "3.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "buttonBot",
    "default_icon": "icon16.png"
  },
  "manifest_version": 2
}
