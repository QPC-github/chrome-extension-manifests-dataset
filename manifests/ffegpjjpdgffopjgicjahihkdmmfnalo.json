{
  "browser_action": {
    "default_icon": "48.png",
    "default_popup": "popup.html",
    "default_title": "Get Coupons"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery.js",
        "js/sweetalert.min.js",
        "js/content.js"
      ],
      "matches": [
        "https://www.amazon.ca/*",
        "https://amazon.ca/*",
        "https://www.amazon.com/*",
        "https://amazon.com/*"
      ]
    }
  ],
  "description": "Get valid coupons from Shopify stores when you search and then click on our Gravy extension.",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Gravy Coupons",
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.amazon.ca/*",
    "https://amazon.ca/*",
    "https://www.amazon.com/*",
    "https://amazon.com/*",
    "https://coupon.gravytrain.io/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.2",
  "web_accessible_resources": [
    "16.png"
  ]
}
