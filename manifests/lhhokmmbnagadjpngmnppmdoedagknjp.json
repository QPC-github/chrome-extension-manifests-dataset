{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AFIP fixer",
  "description": "Extensi√≥n de chrome para corregir errores del sitio de la AFIP",
  "version": "1.1",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.afip.gov.ar/*",
        "https://*.afip.gov.ar/*",
        "http://*.afip.gob.ar/*",
        "https://*.afip.gob.ar/*"
      ],
      "js": [
        "js/overrides.js",
        "js/fixer.js",
        "main.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
