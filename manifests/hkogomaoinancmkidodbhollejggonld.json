{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screenshare Extension",
  "short_name": "A screensharing extension for chrome",
  "description": "Screenshare Extension",
  "version": "1.0.4.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "desktopCapture",
    "<all_urls>"
  ]
}
