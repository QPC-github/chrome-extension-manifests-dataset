{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "URL Hotlink Conversion",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Attempts to directly link you to the image, currently supports Imgur & QuickMeme",
  "background": {
    "scripts": [
      "convert.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://imgur.com/",
    "http://www.quickmeme.com/"
  ]
}
