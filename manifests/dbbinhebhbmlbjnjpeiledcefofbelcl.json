{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Enterprise Verified Access Test Bed",
  "short_name": "Verified Access Test",
  "version": "3.1.28",
  "key": "",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "verified_access_56.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "56": "verified_access_56.png",
    "128": "verified_access_128.png"
  },
  "storage": {
    "managed_schema": "schema.json"
  },
  "content_security_policy": "script-src 'self' https://code.getmdl.io; object-src 'self'",
  "permissions": [
    "http://*/dvproxy/getchallenge*",
    "http://*/dvproxy/verifychallengeresponse?signeddata=*",
    "https://*/dvproxy/getchallenge*",
    "https://*/dvproxy/verifychallengeresponse?signeddata=*",
    "https://*.googleapis.com/*",
    "http://*.sandbox.google.com/*",
    "https://*.sandbox.google.com/*",
    "enterprise.platformKeys",
    "storage"
  ]
}
