{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CoolTab",
  "description": "Feel your new tab cool.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "CoolTab",
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "background.html"
  },
  "manifest_version": 2
}
