    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/app-temp.js"
      ],
      "css": [
        "css/app.css"
      ]
    }
  ]
}
