{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MetroUSS",
  "description": "Metro 2033 User Scanner Script",
  "permissions": [
    "https://alfa-core.net/*",
    "https://api.vk.com/*",
    "https://vk.com/*"
  ],
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-48.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.8.3.min.js",
        "jquery.balloon.js",
        "MD5.js",
        "script.js"
      ],
      "matches": [
        "https://api.vk.com/*",
        "https://vk.com/*",
        "https://alfa-core.net/*"
      ]
    }
  ],
  "icons": {
    "48": "icon-48.png"
  },
  "web_accessible_resources": [
    "icon-48.png",
    "jquery.balloon.js",
    "jquery-1.8.3.min.js",
    "MD5.js",
    "script.js"
  ]
}
