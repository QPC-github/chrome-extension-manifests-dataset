{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Un-Tumblr",
  "description": "Brings back the old reblog layout",
  "version": "1.1",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.1.4.min.js",
        "script.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "http://www.tumblr.com/*",
        "https://www.tumblr.com/*"
      ]
    }
  ],
  "manifest_version": 2
}
