{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Decouvrez.online",
  "description": "A better search for your browser.",
  "version": "1",
  "author": "decouvrez.online",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Search"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "https://decouvrez.online"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
