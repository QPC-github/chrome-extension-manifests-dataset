{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ring Gold Count",
  "version": "1.0",
  "description": "I created this extension to find out last gold count for Tronbet Ring Game",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
