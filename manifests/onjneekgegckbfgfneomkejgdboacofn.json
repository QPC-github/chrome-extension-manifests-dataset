{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Eraser!",
  "description": "The Eraser! is a free extension that clears your selected Internet data from the beginning of time  with a simple click!",
  "version": "1.0",
  "author": "Arun Shridas",
  "permissions": [
    "cookies",
    "history",
    "activeTab",
    "browsingData"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "16.png",
    "default_popup": "popup.html"
  }
}
