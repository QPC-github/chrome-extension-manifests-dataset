{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ratings & Reviews",
  "short_name": "Ratings & Reviews",
  "description": "Rate & review any resource on the internet to share your experiences, thoughts and opinions.",
  "version": "1.4",
  "permissions": [
    "http://*.novanebula.net/",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
