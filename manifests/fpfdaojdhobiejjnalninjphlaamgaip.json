{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "newsmatch.me",
  "description": "Newsmatch uses the articles you read to match you with like-minded singles.",
  "version": "0.1",
  "browser_action": {
    "default_icon": "nm.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://newsmatch.me/",
    "webRequest",
    "webRequestBlocking"
  ]
}
