{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MSession",
  "description": "Save Session on Mobile",
  "version": "0.21",
  "browser_action": {
    "default_icon": "msession.png",
    "default_popup": "msession.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://ajax.googleapis.com/",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' https://*.googleapis.com; object-src 'self'"
}
