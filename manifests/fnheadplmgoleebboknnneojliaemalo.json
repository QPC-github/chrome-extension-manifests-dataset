{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "7.6.0",
  "default_locale": "fr",
  "name": "La Secte Live Notifier",
  "description": "Extension pour suivre les lives de la Secte.",
  "background": {
    "persistent": true,
    "scripts": [
      "src/utils.js",
      "src/settings.js",
      "src/button.js",
      "src/notification.js",
      "src/streamer.js",
      "src/background.js",
      "src/jingle.js",
      "src/theme.js"
    ]
  },
  "author": "Nucktrooper",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "notifications"
  ],
  "optional_permissions": [
    "background"
  ],
  "options_ui": {
    "open_in_tab": true,
    "page": "pages/options/options.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "design/btn_OFF.png"
    },
    "default_popup": "pages/popup/popup.html"
  },
  "applications": {
    "gecko": {
      "update_url": "https://eln-host.herokuapp.com/updates.json"
    }
  }
}
