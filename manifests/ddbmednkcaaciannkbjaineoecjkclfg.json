{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Word to PDF Converter Pro",
  "default_locale": "en",
  "description": "Get your Word files converted to PDF.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.1",
  "icons": {
    "16": "images/pro-icon-16.png",
    "48": "images/pro-icon-48.png",
    "128": "images/pro-icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/pro-icon-128.png"
  }
}
