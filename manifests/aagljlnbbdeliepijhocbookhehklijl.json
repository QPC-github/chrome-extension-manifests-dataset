{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "META SEO Tools",
  "version": "0.1.1.0",
  "minimum_chrome_version": "70",
  "manifest_version": 2,
  "description": "Tools created to help SEO with Shopify",
  "icons": {
    "16": "images/meta-16.png",
    "32": "images/meta-32.png",
    "48": "images/meta-48.png",
    "128": "images/meta-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "/images/meta-48.png",
    "default_popup": "popup.html",
    "default_title": "META SEO Tools Options"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "run_at": "document_end",
      "js": [
        "jquery.js",
        "ip.js"
      ],
      "css": [
        "css/ip.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
