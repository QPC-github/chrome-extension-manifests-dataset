{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Irus Desktop Share Extension",
  "description": "A simple extension that allows you to stream your desktop to Iris.",
  "version": "1.0.7",
  "minimum_chrome_version": "56",
  "icons": {
    "16": "iris_icon_16x16.png",
    "48": "iris_icon_48x48.png",
    "128": "iris_icon_128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://meet.xrtc.cloud/*",
      "*://tpr-jssdk-as-d-001.rtc.sys.comcast.net/*",
      "*://tpr-jssdk-as-d-002.rtc.sys.comcast.net/*",
      "*://tpr-jssdk-as-d-003.rtc.sys.comcast.net/*",
      "*://tpr-jssdk-ho-c-001.rtc.sys.comcast.net/*",
      "*://tpr-jssdk-ho-c-002.rtc.sys.comcast.net/*",
      "*://st-jssdk-asd-002.poc.sys.comcast.net/*",
      "*://st-jssdk-asd-001.poc.sys.comcast.net/*",
      "*://st-jssdk-ch2g-001.poc.sys.comcast.net/*",
      "*://st-jssdk-ch2h-001.poc.sys.comcast.net/*"
    ]
  }
}
