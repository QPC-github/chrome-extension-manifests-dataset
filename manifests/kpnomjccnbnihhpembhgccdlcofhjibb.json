{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extension Live Skyyart",
  "version": "3.1",
  "description": "Extension de Skyyart pour ses lives Twitch",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "img/logo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "notifications"
  ]
}
