{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Add Mercer Intl. MS tags on Amazon",
  "description": "Automatically adds Asa Mercer International Middle School PTSA affiliate tags when you visit Amazon",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "*://*/*",
    "webRequestBlocking"
  ]
}
