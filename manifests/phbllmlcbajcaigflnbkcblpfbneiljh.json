{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bojo Roomba Dog",
  "version": "1.4",
  "description": "Sick of seeing Boris Johnson on your internet? Replace him with a dog on a Roomba with a single click!",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "browser_action": {
    "name": "Click to get URL"
  },
  "icons": {
    "16": "images/roomba-dog-16.png",
    "32": "images/roomba-dog-32.png",
    "48": "images/roomba-dog-48.png",
    "128": "images/roomba-dog-128.png"
  },
  "background": {
    "scripts": [
      "scripts.js"
    ],
    "persistent": false
  }
}
