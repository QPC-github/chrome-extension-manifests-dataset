{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wyrmspel",
  "description": "This extension will Redirect you to https://wyrmspel.com/",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
