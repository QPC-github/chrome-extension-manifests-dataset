{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Medium Unhooked",
  "version": "0.9",
  "description": "Hides the news feed on Medium.com",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://medium.com/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "manifest_version": 2
}
