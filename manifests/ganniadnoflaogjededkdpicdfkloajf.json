{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SimpleLastTab",
  "version": "0.94",
  "manifest_version": 2,
  "description": "Creates a New Tab when you are at your last tab to prevent the window from closing.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "tabs"
  ]
}
