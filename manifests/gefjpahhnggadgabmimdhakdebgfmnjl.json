{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sarah's Scribbles",
  "version": "1.0.0",
  "description": "Display's Sarah's Scribbles",
  "author": "Ivan Chub",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "http://sarahcandersen.com/*",
    "http://*.media.tumblr.com/*",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
