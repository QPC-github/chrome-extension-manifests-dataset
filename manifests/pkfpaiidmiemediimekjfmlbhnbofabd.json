{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "3 Tabs Only",
  "description": "Allows a maximum of 3 open tabs",
  "version": "7.10005",
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
