{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Account Blurrer for AWS",
  "version": "1.0.1",
  "description": "Hides your account information on AWS Management Console.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://console.aws.amazon.com/*",
        "https://*.console.aws.amazon.com/*"
      ],
      "css": [
        "hide.css"
      ]
    }
  ],
  "permissions": [],
  "manifest_version": 2
}
