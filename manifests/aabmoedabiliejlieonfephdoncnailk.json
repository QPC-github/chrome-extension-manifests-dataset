{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GGBlocker",
  "description": "Redirects GamerGate boycott websites to the archive.is versions.",
  "version": "1.3.4",
  "background": {
    "scripts": [
      "filter.js",
      "sync.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "<all_urls>",
    "tabs",
    "storage"
  ]
}
