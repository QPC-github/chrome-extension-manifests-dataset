{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "picker",
  "description": "f2e toolkit",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "img/icon32.png",
    "default_popup": "main.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon64.png",
    "128": "img/icon128.png"
  }
}
