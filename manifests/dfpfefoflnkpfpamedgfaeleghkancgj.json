{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gmail HTML signature",
  "description": "Set your Gmail signature from HTML code",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_title": "Gmail HTML signature",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
