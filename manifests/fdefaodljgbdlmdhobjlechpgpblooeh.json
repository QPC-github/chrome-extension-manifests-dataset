{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "manifest_version": 2,
  "name": "System Memory Usage",
  "short_name": "system-memory-usage",
  "permissions": [
    "storage",
    "system.memory"
  ],
  "background": {
    "scripts": [
      "lib/background.js"
    ]
  },
  "homepage_url": "http://mybrowseraddon.com/system-memory-usage.html",
  "description": "Monitor system's memory via toolbar popup or toolbar button's icon.",
  "browser_action": {
    "default_title": "System Memory Usage",
    "default_popup": "data/popup/popup.html",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
