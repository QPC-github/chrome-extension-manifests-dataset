{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "creativmarket-128.png",
    "default_popup": "index.html",
    "default_title": "CreativeMarket Search"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Search fonts, graphics, phtotos on CreativeMarket.com and open the results in a new tab.",
  "icons": {
    "128": "creativmarket-128.png",
    "64": "creativmarket-64.png",
    "48": "creativmarket-48.png",
    "32": "creativmarket-32.png",
    "16": "creativmarket-16.png"
  },
  "manifest_version": 2,
  "name": "CreativeMarket Search",
  "permissions": [
    "contextMenus",
    "tabs",
    "https://creativemarket.com/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.2"
}
