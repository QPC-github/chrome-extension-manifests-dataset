{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Baseliner",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Overlay grids on your webpage to finetune the content to a baseline grid.",
  "homepage_url": "httpS://projectfive.nl",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Baseliner",
    "default_popup": "baseliner.html"
  }
}
