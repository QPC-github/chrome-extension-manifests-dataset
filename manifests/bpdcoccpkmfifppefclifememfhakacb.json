{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jotform Notifier",
  "version": "0.3.4",
  "description": "Get JotForm form submission notifications on your browser instantly.",
  "permissions": [
    "alarms",
    "cookies",
    "notifications",
    "http://*.jotform.com/*",
    "https://*.jotform.com/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "Jotform Notifier",
    "default_icon": "Jotform-favicon16x16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "142": "chromeStore142x92.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
