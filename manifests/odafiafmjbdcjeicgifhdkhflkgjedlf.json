{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Gizmología",
  "version": "0.7.9",
  "manifest_version": 2,
  "description": "Actualidad sobre tecnología, electrónica, gadgets, dispositivos portátiles, telefonía móvil, Hi-Fi.",
  "icons": {
    "16": "images/icon.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "homepage_url": "http://alt1040.com/",
  "background": {
    "scripts": [
      "scripts/jquery-1.8.1.min.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "http://gizmologia.com/feed-chrome-extension",
    "tabs",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Gizmología en Chrome",
    "default_popup": "popup.html"
  }
}
