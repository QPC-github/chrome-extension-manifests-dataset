{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Type Beautify",
  "description": "Rendering fonts as beautifully as possible for Windows.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "version": "1.0",
  "content_scripts": [
    {
      "css": [
        "css/default.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}
