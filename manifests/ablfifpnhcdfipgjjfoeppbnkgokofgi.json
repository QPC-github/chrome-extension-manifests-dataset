{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AutoIndexBeautifier",
  "description": "Re-skins and adds functionality to Apache's directory listing page.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "version": "1.4",
  "author": "Dan Crossley",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "js/script.js"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/override.css",
    "icons/*"
  ],
  "offline_enabled": true,
  "incognito": "split"
}
