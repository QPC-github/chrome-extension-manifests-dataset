{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Go Direct",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Allows you go directly to the best search bypassing the search engine.",
  "icons": {
    "16": "icons/direct-16.png",
    "48": "icons/direct-48.png",
    "96": "icons/direct-96.png",
    "128": "icons/direct-128.png"
  },
  "omnibox": {
    "keyword": "go"
  },
  "background": {
    "scripts": [
      "direct.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ]
}
