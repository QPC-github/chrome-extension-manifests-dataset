{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StarshipGoogle",
  "description": "Would you like to know more?",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "jquery.js",
        "topics.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "federation.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*/*",
    "tabs",
    "notifications"
  ]
}
