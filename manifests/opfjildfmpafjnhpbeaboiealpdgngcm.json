{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "name": "ZippList",
  "description": "ZippList - The ultimate freebie list for creatives",
  "version": "0.0.1",
  "browser_action": {
    "default_title": "Zipplist"
  }
}
