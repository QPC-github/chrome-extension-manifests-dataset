{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Paste Zone",
  "manifest_version": 2,
  "version": "1.2.4",
  "icons": {
    "48": "48x48.png",
    "24": "24x24.png",
    "128": "128x128.png",
    "64": "64x64.png"
  },
  "permissions": [
    "tabs"
  ],
  "description": "Popup where you can middle-click to paste URLs without having to clear the omnibar first. (Linux only.)",
  "browser_action": {
    "default_icon": "24x24.png",
    "default_popup": "popup.html"
  }
}
