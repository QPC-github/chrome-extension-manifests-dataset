{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "http://*.sporza.be/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "style.css"
  ],
  "manifest_version": 2,
  "description": "Gives the sporza.be a little design boost",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "name": "Nicer Sporza",
  "permissions": [
    "http://*.sporza.be/"
  ],
  "version": "0.4"
}
