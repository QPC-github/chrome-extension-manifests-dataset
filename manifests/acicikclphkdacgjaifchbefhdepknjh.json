{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UrbanBuz-V3/Lightspeed",
  "description": "UrbanBuz v3 Extension For Lightspeed",
  "version": "1.0.0.0",
  "icons": {
    "16": "gfx/icon_16.png",
    "48": "gfx/icon_48.png",
    "128": "gfx/icon_128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "persistent": true,
    "scripts": [
      "lib/jquery-3.2.1.min.js",
      "lib/sha.js",
      "lib/urbanbuz.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://us.merchantos.com/*"
      ],
      "js": [
        "lib/jquery-3.2.1.min.js",
        "cs_common.js",
        "cs_checkout.js"
      ]
    }
  ],
  "page_action": {
    "default_title": "UrbanBuz",
    "default_icon": {
      "16": "gfx/icon_16.png",
      "48": "gfx/icon_48.png",
      "128": "gfx/icon_128.png"
    }
  },
  "permissions": [
    "*://*.urbanbuz.com/*",
    "tabs",
    "storage",
    "notifications"
  ],
  "externally_connectable": {
    "matches": [
      "*://us.merchantos.com/*"
    ]
  },
  "web_accessible_resources": [
    "ls_common.js",
    "ls_checkout.js"
  ]
}
