{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "ru",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "permissions": [
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "*://*.vk.com/*",
    "*://*.mamba.ru/*",
    "*://wamba.com/*",
    "*://*.love.rambler.ru/*",
    "*://*.loveplanet.ru/*",
    "*://fotostrana.ru/*",
    "*://*.facebook.com/*",
    "*://*.badoo.com/*",
    "*://*.mylove.ru/*",
    "*://*.ok.ru/*",
    "*://tabor.ru/*",
    "*://*.mail.ru/*",
    "*://*.yandex.ru/*",
    "*://*.yandex.by/*",
    "*://*.yandex.ua/*",
    "*://*.yandex.kz/*",
    "*://topface.com/*",
    "*://mail.google.com/*",
    "*://*.onliner.by/*",
    "*://*.rambler.ru/*",
    "*://*.twitter.com/*",
    "*://*.badanga.ru/*",
    "*://*.badanga.org/*",
    "*://*.badangadating.com/*"
  ],
  "version": "2.11",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "__MSG_name__",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.2.1.min.js",
        "glob.js"
      ],
      "matches": [
        "*://*.mamba.ru/*",
        "*://wamba.com/*",
        "*://*.love.mail.ru/*",
        "*://*.love.rambler.ru/*",
        "*://*.loveplanet.ru/*",
        "*://fotostrana.ru/*",
        "*://*.facebook.com/*",
        "*://vk.com/*",
        "*://*.badoo.com/*",
        "*://*.mylove.ru/*",
        "*://*.ok.ru/*",
        "*://tabor.ru/*",
        "*://mail.ru/*",
        "*://*.mail.ru/*",
        "*://*.mylove.ru/*",
        "*://*.yandex.ru/*",
        "*://*.yandex.by/*",
        "*://*.yandex.ua/*",
        "*://*.yandex.kz/*",
        "*://topface.com/*",
        "*://mail.google.com/*",
        "*://*.onliner.by/*",
        "*://*.rambler.ru/*",
        "*://*.twitter.com/*",
        "*://*.badanga.ru/*",
        "*://*.badanga.org/*",
        "*://*.badangadating.com/*",
        "*://*.aliexpress.ru/*",
        "*://*.aliexpress.com/*"
      ]
    },
    {
      "js": [
        "site-mamba.ru.js"
      ],
      "matches": [
        "*://*.mamba.ru/*",
        "*://*.wamba.com/*",
        "*://*.love.mail.ru/*"
      ]
    },
    {
      "js": [
        "site-loveplanet.ru.js"
      ],
      "matches": [
        "*://*.loveplanet.ru/*",
        "*://*.love.rambler.ru/*"
      ]
    },
    {
      "js": [
        "site-vk.com.js"
      ],
      "matches": [
        "*://vk.com/*"
      ]
    },
    {
      "js": [
        "site-badoo.com.js"
      ],
      "matches": [
        "*://*.badoo.com/*"
      ]
    },
    {
      "js": [
        "site-yandex.ru.js"
      ],
      "matches": [
        "*://*.yandex.ru/*",
        "*://*.yandex.by/*",
        "*://*.yandex.ua/*",
        "*://*.yandex.kz/*"
      ]
    },
    {
      "js": [
        "site-ok.ru.js"
      ],
      "matches": [
        "*://*.ok.ru/*"
      ]
    },
    {
      "js": [
        "site-badanga.ru.js"
      ],
      "matches": [
        "*://*.badanga.ru/*",
        "*://*.badanga.org/*",
        "*://*.badangadating.com/*"
      ]
    },
    {
      "js": [
        "site-STANDART.js"
      ],
      "matches": [
        "*://mail.google.com/*",
        "*://*.onliner.by/*",
        "*://*.tabor.ru/*",
        "*://fotostrana.ru/*",
        "*://*.mail.ru/*",
        "*://*.rambler.ru/*",
        "*://*.mylove.ru/*",
        "*://*.twitter.com/*",
        "*://*.aliexpress.ru/*",
        "*://*.aliexpress.com/*",
        "*://topface.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "web_accessible_resources": [
    "glob.js",
    "icon48.png"
  ]
}
