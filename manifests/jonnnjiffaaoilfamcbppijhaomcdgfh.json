{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NanoNotion Quick Notion",
  "description": "Post details of the current page to NanoNotion",
  "version": "0.2",
  "author": "Intralearn Software Corporation",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "/popup/popup.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
