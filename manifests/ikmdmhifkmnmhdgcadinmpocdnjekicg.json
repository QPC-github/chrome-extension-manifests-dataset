{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "4.2.9",
  "name": "Office@Hand for Google Chrome",
  "description": "Office@Hand for Google Chrome",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "background",
    "notifications",
    "http://*/",
    "https://*/",
    "storage",
    "identity"
  ],
  "browser_action": {},
  "icons": {
    "16": "att16.png",
    "32": "att32.png",
    "48": "att48.png",
    "128": "att128.png"
  },
  "web_accessible_resources": [
    "redirect.html",
    "client.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content.js"
      ],
      "all_frames": true
    }
  ],
  "content_security_policy": "script-src 'self' https://cdn.segment.com https://cdn.mxpnl.com; object-src 'self'",
  "options_ui": {
    "page": "options.html"
  },
  "oauth2": {
    "client_id": "26985333694-clrp15aceme9ik0aras3bp5a2ehoq66n.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/contacts.readonly",
      "https://www.googleapis.com/auth/admin.directory.user.readonly"
    ]
  }
}
