{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "4.0.6",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://mc.yandex.ru; object-src 'self'",
  "permissions": [
    "history",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/*",
    "topSites"
  ],
  "chrome_settings_overrides": {
    "homepage": "http://www.chedot.com/",
    "startup_pages": [
      "http://www.chedot.com/"
    ]
  }
}
