{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wordbank@gDrive",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "An extension to add new words from the omnibar to wordbank on google drive.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ]
  },
  "omnibox": {
    "keyword": "define"
  },
  "options_page": "src/options/options.html",
  "permissions": [
    "tabs",
    "identity",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.etymonline.com/*"
      ],
      "js": [
        "src/injected/word-origin.js"
      ]
    }
  ],
  "oauth2": {
    "client_id": "319187458275-dtebijonueujc4dt4uki8fut1nbkf4bn.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "key": ""
}
