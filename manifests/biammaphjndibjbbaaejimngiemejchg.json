{
  "version": "1.8.4",
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "short_name": "__MSG_extensionName__",
  "offline_enabled": true,
  "default_locale": "en",
  "icons": {
    "16": "assets/media/icons/16.png",
    "32": "assets/media/icons/32.png",
    "64": "assets/media/icons/64.png",
    "128": "assets/media/icons/128.png"
  },
  "page_action": {
    "default_icon": "assets/media/icons/32.png",
    "default_popup": "popup.html",
    "default_title": "Pugs Wallpapers HD/4K Pugs Themes"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "assets/js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://ajax.googleapis.com https://suggestqueries.google.com https://apis.google.com/ chrome-extension://cgdjnnhnfdmaeojbpkkffpmcfpagkifm/assets/js/ 'unsafe-eval' ; object-src 'self'",
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "http://search.superaddon.com",
      "image_url": "http://search.superaddon.com",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "Search:",
      "name": "SuperAddon:",
      "search_url": "http://search.superaddon.com?q={searchTerms}",
      "search_url_post_params": ""
    }
  },
  "permissions": [
    "chrome://favicon/",
    "cookies",
    "geolocation",
    "declarativeContent",
    "webNavigation",
    "topSites",
    "management",
    "bookmarks",
    "identity",
    "notifications",
    "webRequestBlocking",
    "webRequest",
    "storage",
    "tabs",
    "alarms",
    "unlimitedStorage",
    "topSites",
    "history",
    "https://news.google.com/*",
    "background"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ]
}
