{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wadoku-Kontextmenü",
  "description": "Fügt dem Kontextmenü einen Eintrag zum einfachen Suchen in Wadoku hinzu.",
  "version": "0.1.1",
  "default_locale": "de",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "context.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "context.js"
    ]
  }
}
