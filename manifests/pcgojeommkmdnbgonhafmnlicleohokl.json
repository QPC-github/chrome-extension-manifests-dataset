{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IEEE DTU",
  "version": "0.1.6",
  "manifest_version": 2,
  "description": "Official Updates from IEEE DTU Student Branch",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/ieee_logo-19.png",
    "48": "images/ieee_logo-48.png",
    "128": "images/ieee_logo-128.png"
  },
  "browser_action": {
    "default_icon": "images/ieee_logo-19.png",
    "default_title": "IEEE DTU",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://spreadsheets.google.com; object-src 'self'"
}
