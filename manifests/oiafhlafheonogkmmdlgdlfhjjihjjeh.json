{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Make It Editable",
  "version": "1.0.1",
  "description": "Make any web pages editable.",
  "permissions": [
    "*://*/*",
    "background"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    }
  },
  "manifest_version": 2
}
