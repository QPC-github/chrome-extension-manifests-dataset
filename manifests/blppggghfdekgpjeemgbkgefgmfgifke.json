{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/portraitflip.png"
  },
  "manifest_version": 2,
  "version": "1.0",
  "name": "PortraitFlip",
  "description": "We turn your favorite and most beloved images and photographs into original hand painted portraits.",
  "icons": {
    "128": "icons/portraitflip.png",
    "16": "icons/portraitflip.png",
    "48": "icons/portraitflip.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
