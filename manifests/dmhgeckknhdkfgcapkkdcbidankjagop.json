{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RL Scam Blocker",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Extension created to block Rocket League scam / phishing links in real time.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "RL Scam Blocker"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
