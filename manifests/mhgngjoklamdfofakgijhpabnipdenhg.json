{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extension Kidadou Live",
  "version": "1.7",
  "description": "Pour savoir quand Kidadou est en live",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "webNavigation"
  ],
  "icons": {
    "16": "ExtensionOnline64px.png",
    "48": "ExtensionOnline64px.png",
    "128": "ExtensionOnline64px.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  }
}
