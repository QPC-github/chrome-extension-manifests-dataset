{
  "page_action": {
    "default_icon": "bugon.png",
    "default_title": "EE Bugster"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "EE Bugster adds bug tracker links to bug numbers in the ExpressionEngine change log.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "name": "EE Bugster",
  "permissions": [
    "tabs",
    "http://ellislab.com/*"
  ],
  "icons": {
    "48": "bug-48.png",
    "128": "bug-128.png",
    "16": "bug-16.png"
  },
  "manifest_version": 2,
  "version": "1.0.8"
}
