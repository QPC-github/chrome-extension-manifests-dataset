{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Summa Crossroads Affiliates",
  "version": "1.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "amz.js",
        "humble.js"
      ],
      "matches": [
        "*://*.amazon.com/*",
        "*://*.amazon.ca/*",
        "*://*.humblebundle.com/*"
      ]
    }
  ],
  "description": "Replaces any non-affiliated Amazon or Humble link with one that has the Summa Crossroads affiliation.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon_128.png"
  }
}
