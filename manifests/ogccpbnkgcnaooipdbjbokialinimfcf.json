{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chartify!",
  "short_name": "Chartify",
  "description": "This extension attempts to convert current tab's json output to a chart",
  "version": "0.0.2",
  "author": "KBroloes",
  "browser_action": {
    "default_icon": "Chartify.png",
    "default_popup": "chartify.html",
    "default_title": "Click here to Chartify!"
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/",
    "file://*.json"
  ]
}
