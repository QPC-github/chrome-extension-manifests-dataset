{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "cookies.txt One Click",
  "version": "3.0",
  "description": "Based off cookies.txt at https://chrome.google.com/webstore/detail/njabckikapfpffapmjgojcnbfjonfjfg",
  "short_name": "cookiesOC",
  "offline_enabled": true,
  "permissions": [
    "activeTab",
    "downloads",
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Export cookies.txt",
    "default_icon": "icon.png"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_ui": {
    "page": "options.html"
  }
}
