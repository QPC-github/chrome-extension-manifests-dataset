{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Blazaar, Private Language Classes Online",
  "author": "wwww.phenomena.rocks",
  "version": "1.6",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Blazaar, Private Language Classes Online. screen share extension.",
  "homepage_url": "https://www.blazaar.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://blazaar.com/*",
      "*://*.blazaar.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
