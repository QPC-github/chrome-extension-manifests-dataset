{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Node Alice",
  "description": "Get a visualization of the call stack during the lifetime of the last request.",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*/*",
    "https://*/",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon-tag.png",
    "default_title": "Alice Analyser",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon-tag.png",
    "48": "icon-tag-48.png",
    "128": "icon-tag-128.png"
  },
  "options_page": "options.html"
}
