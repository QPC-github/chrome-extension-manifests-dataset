{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19x19.png",
      "38": "img/icon_38x38.png"
    },
    "default_title": "export cookie"
  },
  "description": "Copy TVU Cookies to Clipboard.",
  "icons": {
    "128": "img/icon_128x128.png",
    "16": "img/icon_16x16.png",
    "48": "img/icon_48x48.png"
  },
  "manifest_version": 2,
  "name": "Cookies Extract",
  "permissions": [
    "http://tvunderground.org.ru/*",
    "cookies",
    "clipboardWrite"
  ],
  "short_name": "cookie extract",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1"
}
