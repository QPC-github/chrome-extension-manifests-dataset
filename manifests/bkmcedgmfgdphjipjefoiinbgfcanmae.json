{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free Music",
  "version": "1.0",
  "description": "Play your music quickly from Google Drive",
  "icons": {
    "512": "icon-512.png",
    "256": "icon-256.png",
    "128": "icon-128.png",
    "96": "icon-96.png",
    "64": "icon-64.png",
    "48": "icon-48.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "container": [
    "DOMAIN_INSTALLABLE",
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "638031026065",
  "app": {
    "launch": {
      "web_url": "https://free-music.freebusinessapps.net/music"
    }
  },
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "audio/mpeg3",
          "audio/x-mpeg-3",
          "audio/mp3",
          "audio/mpeg",
          "audio/mp4",
          "audio/mpg",
          "audio/mp4a-latm",
          "audio/ogg",
          "application/ogg",
          "audio/webm",
          "audio/wav",
          "audio/x-wav",
          "audio/wave",
          "audio/x-flv",
          "audio/x-flac",
          "video/x-mpeg",
          "video/webm",
          "video/x-flv",
          "application/vnd.google.drive.ext-type.mpeg3",
          "application/vnd.google.drive.ext-type.mp3",
          "application/vnd.google.drive.ext-type.mpeg",
          "application/vnd.google.drive.ext-type.mp4",
          "application/vnd.google.drive.ext-type.mpg",
          "application/vnd.google.drive.ext-type.ogg",
          "application/vnd.google.drive.ext-type.wav",
          "application/vnd.google.drive.ext-type.wave",
          "application/vnd.google.drive.ext-type.webm"
        ],
        "href": "https://free-music.freebusinessapps.net/music",
        "title": "Open",
        "disposition": "window"
      }
    ]
  }
}
