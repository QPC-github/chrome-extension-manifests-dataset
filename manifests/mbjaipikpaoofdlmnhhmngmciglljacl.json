{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Real Email Signature Plugin",
  "description": "Insert a handwritten signature on any email",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "sign.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
