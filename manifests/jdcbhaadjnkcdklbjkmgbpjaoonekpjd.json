{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bookmarks Backuper",
  "description": "Never lose your Chrome bookmarks again!",
  "version": "0.2.0.6",
  "container": "GOOGLE_DRIVE",
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline' https://www.gstatic.com https://apis.google.com https://content.googleapis.com https://accounts.google.com; object-src 'self'",
  "permissions": [
    "identity",
    "bookmarks",
    "storage",
    "tabs"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Show Bookmarks Backup",
    "default_icon": "img/icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon128.png",
    "48": "img/icon128.png",
    "128": "img/icon128.png"
  },
  "oauth2": {
    "client_id": "788587416670-d3i89ech7a4pv33qas2stflslookq0el.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/drive.readonly",
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "manifest_version": 2
}
