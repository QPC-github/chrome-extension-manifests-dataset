{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Magento 2 Review Reminder Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Magento Reminder 2 Extension Review is a great solution for your shop to increase conversion rates and booster sales.",
  "homepage_url": "https://www.mageplaza.com/magento-2-one-step-checkout-extension/",
  "short_name": "Magento 2 Review Reminder Extension - Review Booster by Mageplaza",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "geolocation"
  ]
}
