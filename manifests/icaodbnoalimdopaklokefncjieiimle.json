{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Multipass",
  "version": "0.5.2",
  "description": "Multifactor authentication checker for LastPass.",
  "author": "Aru Sahni",
  "homepage_url": "https://arusahni.github.io/multipass",
  "developer": {
    "name": "Aru Sahni",
    "url": "https://arusahni.github.io/multipass"
  },
  "icons": {
    "48": "res/icon-48.png",
    "96": "res/icon-96.png",
    "128": "res/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.lastpass.com/newvault/vault.php*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "config.js",
        "tlds.js",
        "sites.js",
        "parseDomain.js",
        "multipass.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "res/info.svg",
    "res/logo.svg"
  ],
  "applications": {
    "gecko": {
      "id": "multipass@arusahni.net"
    }
  }
}
