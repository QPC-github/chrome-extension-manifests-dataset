{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Intandemly",
  "description": "Aligning Sales & Marketing",
  "short_name": "Intandemly",
  "version": "8.9",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "permissions": [
    "tabs",
    "https://www.linkedin.com/*",
    "https://www.crunchbase.com/*",
    "https://www.facebook.com/*",
    "https://angel.co/*",
    "https://www.owler.com/*",
    "https://www.glassdoor.co.in/*",
    "https://www.glassdoor.com/*",
    "https://v3.intandemly.com/*",
    "https://nischal.intandemly.com/*",
    "https://srikanth.intandemly.com/*",
    "https://dev.intandemly.com/*",
    "https://saran.intandemly.com/*",
    "https://mallesh.intandemly.com/*"
  ],
  "web_accessible_resources": [
    "images/button.png"
  ]
}
