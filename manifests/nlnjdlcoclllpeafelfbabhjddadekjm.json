{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Deal Steal Unreal",
  "short_name": "DSU extension",
  "description": "This extension is used to capture deals from popular websites.",
  "version": "1.17",
  "browser_action": {
    "default_icon": "icon_deal.png",
    "default_popup": "deal_ext_popup.html"
  },
  "permissions": [
    "http://*.dealstealunreal.com/",
    "tabs"
  ]
}
