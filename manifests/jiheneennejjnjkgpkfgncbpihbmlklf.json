{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CashManatee",
  "description": "",
  "version": "0.0.0.1",
  "icons": {
    "128": "icons/128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "web_accessible_resources": [
    "*"
