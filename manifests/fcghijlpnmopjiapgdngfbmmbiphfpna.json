{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Phrazy",
  "version": "0.51",
  "description": "関連の深いWEBサイトを抽出します",
  "options_page": "options.html",
  "icons": {
    "16": "icons/ic-19.png",
    "48": "icons/ic-48.png",
    "128": "icons/ic-128.png"
  },
  "browser_action": {
    "default_icon": "icons/ic-19.png",
    "default_title": "Phrazy",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://stg2or3.collarks.net/",
    "http://frasy.collarks.net/",
    "http://phrazy.herokuapp.com/"
  ]
}
