{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "stupidtube",
  "version": "1.0.0",
  "description": "Removes all of the video information that's on youtube",
  "manifest_version": 2,
  "icons": {
    "16": "stupid-16.png",
    "48": "stupid-48.png",
    "128": "stupid-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "all_frames": true,
      "js": [
        "stupidtube.user.js"
      ]
    }
  ]
}
