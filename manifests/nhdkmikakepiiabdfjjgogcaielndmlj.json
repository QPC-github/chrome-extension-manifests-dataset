{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Camera Effects & Filters",
  "description": "Have fun taking and sharing photos with over 20 different filters, effects and washes. Share to Facebook, Instagram & Twitter!",
  "version": "0.0.0.2",
  "offline_enabled": false,
  "app": {
    "urls": [
      "https://turncamon.com/"
    ],
    "launch": {
      "web_url": "https://turncamon.com/"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
