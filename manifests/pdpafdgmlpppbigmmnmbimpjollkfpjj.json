{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Горящие туры для каждого",
  "description": "Горящие путевки, туры, лучшие цены на отдых. ",
  "version": "1.0",
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
