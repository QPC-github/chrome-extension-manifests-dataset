{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "45",
  "manifest_version": 2,
  "description": "Skrajšaj si čas nakupovanja na spletu! Že na Googlu primerjaj ponudbe vodilnih spletnih trgovin.",
  "name": "1-2-3 Nakup s Ceneje.si",
  "short_name": "1-2-3 Nakup",
  "permissions": [],
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon-48x48.png",
    "128": "favicon-128x128.png"
  },
  "browser_action": {
    "default_icon": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "jquery.ba-throttle-debounce.min.js",
        "helpers.js",
        "getContent.js"
      ],
      "run_at": "document_end"
    }
  ]
}
