{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ted Cruz is the Zodiac Killer",
  "description": "Ted Cruz is the Zodiac Killer",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "content.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
