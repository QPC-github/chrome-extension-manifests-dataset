{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cashback Watch",
  "description": "Find and compare rates offered by a list of cashback sites for the store you are browsing.",
  "version": "0.0.1.4",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
