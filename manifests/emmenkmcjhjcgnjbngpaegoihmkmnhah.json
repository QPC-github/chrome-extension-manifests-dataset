{
  "manifest_version": 2,
  "name": "ESES中国至马来西亚集运代运专家 - 一键添加包裹功能 （淘宝阿里巴巴）",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "https://www.eses.net.cn/"
  ],
  "version": "0.5.0",
  "description": "让您可以轻松在淘宝，阿里巴巴，京东，购物下单，提供更加方便的添加包裹方式到ESES的集运仓库，并能够监控您的包裹是否有从卖家发货过来。省时省心省力",
  "homepage_url": "https://www.eses.net.cn",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "layer/theme/default/layer.css",
        "btn.css"
      ],
      "js": [
        "js/jquery.js",
        "js/jquery.base64.min.js",
        "js/jquery.base64cn.js",
        "js/jquery.cookie.js",
        "layer/layer.js",
        "alibaba.js",
        "tb.js",
        "creat.js",
        "init.js"
      ],
      "matches": [
        "*://*.taobao.com/*",
        "*://*.tmall.com/*",
        "*://*.tmall.hk/*",
        "*://order.jd.com/center/list.action*",
        "*://*.1688.com/*",
        "*://item.jd.com/*",
        "*://item.jd.hk/*"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/jquery.js",
      "background.js",
      "alibaba.js"
    ]
  }
}
