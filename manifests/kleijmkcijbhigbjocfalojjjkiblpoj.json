{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Weather.com Defearifier",
  "version": "0.3",
  "permissions": [
    "http://www.weather.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.weather.com/*"
      ],
      "js": [
        "jquery-1.11.1.js",
        "jquery-ui.js",
        "myscript.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}
