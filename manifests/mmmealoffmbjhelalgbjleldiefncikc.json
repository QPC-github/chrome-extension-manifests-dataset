{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social Streams Reader online",
  "short_name": "Social Reader",
  "description": "Streams Reader from Facebook, Twitter, Google+, Youtube, Pinterest and Flickr",
  "manifest_version": 2,
  "version": "1.3",
  "default_locale": "en",
  "icons": {
    "128": "128.PNG"
  },
  "app": {
    "urls": [
      "http://www.otoboo.com/"
    ],
    "launch": {
      "web_url": "http://www.otoboo.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
