{
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "forteam.css"
      ],
      "js": [
        "js/jquery.min.js",
        "js/forteam.js"
      ],
      "matches": [
        "*://*.forteam.ru/*",
        "*://*.forteam.ru/*"
      ]
    }
  ],
  "description": "Дополнительные возможности.",
  "icons": {
    "128": "images/icon.png",
    "48": "images/bigicon.jpg"
  },
  "manifest_version": 2,
  "name": "ForTeam.ru Additional features",
  "permissions": [
    "tabs",
    "webNavigation",
    "contextMenus",
    "unlimitedStorage",
    "notifications",
    "*://*.forteam.ru/*",
    "*://*.forteam.ru/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.8",
  "web_accessible_resources": [
    "functions.js"
  ]
}
