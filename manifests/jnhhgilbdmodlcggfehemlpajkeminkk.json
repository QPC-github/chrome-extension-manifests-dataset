{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "icons/undo.png"
  ],
  "browser_action": {
    "theme_icons": [
      {
        "dark": "icons/frog-32.png",
        "light": "icons/frog-32.png",
        "size": 32
      }
    ],
    "default_popup": "popup/popup.html",
    "default_title": "Add to KinderMate",
    "default_icon": {
      "32": "icons/frog-32.png",
      "48": "icons/frog-48.png"
    },
    "browser_style": false
  },
  "description": "When you find YouTube videos to share with your kids, put it in KinderMate",
  "icons": {
    "128": "icons/frog-128.png",
    "96": "icons/frog-96.png",
    "48": "icons/frog-48.png",
    "16": "icons/frog-16.png"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ]
  },
  "homepage_url": "https://www.kindermateai.com",
  "version": "0.1.3.2",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "webRequest",
    "activeTab",
    "tabs",
    "storage",
    "cookies"
  ],
  "name": "Add to KinderMate"
}
