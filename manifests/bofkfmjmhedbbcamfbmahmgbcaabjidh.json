{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "uRead.Me",
  "version": "2.17",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "ureadme_16x16.png",
    "48": "ureadme_48x48.png",
    "128": "ureadme_128x128.png"
  },
  "browser_action": {
    "default_icon": "ureadme_48x48.png",
    "default_title": "uRead.Me"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ]
}
