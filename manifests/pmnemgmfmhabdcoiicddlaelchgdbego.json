{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HelpDocs - Knowledge Base Software",
  "short_name": "HelpDocs",
  "version": "2.1.4",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Fly through tickets by searching, copying, creating, and viewing articles from your HelpDocs account in any tab.",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "HelpDocs Knowledge Base",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-WmpagGabBqM/N9wOcwh/qGnt4fUAvXN/tEPJZQ/bpsg='; object-src 'self'",
  "permissions": [
    "cookies",
    "*://*.helpdocs.io/",
    "clipboardWrite",
    "management"
  ]
}
