{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "manifest_version": 2,
  "name": "AppWax",
  "version": "0.1.1",
  "description": "Create icons, splash screens, and promo images for all the app stores instantly.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://appwax.us/",
    "https://appwax.us/",
    "https://www.paypalobjects.com/",
    "https://www.paypal.com/",
    "storage"
  ]
}
