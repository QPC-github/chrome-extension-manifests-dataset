{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Find My Music",
  "description": "This extension allows the user to find a particular song title from different music websites.",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "extension.html",
    "default_title": "Find My Music!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
