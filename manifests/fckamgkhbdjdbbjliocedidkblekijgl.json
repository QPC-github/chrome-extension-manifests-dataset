{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popuplist.html",
    "permissions": [
      "tabs"
    ],
    "css": [
      "style.css"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "description": "Os melhores Links da Internet.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "LinkBait",
  "version": "1.5"
}
