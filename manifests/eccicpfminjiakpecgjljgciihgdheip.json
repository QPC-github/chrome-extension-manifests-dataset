{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Genesis",
  "description": "Lets you look at the design process for a website.",
  "version": "0.9",
  "author": "Mrinal Dhar",
  "short_name": "Genesis",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Genesis - Web design time travel"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
