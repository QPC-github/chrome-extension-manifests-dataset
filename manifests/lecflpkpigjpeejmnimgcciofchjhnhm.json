{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome Web Store Shortcut",
  "version": "1.0",
  "permissions": [
    "contextMenus"
  ],
  "description": "Shortcut for the Chrome Web Store.",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Web Store"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
