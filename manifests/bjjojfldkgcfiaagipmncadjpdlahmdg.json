{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Webzis",
  "description": "DISEÑO Y DESARROLLO DE PÁGINAS Y APLICACIONES WEB",
  "version": "1.5",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "DISEÑO Y DESARROLLO DE PÁGINAS Y APLICACIONES WEB",
    "default_icon": "icon32.png"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
