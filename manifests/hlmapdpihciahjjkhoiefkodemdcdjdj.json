{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Turing Gmail Extension",
  "description": "An extension that brings Turing Cloud features to Gmail.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "inboxsdk.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
