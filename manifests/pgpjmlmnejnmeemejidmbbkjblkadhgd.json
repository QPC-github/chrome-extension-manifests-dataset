{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unofficial Project for Awesome extension",
  "version": "1",
  "manifest_version": 2,
  "description": "This is a quick extension I wrote to check project for awesome status updates at any point",
  "permissions": [
    "http://www.projectforawesome.com/"
  ],
  "web_accessible_resources": [
    "popup.html",
    "popup.js"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "color_web.png",
    "default_title": "Project for Awesome",
    "default_popup": "popup.html"
  }
}
