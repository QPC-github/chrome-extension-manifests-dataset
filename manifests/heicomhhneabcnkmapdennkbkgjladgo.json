{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.3",
  "name": "Trace.it",
  "short_name": "Trace.it",
  "description": "Lightweight To-do list extension.",
  "author": "Vincent Racine",
  "background": "persistent",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "index.html"
  },
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "options.html"
  ],
  "offline_enabled": true
}
