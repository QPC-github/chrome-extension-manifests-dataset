{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SquareStack Single Sign-On Plug-in",
  "version": "2.30",
  "manifest_version": 2,
  "description": "Secure auto login to any app",
  "icons": {
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-1.9.1.js",
        "js/version.js",
        "js/extension.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-1.9.1.js",
        "js/sso-mo-login.js",
        "js/common.js",
        "js/gmail-configure-sf.js",
        "js/lib/js-mcrypt/mcrypt.js",
        "js/lib/js-mcrypt/rijndael.js",
        "js/lib/js-mcrypt/Serpent.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "js/jquery-1.9.1.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "short_name": "SSO Plug-in"
}
