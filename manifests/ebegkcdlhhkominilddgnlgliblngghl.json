{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": " qqmail shortcut",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "yslbackground.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/y.png"
  },
  "commands": {
    "Next": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "Next"
    },
    "Pre": {
      "suggested_key": {
        "default": "Ctrl+Left"
      },
      "description": "Pre"
    },
    "Delete": {
      "suggested_key": {
        "default": "Ctrl+Delete"
      },
      "description": "Delete"
    },
    "Delete2": {
      "suggested_key": {
        "default": "Ctrl+Period"
      },
      "description": "Delete"
    }
  },
  "permissions": [
    "tabs"
  ]
}
