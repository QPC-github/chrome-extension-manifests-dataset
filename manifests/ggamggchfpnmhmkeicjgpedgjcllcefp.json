{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "cardcounter.css"
      ],
      "js": [
        "jquery-1.7.1.min.js",
        "cardcounter.js"
      ],
      "matches": [
        "https://trello.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon-128.png",
    "32": "icon-32.png",
    "48": "icon-48.png"
  },
  "description": "Shows the number of cards in every list in Trello. Adds support for Kanban limits.",
  "name": "Original Card Counter for Trello",
  "version": "1.0.4",
  "manifest_version": 2
}
