{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Really Mute Inactive Tabs LT.",
  "description": "This extension mute inactive tabs",
  "version": "1.0",
  "browser_action": {
    "default_icon": "iconOn.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
