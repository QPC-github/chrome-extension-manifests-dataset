{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1",
  "name": "Flamingo",
  "description": "This is a super random extension that plays flamingo, feel free to install and review it.",
  "author": "Ruben Thomson",
