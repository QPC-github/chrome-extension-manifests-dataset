{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Xpeed Unleashed",
  "short_name": "Play Xpeed Unleashed game",
  "description": "Play the Xpeed Unleashed. Choose your pilot and prepare for a race of spaceships to WipeOut style.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "xpeed_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "xpeed_128.png",
    "48": "xpeed_128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
