{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ArtistAloud",
  "version": "1.1",
  "description": "Independent music: Non Film Songs India. Only distribution platform for exclusive music records of independent and upcoming bands",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "name": "ArtistAloud.com",
    "default_icon": "icons.png"
  }
}
