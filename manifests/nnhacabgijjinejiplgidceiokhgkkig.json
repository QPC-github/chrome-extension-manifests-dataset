{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lineup highlighter",
  "description": "Подсвечивает игроков",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.sports.ru/fantasy/football/team/transfers/*"
