{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "manifest_version": 2,
  "default_locale": "ja",
  "version": "0.0.2",
  "version_name": "FREE",
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "jquery-2.1.4.min.js",
      "Blowfish.js",
      "encoding.js",
      "master.js",
      "strage.js",
      "zangodb.min.js",
      "background.js"
    ]
  },
  "icons": {
    "38": "icon/icon38.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "jquery.cookie.js",
        "strage.js",
        "master.js",
        "main.js",
        "encoding.js",
        "clipboard.min.js",
        "Blowfish.js",
        "papaparse.min.js"
      ]
    }
  ]
}
