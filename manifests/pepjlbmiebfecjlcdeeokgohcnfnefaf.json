{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stackoverflow Hireme No",
  "version": "1.0.0",
  "description": "An extension to hide Stack Overflow Jobs on the sidebar",
  "content_scripts": [
    {
      "matches": [
        "https://stackoverflow.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
