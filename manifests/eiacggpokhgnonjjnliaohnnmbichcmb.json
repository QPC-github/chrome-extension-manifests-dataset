{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ModTest",
  "version": "0.1.4",
  "description": "Test of modifying html and css content.",
  "background": {
    "scripts": [
      "thirdParty/jquery.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "thirdParty/jquery.js",
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
