{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Floozifier",
  "short_name": "Floozifier",
  "description": "Flooz > Bitcoin...and beyond",
  "version": "19.90",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
