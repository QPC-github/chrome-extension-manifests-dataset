{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Create Place Holder",
  "description": "Create a placeholder image on the fly.",
  "version": "1.0.4",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "placeholder.png",
    "default_title": "Create Place Holder"
  },
  "manifest_version": 2
}
