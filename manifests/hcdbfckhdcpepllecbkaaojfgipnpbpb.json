{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Create list of followers/following of an Instagram account",
  "version": "3.0.57",
  "key": "",
  "manifest_version": 2,
  "name": "Export List of Followers from Instagram",
  "short_name": "Export List of Followers from Instagram",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "Helper Tools for Instagram.com",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "management",
    "storage",
    "identity",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "https://www.instagram.com/*",
    "https://i.instagram.com/*"
  ],
  "incognito": "split",
  "oauth2": {
    "client_id": "507799611147-hts6p4lpl59ahe1n3rkohldfhjpmj7j3.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email"
    ]
  }
}
