{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Calculator",
  "short_name": "Simple Calulator",
  "description": "Simple Calculator helps users to calculate simple calculation with great speed and accessibility",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "author": "Stav Green",
  "browser_action": {
    "default_locale": "en",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://api.flickr.com/",
    "https://flicker.com/"
  ],
  "sandbox": {
    "pages": [
      "index.html"
    ]
  }
}
