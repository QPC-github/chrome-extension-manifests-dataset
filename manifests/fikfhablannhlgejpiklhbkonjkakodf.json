{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Search in background tab",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Opens the \"Search google for\" search in a background tab.",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "utils.js",
      "background.js"
    ]
  },
  "options_page": "options.html"
}
