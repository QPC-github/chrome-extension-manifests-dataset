{
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html",
    "icons": [
      "icons/icon.png"
    ]
  },
  "description": "Search and access popular converter quick links instantly from your new tab page!",
  "icons": {
    "128": "icons/icon_128.png",
    "19": "icons/icon.png",
    "48": "icons/icon_48.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://myfirstwebnet.com/favicon.ico",
      "image_url": "https://myfirstwebnet.com/api/redirect-search?sid=6567&t={keyword}",
      "image_url_post_params": "",
      "instant_url": "https://myfirstwebnet.com/api/redirect-search?sid=6567&t={keyword}",
      "instant_url_post_params": "",
      "is_default": true,
      "name": "Converter Guru",
      "search_url": "https://myfirstwebnet.com/api/redirect-search?sid=6567&t={keyword}",
      "search_url_post_params": "",
      "suggest_url": "https://myfirstwebnet.com/api/redirect-search?sid=6567&t={keyword}",
      "suggest_url_post_params": ""
    },
    "startup_pages": [
      "https://myfirstwebnet.com/api/redirect-search?sid=6567&t={keyword}"
    ]
  },
  "manifest_version": 2,
  "name": "Converter Guru",
  "permissions": [
    "activeTab",
    "http://ui.appuniverse.online/*",
    "https://v2.convertapi.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.0"
}
