{
  "author": "Onutrem @onutrem",
  "background": {
    "scripts": [
      "libs/jquery-1.11.2.js",
      "parameters.js",
      "background.js",
      "chrome.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_off.png",
    "default_title": "Chap - Offline"
  },
