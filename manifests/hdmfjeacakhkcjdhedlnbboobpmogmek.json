{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Send To Device",
  "description": "This extension lets you send and receive links from and to devices",
  "version": "3.0.1",
  "options_page": "options.html",
  "permissions": [
    "background",
    "storage",
    "notifications",
    "contextMenus",
    "gcm"
  ],
  "background": {
    "scripts": [
      "min/jquery-1.11.0.min.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "16-16.png",
    "48": "48-48.png",
    "128": "128-128.png"
  }
}
