{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Medscape Medical Updates Reader",
  "version": "2.1",
  "description": "Latest medical news from Medscape. - RSS reader",
  "background": {
    "page": "FeedExtensionBackground.html"
  },
  "browser_action": {
    "default_title": "Medscape Medical Updates - RSS reader",
    "default_icon": "Med.png",
    "default_popup": "FeedExtensionPop.html"
  },
  "icons": {
    "16": "Med.png",
    "48": "Med.png",
    "128": "Med.png"
  },
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "http://*.medscape.com/*",
    "http://*.nicewebtools.com/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' ; object-src 'self';connect-src http://img.medscape.com http://www.medscape.com 'self'"
}
