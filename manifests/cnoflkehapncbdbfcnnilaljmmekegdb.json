{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Seo from Kenny",
  "version": "0.1",
  "description": "Seo теги",
  "icons": {
    "16": "favicon.ico",
    "32": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "ext.kn.content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Посмотреть данные",
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  }
}
