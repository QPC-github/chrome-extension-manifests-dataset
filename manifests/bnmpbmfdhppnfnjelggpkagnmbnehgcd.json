{
  "content_scripts": [
    {
      "js": [
        "jquery-1.10.2.min.js",
        "jquery.cookie.js",
        "background.js"
      ],
      "matches": [
        "http://dota2lounge.com/*",
        "https://dota2lounge.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Dota 2 Lounge Multiple Accounts",
  "permissions": [
    "tabs",
    "storage",
    "http://dota2lounge.com/*",
    "https://dota2lounge.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.4",
  "web_accessible_resources": [
    "jquery-1.10.2.min.map"
  ]
}
