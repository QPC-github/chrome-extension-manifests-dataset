{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.0",
  "name": "Ratz Notifier",
  "background": {
    "scripts": [
      "background.js",
      "extension.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Ratz Notifier"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "notifications"
  ]
}
