{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Delay",
  "description": "Delay certain urls from displaying",
  "author": "roy",
  "version": "0.1.20",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "page": "./background.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "tabs"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
