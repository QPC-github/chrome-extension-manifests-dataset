{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_title__",
  "description": "__MSG_description__",
  "version": "1.2",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://sb.aau.dk/*"
      ],
      "js": [
        "getavg.js"
      ]
    }
  ],
