{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "__MSG_shortName__",
  "description": "__MSG_appDesc__",
  "version": "1.2.14",
  "default_locale": "en",
  "icons": {
    "16": "img/48.png",
    "48": "img/96.png",
    "128": "img/128.png"
  },
  "browser_action": {
    "default_icon": "img/96.png",
    "default_title": "__MSG_shortName__",
    "default_popup": "splash.html"
  },
  "author": "https://amzscout.net",
  "homepage_url": "https://amzscout.net",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "key": "",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.amazon.cn/*",
        "*://www.amazon.nl/*",
        "*://www.amazon.ca/*",
        "*://www.amazon.co.uk/*",
        "*://www.amazon.com/*",
        "*://www.amazon.com.mx/*",
        "*://www.amazon.de/*",
        "*://www.amazon.it/*",
        "*://www.amazon.es/*",
        "*://www.amazon.fr/*",
        "*://www.amazon.in/*",
        "*://www.amazon.com.au/*"
      ],
      "css": [
        "styles.bundle.css",
        "icons.css"
      ],
      "js": [
        "inline.bundle.js",
        "polyfills.bundle.js",
        "main.bundle.js"
      ]
    }
  ]
}
