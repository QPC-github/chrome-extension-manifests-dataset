{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "lib/jquery-2.1.1.min.js",
        "milko/milko.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Makes everything Milko",
  "icons": {
    "128": "milko/logo128.png",
    "16": "milko/logo16.png"
  },
  "manifest_version": 2,
  "name": "Everything Milko",
  "permissions": [
    "tabs",
    "webRequest",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequestBlocking"
  ],
  "short_name": "Milko",
  "version": "1.1"
}
