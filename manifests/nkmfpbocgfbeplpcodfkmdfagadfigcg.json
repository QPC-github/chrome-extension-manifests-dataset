{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Holbæk menu",
  "version": "1.0.16",
  "description": "Holbæk menu - Gør det let at navigere rundt.",
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo19.png"
  },
  "web_accessible_resources": [
    "logo19.png"
  ],
  "permissions": [
    "*://start.skolen.nu/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://sites.google.com/a/skolen.nu",
        "*://sites.google.com/a/skolen.nu/*",
        "*://web.skolekom.emu.dk/",
        "*://web.skolekom.emu.dk/*",
        "https://plus.google.com/up/accounts/upgrade",
        "https://plus.google.com/up/accounts/upgrade/*",
        "https://groups.google.com/*",
        "https://plus.google.com/u/*",
        "https://mail.google.com/*",
        "https://drive.google.com/*",
        "https://calendar.google.com/",
        "https://calendar.google.com/*",
        "https://docs.google.com/*",
        "https://maps.google.dk/",
        "https://maps.google.dk/*",
        "*://translate.google.com/",
        "*://translate.google.com/*",
        "*://www.ordbogen.com/",
        "*://www.ordbogen.com/*",
        "*://www.modultest.dk/",
        "*://www.modultest.dk/*",
        "*://www.dr.dk/nyheder/ligetil",
        "*://www.dr.dk/nyheder/ligetil/*",
        "*://www.google.dk/",
        "*://www.google.dk/*",
        "*://www.nyidanmark.dk/",
        "*://www.nyidanmark.dk/*",
        "*://sproget.dk/",
        "*://sproget.dk/*",
        "*://lexin.emu.dk/",
        "*://lexin.emu.dk/*",
        "*://www.lexin.emu.dk/",
        "*://www.lexin.emu.dk/*",
        "*://plus.google.com/",
        "*://plus.google.com/*",
        "*://www.youtube.com/",
        "*://www.youtube.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
