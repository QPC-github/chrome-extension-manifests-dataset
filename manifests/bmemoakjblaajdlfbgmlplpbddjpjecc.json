{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Control Panel Login",
  "description": "__MSG_extension_description__",
  "version": "1.0",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "page_action": {
    "default_title": "Control Panel Login"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "http://www.whitefirdesign.com/control-panel-login?pk_campaign=CPL-Chrome",
  "offline_enabled": true
}
