{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ISIS to Losers",
  "description": "Defeat extremism with Donald Trumpisms",
  "version": "1.0.1",
  "author": "Antony Bailey",
  "browser_action": {
    "default_icon": {
      "19": "images/enabled.png"
    },
    "default_title": "click to disable ISIS to Losers"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
