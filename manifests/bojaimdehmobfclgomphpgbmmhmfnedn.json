{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FrostCandy's PassBank",
  "short_name": "PassBank",
  "description": "Secure Password Generator by FrostCandy",
  "version": "0.0.2.2",
  "homepage_url": "https://frostcandy.com/_passbank",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "72": "icons/android-icon-72x72.png",
    "48": "icons/android-icon-48x48.png",
    "36": "icons/android-icon-36x36.png"
  },
  "browser_action": {
    "default_icon": "icons/android-icon-36x36.png",
    "default_title": "PassBank",
    "default_popup": "popup/index.html",
    "browser_style": true
  }
}
