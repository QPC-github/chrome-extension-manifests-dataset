{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CalPawsGPA",
  "version": "1.0",
  "manifest_version": 2,
  "description": "calculate GPA for uofs student",
  "icons": {
    "16": "calGPA_Paws16.png",
    "48": "calGPA_Paws48.png",
    "128": "calGPA_Paws128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
