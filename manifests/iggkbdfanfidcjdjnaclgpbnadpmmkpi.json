{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Url Parametor",
  "version": "1.3.3",
  "description": "Create a list of reusable url parameters.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/get_started48.png"
    }
  },
  "icons": {
    "48": "images/get_started48.png"
  },
  "manifest_version": 2
}
