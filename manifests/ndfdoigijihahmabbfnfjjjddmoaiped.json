{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RadarImmo",
  "version": "0.4",
  "description": "Variations des prix des biens immobilier",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Radar Immo"
  },
  "icons": {
    "64": "images/icon.png"
  },
  "background": {
    "scripts": [
      "js/load.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "http://www.radarimmo.com/",
    "https://www.radarimmo.com/",
    "http://www.radarimmo.fr/",
    "https://www.radarimmo.fr/",
    "http://extension.radarimmo.com/",
    "https://extension.radarimmo.com/",
    "http://www.seloger.com/",
    "https://www.seloger.com/",
    "https://www.leboncoin.fr/",
    "http://www.leboncoin.fr/",
    "http://www.logic-immo.com/",
    "https://www.logic-immo.com/",
    "http://www.superimmo.com/",
    "https://www.superimmo.com/"
  ]
}
