{
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": "16.png"
  },
  "description": "Extension to allow screen sharing in InstaVC applications.",
  "externally_connectable": {
    "matches": [
      "*://*.instavc.com/*",
      "*://*.grtjewels.com/*",
      "*://*.wificlasses.com/*",
      "*://*.careclix.com/*",
      "*://*.robotec.com/*",
      "*://*.tutorsweb.com/*",
      "*://*.mpmindia.com/*",
      "*://*.ckclive.com/*"
    ]
  },
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "InstaVC_Li Screensharing",
  "permissions": [
    "desktopCapture",
    "management"
  ],
  "short_name": "Screen sharing for InstaVC",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.1"
}
