{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "T-Mobile",
  "description": "Download T-Mobile - Pay the bill, shop for the latest devices... ",
  "version": "2.1.0",
  "chrome_url_overrides": {
    "newtab": "newpage.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
