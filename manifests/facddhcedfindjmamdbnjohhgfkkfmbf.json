{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Giovesoft - QRCoder - QRCode",
  "short_name": "QRCoder",
  "version": "1.6.1",
  "description": "Create and share on social networks or on your smartphone the current URL via QR Code",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Giovesoft - QRCoder - QRCode",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "permissions": [
    "tabs",
    "http://*.google.com/",
    "https://*.google.com/",
    "https://www.googleapis.com/",
    "https://cuut.it/"
  ],
  "homepage_url": "http://www.giovesoft.com"
}
