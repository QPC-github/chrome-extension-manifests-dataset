{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Diigo Read Later"
  },
  "name": "Diigo Read Later",
  "short_name": "Read Later",
  "description": "Bookmarks the current tab in diigo.com",
  "homepage_url": "https://chrome.google.com/webstore/detail/diigo-read-later/jooajjfbnpnafgndfpeboaehpddfglaj",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.1",
  "manifest_version": 2
}
