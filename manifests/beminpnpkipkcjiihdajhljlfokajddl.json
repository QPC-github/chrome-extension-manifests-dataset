{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hack YouTube Age Restriction",
  "short_name": "HackYouTube",
  "description": "YouTube age restriction bypass plus comments section. Watch age restricted videos without leaving YouTube.",
  "version": "1.0.9",
  "author": "PSIXs",
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "HackYouTube\n\nPress me to unblock video"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content.js",
        "jquery-3.1.0.min.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "*://*.youtube.com/*"
  ]
}
