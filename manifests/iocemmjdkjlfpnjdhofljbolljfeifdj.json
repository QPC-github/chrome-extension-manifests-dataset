{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NYU Login Page Verify",
  "version": "1.2.5",
  "description": "Made by NYU student Daniel Chin.\nShows popup on verified NYU login page. Shows warning on spoofers.",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2
}
