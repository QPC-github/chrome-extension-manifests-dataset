{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SwiftConverter | Free File Converter",
  "short_name": "SwiftConverter",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Convert your video, audio, document and image files to any format free with SwiftConverter.",
  "homepage_url": "http://www.swiftconverter.com",
  "icons": {
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts": [
      "scchromeextension.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Open SwiftConverter"
  },
  "permissions": [
    "http://www.swiftconverter.com/*",
    "https://www.swiftconverter.com/*"
  ]
}
