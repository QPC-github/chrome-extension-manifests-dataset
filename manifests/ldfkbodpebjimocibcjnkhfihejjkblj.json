{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Nampol Petpadoong",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/font-awesome.min.css",
        "css/bootstrap.css",
        "css/style.css"
      ],
      "js": [
        "js/jquery-1.10.2.min.js",
        "js/bootstrap.min.js",
        "js/insQ.js",
        "js/app.js"
      ],
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Real-time credibility evaluation for Thai Tweet.",
  "homepage_url": "https://fb.kku.ac.th/nampol/ThaiTweetCredibility.html",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "name": "ThaiTweetCred",
  "page_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "Click to give feedback about TweetCred."
  },
  "permissions": [
    "storage",
    "http://twitter.com/*",
    "https://twitter.com/*",
    "http://54.169.123.190/*"
  ],
  "version": "1.0.4",
  "web_accessible_resources": [
    "img/*",
    "font/*",
    "js/jquery-1.10.2.min.map"
  ]
}
