{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TableMining",
  "version": "1.0",
  "description": "Transform tables in web pages for data mining",
  "background": {
    "page": "background.html"
  },
  "devtools_page": "chrome-preprocessor.html",
  "manifest_version": 2,
  "icons": {
    "16": "appicon16.png",
    "48": "appicon48.png",
    "128": "appicon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "<all_urls>"
  ]
}
