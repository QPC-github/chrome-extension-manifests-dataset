{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Timed tabs",
  "description": "Wonder for how long some tabs were open? Time your tabs for whatever reason you need to do that",
  "version": "1.0",
  "icons": {
    "16": "logo-16x16.PNG",
    "32": "logo-32x32.PNG",
    "128": "logo-128x128.PNG"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}
