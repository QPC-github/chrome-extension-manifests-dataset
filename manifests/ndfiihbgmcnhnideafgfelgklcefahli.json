{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KDP SEO BOT(tester's group)",
  "version": "1.0.2",
  "description": "The Best And Only Software You Will Need To Publish PROFITABLE Books On Amazon KDP",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "authorize.html"
  },
  "background": {
    "scripts": [
      "jquery_3_3_7.js",
      "license_verify.js",
      "check_authorize.js"
    ],
    "persistent": false
  },
  "version_name": "Beta Version",
  "content_security_policy": "script-src 'self' https://completion.amazon.com/ https://completion.amazon.co.uk/; object-src 'self'",
  "permissions": [
    "cookies",
    "tabs",
    "https://amazon.com/*",
    "http://amazon.com/*",
    "https://www.sendowl.com/*",
    "http://www.sendowl.com/*",
    "https://*.com/*"
  ]
}
