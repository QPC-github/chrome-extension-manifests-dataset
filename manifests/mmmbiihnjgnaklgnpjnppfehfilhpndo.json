  "icons": {
    "48": "logo-48.png",
    "128": "logo-128.png",
    "16": "logo-16.png"
  },
  "homepage_url": "http://rannios.free.fr/?/notepad",
  "manifest_version": 2,
  "version": "1.0"
}
