{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "World Newspapers Online",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Read All Newspapers Online & Browse All Media of the World.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en_US",
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "All Newspapers & Media of the World",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
