{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iflix optimizer",
  "version": "0.1.3",
  "description": "Optimize network requests for iflix",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.iflix.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
