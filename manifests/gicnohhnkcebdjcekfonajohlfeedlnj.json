{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Push to Piping",
  "description": "Allow you to push things to your Piping",
  "version": "0.6",
  "permissions": [
    "contextMenus",
    "http://1.yilaunch.sinaapp.com/"
  ],
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
