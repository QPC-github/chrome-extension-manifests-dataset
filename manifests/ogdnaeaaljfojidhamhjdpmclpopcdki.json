{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "poe.trade Enhancement Suite",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://poe.trade/*",
        "http://poe.trade/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://poe.ninja/*",
    "https://poe.ninja/*"
  ]
}
