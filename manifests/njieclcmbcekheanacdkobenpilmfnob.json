{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "20xx Ladder",
  "short_name": "20xx",
  "version": "1.0.4",
  "description": "SmashLadder+ for new design features, customization, and THEORIES",
  "content_scripts": [
    {
      "js": [
        "thescript.js"
      ],
      "css": [
        "goodtheme.css"
      ],
      "matches": [
        "*://*.smashladder.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "foxshine.png",
    "popup.js"
  ],
  "permissions": [
    "tabs",
    "*://*.smashladder.com/netplay*",
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_icon": "foxshine.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "foxshine.png",
    "48": "foxshine.png",
    "128": "foxshine128.png"
  }
}
