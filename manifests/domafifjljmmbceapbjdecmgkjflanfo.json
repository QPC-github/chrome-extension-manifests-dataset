{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Site Cycler",
  "description": "Navigate through your favorite sites with ease",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "BSChromeConsole.js",
        "BSStringHelper.js",
        "RNStations.js",
        "RNLocalStoreHelper.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "BSChromeConsole.js",
      "BSStringHelper.js",
      "RNStations.js",
      "onClick.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*",
    "storage"
  ],
  "web_accessible_resources": [],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
