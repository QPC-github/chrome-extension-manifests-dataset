{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "B*gshide",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Hide B*gs popup. It has disable option but it doesn't work for me. So I made this little extension.",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://www.bugs.co.kr/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.bugs.co.kr/*"
      ],
      "js": [
        "jquery-1.7.2.min.js",
        "app.js"
      ]
    }
  ]
}
