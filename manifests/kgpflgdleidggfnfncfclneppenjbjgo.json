{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Signal Opener",
  "description": "This extensions opens the Signal app",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "management"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
