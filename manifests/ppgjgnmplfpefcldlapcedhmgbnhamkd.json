{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clean & Refresh",
  "description": "Cleans local and session storage from the active tab, then Refreshes the page",
  "version": "4.82",
  "author": "Tony Wilson - T.Wilson@mosman.nsw.gov.au - 2018 - Icons by Google - Apache 2.0 Licence",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Clean & Refresh",
    "default_icon": "icon16.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
