{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lakewood Leader News",
  "description": "Click to check the monthly newsletters at Lakewood Elementary.",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png"
