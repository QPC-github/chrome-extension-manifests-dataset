{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Show Me Tieba Code",
  "version": "1.3.2",
  "description": "Extends the Developer Tools, adding a sidebar that displays the Tieba FE code and modules associated with the selected DOM element.",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "short_name": "Show Me Tieba Code",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://fedev.baidu.com/~liudaoyu/*",
    "http://tieba.baidu.com/"
  ]
}
