{
  "background": {
    "scripts": [
      "js/jquery-2.1.4.js",
      "js/jfeed.js",
      "js/feedManager.js",
      "js/linq.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Portal 3lagoas.com.br"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery-2.1.4.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Saiba mais sobre Três Lagoas agora no seu navegador. Vagas de empregos, clima e notícias.",
  "icons": {
    "128": "images/icon_128.png",
    "256": "images/icon_256.png",
    "64": "images/icon_32.png"
  },
  "manifest_version": 2,
  "name": "Portal 3lagoas.com.br",
  "permissions": [
    "http://*/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.3"
}
