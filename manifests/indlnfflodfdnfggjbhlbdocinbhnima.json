{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My Online Session Screen Sharing",
  "author": "prosoft",
  "version": "1.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed for https://myonlinesession.com/. This extension captures the content of your screen.",
  "homepage_url": "https://myonlinesession.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "https://localhost/*",
      "https://myonlinesession.com/*",
      "https://*.myonlinesession.com/*",
      "https://localtest.me/*",
      "https://*.localtest.me/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
