{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HOH Screen Sharing Plug-in",
  "description": "A simple extension that allows you to stream your desktop with Dotdream Co.,Ltd. Streaming Server.",
  "version": "1.0",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "hohdd-logo-16x16.png",
    "48": "hohdd-logo-48x48.png",
    "128": "hohdd-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://203.150.102.235/*"
    ]
  }
}
