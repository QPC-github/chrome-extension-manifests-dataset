{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WebTao App",
  "description": "This extension helps you be more productive and increase your coding skills",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "https://www.codewars.com/api/v1/code-challenges/",
    "geolocation",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://use.fontawesome.com/9acb8c974f.js https://code.jquery.com/jquery-3.1.1.min.js https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js https://cdnjs.cloudflare.com/ajax/libs/showdown/1.0.0/showdown.min.js https://rawgit.com/jesstelford/a981e6877034231bd43b526a07b3a7b4/raw/js1-utilities.js; object-src 'self'"
}
