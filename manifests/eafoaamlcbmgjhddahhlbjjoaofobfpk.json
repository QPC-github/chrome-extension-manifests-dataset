{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebMod",
  "version": "1.0.0.3",
  "description": "This modifies the look and feel of certain websites.",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "tv.css"
      ],
      "js": [
        "tv.js"
      ],
      "matches": [
        "https://go.trackvia.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
