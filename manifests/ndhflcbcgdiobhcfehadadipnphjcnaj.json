{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Golpe to Bunda",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "ao.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery.js",
    "ao.js",
    "jquery.min.map"
  ]
}
