{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sophos Extension",
  "description": "Sophos Cloud Web Gateway Extension",
  "version": "1.1.11",
  "icons": {
    "16": "images/icon-sophos16.png",
    "48": "images/icon-sophos48.png",
    "128": "images/icon-sophos128.png"
  },
  "browser_action": {
    "default_icon": "images/icon-sophos-grey.png",
    "default_title": "Sophos"
  },
  "permissions": [
    "proxy",
    "background",
    "tabs",
    "identity",
    "identity.email",
    "idle",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "storage": {
    "managed_schema": "managed_schema.json"
  },
  "incognito": "spanning",
  "manifest_version": 2
}
