{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Inbox Helper",
  "version": "0.2",
  "permissions": [
    "http://inbox.google.com/*",
    "https://inbox.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://inbox.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "copier.js"
      ]
    }
  ]
}
