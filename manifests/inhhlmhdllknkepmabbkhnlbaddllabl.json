{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Calculator",
  "short_name": "Calculator",
  "description": "One different scientific calculator (desktop version)...",
  "version": "3.1.0.0",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "manifest_version": 2,
  "offline_enabled": true,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+C",
        "windows": "Alt+C",
        "mac": "Alt+C",
        "chromeos": "Alt+C",
        "linux": "Alt+C"
      }
    }
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "app",
    "storage"
  ]
}
