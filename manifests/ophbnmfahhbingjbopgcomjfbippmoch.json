{
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/logox19.png",
      "38": "images/logox38.png"
    },
    "default_popup": "main_screen.html",
    "default_title": "تلفزيون الآن‎"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "حمّل شريط تلفزيون الآن الإخباري ولا تفوت الفرصة لمتابعة آخر الأخبار.‎",
  "icons": {
    "128": "images/logox128.png",
    "48": "images/logox48.png"
  },
  "manifest_version": 2,
  "name": "تلفزيون الآن‎",
  "permissions": [
    "tabs",
    "http://alaan.tv/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "web_accessible_resources": [
    "images/logo.png"
  ]
}
