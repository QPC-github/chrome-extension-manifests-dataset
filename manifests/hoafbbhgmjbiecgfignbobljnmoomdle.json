{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Current - A Chatter App",
  "description": "Read, monitor, and chat better with Chatter",
  "version": "0.2.4",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "minimum_chrome_version": "29",
  "permissions": [
    "https://*.salesforce.com/",
    "identity",
    "storage"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
