{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Whooing AwesomeBox",
  "version": "0.57",
  "manifest_version": 2,
  "description": "The AwesomeBox of whooing.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Whooing AwesomeBox",
    "default_popup": "awesome.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "webRequest",
    "https://whooing.com/*"
  ],
  "content_security_policy": "script-src 'self' https://whooing.com; object-src 'self'"
}
