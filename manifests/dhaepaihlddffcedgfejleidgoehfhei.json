{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "4 Pics, 1 Word Tagalog",
  "version": "1.1",
  "description": "Test your knowledge of the Tagalog language and challenge your friends!",
  "manifest_version": 2,
  "app": {
    "launch": {
      "web_url": "http://www.4picstagalog.com?utm_source=chrome_ext",
