{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Eye Bleach",
  "description": "Relaxes your mind and improves your mood through cute pics and gifs",
  "version": "1.12",
  "permissions": [
    "https://temporary.media/*"
  ],
  "browser_action": {
    "default_title": "Kawaaiiii",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}
