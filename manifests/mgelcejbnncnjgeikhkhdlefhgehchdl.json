{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Isaac Pintosevich Extension",
  "version": "0.0.1.0",
  "icons": {
    "48": "ips/images/icons/icon-48x48.png",
    "128": "ips/images/icons/icon-128x128.png"
  },
  "background": {
    "scripts": [
      "pintosevich.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "ips/extension.html"
  },
  "browser_action": {
    "default_title": "Isaac Pintosevich Extension",
    "default_icon": {
      "48": "ips/images/icons/icon-48x48.png"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "https://pintosevich.live/*"
  ]
}
