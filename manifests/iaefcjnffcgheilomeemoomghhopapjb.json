{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "moone.in",
  "description": "Open YouTube Vid on moone.in",
  "icons": {
    "32": "moone_logo_300.png",
    "48": "moone_logo_300.png"
  },
  "version": "1.0",
  "browser_action": {
    "default_icon": "moone_logo_300.png"
  },
  "permissions": [
    "tabs",
    "http://www.google.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
