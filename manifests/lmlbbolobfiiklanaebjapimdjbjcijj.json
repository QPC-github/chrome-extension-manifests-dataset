{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Troll-E",
  "description": "Heels, Wheels and all that you please !!!",
  "version": "1.0",
  "permissions": [
    "http://localhost/liki2/",
    "tabs",
    "<all_urls>",
    "https://www.amazon.com/*"
  ],
  "browser_action": {
    "default_icon": "zoe.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
