{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SchoolBranch Classgrounds",
  "short_name": "Classgrounds",
  "version": "1.3.7",
  "description": "Interact with Chromebooks in the classroom",
  "oauth2": {
    "client_id": "332944261312-d9at83prhdfkdumvmf1fqen3metrchqq.apps.googleusercontent.com",
    "scopes": [
      "email",
      "profile"
    ]
  },
  "background": {
    "scripts": [
      "jquery-1.11.1.min.js",
      "js/ortc.js",
      "simplepeer.min.js",
      "background.min.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "identity",
    "identity.email",
    "tabCapture",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://ortc-developers.realtime.co; object-src 'self' blob: filesystem: chrome-extension-resource:",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "SchoolBranch Classgrounds",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
