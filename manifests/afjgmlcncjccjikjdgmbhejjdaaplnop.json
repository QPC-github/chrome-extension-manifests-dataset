{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Full Page Screenshots",
  "description": "Take screenshot of a fullpage tab.",
  "version": "1.0.2",
  "container": [
    "GOOGLE_DRIVE",
    "DOMAIN_INSTALLABLE"
  ],
  "api_console_project_id": "398931042402",
  "homepage_url": "https://screenshot.freeusefulapps.net/",
  "icons": {
    "128": "logo-128.png",
    "48": "logo-48.png",
    "16": "logo-16.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "logo-16.png",
      "32": "logo-32.png"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "desktopCapture",
    "tabs",
    "activeTab",
    "<all_urls>",
    "identity"
  ],
  "web_accessible_resources": [
    "logo-512.png"
  ],
  "background": {
    "scripts": [
      "js/tabCapture.js",
      "js/main.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "key": "",
  "oauth2": {
    "client_id": "398931042402-jsf76lommddqd3pmudkqeakcg94n18b5.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  }
}
