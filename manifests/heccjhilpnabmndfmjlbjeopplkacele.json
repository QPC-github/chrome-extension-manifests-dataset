{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "750 Words Navigation",
  "description": "This turns the blank margins at 750words.com into previous/next day/month navigation buttons.",
  "version": "1.0.3",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://750words.com/*",
        "https://750words.com/*"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "750words-nav.js"
      ]
    }
  ]
}
