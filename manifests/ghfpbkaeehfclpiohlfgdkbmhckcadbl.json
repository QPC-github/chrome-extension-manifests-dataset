{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Compass Greenhouse Ninja",
  "version": "1.1",
  "description": "A Greenhouse chrome extension for Compass recruiters and interviewers.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "permissions": [
    "tabs",
    "https://compass.greenhouse.io/*/"
  ]
}
