{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_application_title__",
  "version": "1.0.5",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_application_description__",
  "offline_enabled": true,
  "container": [
    "GOOGLE_DRIVE"
  ],
  "icons": {
    "512": "icon-512.png",
    "256": "icon-256.png",
    "128": "icon-128.png",
    "64": "icon-64.png",
    "48": "icon-48.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "browser_action": {
    "default_icon": {
      "512": "icon-512.png",
      "256": "icon-256.png",
      "128": "icon-128.png",
      "64": "icon-64.png",
      "48": "icon-48.png",
      "32": "icon-32.png",
      "16": "icon-16.png"
    },
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "https://*.freebusinessapps.net/*",
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js",
      "/notify/update-conf.js",
      "/notify/update.min.js"
    ]
  }
}
