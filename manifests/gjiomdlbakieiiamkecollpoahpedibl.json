{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "iCoreTech, Inc.",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_popup": "popup.html"
  },
  "description": "This extension shows latest price ticker of popular ELTCOIN trading markets.",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "homepage_url": "https://pandoon.cash/markets/eltcoin",
  "manifest_version": 2,
  "name": "Pandoon ELTCOIN Ticker",
  "short_name": "ELTCOIN Ticker",
  "options_page": "options.html",
  "permissions": [
    "https://api.coinmarketcap.com/",
    "https://min-api.cryptocompare.com/"
  ],
  "version": "0.1"
}
