{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The History (alpha)",
  "short_name": "The History",
  "version": "0.0.10",
  "description": "A new way to browse internet and your browser history",
  "offline_enabled": true,
  "background": {
    "page": "bg.html",
    "persistent": true
  },
  "browser_action": {},
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost:7000/faye; object-src 'self';",
  "options_page": "options.html",
  "content_scripts": [],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+9",
        "mac": "Ctrl+Shift+9"
      }
    }
  },
  "manifest_version": 2
}
