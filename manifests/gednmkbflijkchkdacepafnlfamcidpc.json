{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ServiceTick API Helper",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "This extension provides additional survey information to assist API usage.",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.servicetick.com/SurveyPortal/*",
        "*://*.servicetick.com/surveyportal/*"
      ]
    }
  ]
}
