{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PWNHealth - Screensharing extension",
  "author": "PWNHealth",
  "version": "3.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Extension used for screensharing.",
  "homepage_url": "https://pwnhealth.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.pwnhealth.com/*",
      "*://*.integratedgenetics.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
