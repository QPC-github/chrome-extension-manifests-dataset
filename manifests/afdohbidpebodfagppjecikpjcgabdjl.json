{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "resources/image/icon_19.png",
    "default_popup": "popupaliexpress.html",
    "default_title": "3dcart - Aliexpress Product Importer"
  },
  "content_scripts": [
    {
      "css": [
        "resources/style/cscart_import.css"
      ],
      "js": [
        "resources/js/jquery.js",
        "resources/js/content.js"
      ],
      "matches": [
        "*://*.aliexpress.com/*"
      ]
    }
  ],
  "description": "3dcart is a complete shopping cart solution. Use this extension to import products from Aliexpress directly into your 3dcart Store.",
  "icons": {
    "128": "resources/image/icon_128.png"
  },
  "manifest_version": 2,
  "name": "3dcart Aliexpress Importer",
  "permissions": [
    "identity",
    "activeTab",
    "storage",
    "tabs",
    "https://app-aliexpressextension.3dcart.com/*",
    "https://apirest.3dcart.com/*",
    "https://apirest.3dcart.com/oauth/*"
  ],
  "version": "1.0.3",
  "web_accessible_resources": [
    "resources/image/*.png",
    "resources/js/cartaliimporter.js",
    "resources/js/jquery.js",
    "resources/style/cscart_import.css"
  ],
  "background": {
    "scripts": [
      "resources/js/background.js"
    ],
    "persistent": false
  }
}
