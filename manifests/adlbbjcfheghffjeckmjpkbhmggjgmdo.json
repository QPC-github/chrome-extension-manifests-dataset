{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "context.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Search Selected text in Enadoc",
  "icons": {
    "16": "logo-16.png"
  },
  "manifest_version": 2,
  "name": "Enadoc Search",
  "permissions": [
    "storage",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "version": "2.0.1"
}
