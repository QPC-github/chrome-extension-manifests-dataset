{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sumopaint",
  "short_name": "Sumopaint",
  "version": "10.2",
  "description": "The most popular photo editor and painting app. Over 30 million users & 500 million saved images.",
  "author": "Lauri Koutaniemi",
  "browser_action": {
    "default_icon": "sumo128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "sumo16.png",
    "32": "sumo32.png",
    "48": "sumo48.png",
    "128": "sumo128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
