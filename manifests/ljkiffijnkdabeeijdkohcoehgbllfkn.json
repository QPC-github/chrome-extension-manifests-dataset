{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Deadpool HD wallpapers in new tab Unlimited",
  "short_name": "Deadpool HD wallpapers new tab",
  "author": "Viktor Magaev",
  "description": "See new Deadpool HD wallpaper each time, when you open new tab",
  "version": "1.0.0",
  "icons": {
    "128": "128.png"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "topSites",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://api.flickr.com; object-src 'self'",
  "default_locale": "en"
}
