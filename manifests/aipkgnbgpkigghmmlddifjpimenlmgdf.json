{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Amtrak Enhancements",
  "version": "0.4",
  "description": "Enhances Amtrak's website.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://tickets.amtrak.com/itd/amtrak*",
        "https://tickets.amtrak.com/itd/amtrak*"
      ],
      "js": [
        "fix_ticket.js"
      ],
      "run_at": "document_end"
    }
  ]
}
