{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "White Noise App",
  "description": "This extension plays white noise in your browser",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "whiteNoise.js"
    ],
    "persistent": false
  },
  "content_security_policy": "default-src",
  "permissions": []
}
