{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lezago - Finde den besten Preis",
  "description": "Lezago fuegt Gutscheincodes automatisch einer Bestellung in einem Onlineshop hinzu!",
  "version": "1.1.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content_script.css"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon-48x48.png",
    "128": "favicon-128x128.png"
  }
}
