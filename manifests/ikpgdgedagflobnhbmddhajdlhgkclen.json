{
  "author": "Daiscon",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://www.daiscon.com/*",
        "https://daiscon.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This WebRTC screen capturing extension capture screen of daiscon.com",
  "homepage_url": "https://daiscon.com/",
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Daiscon Screen Capturing",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
