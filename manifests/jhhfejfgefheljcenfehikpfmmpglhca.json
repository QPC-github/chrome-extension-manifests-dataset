{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.0.0",
  "name": "Gitmoji",
  "description": "Get gitmojis copied to clipboard",
  "browser_action": {
    "default_title": "Gitmoji",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
