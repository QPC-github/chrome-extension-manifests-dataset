{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Youtube Box",
  "description": "Clicking any YouTube link will open up video in a box.",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "css": [
        "css/jquery-ui-1.10.4.min.css",
        "css/ytbox.css",
        "lib/ionicons-1.4.1/css/ionicons.min.css"
      ],
      "js": [
        "lib/jquery.min.js",
        "src/ytboxinit.js"
      ]
    }
  ],
  "permissions": [
    "*://*.reddit.com/*"
  ],
  "web_accessible_resources": [
    "img/jquery-ui/animated-overlay.gif",
    "img/jquery-ui/ui-bg_diagonals-thick_18_b81900_40x40.png",
    "img/jquery-ui/ui-bg_diagonals-thick_20_666666_40x40.png",
    "img/jquery-ui/ui-bg_flat_10_000000_40x100.png",
    "img/jquery-ui/ui-bg_glass_100_f6f6f6_1x400.png",
    "img/jquery-ui/ui-bg_glass_100_fdf5ce_1x400.png",
    "img/jquery-ui/ui-bg_glass_65_ffffff_1x400.png",
    "img/jquery-ui/ui-bg_gloss-wave_35_f6a828_500x100.png",
    "img/jquery-ui/ui-bg_highlight-soft_100_eeeeee_1x100.png",
    "img/jquery-ui/ui-bg_highlight-soft_75_ffe45c_1x100.png",
    "img/jquery-ui/ui-icons_222222_256x240.png",
    "img/jquery-ui/ui-icons_228ef1_256x240.png",
    "img/jquery-ui/ui-icons_ef8c08_256x240.png",
    "img/jquery-ui/ui-icons_ffd27a_256x240.png",
    "img/jquery-ui/ui-icons_ffffff_256x240.png",
    "src/ytapifn.js",
    "src/ytbox.js",
    "src/ytboxinit.js",
    "lib/jquery-1.10.2.js",
    "lib/ionicons-1.4.1/fonts/ionicons.eot",
    "lib/ionicons-1.4.1/fonts/ionicons.svg",
    "lib/ionicons-1.4.1/fonts/ionicons.ttf",
    "lib/ionicons-1.4.1/fonts/ionicons.woff",
    "src/ytBoxRedirector.js"
  ]
}
