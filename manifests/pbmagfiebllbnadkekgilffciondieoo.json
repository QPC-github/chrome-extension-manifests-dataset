{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GamingRoom.tv - Notifications",
  "short_name": "GRTV",
  "version": "1.1.2",
  "description": "Restez informé lorsqu'un live démarre sur la GamingRoom.tv. Grâce à cette extension, vous ne manquerez rien!",
  "icons": {
    "128": "img/128.png"
  },
  "permissions": [
    "*://www.gamingroom.tv/*",
    "notifications",
    "management",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js",
      "js/options.js"
    ]
  },
  "homepage_url": "http://www.gamingroom.tv/",
  "manifest_version": 2
}
