{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TutorOcean",
  "version": "0.19",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.tutorocean.com/*",
      "*://*.tutorloo.com/*",
      "*://student.ansarimath.com/*",
      "*://portal.educoeducation.ca/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
