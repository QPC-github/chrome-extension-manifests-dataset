{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "真便宜 自动提醒",
  "version": "1.1",
  "manifest_version": 2,
  "description": "真便宜_推荐购物自动提醒程序",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "notifications",
    "http://*.zhencheap.com/"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "images/icon48.png"
  ],
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_title": "真便宜",
    "default_popup": "popup.html"
  }
}
