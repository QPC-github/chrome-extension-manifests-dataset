{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MultiBar",
  "description": "Allows switching between multiple sets of bookmarks on the bookmarks bar",
  "version": "1.0",
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "MultiBar"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
