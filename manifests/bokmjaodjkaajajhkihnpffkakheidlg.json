{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jira Context Search",
  "short_name": "JCS",
  "description": "Jira Context Search",
  "version": "2.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "./16x16.png",
    "32": "./32x32.png",
    "48": "./48x48.png",
    "96": "./96x96.png",
    "128": "./128x128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "commands": {
    "show_extension": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Show extension on Chrome extension toolbar"
    }
  }
}
