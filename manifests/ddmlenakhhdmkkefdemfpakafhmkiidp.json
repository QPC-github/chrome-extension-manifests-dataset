{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Twitch Viewercount Title",
  "version": "1.11",
  "manifest_version": 2,
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "description": "Shows twitch viewercount in tab title",
  "background": "background.html",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
