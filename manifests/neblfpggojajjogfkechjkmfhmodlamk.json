{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "poker.png",
    "default_popup": "poker_info.html"
  },
  "content_scripts": [
    {
      "js": [
        "thirdParty/jquery.min.js",
        "prikker.js",
        "indstillinger.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ]
    }
  ],
  "description": "Prik automatisk dine venner, hvis de prikker dig p√• Facebook",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "Facebook Auto Prikker",
  "permissions": [
    "storage"
  ],
  "short_name": "Auto Prik",
  "version": "1.0.2"
}
