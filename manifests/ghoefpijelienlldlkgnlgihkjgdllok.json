{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Find sentence examples",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Search for sentence examples of the selected text",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "your_dictionary.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "author": [
    {
      "name": "Quy Nguyen",
      "email": "nguyenledinhquy@gmail.com"
    }
  ]
}
