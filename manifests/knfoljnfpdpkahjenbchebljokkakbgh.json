{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Messagenes - Look at your Newsrooms",
  "version": "1.1.1",
  "description": "Allows you to look at your Newsrooms",
  "default_locale": "es",
  "icons": {
    "16": "messagenes_16.png",
    "48": "messagenes_48.png",
    "128": "messagenes_128.png"
  },
  "browser_action": {
    "default_icon": "messagenes_16.png",
    "default_title": "Searching a Newsroom",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "web_accessible_resources": [
    "images/search.png"
  ],
  "short_name": "Messagenes"
}
