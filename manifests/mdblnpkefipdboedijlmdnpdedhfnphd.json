{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabLimiter",
  "description": "Limits your tabs to help prevent too-many-tabs syndrome.",
  "version": "0.0.1",
  "author": "GrayFalls",
  "manifest_version": 2,
  "icons": {
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "Tab Limiter",
    "default_popup": "dist/options.html"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}
