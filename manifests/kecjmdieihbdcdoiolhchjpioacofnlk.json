{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Youtube Recommended Video Block",
  "description": "try to make you waste less time on recommended videos",
  "version": "1.0.0",
  "author": "watashiSHUN",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "DOMchange.js"
      ],
      "run_at": "document_start"
    }
  ]
}
