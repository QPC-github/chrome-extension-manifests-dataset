{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Image Stamps",
  "description": "Add some temporary stamps to webpages (until page is refreshed).",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  }
}
