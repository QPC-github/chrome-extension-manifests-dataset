{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Aternity Web Activity Creator (Beta)",
  "short_name": "OnApp",
  "description": "Aternity Web Activity Creator (Beta)",
  "author": "Riverbed",
  "homepage_url": "https://help.aternity.com/csh?Product=latest&topicname=admin_config_wac2.html",
  "minimum_chrome_version": "50.0.0",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Aternity Web Activity Creator 2.0"
  },
  "commands": {
    "add-element": {
      "suggested_key": {
        "default": "Ctrl+Shift+A"
      },
      "description": "Add Element"
    },
    "Reload": {
      "description": "Restart Extension"
    }
  },
  "web_accessible_resources": [
    "injected-content.js.map",
    "overlay.html"
  ],
  "permissions": [
    "tabs",
    "management",
    "history",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "2.0.188"
}
