{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Search It Highlighter",
  "short_name": "SIH",
  "version": "1.4",
  "description": "Easily google, bing, yahoo, etc. search any highlighted text!",
  "author": "Nikki Louis Quibin",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "web_accessible_resources": [
    "assets/google.svg",
    "assets/bing.svg",
    "assets/yahoo.svg"
  ]
}
