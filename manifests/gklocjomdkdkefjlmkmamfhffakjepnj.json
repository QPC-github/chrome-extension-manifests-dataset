{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Huga Travel",
  "description": "An extension to help create new cards and edits cards on your trips in Huga.",
  "background": {
    "scripts": [
      "js/jquery-3.2.1.js",
      "js/background.js"
    ],
    "css": [
      "style.css"
    ]
  },
  "version": "0.0.0.5",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Huga Travel",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "tabs",
    "webNavigation",
    "http://127.0.0.1:8000/*",
    "https://hugatravel.com/*"
  ],
  "icons": {
    "16": "images/hugga.png",
    "48": "images/hugga.png",
    "128": "images/hugga.png"
  }
}
