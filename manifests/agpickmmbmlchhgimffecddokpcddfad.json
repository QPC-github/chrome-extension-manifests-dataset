{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reddit Comment Blocker",
  "description": "This extension will block the ability to eaisly comment on Reddit",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*"
      ],
      "js": [
        "jquery-2.1.0.min.js",
        "hide.js"
      ]
    }
  ]
}
