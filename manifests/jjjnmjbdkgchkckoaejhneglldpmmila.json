{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "视维桌面共享",
  "description": "使用视维桌面共享扩展程序在会议中一键共享您的桌面.",
  "version": "1.2.2",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "SuperV-logo-16x16.png",
    "48": "SuperV-logo-48x48.png",
    "128": "SuperV-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://lite.g3meeting.com/*",
      "*://home.g3meeting.com/*",
      "*://v.sdu.edu.cn/*",
      "*://sd.g3meeting.com/*",
      "*://med.g3meeting.com/*",
      "*://bj.g3meeting.com/*",
      "*://jn.wsxf.gov.cn/*",
      "*://www.sdhangu.com/*",
      "*://www.womeeting.com/*",
      "*://www.super-v.cn/*",
      "*://swhy.tcgqxx.com/*",
      "*://www.coinkero.com/*"
    ]
  }
}
