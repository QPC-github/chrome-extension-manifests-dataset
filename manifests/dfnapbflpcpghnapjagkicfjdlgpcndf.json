{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Convert Base64",
  "version": "1.3.1",
  "manifest_version": 2,
  "icons": {
    "128": "logoprd.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/src/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logoprd.png",
    "default_title": "Convert Base64",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "<all_urls>",
    "contextMenus"
  ]
}
