{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2",
  "name": "Adjaranet Adblocker",
  "description": "Adblocker for net.adjara.com",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "webRequest",
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Adjaranet Adblocker",
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
