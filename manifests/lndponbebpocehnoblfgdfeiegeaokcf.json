{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VMware Horizon Geolocation Redirection",
  "version": "7.12.0.0",
  "version_name": "7.12.0.0",
  "key": "",
  "description": "Chrome extension to enable Geolocation Redirection inside VMware Horizon Agent.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "nativeMessaging"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "geoRedir.js"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    }
  ],
  "browser_action": {
    "default_icon": "icon-16.png"
  }
}
