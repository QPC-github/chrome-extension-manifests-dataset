{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "icon-128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "default_locale": "en",
  "description": "Beautiful Christmas Gift wallpaper with useful utilities",
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "Christmas Gift Wallpapers HD New Tab",
  "offline_enabled": true,
  "permissions": [
    "topSites",
    "*://www.google.com/*",
    "chrome://favicon/*",
    "webRequestBlocking",
    "webRequest"
  ],
  "version": "1.3"
}
