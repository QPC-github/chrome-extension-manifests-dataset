{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Skill Builder Spelling - By Kaiserapps",
  "description": "A completely offline spelling application.",
  "version": "1.0.7",
  "offline_enabled": true,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "24": "img/icon24.png",
    "128": "img/icon128.png"
  }
}
