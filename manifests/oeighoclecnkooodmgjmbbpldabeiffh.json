{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Rupor Notifier",
  "version": "1.0.5",
  "description": "",
  "manifest_version": 2,
  "permissions": [
    "alarms",
    "tabs",
    "*://rupor.sampo.ru/*"
  ],
  "web_accessible_resources": [
    "popup.css"
  ],
  "icons": {
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "rupor-out.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "homepage_url": "http://anakee.ru/chrome/"
}
