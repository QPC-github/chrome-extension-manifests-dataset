{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "32": "images/box-32.png",
    "64": "images/box-64.png",
    "128": "images/box-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "lib/background/index.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "images/box-32.png"
    },
    "default_title": "Bloglovin'"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.3.1"
}
