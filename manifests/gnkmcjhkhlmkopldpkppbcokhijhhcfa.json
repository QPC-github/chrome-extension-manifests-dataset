{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "content.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.7.1.min.js",
        "script.js",
        "script2.js"
      ],
      "matches": [
        "http://*.xerq.net/*",
        "http://*.xerq.org/*",
        "https://*.xerq.net/*",
        "https://*.xerq.org/*",
        "https://*.xerq.io/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "PlsGo",
  "permissions": [
    "http://*.xerq.net/*",
    "http://*.xerq.org/*",
    "https://*.xerq.net/*",
    "https://*.xerq.org/*",
    "https://*.xerq.io/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.5",
  "web_accessible_resources": [
    "images/1.png",
    "images/2.png"
  ]
}
