{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RealPresence WebSuite Easy Meet Extension",
  "version": "2.1.0",
  "description": "Create a WebSuite meeting from Google Chrome",
  "default_local": "en",
  "short_name": "RPWS Easy Meet Extension",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "RealPresence WebSuite Easy Meet Extension",
    "default_icon": "icon_16.png",
    "default_popup": "cax.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
