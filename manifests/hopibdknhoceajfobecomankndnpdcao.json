{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WikiDead",
  "description": "Extension to list people on a Wikipedia page who are already deceased",
  "version": "1.0.3",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "gr-128.png",
    "default_title": "See who's dead",
    "js": [
      "checker.js",
      "script.js",
      "jquery.min.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://wikipedia.org/*",
    "https://wikipedia.org/*",
    "http://*.wikipedia.org/*",
    "https://*.wikipedia.org/*",
    "declarativeContent"
  ],
  "icons": {
    "128": "gr-128.png",
    "48": "gr-48.png",
    "16": "gr-16.png"
  },
  "manifest_version": 2
}
