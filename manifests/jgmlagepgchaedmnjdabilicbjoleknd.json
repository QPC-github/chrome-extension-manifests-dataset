{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CxBlock",
  "version": "1.3",
  "short_name": "CxBlock",
  "author": "Jahastech",
  "description": "Chromebook filtering agent for NxFilter.",
  "icons": {
    "16": "16_nxrb2.png",
    "48": "48_nxrb2.png",
    "128": "128_nxrb2.png"
  },
  "browser_action": {
    "default_icon": "16_nxrb2.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "*://*/*",
    "tabs",
    "storage",
    "webRequest",
    "webNavigation",
    "webRequestBlocking",
    "identity",
    "identity.email"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
