{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rubrik App Stream",
  "version": "1.0",
  "description": "Reads the url of active tab as anonymous user",
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "history",
    "identity",
    "identity.email",
    "http://10.0.118.210/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
