{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JPugh",
  "version": "1.0",
  "description": "Replace all youtube videos with J-Pugh falling down the stairs",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "JohnFalling.gif"
  ]
}
