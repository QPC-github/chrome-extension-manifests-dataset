{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PewDiePie-YouTube-DeleteLikeButton",
  "version": "1.0.0.2",
  "description": "Chrome addon to delete like button so you dont accidentally click on it",
  "homepage_url": "https://www.youtube.com/watch?v=KSddAqfUJ4A",
  "manifest_version": 2,
  "icons": {
    "16": "dislike16x16.png",
    "48": "dislike48x48.png",
    "128": "dislike128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://yt.be/*",
        "https://youtu.be/*"
      ],
      "css": [
        "youtube.css"
      ]
    }
  ]
}
