{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Scraping La Bonne Boite",
  "manifest_version": 2,
  "name": "Scraping La Bonne Boite",
  "version": "0.0.1",
  "homepage_url": "https://www.aymericdev.fr/produit/4",
  "background": {
    "scripts": [
      "js/browser-polyfill.js",
      "js/background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "20": "icons/on-300.png"
    },
    "default_title": "Scraping La Bonne Boite",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://labonneboite.pole-emploi.fr/entreprises*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/on-16.png",
    "32": "icons/on-32.png",
    "128": "icons/on-128.png",
    "300": "icons/on-300.png"
  },
  "permissions": [
    "*://labonneboite.pole-emploi.fr/*",
    "*://aymericdev.fr/api/*",
    "activeTab",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ]
}
