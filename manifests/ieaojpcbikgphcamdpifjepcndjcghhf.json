{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "A Photographic Start",
  "description": "A start page with pretty pictures from around the web.",
  "icons": {
    "16": "button/camera_16x16.png",
    "32": "button/camera_32x32.png",
    "48": "button/camera_48x48.png",
    "96": "button/camera_96x96.png",
    "128": "button/camera_128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "content/new_tab.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ],
  "version": "1.4.1"
}
