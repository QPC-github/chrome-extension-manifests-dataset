{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Window Jump",
  "version": "1.1",
  "description": "Moves the currently selected tab(s) to another open window.",
  "icons": {
    "16": "monitor_go.png",
    "48": "window_jump_48.png",
    "128": "window_jump_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "monitor_go.png",
    "default_title": "Window Jump"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "manifest_version": 2
}
