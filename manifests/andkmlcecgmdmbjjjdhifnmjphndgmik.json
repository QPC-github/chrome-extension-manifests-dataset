{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FrameTube",
  "description": "Creates a frame of a youtube video.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.0",
  "icons": {
    "19": "images/icon19.png",
    "38": "images/icon38.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "https://*.youtube.com/*",
    "http://*.youtube.com/*",
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png",
      "128": "images/icon128.png"
    }
  }
}
