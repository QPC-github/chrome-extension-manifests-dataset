{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Webhosting Coupon Codes India",
  "description": "This extension provides Indian webhosting coupon codes.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Webhosting Coupon Codes India",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "jquery.jfeed.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}
