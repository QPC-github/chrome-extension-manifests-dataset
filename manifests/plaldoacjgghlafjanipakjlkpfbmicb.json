{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "qwNote",
  "description": "qwNote Chrome Extension",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "version": "1.0.1",
  "permissions": [
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Open qwNote",
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "48.png"
  ]
}
