{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Not Now",
  "short_name": "Filtering facebook so you can live your life",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "file://*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": {
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
