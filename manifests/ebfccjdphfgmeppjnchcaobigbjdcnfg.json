{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Boon It",
  "version": "0.1.3",
  "description": "Importer un produit sur vos wishlists Whataboon",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon_toolbar.png",
      "38": "icon_toolbar@2x.png"
    },
    "default_title": "Boon It"
  },
  "permissions": [
    "*://*/*",
    "notifications",
    "cookies",
    "tabs",
    "storage",
    "contextMenus",
    "background"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
