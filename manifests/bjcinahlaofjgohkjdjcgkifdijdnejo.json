{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Critical Mix Navigation Buttons",
  "version": "1.0",
  "description": "Adds 'First', 'Previous', 'Next' and 'Last' buttons to Escapist Magazine's Critical Mix comic",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.escapistmagazine.com/*/critical-mix-*",
        "*://*.escapistmagazine.com/*/critical-miss-*"
      ],
      "js": [
        "js/contentScript.js"
      ]
    }
  ]
}
