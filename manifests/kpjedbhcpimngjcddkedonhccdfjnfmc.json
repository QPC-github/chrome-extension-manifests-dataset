{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "kafanzeratul.css"
      ],
      "matches": [
        "http://*.kafan.cn/*"
      ],
      "run_at": "document_start"
    },
    {
      "css": [
        "kafandomain.css"
      ],
      "matches": [
        "http://*.kafan.cn/search*",
        "http://u.kafan.cn/*"
      ],
      "run_at": "document_end"
    },
    {
      "css": [
        "pcbeta.css"
      ],
      "matches": [
        "http://*.pcbeta.com/*"
      ]
    }
  ],
  "converted_from_user_script": true,
  "description": "全新的浏览体验-卡饭论坛样式",
  "manifest_version": 2,
  "icons": {
    "128": "ico.png"
  },
  "name": "Kafan zeratul",
  "version": "0.2"
}
