{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zerocarbon: consumo consciente",
  "version": "3.0",
  "description": "Zere o impacto ambiental de suas compras atrav√©s da ZeroCarbon",
  "permissions": [
    "declarativeContent",
    "unlimitedStorage",
    "storage",
    "tabs",
    "<all_urls>",
    "activeTab",
    "https://www.zerocarbon.com.br/*"
  ],
  "background": {
    "scripts": [
      "scripts/background.js",
      "scripts/jquery-3.5.1.min.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "https://www.zerocarbon.com.br/carregando*"
      ],
      "all_frames": true,
      "js": [
        "scripts/jquery-3.5.1.min.js",
        "scripts/content.js"
      ],
      "css": [
        "styles/content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {},
  "icons": {
    "64": "images/logo-chrome.png"
  },
  "manifest_version": 2
}
