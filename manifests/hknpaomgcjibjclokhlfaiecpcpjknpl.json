{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "'View Image' Button",
  "description": "Restores the 'View Images' button when browsing images on Google Images.",
  "version": "0.1.1",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://google.com/*",
        "*://*.google.com/*"
      ],
      "css": [],
      "js": [
        "inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "fixButtons.js"
  ],
  "permissions": [
    "activeTab"
  ]
}
