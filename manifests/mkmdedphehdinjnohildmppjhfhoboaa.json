{
  "manifest_version": 2,
  "author": "Tony Serum",
  "name": "Niko Bull TV - Alerte Twitch Live",
  "description": "Permet de notifier l'utilisateur Ã  l'aide d'une popup ainsi qu'une notification sonore ...",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.5.1.1",
  "background": {
    "scripts": [
      "js/jquery-2.2.3.min.js",
      "js/parameters.js",
      "js/background.js",
      "js/chrome.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_off.png",
    "default_popup": "popup.html",
    "default_title": "Niko Bull - Hors Ligne"
  },
  "icons": {
    "256": "ressources/img/icons/logo256.png",
    "128": "ressources/img/icons/logo128.png",
    "64": "ressources/img/icons/logo64.png",
    "32": "ressources/img/icons/logo32.png"
  },
  "permissions": [
    "https://api.twitch.tv/",
    "https://twitch.tv/",
    "notifications",
    "storage",
    "contextMenus",
    "background"
  ],
  "options_page": "options/options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
