{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ship It Button",
  "description": "Adds a ship it button to GitHub pull requests",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*/pull/*"
      ],
      "js": [
        "ship_it.js"
      ]
    }
  ]
}
