{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "createView.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "arrowLeft.png",
    "arrowRight.png"
  ],
  "manifest_version": 2,
  "name": "BookmarkER",
  "description": "Bookmark your favourite pages to show up on the bottom of every page.",
  "version": "0.2.0"
}
