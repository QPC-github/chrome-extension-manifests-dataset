{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "海词词典",
  "description": "Dict.cn Extension",
  "version": "1.0",
  "permissions": [
    "http://dict.cn/",
    "http://*.dict.cn/*",
    "http://cdn.dfile.cn/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "js/dict.min.js",
    "js/dplayer.min.js",
    "js/encrypt.js",
    "js/jquery-1.8.0.min.js",
    "js/player.swf"
  ]
}
