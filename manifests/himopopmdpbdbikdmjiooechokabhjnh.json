{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "The Paw-ry Party",
  "short_name": "The Paw-ry Party",
  "version": "1.2",
  "description": "Fed up of having to see Boris Johnson's face everyday? Look no further...",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "author": "Impression Digital",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "32": "images/32X32_Icon.png",
    "48": "images/48X48_Icon.png",
    "64": "images/64X64_Icon.png",
    "128": "images/128X128_Icon.png"
  },
  "web_accessible_resources": [
    "*.jpg",
    "*.jpeg",
    "*.JPG",
    ".png",
    "."
  ],
  "homepage_url": "https://www.impression.co.uk/boris/"
}
