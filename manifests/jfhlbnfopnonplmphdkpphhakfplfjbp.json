{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Image Tools",
  "version": "1.1",
  "description": "Adds several image related context menu items.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
