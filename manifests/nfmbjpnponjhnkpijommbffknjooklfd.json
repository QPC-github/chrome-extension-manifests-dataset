{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MyBlocks",
  "version": "0.0.0.4",
  "description": "Help Scratchers search and reuse custom blocks",
  "page_action": {
    "default_popup": "index.html",
    "default_title": "MyBlocks Search"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icons/scratch_lib16.png",
    "32": "icons/scratch_lib32.png",
    "48": "icons/scratch_lib48.png",
    "128": "icons/scratch_lib128.png"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "https://my-blocks.appspot.com/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'"
}
