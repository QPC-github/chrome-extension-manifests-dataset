{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "uri-form-fill",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Auto-fill form fields by parsing an inline variable",
  "icons": {
    "128": "icons/fill_form.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Fill form from URL",
    "default_icon": "icons/fill_form.png"
  },
  "permissions": [
    "activeTab"
  ]
}
