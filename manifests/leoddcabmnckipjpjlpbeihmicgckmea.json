{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Linkedin Spinner",
  "version": "1.3",
  "description": "Spin Linkedin!",
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "js": [
        "imgSpinner.js"
      ],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_popup": "popupui.html",
    "default_icon": {
      "128": "linkedin-128.png"
    }
  },
  "icons": {
    "128": "linkedin-128.png"
  },
  "manifest_version": 2
}
