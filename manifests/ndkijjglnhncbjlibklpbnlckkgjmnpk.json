{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quantify - Influencer Marketing on Instagram",
  "short_name": "Quantify",
  "description": "Get data insights, audience and stories from any Instagram, TikTok or YouTube profile. Organize your Influencer Marketing campaigns.",
  "version": "1.1.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon_19.png",
      "38": "icon_38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.instagram.com/*",
        "https://*.youtube.com/*",
        "https://*.woomio.com/*",
        "https://*.tiktok.com/*"
      ],
      "js": [
        "jquery-1.11.2.min.js",
        "main.js",
        "helpers.js"
      ],
      "css": [
        "assets/css/font-awesome.min.css"
      ]
    }
  ],
  "permissions": [
    "https://*.instagram.com/*",
    "https://*.youtube.com/*",
    "https://*.tiktok.com/*"
  ],
  "web_accessible_resources": [
    "icon_48.png",
    "icon_38.png",
    "logo.svg",
    "download.png",
    "assets/fonts/*",
    "builds/*",
    "helpers.js"
  ]
}
