{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CompuSystems ADP Reminder",
  "version": "0.3",
  "description": "This is a reminder application for filling ADP.",
  "permissions": [
    "background",
    "tts",
    "notifications",
    "tabs"
  ],
  "short_name": "CompuSystems Reminder",
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "clock-19.png",
    "default_title": "CompuSystems ADP Reminder",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "clock-16.png",
    "48": "clock-48.png",
    "128": "clock-128.png",
    "256": "clock-256.png"
  },
  "manifest_version": 2
}
