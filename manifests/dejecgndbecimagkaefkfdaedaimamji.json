{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Harvest: Take Notes and Learn Passively",
  "short_name": "Harvest",
  "description": "Harvest lets you take notes from anything you read, then learn and remember them using a memory algorithm called spaced repetition.",
  "version": "0.3.3.2",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": "harvest-icon.png"
  },
  "permissions": [
    "identity",
    "activeTab",
    "storage",
    "https://api.harvest.li/"
  ],
  "oauth2": {
    "client_id": "834341968470-mti73k2h23kp92mm3jkcotv98mpkrt8r.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
