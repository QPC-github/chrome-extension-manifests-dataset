{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Edvidi Student",
  "short_name": "Edvidi Student",
  "description": "Edvidi Student",
  "version": "184",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "oauth2": {
    "client_id": "188834418196-9ank8vno3qlhc0c8urcpoq41n8p6qc7o.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "openid",
      "https://www.googleapis.com/auth/classroom.courses.readonly",
      "https://www.googleapis.com/auth/classroom.rosters.readonly"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main/index.html",
    "default_title": "Edvidi Student"
  },
  "permissions": [
    "<all_urls>",
    "identity",
    "identity.email",
    "history",
    "storage",
    "tabs"
