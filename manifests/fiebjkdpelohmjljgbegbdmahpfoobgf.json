{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keep-a-Note",
  "description": "Take notes while using the web and keep them in your Google Drive.",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "24": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "background": {
    "scripts": [
      "js/jquery-3.1.1.min.js",
      "js/background.js"
    ]
  },
  "homepage_url": "https://keepanote.com/gmail/",
  "browser_action": {
    "default_icon": {
      "128": "icons/logo.png"
    },
    "default_title": "Keep-a-Note",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "https://keepanote.com/*",
    "cookies"
  ]
}
