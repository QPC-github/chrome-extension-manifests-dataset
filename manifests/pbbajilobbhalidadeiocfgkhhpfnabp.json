{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Teladoc Meet",
  "description": "Telado Meeting extenion!",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo-teladoc-16-16.png",
    "48": "logo-teladoc-48-48.png",
    "128": "logo-teladoc-128-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://tdrmeet.spitter.chat/*"
    ]
  }
}
