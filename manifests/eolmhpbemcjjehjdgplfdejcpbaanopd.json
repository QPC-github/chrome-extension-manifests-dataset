{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "id": "com.qrcode.assistant",
  "request_api_version": 1,
  "author": "Huitao",
  "show_icon": true,
  "default_locale": "en",
  "name": "__MSG_title__",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon.png"
  },
  "homepage_url": "http://tool.huitaoyouhui.com",
  "version": "1.0.2",
  "description": "__MSG_description___",
  "web_accessible_resources": [
    "content.css"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "*://*/*",
    "<all_urls>",
    "contentSettings",
    "tabs"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "__MSG_title__",
    "default_popup": "popup.html"
  }
}
