{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scroll To Bottom",
  "version": "0.0.0.1",
  "description": "This extension will help you scroll to bottom",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Scroll To Bottom of The Page",
    "default_icon": {
      "16": "images/scrollToBottom.png",
      "24": "images/scrollToBottom.png",
      "32": "images/scrollToBottom.png"
    }
  },
  "icons": {
    "16": "images/scrollToBottom.png",
    "32": "images/scrollToBottom.png",
    "48": "images/scrollToBottom.png",
    "128": "images/scrollToBottom.png"
  },
  "manifest_version": 2
}
