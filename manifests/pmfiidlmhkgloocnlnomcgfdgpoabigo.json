{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "adBlock.js"
      ],
      "matches": [
        "http://www.canalplus.fr/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Supprimez les pubs canal",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "web_accessible_resources": [
    "ressources/*"
  ],
  "manifest_version": 2,
  "name": "CanalBlock",
  "permissions": [
    "*://static.canal-plus.net/pub/*",
    "*://broadcast.pm/*",
    "*://*.piximedia.fr/*",
    "*://*.2mdn.net/*",
    "*://www.wat.tv/get/externalpubhigh/*",
    "*://slpubmedias.tf1.fr/*",
    "*://*.fwmrm.net/*",
    "*://*.nuggad.net/*",
    "*://*.revsci.net/*",
    "*://openad.tf1.fr/*",
    "*://*.churccivic.com/*",
    "*://*.adsafeprotected.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.2"
}
