{
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/logo38.png",
    "default_popup": "popup/popup.html",
    "default_title": "Lose forme Cash Back Button"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.9.1.min.js",
        "popup/templates.js"
      ],
      "matches": [
        "http://loganlobes.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Lose forme-Hot Deals, Top Stores, Your Account Information and Cash Back are all just one click away.",
  "homepage_url": "http://lessforme.com/",
  "icons": {
    "128": "img/LessForMe_128.png",
    "16": "img/LessForMe_16.png",
    "18": "img/logo_18.png",
    "32": "img/logo_32.png",
    "48": "img/LessForMe_48.png",
    "64": "img/LessForMe_64.png"
  },
  "manifest_version": 2,
  "name": "Lose forme",
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "webNavigation",
    "contextMenus",
    "storage",
    "cookies",
    "http://loganlobes.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.7.0.2",
  "web_accessible_resources": [
    "www.lessforme.com/*",
    "img/*",
    "popup/notif.html"
  ]
}
