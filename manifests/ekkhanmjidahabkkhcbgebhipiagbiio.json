{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Craigslisting Research",
  "author": "Ethan Weeks",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Craigslist Buyers & Sellers! Easily evaluate your product market with graphs and other data visualizations.",
  "permissions": [
    "declarativeContent",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/clr_16px.png",
      "32": "img/clr_32px.png",
      "48": "img/clr_48px.png",
      "128": "img/clr_128px.png"
    }
  },
  "icons": {
    "16": "img/clr_16px.png",
    "32": "img/clr_32px.png",
    "48": "img/clr_48px.png",
    "128": "img/clr_128px.png"
  },
  "options_page": "options.html"
}
