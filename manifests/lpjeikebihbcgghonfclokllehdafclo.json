{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SessionTabs",
  "description": "Collect and save websites that are important for specific activity, to reopen later",
  "version": "0.05",
  "author": "Allan Soria",
  "background": {
    "scripts": [
      "script/backgd.js"
    ],
    "persistent": false
  },
  "content_security_policy": " script-src 'self' https://ajax.googleapis.com ; object-src 'self' ",
  "browser_action": {
    "default_title": "Have a good day",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "<all_urls>"
  ]
}
