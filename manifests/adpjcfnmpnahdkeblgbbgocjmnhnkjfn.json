{
  "author": "Marinache Radu Viorel",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon64.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "keep.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Utility for Oracle Support Engineers",
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "manifest_version": 2,
  "name": "Fusion Tables",
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "clipboardWrite",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.1.5"
}
