{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tracker Blocker",
  "version": "0.1",
  "manifest_version": 2,
  "description": "_(:з」∠)_",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/psl.min.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_popup": "blocker.html",
    "default_icon": {
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "128": "icon/128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "track.js"
      ]
    }
  ]
}
