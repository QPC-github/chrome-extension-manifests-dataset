{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VT Beer Trail",
  "short_name": "VT Beer Trail",
  "description": "Explore and Discover The World of Vermont Craft Beer",
  "version": "1.0.1",
  "author": "MooseDog Studios: Robin Hoover",
  "homepage_url": "http://www.moosedog.io",
  "icons": {
    "38": "images/icon_38.png",
    "64": "images/icon_64.png",
    "128": "images/icon_128.png",
    "256": "images/icon_256.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "images/icon_38.png",
      "64": "images/icon_64.png",
      "128": "images/icon_128.png",
      "256": "images/icon_256.png"
    },
    "default_title": "VT Beer Trail",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://maps.googleapis.com/",
    "https://maps.gstatic.com/",
    "https://google-analytics.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://google-analytics.com/analytics.js https://maps.googleapis.com/maps/api/js?true=false&language=en&callback=initMap https://maps.gstatic.com/maps-api-v3/api/js/19/3/main.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/common.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/map.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/util.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/marker.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/infowindow.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/stats.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/controls.js https://maps.gstatic.com/maps-api-v3/api/js/19/3/onion.js; object-src 'self' "
}
