{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CFCA CryptoKit.bytter Extension",
  "version": "3.4.0.6",
  "description": "CFCA Security Application Development Kit",
  "author": "CFCA",
  "minimum_chrome_version": "31",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "http://ca.t2.bytter.com/*",
      "*://*.qiejf.cn/*",
      "http://funds-test.longi-silicon.com/*",
      "*://bt.csc-sz.com/*",
      "https://funds.longi-silicon.com/*"
    ]
  }
}
