{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Construtor de vendas - Gestor Plus",
  "short_name": "CV - Gestor Plus",
  "version": "1.5",
  "description": "Acesse as informações do CONSTRUTOR DE VENDAS sem precisar abrir uma Nova aba no seu navegador",
  "icons": {
    "16": "icons/ico16.png",
    "48": "icons/ico48.png",
    "128": "icons/ico128.png"
  },
  "browser_action": {
    "default_icon": "icons/ico16.png",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": [
      "eventPage.js",
      "jquery.min.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "tabs",
    "notifications",
    "alarms",
    "storage",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "icons/avatar.png"
  ],
  "manifest_version": 2
}
