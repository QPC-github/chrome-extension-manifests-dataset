{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CvMaster - accelerate talent sourcing",
  "version": "1.0",
  "description": "Find your ultimate talent, parse the PDF profile and be the first to complete the hiring process! (by www.jobmster.co.il)",
  "permissions": [
    "activeTab",
    "cookies",
    "https://www.linkedin.com/*",
    "https://cv.jobmaster.co.il/*"
  ],
  "browser_action": {
    "default_title": "cvMaster",
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "64": "images/64.png",
      "128": "images/128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
