{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick History",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "history"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": false
  }
}
