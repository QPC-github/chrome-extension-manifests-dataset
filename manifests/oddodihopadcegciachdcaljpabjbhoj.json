{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Plus Real Time G+ Adjuster",
  "version": "5.0.2",
  "manifest_version": 2,
  "description": "Change the look of G+",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "69": "images/icon69.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://plus.google.com/*"
      ],
      "css": [
        "css/gplus_styles.css"
      ]
    }
  ]
}
