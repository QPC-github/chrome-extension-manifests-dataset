{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabSearch",
  "short_name": "TabSearch",
  "version": "3.1.1",
  "manifest_version": 2,
  "description": "TabSearch, uber search extension for Tableau Server & Online. Created by Craig Bloodworth (The Information Lab), powered by Tableau",
  "author": "Craig Bloodworth, The Information Lab",
  "icons": {
    "16": "img/tabsearch16.png",
    "48": "img/tabsearch48.png",
    "128": "img/tabsearch128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/bin/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/tabsearch128.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "tabs",
    "cookies",
    "gcm"
  ],
  "offline_enabled": false
}
