{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Custom Table Filter",
  "version": "1.0",
  "description": "Custom Table Filter!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "https://confluence.newegg.org/*",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/CTF_128.png"
    }
  },
  "icons": {
    "128": "images/CTF_128.png"
  },
  "manifest_version": 2
}
