{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "thinkery",
  "version": "1.22",
  "description": "Easily add URLs, text selections and things to thinkery. Your brain, organized.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Add to thinkery"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "homepage_url": "http://thinkery.me",
  "icons": {
    "16": "img/16.png",
    "24": "img/24.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "bookmarks",
    "contextMenus"
  ]
}
