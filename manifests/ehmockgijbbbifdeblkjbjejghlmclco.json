{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trello Pluso",
  "version": "1.1.2",
  "homepage_url": "http://www.DivanDesign.ru/",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.trello.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jQuery-3.2.1.min.js",
        "jQuery.ddTools-2.0.min.js",
        "general.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_title": "Trello Pluso"
  },
  "permissions": [
    "https://*.trello.com/*"
  ],
  "web_accessible_resources": [
    "fonts/*.*"
  ]
}
