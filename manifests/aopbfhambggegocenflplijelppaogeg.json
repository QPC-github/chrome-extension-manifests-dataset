{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VNotebooks Launcher",
  "description": "Launcher for VNotebooks app",
  "version": "0.0.1.4",
  "permissions": [
    "management",
    "tabs",
    "activeTab",
    "<all_urls>",
    "pageCapture",
    "downloads",
    "identity"
  ],
  "icons": {
    "128": "vnb.png"
  },
  "background": {
    "scripts": [
      "vnb_launcher.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "vnb.png",
    "default_title": "VNotebooks Launcher"
  },
  "manifest_version": 2
}
