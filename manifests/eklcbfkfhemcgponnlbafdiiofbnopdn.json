{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Search Console Full Width View",
  "version": "1.1.2",
  "description": "View Google Search Console at full width",
  "short_name": "GSC Full Width",
  "icons": {
    "16": "icon3-16.png",
    "48": "icon3-48.png",
    "128": "icon3-128.png"
  },
  "browser_action": {
    "default_icon": "icon3-128.png"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "https://search.google.com/*"
      ]
    }
  ]
}
