{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Redditly",
  "version": "1.7.1",
  "description": "Search reddit from Chrome with built in quicklinks",
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png",
    "16": "img/icon_16.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Redditly",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
