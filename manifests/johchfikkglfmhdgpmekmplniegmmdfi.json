{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Domain Grabber",
  "description": "Ever type in a domain that doesn't exist? Now get forwarded to a place to buy it!",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "short_name": "Domain Grabber",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "webNavigation",
    "<all_urls>",
    "tabs"
  ]
}
