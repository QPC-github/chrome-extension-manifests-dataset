{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IAmSpartacus",
  "short_name": "iamspartacus",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "128": "spartacus128.png",
    "48": "spartacus48.png",
    "16": "spartacus16.png"
  }
}
