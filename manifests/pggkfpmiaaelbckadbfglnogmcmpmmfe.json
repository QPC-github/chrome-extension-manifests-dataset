{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "G:Gist - Create Github Gist",
  "version": "1.5.1",
  "icons": {
    "16": "img/icon16px.png",
    "48": "img/icon48px.png",
    "128": "img/icon128px.png"
  },
  "manifest_version": 2,
  "description": "G:Gist from Lab1521.com. Create Github Gist Chrome Extension.",
  "browser_action": {
    "default_icon": "img/icon19px.png",
    "default_popup": "popup.html"
  },
  "background": "background.html",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "chrome://favicon/",
    "clipboardRead",
    "contextMenus",
    "notifications",
    "http://lab1521.com/ggist/*",
    "https://api.github.com/gists",
    "https://github.com/login/oauth/access_token"
  ]
}
