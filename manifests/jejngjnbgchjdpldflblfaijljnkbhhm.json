{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "capturetext.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "*://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "description": "__MSG_app_description__",
  "name": "__MSG_app_name__",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "ftp://*/*",
    "*://*/*"
  ],
  "version": "2.0.1"
}
