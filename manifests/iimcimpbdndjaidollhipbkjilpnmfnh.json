{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Course Hero Search Button",
  "short_name": "CH Search",
  "description": "A convenient button for quickly searching millions of Course Hero study resources from anywhere on the web!",
  "version": "1.0.8",
  "icons": {
    "128": "img/ch-extension-logo-128.png"
  },
  "homepage_url": "https://www.coursehero.com/",
  "background": {
    "scripts": [
      "js/jquery-2.1.3.min.js",
      "js/common.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/ch-extension-logo-19.png",
    "default_popup": "popup.html",
    "default_title": "Course Hero Search Button"
  },
  "permissions": [
    "contextMenus"
  ],
  "offline_enabled": false
}
