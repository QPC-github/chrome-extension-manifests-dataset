{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "EE Search Docs",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "EE Search Docs replaces the old google search with the new EllisLab.com google search.",
  "icons": {
    "16": "search-16.png",
    "48": "search-48.png",
    "128": "search-128.png"
  },
  "permissions": [
    "tabs",
    "http://ellislab.com/*"
  ],
  "page_action": {
    "default_icon": "searchon.png",
    "default_title": "EE Seach Docs"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
