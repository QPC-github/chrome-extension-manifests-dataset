{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DifBux AdAlert",
  "description": "This extension notifies you when there are new advertisements from DifBux available",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
