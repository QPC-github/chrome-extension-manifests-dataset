{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name": "Juan Ramón Jimenez Quick WordPress reader",
  "version": "1.0",
  "description": "Accede rápido a los post en juanramonjimenez.es además te notificará cuando haya nuevo contenido",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Juan Ramón Quick WordPress reader",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "alarms",
    "http://juanramonjimenez.es/"
  ],
  "background": {
    "scripts": [
      "alarms.js"
    ]
  }
}
