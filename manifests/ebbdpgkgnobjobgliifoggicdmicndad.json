{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JioSaavn Controller",
  "description": "Control JioSaavn playlist without Navigating to the tab.",
  "short_name": "REMOTE",
  "version": "1.7",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*.jiosaavn.com/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/i-16.png",
      "24": "icons/i-24.png",
      "32": "icons/i-32.png",
      "48": "icons/i-48.png",
      "128": "icons/i-128.png",
      "256": "icons/i-256.png"
    },
    "default_title": "JioSaavn PlayList Remote",
    "default_popup": "popup/ak1.html"
  },
  "icons": {
    "16": "icons/i-16.png",
    "24": "icons/i-24.png",
    "32": "icons/i-32.png",
    "48": "icons/i-48.png",
    "128": "icons/i-128.png",
    "256": "icons/i-256.png"
  },
  "manifest_version": 2
}
