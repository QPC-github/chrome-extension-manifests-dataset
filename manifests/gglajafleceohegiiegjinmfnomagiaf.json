{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Servibox Descarga CFDI SAT",
  "description": "Descarga todos los CFDIs en la busqueda del porta SAT",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "downloads",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icono_servibox.png"
  },
  "manifest_version": 2,
  "icons": {
    "128": "icono_servibox.png"
  }
}
