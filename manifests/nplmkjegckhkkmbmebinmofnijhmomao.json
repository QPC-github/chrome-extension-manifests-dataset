{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TexAgs Fix Text Area",
  "version": "1.0.4",
  "description": "Fix the text area width",
  "icons": {
    "128": "texagsicon.jpeg",
    "48": "taicon.jpeg"
  },
  "content_scripts": [
    {
      "js": [
        "texagsfixtextarea.user.js"
      ],
      "matches": [
        "http://www.texags.com/*",
        "https://www.texags.com/*",
        "http://texags.com/*",
        "https://texags.com/*",
        "http://*.texags.com/*",
        "https://*.texags.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "short_name": "TAFTA"
}
