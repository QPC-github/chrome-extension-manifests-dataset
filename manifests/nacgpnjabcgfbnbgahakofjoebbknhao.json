{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Stop Backspace key shortcut taking you back to previous page.",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "name": "Backstay",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "3.0.1"
}
