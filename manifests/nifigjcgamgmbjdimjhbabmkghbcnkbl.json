{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabShifting",
  "description": "It is time to take control of all your Tabs.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "todo.html",
    "default_icon": "icon128.png",
    "default_title": "TabShifting"
  }
}
