{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FORDATA ClickOnce Helper for Chrome",
  "version": "1.4",
  "key": "",
  "manifest_version": 2,
  "description": "Umożliwia komunikację z komponentami FORDATA / Enables communication with FORDATA components",
  "icons": {
    "16": "icon-16.png",
    "24": "icon-24.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "96": "icon-96.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.fordata.pl/*",
        "https://fordata-vdr.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "https://*.fordata.pl/*",
    "https://fordata-vdr.com/*"
  ]
}
