{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Universe to Christ",
  "description": "Replaces the text 'The Universe' with 'Christ', as well as 'Universe' with 'Christ'.",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
