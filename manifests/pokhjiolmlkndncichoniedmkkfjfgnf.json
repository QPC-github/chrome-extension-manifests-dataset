{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dhruv New Tab",
  "description": "This extension gives you a new tab with a calendar and notes, perfect for every tab!",
  "version": "0.2",
  "incognito": "split",
  "popup_action": {
    "default_icon": "eagle_logo_2x.jpeg"
  },
  "chrome_url_overrides": {
    "newtab": "background.html"
  },
  "permissions": [
    "identity",
    "identity.email"
  ]
}
