{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "search in a youtube video",
  "description": "search in a video",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "script": "popup.js"
  },
  "content_security_policy": "script-src * ",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
