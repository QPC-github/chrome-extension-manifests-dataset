{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "48": "icon2.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "name": "Binance cryptocurrency price alerts",
  "description": "Set price alerts with sound on binance with ease",
  "version": "2.3",
  "options_page": "options.html",
  "author": "kaleem hunzai",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://api.binance.com/api/v1/ticker/allPrices",
    "https://www.binance.com/en/trade/pro/*"
  ]
}
