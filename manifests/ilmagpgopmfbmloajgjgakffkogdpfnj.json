{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Minesweeper Extension",
  "version": "2.0.15",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "128": "icon_128.png"
  },
  "description": "Minesweeper Extension",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://m1ccs.free.fr/*"
  ],
  "browser_action": {
    "default_title": "Minesweeper Extension",
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
