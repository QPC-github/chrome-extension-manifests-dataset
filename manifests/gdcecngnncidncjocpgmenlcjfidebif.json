{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AngelfonMin",
  "version": "0.0.7",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/script.js",
        "js/jquery.min.js"
      ]
    }
  ],
  "description": "A veces s√≥lo necesitas enviar un mensaje, descarga Angelfon para Chrome",
  "homepage_url": "http://www.angelfon.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Angelfon Min",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "https://dev.angelfon.com/chrome/*",
    "https://corporate.angelfon.com/images/*",
    "https://min.angelfon.com/*",
    "https://127.0.0.1:80/*",
    "contextMenus",
    "tabs"
  ]
}
