{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Log Marker",
  "version": "0.0.0.7",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab",
    "file://*/*"
  ],
  "background": {
    "scripts": [
      "search.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  }
}
