{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "jquery.min.js",
        "SlamControls.user.js"
      ],
      "matches": [
        "http://comeonandsl.am/*",
        "https://comeonandsl.am/*"
      ],
      "css": [
        "SlamControls.css"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "description": "Adds controls to http://comeonandsl.am",
  "name": "SlamControls",
  "version": "1"
}
