{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to Zinthu",
  "short_name": "Zinthu",
  "description": "Save and Share links and notes with Team Members. Zinthu is an information sharing and knowledge management application.",
  "version": "1.1.14",
  "version_name": "1.1.14",
  "default_locale": "en",
  "icons": {
    "19": "img/zinthuicon19.png",
    "38": "img/zinthuicon38.png",
    "48": "img/zinthuicon48.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "img/zinthuicon19.png",
      "38": "img/zinthuicon38.png"
    },
    "default_title": "Save to Zinthu DEV",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "https://zinthu.com/*",
    "http://www.zinthu.com/*",
    "https://www.zinthu.com/*"
  ]
}
