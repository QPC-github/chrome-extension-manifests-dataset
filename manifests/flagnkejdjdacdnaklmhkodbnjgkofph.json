{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TeamCity Playlist",
  "version": "1.5.3",
  "description": "Adds a button to TeamCity test results and investigations page to download failed tests to a Visual Studio playlist",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
