{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Anti-zon",
  "version": "1.0.2",
  "description": "Randomizing links on Amazon allowing for random product exploration.",
  "homepage_url": "http://arts445.courses.bengrosser.com/",
  "manifest_version": 2,
  "icons": {
    "16": "antizon-16.jpg",
    "32": "antizon-32.jpg",
    "48": "antizon-48.jpg",
    "64": "antizon-64.jpg",
    "128": "antizon-128.jpg",
    "256": "antizon-256.jpg",
    "512": "antizon-512.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.amazon.com/*"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "css": [
        "basic-extension.css"
      ],
      "js": [
        "jquery.min.js",
        "ready.js",
        "basic-extension.user.js"
      ]
    }
  ]
}
