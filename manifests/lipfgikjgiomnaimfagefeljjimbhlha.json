{
  "author": "Alexander Sheleheda",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.inactive.16.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_extname__"
  },
  "content_scripts": [
    {
      "matches": [
        "https://rapidcatch.com/platform/setting/*"
      ],
      "js": [
        "jquery.js",
        "noty.js",
        "synchronize.js"
      ]
    }
  ],
  "default_locale": "en",
  "description": "__MSG_chrome_extension_description__",
  "homepage_url": "https://rapidcatch.com/",
  "icons": {
    "128": "icon.active.128.png",
    "16": "icon.active.16.png",
    "48": "icon.active.48.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extname__",
  "offline_enabled": false,
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "cookies",
    "https://rapidcatch.com/*",
    "*://*.ebay.com/*",
    "*://*.ebay.co.uk/*",
    "*://*.ebay.de/*",
    "*://*.ebay.fr/*",
    "*://*.ebay.it/*",
    "https://*.delcampe.net/*",
    "https://aukro.cz/*",
    "https://allegro.pl/*",
    "https://meshok.net/*",
    "https://auction.ru/*",
    "https://newauction.com.ua/*"
  ],
  "short_name": "Rapidcatch",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.13"
}
