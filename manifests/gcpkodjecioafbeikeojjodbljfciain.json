{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Favoris (add favorite) - Favinbook.com",
  "description": "Gestionnaire de favoris intelligent - Intelligent favorites manager - Favinbook.com",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "img/icon_16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon_128.png",
    "64": "img/icon_64.png",
    "32": "img/icon_32.png",
    "16": "img/icon_16.png"
  }
}
