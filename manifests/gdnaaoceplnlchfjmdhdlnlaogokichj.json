{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Usabili.me Notification Extension",
  "author": "Agility Feat, Inc.",
  "version": "1.1",
  "description": "This extension notify when a user is connected to a session",
  "homepage_url": "https://www.usabili.me/",
  "minimum_chrome_version": "34",
  "permissions": [
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "http://localhost:3000/*",
      "*://usabili.me/*",
      "*://usabilime-staging.herokuapp.com/*",
      "*://usabilime-testing.herokuapp.com/*"
    ]
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
