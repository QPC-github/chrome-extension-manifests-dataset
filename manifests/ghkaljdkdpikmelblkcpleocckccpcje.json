{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scouting Den Hoorn Sponsorkliks",
  "version": "1.2",
  "description": "Verdien eenvoudig geld voor scouting Den Hoorn door Sponsorkliks automatisch te gebruiken!",
  "icons": {
    "16": "icons/scoutingdenhoorn16.png",
    "48": "icons/scoutingdenhoorn48.png",
    "128": "icons/scoutingdenhoorn128.png"
  },
  "author": "Daan van Vuuren",
  "content_scripts": [
    {
      "js": [
        "shops.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
