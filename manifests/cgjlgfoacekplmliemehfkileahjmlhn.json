{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unblock Erowid",
  "description": "Regain access to erowid.org if it's blocked in your country.",
  "version": "1.0.1",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://erowid.org/*",
    "*://www.erowid.org/*",
    "webRequestBlocking"
  ]
}
