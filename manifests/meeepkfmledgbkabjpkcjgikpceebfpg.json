{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "iSearch Tab",
  "version": "1.0.0",
  "homepage_url": "http://www.isearchtab.com",
  "description": "Reset your New Tab Page to iSearch Tab",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "38": "icon38.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/values.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon38.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "manifest_version": 2
}
