{
  "version": "2.7",
  "icons": {
    "128": "icn/128х128.png"
  },
  "browser_action": {
    "default_icon": "icn/128х128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "src/assets/backgound.js"
    ]
  },
  "description": "__MSG_appDesc__",
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_appName__",
  "content_security_policy": "connect-src *; script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
