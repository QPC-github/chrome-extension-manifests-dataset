{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Goslash Image Downloader",
  "description": "Goslash Product Images Downloader.",
  "version": "1.0",
  "author": "Varun Developer",
  "icons": {
    "48": "images/get_started48.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
