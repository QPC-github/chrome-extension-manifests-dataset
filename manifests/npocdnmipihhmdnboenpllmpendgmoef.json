{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background_page": "background.html",
  "description": "Esta extensão substitui todas as ocorrências do termo 'Galicia' por 'Galiza'; via projeto @galiza",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Galizator",
  "version": "0.0.0.1"
}
