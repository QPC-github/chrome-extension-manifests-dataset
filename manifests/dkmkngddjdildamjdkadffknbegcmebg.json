{
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "jquery.min.js",
        "cscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Jediná možnost, jak číst zpravodajské servery.",
  "icons": {
    "128": "logo128.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "name": "ZemanBlock",
  "permissions": [
    "http://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4"
}
