{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bitcoin Ticker",
  "description": "Valores do bitcoin em tempo real de compra e venda das últimas negociações",
  "version": "1.3",
  "browser_action": {
    "default_icon": "bitcoin_badge.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
