{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Focus on Issues",
  "version": "1",
  "description": "Focus the issues search bar by typing / at github.com/issues",
  "permissions": [
    "*://github.com/issues*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/issues*"
      ],
      "js": [
        "focus.js"
      ]
    }
  ]
}
