{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LetsBzzzzzzzzz",
  "description": "Appends some quick buttons like bz, dice or bobei for Plurk.",
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.plurk.com/*",
        "https://www.plurk.com/*"
      ],
      "js": [
        "jquery-1.7.min.js",
        "app.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
