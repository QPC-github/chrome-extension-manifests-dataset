{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "One Click Login",
  "manifest_version": 2,
  "version": "0.2",
  "description": "Store your login details at one place. So, you can easily login via single click",
  "icons": {
    "16": "P16.png",
    "48": "P48.png",
    "128": "P48.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "P.png"
  },
  "short_name": "OCL",
  "author": "Ankit Kumar Gupta",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
