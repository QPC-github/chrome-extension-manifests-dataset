{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VMware Horizon Browser Redirection",
  "version": "7.12.0.1",
  "version_name": "7.12.0.1",
  "description": "Chrome extension to enable Browser Tab Redirection inside VMware Horizon Agent.",
  "key": "",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-16.png"
  }
}
