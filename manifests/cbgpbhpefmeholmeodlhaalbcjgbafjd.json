{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Timebox++",
  "description": "Master your time the visual way!",
  "version": "1.2.4",
  "offline_enabled": true,
  "icons": {
    "16": "icon/icon-16.png",
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon-19.png",
      "38": "icon/icon-38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Timebox++"
  },
  "background": {
    "scripts": [
      "js/config.js",
      "js/fn.js",
      "js/storage-local.js",
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications"
  ]
}
