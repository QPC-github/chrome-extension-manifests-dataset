{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup2.html",
    "default_title": "*Dashboard for Chrome*"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "A convinient Side-by-side Notepad and Calculator with autosave",
  "icons": {
    "128": "icon.png",
    "32": "icon.png"
  },
  "name": "*Dashboard for Chrome*",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "homepage_url": "http://dealfinder.online/",
  "manifest_version": 2,
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://dealfinder.online/*"
    ]
  },
  "version": "0.1.53"
}
