{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Maxabout",
  "version": "5.1",
  "description": "Maxabout Chrome Extension",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Maxabout",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "Maxabout",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
