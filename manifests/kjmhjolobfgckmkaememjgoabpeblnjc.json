{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Game Ended",
  "description": "Replaces all instances of 'kill' with 'game end'",
  "version": "1.0",
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
