{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WP Admin Extension",
  "description": "redirect to site's wordpress admin page",
  "version": "0.0.0.1",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "core.js"
    ]
  }
}
