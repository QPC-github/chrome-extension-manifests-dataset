{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "en",
  "name": "FindZilla",
  "description": "__MSG_extension_description__",
  "version": "0.5.2",
  "manifest_version": 2,
  "minimum_chrome_version": "26",
  "icons": {
    "16": "images/icon16.png",
    "19": "images/icon19.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "options_page": "html/options.html",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_title": "__MSG_toggle_browser_action_title__ (__MSG_toggle_hotkey_prefix____MSG_toggle_hotkey_default__)",
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "commands": {
    "findzilla-toggle": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "__MSG_toggle_command_description__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "lib/jquery-ui/css/ui-darkness/jquery-ui-1.10.2.custom.min.css",
        "css/findbar.css"
      ],
      "js": [
        "lib/jquery-1.9.1.min.js",
        "lib/jquery-ui/js/jquery-ui-1.10.2.custom.min.js",
        "js/common.js",
        "js/findwalker.js",
        "js/findbar.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "lib/jquery-ui/css/ui-darkness/images/animated-overlay.gif",
    "lib/jquery-ui/css/ui-darkness/images/ui-icons_ffffff_256x240.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-icons_cccccc_256x240.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-icons_a83300_256x240.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-icons_222222_256x240.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-icons_4b8e0b_256x240.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_inset-soft_30_f58400_1x100.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_inset-soft_25_000000_1x100.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_highlight-soft_80_eeeeee_1x100.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_gloss-wave_25_333333_500x100.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_glass_40_ffc73d_1x400.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_glass_40_0078a3_1x400.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_glass_20_555555_1x400.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_flat_50_5c5c5c_40x100.png",
    "lib/jquery-ui/css/ui-darkness/images/ui-bg_flat_30_cccccc_40x100.png"
  ]
}
