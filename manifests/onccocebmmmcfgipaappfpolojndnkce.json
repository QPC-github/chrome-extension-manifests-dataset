{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Disqus Downvote Exposer",
  "description": "This extension shows the number of downvotes of Disqus comments.",
  "version": "1.0",
  "author": "Yunku Daniel Kang",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*.disqus.com/*",
        "*://disqus.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
