{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GamdomTweak",
  "description": "Creates a custom dark theme for Gamdom",
  "version": "1.1.5",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "*://gamdom.com/*"
  ],
  "web_accessible_resources": [
    "images/rain1.gif",
    "images/snow.gif",
    "images/spring.gif",
    "images/summer.gif",
    "images/fall.gif",
    "images/yt.png",
    "images/mod.png",
    "images/admin.png",
    "images/sparkle.gif",
    "images/g_joker.svg",
    "images/joker.svg",
    "images/green.png"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "background": {
    "scripts": [
      "load.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://gamdom.com/*"
      ],
      "all_frames": true,
      "css": [
        "gam.css"
      ]
    }
  ],
  "manifest_version": 2
}
