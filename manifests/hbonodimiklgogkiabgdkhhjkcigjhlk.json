{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Closed Tabs History",
  "version": "0.0.0.1",
  "description": "Displays recently closed tabs and allows to restore them.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon 19.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "sessions"
  ],
  "manifest_version": 2
}
