{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Ebay for the Color Blind",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.ebay.com/*"
