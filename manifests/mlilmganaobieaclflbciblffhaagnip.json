{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "name": "PHP Docs-to-go",
  "description": "This is the complete php manual including user notes that works 100% OFFLINE.",
  "version": "2.0.0.2",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "op"
  },
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "management",
    "tabs"
  ],
  "icons": {
    "16": "icons/128.png",
    "48": "icons/128.png",
    "128": "icons/128.png"
  }
}
