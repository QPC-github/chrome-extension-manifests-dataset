{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Slice Tube",
  "version": "1.01",
  "description": "Cut youtube into embedded links to send to your friends!",
  "permissions": [
    "storage",
    "tabs",
    "clipboardWrite",
    "clipboardRead"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "manifest_version": 2
}
