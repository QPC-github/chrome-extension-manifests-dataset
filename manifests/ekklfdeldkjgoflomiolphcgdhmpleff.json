{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Niche Planner",
  "version": "1.0.1",
  "description": "Optimization tool for dropshipping",
  "permissions": [
    "activeTab",
    "cookies",
    "https://nicheplanner.io/*",
    "https://ads.google.com/*"
  ],
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo.png",
      "24": "images/logo.png",
      "32": "images/logo.png"
    },
    "default_title": "Niche Planner",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
