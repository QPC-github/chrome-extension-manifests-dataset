{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Micro Learn - Language",
  "version": "1.0",
  "description": "Open a new tab and learn a new Language. One word per tab",
  "manifest_version": 2,
  "permissions": [
    "management"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "micro-learn.html"
  }
}
