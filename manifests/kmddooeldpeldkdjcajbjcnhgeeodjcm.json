{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Tracking",
  "version": "1.1",
  "description": "Time Tracking The very best extension to track time you spend on sites",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/config.js",
      "js/sites.js",
      "js/tracker.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "alarms",
    "idle",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Time Tracking",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "19": "images/icon19.png",
    "38": "images/icon38.png",
    "128": "images/icon.png"
  }
}
