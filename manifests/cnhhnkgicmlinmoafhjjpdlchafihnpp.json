{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Add a Tab",
  "version": "1",
  "manifest_version": 2,
  "description": "A start page that gives you the top social media and top shopping pages right on your browser.",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "offline_enabled": true,
  "background": {
    "page": "background.html"
  },
  "chrome_url_overrides": {
    "newtab": "search.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://s3.amazonaws.com; object-src 'self'"
}
