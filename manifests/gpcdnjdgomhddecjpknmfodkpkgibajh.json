{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "图流-看图助手",
  "version": "1.3.8",
  "manifest_version": 2,
  "homepage_url": "http://tuliu.aacc.in/",
  "description": "图流,美女写真,简单看图,愉悦心情.图片瀑布流展示,photo in masonry,SimpSee,草榴看图插件,草榴看图助手, 自定义规则看图",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "图流"
  },
  "background": {
    "scripts": [
      "dist/background.entry.js"
    ]
  },
  "optional_permissions": [
    "tabs",
    "<all_urls>"
  ],
  "permissions": [
    "storage",
    "notifications",
    "activeTab"
  ],
  "options_page": "option/index.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://pingjs.qq.com https://www.zhihu.com; object-src 'self' https://www.zhihu.com;"
}
