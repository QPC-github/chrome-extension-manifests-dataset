{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extNameBm__",
  "author": "ACCS Consulting Group",
  "version": "0.5",
  "manifest_version": 2,
  "default_locale": "en",
  "minimum_chrome_version": "34",
  "description": "__MSG_extDescriptionBm__",
  "homepage_url": "https://user.best-mtg.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.best-mtg.local/*",
      "https://*.best-mtg.co/*",
      "https://*.best-mtg.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
