{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "구매요정",
  "version": "1.1",
  "description": "미국의 패션을 집앞으로 가져다주는 마법! 해외 구매대행 서비스, 구매요정.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/gmyz16.png",
      "32": "images/gmyz32.png",
      "48": "images/gmyz48.png",
      "64": "images/gmyz64.png",
      "128": "images/gmyz128.png"
    }
  },
  "icons": {
    "16": "images/gmyz16.png",
    "32": "images/gmyz32.png",
    "48": "images/gmyz48.png",
    "64": "images/gmyz64.png",
    "128": "images/gmyz128.png"
  },
  "manifest_version": 2
}
