{
  "applications": {
    "gecko": {
      "id": "mushway@leomelki.fr",
      "update_url": "https://leomelki.fr/mushway/extension/api/updates.json"
    }
  },
  "author": "leomelki",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/logo_128_off.png",
    "default_popup": "html/popup.html",
    "default_title": "Mushway"
  },
  "content_scripts": [
    {
      "js": [
        "js/extension_present.js"
      ],
      "matches": [
        "*://mushway.leomelki.fr/*"
      ]
    }
  ],
  "description": "Extension de Mushway. Notifie lors des lives et des sorties de vid√©os.",
  "homepage_url": "https://leomelki.fr",
  "icons": {
    "128": "images/logo_128.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Mushway",
  "permissions": [
    "tabs",
    "background",
    "notifications",
    "storage",
    "https://i.ytimg.com/*",
    "https://www.googleapis.com/*",
    "https://leomelki.fr/mushway/extension/api/*"
  ],
  "short_name": "Mushway",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.4"
}
