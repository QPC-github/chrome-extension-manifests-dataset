{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keyboard Navigator",
  "version": "0.4",
  "description": "Use your keyboard to follow defined links on page like next google results",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "22.0",
  "icons": {
    "48": "img/shortcut-48.png",
    "128": "img/shortcut-128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
