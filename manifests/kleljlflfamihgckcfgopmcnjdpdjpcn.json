{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Send reminder to group invites for Facebookâ„¢",
  "version": "2020.09.28",
  "manifest_version": 2,
  "web_accessible_resources": [
    "content.css",
    "fonts/roboto-latin-400.woff2",
    "fonts/roboto-latin-700.woff2",
    "screen_help.jpg",
    "screen_help2.jpg"
