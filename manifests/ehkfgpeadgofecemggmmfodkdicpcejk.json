{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Traffic Map Moscow",
  "version": "1.5",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Traffic Map Moscow",
    "default_popup": "popup.html"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "manifest_version": 2
}
