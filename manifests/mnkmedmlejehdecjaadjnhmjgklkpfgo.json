{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Screencastify Video Editor",
  "short_name": "Screencastify Video Editor",
  "description": "Browser-based video editor with the simplicity you expect from Screencastify. Integrates with Google Drive.",
  "version": "0.2.2",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "container": [
    "DOMAIN_INSTALLABLE",
    "GOOGLE_DRIVE"
  ],
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "video/mp4",
          "video/webm",
          "video/x-matroska",
          "video/x-msvideo",
          "video/quicktime",
          "video/mpeg"
        ],
        "href": "https://editor.screencastify.com/",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "api_console_project_id": "242262968495",
  "app": {
    "urls": [
      "https://editor.screencastify.com/"
    ],
    "launch": {
      "web_url": "https://editor.screencastify.com/"
    }
  }
}
