{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kaitlyn Happiness Dashboard",
  "version": "1.0",
  "description": "This Chrome extension gives the user happiness each time they open a new tab",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ],
  "icons": {
    "128": "assets/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon19.png"
  }
}
