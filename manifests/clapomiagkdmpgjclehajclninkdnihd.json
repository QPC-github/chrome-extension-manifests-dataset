{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open bookmark folder",
  "description": "To use, type 'ob' plus a folder name into the Omnibox. ",
  "version": "0.9",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "bookmarks"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "ob"
  },
  "manifest_version": 2
}
