{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Walmart Price Watch",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Walmart Price Watch",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
