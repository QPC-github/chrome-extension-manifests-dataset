{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GPA-me",
  "description": "A simple extension for editting your un-submitted grades to see what your GPA will be!",
  "version": "4",
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": [
        "gpa-me.css"
      ],
      "matches": [
        "https://sis.jhu.edu/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "options_page": "options.html"
}
