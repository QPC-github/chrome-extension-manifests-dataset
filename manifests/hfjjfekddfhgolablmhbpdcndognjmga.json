{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Beautify LH",
  "version": "0.91",
  "description": "Improves the look and feel of HBS Learning Hub.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://lh.hbs.edu/*"
      ],
      "js": [
        "jquery.js",
        "beautify.js"
      ],
      "css": [
        "beautify.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    "img/hbs-logo.png",
    "img/icon-home.png",
    "img/arrow-up.png",
    "img/arrow-up-down.png",
    "img/icon-calendar.png",
    "img/icon-module.png",
    "img/icon-bookmark.png",
    "img/icon-close.png",
    "img/icon-plus.png",
    "img/icon-plus-dark.png",
    "img/icon-pencil.png",
    "img/icon-arrow-back.png",
    "img/icon-folder.png",
    "img/icon-chevron-left.png",
    "img/icon-chevron-right.png",
    "img/icon-link.png",
    "img/ui-icons_888888_256x240.png"
  ],
  "manifest_version": 2
}
