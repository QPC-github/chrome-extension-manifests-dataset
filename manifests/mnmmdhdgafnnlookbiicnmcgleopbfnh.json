{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "english anime list",
  "version": "1.0",
  "manifest_version": 2,
  "description": "shows english titles on myanimelist.net/animelist",
  "content_scripts": [
    {
      "matches": [
        "http://myanimelist.net/animelist/*"
      ],
      "js": [
        "js/jquery-2.1.1.min.js",
        "js/inject.js"
      ]
    }
  ]
}
