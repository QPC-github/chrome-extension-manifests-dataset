{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Insider",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Search for a content/word in a YouTube video and start the video from that time-stamp.",
  "browser_action": {
    "default_icon": "find.png",
    "default_title": "YouTube Insider",
    "default_popup": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.goggle.com; object-src 'self'",
  "icons": {
    "128": "find.png"
  },
  "permissions": [
    "identity"
  ],
  "oauth2": {
    "client_id": "852034405999-d1k3emaau8i5r9bsfqqkhouogl473kd5.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  }
}
