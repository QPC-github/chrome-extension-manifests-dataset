{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Text Splitter",
  "description": "テキストを指定の区切り文字で分割してGoogleスプレッドシートにコピー＆ペースト可能な形式に変換する拡張機能です。",
  "version": "1.0.0",
  "icons": {
    "128": "images/icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon/icon_128.png"
    },
    "default_title": "Text Splitter",
    "default_popup": "popup.html"
  }
}
