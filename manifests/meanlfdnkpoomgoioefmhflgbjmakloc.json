{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "EDT",
  "background": {
    "scripts": [
      "js/global.js",
      "js/jquery.js",
      "js/utils.js",
      "js/cookie.js",
      "js/synchronization.js",
      "js/background.js"
    ]
  },
  "description": "驾校通过使用“同步助手”插件，可实现驾校同步学员数据，提升驾校管理效率。",
  "externally_connectable": {
    "matches": [
      "*://*.122.gov.cn/*"
    ]
  },
  "icons": {
    "128": "images/jxedt.png",
    "48": "images/jxedt.png"
  },
  "manifest_version": 2,
  "name": "同步助手",
  "page_action": {
    "default_icon": "images/jxedt.png",
    "default_popup": "popup.html",
    "default_title": "同步助手"
  },
  "permissions": [
    "cookies",
    "tabs",
    "notifications",
    "webRequest",
    "*://*.122.gov.cn/*",
    "unlimitedStorage"
  ],
  "version": "1.0.1"
}
