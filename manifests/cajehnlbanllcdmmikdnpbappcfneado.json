{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "mp4 Viewer for SFDC Chatter",
  "description": "This extension allows mp4 files to be viewed directly within Chatter in SFDC.",
  "version": "1.0",
  "icons": {
    "16": "mp4-16.png",
    "48": "mp4-48.png",
    "128": "mp4-128.png"
  },
  "permissions": [
    "https://*.salesforce.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.salesforce.com/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ]
}
