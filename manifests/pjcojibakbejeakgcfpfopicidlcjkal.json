{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Teach Me Now Screen Sharing",
  "version": "1.6",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.teachmenow.com/*",
      "*://*.getbee.com/*",
      "*://*.akademiet.no/*",
      "*://*.medacademyonline.com/*",
      "*://*.norwegianteaching.com/*",
      "*://*.note.no/*",
      "*://*.quduedu.com/*",
      "*://*.drsamaaelabd.com/*",
      "*://*.edtechsolutions.org/*",
      "*://*.exclusivementor.com/*",
      "*://*.theamyhrvold.com/*",
      "*://*.selkar.me/*",
      "*://*.2blimitless.com/*",
      "*://*.novomed.com/*",
      "*://*.gemstutor.com/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
