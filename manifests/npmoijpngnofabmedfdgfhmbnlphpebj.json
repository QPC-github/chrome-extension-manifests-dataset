{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cheap Hotels in USA",
  "manifest_version": 2,
  "short_name": "Cheap Hotels USA",
  "description": "Book the Best Hotels with cheapest rate in USA",
  "version": "2.8",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "browser_action": {
    "default_icon": "hotels1.png",
    "default_title": "Cheap Hotels USA",
    "default_popup": "chromiflash.html"
  },
  "icons": {
    "128": "hotels3.png"
  }
}
