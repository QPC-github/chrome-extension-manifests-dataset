{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "微拍视频下载器",
  "version": "1.1",
  "description": "微拍视频下载器",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "http://weipai.cn/",
    "http://www.weipai.cn/",
    "http://weipai.com/",
    "http://www.weipai.com/"
  ],
  "web_accessible_resources": [
    "jquery.js",
    "down.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://weipai.cn/*",
        "http://www.weipai.cn/*",
        "http://weipai.com/*",
        "http://www.weipai.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}
