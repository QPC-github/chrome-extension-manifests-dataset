{
  "name": "Speed Dial Pro",
  "version": "1.2.3",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "content_security_policy": "script-src 'self'  'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "management",
    "geolocation",
    "history",
    "chrome://favicon/",
    "http://*/",
    "https://*/",
    "bookmarks",
    "tabs",
    "contextMenus",
    "notifications",
    "topSites",
    "unlimitedStorage",
    "cookies"
  ],
  "content_scripts": [],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png"
  }
}
