{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ciao Extension",
  "description": "Ciao Extension",
  "version": "1.0.3",
  "manifest_version": 2,
  "icons": {
    "128": "ciao.ico"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.voiceloco.com/*",
        "https://*.dot-connect.io/*"
      ],
      "all_frames": true,
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "desktopCapture",
    "tabs",
    "app",
    "https://*.voiceloco.com/*",
    "https://*.dot-connect.io/*"
  ]
}
