{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDesc__",
  "version": "1.3",
  "default_locale": "en",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "__MSG_appTitle__"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "cookies",
    "*://*.yummytabs.com/",
    "storage"
  ],
  "optional_permissions": [
    "topSites"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/jquery.min.js",
        "js/newtab.js"
      ],
      "matches": [
        "*://*.yummytabs.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
