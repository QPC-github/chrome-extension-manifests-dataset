{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Greenlight Extension",
  "description": "This extension shows a description from Greenlight company",
  "version": "1.5",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "index.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*",
    "https://www.google.com/*",
    "http://www.google.com/*"
  ]
}
