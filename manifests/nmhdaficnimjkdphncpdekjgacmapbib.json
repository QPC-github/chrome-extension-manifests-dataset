{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "SECURITE LOGICIEL",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/applogogrey.png",
      "38": "images/applogogrey48.png"
    },
    "default_title": "Site-Officiel by 3cbSecure : Indicateur de confiance",
    "default_popup": "popup.html"
  },
  "description": "Accès facile au site officiel pour chaque démarche !\n\n3cbSecure vous précise la sécurité (SSL) des sites avant vos paiements CB.",
  "icons": {
    "16": "images/applogogrey.png",
    "48": "images/applogogrey48.png",
    "128": "images/applogogrey128.png"
  },
  "manifest_version": 2,
  "name": "Site-Officiel by 3cbSecure",
  "permissions": [
    "cookies",
    "notifications",
    "contextMenus",
    "tabs",
    "unlimitedStorage",
    "storage",
    "http://*/",
    "https://*/",
    "background",
    "management"
  ],
  "version": "1.0.3"
}
