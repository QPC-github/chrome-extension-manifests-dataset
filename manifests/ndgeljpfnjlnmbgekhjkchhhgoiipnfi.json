{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Discover® Deals and Cashback Rewards Notifier",
  "version": "5.3.0",
  "version_name": "v5.3.0",
  "description": "Shows a notification when visiting sites that qualify for Discover® Deals or Cashback Rewards!",
  "icons": {
    "38": "icon/prod-icon-38.png",
    "512": "icon/prod-icon-512.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "No Discover Deal for this page",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "tabs",
    "alarms",
    "https://www.tejunareddy.com/discover-rewards-notifier/*"
  ],
  "short_name": "Discover® Rewards Notifier",
  "offline_enabled": true,
  "manifest_version": 2
}
