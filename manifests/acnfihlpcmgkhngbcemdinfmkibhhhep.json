{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "applications": {
    "gecko": {
      "id": "archiver@menu.com",
      "strict_min_version": "63.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "64": "icons/icon1-64.png"
    }
  },
  "default_locale": "en",
  "description": "__MSG_extensionDesc__",
  "icons": {
    "64": "icons/icon1-64.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+0"
      },
      "description": "shortcut for browser action button "
    }
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "options_ui": {
    "browser_style": false,
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "contextMenus",
    "bookmarks",
    "webNavigation",
    "notifications",
    "webRequest",
    "downloads",
    "webRequestBlocking",
    "clipboardWrite",
    "pageCapture",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cse.google.com/  https://www.google.com/;  object-src 'self'",
  "short_name": "Page Archiver",
  "version": "2.0.5"
}
