{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Dogecoin Dashboard by PiQue",
  "description": "Display Dogecoin Currency Information",
  "version": "0.1.31",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "[dev]Dogecoin Dashboard by PiQue",
    "default_icon": "dogecoin16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "dogecoin16.png",
    "128": "dogecoin128.png"
  },
  "permissions": [
    "tabs",
    "http://pique.co.za/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
