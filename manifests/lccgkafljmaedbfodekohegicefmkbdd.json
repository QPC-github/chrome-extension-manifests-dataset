{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "assets/scripts/deskmetrics.js",
      "assets/scripts/search/searchUtil.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://get-haiku.com/assets/images/iconHaiku-16.png",
      "is_default": true,
      "keyword": "haiku",
      "name": "Haiku, Search Powered by Bing",
      "search_url": "http://get-haiku.com/search?q={searchTerms}&PCSF=SU_DEFAULT",
      "suggest_url": "http://get-haiku.com/suggest?prefix={searchTerms}&PCSF=SU_SUGGEST"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://query.yahooapis.com; script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Receive a daily Haiku, gorgeous imagery, weather, search and more on your new tab page.",
  "icons": {
    "128": "assets/images/iconHaiku-128.png",
    "16": "assets/images/iconHaiku-16.png",
    "48": "assets/images/iconHaiku-48.png"
  },
  "manifest_version": 2,
  "name": "Haiku, Search Powered by Bing",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "geolocation",
    "tabs",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.0.9"
}
