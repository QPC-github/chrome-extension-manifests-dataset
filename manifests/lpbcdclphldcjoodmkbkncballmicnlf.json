{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.1",
  "manifest_version": 2,
  "name": "__MSG_name__",
  "short_name": "__MSG_shortName__",
  "description": "__MSG_description__",
  "default_locale": "fi",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*",
      "*://*.dna.fi:*/*"
    ]
  }
}
