{
  "author": "IQBot",
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_popup": "poup.html",
    "default_title": "IQBot (IQOption robot)"
  },
  "content_scripts": [
    {
      "js": [
        "ot.js",
        "sweetalert.min.js"
      ],
      "css": [
        "css/sweetalert.css"
      ],
      "matches": [
        "https://iqoption.com/*"
      ]
    }
  ],
  "description": "Robot for IQ Option (iqoption trading bot)",
  "manifest_version": 2,
  "name": "IQBot (IQOption robot)",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "cookies",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4"
}
