{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SubFire Mini Player for Subsonic",
  "short_name": "SubFire Mini",
  "description": "Chrome extension launcher for the SubFire Mini/Mobile players, a client for the Subsonic Media Streamer server.",
  "version": "2.3.0.0",
  "permissions": [
    "http://subfiresuite.com/",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "SubFire-016t.png",
    "48": "SubFire-048t.png",
    "128": "SubFire-128t.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "SubFire-019t.png"
  }
}
