{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blinkk",
  "description": "Extension to keep your eyes healthy",
  "version": "1.0",
  "web_accessible_resources": [
    "*.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": [
      "blinkk.js"
    ]
  }
}
