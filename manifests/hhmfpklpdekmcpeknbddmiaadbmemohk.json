{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "On Page SEO by auutom.com",
  "version": "1.0.15",
  "description": "Show on page SEO for the first 7 pages in google search below the search bar for a specific keyword",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/search?*"
      ]
    }
  ],
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": {
      "38": "logo.png"
    }
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
