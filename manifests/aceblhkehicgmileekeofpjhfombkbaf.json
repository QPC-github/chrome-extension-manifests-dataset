{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NetEast Music Helper",
  "description": "Help you play music @music.163.com, even when you are abroad.",
  "version": "1.0",
  "permissions": [
    "http://music.163.com/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.music.163.com/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
