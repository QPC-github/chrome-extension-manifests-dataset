{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SteamProphet Ex",
  "description": "Make your SteamProphet vote from any steam related website",
  "version": "0.9.2",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "https://ajax.googleapis.com/",
    "http://steamspy.com/",
    "https://www.steamprophet.com/"
  ]
}
