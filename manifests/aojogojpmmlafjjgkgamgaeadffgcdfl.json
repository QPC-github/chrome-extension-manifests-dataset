{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spreadsheet Tabs On Top",
  "version": "1.0",
  "description": "Move Google Spreadsheet tabs to the top",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "content.css"
      ],
      "matches": [
        "http://docs.google.com/*",
        "https://docs.google.com/*"
      ]
    }
  ],
  "manifest_version": 2
}
