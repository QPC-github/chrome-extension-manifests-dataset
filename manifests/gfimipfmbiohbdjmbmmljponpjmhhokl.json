{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Mouse Gestures.",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "author": "KT",
  "name": "KT Mouse Gestures",
  "permissions": [
    "tabs",
    "history",
    "<all_urls>"
  ],
  "version": "17.8.8.0"
}
