{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cage Mode",
  "description": "This extension Cage's you, you love it, you embrace it, you are caged",
  "version": "1.0",
  "browser_action": {
    "default_icon": "cage.png",
    "default_title": "Cage Mode",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
