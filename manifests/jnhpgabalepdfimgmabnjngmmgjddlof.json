{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/magnify.png",
    "default_popup": "popup.html"
  },
  "description": "Zoom online text",
  "icons": {
    "128": "icons/magnify.png",
    "48": "icons/magnify.png",
    "16": "icons/magnify.png"
  },
  "manifest_version": 2,
  "author": "Dominix",
  "name": "Zoomer",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "notifications",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
