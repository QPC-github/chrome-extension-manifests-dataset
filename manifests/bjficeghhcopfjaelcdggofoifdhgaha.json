{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Go Adrina!",
  "description": "This extension allows the user to perform search.",
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": [
        "https://*.goadrina.com/*"
      ],
      "js": [
        "jquery.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
