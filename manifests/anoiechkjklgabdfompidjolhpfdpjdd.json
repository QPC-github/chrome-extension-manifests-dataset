{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Magio GO",
  "version": "4.0",
  "description": "Vychutnajte si televizny zazitok s Magio GO v prehliadaci Chrome",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*SamplePlayerPPAPI.html",
      "*://demo.visualon.info:*/*SamplePlayerPPAPI.html",
      "*://www.magiogo.sk/*",
      "*://magiogo.sk/*",
      "*://grep.magio.tv/*",
      "*://magio.tv/*",
      "*://www.magiogo.tv/*"
    ]
  }
}
