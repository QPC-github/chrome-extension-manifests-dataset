{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tree Page Paths",
  "version": "0.1",
  "description": "Simple tree for fast navigating by paths from root path.",
  "author": "Anton Krylov, https://github.com/Krylovsentry",
  "short_name": "Treepp",
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "Tree Page Paths",
    "default_icon": "images/tree-128.png",
    "default_popup": "popup.html"
  }
}
