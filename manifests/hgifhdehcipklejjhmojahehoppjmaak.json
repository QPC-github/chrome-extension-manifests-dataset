{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NetTvPlayTimer",
  "short_name": "APT",
  "version": "1.1",
  "manifest_version": 2,
  "description": "I will reserve playing of Net TV.",
  "permissions": [
    "alarms",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "NetTvPlayTimer_19.png",
    "default_title": "NetTvPlayTimer",
    "default_popup": "popup.html"
  }
}
