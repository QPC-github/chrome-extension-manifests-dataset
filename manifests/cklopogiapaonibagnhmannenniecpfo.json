{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Gautam Eswaran",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Clear Website Session"
  },
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "128": "images/icon-128.png",
    "16": "images/icon-16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "options.html",
  "permissions": [
    "cookies",
    "*://*/*",
    "contextMenus"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Period",
        "mac": "Command+Period",
        "chromeos": "Ctrl+Period",
        "linux": "Ctrl+Period"
      }
    },
    "clear-all-reload": {
      "suggested_key": {
        "default": "Ctrl+Shift+Period",
        "mac": "Command+Shift+Period",
        "chromeos": "Ctrl+Shift+Period",
        "linux": "Ctrl+Shift+Period"
      },
      "description": "Clear All + Reload page"
    },
    "clear-cookies": {
      "suggested_key": {
        "default": "Ctrl+Comma",
        "mac": "Command+Comma",
        "chromeos": "Ctrl+Comma",
        "linux": "Ctrl+Comma"
      },
      "description": "Clear only Cookies"
    },
    "clear-cookies-reload": {
      "suggested_key": {
        "default": "Ctrl+Shift+Comma",
        "mac": "Command+Shift+Comma",
        "chromeos": "Ctrl+Shift+Comma",
        "linux": "Ctrl+Shift+Comma"
      },
      "description": "Clear Cookies + Reload page"
    }
  },
  "minimum_chrome_version": "20",
  "version": "1.3.1"
}
