{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NeoHover",
  "manifest_version": 2,
  "version": "0.31",
  "description": "Hover over links to quoted NeoGAF posts to view their content",
  "content_scripts": [
    {
      "matches": [
        "http://www.neogaf.com/forum/*",
        "http://neogaf.com/*"
      ],
      "css": [
        "basic.css"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js"
      ]
    }
  ]
}
