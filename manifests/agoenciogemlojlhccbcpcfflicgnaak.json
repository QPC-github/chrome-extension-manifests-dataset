{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BIODIGITAL HUMAN",
  "description": "The BioDigital Human is a 3D platform that simplifies the understanding of anatomy, disease and treatments. Explore the body in 3D!",
  "version": "3.1.3",
  "manifest_version": 2,
  "app": {
    "urls": [
      "https://www.biodigital.com/"
    ],
    "launch": {
      "web_url": "https://www.biodigital.com"
    }
  },
  "icons": {
    "128": "icon_128_square.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
