{
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "minimum_chrome_version": "16.0",
  "incognito": "spanning",
  "options_page": "options.html",
  "homepage_url": "https://github.com/osdc/osdc-backpack-chrome",
  "version": "0.1",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "OSDC backpack",
  "name": "OSDC Backpack",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "contentSettings",
    "contextMenus",
    "debugger"
  ],
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "16": "img/icon16.png"
  },
  "manifest_version": 2
}
