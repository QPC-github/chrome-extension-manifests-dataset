{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google Button in Baidu",
  "description": "在百度搜索页面显示谷歌搜索按钮，一键谷歌搜索当前关键词。",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.baidu.com/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "main.js"
      ]
    }
  ]
}
