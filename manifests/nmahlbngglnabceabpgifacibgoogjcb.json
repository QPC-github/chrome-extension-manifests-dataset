{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IQDB Context",
  "description": "Right-click anime images to reverse-image-search for larger versions through IQDB.",
  "version": "0.6.1",
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "search_16.png",
    "48": "search_48.png",
    "128": "search_128.png"
  }
}
