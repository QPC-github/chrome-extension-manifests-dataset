{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickLink",
  "description": "To rozszerzenie zmienia dłuuuugie linki w krótkie. :3",
  "version": "1.17.4",
  "icons": {
    "16": "icons/ic_link_red_16px.png",
    "48": "icons/ic_link_red_48px.png",
    "128": "icons/ic_link_red_128px.png"
  },
  "browser_action": {
    "default_icon": "icons/ic_link_red_16px.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/jquery.min.js",
      "js/menu.js",
      "js/listener.js"
    ]
  },
  "permissions": [
    "clipboardWrite",
    "activeTab",
    "contextMenus",
    "http://nosiu.ml/"
  ]
}
