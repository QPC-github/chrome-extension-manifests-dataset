{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ExploraTodo",
  "version": "1.0.0",
  "description": "Haz que ExploraTodo sea tu p√°gina de inicio",
  "icons": {
    "16": "exploratodo-16x16.png",
    "48": "exploratodo-48x48.png",
    "128": "exploratodo-128x128.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.exploratodo.com",
    "search_provider": {
      "name": "ExploraTodo",
      "keyword": "busca",
      "search_url": "http://www.exploratodo.com/resultados?q={searchTerms}&t=web",
      "favicon_url": "http://www.exploratodo.com/exploratodo-16x16.png",
      "encoding": "UTF-8",
      "is_default": false
    },
    "startup_pages": [
      "http://www.exploratodo.com"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "exploratodo.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
