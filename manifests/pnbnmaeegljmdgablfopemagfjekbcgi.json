{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Time sheet for JIRA",
  "short_name": "Jira-TimeSheet",
  "description": "Gets the worklogs from the project, filter and group them by user and generate a few days report.",
  "version": "0.8.1",
  "permissions": [
    "http://*/",
    "https://*/",
    "storage",
    "notifications"
  ],
  "icons": {
    "128": "icons/icon128.png",
    "64": "icons/icon64.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Jira Timesheet",
    "default_popup": "jump.html"
  },
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
