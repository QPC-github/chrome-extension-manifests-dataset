{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CNET Autoplay Blocker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Prevent CNET from assaulting you with irrelevant videos",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://cnet.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.cnet.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ]
}
