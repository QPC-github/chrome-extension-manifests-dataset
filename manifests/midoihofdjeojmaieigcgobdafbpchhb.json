{
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html",
    "default_title": "Get QRcode for current URL"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Get QRcode for selection/page/image/link in a popup window.",
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "name": "QRPopup",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "manifest_version": 2,
  "version": "1.0"
}
