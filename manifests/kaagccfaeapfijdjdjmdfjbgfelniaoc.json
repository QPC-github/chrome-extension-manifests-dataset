{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Honey Tab Start Page",
  "version": "1.29.0",
  "manifest_version": 2,
  "description": "Customize your new tab page with your favorite site shortcuts. Hundreds of pre-installed shortcuts and you can create your own.",
  "homepage_url": "http://www.honeytabextension.com",
  "default_locale": "en",
  "browser_action": {
    "default_title": "Honey Tab Start Page"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "assets/js/jquery.min.js",
      "assets/js/lodash.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "geolocation",
    "history",
    "browsingData",
    "tabs",
    "chrome://favicon/"
  ],
  "icons": {
    "16": "assets/img/honey_tab_16x16.png",
    "32": "assets/img/honey_tab_32x32.png",
    "48": "assets/img/honey_tab_48x48.png",
    "128": "assets/img/honey_tab_128x128.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
    "**.jpg",
    "**.png",
    "**.html",
    "**.css",
    "**.js"
  ]
}
