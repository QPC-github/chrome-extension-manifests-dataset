{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "128": "public/img/icon128.png"
  },
  "default_locale": "en",
  "version": "1.5",
  "content_security_policy": "connect-src *; script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "manifest_version": 2,
  "description": "__MSG_appDesc__",
  "browser_action": {
    "default_icon": "public/img/icon128.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "name": "__MSG_appName__",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  }
}
