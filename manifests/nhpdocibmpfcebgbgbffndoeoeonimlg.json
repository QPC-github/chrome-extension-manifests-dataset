{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "My Serials",
  "version": "1.4.2",
  "manifest_version": 2,
  "description": "Extension for saving progress of seeing serials",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
