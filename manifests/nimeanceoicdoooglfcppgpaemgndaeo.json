{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RMLSweb improved",
  "version": "1.0.0",
  "description": "Improvements to the standard RMLSweb experience.",
  "permissions": [
    "http://www.redfin.com/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.rmlsweb.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
