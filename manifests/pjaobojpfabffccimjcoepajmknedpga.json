{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "M2 FPC Monitor",
  "short_name": "M2 FPC",
  "version": "1.0",
  "description": "Identify if a page was served by Magento FPC",
  "permissions": [
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2
}
