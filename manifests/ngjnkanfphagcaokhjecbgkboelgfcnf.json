{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Print",
  "version": "1.0.1",
  "description": "Print your current page.",
  "icons": {
    "72": "icon_black.png"
  },
  "browser_action": {
    "default_title": "Print",
    "default_icon": "icon_black.png"
  },
  "background": {
    "page": "print.html",
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}
