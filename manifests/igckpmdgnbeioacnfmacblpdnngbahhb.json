{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "What is my IP?",
  "description": "Easily find out your IP-Address",
  "version": "0.3",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "What is my IP?",
    "default_popup": "window.html"
  },
  "manifest_version": 2
}
