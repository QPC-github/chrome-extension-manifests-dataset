{
  "background": {
    "scripts": [
      "background.js",
      "js/ga.js"
    ]
  },
  "chrome_settings_overrides": {
    "homepage": "http://start.everything-starts.com?type=chhp&from=chext",
    "startup_pages": [
      "http://start.everything-starts.com/?type=chhp&from=chext"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/content.js",
        "js/ga.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://clients1.google.com; object-src 'self'",
  "default_locale": "en",
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "manifest_version": 2,
  "name": "__MSG_display_name__",
  "permissions": [
    "storage",
    "webRequest",
    "tabs",
    "unlimitedStorage",
    "chrome://favicon/",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.1.0",
  "web_accessible_resources": [
    "newtab.html"
  ]
}
