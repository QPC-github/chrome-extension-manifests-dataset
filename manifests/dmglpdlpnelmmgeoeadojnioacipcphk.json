{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TweetRater",
  "description": "Lets you rate tweets from your stream as valuable or pollution and keep a total score for each of the people you follow.",
  "version": "1.2",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "tweetrater.css"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "tweetrater.js"
      ]
    }
  ]
}
