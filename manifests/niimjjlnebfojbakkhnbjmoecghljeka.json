{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Barbora Kalorijos",
  "version": "1.0.3",
  "description": "Jei įmanoma, apskaičiuoja kiek kalorijų yra visoje pakuotėje arba aprašyme pateiktame svoryje.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.barbora.lt/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "128": "images/Barbora-Kalorijos-Logo.png"
  },
  "manifest_version": 2
}
