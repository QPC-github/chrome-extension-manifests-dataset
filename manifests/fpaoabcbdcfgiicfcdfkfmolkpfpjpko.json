{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Sponsormeter reminder",
  "version": "1.26.45",
  "description": "sponsormeter.com enables fundraising through online shopping.",
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "browser_action": {
    "default_icon": "images/default-icon.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "external/jquery.js",
      "external/custom-helper-functions.js",
      "background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "cookies",
    "notifications",
    "contextMenus",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage"
  ]
}
