{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "description": "Force Firefox User Agent for OWA",
  "manifest_version": 2,
  "name": "YAOWAO (Yet Another OWA Override",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "https://*/owa/*",
    "https://*/OWA/*"
  ],
  "version": "1.1"
}
