{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MettleTalent - FirstCall",
  "description": "A LinkedIn powered Google Chrome plugin that makes direct search candidate screening easy, for anyone",
  "background": {
    "scripts": [
      "chromeExtension.js"
    ]
  },
  "version": "3.0.8",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "MettleTalent-FirstCall"
  },
  "web_accessible_resources": [
    "recruitassist.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "webNavigation"
  ]
}
