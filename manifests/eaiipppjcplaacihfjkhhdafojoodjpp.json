{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Crypto Toolbox",
  "description": "Keep up with the world of cryptocurrencies.",
  "version": "1.1",
  "options_page": "options.html",
  "key": "",
  "icons": {
    "48": "icon_2048.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "socket.io.js",
      "ccc-streamer-utilities.js",
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "<all_urls>"
  ]
}
