{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Square Twits",
  "description": "css to make avatars square",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "squaretwits.css"
      ]
    }
  ]
}
