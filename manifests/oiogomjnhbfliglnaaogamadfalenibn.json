{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unwanted History: Filter your history",
  "short_name": "Unwanted History",
  "description": "Filter what gets saved into your history. For any questions or suggestions, please write to luisgrases@gmail.com.",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab",
    "history",
    "https://ajax.googleapis.com/"
  ]
}
