{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cipafilter Direct Authenticator BETA",
  "description": "Automatically authenticate with Cipafilter",
  "version": "0.0.8",
  "permissions": [
    "alarms",
    "background",
    "identity",
    "identity.email",
    "proxy",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/"
  ],
  "browser_action": {
    "name": "Triple click for debug"
  },
  "icons": {
    "16": "gfx/lockicon16.png",
    "48": "gfx/lockicon48.png",
    "128": "gfx/lockicon128.png"
  },
  "background": {
    "scripts": [
      "js/common.js",
      "js/background.js"
    ],
    "persistent": true
  }
}
