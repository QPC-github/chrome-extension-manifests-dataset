{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "default_locale": "es",
  "manifest_version": 2,
  "name": "Easy QR Code",
  "description": "__MSG_extDescription__",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "qr.js-master/qr.js",
      "main.js"
    ]
  }
}
