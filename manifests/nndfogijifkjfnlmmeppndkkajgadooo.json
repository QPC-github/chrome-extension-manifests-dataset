{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Develoption",
  "version": "0.0.1.1",
  "description": "Develoption",
  "browser_action": {
    "default_title": "Develoption",
    "default_icon": "icono.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardRead",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "control.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
