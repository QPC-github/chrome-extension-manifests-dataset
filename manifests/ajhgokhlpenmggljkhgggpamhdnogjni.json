{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Slack Overflow",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "An addon to disguise your reading materials in a stack overflow page",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://stackoverflow.com/*"
      ],
      "js": [
        "jquery-1.8.3.min.js",
        "slackoverflow.js"
      ]
    }
  ]
}
