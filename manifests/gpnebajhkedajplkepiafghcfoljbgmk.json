{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unblocker for YouTube",
  "description": "Free Unblocker for YouTube. This extension lets you unblock and watch YouTube if itâ€™s blocked by your ISP or network administrator.",
  "version": "2.5.0",
  "key": "",
  "icons": {
    "128": "youtube-on.png"
  },
  "browser_action": {
    "default_icon": "youtube.png",
    "default_popup": "app.html",
    "default_title": "Unblock YouTube by Wachee"
  },
  "background": {
    "scripts": [
      "libs.js",
      "buy.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "jquery.js",
        "content/contentScript.js"
      ],
      "css": [
        "content/contentStyle.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "logo@2x.png",
    "YouTubeUnblocker-Watermark.png",
    "close-ad.png",
    "wachee-ad.png",
    "fonts/Roboto/Roboto-Regular.ttf",
    "fonts/Roboto/Roboto-Bold.ttf",
    "content/ad.html"
  ],
  "offline_enabled": false,
  "permissions": [
    "*://*.youtube.com/*",
    "*://*.wachee.co/*",
    "proxy",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://code.jquery.com https://www.google-analytics.com; object-src 'self'"
}
