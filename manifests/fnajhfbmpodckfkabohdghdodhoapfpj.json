{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "G-NewsFeeds",
  "description": "日本のニュースが簡単に取得できる拡張アプリ",
  "browser_action": {
    "default_icon": "/image/logo.png",
    "default_title": "日本のニュースが表示できるクローム拡張",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "/image/logo.png"
  },
  "background": {},
  "permissions": [
    "tabs"
  ]
}
