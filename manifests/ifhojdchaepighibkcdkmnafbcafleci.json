{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Harvard Library URL",
  "version": "0.1.1",
  "description": "Adds .ezp-prod1.hul.harvard.edu to a URL",
  "background": {
    "scripts": [
      "urlchange.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
