{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "netflix.js"
      ],
      "matches": [
        "http://*.netflix.com/*",
        "https://*.netflix.com/*",
        "http://*.netflix.ca/*",
        "https://*.netflix.ca/*"
      ]
    }
  ],
  "author": "Matthew Lewis",
  "description": "This extension removes popup dialog on netflix asking to continue watching.",
  "manifest_version": 2,
  "name": "AllDayNetflix",
  "version": "1.0"
}
