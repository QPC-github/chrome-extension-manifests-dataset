{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Singleton",
  "short_name": "Search your tabs in a flash!",
  "description": "The quickest way to search your browser tabs.",
  "version": "0.0.5",
  "omnibox": {
    "keyword": "s"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background_snap.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/",
    "https://*/"
  ]
}
