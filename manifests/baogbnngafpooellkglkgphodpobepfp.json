{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK Editor",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "css": [
        "src/main.css"
      ],
      "js": [
        "src/main.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://vk.com/*"
  ]
}
