{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Up Work Test",
  "description": "This extension will show client name in upwork site.",
  "version": "3.9",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://www.upwork.com/*"
      ],
      "js": [
        "jquery-1.11.3.min.js",
        "script.js",
        "angular.min.js"
      ],
      "css": [
        "css/mycss.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*.png"
  ]
}
