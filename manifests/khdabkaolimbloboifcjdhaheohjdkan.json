{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lexulous Solitaire Word Game",
  "short_name": "Lexulous Solitaire Word Game",
  "version": "3.7",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/jquery-1.7.2.min.js",
        "js/validate.js",
        "js/activity.js",
        "main.js",
        "js/jquery-ui-1.8.21a.min.js",
        "js/LexulousPractice.js"
      ]
    }
  },
  "oauth2": {
    "client_id": "867466820168-fc7a3mtudu7g1dcd0i3hqg77mok4c90e.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "offline_enabled": false,
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "storage",
    "webview",
    "http://*/*",
    "https://*/*",
    "fileSystem",
    "gcm",
    "notifications",
    "identity"
  ]
}
