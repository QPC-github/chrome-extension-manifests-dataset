{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Equi Organica",
  "description": "Equi Organica App Chrome",
  "manifest_version": 2,
  "version": "2.9.8",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Programa O Evangelho",
    "default_icon": "AKEL_icon.png",
    "default_popup": "ExamplePage.html"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "icons": {
    "128": "AKEL_icon.png"
  }
}
