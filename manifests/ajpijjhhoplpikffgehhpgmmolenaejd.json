{
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "commons.min.js",
      "bg.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/cb-not-activated-icon.png",
    "default_popup": "index.html",
    "default_title": "__MSG_extDefaultTitle__"
  },
  "content_scripts": [
    {
      "css": [
        "css/serp.css"
      ],
      "include_globs": [
        "*://*.google.*/*",
        "*://*.yahoo.*/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "commons.min.js",
        "serp.min.js"
      ],
      "matches": [
        "*://*/*"
      ]
    },
    {
      "css": [
        "css/notification.css"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "commons.min.js",
        "notification.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://connect.facebook.net https://apis.google.com; object-src 'self'",
  "externally_connectable": {
    "matches": [
      "*://*.dubli.com/*",
      "*://*.bsp-rewards.com/*"
    ]
  },
  "icons": {
    "16": "images/cb-not-activated-icon.png",
    "32": "images/cb-not-activated-icon@2x.png",
    "48": "images/cb-not-activated-icon@3x.png",
    "128": "images/cb-not-activated-icon@128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "description": "__MSG_extDescription__",
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "storage",
    "<all_urls>",
    "cookies"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.5",
  "default_locale": "en"
}
