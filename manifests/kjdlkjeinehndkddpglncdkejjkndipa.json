{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Listar en compraymudate",
  "version": "4.0",
  "manifest_version": 2,
  "description": "Publique su propiedad a su cuenta en compraymudate",
  "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self';style-src 'unsafe-inline';",
  "web_accessible_resources": [
    "images/icon128.png",
    "images/icon48.png",
    "images/ajax2.gif",
    "images/bg-line.gif",
    "images/icon.png",
    "images/logo_house_mini.png"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://compraymudate.com/admin/apilogin",
    "http://compraymudate.com/admin/apipost",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Send to Compraymudate",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
