{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JuiceNote",
  "version": "2.2.0",
  "description": "Simplify the way you add notes in google chrome.",
  "icons": {
    "19": "skin/icon16.png",
    "48": "skin/icon48.png",
    "128": "skin/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "*://juicenote.com/*"
  ],
  "options_page": "html/options.htm",
  "browser_action": {
    "default_title": "new JNote",
    "default_icon": "skin/icon16.png",
    "default_popup": "html/popup.htm"
  }
}
