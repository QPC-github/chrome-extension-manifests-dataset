{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "pictugram",
  "version": "0.2.16",
  "manifest_version": 2,
  "description": "View Instagram images in a popup in any site that has a link to it.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
