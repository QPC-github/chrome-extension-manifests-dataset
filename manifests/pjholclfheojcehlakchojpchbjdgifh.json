{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CL AQA Tools",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "It will help you to generate playlist from Jenkins after automation execution, results will be generated based on Jenkins build",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "options_page": "options.html"
}
