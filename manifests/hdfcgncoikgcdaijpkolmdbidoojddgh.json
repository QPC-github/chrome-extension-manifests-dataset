{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Convert - Helper",
  "description": "Extens√£o da Lettel Data para ajustes",
  "version": "1.0.0",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icone.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": [
        "js/jquery.js",
        "js/viewer.min.js",
        "inject.js"
      ],
      "css": [
        "css/viewer.min.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
