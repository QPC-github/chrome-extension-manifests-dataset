{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Force=Horse",
  "short_name": "Force=Horse",
  "version": "0.3",
  "permissions": [
    "http://*.reddit.com/*",
    "https://*.google.com/*",
    "http://*.wikipedia.org/*",
    "http://*.nytimes.com/*",
    "http://*.wsj.com/*",
    "http://*.usatoday.com/*",
    "http://*.latimes.com/*",
    "http://*.washingtonpost.com/*",
    "http://*.nypost.com/*",
    "http://*.suntimes.com/*",
    "http://*.chicagotribune.com/*",
    "http://*.cnn.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.reddit.com/*",
        "https://*.google.com/*",
        "http://*.nytimes.com/*",
        "http://*.wsj.com/*",
        "http://*.usatoday.com/*",
        "http://*.latimes.com/*",
        "http://*.washingtonpost.com/*",
        "http://*.nypost.com/*",
        "http://*.suntimes.com/*",
        "http://*.chicagotribune.com/*",
        "http://edition.cnn.com/*"
      ],
      "js": [
        "popup.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
