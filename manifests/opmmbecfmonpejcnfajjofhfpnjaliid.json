{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tymeshift",
  "description": "",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "scripts/jquery-3.2.1.min.js",
      "scripts/eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}
