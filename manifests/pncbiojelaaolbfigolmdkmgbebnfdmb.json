{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bdword Dictionary",
  "version": "3",
  "permissions": [
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "bdword-all.js"
      ]
    }
  ],
  "browser_action": {
    "name": "bdword-all",
    "icons": [
      "icon.jpg"
    ]
  },
  "manifest_version": 2
}
