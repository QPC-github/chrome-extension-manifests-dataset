{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Middle Click To Go Back",
  "version": "1.8.1",
  "manifest_version": 2,
  "description": "Go back with your middle mouse button.",
  "icons": {
    "48": "back48.png",
    "128": "back128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "all_frames": true,
      "js": [
        "mctgb.js"
      ]
    }
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
