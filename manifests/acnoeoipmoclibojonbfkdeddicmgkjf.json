{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dagomys Weahter Now",
  "version": "1.5.4",
  "description": "Погода в Дагомысе",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "https://dagomysweather.ru/*"
  ],
  "browser_action": {
    "default_title": "Узнай погоду в Дагомысе",
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  }
}
