{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickLink newtab",
  "short_name": "Quicktab",
  "description": "This extension opens up a default New tab",
  "version": "1.0.5",
  "browser_action": {
    "default_icon": "images/icon.jpg",
    "default_title": "blank page"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "management"
  ]
}
