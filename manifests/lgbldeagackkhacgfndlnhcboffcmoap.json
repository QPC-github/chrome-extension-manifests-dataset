{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Imperial annotation",
  "description": "This extension try to annotate imperial units to metric units in webpage you visit.",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "killimperial.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
