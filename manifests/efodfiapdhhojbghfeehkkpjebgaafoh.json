{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "F t.co",
  "description": "This browser extension unwraps t.co links in Tweet Deck and Twitter.com, making your experience faster and less cumbersome",
  "version": "1.0.2",
  "icons": {
    "128": "ftco.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://*.twitter.com/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ]
}
