{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Authbee",
  "description": "Authenticate and Authorize",
  "version": "20.101.119.10356",
  "manifest_version": 2,
  "author": "Alappin",
  "homepage_url": "https://authbee.com",
  "key": "",
  "icons": {
    "16": "static/icons/icon_16@1x.png",
    "128": "static/icons/icon_128@1x.png"
  },
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "static/icons/icon_16@1x.png",
      "32": "static/icons/icon_32@1x.png"
    },
    "default_title": "Authbee",
    "default_popup": "pages/popup/index.html"
  }
}
