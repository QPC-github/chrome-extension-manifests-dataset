{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "public/js/jquery.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "public/css/fh.css"
      ],
      "js": [
        "public/js/jquery.js",
        "content.js"
      ],
      "matches": [
        "*://battlelog.battlefield.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Use this Chrome extension to keep track of hackers and manage your hacker list directly from Battlelog.",
  "homepage_url": "http://www.fuckinghackers.com",
  "icons": {},
  "manifest_version": 2,
  "minimum_chrome_version": "22.0",
  "name": "Fucking Hackers",
  "permissions": [
    "cookies",
    "tabs",
    "*://*.fuckinghackers.com/"
  ],
  "short_name": "fh",
  "version": "0.9.8"
}
