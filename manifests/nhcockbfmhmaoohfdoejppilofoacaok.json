{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Upper and Lowercase Conversion(大文字・小文字変換)",
  "manifest_version": 2,
  "description": "ウェブページに表示中の英字をその場で大文字と小文字に変換してテキストフィールドへ表示します",
  "icons": {
    "128": "uppercase-lowercase128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "search.js"
      ]
    }
  ],
  "version": "1.1"
}
