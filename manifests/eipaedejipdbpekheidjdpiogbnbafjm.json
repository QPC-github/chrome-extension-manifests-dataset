{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Collect UI Collector's Extension",
  "description": "Collect designs from Dribbble and Panda Dribbble Feed",
  "version": "0.1.6",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://dribbble.com/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "manifest_version": 2
}
