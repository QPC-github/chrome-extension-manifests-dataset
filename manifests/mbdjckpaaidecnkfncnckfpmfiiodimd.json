{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "takipcisatinal.org Haberleri",
  "description": "Bu uygulama Google Chrome üzerinden takipcisatinal.org haberlerini takip etmenizi sağlar.",
  "background": {
    "page": "background.html"
  },
  "default_locale": "tr",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "*://www.takipcisatinal.org/*",
    "*://takipcisatinal.org/*",
    "*://www.takipcisatinal.org*",
    "*://takipcisatinal.org*"
  ],
  "version": "1.0"
}
