{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mr. President",
  "version": "0.0.1",
  "description": "A browser extension that replaces all mentions of Donald Trump with \"The President of the United States of America\".",
  "author": "Tyler Argo",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
