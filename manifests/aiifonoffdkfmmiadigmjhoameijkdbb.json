{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Simple Drag & Drop Search",
  "description": "Simple Drag & Drop Search",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "l.png",
    "r.png",
    "u.png",
    "d.png",
    "lu.png",
    "ru.png",
    "ld.png",
    "rd.png",
    "s.png",
    "favicon/baidu.png",
    "favicon/bing.png",
    "favicon/duckduckgo.png",
    "favicon/flickr.png",
    "favicon/gmail.png",
    "favicon/google.png",
    "favicon/googlemaps.png",
    "favicon/imdb.png",
    "favicon/stackoverflow.png",
    "favicon/tumblr.png",
    "favicon/twitter.png",
    "favicon/wikipedia.png",
    "favicon/youtube.png",
    "favicon/amazon.png",
    "favicon/naver.png",
    "favicon/yandex.png",
    "favicon/yahoo.png",
    "favicon/hatena.png",
    "favicon/right.png",
    "favicon/clip.png",
    "favicon/vine.png",
    "favicon/pint.png",
    "favicon/reddit.png",
    "favicon/digg.png",
    "favicon/yanimg.png",
    "favicon/one.png",
    "favicon/az.png",
    "favicon/textfile.png",
    "favicon/memo.png",
    "favicon/speech.png"
  ],
  "permissions": [
    "tabs",
    "storage",
    "clipboardWrite",
    "clipboardRead",
    "downloads",
    "tts"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "options_page": "options.html",
  "version": "5.7"
}
