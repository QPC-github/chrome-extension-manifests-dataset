{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OnMyMindDaily",
  "description": "This extension gives you a way to comment",
  "version": "1.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "https://www.onmyminddaily.com/",
    "http://www.onmyminddaily.com/",
    "activeTab"
  ]
}
