{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Reverse Genie - Phone & Email",
  "version": "2.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "description": "Need to know whoâ€™s calling you from an unfamiliar number? Install ReverseGenie and discover who is contacting you by phone!",
  "homepage_url": "http://www.reversegenie.com/",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "reverseGenie.css"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "web_accessible_resources": [
    "imgs/top_left.png",
    "imgs/top_right.png",
    "imgs/bottom_left.png",
    "imgs/bottom_right.png"
  ]
}
