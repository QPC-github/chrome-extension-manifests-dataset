{
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.ipn.mx/*"
      ]
    }
  ],
  "default_locale": "es",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "SAES IPN",
  "description": "Extensi√≥n que agrega funcionalidades y aplica algunas correcciones al sitio web SAES del IPN.",
  "permissions": [
    "tabs",
    "*://*.ipn.mx/*",
    "*://*.google.com/*",
    "storage",
    "unlimitedStorage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6"
}
