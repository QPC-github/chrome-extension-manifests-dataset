{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "secvencya@gmail.com",
  "name": "TheVR",
  "version": "4.6.0",
  "manifest_version": 2,
  "options_page": "options/options.html",
  "description": "A TheVR-nak és rajongóknak készített böngésző kiegészítő, hogy soha ne maradjunk le semmiről :D",
  "offline_enabled": false,
  "icons": {
    "128": "thevr.png"
  },
  "background": {
    "scripts": [
      "config.js",
      "jquery.min.js",
      "vr-js/bg_vrNotification.js",
      "vr-js/bg_vrEngine.js",
      "twitch-js/bg_events_danx27.js",
      "twitch-js/bg_twnoti.js",
      "twitch-js/bg_OtherStreamers.js",
      "firstRun.js"
    ]
  },
  "browser_action": {
    "default_icon": "thevr.png",
    "default_popup": "html/thevr.html"
  },
  "permissions": [
    "https://*.ytimg.com/*",
    "notifications",
    "storage"
  ]
}
