{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Facebook News-feed Destroyer",
  "description": "This extension destroys Facebook's news feed.",
  "version": "1.0",
  "permissions": [
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*"
      ],
      "js": [
        "jquery.js",
        "myscript.js"
      ]
    }
  ]
}
