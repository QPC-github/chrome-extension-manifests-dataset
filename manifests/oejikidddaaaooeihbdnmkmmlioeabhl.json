{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bitcointalk Custom CSS",
  "version": "0.1",
  "description": "Only works in bitcointalk.org",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "css": [
        "css.css"
      ],
      "js": [
        "app.js"
      ],
      "matches": [
        "https://bitcointalk.org/*"
      ]
    }
  ]
}
