  "short_name": "Cauta pe Price.ro",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "6",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "cauta_pricero.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
