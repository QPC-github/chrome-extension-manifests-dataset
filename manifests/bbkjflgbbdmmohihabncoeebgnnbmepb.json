{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "X-Files Button",
  "version": "0.1",
  "description": "the truth is out there",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "x-files",
    "default_icon": "icon16.png"
  },
  "web_accessible_resources": [
    "x-files.mp3"
  ],
  "manifest_version": 2
}
