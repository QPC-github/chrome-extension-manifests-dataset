{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabs Counter",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "file://*/"
  ],
  "version": "0.4",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
