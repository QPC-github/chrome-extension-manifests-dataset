{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "eBay hide multi item listings",
  "description": "This extension allows the user to hide eBay multi item listings.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.gif"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.ebay.com/*",
        "http://*.ebay.co.uk/*",
        "http://*.ebay.com.au/*",
        "http://*.ebay.de/*",
        "http://*.ebay.ca/*",
        "http://*.ebay.at/*",
        "http://*.ebay.be/*",
        "http://*.ebay.com.hk/*",
        "http://*.ebay.in/*",
        "http://*.ebay.ie/*",
        "http://*.ebay.com.my/*",
        "http://*.ebay.it/*",
        "http://*.ebay.nl/*",
        "http://*.ebay.ph/*",
        "http://*.ebay.pl/*",
        "http://*.ebay.com.sg/*",
        "http://*.ebay.es/*",
        "http://*.ebay.ch/*",
        "http://*.ebay.fr/*",
        "https://*.ebay.com/*",
        "https://*.ebay.co.uk/*",
        "https://*.ebay.com.au/*",
        "https://*.ebay.de/*",
        "https://*.ebay.ca/*",
        "https://*.ebay.at/*",
        "https://*.ebay.be/*",
        "https://*.ebay.com.hk/*",
        "https://*.ebay.in/*",
        "https://*.ebay.ie/*",
        "https://*.ebay.com.my/*",
        "https://*.ebay.it/*",
        "https://*.ebay.nl/*",
        "https://*.ebay.ph/*",
        "https://*.ebay.pl/*",
        "https://*.ebay.com.sg/*",
        "https://*.ebay.es/*",
        "https://*.ebay.ch/*",
        "https://*.ebay.fr/*"
      ],
      "js": [
        "jquery-3.2.1.min.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
