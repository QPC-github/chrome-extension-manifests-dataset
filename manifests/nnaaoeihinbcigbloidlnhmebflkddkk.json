{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "github-extension",
  "description": "This extension make github more accessible",
  "version": "0.0.2",
  "permissions": [
    "tabs",
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "github-extension.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
