{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hacker Nope",
  "description": "Hides all but the top seven stories on Hacker News.",
  "version": "0.4.0",
  "icons": {
    "128": "hacker-y.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "css": [
        "limit-distractions.css"
      ]
    }
  ]
}
