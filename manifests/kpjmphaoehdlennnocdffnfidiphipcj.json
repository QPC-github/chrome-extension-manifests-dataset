{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Manşet Emlak",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Manşet Emlak websitesinden yeni gelişmeleri anında size sunup haberdar eder.",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Manşet Emlak",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "*://*.mansetemlak.com/*",
    "notifications"
  ]
}
