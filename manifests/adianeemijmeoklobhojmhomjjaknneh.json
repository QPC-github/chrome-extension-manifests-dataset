{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL Uncover for Chrome",
  "version": "2.0.0",
  "description": "Uncover any short url including adf.ly, linkbucks.com and more with ease",
  "icons": {
    "48": "icon48.png",
    "128": "icon64.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "URL Uncover for Chrome",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
