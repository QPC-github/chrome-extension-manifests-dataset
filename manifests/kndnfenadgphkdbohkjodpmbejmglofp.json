{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark All",
  "description": "Bookmark all open page",
  "version": "2.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "Bookmark All"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "128": "128.png"
  }
}
