{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Oodalicious",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.oodalicious.com/*"
  ],
  "version": "1.0.0.16",
  "browser_action": {
    "default_icon": {
      "19": "images/ooda-19.png",
      "38": "images/ooda-38.png"
    },
    "default_popup": "ooda.html",
    "default_title": "Oodalicious on-the-go."
  },
  "description": "Oodalicious on-the-go.",
  "icons": {
    "128": "images/ooda-128.png",
    "16": "images/ooda-16.png",
    "32": "images/ooda-32.png",
    "48": "images/ooda-48.png",
    "96": "images/ooda-96.png"
  }
}
