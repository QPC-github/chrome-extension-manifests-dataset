{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "player.css"
      ],
      "js": [
        "jquery.js",
        "popup.js"
      ],
      "matches": [
        "http://www.mediafire.com/*/player"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "BreackerzMFStream",
  "description": "Disfruta de los videos de MediaFire sin la necesidad de descargarlos.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0",
  "icons": {
    "16": "img/icon16.png",
    "24": "img/icon24.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
