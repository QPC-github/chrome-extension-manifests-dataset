{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Folo It",
  "description": "Folo It lets you follow a particular topic on a given website.",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "FoloItLogo.png",
    "content.js"
  ],
  "background": {
    "scripts": [
      "content.js"
    ]
  },
  "icons": {
    "16": "Logo.png",
    "48": "Logo.png",
    "128": "Logo.png"
  },
  "manifest_version": 2
}
