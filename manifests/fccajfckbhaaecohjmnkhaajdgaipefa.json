{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Binghamton University Libraries Proxy Button",
  "short_name": "BU Libraries Proxy Button",
  "description": "Loads Binghamton University Libraries' proxy service on Google Scholar and other websites.",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Go Through Library Proxy",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
