{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Downloads Notification",
  "description": "Move your download list from the fat bottom bar to Chrome Notification Center",
  "version": "1.5",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "downloads",
    "downloads.shelf",
    "downloads.open",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
