{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js",
      "bootstrap4/js/bootstrap.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon/icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "Get Facebook Cookie And User-Agent"
  },
  "description": "Easy to get your facebook cookie and user-agent",
  "icons": {
    "128": "icon/icon_big.png"
  },
  "name": "Get Cookie For Facebook",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "*://www.facebook.com/*"
  ],
  "version": "1.0",
  "manifest_version": 2
}
