{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://natter.com/; object-src 'self'",
  "description": "Natter | Just Three Words",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "36": "images/icon-36.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "name": "Natter",
  "permissions": [
    "https://natter.com/*",
    "tabs"
  ],
  "version": "0.0.1"
}
