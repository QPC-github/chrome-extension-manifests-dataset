{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Game Blast",
  "version": "1.0.0.1",
  "description": "Get faster game access. One click games right from your new tab home page.",
  "icons": {
    "16": "16.jpg",
    "32": "32.jpg",
    "48": "48.jpg",
    "128": "128.jpg"
  },
  "browser_action": {
    "default_icon": "logo19.png"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://*.zilliuns.com/*"
    ]
  }
}
