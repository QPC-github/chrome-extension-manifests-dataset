{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SigueMex",
  "description": "SigueMex",
  "version": "2.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SigueMex"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "Mexico-icon.png"
  }
}
