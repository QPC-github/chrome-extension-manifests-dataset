{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "img/16.png",
    "default_popup": "popup.html"
  },
  "description": "Dump ExtensionInfo data of all installed extensions and apps in Google Chrome",
  "icons": {
    "128": "img/128.png",
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "manifest_version": 2,
  "name": "DumpExtensionInfo",
  "offline_enabled": true,
  "permissions": [
    "management"
  ],
  "version": "1.0"
}
