{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SimpleTabOrder",
  "version": "0.5.12",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_manifest_description__",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "tab.js",
      "bg.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ]
}
