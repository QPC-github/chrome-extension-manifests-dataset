{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Donald Diagnosed",
  "version": "1.0.6",
  "description": "Browse with Context!",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "icons/dd-icon-48.png",
    "128": "icons/dd-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/js/jquery-3.4.1.min.js",
        "js/skipWords.js",
        "js/modifyText.js"
      ]
    }
  ]
}
