{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pieter Levels inspiration",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Quotes and motivation from Pieter Levels - every time you open a new tab!",
  "icons": {
    "64": "icons/favicon.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
