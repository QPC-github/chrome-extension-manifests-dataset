{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zodiac Ted",
  "icons": {
    "128": "assets/img/icon.png"
  },
  "version": "2.1.1",
  "author": "Ben Hiatt",
  "description": "Reveals the true identity of the man who claims to be Ted Cruz.",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_icon": "assets/img/icon.png",
    "default_popup": "popup.html"
  }
}
