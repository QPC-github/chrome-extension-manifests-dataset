{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ESPN PST Time",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Makes times on ESPN's site PST.",
  "content_scripts": [
    {
      "matches": [
        "http://games.espn.go.com/*",
        "https://games.espn.go.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
