{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tek-Screenshare",
  "author": "sciprojectservices",
  "description": "A simple extension that allows you to stream your desktop into meetings with Collaboration Tool",
  "homepage_url": "http://www.teknuance.com",
  "version": "1.5.2",
  "minimum_chrome_version": "34",
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "https://172.16.20.61/*",
      "https://localhost/*",
      "https://sciantra.biz/*"
    ]
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}
