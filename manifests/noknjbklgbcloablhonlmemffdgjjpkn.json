{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search Owl",
  "version": "2.8",
  "description": "The Smartest Search Alive",
  "background": {
    "scripts": [
      "js/main.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "owl.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com/jquery-2.2.0.min.js https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.js; object-src 'self'"
}
