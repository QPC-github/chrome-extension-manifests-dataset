{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "reddit18",
  "description": "after reddit17, before reddit19",
  "version": "0.9.1",
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "48": "48.png"
  },
  "permissions": [
    "tabs",
    "*://*.reddit.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "js": [
        "jquery.min.js",
        "incognito.js"
      ]
    }
  ]
}
