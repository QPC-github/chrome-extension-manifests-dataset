{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IManHua Unblocker",
  "description": "Unblock www.imanhua.com for users away from China mainland.",
  "version": "1.1",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "http://*.imanhua.com/*"
  ],
  "page_action": {
    "default_icon": "icon19.png",
    "default_title": "IManHua Unblocker is working."
  },
  "web_accessible_resources": [
    "inject.js"
  ]
}
