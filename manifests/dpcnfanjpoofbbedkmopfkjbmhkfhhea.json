{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Auto Allow Fullscreen",
  "short_name": "Automatically allow fullscreen mode",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "author": "Daniel Kladnik @ kiboke studio",
  "permissions": [
    "contentSettings",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "data/script.js"
    ]
  }
}
