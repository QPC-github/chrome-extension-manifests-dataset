{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Big Gift List",
  "author": "Design Lynx Ltd",
  "version": "2.0",
  "description": "Easily add items to your 'The Big Gift List' lists",
  "icons": {
    "32": "toolbar-large.png",
    "16": "toolbar-small.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "The Big Gift List",
    "default_icon": "toolbar-small.png",
    "default_popup": "popup.html"
  }
}
