{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save to prsnt.app",
  "version": "2.4.5",
  "author": "prsnt.app",
  "description": "Save product and material information directly to your presentations.",
  "icons": {
    "16": "assets/icons/arazoo_logo_16.png",
    "24": "assets/icons/arazoo_logo_24.png",
    "48": "assets/icons/arazoo_logo_48.png",
    "96": "assets/icons/arazoo_logo_96.png",
    "128": "assets/icons/arazoo_logo_128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "activeTab",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "https://*.prsnt.app/",
    "https://prsnt.app/"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/arazoo_logo_16.png",
      "24": "assets/icons/arazoo_logo_24.png",
      "48": "assets/icons/arazoo_logo_48.png",
      "96": "assets/icons/arazoo_logo_96.png",
      "128": "assets/icons/arazoo_logo_128.png"
    },
    "default_title": "Save To prsnt.app"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "html/iframe.html"
  ]
}
