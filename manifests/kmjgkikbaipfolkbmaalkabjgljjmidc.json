{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AdsInserter debugger",
  "description": "This extension allows the user to visual debug placements and units on the page",
  "version": "1.0",
  "background": {
    "scripts": [
      "loaded.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Enable AdsInserter debugger",
    "default_icon": "icons/icon-enabled.png"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
