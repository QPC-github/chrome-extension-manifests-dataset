{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "description": "Make serfing faster with our tabs",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "background.html"
  },
  "permissions": [
    "activeTab"
  ]
}
