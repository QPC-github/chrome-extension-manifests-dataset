{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "js/background.js"
    ]
  },
  "description": "Enable ClickOnce support in Google Chrome.",
  "icons": {
    "128": "icons/logaltyclickonce-128.png",
    "16": "icons/logaltyclickonce-16.png",
    "32": "icons/logaltyclickonce-32.png",
    "48": "icons/logaltyclickonce-48.png",
    "64": "icons/logaltyclickonce-64.png"
  },
  "manifest_version": 2,
  "name": "Logalty ClickOnce",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.12",
  "web_accessible_resources": [
    "images/blank.png"
  ]
}
