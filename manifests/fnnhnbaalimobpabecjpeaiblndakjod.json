{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "getId.in",
  "description": "This extension smartly fill your email address box with smart @getid.in email. You need to login into your getid.in account.",
  "version": "3.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "js/jquery-1.11.1.min.js",
        "js/autofill.js"
      ],
      "css": [
        "styles/autofill.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  }
}
