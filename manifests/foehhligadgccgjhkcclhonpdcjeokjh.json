{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JustSnipe eBay Auction Sniper",
  "version": "1.7.1",
  "description": "The extension is a browser add-on to JustSnipe.com for placing bid at the last seconds on eBay.",
  "icons": {
    "128": "images/logo128.png"
  },
  "background": {
    "page": "bg.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/justsnipe-logo1-38.png"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
