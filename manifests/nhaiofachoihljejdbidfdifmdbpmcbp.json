{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MAX-info",
  "version": "2.0",
  "description": "Последние публикации сайта max-info.by прямо у Вас в браузере! Узнавайте о последних публикациях сайта первыми.",
  "icons": {
    "128": "img/icon.png"
  },
  "permissions": [
    "http://www.max-info.by/*"
  ],
  "browser_action": {
    "default_title": "MAX-info!",
    "default_icon": "img/icon.png",
    "default_popup": "list.html"
  }
}
