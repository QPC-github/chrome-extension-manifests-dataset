{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extension Manager",
  "description": "One extension to manage them all",
  "version": "0.0.2",
  "permissions": [
    "activeTab",
    "management"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "O"
  },
  "manifest_version": 2
}
