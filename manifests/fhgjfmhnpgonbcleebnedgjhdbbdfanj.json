{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "embedyoutube.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Embed Youtube HTML5",
  "icons": {
    "128": "youtube-128.png",
    "32": "youtube-32.png",
    "512": "youtube-512.png"
  },
  "manifest_version": 2,
  "name": "Embed Youtube HTML5",
  "version": "0.2"
}
