{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Check Shortlinks"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "background": {
    "scripts": [
      "context.js"
    ]
  },
  "manifest_version": 2,
  "name": "ShortLink Inspector",
  "description": "Inspect where short-links are leading to.",
  "permissions": [
    "https://inspector.staxy.de/",
    "contextMenus"
  ],
  "short_name": "ShoLiSpec",
  "version": "1.1"
}
