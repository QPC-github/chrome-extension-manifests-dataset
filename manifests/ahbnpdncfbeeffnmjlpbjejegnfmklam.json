{
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
  "browser_action": {
    "default_icon": "16.png",
    "default_title": "SEO Metrics",
    "default_popup": "popup.html"
  },
  "description": "SEO Extension for Google Chrome ",
  "short_name": "Quickly and easily inspect site meta values.",
  "homepage_url": "https://www.mertcanisler.com",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "name": "SEO Metrics",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1",
  "manifest_version": 2
}
