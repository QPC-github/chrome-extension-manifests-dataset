{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Audiosurf 2 Yashgen Addon",
  "version": "1.5",
  "author": "orell12",
  "description": "Addon so you can run audiosurf2.info yashgen from youtube",
  "homepage_url": "https://github.com/audiosurf/audiosurf2.info-yashgen-addon",
  "icons": {
    "16": "image/icon_16.png",
    "48": "image/icon_48.png",
    "128": "image/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*.youtube.com/*",
    "*://audiosurf2.info/*",
    "*://soundcloud.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "image/icon_16.png",
      "48": "image/icon_48.png",
      "128": "image/icon_128.png"
    }
  }
}
