{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Smile Suggest",
  "short_name": "SmileSuggest",
  "description": "Recommend content by smiling",
  "version": "1.4.1",
  "browser_action": {
    "default_icon": {
      "19": "icon19x19.png",
      "38": "icon76x76.png"
    }
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_page": "setup.html",
  "permissions": [
    "tabs",
    "storage",
    "background",
    "bookmarks",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
