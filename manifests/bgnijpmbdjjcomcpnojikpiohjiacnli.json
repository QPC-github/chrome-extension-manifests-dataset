{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PartnerCart",
  "description": "Get a better overview of your receipts and purchased items",
  "version": "1.1",
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "permissions": [
    "declarativeContent",
    "https://www.partnerkaart.ee/iseteenindus/minu-tehingud"
  ],
  "page_action": {
    "default_icon": {
      "19": "icons/icon-16x16.png",
      "38": "icons/icon-48x48.png"
    },
    "default_title": "PartnerCart"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
