{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [],
  "browser_action": {
    "default_icon": "images/firefile_16.png",
    "default_title": "Save Changes"
  },
  "background": {
    "scripts": [
      "lib/jquery.min.js",
      "bg.js"
    ]
  },
  "description": "FireFile is a Google Chrome extension that allows you to save the CSS files edited with the inspector live to a web server",
  "devtools_page": "devtools.html",
  "icons": {
    "128": "images/firefile_128.png",
    "16": "images/firefile_16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "FireFile",
  "permissions": [
    "tabs",
    "http://www.firefile.at/api/*"
  ],
  "version": "0.9.3",
  "options_page": "panel.html"
}
