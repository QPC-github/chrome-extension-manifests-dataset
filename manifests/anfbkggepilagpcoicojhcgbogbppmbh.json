{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Naver News Ex",
  "short_name": "Naver News Ex",
  "version": "0.4",
  "description": "__MSG_app_description__",
  "default_locale": "ko",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://news.naver.com/*"
      ]
    }
  ]
}
