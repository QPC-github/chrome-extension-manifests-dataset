{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HLP-NewTabMod",
  "description": "This will allow a user to setup a custom URL for the New Tab page.",
  "version": "3.1.4",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": [
    "storage",
    "chrome://favicon/"
  ],
  "optional_permissions": [
    "tabs",
    "topSites",
    "management",
    "bookmarks"
  ],
  "icons": {
    "200": "images/icon200.png",
    "128": "images/icon128.png",
    "19": "images/icon19.png"
  },
  "incognito": "split",
  "minimum_chrome_version": "22"
}
