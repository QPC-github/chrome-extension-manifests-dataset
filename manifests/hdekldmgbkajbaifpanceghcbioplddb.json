{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LoginTC",
  "version": "1.3.3",
  "description": "__MSG_app_description__",
  "icons": {
    "16": "assets/img/icon-16.png",
    "128": "assets/img/icon-128.png",
    "256": "assets/img/icon-256.png"
  },
  "permissions": [
    "background",
    "gcm",
    "storage",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "LoginTC",
    "default_icon": "icon.png",
    "default_popup": "build.html"
  },
  "background": {
    "scripts": [
      "assets/js/ext/firebase/firebase-app.js",
      "assets/js/ext/firebase/firebase-messaging.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "default_locale": "en"
}
