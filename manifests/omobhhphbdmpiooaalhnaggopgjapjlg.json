{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Diamond Voice",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.12.4.js",
        "content.js",
        "jquery-ui.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "fevicon_16x16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "fevicon_16x16.png",
    "48": "FavIcon-1.png",
    "128": "FavIcon-1.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
