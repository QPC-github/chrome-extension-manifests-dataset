{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Recently Accessed Bookmark",
  "version": "0.1",
  "description": "最近アクセスされた順にブックマークをソートします",
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon.png",
    "128": "image/icon.png"
  },
  "permissions": [
    "history",
    "bookmarks",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "image/icon.png",
    "default_title": "ブックマークを表示",
    "default_popup": "popup.html"
  }
}
