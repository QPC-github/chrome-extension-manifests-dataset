{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CauseMP (by CauseHub)",
  "description": "This is an extension for indexing web pages and return relevent mp's that may be interested in it.",
  "version": "1.0.3",
  "permissions": [
    "tabs",
    "http://*/",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "CauseMP",
    "default_icon": "icons/icon19.png",
    "default_popup": "display.html"
  },
  "author": "CauseHub",
  "short_name": "CauseMP",
  "homepage_url": "http://causehub.io",
  "offline_enabled": false
}
