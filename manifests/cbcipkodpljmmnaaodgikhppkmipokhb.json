{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QR Code",
  "description": "This extension allows the user to share QR code of current URL with Other devices.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "show QR Code"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
