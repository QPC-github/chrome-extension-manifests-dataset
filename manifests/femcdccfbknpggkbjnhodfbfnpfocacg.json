{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickNote",
  "short_name": "QuickNote",
  "version": "1.0.6",
  "description": "Create quick, anonymous notes",
  "author": "https://manuel.friger.io",
  "homepage_url": "https://quicknote.io",
  "omnibox": {
    "keyword": "quick"
  },
