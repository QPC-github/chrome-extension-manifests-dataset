{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Show Interface."
  },
  "description": "Find and use new useful scripts to exhance your online experience!",
  "icons": {
    "64": "icon.png"
  },
  "manifest_version": 2,
  "name": "SnowLord's Script Extension",
  "version": "6.3",
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus"
  ],
  "short_name": "SnowLord's Extension",
  "web_accessible_resources": [
    "main.js"
  ]
}
