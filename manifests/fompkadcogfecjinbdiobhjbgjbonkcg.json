{
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "云直播培训版桌面共享",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Screen sharing",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.0",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "云直播培训版桌面共享",
  "externally_connectable": {
    "matches": [
      "*://*.vhall.com/*"
    ]
  },
  "icons": {
    "128": "logo-right.png"
  },
  "browser_action": {
    "default_icon": "logo-right.png"
  }
}
