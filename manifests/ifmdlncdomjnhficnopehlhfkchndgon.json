{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "messiosmarios",
  "short_name": "smm",
  "description": "Replace the new tab with a list of trending projects from studio messiosmarios.",
  "version": "1",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "*://*.messiosmarios.com/index.html/*"
  ],
  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "studio messiosmarios - Projects.html"
  },
  "homepage_url": "http://messiosmarios.com/index.html",
  "options_page": "studio messiosmarios - Projects.html",
  "browser_action": {
    "default_title": "studio messiosmarios",
    "default_icon": "assets/img/icon128.png"
  }
}
