{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save to voolt.io",
  "short_name": "voolt.io",
  "description": "Instantly save images from the web to your own private place with voolt.io",
  "version": "1.0.3",
  "permissions": [
    "contextMenus",
    "https://voolt.io/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "brand/logo.128.png",
    "48": "brand/logo.48.png",
    "16": "brand/logo.16.png"
  },
  "manifest_version": 2
}
