{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "quickreach",
  "description": "quickreach finds verified prospect contact info in real time from within your browser",
  "version": "1.24",
  "browser_action": {
    "default_icon": "icon-active.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "outer_iframe.html"
  ],
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
