{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "谷歌浏览器代理设置",
  "version": "0.4",
  "description": "基于谷歌Demo代码修改, 增加保存原 pacURL 数据的功能, 方便切换.",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "proxy_form_controller.js",
      "proxy_error_handler.js",
      "background.js"
    ]
  },
  "permissions": [
    "proxy",
    "storage"
  ],
  "manifest_version": 2
}
