{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Conquer Club Notifier",
  "short_name": "CCNotifier",
  "description": "Conquerclub game notifier",
  "version": "0.9.3",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "http://www.conquerclub.com/",
    "https://www.conquerclub.com/",
    "http://static.conquerclub.com/",
    "https://static.conquerclub.com/",
    "http://maps.conquerclub.com/",
    "https://maps.conquerclub.com/"
  ],
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/formatConquerClubGameData.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/cc_inactive.png",
    "default_popup": "popup.html"
  }
}
