{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "SmartContact"
  },
  "description": "SmartContact Lite",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "SmartContact Chrome Extension",
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "version": "0.0.0.8",
  "content_scripts": [
    {
      "matches": [
        "https://agentweb.jmeservicios.com/lite"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
