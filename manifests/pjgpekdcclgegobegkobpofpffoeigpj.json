{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RACE Fill",
  "description": "Automatically populate a RACE reservation form with the details for a 2 day reservation",
  "version": "1.6",
  "browser_action": {
    "default_title": "RACE Fill",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/SAS_logo_16.png",
    "32": "images/SAS_logo_32.png",
    "48": "images/SAS_logo_48.png",
    "128": "images/SAS_logo_128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
