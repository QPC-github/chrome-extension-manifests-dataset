{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Identifier",
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "http://*.ksskdm.com:6692/*",
      "http://*.dev.localhost:4200/*",
      "https://*.dev.localhost:4200/*",
      "https://*.ksskdm.com:6692/*",
      "http://*.skycinex.com:6692/*",
      "https://*.skycinex.com:6692/*"
    ]
  },
  "description": "This extension provides you with a unique client ID",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Generate ID"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
