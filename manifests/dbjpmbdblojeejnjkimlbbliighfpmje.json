{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Putinizer",
  "version": "0.2",
  "description": "Makes Putin in rainbow colors in your news.",
  "icons": {
    "128": "putinizer128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "putinizer.user.js"
      ]
    }
  ]
}
