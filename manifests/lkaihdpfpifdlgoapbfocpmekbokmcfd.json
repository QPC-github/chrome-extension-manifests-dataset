{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AutoControl Shortcut Manager",
  "short_name": "AutoControl",
  "version": "2020.9.14",
  "manifest_version": 2,
  "description": "Customize Chrome's native shortcuts, Create your own shortcuts, Mouse gestures, Bookmark shortcuts, and more.",
  "minimum_chrome_version": "49",
  "homepage_url": "https://www.autocontrol.app",
  "options_page": "main.html",
  "background": {
    "page": "file30.html",
    "persistent": true
  },
  "sandbox": {
    "pages": [
      "file37.html"
    ]
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/ ; object-src 'self'",
  "permissions": [
    "nativeMessaging",
    "tabs",
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "system.display",
    "idle",
    "chrome://favicon/"
  ],
  "optional_permissions": [
    "bookmarks",
    "sessions",
    "background"
  ],
  "browser_action": {
    "default_icon": {
      "16": "AutoCtrl/logo16.png",
      "24": "AutoCtrl/logo24.png",
      "32": "AutoCtrl/logo32.png"
    },
    "default_title": "AutoControl Configuration"
  },
  "icons": {
    "16": "AutoCtrl/logo16.png",
    "48": "AutoCtrl/logo48.png",
    "128": "AutoCtrl/logo128.png"
  },
  "offline_enabled": true,
  "key": ""
}
