{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "D-LogoKiller",
  "name": "Drive Logo Killer",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://drive.google.com/drive/u/*/*"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "main.js"
      ],
      "run_at": "document_end"
    }
  ]
}
