{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "极速新标签页pro",
  "version": "2.9",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "contextMenus",
    "bookmarks",
    "history",
    "management",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "unlimitedStorage",
    "notifications",
    "<all_urls>",
    "topSites"
  ],
  "description": "极速新标签页pro版本,chrome浏览器用户专用。简单而极速，自定义常用网址。",
  "background": {
    "page": "bg.html"
  },
  "icons": {
    "128": "icons/icon-128.png",
    "16": "icons/icon.png"
  },
  "content_security_policy": "script-src'self'https://sug.so.360.cnhttps://www.google-analytics.com;object-src'self'",
  "chrome_url_overrides": {
    "newtab": "home.htm"
  }
}
