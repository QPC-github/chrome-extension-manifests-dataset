{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dakola QR Code Genetor",
  "description": "Generate the QR Code of the URL of the site you are visiting by using the Genrerator on dakola.com",
  "version": "1.0",
  "permissions": [
    "http://www.dakola.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon-48.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
