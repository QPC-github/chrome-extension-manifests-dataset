{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "More Colors",
  "version": "1.0.2",
  "permissions": [
    "activeTab",
    "tabs",
    "background"
  ],
  "icons": {
    "16": "color.png"
  },
  "browser_action": {
    "default_title": "Makes your internet experience a little more colorful",
    "default_icon": "color.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "hover.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
