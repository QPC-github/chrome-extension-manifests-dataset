{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Material",
  "version": "0.1",
  "description": "Material design for google search engine",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "96": "img/icon96.png",
      "128": "img/icon128.png",
      "300": "img/icon300.png"
    }
  },
  "content_scripts": [
    {
      "css": [
        "css/gmaterial.css"
      ],
      "matches": [
        "https://www.google.com/*",
        "https://www.google.es/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/gmaterial.css"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "96": "img/icon96.png",
    "128": "img/icon128.png",
    "300": "img/icon300.png"
  }
}
