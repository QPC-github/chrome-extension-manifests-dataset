{
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/html.js",
      "js/background.js",
      "js/url_manager.js",
      "js/content.js",
      "main.html"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png"
    },
    "default_title": "EPROLO - Aliexpress product importer"
  },
  "content_scripts": [
    {
      "css": [
        "css/crawl_dxm.css",
        "css/main.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js",
        "js/html.js",
        "js/url_manager.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "One click import and sync Aliexpress products to your Shopify and Woocommerce store.",
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.eprolo.com/*"
    ]
  },
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "EPROLO - Aliexpress product importer",
  "permissions": [
    "https://*.eprolo.com/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "notifications",
    "storage",
    "unlimitedStorage",
    "http://*.eprolo.com/",
    "http://*.aliexpress.com/",
    "https://*.aliexpress.com/"
  ],
  "short_name": "Product Importer",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.1",
  "web_accessible_resources": [
    "main.html"
  ]
}
