{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TabPacker",
  "version": "2.1",
  "description": "Bring all tabs into TabPacker",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "TabPacker"
  },
  "web_accessible_resources": [
    "stack.html",
    "load.html"
  ],
  "commands": {
    "myCommand_tabpacker": {
      "suggested_key": {
        "default": "Alt+P"
      },
      "description": "Send tabs command"
    }
  },
  "permissions": [
    "chrome://favicon/*",
    "tabs",
    "<all_urls>",
    "storage",
    "sessions",
    "unlimitedStorage"
  ]
}
