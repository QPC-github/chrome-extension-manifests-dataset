{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Upladder",
  "version": "2.0",
  "description": "This extension provides 'Go to upper URL' button",
  "background": {
    "scripts": [
      "upladder.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "upladder16.png",
    "32": "upladder32.png",
    "48": "upladder48.png",
    "128": "upladder128.png"
  },
  "browser_action": {
    "default_title": "Go to upper URL",
    "default_icon": "upladder19.png"
  }
}
