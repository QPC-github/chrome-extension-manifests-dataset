{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Make News Great Again",
  "manifest_version": 2,
  "version": "0.3",
  "description": "Turns nasty news into fun news",
  "content_scripts": [
    {
      "matches": [
        "*://*.bbc.co.uk/news*",
        "*://*.bbc.com/news*",
        "*://news.yahoo.com/*",
        "*://news.google.com/*",
        "*://*.huffingtonpost.com/*",
        "*://*.cnn.com/*",
        "*://*.nytimes.com/*",
        "*://*.foxnews.com/*",
        "*://*.dailymail.co.uk/*",
        "*://*.washingtonpost.com/*",
        "*://*.theguardian.com/*",
        "*://*.wsj.com/*",
        "*://abcnews.go.com/*",
        "*://*.usatoday.com/*",
        "*://*.latimes.com/*",
        "*://*.indiatimes.com/*",
        "*://*.bloomberg.com/*",
        "*://*.reuters.com/*",
        "*://*.forbes.com/*",
        "*://*.time.com/*",
        "*://*.indianexpress.com/*",
        "*://*.cnbc.com/*",
        "*://*.cbsnews.com/*",
        "*://*.nypost.com/*",
        "*://news.com.au/*",
        "*://*.reddit.com/*",
        "*://*.theatlantic.com/*",
        "*://*.chicagotribune.com/*"
      ],
      "all_frames": true,
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.json"
  ]
}
