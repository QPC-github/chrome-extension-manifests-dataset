{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrumpWLB",
  "description": "Updates web-browsing experience by creating a more accurate representation of Donald Trump",
  "version": "0.1.4",
  "icons": {
    "16": "WLB16.png",
    "48": "WLB48.png",
    "32": "WLB32.png",
    "128": "WLB128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "TrumpWLB.png"
  ]
}
