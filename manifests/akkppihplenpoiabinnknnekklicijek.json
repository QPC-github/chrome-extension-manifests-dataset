{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OpenAir Enhancement Suite",
  "short_name": "OAES",
  "version": "1.5.1",
  "manifest_version": 2,
  "author": "Brandon Flynn",
  "description": "A useability upgrade for Openair.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://*.openair.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
