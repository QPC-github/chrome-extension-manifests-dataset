{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nelson Alexander",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Nelson Alexander Property Showcase & Launchpad",
  "icons": {
    "16": "assets/img/nelson-logo-16.png",
    "48": "assets/img/nelson-logo-48.png",
    "128": "assets/img/nelson-logo-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "views/index.html"
  },
  "browser_action": {
    "default_title": "Nelson Alexander Property Showcase & Launchpad"
  },
  "permissions": [
    "storage",
    "https://www.nelsonalexander.com.au/",
    "https://www.google-analytics.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
