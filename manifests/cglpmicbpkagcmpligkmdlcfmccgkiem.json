  "version": "2.0",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://timetable.nctu.edu.tw/*",
        "https://course.nctu.edu.tw/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "manifest.js"
      ],
      "all_frames": true
    }
  ]
}
