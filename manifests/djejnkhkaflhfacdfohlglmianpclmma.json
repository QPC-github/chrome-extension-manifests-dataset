{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Send Overnight Mail",
  "short_name": "Send Overnight Mail",
  "version": "1.0.1.0",
  "manifest_version": 2,
  "description": "The easiest way to send overnight mail, online or offline: Right from your Computer, Simple and Secure",
  "container": [
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "1042952188359",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/pdf",
          "application/vnd.google.drive.ext-type.pdf"
        ],
        "href": "https://app.sendovernightmail.com/service/integration/v1/drive",
        "title": "Open",
        "disposition": "window"
      }
    ]
  },
  "icons": {
    "128": "logo.128x128.png"
  },
  "app": {
    "urls": [
      "https://www.sendovernightmail.com/"
    ],
    "launch": {
      "web_url": "https://www.sendovernightmail.com/apps/google/drive/"
    }
  }
}
