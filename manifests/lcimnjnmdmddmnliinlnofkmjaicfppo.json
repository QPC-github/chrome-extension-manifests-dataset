{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VMANINFO",
  "description": "Den optimale funktionalitet p√• Virtual Manager",
  "version": "1.10.10",
  "browser_action": {
    "name": "Set weights",
    "icons": [
      "icon64.png"
    ],
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "web_accessible_resources": [
    "48.png"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.virtualmanager.com/squads/*/training_exercises",
        "https://www.virtualmanager.com/players/*",
        "https://www.virtualmanager.com/auctions/buying*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "jquery-ui.min.js",
        "training_optimizer.js"
      ],
      "css": [
        "jquery-ui.min.css"
      ]
    },
    {
      "matches": [
        "https://www.virtualmanager.com/clubs/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "shortlink_club.js"
      ]
    },
    {
      "matches": [
        "https://www.virtualmanager.com/players/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "player_enhancer.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "https://www.virtualmanager.com/squads/*/training_exercises/*",
    "https://www.virtualmanager.com/clubs/*",
    "https://www.virtualmanager.com/players/*",
    "https://www.virtualmanager.com/auctions/*",
    "https://beta.vmaninfo.dk/*"
  ]
}
