{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Movie Quotes",
  "author": "Emil Carlsson",
  "description": "Famous quotes from movies and TV shows featured right on your new tab.",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "offline_enabled": true
}
