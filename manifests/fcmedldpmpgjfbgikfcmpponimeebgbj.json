{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Full Name",
  "description": "Stops YouTube cutting off a video's title",
  "version": "0.1",
  "manifest_version": 2,
  "author": "lmfrgsn",
  "icons": {
    "48": "icon.png",
    "96": "icon@2x.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
