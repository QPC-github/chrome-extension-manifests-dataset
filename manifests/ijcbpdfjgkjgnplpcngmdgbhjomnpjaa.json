{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Alphabet Tutor",
  "description": "Learn the names of the letters by exploring the keyboard and test your knowledge in quiz mode.",
  "version": "2.1",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "overrideEscFullscreen"
  ]
}
