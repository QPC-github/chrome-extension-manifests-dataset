{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YoDash",
  "description": "Let's jump to ListenYo and enjoy listening to your favorite music by a single click.",
  "version": "1.0.2",
  "icons": {
    "128": "icon128new.png"
  },
  "browser_action": {
    "default_title": "Jump to ListenYo"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "https://youtube.com/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
