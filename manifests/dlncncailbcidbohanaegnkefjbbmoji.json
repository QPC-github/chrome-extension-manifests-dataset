{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "libraries/jquery.js",
        "script.js"
      ],
      "matches": [
        "http://rutracker.org/*",
        "https://rutracker.org/*"
      ]
    }
  ],
  "description": "Расширенное отображение фильмов на rutracker.org",
  "icons": {
    "128": "images/Rutracker128.png",
    "16": "images/Rutracker16.png",
    "48": "images/Rutracker48.png"
  },
  "web_accessible_resources": [
    "images/Button.png",
    "images/Button1.png",
    "images/ButtonTop.png",
    "css/style.css",
    "libraries/jquery.js"
  ],
  "manifest_version": 2,
  "name": "Rutracker+",
  "permissions": [
    "http://omdbapi.com/"
  ],
  "version": "0.2.10"
}
