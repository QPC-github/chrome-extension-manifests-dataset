{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://xkcd.com/*"
      ],
      "js": [
        "scripts/third_party/jquery-1.11.1.min.js",
        "scripts/script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en"
}
