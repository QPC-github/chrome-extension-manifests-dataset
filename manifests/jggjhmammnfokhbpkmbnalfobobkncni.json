{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Backgroundify",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Put pages in the background!",
  "homepage_url": "http://technobaboo.tking.org",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "clipboardRead"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
