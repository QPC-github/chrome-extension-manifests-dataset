{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Saia Research",
  "version": "0.30",
  "description": "Create Quality Content Faster!",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "identity",
    "identity.email",
    "contextMenus",
    "storage",
    "tabs",
    "notifications",
    "activeTab",
    "http://localhost/*",
    "https://www.googleapis.com/*",
    "https://api.saia.ai/*",
    "https://app.saia.ai/*"
  ],
  "content_security_policy": " script-src 'self' 'unsafe-eval' https://www.gstatic.com/ https://www.googleapis.com https://apis.google.com; object-src 'self'",
  "oauth2": {
    "client_id": "50194674192-ffe9egt3a6nkdj533h37n1mkh5070bm3.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
