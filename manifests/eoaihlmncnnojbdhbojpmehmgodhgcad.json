{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chrome JIRA",
  "description": "Enhanced JIRA Cloud by adding a single click, copy to clipboard, the focused JIRA ticket into a smark commit",
  "manifest_version": 2,
  "version": "1.1.0",
  "icons": {
    "16": "images/jira.png",
    "128": "images/jira.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
    "default_icon": "images/jira.png",
    "default_title": "Get JIRA Smart Commit",
    "default_popup": "./app/popup.html"
  },
  "permissions": [
    "https://*.atlassian.net/"
  ],
  "web_accessible_resources": [
    "images/*"
  ]
}
