{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lectio Auto login",
  "description": "Dette plugin tilf√∏jere auto login til lectio..",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "http://*.lectio.dk/lectio/*/*.aspx*",
        "https://*.lectio.dk/lectio/*/*.aspx*"
      ],
      "js": [
        "jquery.min.js",
        "background.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "https://lectio.dk/*",
    "https://www.lectio.dk/*"
  ]
}
