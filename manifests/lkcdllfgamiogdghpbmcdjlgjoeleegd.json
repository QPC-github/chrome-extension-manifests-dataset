{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scribd Search",
  "description": "Search to read with scribd",
  "version": "1.2.1.0",
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "scribd"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open Scribd"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "128": "icon.png"
  }
}
