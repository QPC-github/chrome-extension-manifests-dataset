{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fanplayr Adaptor Wizard",
  "description": "This extension is used to build an adaptor for your store to integrate with Fanplayr.",
  "version": "1.1.0",
  "icons": {
    "128": "img/icon-128.png",
    "48": "img/icon-48.png",
    "16": "img/icon-16.png"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "storage",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "browser_action": {
    "default_title": "Fanplayr Adaptor Wizard",
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "lib/jquery.js",
      "lib/emitter.js",
      "lib/psl.min.js",
      "lib/background.js"
    ]
  },
  "web_accessible_resources": []
}
