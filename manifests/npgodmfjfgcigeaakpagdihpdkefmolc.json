{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Visual-Tinnitus",
  "version": "0.0.3",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "css": [
        "main.css"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "app.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  }
}
