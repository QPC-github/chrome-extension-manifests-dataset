{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Download only once",
  "description": "Asks for replacing or opening existing files instead of adding ' (1)', ' (2)', etc.",
  "version": "0.8",
  "minimum_chrome_version": "26.0.1428",
  "author": "Vendel Nagy",
  "icons": {
    "16": "fav16.png",
    "32": "fav32.png",
    "64": "fav64.png",
    "128": "fav128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "downloads",
    "downloads.open"
  ],
  "manifest_version": 2
}
