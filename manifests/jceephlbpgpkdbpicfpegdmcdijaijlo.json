{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kids these days",
  "description": "Replace 'millennials' with 'kids these days'",
  "version": "0.1.1",
  "author": "David Postlethwaite",
  "manifest_version": 2,
  "icons": {
    "16": "kidsthesedays_16.png",
    "32": "kidsthesedays_32.png",
    "64": "kidsthesedays_64.png",
    "128": "kidsthesedays_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
