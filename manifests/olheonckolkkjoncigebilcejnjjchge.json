{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IMDb/Rotten Tomatoes to Wikifeet",
  "description": "This extension will display a button to wikifeet page when it's an actor page on imdb or Rotten Tomatoes",
  "version": "0.0.5",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "wikifeet-19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "activeTab"
  ],
  "icons": {
    "48": "wikifeet-48.png",
    "128": "wikifeet-128.png"
  }
}
