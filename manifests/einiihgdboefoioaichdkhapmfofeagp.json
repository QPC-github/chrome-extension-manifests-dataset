{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Поиск на портале my-tender.ru",
  "version": "0.0.2",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "tabs",
    "http://www.my-tender.ru/*"
  ],
  "browser_action": {
    "default_title": "Поиск на портале my-tender.ru",
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  }
}
