{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "SimpleQR",
  "icons": {
    "128": "icon.png",
    "32": "icon.png"
  },
  "name": "SimpleQR",
  "options_page": "option.html",
  "permissions": [
    "storage",
    "notifications",
    "tts",
    "webRequest",
    "webRequestBlocking",
    "*://*/",
    "tabs",
    "contextMenus",
    "webNavigation",
    "clipboardWrite",
    "clipboardRead"
  ],
  "version": "1.1"
}
