{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github Latest",
  "description": "Helps you find Up & Coming Projects on Github.",
  "version": "0.1.0",
  "permissions": [
    "https://api.github.com/",
    "tabs"
  ],
  "short_name": "Github Latest",
  "icons": {
    "16": "img/icon56.png",
    "32": "img/icon56.png",
    "48": "img/icon56.png",
    "128": "img/icon56.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "img/icon56.png",
    "default_popup": "popup.html"
  }
}
