{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Grab the links from Tabs",
  "version": "1.707",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "new.js",
    "can.png"
  ],
  "browser_action": {
    "name": "Grab",
    "default_icon": "icon.jpg"
  }
}
