{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Emmet",
  "description": "Automatically adds Emmet capabilites to all textareas",
  "manifest_version": 2,
  "version": "1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "emmet.min.js"
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "textarea.png",
    "48": "textarea.png",
    "128": "textarea.png"
  }
}
