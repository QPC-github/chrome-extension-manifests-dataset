{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "9holotpk",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "icons/link_16.png",
      "32": "icons/link_32.png"
    },
    "default_title": "Shorten URL & QR Code",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "MacCtrl+I"
      },
      "description": "Opens popup.html"
    }
  },
  "description": "Fast Shorten URL for Web",
  "icons": {
    "16": "icons/link_16.png",
    "64": "icons/link_64.png",
    "128": "icons/link_128.png"
  },
  "manifest_version": 2,
  "name": "URL Shortener QR Code & Share - iShortener",
  "permissions": [
    "storage",
    "activeTab",
    "clipboardWrite",
    "declarativeContent"
  ],
  "short_name": "iShortener",
  "version": "3"
}
