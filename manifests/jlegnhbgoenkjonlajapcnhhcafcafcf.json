{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Let Me Product Hunt That For You",
  "short_name": "letmephthat for you",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "https://www.producthunt.com/*",
    "https://api.producthunt.com/*",
    "tabs",
    "identity"
  ],
  "manifest_version": 2,
  "description": "A Product Hunt Makers Festival (API edition) creation",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "product-hunt-vertical-logo-red.png",
    "default_popup": "popup.html"
  },
  "key": ""
}
