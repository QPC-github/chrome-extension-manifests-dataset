{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WP Dev Suite Demo",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Demo of Plugin Boss from WP Dev Suite",
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "Logo128x128forChromicode.png",
    "default_title": "Demo of Plugin Boss from WP Dev Suite",
    "default_popup": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "Logo128x128forChromicode.png"
  }
}
