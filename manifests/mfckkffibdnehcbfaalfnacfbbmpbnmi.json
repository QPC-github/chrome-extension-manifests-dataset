{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Image Tracer",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Tracking and downloading images from the Internet.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Click to download images",
    "default_icon": "download.png"
  },
  "incognito": "split",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "images.html"
  ]
}
