{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "version": "2.17.1",
  "minimum_chrome_version": "54",
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "homepage_url": "https://workona.com/0/",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_96.png",
    "96": "images/icon_96.png",
    "128": "images/icon_128.png",
    "256": "images/icon_256.png"
  },
  "background": {
    "scripts": [
      "runtime.js",
      "2.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "32": "images/bookmark_btn.png"
    },
    "default_title": "Workona button"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+D"
      },
      "description": "Save current tab to workspace (d)"
    },
    "manager": {
      "suggested_key": {
        "default": "Alt+A"
      },
      "description": "Open Workona (a)"
    },
    "switch": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Switch workspace (s)"
    },
    "search": {
      "suggested_key": {
        "default": "Alt+F"
      },
      "description": "Find or create (f)"
    },
    "new": {
      "description": "New workspace (n)"
    },
    "close": {
      "description": "Close workspace (w)"
    }
  },
  "permissions": [
    "contextMenus",
    "history",
    "tabs",
    "unlimitedStorage",
    "chrome://favicon/"
  ],
  "optional_permissions": [
    "system.memory"
  ],
  "omnibox": {
    "keyword": ";"
  },
  "externally_connectable": {
    "matches": [
      "https://workona.com/*",
      "https://*.workona.com/*"
    ]
  }
}
