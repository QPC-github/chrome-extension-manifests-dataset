{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Convert Text",
  "short_name": "Convert Text",
  "version": "1.1.0",
  "author": "9999yea.rs",
  "description": "Allows you to convert text from a context menu.",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "offline_enabled": true
}
