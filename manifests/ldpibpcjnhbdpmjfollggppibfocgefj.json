{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Zhuyin",
  "description": "Add zhuyin fuhao next to Chinese characters.",
  "version": "3.2",
  "background": {
    "scripts": [
      "zhuyin.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Zhuyin"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "wp010-05.ttf"
  ]
}
