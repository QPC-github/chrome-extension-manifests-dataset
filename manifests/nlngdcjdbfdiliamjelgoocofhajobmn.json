{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Visitas Pirata 3.1",
  "version": "3.1",
  "short_name": "VP_3.1",
  "description": "Recibe miles de visitas a tu sitio web, blog y redes sociales.",
  "icons": {
    "16": "logoVP-16.png",
    "48": "logoVP-48.png",
    "128": "logoVP-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "32": "logoVP-32.png"
    },
    "default_title": "Visitas Pirata",
    "default_popup": "http://www.visitaspirata.com/index.php?_a=popup_sumario"
  },
  "start_url": "http://www.visitaspirata.com/index.php?_a=popup_sumario"
}
