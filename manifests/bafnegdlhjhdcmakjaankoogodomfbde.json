{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "Continuum Explorer Community",
  "name": "Continuum Explorer Community",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Continuum Explorer Community"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "optional_permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "version": "1.11.0",
  "icons": {
    "128": "icon128.png"
  }
}
