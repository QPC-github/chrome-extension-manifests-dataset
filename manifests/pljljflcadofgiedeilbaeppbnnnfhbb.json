{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Noticias de Mar del Plata - Puntonoticias.com",
  "description": "La extensión para Google Chrome te permite saber acerca últimas noticias de Puntonoticias.com antes que nadie",
  "background": {
    "page": "background.html"
  },
  "default_locale": "es",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "notifications",
    "*://*/*"
  ],
  "version": "1.0.6"
}
