{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Latino-Latinx",
  "version": "2.1",
  "description": "This Chrome extension ensures that only 'Latino' or 'Latinx' are used to refer to people with Latin-American heritage.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "letter-l-16.png",
    "32": "letter-l-32.png",
    "48": "letter-l-48.png",
    "128": "letter-l-128.png"
  },
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "letter-l-16.png",
      "32": "letter-l-32.png",
      "48": "letter-l-48.png",
      "128": "letter-l-128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2
}
