{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "block-beggar",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Block wikipedia beggar contents.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ja.wikipedia.org/*"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_start"
    }
  ]
}
