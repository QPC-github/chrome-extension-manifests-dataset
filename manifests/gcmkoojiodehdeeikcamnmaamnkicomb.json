{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GitHub enterprise SSO to name",
  "version": "1.1.1",
  "description": "Convert enterprise SSOs to real names on GitHub",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/id-card16.png",
    "32": "images/id-card32.png",
    "48": "images/id-card48.png",
    "128": "images/id-card128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
