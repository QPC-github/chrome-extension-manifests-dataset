{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.7",
  "name": "Flanny Live",
  "description": "Sois notifié de chaque live et interagis en direct avec ta streameuse préférée !",
  "icons": {
    "128": "icon_notification.png"
  },
  "browser_action": {
    "default_popup": "pluginWindow.html"
  },
  "background": {
    "scripts": [
      "update.js"
    ]
  },
  "permissions": [
    "alarms",
    "notifications"
  ]
}
