{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/background.js",
      "js/jquery.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_64.png",
    "default_title": "Banckle.Chat Offline Notifier"
  },
  "description": "Banckle.Chat Offline Notifier help you to receive offline notifications while you are not active on Banckle.Chat Web App.",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "author": "Assad Mahmood Qazi",
  "name": "Banckle.Chat Offline Notifier",
  "permissions": [
    "tabs",
    "notifications",
    "webRequest",
    "storage",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.0.1"
}
