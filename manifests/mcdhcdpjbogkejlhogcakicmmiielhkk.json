{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fireplace",
  "version": "1.1",
  "description": "The online virtual fireplace extension. Turn any new tab into a relaxing roaring fireplace.",
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "browser_action": {},
  "permissions": [
    "storage"
  ]
}
