{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RoR News",
  "description": "Project Broomble app using JSONp API",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "news.html"
  },
  "permissions": [
    "https://projectbroomble.ap01.aws.af.cm/"
  ],
  "content_security_policy": "script-src 'self' https://projectbroomble.ap01.aws.af.cm; object-src 'self'"
}
