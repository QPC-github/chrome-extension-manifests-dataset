{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "page": "background.html"
    }
  },
  "author": "Daniel Herr",
  "description": "A simple and efficient code editor.",
  "file_handlers": {
    "any": {
      "extensions": [
        "*"
      ],
      "include_directories": true
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "name": "Code Editor",
  "permissions": [
    "contextMenus",
    "storage",
    {
      "fileSystem": [
        "directory",
        "retainEntries",
        "write"
      ]
    }
  ],
  "version": "2016.161.2",
  "version_name": "2016.161"
}
