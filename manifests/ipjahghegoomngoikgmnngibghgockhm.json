{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Look-it-up",
  "description": "Adds context menu item that looks up selected text on dictionary sites",
  "version": "0.2",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "app.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "book16.png",
    "32": "book32.png",
    "48": "book48.png",
    "128": "book128.png"
  },
  "manifest_version": 2
}
