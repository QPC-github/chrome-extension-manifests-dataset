{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FIN|Stack",
  "description": "FIN|Stack Chrome Extension",
  "version": "4.0.1",
  "icons": {
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "FIN|Stack 4.0"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/graphicsBuilder/*",
        "*://*/pub/index.html"
      ],
      "js": [
        "core/graphicsBuilderProxy.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "core/graphicsBuilder.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
