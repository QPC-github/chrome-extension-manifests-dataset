{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OP.GG Champion Search",
  "description": "Quicken the search for champion data and builds.",
  "version": "1.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.op.gg/*"
      ],
      "js": [
        "custom.js",
        "jquery-3.1.0.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "opgg16.png"
  },
  "icons": {
    "16": "opgg16.png",
    "48": "opgg48.png",
    "128": "opgg128.png"
  }
}
