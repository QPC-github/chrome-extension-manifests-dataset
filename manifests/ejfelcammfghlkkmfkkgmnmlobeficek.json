{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Flowformer Extension",
  "description": "A free focus tool to help you get one thing done or not done each day.",
  "version": "3",
  "manifest_version": 2,
  "permissions": [
    "http://flowformer.com/",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icon_16.png",
    "128": "chrome_icon_128.png"
  },
  "browser_action": {
    "default_title": "Flowformer",
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "offline_enabled": true
}
