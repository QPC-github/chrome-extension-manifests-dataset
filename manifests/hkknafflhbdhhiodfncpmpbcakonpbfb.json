{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RepoSecure",
  "version": "3.1",
  "manifest_version": 2,
  "web_accessible_resources": [
    "index.js",
    "index.html"
  ],
  "minimum_chrome_version": "33",
  "description": "This is extension for scanning github Repo",
  "browser_action": {},
  "icons": {
    "16": "assets/RepoSecure Logo V1.png",
    "128": "assets/RepoSecure Logo V1.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "identity",
    "https://github.com/",
    "http://localhost:3000/api/",
    "<all_urls>",
    "tabs",
    "storage",
    "background"
  ]
}
