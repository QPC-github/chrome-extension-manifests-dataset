{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fractal Network",
  "version": "0.2.3",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "cookies",
    "*://www.8fractals.com/",
    "*://*.8fractals.com/",
    "*://8fractals.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon_175.png",
    "default_title": "Fractal Network",
    "default_popup": "home.html"
  },
  "web_accessible_resources": [
    "presence.js"
  ]
}
