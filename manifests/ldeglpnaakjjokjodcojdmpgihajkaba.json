{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "OKSuperBoost for OKCupid",
  "short_name": "OKSuperBoost OKCupid",
  "description": "This extension boosts your visibility on OKCupid by viewing profiles for you automatically!",
  "version": "0.34",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://www.okcupid.com/*",
    "http://www.oksuperboost.appspot.com/*"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "oksb.js"
    ]
  },
  "icons": {
    "16": "oksb_16.png",
    "48": "oksb_48.png",
    "128": "oksb_128.png"
  }
}
