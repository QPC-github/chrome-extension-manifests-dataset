{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "iimob.me",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Cortando a distância entre você e a informação",
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "background.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://iimob.me/*",
    "tabs"
  ]
}
