{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Gyadoblock",
  "version": "1.4",
  "manifest_version": 2,
  "description": "This extension blocks the gyazo ads.",
  "content_scripts": [
    {
      "matches": [
        "http://gyazo.com/*"
      ],
      "css": [
        "gyadoblock.css"
      ]
    }
  ]
}
