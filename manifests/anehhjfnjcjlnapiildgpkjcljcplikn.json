{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DiG",
  "description": "Scrape your social media data at ease. Now more faster and efficient than before",
  "version": "0.0.7.1",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://facebook.com/connect/*",
    "*://*.facebook.com/*",
    "http://127.0.0.1:5000/*",
    "https://young-sea-84947.herokuapp.com/*",
    "https://www.googletagmanager.com/*",
    "*://*.producthunt.com/*",
    "*://*.linkedin.com/",
    "webRequest"
  ]
}
