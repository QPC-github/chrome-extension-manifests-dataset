{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabs to Front v2",
  "version": "2.0",
  "description": "Brings newly created tabs to the foreground.",
  "icons": {
    "16": "icon-16.jpg",
    "32": "icon-32.jpg",
    "48": "icon-48.jpg",
    "128": "icon-128.jpg"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "permissions": [
      "tabs"
    ]
  }
}
