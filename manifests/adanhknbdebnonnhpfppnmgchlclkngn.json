{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Your Email",
  "description": "Providing quick solutions for e-mail login, search and many other features at your address-bar",
  "version": "2.0",
  "default_locale": "en",
  "manifest_version": 2,
  "incognito": "split",
  "background": {},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/popup/popup.html"
  },
  "permissions": [
    "webNavigation"
  ],
  "options_ui": {
    "page": "html/popup/description.html",
    "chrome_style": true
  },
  "web_accessible_resources": [
    "*.json"
  ],
  "icons": {
    "128": "icon.png"
  }
}
