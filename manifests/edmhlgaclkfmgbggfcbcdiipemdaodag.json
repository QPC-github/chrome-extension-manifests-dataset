{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PideloRapido Toolbar",
  "version": "1.0.0.2",
  "description": "Toolbar cotizador de PideloRapido.com",
  "icons": {
    "16": "img/logos/16.png",
    "48": "img/logos/48.png",
    "128": "img/logos/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/config.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
