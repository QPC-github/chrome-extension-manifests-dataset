{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "QuickTicSearch",
  "description": "Search for any sporting event or concert from multiple market places, all at once.",
  "version": "0.01",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "QuickTicSearch"
  },
  "icons": {
    "24": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
