{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Motorlot Download Fixer",
  "description": "Fixes motorlot pdf downloads...",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "downloads",
    "tabs"
  ],
  "background": {
    "scripts": [
      "motorlotDownloads.js"
    ],
    "persistent": false
  }
}
