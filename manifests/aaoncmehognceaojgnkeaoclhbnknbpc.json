{
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "ico/lentii_extension_16X16.png",
      "32": "ico/lentii_extension_32x32.png",
      "48": "ico/lentii_extension_48x48.png",
      "128": "ico/lentii_extension_128X128.png"
    },
    "default_title": "Lentii"
