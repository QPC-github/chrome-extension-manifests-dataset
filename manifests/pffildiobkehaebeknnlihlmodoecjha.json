{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Outcognito",
  "description": "This extention disables incognito mode",
  "version": "1.0",
  "browser_action": {
    "default_icon": "outcognito.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "windows"
  ]
}
