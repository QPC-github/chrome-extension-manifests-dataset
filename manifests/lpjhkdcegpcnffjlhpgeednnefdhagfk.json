{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Prepd Offline",
  "version": "1.1.3",
  "description": "Prepd Chrome extension for competing while offline",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Prepd Offline"
  },
  "icons": {
    "48": "assets/img/png/offline-icon-48.png",
    "128": "assets/img/png/offline-icon-128.png"
  },
  "permissions": [
    "tabs",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "index.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
