{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "ALHS Daily",
    "default_popup": "popup.html"
  },
  "description": "This extension adds a tab that links to the daily announcements, lunch menu, and today's events.",
  "icons": {
    "128": "128.png"
  },
  "name": "ALHS Daily (by Project Laser)",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.1",
  "manifest_version": 2
}
