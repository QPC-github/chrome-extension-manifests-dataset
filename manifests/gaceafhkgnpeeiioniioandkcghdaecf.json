{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "name": "Fast Print",
  "description": "Fast print the current web page.",
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/ico_19.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "icons": {
    "16": "img/ico_16.png",
    "48": "img/ico_48.png"
  }
}
