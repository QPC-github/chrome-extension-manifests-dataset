{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Napolitan Weather - O' Tiemp",
  "description": "This extension allows the user to see the weather forecast in napolitan style.",
  "version": "1.8.4",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "weather.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "geolocation"
  ]
}
