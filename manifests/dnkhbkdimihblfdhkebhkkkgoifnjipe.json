{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SMMPrice History of orders",
  "version": "1.2",
  "description": "Показывает историю заказов сервиса smmprice.ru (ID заказа, ссылку, количество и актуальный статус).",
  "background": {
    "scripts": [
      "js/backend.bundle.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "img/128.png",
    "48": "img/128.png",
    "128": "img/128.png"
  },
  "browser_action": {
    "default_title": "SMMPrice: Статусы заказов",
    "default_icon": "img/128.png",
    "icons": {
      "16": "img/128.png",
      "48": "img/128.png",
      "128": "img/128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://smmprice.ru/"
  ]
}
