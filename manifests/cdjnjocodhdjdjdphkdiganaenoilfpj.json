{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "greyman",
  "version": "3.0",
  "description": "Generates a different identity for every website you visit.",
  "permissions": [
    "contentSettings",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "greyman19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "greyman128.png"
  }
}
