{
  "name": "Kzados",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Kzados - Eu vos Declaro...",
  "browser_action": {
    "default_title": "Kzados - Eu vos Declaro...",
    "default_icon": "icon16.png",
    "default_popup": "kzados_rss.html"
  },
  "icons": {
