{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "Methodist.io",
  "name": "Methodist.io - Scrum poker",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Methodist"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "version": "1.0.5",
  "author": "Volodymyr Golosay",
  "description": "Methodist.io JIRA integration plugin. Create planning room with selected issues from your JIRA backlog page.",
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  }
}
