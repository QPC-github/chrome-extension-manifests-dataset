{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  },
  "description": "This extension allows any picture from any site to be added to your NSFWit Image Bank",
  "icons": {
    "128": "bank-128.png",
    "64": "bank-64.png",
    "32": "bank-32.png",
    "16": "bank-16.png"
  },
  "manifest_version": 2,
  "name": "NSFWit.com Image Bank Adder",
  "short_name": "NSFWit.com Bank",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
    "contextMenus",
    "tabs",
    "https://nsfwit.com/*",
    "storage"
  ],
  "version": "0.1"
}
