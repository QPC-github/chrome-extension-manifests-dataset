{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TenderBro — помощник в госзакупках",
  "short_name": "TenderBro",
  "version": "1.3.2",
  "author": "Дмитрий Пройдак",
  "description": "TenderBro — помощник в госзакупках",
  "icons": {
    "16": "icons/128.png",
    "32": "icons/128.png",
    "48": "icons/128.png",
    "64": "icons/128.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "*://zakupki.gov.ru/*",
    "*://goscontract.info/*",
    "*://kad.arbitr.ru/*"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "all_frames": false,
      "matches": [
        "*://zakupki.gov.ru/*",
        "*://goscontract.info/*",
        "*://kad.arbitr.ru/*"
      ],
      "js": [
        "js/jquery-3.3.1.min.js",
        "js/script.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Помощник zakupki.gov.ru",
    "default_icon": "icons/128.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/*.png"
  ]
}
