{
  "background": {
    "scripts": [
      "js/background.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_popup": "popup.html",
    "default_title": "百度云分享链接一键登录"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.2.1.4.min.js",
        "js/content.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "64": "images/icon_64.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2,
  "name": "百度云分享链接一键登录",
  "short_name": "百度云分享一键登录",
  "description": "检测网页中的百度云分享链接，在扩展中点击链接可以一键登录，免去输入密码的繁琐步骤。",
  "author": "bmqy",
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.12"
}
