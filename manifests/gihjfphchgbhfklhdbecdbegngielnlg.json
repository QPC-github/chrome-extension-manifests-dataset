{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Snake",
  "version": "1.2",
  "description": "A classic Snake Game recreated for easy use!",
  "icons": {
    "128": "snake128.png",
    "48": "snake48.png",
    "16": "snake16.png"
  },
  "browser_action": {
    "default_icon": "snake16.png",
    "default_popup": "snake.html"
  },
  "permissions": [
    "storage"
  ]
}
