{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NewTabkr",
  "description": "NewTabkr shows you lovely Flickr images of a chosen topic on your new Chrome tab",
  "version": "1.0.3",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
