{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Fracture it",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Shorten URLs like a boss",
  "browser_action": {
    "default_icon": "fracture_19.png",
    "default_title": "Fracture it"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "listener.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://fracture.it/"
  ],
  "icons": {
    "48": "fracture_48.png",
    "128": "fracture_128.png"
  }
}
