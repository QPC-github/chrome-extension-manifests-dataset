{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Video Durations for reddit",
  "version": "1.0.3",
  "icons": {
    "48": "vdfr48.png",
    "128": "vdfr128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "js": [
        "jquery-2.2.4.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
