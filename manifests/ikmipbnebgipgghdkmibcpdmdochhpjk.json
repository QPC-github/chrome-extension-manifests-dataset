{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Shell Search",
  "description": "Search commands for omnibox",
  "manifest_version": 2,
  "version": "0.0.7.0",
  "background": {
    "persist": false,
    "scripts": [
      "jquery.js",
      "map.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "omnibox": {
    "keyword": "s"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ]
}
