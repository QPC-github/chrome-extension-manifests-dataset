{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": true,
    "scripts": [
      "script.js"
    ]
  },
  "description": "Extension to allow screen sharing via Joinin TV.",
  "externally_connectable": {
    "matches": [
      "*://*.joinin.tv/*"
    ]
  },
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "web_accessible_resources": [
    "icon_16.png"
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Joinin TV",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "joinin.tv",
  "version": "0.0.0.8"
}
