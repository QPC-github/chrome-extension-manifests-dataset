{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TrafficArc.com/VisitExchange",
  "description": "System for visit exchange",
  "version": "0.0.1",
  "icons": {
    "16": "images/trafficark.16.png",
    "48": "images/trafficark.48.png",
    "128": "images/trafficark.128.png"
  },
  "browser_action": {
    "default_icon": "images/trafficark.128.png",
    "default_title": "Start visit exchange"
  },
  "background": {
    "scripts": [
      "scripts/app.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
