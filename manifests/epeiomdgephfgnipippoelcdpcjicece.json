{
  "name": "IP button",
  "version": "0.3",
  "manifest_version": 2,
  "description": "It shows you your IP without going on other websites",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "permissions": [
    "webRequest",
    "http://banana-soft.com/"
  ]
}
