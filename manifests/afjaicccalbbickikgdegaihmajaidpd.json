{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "I'm a Gentleman",
  "version": "36",
  "manifest_version": 2,
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "19.png",
    "default_title": "__MSG_buttonTip__"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "images.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "downloads"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
