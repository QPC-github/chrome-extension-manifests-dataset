{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab Anywhere",
  "description": "New Tab Anywhere",
  "version": "0.1.02",
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "tabs128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus",
    "storage",
    "webNavigation"
  ],
  "icons": {
    "16": "tabs16.png",
    "48": "tabs48.png",
    "128": "tabs128.png"
  }
}
