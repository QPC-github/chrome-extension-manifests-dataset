{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "True DNS Analysis / intoDNS",
  "description": "1 click instant access, CAPTCHA free DNS lookup of any domain including ccTLD and new gTLD domains",
  "version": "2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-1.7.2.min.js",
      "effectiveTLDs.inc.js",
      "regDomain.inc.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png"
  }
}
