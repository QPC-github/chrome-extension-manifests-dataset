{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "archive.is paster",
  "version": "1.2",
  "description": "simple archive.is paster",
  "background": {
    "scripts": [
      "observer.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "archive.is 로 추가합니다"
  },
  "permissions": [
    "activeTab",
    "*://*.archive.is/*"
  ],
  "manifest_version": 2
}
