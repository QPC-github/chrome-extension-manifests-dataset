{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BeyondCarl速 - Inspirational Quotes",
  "description": "BeyondCarl速 is the donkey sidekick who makes your day a bit more interesting with anecdotal observations and funny quotes.",
  "short_name": "BeyondCarl速 - Inspirational Quotes",
  "author": "BeyondCarl速 - Creators",
  "version": "0.8.9.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "contextMenus",
    "cookies",
    "activeTab",
    "storage",
    "notifications",
    "<all_urls>",
    "activeTab",
    "webRequest",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/animate.css",
        "css/bubble.css",
        "css/style.css"
      ],
      "js": [
        "js/jquery.js",
        "js/main.js"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  }
}
