{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Google Eyes by Blue Fountain Media",
  "description": "View a web page through Google's eyes by loading the Google cached text-only version of the current page.",
  "manifest_version": 2,
  "version": "2",
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "page": "to.html"
  },
  "permissions": [
    "tabs",
    "http://webcache.googleusercontent.com/*",
    "https://webcache.googleusercontent.com/*"
  ]
}
