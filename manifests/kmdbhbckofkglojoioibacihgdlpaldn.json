{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Breda Blocker",
  "version": "1.1",
  "description": "Hide Breda O'Brien and John Waters from the Irish Times Website",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.irishtimes.com/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ]
}
