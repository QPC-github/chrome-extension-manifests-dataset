{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IonEPPM Extension",
  "manifest_version": 2,
  "version": "0.0.0.2",
  "description": "Extension for IonEPPM application",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "core.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "privacy",
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}
