{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SimpleNotes",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Take notes, save them.",
  "options_page": "popup.html",
  "short_name": "SimpleNotes",
  "browser_action": {
    "default_title": "Notes",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ]
}
