{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bitglass Chrome Extension",
  "version": "1.3.0",
  "manifest_version": 2,
  "externally_connectable": {
    "matches": [
      "https://*.btglss.net/*",
      "https://*.us.bitglass.net/*",
      "https://*.us.bgcls.com/*",
      "https://*.bgcls2.net/*"
    ]
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "128": "128.png"
  },
  "incognito": "split",
  "permissions": [
    "desktopCapture",
    "processes",
    "system.cpu"
  ]
}
