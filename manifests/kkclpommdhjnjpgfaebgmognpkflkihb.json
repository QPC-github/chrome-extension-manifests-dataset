{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Open In UK SEMRUSH",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Opens the current domain in UK SEMRUSH",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
