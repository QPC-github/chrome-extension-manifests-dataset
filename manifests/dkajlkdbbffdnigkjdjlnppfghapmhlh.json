{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "百度搜索自动转到Bing",
  "description": "再见,垃圾百度.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "background",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://www.baidu.com/s?*"
  ]
}
