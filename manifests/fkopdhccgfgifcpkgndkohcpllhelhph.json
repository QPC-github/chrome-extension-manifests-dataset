{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Haskell inspector",
  "description": "Adds an '>>' omnibox command and find results from hoogle, hackage",
  "version": "1.0",
  "author": "Gonthier Olivier",
  "permissions": [
    "http://haskell.org/*",
    "http://*.haskell.org/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "omnibox": {
    "keyword": ">>"
  },
  "background": {
    "scripts": [
      "background.js",
      "jquery-2.1.0.min.js"
    ]
  }
}
