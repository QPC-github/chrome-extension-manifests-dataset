{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BTC-E exchange rates",
  "description": "BTC-E bitcoin exchange rates plugin.",
  "short_name": "BTC-E Rates",
  "version": "1.0",
  "icons": {
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://btc-e.com/*"
  ]
}
