{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "installed.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "OpenText Secure MFT",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "OpenText Secure MFT",
  "permissions": [
    "tabs",
    "nativeMessaging",
    "https://*/*",
    "http://*/*"
  ],
  "version": "1.10",
  "applications": {
    "gecko": {
      "id": "securemft_opentextappstore@gmail.com",
      "strict_min_version": "52.0"
    }
  }
}
