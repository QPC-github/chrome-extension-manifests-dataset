{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scroll Up/Down",
  "version": "1.0.0",
  "description": "Convenient extension with which you can quickly navigate to the page up/down or to the comments.",
  "default_locale": "en",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/top.png",
    "images/bottom.png",
    "images/comment.png"
  ]
}
