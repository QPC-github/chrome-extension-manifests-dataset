{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Your Chrome Assistant",
  "short_name": "YCA",
  "version": "1.0",
  "description": "This extension provide multiple handy features which can be very useful while browsing.",
  "author": "Dipak Jain",
  "permissions": [
    "processes",
    "tabs",
    "chrome://favicon/*"
  ],
  "browser_action": {
    "default_title": "Your Chrome Assistant",
    "default_icon": "app-icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
