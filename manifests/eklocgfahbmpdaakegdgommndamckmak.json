{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide News Feed",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Hide the Facebook(new design) News Feed to focus more on your goals",
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/",
        "https://*.facebook.com/?*"
      ],
      "css": [
        "hide-facebook-newsfeed.css"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
