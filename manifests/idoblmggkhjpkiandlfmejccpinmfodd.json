{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Fallingrain Modern NewTab",
  "options_page": "options.html",
  "version": "1.1",
  "description": "This newtab represents the modern era! A Fallingrain modern newtab featuring time and date.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "history"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "page.html"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "images/icon.png"
    }
  },
  "manifest_version": 2
}
