{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "images/logo48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/logo128.png",
    "16": "images/logo16.png",
    "32": "images/logo32.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "This tool aims at giving Google Apps Administrators easy links to essential troubleshooting tools and to Revevol Support Platform.",
  "manifest_version": 2,
  "name": "GApps Tools for Admins by Revevol",
  "permissions": [
    "https://*.google.com/*"
  ],
  "version": "0.6.1"
}
