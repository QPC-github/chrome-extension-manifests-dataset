{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JT's Stock Search",
  "description": "This extension allows the user to search stock information and save it as favorite.",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon1.png",
    "default_popup": "popup.html",
    "default_title": "Stock Search"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://code.angularjs.org https://cdnjs.cloudflare.com https://maxcdn.bootstrapcdn.com https://gitcdn.github.io; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "http://cs-server.usc.edu:37394/hw8_backend/index.php",
    "https://code.angularjs.org/1.5.0/angular.js",
    "https://code.angularjs.org/1.5.0/angular-aria.js",
    "https://code.angularjs.org/1.5.0/angular-animate.js",
    "https://cdnjs.cloudflare.com/ajax/libs/angular-material/1.1.5/angular-material.min.css",
    "https://cdnjs.cloudflare.com/ajax/libs/angular-material/1.1.5/angular-material.min.js",
    "https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js",
    "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css",
    "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
    "https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css",
    "https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js",
    "file://*/*"
  ]
}
