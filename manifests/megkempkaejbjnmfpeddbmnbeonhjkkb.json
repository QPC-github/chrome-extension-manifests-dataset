{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chromforter",
  "version": "1.5.1",
  "description": "__MSG_manifest_description__",
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "notifications",
    "downloads",
    "downloads.open",
    "downloads.shelf",
    "alarms",
    "contextMenus",
    "storage"
  ],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_title": "Chromeforter",
    "default_popup": "browserAction/popup.html"
  },
  "manifest_version": 2
}
