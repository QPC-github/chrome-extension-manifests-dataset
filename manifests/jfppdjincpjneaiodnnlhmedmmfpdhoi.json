{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cloud Converter",
  "version": "1.0.0.1",
  "description": "Convert your audio and video files within seconds. Free and simple to use.",
  "icons": {
    "512": "icon-512.png",
    "256": "icon-256.png",
    "128": "icon-128.png",
    "96": "icon-96.png",
    "64": "icon-64.png",
    "48": "icon-48.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "container": [
    "DOMAIN_INSTALLABLE",
    "GOOGLE_DRIVE"
  ],
  "api_console_project_id": "214567865127",
  "app": {
    "launch": {
      "web_url": "https://www.convertaudiofiles.com/convert"
    }
  },
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/vnd.google.drive.ext-type.aiff",
          "application/vnd.google.drive.ext-type.asf",
          "application/vnd.google.drive.ext-type.avi",
          "application/vnd.google.drive.ext-type.bfi",
          "application/vnd.google.drive.ext-type.caf",
          "application/vnd.google.drive.ext-type.flv",
          "application/vnd.google.drive.ext-type.gif",
          "application/vnd.google.drive.ext-type.gxf",
          "application/vnd.google.drive.ext-type.hls",
          "application/vnd.google.drive.ext-type.iff",
          "application/vnd.google.drive.ext-type.mkv",
          "application/vnd.google.drive.ext-type.mpeg",
          "application/vnd.google.drive.ext-type.mxf",
          "application/vnd.google.drive.ext-type.nut",
          "application/vnd.google.drive.ext-type.ogg",
          "application/vnd.google.drive.ext-type.oma",
          "application/vnd.google.drive.ext-type.rl2",
          "application/vnd.google.drive.ext-type.txd",
          "application/vnd.google.drive.ext-type.wtv",
          "application/vnd.google.drive.ext-type.mp3"
        ],
        "href": "https://www.convertaudiofiles.com/convert",
        "title": "Open",
        "disposition": "window"
      }
    ]
  }
}
