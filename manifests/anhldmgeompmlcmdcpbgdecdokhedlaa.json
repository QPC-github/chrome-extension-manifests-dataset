{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "jarPlug",
  "version": "1.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.plug.dj/*"
      ],
      "js": [
        "loadJarPlug.js"
      ]
    }
  ],
  "permissions": [
    "notifications"
  ]
}
