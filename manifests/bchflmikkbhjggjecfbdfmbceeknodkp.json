{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Safename Lite",
  "version": "1.1.0",
  "description": "Self-contained Search Box Tool for SafeName and Address",
  "permissions": [
    "declarativeContent",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/x16.png",
    "32": "images/x32.png",
    "48": "images/x48.png",
    "128": "images/x128.png"
  },
  "manifest_version": 2
}
