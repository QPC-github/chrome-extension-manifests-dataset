{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SkipAds",
  "short_name": "SkipAds",
  "description": "Utility to skip ad automatically",
  "manifest_version": 2,
  "version": "0.3",
  "author": "Aman A. Agarwal",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
