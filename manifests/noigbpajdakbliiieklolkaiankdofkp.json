{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Search engine plus",
  "version": "1.3",
  "description": " Power up your search experience.",
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*",
        "*://*.google.co.in/*",
        "*://*.google.ca/*",
        "*://*.google.cn/*",
        "*://*.google.de/*",
        "*://*.google.fi/*",
        "*://*.google.fr/*",
        "*://*.google.hk/*",
        "*://*.google.co.id/*",
        "*://*.google.co.il/*",
        "*://*.google.co.jp/*",
        "*://*.google.co.th/*",
        "*://*.google.co.uk/*",
        "*://*.google.co.zs/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "like.png",
    "reddit.png"
  ],
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png"
  }
}
