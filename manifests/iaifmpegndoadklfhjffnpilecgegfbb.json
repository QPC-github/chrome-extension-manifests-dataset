{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pinterest Sort Premium",
  "description": "...",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "contentscript.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "successPopup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications"
  ]
}
