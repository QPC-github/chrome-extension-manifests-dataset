{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "myUnina",
  "description": "Tieni sempre sotto controllo gli avvisi dei docenti!",
  "version": "0.4.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },
  "permissions": [
    "*://*.docenti.unina.it/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "jquery.min.js",
      "date.js",
      "sha256.js",
      "background.js"
    ]
  }
}
