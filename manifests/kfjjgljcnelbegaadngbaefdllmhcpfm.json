{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stack Snippets",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Used to insert code into stack snippets",
  "homepage_url": "https://boywithsilverwings.github.io",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://stackoverflow.com/questions/*",
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs"
  ],
  "page_action": {
    "default_popup": "./index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    }
  }
}
