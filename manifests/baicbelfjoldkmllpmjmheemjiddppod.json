{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Garth Blocks",
  "description": "Clean up websites from Garth Brooks mentions.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-latest.js",
        "garth-blocks.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/garth-blocks16.png",
    "32": "icons/garth-blocks32.png",
    "48": "icons/garth-blocks48.png",
    "64": "icons/garth-blocks64.png",
    "128": "icons/garth-blocks128.png",
    "256": "icons/garth-blocks256.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0.2"
}
