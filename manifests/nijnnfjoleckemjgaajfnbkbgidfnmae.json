{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tedyfy",
  "description": "Make the current page Tedy",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js",
      "gettedy.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/tedy16.png",
    "48": "images/tedy48.png",
    "128": "images/tedy128.png"
  },
  "browser_action": {
    "default_title": "Tedyfy"
  },
  "web_accessible_resources": [
    "images/*.png",
    "images/*.jpg"
  ],
  "manifest_version": 2
}
