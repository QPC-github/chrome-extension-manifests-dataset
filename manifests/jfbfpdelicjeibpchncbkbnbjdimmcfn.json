{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Too Much Bernie",
  "description": "Hides Bernie on Reddit",
  "version": "1.0",
  "permissions": [
    "tabs",
    "*://www.reddit.com/*",
    "contextMenus"
  ],
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.reddit.com/*"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ]
    }
  ]
}
