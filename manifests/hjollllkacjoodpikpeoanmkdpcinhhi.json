{
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery/jquery.min.js",
      "src/bg/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "src/browser_action/login_form.html",
    "default_title": "TxTDash Live Chat Manager"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/login.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "Manages all your active text and web chats with live notifications",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "TxTDash Live Chat Manager : Ver 2.8.0",
  "permissions": [
    "tabs",
    "alarms",
    "activeTab",
    "contextMenus",
    "storage",
    "cookies",
    "*://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.8.0",
  "web_accessible_resources": [
    "src/browser_action/Close_Box_Red.png",
    "src/browser_action/digarrow.png",
    "src/browser_action/soun1060.mp3",
    "src/browser_action/insertClicks.js"
  ]
}
