{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "T21 Finder",
  "description": "Finds T-21 devices on the local network",
  "manifest_version": 2,
  "version": "0.1.0",
  "app": {
    "background": {
      "scripts": [
        "background.js",
        "jquery-3.1.0.min.js"
      ]
    }
  },
  "sockets": {
    "udp": {
      "bind": "*",
      "send": "*"
    }
  },
  "permissions": [
    "system.network",
    "http://*/",
    "webview"
  ],
  "icons": {
    "128": "img/icon-120.png",
    "16": "img/icon-16.png"
  }
}
