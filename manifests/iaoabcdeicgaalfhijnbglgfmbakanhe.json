{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AWS re:Invent Catalog Search",
  "description": "This extension searches for selected text in the current AWS reInvent Catalog via right-click and shows the results in a new tab.",
  "version": "1.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "locales.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {
    "16": "reInvent16.png",
    "48": "reInvent48.png",
    "128": "reInvent128.png"
  }
}
