{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chrome With Quote",
  "description": "Every new tab in chrome improves your general knowledge about numbers and year",
  "author": "Shivam Dembla",
  "homepage_url": "http://shivamdembla.com",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "120": "icons/icon120.png"
  },
  "permissions": [
    "https://source.unsplash.com/",
    "http://numbersapi.com/random/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
