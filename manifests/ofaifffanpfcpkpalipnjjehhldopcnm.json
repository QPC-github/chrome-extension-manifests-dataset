{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MovieTabs",
  "description": "Discover new movies and tv shows with every tab. Every new tab you open will present wallpapers from a random movie or tv show.",
  "version": "1.0.6",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "movies.html"
  },
  "permissions": [
    "storage",
    "http://*.wipmania.com/"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  },
  "options_page": "options.html"
}
