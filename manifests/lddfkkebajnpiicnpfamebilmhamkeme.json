{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Github +1s",
  "description": "Shows you the counts and avatars of those noisy '+1' comments and hides the original comments.",
  "version": "1.4.0",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "128": "plusone.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
