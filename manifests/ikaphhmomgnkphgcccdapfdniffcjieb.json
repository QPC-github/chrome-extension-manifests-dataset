{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Teamforge Search Quick Access",
  "description": "This extension allows quick access to TF searches and shows the number of results",
  "version": "0.4",
  "options_page": "settings.html",
  "background": {
    "scripts": [
      "thirdParty/jquery-3.1.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "background",
    "notifications",
    "storage",
    "alarms"
  ]
}
