{
  "browser_action": {
    "default_icon": "icon128T.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "This extension allow to see youtube's videos without any problems.",
  "name": "the free-watcher",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png",
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "version": "1.0.1.1"
}
