{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Stash It",
  "short_name": "Stash It",
  "description": "Bookmarks any page to your account at Stash It",
  "homepage_url": "https://stashit.pw",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Stash It"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "1.8",
  "manifest_version": 2
}
