{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "bijin-schedule",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://www.google.com/calendar/feeds/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken",
    "http://chromeapp.bijint.com/*",
    "http://chromeapp.bijint.com/main/*",
    "http://chromeapp.bijint.com/popup/*",
    "http://www.bijint.com/*",
    "http://daily.bijint.com/gadget/desctop.json",
    "https://ssl.google-analytics.com/ga.js",
    "tabs",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "description": "bijin-schedule",
  "icons": {
    "128": "icon128.png",
    "96": "icon96.png",
    "48": "icon48.png",
    "19": "icon19.png"
  },
  "version": "1.1.1",
  "browser_action": {
    "default_icon": "icon96.png",
    "default_title": "bijin-schedule",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "chrome_48.png",
    "https://ssl.google-analytics.com/ga.js"
  ],
  "default_locale": "en"
}
