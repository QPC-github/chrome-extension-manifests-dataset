{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Make UNMJobs Great Again",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://unm-pilot.csod.com/*",
        "https://unm.csod.com/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Make UNMJobs Great Again!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
