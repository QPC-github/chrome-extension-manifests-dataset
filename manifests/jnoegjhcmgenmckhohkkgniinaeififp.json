{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Readd Google+ Notifications",
  "description": "Injects the notification icon back into Google+.",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://plus.google.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2
}
