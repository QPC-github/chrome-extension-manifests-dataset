{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "short_name": "__MSG_extShortName__",
  "homepage_url": "https://trackblock.org",
  "icons": {
    "16": "icons/active/icon16.png",
    "48": "icons/active/icon48.png",
    "128": "icons/active/icon128.png"
  },
  "default_locale": "en",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://ssl.google-analytics.com https://www.google.com; object-src https://www.google-analytics.com https://ssl.google-analytics.com https://www.google.com 'self'",
  "background": {
    "scripts": [
      "src/bg/background.js",
      "src/bg/stuff.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/active/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "contextMenus",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "*"
  ]
}
