{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "One Extension",
  "description": "One Extension Manages all extensions",
  "version": "1.0.9",
  "icons": {
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_title": "One Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "management",
    "storage",
    "webRequest"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
