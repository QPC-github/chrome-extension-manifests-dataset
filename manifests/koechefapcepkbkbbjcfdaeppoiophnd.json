{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/eventPage.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/logo16.png",
    "default_popup": "oneindia.html",
    "default_title": "Oneindia Latest News"
  },
  "description": "News, Latest News, Lifestyle Updates, Filmi Gossips, Gadgets , Automobiles News, Business News, Stock Market Rates, Gold Rates",
  "icons": {
    "128": "images/logo128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://pagead2.googlesyndication.com https://adservice.google.co.in https://adservice.google.com; object-src 'self'",
  "name": "Oneindia News",
  "permissions": [
    "idle",
    "tabs",
    "activeTab",
    "contextMenus",
    "notifications",
    "alarms",
    "*://*.oneindia.com/*",
    "*://*.gizbot.com/*",
    "*://*.drivespark.com/*",
    "*://*.goodreturns.com/*",
    "*://*.filmibeat.com/*",
    "*://*.boldsky.com/*",
    "*://*.mykhel.com/*",
    "*://*.nativeplanet.com/*",
    "http://rlinks.one.in/*",
    "http://www.oiscripts.com/*"
  ],
  "version": "9.2",
  "web_accessible_resources": [
    "images/logo128.png",
    "fonts/*.*"
  ],
  "short_name": "Oneindia Latest News"
}
