{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Night",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "/night.js"
  ],
  "permissions": [
    "activeTab"
  ]
}
