{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shhh - history blocker",
  "description": "no one will know... ever...",
  "version": "0.4",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "storage",
    "history",
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon-enabled16.png"
  }
}
