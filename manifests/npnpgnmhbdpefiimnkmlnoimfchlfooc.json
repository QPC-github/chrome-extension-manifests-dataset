{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_ext_name__",
  "short_name": "__MSG_short_name__",
  "description": "__MSG_ext_desc__",
  "default_locale": "ru",
  "manifest_version": 2,
  "version": "1.1.2",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "__MSG_default_title__"
  },
  "permissions": [
    "background",
    "bookmarks",
    "unlimitedStorage",
    "<all_urls>",
    "notifications",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  }
}
