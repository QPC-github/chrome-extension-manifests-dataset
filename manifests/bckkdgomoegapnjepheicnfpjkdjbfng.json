{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Wingobear's Extension",
  "description": "Informe l'utilisateur de l'activité du stream de WINGOBEAR et le previent grâce à une notification.",
  "version": "1.7.1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "64": "./img/icon_64(1).png"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ]
  },
  "options_page": "options/options.html",
  "permissions": [
    "notifications",
    "storage"
  ]
}
