{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Nope",
  "version": "1.1",
  "manifest_version": 2,
  "author": "Armand Grillet",
  "description": "Are you spending too much time on a website? Nope it!",
  "homepage_url": "https://github.com/ArmandGrillet/Nope",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "nope.html"
  ]
}
