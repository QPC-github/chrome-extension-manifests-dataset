{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vëllezërit Hajni",
  "version": "1.0",
  "description": "Zëvendëson mbiemrat e vëllezërve me mbiemrin e tyre të përbashkët, Hajni.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
