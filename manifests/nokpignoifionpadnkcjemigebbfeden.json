{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "miki",
  "version": "0.1.0",
  "description": "Mobile wikipedia",
  "manifest_version": 2,
  "minimum_chrome_version": "50",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://en.wikipedia.org/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
