{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "companyBubble",
  "description": "displays employment-related company information in the form of tooltips (bubbles)",
  "version": "0.0.2",
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "webNavigation",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "companyBubble2.png",
    "48": "companyBubble2.png",
    "128": "companyBubble2.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "companyBubble2.png",
      "38": "companyBubble2.png"
    },
    "default_title": "companyRS",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://api.indeed.com; object-src 'self'",
  "manifest_version": 2
}
