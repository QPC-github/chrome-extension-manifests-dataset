{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Agile",
  "version": "1.75",
  "description": "A simple grid of handy web shortcuts. Changes your NewTab page to Agile as well as adding a quick access menu in the extension icon.",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
