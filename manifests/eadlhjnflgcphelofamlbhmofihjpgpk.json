{
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "ShoppersOffers Finder",
  "homepage_url": "http://shoppersoffers.co/",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/button.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "ShoppersOffers Finder",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1",
  "web_accessible_resources": [
    "extension.js"
  ]
}
