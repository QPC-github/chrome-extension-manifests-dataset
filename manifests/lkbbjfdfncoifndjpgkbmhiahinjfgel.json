{
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/icone/128.png",
    "default_title": "Full HD NewTab"
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "description": "This extension themes your new tab with HD wallpapers from the epic game and movie series Tomb Raider (Lara Croft)",
  "icons": {
    "128": "img/icone/128.png"
  },
  "name": "Tomb Raider Wallpapers New Tab Theme.",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "contextMenus",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "topSites"
  ],
  "short_name": "Full HD NewTab",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2",
  "web_accessible_resources": [
    "pages/newtab.html"
  ]
}
