{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Referenciar links e imagens - Design Aqui",
  "description": "Salve seus links favoritos e de interesse em seu perfil no Design Aqui",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Bookmark this page!"
  },
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "128": "icon128.png"
  }
}
