{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "menu.html",
    "default_title": "Speed Dial"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "homepage_url": "http://speeddial2.com",
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://stats.g.doubleclick.net https://cdn.paddle.com https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "topSites",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "history",
    "management",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "<all_urls>",
    "contextMenus",
    "alarms"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.1.1",
  "web_accessible_resources": [
    "newtab.html"
  ]
}
