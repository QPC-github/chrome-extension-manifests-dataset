{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UIU eLMS Simplifier",
  "version": "1.0",
  "author": "Sardar Moktadir Ibna Mohsin",
  "description": "Make UIU eLMS a better place.",
  "icons": {
    "16": "icons/logo16.png",
    "32": "icons/logo32.png",
    "48": "icons/logo48.png",
    "96": "icons/logo96.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/logo16.png",
      "32": "icons/logo32.png",
      "48": "icons/logo48.png",
      "96": "icons/logo96.png"
    },
    "default_title": "UIU eLMS Simplifier"
  },
  "content_scripts": [
    {
      "matches": [
        "*://elms.uiu.ac.bd/*"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/script.js"
      ]
    }
  ]
}
