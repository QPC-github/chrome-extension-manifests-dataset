{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Granblue DPS/DPT",
  "description": "グラブルのDPS/DPTが計測できます。ディベロッパーツール内(F12)で動作します。",
  "version": "1.15",
  "icons": {
    "48": "image/icon48.png"
  },
  "background": {
    "scripts": [
      "js/jquery-3.2.1.min.js",
      "js/common.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "image/icon19.png",
    "default_title": "Granbule DPS/DPT"
  },
  "devtools_page": "html/devtools.html",
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "storage",
    "*://*.granbluefantasy.jp/*"
  ]
}
