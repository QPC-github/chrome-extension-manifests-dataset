{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Óvirkur í athugasemdum",
  "version": "0.1.5",
  "description": "Felur athugasemdir frá sjálfskipuðum sérfræðingum.",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.visir.is/*",
        "*://*.dv.is/*",
        "*://*.frettabladid.is/*",
        "*://*.stundin.is/*"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
