{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "2IP сервисы",
  "description": "Тесты и сервисы 2IP.ru в Вашем браузере",
  "version": "1.3",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_title": "2IP",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-block.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  }
}
