{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Linkedin Bookmark",
  "description": "Bookmark favorite Linkedin feed items",
  "version": "2.1.3",
  "manifest_version": 2,
  "permissions": [
    "https://www.linkedin.com/*",
    "bookmarks"
  ],
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/jquery-ui.js",
        "js/script.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "/js/options_manager.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "html/options.html",
  "web_accessible_resources": [
    "/html/bookmark_ui.html",
    "img/*"
  ]
}
