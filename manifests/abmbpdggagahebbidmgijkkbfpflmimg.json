{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GIF playmate",
  "version": "1.0.2",
  "description": "Play GIFs in Google Search Results",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*tbm=isch*",
        "https://www.google.co.jp/search*tbm=isch*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "manifest_version": 2
}
