{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JalanTikus.com",
  "short_name": "JalanTikus",
  "version": "1.9.8",
  "description": "Best Tech Site No. 1 in Indonesia: Solusi Lengkap Download, Informasi Teknologi dan Gadget",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "background",
    "*://jalantikus.com/*",
    "*://assets.jalantikus.com/*"
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
