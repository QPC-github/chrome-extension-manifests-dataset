{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MohabatNews",
  "version": "1.5.1",
  "manifest_version": 2,
  "description": "MohabatNews content aggregator.",
  "author": "Danial Shahri",
  "permissions": [
    "http://mohabatnews.com/*/*"
  ],
  "icons": {
    "16": "./img/16x16.png",
    "48": "./img/48x48.png",
    "128": "./img/128x128.png"
  },
  "browser_action": {
    "default_icon": "./img/128x128.png",
    "default_title": "Mohabat News",
    "default_popup": "popup.html"
  }
}
