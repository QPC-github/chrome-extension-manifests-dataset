{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Gifs for Twitter",
  "description": "Inlines gifs into twitter.com",
  "version": "1.0.3",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "https://twitter.com/*",
    "tabs"
  ],
  "page_action": {
    "default_icon": {
      "19": "icon-19.png"
    }
  },
  "manifest_version": 2,
  "icons": {
    "19": "icon-19.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "eventpage.js"
    ],
    "persistent": false
  }
}
