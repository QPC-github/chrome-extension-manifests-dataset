{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yag",
  "description": "This extension allows the user to cpost a web page to his profile",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://yag.social/oauth/token",
    "https://yag.social/api/post"
  ]
}
