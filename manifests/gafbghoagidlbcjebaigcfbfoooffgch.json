{
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/128.png"
  },
  "content_scripts": [
    {
      "css": [
        "css/styles.css"
      ],
      "js": [
        "js/jquery-3.4.2.min.js",
        "js/content.js"
      ],
      "matches": [
        "https://www.google.com/*",
        "https://www.google.fr/*",
        "https://anywarestudio.forumactif.com/*",
        "https://www.facebook.com/*",
        "https://wwww.twitter.com/*",
        "https://www.discord.gg/*",
        "https://www.astragon.de/*",
        "https://www.rockstarmag.fr/*",
        "https://www.bussimulatorguide.com/*",
