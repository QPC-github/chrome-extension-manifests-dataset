{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Boldrinize",
  "description": "Immaginate se la gente reagisse ai gesti di Salvini o Di Maio se fossero opera della Boldrini.",
  "version": "1.0.1",
  "author": "Daniele Mancino",
  "homepage_url": "http://danielemancino.com/boldrinize",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "icon.png",
    "96": "icon@2x.png"
  }
}
