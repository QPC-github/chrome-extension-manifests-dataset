{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No More Empty New Tabs",
  "description": "Open new tabs with your desired website.",
  "version": "1.1.5",
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "popup.html",
    "default_title": "No More Empty New Tabs"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "management"
  ],
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  }
}
