{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SMOService Invoices",
  "version": "2.0",
  "description": "Показывает основную информацию о заказах с сайта smoservice.ru (ссылка, номер заказа и статус выполнения).",
  "background": {
    "scripts": [
      "js/backend.bundle.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "img/128logo.png",
    "48": "img/128logo.png",
    "128": "img/128logo.png"
  },
  "browser_action": {
    "default_title": "SMOService: Статусы заказов",
    "default_icon": "img/128.png",
    "icons": {
      "16": "img/128.png",
      "48": "img/128.png",
      "128": "img/128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://smoservice.ru/"
  ]
}
