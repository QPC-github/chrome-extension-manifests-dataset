{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Twlets"
  },
  "name": "Twlets: Twitter to Excel",
  "short_name": "Twlets",
  "description": "Download tweets, followers, likes and more from Twitter to Excel with a single click.",
  "homepage_url": "http://twlets.com",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://twlets.com/*",
    "https://twitter.com/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "version": "0.3.0",
  "manifest_version": 2,
  "author": "Talha Oz"
}
