{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GoodPix Product Clipper",
  "version": "4.4",
  "description": "Easily grab a product's image, link and product details for your GoodPix catalog.",
  "browser_action": {},
  "permissions": [
    "*://localhost/*",
    "*://mjyn9nihsh.execute-api.us-east-1.amazonaws.com/dev/clipper/*",
    "*://otfe2tnhsh.execute-api.us-east-1.amazonaws.com/prod/clipper/*",
    "contextMenus",
    "cookies",
    "activeTab",
    "*://*.goodpix.co/*",
    "*://*.goodpix.io/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "clipper2x.png",
    "48": "clipper2x.png",
    "128": "clipper2x.png",
    "400": "clipper2x.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
