{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Time Tracker",
  "description": "That tool usefull for all employee and Company Both.",
  "version": "1.3",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "cookies",
    "storage",
    "http://*/*",
    "https://*/*",
    "tabs",
    "alarms"
  ],
  "manifest_version": 2
}
