{
  "name": "Праздники VoiceCards.ru",
  "description": "Напоминания о ближайших проффесиональных и календарных праздников от сервиса голосовых открыток VoiceCards.ru",
  "browser_action": {
    "default_icon": "19x19.png",
    "default_title": "Ближайшие праздники",
    "default_popup": "popup.html"
  },
  "icons": {
    "19": "19x19.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0.5",
  "permissions": [
    "notifications",
    "https://*.voicecards.ru/*"
  ]
}
