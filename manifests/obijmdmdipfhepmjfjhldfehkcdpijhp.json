{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VirusTotal Search",
  "description": "Search for selection on VirusTotal",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "clipboardRead",
    "<all_urls>"
  ]
}
