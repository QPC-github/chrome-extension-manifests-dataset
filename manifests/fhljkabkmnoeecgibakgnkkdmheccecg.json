{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DeviantArt easy download",
  "description": "Simplify image download on Deviant Art",
  "version": "0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.deviantart.com/*",
        "*://*.deviantart.net/*"
      ],
      "js": [
        "setjsgallery.js"
      ],
      "run_at": "document_end"
    }
  ]
}
