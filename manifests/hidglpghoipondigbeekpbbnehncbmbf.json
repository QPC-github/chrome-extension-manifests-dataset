{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scylla",
  "version": "1.1",
  "description": "Doneert geld aan volleybal vereniging Scylla bij online aankopen",
  "background": {
    "page": "bg.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "scylla19.png",
    "48": "scylla48.png",
    "128": "scylla128.png"
  },
  "permissions": [
    "tabs"
  ]
}
