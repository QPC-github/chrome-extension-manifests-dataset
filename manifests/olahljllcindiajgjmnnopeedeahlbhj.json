{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TekPassword (App) a password generator",
  "default_locale": "en",
  "version": "1.0.13",
  "description": "Simple random password generator.  The  way to generate passwords. ",
  "app": {
    "launch": {
      "local_path": "index.html",
      "container": "panel",
      "height": 430,
      "width": 440
    }
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png"
  },
  "offline_enabled": true,
  "permissions": []
}
