{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FTSE-MIB variation",
  "short_name": "FTSE-MIB",
  "description": "Displays the current FTSE-MIB value.",
  "version": "1.3.4",
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "default_title": "Click to show today's chart and additional data",
    "default_popup": "popup.htm",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "permissions": [
    "http://www.borsaitaliana.it/",
    "https://www.borsaitaliana.it/"
  ]
}
