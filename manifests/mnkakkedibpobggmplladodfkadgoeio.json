{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tinify URL Shortner",
  "description": "Tinify All Types of URLs into Tinify.iR Tiny URL",
  "version": "1.0",
  "icons": {
    "128": "logo_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://tinify.ir/",
    "activeTab"
  ]
}
