{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "411Screen",
  "version": "1.0.1",
  "description": "Take screenshot and edit it before printing, Saving and Sharing.",
  "background": {
    "persistent": false,
    "scripts": [
      "event.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png"
    },
    "default_title": "Take Screenshot"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png",
    "512": "img/icon512.png"
  },
  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "downloads",
    "http://411screen.com/*",
    "notifications",
    "pageCapture"
  ],
  "manifest_version": 2
}
