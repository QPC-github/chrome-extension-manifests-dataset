{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FastAccent",
  "description": "This extension allows the user to easily insert accented characters",
  "version": "0.2.0",
  "browser_action": {
    "default_icon": "FastAccentIcon.png"
  },
  "permissions": [
    "input"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "input2.js"
      ]
    }
  ]
}
