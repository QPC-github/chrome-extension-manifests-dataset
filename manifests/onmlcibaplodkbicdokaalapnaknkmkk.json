{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PHP Function Lookup",
  "version": "0.9.2",
  "description": "Serch PHP Manual on the go!",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "perssistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "notifications"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "options_page": "options.html"
}
