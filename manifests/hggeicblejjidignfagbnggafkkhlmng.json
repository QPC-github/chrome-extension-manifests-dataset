{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Next IL Shopping-bag item links",
  "version": "2.0.0",
  "description": "Adds links to Next IL Shopping-bag items",
  "manifest_version": 2,
  "permissions": [
    "http://*.nextdirect.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.nextdirect.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}
