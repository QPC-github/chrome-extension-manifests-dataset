{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tour de France 2016 videovæg",
  "description": "Chrome plugin til Tour de France videovæg. Spørgsmål og fejlmelding til said@tv2.dk",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "tv2.logo.png",
    "128": "tv2.logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://videodesk.opdatering.tv2.dk/*"
      ],
      "js": [
        "js/jquery.1.10.2.min.js",
        "js/app.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "template.html"
  ]
}
