{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "So Many Me - Demo",
  "description": "A puzzle platformer starring Filo and his band of misfit clones as they attempt to save a world threatened by an ancient evil.",
  "version": "2.6",
  "manifest_version": 2,
  "offline_enabled": true,
  "requirements": {
    "3D": {
      "features": [
        "css3d",
        "webgl"
      ]
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "persistent": false,
      "scripts": [
        "launch.js"
      ]
    }
  },
  "permissions": [
    "fullscreen",
    "webview",
    "storage",
    "unlimitedStorage"
  ]
}
