{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Make Trump Fuckface!",
  "version": "1.2",
  "description": "Add this extension for your Chrome browser to replace all instances of \"Trump\" with \"Fuckface.\"",
  "icons": {
    "16": "no-symbol_16.png",
    "48": "no-symbol_48.png",
    "128": "no-symbol_128.png",
    "256": "no-symbol_256.png",
    "512": "no-symbol_512.png",
    "640": "no-symbol_640.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "no-symbol_16.png",
      "48": "no-symbol_48.png",
      "128": "no-symbol_128.png",
      "256": "no-symbol_256.png",
      "512": "no-symbol_512.png",
      "640": "no-symbol_640.png"
    },
    "default_popup": "popup.html",
    "default_title": "Make Trump Fuckface!"
  },
  "author": "Doug Rindfleisch",
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "short_name": "Trump2FuckFace",
  "permissions": [
    "http://*.google.com/",
    "http://*.com/",
    "tabs"
  ]
}
