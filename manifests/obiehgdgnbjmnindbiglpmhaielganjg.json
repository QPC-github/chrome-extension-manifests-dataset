{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js",
      "jquery.min.js"
    ]
  },
  "name": "Latest Scientific Discoveries",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Latest Scientific Discoveries provides news from latest Science in Astronomy, Genetics, Evolution, Energy.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "*://*/*"
  ]
}
