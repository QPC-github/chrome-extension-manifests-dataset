{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "ColdSearch"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://www.coldsearch.com/favicon.png",
      "instant_url": "https://www.coldsearch.com/webs?&ext=qs&q={searchTerms}",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "ColdSearch",
      "name": "Cold",
      "search_url": "https://www.coldsearch.com/webs?&ext=qs&q={searchTerms}",
      "search_url_post_params": ""
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "ColdSearch setting your browser default search",
  "homepage_url": "https://www.coldsearch.com/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "ColdSearch",
  "permissions": [
    "storage",
    "http://www.code313pay.com/",
    "https://www.coldsearch.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.2",
  "web_accessible_resources": [
    "check.png"
  ]
}
