{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tamil New Tab",
  "author": "OLIVER JACOB G",
  "description": "Replace new tab page with a Tamil Quote in Tamil and English language",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "Tamil.html"
  },
  "icons": {
    "16": "images/icon/icon-16.png",
    "48": "images/icon/icon-48.png",
    "128": "images/icon/icon-128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
