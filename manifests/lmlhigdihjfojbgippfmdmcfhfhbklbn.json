{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dogecoin To The Moon",
  "description": "This extension gathers different informations and useful things.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "https://api.vircurex.com/api/*"
  ],
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png",
    "32": "icon_32.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "tpl/popup.html"
  }
}
