{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IST34 Token Wallet",
  "version": "2.0.0",
  "description": "IST34 Token Wallet V2 is the perfect way to store, send or receive your IST34 Tokens.",
  "icons": {
    "16": "assets/img/icons/icon_16.png",
    "48": "assets/img/icons/icon_48.png",
    "128": "assets/img/icons/icon_96.png"
  },
  "background": {
    "scripts": [
      "assets/js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "IST34 Token Wallet",
    "default_icon": "assets/img/icons/icon_128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' ; object-src 'self'",
  "permissions": [
    "storage",
    "https://hiperteknoloji.org/"
  ]
}
