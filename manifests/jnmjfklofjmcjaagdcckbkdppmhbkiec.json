{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WowDB to Wowhead",
  "version": "1.0",
  "description": "Redirects WowDB pages to Wowhead pages",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.wowdb.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ]
}
