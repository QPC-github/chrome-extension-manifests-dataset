{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Costello",
  "short_name": "Costello",
  "version": "2.19.5",
  "description": "Costello helps you answer sales objections in real-time, capture notes, and sync everything to your CRM after a sales call ends.",
  "browser_action": {
    "default_title": "Costello"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "frame.html",
    "Costello.svg",
    "SalesLoft-icon-blue.svg"
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "https://app.andcostello.com/*"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
