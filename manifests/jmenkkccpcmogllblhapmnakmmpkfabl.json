{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Levné letenky a zájezdy | DovolenaTunis.cz",
  "description": "Rozšiření pro monitorováni levných letenek a last minute dovolené do Tuniska",
  "background": {
    "page": "background.html"
  },
  "default_locale": "cs",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "*://*/*"
  ],
  "version": "1.1.2"
}
