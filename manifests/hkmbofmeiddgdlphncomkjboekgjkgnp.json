{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hubskip",
  "short_name": "Hubskip",
  "version": "1.0",
  "description": "Skipping forms on Hubspot is just a hop, skip and click away.",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Hubskip"
  },
  "author": "GrowthSmackers.com",
  "homepage_url": "http://www.growthsmackers.com?utm_source=chrome-ext&utm_medium=referral&utm_campaign=homepage",
  "permissions": [
    "tabs",
    "*://offers.hubspot.com/*"
  ],
  "background": {
    "scripts": [
      "lib/brain.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  }
}
