{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tota11y Plugin, from Khan Academy",
  "short_name": "tota11y",
  "description": "This extension will implement the tota11y JS file from Khan Academy",
  "version": "0.1.5",
  "icons": {
    "16": "off.png",
    "48": "tota11y-48.png",
    "128": "tota11y-128.png"
  },
  "browser_action": {
    "default_icon": "off.png",
    "name": "Click to get URL"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "tota11y.min.js",
    "dothatscriptthang.js"
  ],
  "permissions": [
    "activeTab"
  ]
}
