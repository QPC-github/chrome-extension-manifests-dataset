{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Shuffle SR",
  "description": "Lets you shuffle a lot of tracks at Sr",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://sverigesradio.se/*"
      ],
      "js": [
        "playRadio.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "code.js"
  ]
}
