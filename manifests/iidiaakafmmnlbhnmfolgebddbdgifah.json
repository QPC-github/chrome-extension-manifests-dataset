{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hotel Search",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Find the cheapest hotel using HotelsCombined",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self' https://secure.hotelscombined.com/"
}
