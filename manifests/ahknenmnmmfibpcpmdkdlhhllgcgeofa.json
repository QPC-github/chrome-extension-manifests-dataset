{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Web debug tool to override url",
  "version": "1.1.2",
  "name": "Redirectly",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "manifest_version": 2
}
