{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UnitedDialogue.",
  "short_name": "UD.",
  "description": "Manage all your social accounts in one place.",
  "version": "0.5.4",
  "manifest_version": 2,
  "author": "Voris",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "bg.js",
      "context.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "UnitedDialogue.",
    "default_popup": "main.html",
    "default_icon": "./static/ud_32.png"
  },
  "icons": {
    "16": "./static/ud_16.png",
    "32": "./static/ud_32.png",
    "48": "./static/ud_48.png",
    "128": "./static/ud_128.png"
  }
}
