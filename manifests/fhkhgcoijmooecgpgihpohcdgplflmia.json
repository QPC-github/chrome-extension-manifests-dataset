{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Code Hero Academy",
  "description": "Easy and fast real time access of Code Hero Challenges and Tasks for the Students",
  "version": "2.0.2",
  "icons": {
    "128": "CodeHeroLogo.png",
    "48": "CodeHeroLogo.png",
    "16": "CodeHeroLogo.png"
  },
  "browser_action": {
    "default_icon": "CodeHeroLogo.png",
    "default_popup": "Calimatic.html"
  },
  "content_security_policy": "script-src 'self' http://localhost:53225/; object-src 'self'",
  "permissions": [
    "https://*/",
    "http://*/",
    "storage"
  ]
}
