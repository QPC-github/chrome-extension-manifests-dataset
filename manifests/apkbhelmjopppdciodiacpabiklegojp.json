{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Botify Extension",
  "description": "Botify Extension",
  "version": "1.10.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-1mKXqJ+bYCOlPpZpE2tT1sFUKEEVb5Ps+/5oU/N+9RA=' https://*.woopra.com/ https://*.gstatic.com/ https://*.segment.com/ https://*.customer.io/ https://*.google-analytics.com/ https://*.facebook.net/ https://*.mxpnl.com/ https://*.satismeter.com/ https://*.licdn.com/ https://*.pendo.io/ https://*.fullstory.com/; object-src 'self'",
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "96": "images/favicon-96x96.png",
    "128": "images/favicon-128x128.png",
    "160": "images/favicon-160x160.png",
    "192": "images/favicon-192x192.png"
  },
  "browser_action": {
    "default_icon": "images/favicon-192x192.png",
    "default_popup": "public/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "injecter.bundle.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "webRequest",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "fonts/*",
    "css/*",
    "public/*"
  ]
}
