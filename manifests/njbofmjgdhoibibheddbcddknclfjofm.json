{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My Passwords",
  "description": "Generate passwords while saving them in your Google sheets.",
  "version": "0.8",
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "16": "images/myPassword-logo-16.png",
    "48": "images/myPassword-logo-48.png",
    "128": "images/myPassword-logo-128.png"
  },
  "browser_action": {
    "default_icon": "images/myPassword-logo-64.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "MacCtrl+Shift+U"
      }
    }
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js; object-src 'self'"
}
