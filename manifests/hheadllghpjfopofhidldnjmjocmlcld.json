{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LeftLane Sports",
  "description": "LeftLane Sports + Chrome is the fastest, easiest way to shop your favorite Outdoor and Fitness brands at up to 70% off retail.",
  "version": "1.3",
  "permissions": [
    "tabs",
    "http://www.leftlanesports.com/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup.html"
  }
}
