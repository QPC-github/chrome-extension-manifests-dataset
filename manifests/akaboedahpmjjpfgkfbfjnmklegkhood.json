{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Recurpost",
  "version": "1.3",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "identity",
    "cookies",
    "<all_urls>"
  ]
}
