{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Q Posts Search",
  "short_name": "QPostsSearch",
  "description": "Search Q Posts for any highlighted text in a browser.",
  "version": "1",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "searchq.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "16x16singleq.png",
    "48": "48x48singleq.png",
    "128": "128x128singleq.png"
  }
}
