{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PySearch",
  "version": "0.2.2",
  "manifest_version": 2,
  "description": "Search Python documentation in the Omnibox.",
  "icons": {
    "16": "python-logo-16.png",
    "48": "python-logo-48.png",
    "128": "python-logo-128.png"
  },
  "background": {
    "scripts": [
      "config.js",
      "indexer.js",
      "renderer.js",
      "index-data.js",
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "py"
  },
  "permissions": [
    "tabs",
    "http://docs.python.org/"
  ],
  "minimum_chrome_version": "18"
}
