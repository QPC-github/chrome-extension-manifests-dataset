{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Denham Preen",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://* https://* https://*.google.com https://*.google-analytics.com; object-src 'self'",
  "description": "World Scratch Map New Tab Extension",
  "icons": {
    "16": "img/icon16.png"
  },
  "manifest_version": 2,
  "name": "World Scratch Map",
  "offline_enabled": true,
  "optional_permissions": [
    "system.storage"
  ],
  "options_page": "options.html",
  "permissions": [
    "storage",
    "geolocation",
    "http://suggestqueries.google.com/",
    "http://suggestqueries.google.com/*",
    "https://suggestqueries.google.com/*"
  ],
  "short_name": "World Map",
  "version": "0.0.2"
}
