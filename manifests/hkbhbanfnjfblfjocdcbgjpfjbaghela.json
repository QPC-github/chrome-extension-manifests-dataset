{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Move tab to new window",
  "description": "Moves the current tab to a new window",
  "version": "1.0",
  "icons": {
    "128": "tab_mover_icon.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "short_name": "Tab Mover",
  "manifest_version": 2
}
