{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Imgflip Quick Submission",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "Simply right click on any image while browsing, then click Submit Image.",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "favicon02.png",
    "64": "icon64.png"
  },
  "background": {
    "scripts": [
      "context.js"
    ]
  }
}
