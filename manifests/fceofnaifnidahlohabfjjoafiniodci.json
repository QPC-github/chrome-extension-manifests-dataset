{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Photos Triage",
  "description": "Make G+ Photos ever so slightly better",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "https://plus.google.com/photos*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://plus.google.com/photos?pid=*",
        "https://plus.google.com/photos/*"
      ],
      "js": [
        "jquery-1.9.1.min.js",
        "content.js"
      ]
    }
  ]
}
