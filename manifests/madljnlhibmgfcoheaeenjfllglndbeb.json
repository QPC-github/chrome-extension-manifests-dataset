{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zazzle Quick Search",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Quickly search and browse for items on Zazzle.com and keep up with the latest Zazzle coupon codes",
  "manifest_version": 2,
  "permissions": [
    "http://*.zazzle.com/*",
    "http://api.zunix.co/*",
    "webNavigation"
  ],
  "version": "1.5",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
