{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AspTeleConnect ScreenSharing",
  "author": "Hitforward pvt ltd.",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "AspTelecares Screenshare extesnion",
  "homepage_url": "http://tokbox.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://aspteleconnect.azurewebsites.net/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
