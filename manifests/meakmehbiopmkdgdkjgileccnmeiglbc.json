{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "D - Day",
  "description": "D-day chrome extension from (https: //github.com/underdogdude/d-day)",
  "version": "1",
  "author": "Auttawut Wiriyakreng",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },
  "chrome_url_overrides": {
    "newtab": "d-day/build/index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
