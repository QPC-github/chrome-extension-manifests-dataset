{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Enter on Twitter",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Use your enter key to tweet.",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "96": "logo.png"
  },
  "web_accessible_resources": [
    "logo48.png",
    "logo48nb.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "eot.js"
      ]
    }
  ]
}
