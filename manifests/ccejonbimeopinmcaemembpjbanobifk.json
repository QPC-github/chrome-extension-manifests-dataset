{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "request.js"
    ]
  },
  "description": "Support The Walsh Garage automatically every time you shop at Amazon. Automatically redirects amazon.com to smile.amazon.com.",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "The Garage - Shop to Support",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "http://www.amazon.com/*",
    "https://www.amazon.com/*",
    "http://smile.amazon.com/*",
    "https://smile.amazon.com/*"
  ],
  "version": "1.4",
  "homepage_url": "http://www.walshgarage.com/"
}
