{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Checkerboard Background 4 Transparent Images",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Checkerboard background for transparent images",
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "64": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "*://*/*.GIF",
        "*://*/*.gif",
        "*://*/*.JPG",
        "*://*/*.jpg",
        "*://*/*.PNG",
        "*://*/*.png",
        "file:///*.GIF",
        "file:///*.gif",
        "file:///*.JPG",
        "file:///*.jpg",
        "file:///*.png",
        "file:///*.PNG"
      ]
    }
  ]
}
