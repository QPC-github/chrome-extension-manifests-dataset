{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Form Guard",
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "tabs",
    "webNavigation"
  ],
  "version": "3.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
