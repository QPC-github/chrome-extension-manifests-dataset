{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Event Store Stream Viewer",
  "version": "0.1",
  "description": "Makes json easier to read in EventStore stream browser",
  "author": "Richard Dutton",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "jquery.json-viewer.css"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "jquery.json-viewer.js",
        "api.js"
      ],
      "run_at": "document_end"
    }
  ]
}
