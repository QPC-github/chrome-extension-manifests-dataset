{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "タグインスペクター",
  "version": "1.9.25",
  "manifest_version": 2,
  "description": "Yahoo!タグマネージャーに設定されているサービスタグの情報が確認できます",
  "icons": {
    "48": "assets-images/ja-JP/YahooJapanTagInspector48.png",
    "128": "assets-images/ja-JP/YahooJapanTagInspector128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets-images/ja-JP/icon19-off.png",
      "38": "assets-images/ja-JP/icon38-off.png"
    },
    "default_title": "Yahoo Japan Tag Inspector",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "assets-js/diagnostic.js"
    ]
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>"
  ]
}
