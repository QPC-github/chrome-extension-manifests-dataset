{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "windows-1251",
      "favicon_url": "http://go.mail.ru/favicon.ico",
      "is_default": true,
      "keyword": "mail.ru",
