{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Auto Scroll Chrome",
  "version": "1.6",
  "description": "Auto Scroll Chrome Extension",
  "permissions": [
    "tabs",
    "<all_urls>",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ],
      "run_at": "document_end",
      "All_frames": false
    }
  ],
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "48": "50.png",
    "128": "128.png"
  }
}
