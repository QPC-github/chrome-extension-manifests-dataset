{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Balloon",
  "description": "Counts up your age",
  "version": "1.0",
  "browser_action": {
    "default_icon": "Balloon.png",
    "default_popup": "newtab.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "js": [
        "birthday.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ]
}
