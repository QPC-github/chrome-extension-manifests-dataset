{
  "background": {
    "persistence": false,
    "scripts": [
      "/scripts/background/main.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "/scripts/content/jquery-1.12.0.js",
        "/scripts/content/jquery-ui.js",
        "/scripts/content/ace.js",
        "/scripts/content/jquery.validate.min.js",
        "/scripts/content/detectUI5.js",
        "/scripts/content/Github.js"
      ],
      "css": [
        "/scripts/content/jquery-ui.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "default-src 'self'; style-src 'unsafe-inline';",
  "description": "With UI5 Automation Designer, you can easily automate and debug your OpenUI5 or SAPUI5-based apps.[UI5 Automation Designer: 0.4]",
  "devtools_page": "/html/devtools/index.html",
  "icons": {
    "128": "/images/vyper1.gif",
    "16": "/images/vyper1.gif"
  },
  "manifest_version": 2,
  "name": "UI5 Automation Designer",
  "page_action": {
    "default_icon": {
      "19": "/images/vyper1.gif",
      "38": "/images/vyper1.gif"
    },
    "default_popup": "/html/popup/index.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "debugger",
    "history",
    "management",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.4",
  "web_accessible_resources": [
    "/scripts/injected/*.js",
    "/html/popup/previewWindow.html",
    "/scripts/content/*.js",
    "/scripts/content/*.css",
    "/scripts/content/images/*.png",
    "/images/*",
    "/scripts/content/jquery-1.12.0.js",
    "/scripts/content/jquery-ui.js",
    "/scripts/content/jquery-ui.css",
    "/vendor/ToolsAPI.js"
  ]
}
