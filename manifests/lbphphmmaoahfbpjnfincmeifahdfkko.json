{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "k3ats",
  "version": "0.6",
  "description": "Bulk downloads files from our beloved keats",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_popup": "ui/popup.html"
  },
  "permissions": [
    "tabs",
    "https://keats.kcl.ac.uk/*",
    "downloads"
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://keats.kcl.ac.uk/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ]
    }
  ]
}
