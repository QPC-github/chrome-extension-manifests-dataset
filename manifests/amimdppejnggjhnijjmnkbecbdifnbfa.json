{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CxLogon",
  "version": "1.0.0",
  "short_name": "CxLogon",
  "author": "Jahastech",
  "description": "NxFilter's single sign-on agent for Chrome.",
  "icons": {
    "16": "16_nxrb2.png",
    "48": "48_nxrb2.png",
    "128": "128_nxrb2.png"
  },
  "browser_action": {
    "default_icon": "16_nxrb2.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*/*",
    "identity",
    "identity.email"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
