{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "canvas2png",
  "version": "0.1",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "64": "icon/64.png",
    "128": "icon/128.png",
    "256": "icon/256.png"
  },
  "browser_action": {
    "default_title": "canvas2png",
    "default_icon": {
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "64": "icon/64.png",
      "128": "icon/128.png",
      "256": "icon/256.png"
    }
  },
  "web_accessible_resources": [
    "img/png.svg"
  ],
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  }
}
