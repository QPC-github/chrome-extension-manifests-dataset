{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Abilita Screen Share su Docetys",
  "short_name": "Docetys ScreenShare",
  "description": "Estensione necessaria ad abilitare la condivisione dello schermo per Chrome Browser su www.docetys.eu",
  "version": "1.0.4",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "icons": {
    "36": "icon-0-75x.png",
    "48": "icon-1x.png",
    "72": "icon-1-5x.png",
    "96": "icon-2x.png",
    "144": "icon-3x.png",
    "192": "icon-4x.png",
    "512": "icon-512.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.docetys.eu/*"
    ]
  },
  "homepage_url": "https://www.docetys.eu",
  "permissions": [
    "desktopCapture",
    "displaySource",
    "tabCapture",
    "pageCapture",
    "tabs"
  ]
}
