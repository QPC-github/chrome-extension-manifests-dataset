{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yugipedia Redirect",
  "description": "Redirect from Yu-Gi-OH Wikia to Yugipedia.",
  "version": "1.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "./assets/16x16.png",
    "48": "./assets/48x48.png",
    "128": "./assets/128x128.png"
  },
  "permissions": [
    "webRequest",
    "*://yugioh.wikia.com/*",
    "*://www.yugioh.wikia.com/*",
    "webRequestBlocking"
  ]
}
