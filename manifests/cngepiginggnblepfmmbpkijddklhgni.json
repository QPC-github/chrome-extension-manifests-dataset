{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Skeleton J. Warrior",
  "version": "1.3",
  "description": "Replaces the text 'SJW' with 'skeleton' because that's hilarious.",
  "icons": {
    "16": "swicon16.png",
    "48": "swicon48.png",
    "128": "swicon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
