{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump your friend",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*.gif"
  ],
  "browser_action": {
    "default_icon": "trumpicon.png"
  }
}
