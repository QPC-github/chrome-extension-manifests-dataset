{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GoSession Desktop Sharing",
  "short_name": "GoSession",
  "description": "Allows you to stream your desktop into meetings GoSession Audio, Video Conference Calls for E-Learning purpose.",
  "version": "1.0.2",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "GoSession-logo-16x16.png",
    "48": "GoSession-logo-48x48.png",
    "128": "GoSession-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://video.gosession.com/*"
    ]
  }
}
