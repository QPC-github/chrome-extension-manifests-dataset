{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "discoverit-favorites",
  "version": "0.1.4",
  "short_name": "Discoverit Favorites",
  "description": "Add every url to your discover-it favorites",
  "author": "Innovation Engineering",
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "https://www.discover-it.eu/discover-it-web/",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/discoverit-48.png"
  },
  "background": {
    "scripts": [
      "js/jquery-1.10.2.min.js",
      "js/discoverit.js"
    ]
  },
  "icons": {
    "16": "images/discoverit-16.png",
    "48": "images/discoverit-48.png",
    "128": "images/discoverit-128.png"
  },
  "web_accessible_resources": [
    "images/discoverit-48.png"
  ]
}
