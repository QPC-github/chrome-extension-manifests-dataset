{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Developer (developer@intalk.io)",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost;  object-src 'self' ",
  "browser_action": {
    "default_icon": {
      "19": "ext_icons/icon_19.png",
      "38": "ext_icons/icon_38.png"
    },
    "default_popup": "index.html",
    "default_title": "InTalk.io WebRTC Phone"
  },
  "description": "InTalk.io WebRTC Phone.",
  "icons": {
    "128": "ext_icons/icon_128.png",
    "16": "ext_icons/icon_16.png",
    "48": "ext_icons/icon_48.png"
  },
  "manifest_version": 2,
  "name": "InTalk.io WebRTC Agent Phone",
  "options_page": "option.html",
  "permissions": [
    "activeTab",
    "background",
    "tabs",
    "http://*/",
    "contextMenus",
    "unlimitedStorage",
    "storage",
    "notifications",
    "cookies"
  ],
  "version": "1.0.4",
  "web_accessible_resources": [
    "*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://crm.zoho.com/*",
        "https://crm.zoho.eu/*",
        "https://crm.zoho.us/*",
        "https://crm.zoho.in/*",
        "https://recruit.zoho.com/*",
        "https://recruit.zoho.in/*",
        "https://recruit.zoho.us/*",
        "https://recruit.zoho.eu/*"
      ],
      "js": [
        "theme/chrome/js/jquery.js",
        "theme/chrome/js/content-script.js"
      ]
    }
  ],
  "externally_connectable": {
    "ids": [
      "ignaahgcfogndlphmhcmmnojeefgccpl"
    ],
    "matches": [
      "https://crm.zoho.com/*",
      "https://crm.zoho.in/*",
      "https://crm.zoho.eu/*",
      "https://crm.zoho.us/*",
      "https://recruit.zoho.com/*",
      "https://recruit.zoho.in/*",
      "https://recruit.zoho.us/*",
      "https://recruit.zoho.eu/*"
    ],
    "accepts_tls_channel_id": false
  }
}
