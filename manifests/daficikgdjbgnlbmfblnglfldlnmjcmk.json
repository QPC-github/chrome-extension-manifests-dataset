{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rádio Gazin",
  "version": "1.1",
  "description": "Rádio Gazin - Chrome",
  "content_scripts": [
    {
      "matches": [
        "http://facebook.com/*",
        "https://facebook.com/*",
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "js": [
        "magic.js"
      ],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_icon": "icone.png"
  },
  "icons": {
    "16": "icone16.png",
    "48": "icone48.png",
    "128": "icone128.png"
  },
  "manifest_version": 2
}
