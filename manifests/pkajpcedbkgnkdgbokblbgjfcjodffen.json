{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "拡張を切り替えたがっているんだ",
  "version": "0.0.3",
  "description": "拡張機能を分類し、すばやく切り替えます",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "management"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com/; object-src 'self'",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/16.png",
    "default_title": "拡張を切り替えたがっているんだ",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
