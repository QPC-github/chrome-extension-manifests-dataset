{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TubingKeys",
  "description": "This extension extends the basic Youtube Hotkeys to give the user more functionality.",
  "version": "2.1",
  "browser_action": {
    "default_icon": "icon_16.png",
    "default_title": "TubingKeys by FlexXx"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
