{
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://www.nicovideo.jp/watch/*"
      ]
    }
  ],
  "description": "コミュニティ再生される動画で、一般視聴へのリンクボタンを追加します。",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png"
  },
  "name": "ニコニコ動画のコミュニティ動画を一般視聴する",
  "permissions": [
    "http://www.nicovideo.jp/watch/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.3"
}
