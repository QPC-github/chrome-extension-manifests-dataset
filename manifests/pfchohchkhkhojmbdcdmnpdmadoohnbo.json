{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Linux Command Line",
  "short_name": "Chrome shell",
  "version": "1.0.2",
  "description": "Replace new tab page with a linux style command line featuring bookmarks, tab management, custom actions, and easter eggs!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "topSites",
    "history",
    "bookmarks"
  ],
  "icons": {
    "16": "images/linchro16.png",
    "32": "images/linchro32.png",
    "48": "images/linchro48.png",
    "128": "images/linchro128.png"
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2
}
