{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "http://classic.magictraders.com/*",
    "http://forums.magictraders.com/*",
    "http://refs.magictraders.com/*",
    "background",
    "contextMenus",
    "http://*.tcgplayer.com/*",
    "https://*.tcgplayer.com/*",
    "http://magictcgprices.appspot.com/*",
    "http://forums.magictraders.com/ubbmisc.cgi",
    "http://classic.magictraders.com/cgi-bin/ubb/ubbmisc.cgi",
    "http://classic.magictraders.com/ubb/Forum2/HTML/*"
  ],
  "name": "MOTL Tools",
  "version": "2.5.1",
  "description": "Tools for MOTL by harbingerofthevoid",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "event.js",
      "options.js",
      "hwResults.js",
      "hwSearch.js",
      "auctionSearch.js",
      "highlight.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "hwResults.html",
    "whiteFade.png"
  ]
}
