{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wordpress Web Design Brighton",
  "manifest_version": 2,
  "short_name": "ajmwebservices",
  "description": "For faster and easier access to Wordpress Services for web design or website issues, please  install this simple Chrome Extension.",
  "version": "1.0",
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal2.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "16px_433-1503121398.png"
  },
  "icons": {
    "128": "128px_433-1503121316.png"
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity"
  ]
}
