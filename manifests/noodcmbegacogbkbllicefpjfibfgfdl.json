{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TracError Screen Recorder",
  "short_name": "TracError",
  "author": "Shubham Jaiswal",
  "description": "Record your Screen while Classification and Pause when you are not working and stop when done ",
  "version": "0.55",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js",
      "back.js",
      "ourrecorder.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "receiver.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "desktopCapture",
    "notifications",
    "tabs",
    "tabCapture",
    "activeTab",
    "storage",
    "identity",
    "unlimitedStorage",
    "downloads"
  ]
}
