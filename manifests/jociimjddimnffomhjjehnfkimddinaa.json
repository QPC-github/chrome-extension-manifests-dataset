{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vimbox Субтитры от Skyeng",
  "description": "Смотрите любимые фильмы и сериалы на английском языке и узнавайте значения слов прямо во время просмотра",
  "version": "1.2.17",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "amediateka.js",
    "netflix.js",
    "primevideo.js",
    "dictionary.js",
    "tutorial.js",
    "auth.js"
  ],
  "permissions": [
    "tabs",
    "https://www.amediateka.ru/",
    "https://www.netflix.com/",
    "https://www.primevideo.com/",
    "https://ext.skyeng.tv/",
    "https://oauth.skyeng.ru/",
    "https://dictionary.skyeng.ru/",
    "https://api.words.skyeng.ru/",
    "https://www.google-analytics.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.amediateka.ru/*",
        "https://www.netflix.com/*",
        "https://www.primevideo.com/*",
        "https://ext.skyeng.tv/auth-tv/*",
        "*://*.skyeng.ru/*"
      ],
      "js": [
        "loader.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://ext.skyeng.tv/dictionary.html",
        "https://ext.skyeng.tv/tutorial.html",
        "https://ext.skyeng.tv/howto.html",
        "http://localhost:3033/*.html"
      ],
      "js": [
        "loader.js"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16x16.png",
      "19": "icon-19x19.png",
      "32": "icon-32x32.png",
      "38": "icon-38x38.png"
    },
    "default_title": "Vimbox Субтитры от Skyeng",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "manifest_version": 2
}
