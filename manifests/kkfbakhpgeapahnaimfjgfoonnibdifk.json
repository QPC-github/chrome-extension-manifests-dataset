{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Blablaland Style",
  "short_name": "BBLstyle",
  "version": "1.0.5",
  "manifest_version": 2,
  "author": "Cocoduf",
  "description": "Cette extension simplifiera votre vie Blablalandienne.",
  "content_scripts": [
    {
      "js": [
        "mon_compte/modif_infos.js"
      ],
      "matches": [
        "http://*.blablaland.com/site/mon_compte.php?modif=infos*"
      ]
    },
    {
      "js": [
        "forum/reponse_eq99.js",
        "forum/format_message.js",
        "forum/quote_snip.js"
      ],
      "matches": [
        "http://*.blablaland.com/forum/viewtopic.php*"
      ]
    },
    {
      "js": [
        "forum/bbcode_buttons.js"
      ],
      "matches": [
        "http://*.blablaland.com/forum/*"
      ]
    },
    {
      "js": [
        "forum/post_snip.js"
      ],
      "matches": [
        "http://*.blablaland.com/forum/post.php*"
      ]
    },
    {
      "js": [
        "tchat/tchat.js"
      ],
      "matches": [
        "http://www.blablaland.com/chat/bbl_chat.php*"
      ]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "128_icon.png"
  }
}
