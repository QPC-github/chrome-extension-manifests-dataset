{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Myspace-ify",
  "description": "This extension turns Facebook into an old school Myspace page",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Turn Facebook into Myspace!"
  },
  "manifest_version": 2
}
