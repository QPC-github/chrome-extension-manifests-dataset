{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Desaturate",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Remove all color from the active page.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
