{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Subreddit",
  "description": "Quickly navigate to subreddits through the URL bar.",
  "version": "0.0.1",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "incognito": "spanning",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "r"
  }
}
