{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Meme Maker",
  "version": "1.850",
  "short_name": "Meme maker",
  "manifest_version": 2,
  "minimum_chrome_version": "32",
  "description": "A tool to make Meme images.",
  "offline_enabled": true,
  "icons": {
    "16": "img/icon.png",
    "64": "img/icon.png",
    "128": "img/icon.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    },
    "storage",
    "<all_urls>",
    "notifications",
    "alwaysOnTopWindows",
    "identity",
    "webview",
    "https://www.google-analytics.com/"
  ],
  "file_handlers": {
    "image": {
      "types": [
        "image/png",
        "image/jpeg"
      ]
    }
  }
}
