{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tower",
  "version": "0.1",
  "author": "Chen He",
  "homepage_url": "https://github.com/hechen/Tower",
  "description": "Open your git url from Git Tower Client.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "images/tower_16.png",
    "32": "images/tower_32.png",
    "48": "images/tower_48.png",
    "128": "images/tower_128.png"
  }
}
