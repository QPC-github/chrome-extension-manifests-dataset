{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Toonbook+",
  "description": "A collection of add-ons and features that improve the Toonbook experience.",
  "version": "0.2.1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.toonbook.me/*"
      ],
      "js": [
        "jquery.js",
        "cookies.js",
        "api.js",
        "color.js",
        "tags.js",
        "chat.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "api_script.js"
  ]
}
