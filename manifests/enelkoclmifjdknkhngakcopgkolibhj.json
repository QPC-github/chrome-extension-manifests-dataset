{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "webNavigation",
    "management",
    "webRequest",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "nativeMessaging",
    "background"
  ],
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "webDialogs.js",
    "select2Hooking.js",
    "select2Playback.js"
  ],
  "name": "Qualibrate V2",
  "description": "Qualibrate Recording & Playback Chrome Extension",
  "version": "2.1.8"
}
