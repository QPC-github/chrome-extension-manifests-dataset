{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TT QueueLights",
  "description": "Highlights played songs and artists in your queue on turntable.fm",
  "version": "1.1.1.6",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "css": [
        "QueueLights.css"
      ],
      "matches": [
        "http://turntable.fm/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "QueueLights.css",
    "jquery-1.8.3.min.js",
    "QueueLights.js"
  ]
}
