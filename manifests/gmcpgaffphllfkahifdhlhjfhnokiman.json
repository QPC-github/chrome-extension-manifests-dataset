{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Erase block user message in Discord.",
  "manifest_version": 2,
  "name": "erase-blocked-user-in-discord",
  "version": "1.0.3",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "64": "64.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://discordapp.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "*://discordapp.com/*"
  ]
}
