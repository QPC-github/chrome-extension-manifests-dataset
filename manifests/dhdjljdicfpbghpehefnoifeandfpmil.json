{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoodDrinker",
  "short_name": "GDrinker",
  "description": "GoodDrinker",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "2.0.0",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "icons": {
    "16": "img/icon-16.png",
    "128": "img/icon-128.png"
  },
  "permissions": [
    "storage",
    "https://www.google-analytics.com/",
    "http://www.gdrinker.com/"
  ]
}
