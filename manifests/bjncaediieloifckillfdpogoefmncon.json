{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Smart Logout",
  "description": "Extension that enables you to login or logout all sites at once with a Single key. No need to login each sites.",
  "version": "1.1.0",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
