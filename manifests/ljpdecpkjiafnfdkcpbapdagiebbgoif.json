{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Jingle.ro Căutare în Magazinele Online",
  "version": "1.12",
  "manifest_version": 2,
  "description": "Cauta mai usor in magazinele online romanesti direct din Google Chrome™ cu Jingle.ro",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "http://www.jingle.ro/*",
    "tabs"
  ]
}
