{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Website Opener",
  "version": "0.0.0.1",
  "description": "Open all the websites you need with 1 click.",
  "icons": {
    "128": "icons/WebsiteOpenerEdits128px.png",
    "48": "icons/WebsiteOpenerEdits48px.png",
    "32": "icons/WebsiteOpenerEdits32px.png",
    "16": "icons/WebsiteOpenerEdits16px.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/WebsiteOpenerEdits32px.png",
    "default_title": "Website Opener",
    "default_popup": "popup/open-options.html"
  }
}
