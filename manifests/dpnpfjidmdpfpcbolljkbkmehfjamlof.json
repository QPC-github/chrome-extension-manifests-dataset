{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Scratch Multi Sign-on",
  "version": "1.0.0.3",
  "manifest_version": 2,
  "description": "Seamlessly switch between your Scratch accounts",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    },
    "default_title": "Scratch Account Switcher",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "http://*.scratch.mit.edu/"
  ]
}
