{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Futebol - placar ao vivo",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Results of many soccer tournaments in Brazil and worldwide with live score! Check now all standings in one click",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": [
      "js/angular.min.js",
      "js/underscore-min.js"
    ]
  },
  "short_name": "Placar ao vivo",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
