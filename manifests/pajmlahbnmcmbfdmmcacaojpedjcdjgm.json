{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Gitark",
  "version": "0.0.9",
  "short_name": "Gitark",
  "author": "imfunny and community",
  "description": "The most popular dark theme extension for github",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon3.png",
    "48": "assets/icon2.png",
    "128": "assets/icon1.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ]
}
