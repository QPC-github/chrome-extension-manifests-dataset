{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Facebook Football Post Blocker",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Hides Facebook posts about football.",
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/*",
        "http://*.facebook.com/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
