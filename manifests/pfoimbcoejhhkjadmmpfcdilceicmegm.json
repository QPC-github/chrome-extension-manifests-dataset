{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Talentnow One Click Import Tool",
  "manifest_version": 2,
  "version": "21.27",
  "description": "Do easy, one-click candidate import from job boards into Talentnow Recruit applicant tracking system.",
  "browser_action": {
    "default_title": "Talentnow Recruit - One Click Import Tool",
    "default_popup": "view/popup.html"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "contextMenus",
    "activeTab",
    "https://*.talentnow.com/",
    "https://*.timesjobs.com/",
    "https://*.naukri.com",
    "https://*.shine.com/*"
  ],
  "background": {
    "page": "view/background.html"
  },
  "options_page": "view/settings.html",
  "icons": {
    "16": "images/ImCa16.png",
    "48": "images/ImCa16.png",
    "128": "images/ImCa128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' https://static.licdn.com/*",
  "web_accessible_resources": [
    "view/DupNotification.htm",
    "js/iframeInjector.js",
    "js/validation.js",
    "js/dupvalidation.js"
  ]
}
