{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Reasoned Discussions",
  "description": "Helps you find quality links, and debunk bad links",
  "version": "1.7",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://reasoneddiscussions.com/*"
  ]
}
