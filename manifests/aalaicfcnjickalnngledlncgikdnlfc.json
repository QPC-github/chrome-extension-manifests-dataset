{
  "name": "AirBnB cleaning & deposit in search results",
  "short_name": "NeatBnB",
  "version": "1.0.1",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "See AirBnB cleaning fee, security deposit without opening each listing",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.0.2.min.js",
        "content.js",
        "underscore-min.js"
      ],
      "css": [
        "customStyles.css"
      ],
      "matches": [
        "https://www.airbnb.com/s/*",
        "https://www.airbnb.co.uk/s/*",
        "https://www.airbnb.co.nz/s/*",
        "https://www.airbnb.ca/s/*"
      ]
    }
  ],
  "permissions": [
    "cookies",
    "background",
    "https://www.airbnb.com/s/*",
    "https://www.airbnb.co.uk/s/*",
    "https://www.airbnb.co.nz/s/*",
    "https://www.airbnb.fr/s/*",
    "https://www.airbnb.ca/s/*"
  ]
}
