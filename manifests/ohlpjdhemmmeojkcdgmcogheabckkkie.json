{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Muddleme Lite",
  "description": "A Muddleme Lite extension.",
  "version": "1.0.5",
  "manifest_version": 2,
  "minimum_chrome_version": "20.0",
  "page_action": {
    "default_title": "Muddleme Lite"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*",
        "https://www.google.com/*",
        "http://www.yahoo.com/*",
        "https://www.yahoo.com/*",
        "http://search.yahoo.com/*",
        "https://search.yahoo.com/*",
        "http://www.bing.com/*",
        "https://www.bing.com/*"
      ],
      "all_frames": false,
      "run_at": "document_end",
      "css": [
        "data/css/search.css"
      ],
      "js": [
        "data/js/lib/jquery-1.7.1.min.js",
        "data/js/lib/jquery.tipsy.custom.js",
        "data/js/lib/bootstrap-custom.js",
        "data/js/lib/jquery.trunk8.custom.js",
        "data/js/lib/jquery.bxslider.min.js",
        "data/js/global.js",
        "data/js/mmbox/page.js",
        "data/js/mmbox/utils.js",
        "data/js/mmbox/mm-engine.js",
        "data/js/mmbox/mm-box.js",
        "data/js/mmbox/mm-icons.js",
        "data/js/injected.js"
      ]
    }
  ],
  "icons": {
    "48": "data/icons/icon48.png",
    "128": "data/icons/icon128.png"
  }
}
