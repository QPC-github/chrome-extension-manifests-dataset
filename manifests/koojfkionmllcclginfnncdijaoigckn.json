{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "1.0.1",
  "name": "Расписание занятий СГОАН",
  "description": "Расширение позволяет просмотреть расписание занятий группы на определённую дату",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "nu16.png",
    "48": "nu48.png",
    "128": "nu128.png"
  },
  "browser_action": {
    "default_icon": "nu128.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "nu48.png"
  ]
}
