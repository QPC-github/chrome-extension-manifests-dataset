{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "name": "Fennekin Pokemon Wallpapers HD for NewTab",
  "description": "Get a lot of HD Wallpapers Fennekin Pokemon for chrome new tab background",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/start/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/start/libs/jquery-2.1.1.min.js",
      "/start/background/init/search-engines.js",
      "/start/background/init/settings.js",
      "/start/background/init/prefs-sys.js",
      "/start/background/methodsExtend/utils.js",
      "/start/background/handlers/common.js",
      "/start/background/handlers/setup.js",
      "/start/background/handlers/analytics.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/start/static/icons/defaultIcons/128.png"
    }
  },
  "icons": {
    "128": "/start/static/icons/defaultIcons/128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "chromenewtabbackground",
      "name": "chromenewtabbackground",
      "favicon_url": "http://chromenewtabbackground.com/favicon.ico",
      "search_url": "http://chromenewtabbackground.com/search?q={searchTerms}",
      "is_default": true,
      "encoding": "UTF-8"
    }
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "cookies",
    "management",
    "topSites"
  ],
  "web_accessible_resources": [
    "*"
  ],
  "manifest_version": 2
}
