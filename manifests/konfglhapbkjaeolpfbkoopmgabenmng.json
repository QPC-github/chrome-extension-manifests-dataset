{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Page Forwarder",
  "version": "1.0.5",
  "description": "Forwards you to pages you've been meaning to visit.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "apple.png",
    "48": "apple.png",
    "128": "apple.png"
  },
  "page_action": {
    "default_icon": {
      "19": "apple.png",
      "38": "apple.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage"
  ]
}
