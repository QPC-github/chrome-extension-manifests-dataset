{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "description": "Lucifers FB Message Expandit It will let you peek at facebook messages without them knowing you have seen or read them.",
  "name": "FB Message ExpandIt",
  "version": "1.14",
  "icons": {
    "128": "images/lucifer128.png",
    "64": "images/lucifer64.png",
    "32": "images/lucifer32.png",
    "16": "images/lucifer16.png",
    "48": "images/lucifer48.png"
  },
  "manifest_version": 2,
  "permissions": [
    "*://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/*",
        "http://*.facebook.com/*"
      ],
      "js": [
        "expandit_packed.user.js"
      ]
    }
  ]
}
