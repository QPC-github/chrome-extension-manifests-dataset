{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fap Fap Fap Hayhay",
  "description": "This extension clear ads in hayhay.",
  "version": "1.0",
  "icons": {
    "48": "icons/hayhay.ico"
  },
  "permissions": [
    "tabs",
    "*://www.hayhaytv.vn/xem-phim/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.hayhaytv.vn/xem-phim/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "hayhay.js"
  ]
}
