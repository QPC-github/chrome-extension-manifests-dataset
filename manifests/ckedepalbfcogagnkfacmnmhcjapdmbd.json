{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Amazin Book Finder",
  "description": "This extension matches Barnes & Noble College books with amazon, so that you can save money.",
  "version": "1.0",
  "icons": {
    "128": "amazinbooksicon.png"
  },
  "content_scripts": [
    {
      "css": [
        "mycss.css"
      ],
      "js": [
        "myscript.js"
      ],
      "matches": [
        "https://*.bncollege.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
