{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "P100 Quicktab",
  "description": "Marketing Team for Busy People",
  "version": "1.8",
  "icons": {
    "16": "p100logo.png",
    "48": "p100logo.png",
    "128": "p100logo.png"
  },
  "browser_action": {
    "default_title": "Open a new tab"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/aos/2.1.1/aos.js; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "p100v1/index.html"
  }
}
