{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Creator of Noise",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "changefont.css"
      ]
    }
  ]
}
