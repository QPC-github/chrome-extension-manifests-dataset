{
  "content_scripts": [
    {
      "js": [
        "content/gmailsiggittplugin.js",
        "content/yahoosiggittplugin.js",
        "content/hotmailsiggittplugin.js"
      ],
      "matches": [
        "*://mail.google.com/*",
        "*://gmail.google.com/*",
        "*://*.mail.yahoo.com/*",
        "*://*.live.com/*"
      ]
    }
  ],
  "converted_from_user_script": true,
  "description": "It is used to automate the feature rich signatures on your Gmail, Yahoomail and Hotmail accounts.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://mail.google.com/*",
    "https://mail.google.com/*",
    "http://gmail.google.com/*",
    "https://gmail.google.com/*",
    "http://siggitt.com/siggitt/*",
    "http://*mail.yahoo.com/*",
    "https://*mail.yahoo.com/*",
    "http://*.live.com/*",
    "https://*.live.com/*"
  ],
  "manifest_version": 2,
  "name": "Siggitt - Email Signatures",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.3.39"
}
