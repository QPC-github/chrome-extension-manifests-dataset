{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HarmfulAnnouncer",
  "short_name": "cyberNotif",
  "description": "Keeps you up to date with live streams by Harmful Opinions.",
  "author": "Karar Al-Remahy",
  "version": "1.5.2",
  "version_name": "Version 1.5",
  "background": {
    "page": "cyberNotif.html"
  },
  "browser_action": {
    "default_icon": "/icons/cyberIcon-2.png",
    "default_popup": "cyberMenu.html"
  },
  "web_accessible_resources": [
    "/icons/cyberIcon-1.png",
    "/icons/cyberIcon-2.png",
    "/icons/cyberIcon-3.png",
    "/icons/cyberIcon-4.png"
  ],
  "icons": {
    "128": "/icons/cyberIcon-4.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "https://*.twitch.tv/",
    "https://*.jtvnw.net/"
  ]
}
