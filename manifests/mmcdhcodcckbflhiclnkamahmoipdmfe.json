{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZPFont",
  "version": "3.0.0",
  "manifest_version": 2,
  "description": "Zawgyi-One and Pyidaungsu (c) MyanmarFontTagger",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "css": [
        "my.css"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "fonts/*.*"
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
