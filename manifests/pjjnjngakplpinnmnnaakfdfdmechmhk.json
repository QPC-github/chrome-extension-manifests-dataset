{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Silverback Softphone Connector",
  "short_name": "Silverback Softphone Connector",
  "description": "Silverback Softphone Connector by Contact Cubed",
  "version": "1.2.2.1",
  "author": "ContactCubed",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*.silverbacktele.com/*",
        "*://*.silverbackui.com/*"
      ],
      "js": [
        "Content/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "Resources/logo.png",
    "default_title": "Silverback Softphone Connector"
  },
  "icons": {
    "16": "Resources/Icons/logo16.png",
    "48": "Resources/Icons/logo48.png",
    "128": "Resources/Icons/logo128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.silverbacktele.com/*",
      "*://*.silverbackui.com/*"
    ]
  },
  "web_accessible_resources": [
    "*"
  ],
  "permissions": [
    "activeTab",
    "*://*.silverbacktele.com/*",
    "*://*.silverbackui.com/*"
  ],
  "background": {
    "scripts": [
      "Resources/jquery.min.js",
      "Background/background.js"
    ],
    "persistent": true,
    "matches": [
      "*://*.silverbacktele.com/*",
      "*://*.silverbackui.com/*"
    ]
  }
}
