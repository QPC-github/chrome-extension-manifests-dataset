{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Diary working on Google Calendar",
  "short_name": "Diary",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Write your diary daily and let it be stored in google calendar",
  "icons": {
    "128": "icons/128.png"
  },
  "options_page": "public/book.html",
  "permissions": [
    "storage",
    "cookies",
    "identity"
  ],
  "browser_action": {
    "default_title": "Diary",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/calendar.js"
    ],
    "persistent": true
  },
  "oauth2": {
    "client_id": "847999345912-f8bfj3h0ua8dh738j9jrp0hi7ubqqpat.apps.googleusercontent.com",
    "project_id": "diary-1533069840652",
    "auth_uri": "https://accounts.google.com/o/oauth2/auth",
    "token_uri": "https://oauth2.googleapis.com/token",
    "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}
