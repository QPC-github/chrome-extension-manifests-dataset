{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Uncognito",
  "version": "1.1",
  "description": "Switches incognito tabs and links into normal tabs",
  "icons": {
    "128": "icon.png"
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Uncognito"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
