{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "123Calendars",
  "version": "0.0.2",
  "manifest_version": 2,
  "decription": "Easily Print Monthly Calendars with just one click!.. Add your notes, official holidays before you print. Your printable calendar is ready. Just click the print button.",
  "homepage_url": "https://www.123calendars.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icon/icons128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
