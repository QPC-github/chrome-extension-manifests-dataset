{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddit redirect",
  "version": "1.0",
  "description": "Redirects from new reddit to old reddit whenever you click on a reddit link",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "worker.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://reddit.com/*",
    "*://www.reddit.com/*",
    "*://np.reddit.com/*",
    "*://new.reddit.com/*",
    "*://pay.reddit.com/*"
  ]
}
