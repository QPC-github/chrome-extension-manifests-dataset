{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Print this webpage",
  "description": "Adds a print button to the web browser.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Print this page",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
