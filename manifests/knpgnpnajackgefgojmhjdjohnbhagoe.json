{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Bleed",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Mathew Jibin",
  "short_name": "Bleed",
  "description": "Bleed content to chrome from your devices.",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "96": "images/icon_96.png",
    "192": "images/icon_192.png"
  },
  "homepage_url": "http://bleed.link/",
  "background": {
    "scripts": [
      "eventPage.js",
      "hub.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://bleed.link/*",
    "http://fonts.googleapis.com/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon_16.png",
    "default_title": "Bleed url, text or images from your devices to chrome",
    "default_popup": "popup.html"
  }
}
