{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Paul Lin",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "128": "icons/favicon.ico",
    "16": "icons/favicon.ico",
    "19": "icons/favicon.ico",
    "24": "icons/favicon.ico",
    "32": "icons/favicon.ico",
    "38": "icons/favicon.ico",
    "48": "icons/favicon.ico",
    "64": "icons/favicon.ico"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "NetGuard",
  "homepage_url": "https://keywordadmin.net/",
  "manifest_version": 2,
  "name": "NetGuard",
