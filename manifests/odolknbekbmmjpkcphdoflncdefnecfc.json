{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extentionTitle__",
  "description": "__MSG_extentionDescription__",
  "version": "2.0.2",
  "default_locale": "en",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "__MSG_default_title__"
  },
  "icons": {
    "16": "icons/app16.png",
    "32": "icons/app32.png",
    "64": "icons/app64.png",
    "128": "icons/app128.png"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "contextMenus"
  ],
  "content_security_policy": "default-src 'self'; img-src 'self' http://* https://* data:;script-src 'self'; style-src 'self' http://* https://* 'unsafe-inline'; connect-src 'self' http://* https://*;"
}
