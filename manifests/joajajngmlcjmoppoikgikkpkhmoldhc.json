{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ojooo by Dj Vismut",
  "description": "Check Ojooo balance, and notifies if he changed",
  "version": "1.1",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "*://*.ojooo.com/",
    "notifications"
  ],
  "browser_action": {
    "default_title": "Ojooo",
    "default_icon": "images/128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
