{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "To-do-list",
  "version": "1.0",
  "manifest_version": 2,
  "background": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "todos.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "ToDos"
  },
  "icons": {
    "48": "todo48by48.png",
    "128": "128x128.png"
  }
}
