{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "rgbcolor.js",
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "Adjust web page text for night time reading",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Night Reading Mode",
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "0.0.0.6"
}
