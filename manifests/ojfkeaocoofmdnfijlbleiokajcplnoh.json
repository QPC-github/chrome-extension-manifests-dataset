{
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "chrome_settings_overrides": {
    "default_locale": "de",
    "homepage": "http://www.yaeemo.com/?ref=__PARAM__",
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "http://www.yaeemo.com/favicon.ico",
      "is_default": true,
      "keyword": "keyword.__MSG_url_domain__",
      "name": "yaeemo.com",
      "search_url": "http://www.yaeemo.com/search?q={searchTerms}&ref=__PARAM__"
    },
    "startup_pages": [
      "http://www.yaeemo.com/?ref=__PARAM__"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "description": "Redirects the homepage",
  "icons": {
    "128": "yaeemo.jpg",
    "16": "yaeemo.jpg",
    "48": "yaeemo.jpg"
  },
  "manifest_version": 2,
  "name": "Yaeemo extension",
  "permissions": [
    "tabs",
    "webNavigation",
    "<all_urls>",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.9"
}
