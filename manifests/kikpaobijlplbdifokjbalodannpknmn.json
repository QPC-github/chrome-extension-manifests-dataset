{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Rose Garden Forever",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "description": "Corrects a common misspelling of Rose Garden Arena, home of the Portland Trail Blazers.",
  "version": "1.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
