{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "SiteSwitcher",
  "name": "SiteSwitcher",
  "description": "SiteSwitcher. Easily switch between sites. You can change URLs on the fly using regular expressions",
  "version": "1.0.1.5",
  "version_name": "1.0.1.5",
  "page_action": {
    "default_icon": "icon-32.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icon-32.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  }
}
