{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Docker Hub Search",
  "version": "1.0",
  "description": "Search Docker Hub repos",
  "homepage_url": "https://github.com/wbierbower/docker-search-chrome-ext",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "omnibox": {
    "keyword": "dh"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "https://hub.docker.com/"
  ],
  "background": {
    "scripts": [
      "o.js",
      "docker-search.js"
    ]
  }
}
