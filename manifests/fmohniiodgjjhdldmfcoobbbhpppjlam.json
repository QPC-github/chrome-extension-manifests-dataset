{
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "pro.js"
      ],
      "matches": [
        "*://www.tumblr.com/dashboard",
        "*://www.tumblr.com/tagged/*",
        "*://www.tumblr.com/explore",
        "*://www.tumblr.com/inbox",
        "*://www.tumblr.com/blog/*",
        "*://www.tumblr.com/likes",
        "*://www.tumblr.com/following",
        "*://www.tumblr.com/spotlight/*",
        "*://www.tumblr.com/lookup"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Tumblr Pro, get your hat, show your pride.",
  "homepage_url": "http://kiricon.net",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Tumblr Pro",
  "permissions": [
    "cookies",
    "http://www.tumblr.com/dashboard"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.7",
  "web_accessible_resources": [
    "*"
  ]
}
