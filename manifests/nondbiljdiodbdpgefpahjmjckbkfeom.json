{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Lead Distribution Notifications",
  "short_name": "ATD Notifications",
  "description": "This extension shows Lead Distribution Notifications",
  "version": "3.0.11",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Lead Distribution Notifications"
  },
  "permissions": [
    "notifications",
    "gcm",
    "identity",
    "storage",
    "https://app.autotextdriver.com/*",
    "https://testapp.autotextdriver.com/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery.min.js",
      "jquery.ajax-cross-origin.min.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "icons": {
    "128": "icon.png"
  }
}
