{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TwitchMoreButton",
  "description": "TwitchMoreButton",
  "version": "0.0.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://www.twitch.tv/*"
      ],
      "js": [
        "jquery-1.12.4.min.js",
        "more_button/document_end.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
