{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "prog Normalize",
  "version": "1.2.0",
  "description": "Dedicated Ad Block",
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.prog.co.il/*"
      ],
      "run_at": "document_end",
      "js": [
        "blocker.js"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.jdn.co.il/*",
    "https://info.jdn.co.il/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
