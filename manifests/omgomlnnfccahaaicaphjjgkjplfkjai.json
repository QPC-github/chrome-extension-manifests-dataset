{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TabNotes",
  "description": "Take notes in new tab",
  "version": "1.1.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "newtab.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "TabNotes",
    "default_icon": "icon48.png"
  }
}
