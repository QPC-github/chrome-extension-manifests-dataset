{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PoppySearch Extension",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "128": "poppysearchicon.png",
    "46": "poppysearchicon46.png"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.poppysearch.com/",
    "search_provider": {
      "name": "poppysearch",
      "keyword": "poppysearch",
      "search_url": "http://www.poppysearch.com/results.html?q={searchTerms}",
      "favicon_url": "http://www.poppysearch.com/favicon.ico",
      "suggest_url": "http://www.poppysearch.com/suggest?q={searchTerms}",
      "instant_url": "http://www.poppysearch.com/instant?q={searchTerms}",
      "image_url": "http://www.poppysearch.com/image?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    },
    "startup_pages": [
      "http://www.poppysearch.com/"
    ]
  }
}
