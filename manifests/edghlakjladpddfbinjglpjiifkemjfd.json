{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "background": {
    "page": "background/background.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "default_locale": "en",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "name": "__MSG_evil_ext_name__",
  "description": "__MSG_evil_ext_desc__",
  "offline_enabled": true,
  "icons": {
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/48.png"
    },
    "default_title": "__MSG_evil_ext_name__"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_address_bar_search_name__",
      "keyword": "search",
      "favicon_url": "https://search.extfans.com/query/favicon.png",
      "search_url": "https://search.extfans.com/query/__MSG_search_lang__/?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "permissions": [
    "storage",
    "background",
    "https://www.yahoo.com/",
    "https://suggestqueries.google.com/",
    "chrome://favicon/"
  ],
  "optional_permissions": [
    "management",
    "topSites"
  ],
  "version": "19.3.4.1"
}
