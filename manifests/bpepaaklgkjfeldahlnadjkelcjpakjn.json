{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "de",
  "minimum_chrome_version": "34",
  "author": "Valiant Bank AG",
  "homepage_url": "https://www.valiant.ch",
  "version": "4.2.17",
  "icons": {
    "16": "logo_16x16.png",
    "32": "logo_32x32.png",
    "48": "logo_48x48.png",
    "128": "logo_128x128.png"
  },
  "web_accessible_resources": [
    "logo_16x16.png",
    "logo_32x32.png",
    "logo_48x48.png",
    "logo_128x128.png"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "matches": [
        "https://*.valiant.ch/*"
      ]
    }
  ],
  "permissions": [
    "desktopCapture"
  ]
}
