{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Apexticky for APEX 4.2",
  "description": "Improves the navigation for Oracle APEX 4.2 by @matt_mulvaney @Explorerukltd",
  "version": "1.2.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*f?p=4000*",
        "*://*/*wwv_flow.accept*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "jquery.sticky.js",
        "mousetrap.min.js",
        "clipboard.min.js",
        "notiny.min.js",
        "content.js"
      ],
      "css": [
        "notiny.min.css",
        "apexticky.css"
      ]
    }
  ],
  "browser_action": {
    "default_title": "",
    "default_popup": ""
  },
  "icons": {
    "16": "icon1.png",
    "48": "icon1.png",
    "128": "icon1.png"
  },
  "permissions": [],
  "short_name": "Apexticky",
  "manifest_version": 2
}
