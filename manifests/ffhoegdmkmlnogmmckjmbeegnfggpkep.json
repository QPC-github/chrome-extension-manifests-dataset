{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rdbase",
  "version": "1",
  "manifest_version": 2,
  "description": "Rdbase google patents tool.",
  "content_security_policy": "default-src 'self' https://maxcdn.bootstrapcdn.com;style-src 'unsafe-inline' https://maxcdn.bootstrapcdn.com;script-src 'self' https://dev.rdbase.net http://localhost:8888",
  "browser_action": {
    "default_icon": "logo.jpg",
    "default_title": "Rdbase",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jq.js",
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "downloads"
  ]
}
