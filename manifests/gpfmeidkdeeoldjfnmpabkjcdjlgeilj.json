{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "logo_128.png"
  },
  "browser_action": {
    "default_icon": "logo_128.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "description": "Персонализация YouHack на твой вкус!",
  "content_scripts": [
    {
      "js": [
        "test.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "YHDesign",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
