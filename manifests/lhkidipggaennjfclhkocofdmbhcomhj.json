{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clean Street View",
  "description": "Remove the clutter from Google Street view for cleaner screen grabs",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Clean Street View "
  },
  "manifest_version": 2
}
