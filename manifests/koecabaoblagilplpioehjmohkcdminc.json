{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Data Coin History",
  "version": "2.3",
  "description": "Downloads your Chrome browser history to Sellin.co.za",
  "permissions": [
    "history",
    "https://sellin.co.za/registration/saveHistory.php",
    "https://sellin.co.za/registration/complete.php"
  ],
  "browser_action": {
    "default_popup": "typedUrls.html",
    "default_icon": "clock.png"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://sellin.co.za/registration/index.php",
      "https://sellin.co.za/registration/"
    ]
  },
  "manifest_version": 2
}
