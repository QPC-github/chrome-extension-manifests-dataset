{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "goggul.pls",
  "version": "0.1.1",
  "description": "Make the google homepage better using this simple extension!",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://www.google.com/*",
        "*://www.google.be/*"
      ]
    }
  ]
}
