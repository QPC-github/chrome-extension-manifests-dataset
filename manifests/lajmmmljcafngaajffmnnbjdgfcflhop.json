{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "What do you feel?",
  "description": "Changes Facebook's \"What's on your mind?\" question.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "what-do-you-feel.js"
      ],
      "run_at": "document_start"
    }
  ]
}
