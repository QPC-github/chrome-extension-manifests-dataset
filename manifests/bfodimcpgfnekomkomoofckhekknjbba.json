{
  "browser_action": {
    "default_icon": "img/icon_19.png",
    "default_popup": "popup.html",
    "default_title": "GMScript by Jupiter N"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/script2.js"
      ],
      "matches": [
        "*://*.vk.com/*",
        "*://vk.com/*",
        "*://*.fightplay.ru/*",
        "*://*.ihaskin.ru/*"
      ]
    }
  ],
  "description": "Расширение Google Chrome для проверки игрока Город Мертвых.",
  "icons": {
    "128": "img/icon_128.png",
    "49": "img/icon_49.png"
  },
  "manifest_version": 2,
  "name": "GMScript by Jupiter",
  "permissions": [
    "webNavigation",
    "tabs",
    "*://*.vk.com/*",
    "*://vk.com/*",
    "*://*.fightplay.ru/*",
    "*://*.ihaskin.ru/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.2.2",
  "web_accessible_resources": [
    "img/icon_128.png",
    "js/jquery.js",
    "js/script.js"
  ]
}
