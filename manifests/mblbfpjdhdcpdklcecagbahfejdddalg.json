{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cleaner Tab",
  "version": "2.6",
  "description": "Cleaner new-tab page without most-visited and with compact apps block (no scroll bar).",
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*://*.google.*/_/chrome/newtab*"
      ]
    }
  ],
  "offline_enabled": true,
  "manifest_version": 2
}
