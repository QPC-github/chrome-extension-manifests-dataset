{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Adobe Experience Cloud Bookmarks",
  "short_name": "Adobe Bookmarks",
  "description": "Bookmarks to Adobe Experience Cloud: Analytics, Launch, Target, Campaign, AEM, DTM and other solutions, core services and resources.",
  "version": "1.4.29",
  "author": "Andrey Osadchuk",
  "icons": {
    "16": "extension_icon16.png",
    "32": "extension_icon32.png",
    "48": "extension_icon48.png",
    "128": "extension_icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://*.status.adobe.com/"
  ],
  "optional_permissions": [
    "https://*.omniture.com/",
    "https://*.an.adobe.com/",
    "https://experience.adobe.com/",
    "https://*.experiencecloud.adobe.com/",
    "https://launch.adobe.com/",
    "https://assets.adobedtm.com/"
  ],
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "popup.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "extension_icon16.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "img/*.png"
  ],
  "options_page": "options.html"
}
