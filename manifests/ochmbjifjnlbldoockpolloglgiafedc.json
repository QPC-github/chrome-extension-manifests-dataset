{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BOC Receipt",
  "description": "Prints receipt",
  "version": "1.0.4",
  "content_scripts": [
    {
      "matches": [
        "https://xola.com/seller",
        "https://*.xola.com/seller",
        "http://xola.local/seller"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "main.js"
  ]
}
