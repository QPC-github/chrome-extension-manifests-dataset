{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Chromazing",
  "description": "Change your webpages to ensure a safer, more dynamic and fun browsing experience.",
  "version": "0.3.1.1",
  "icons": {
    "16": "images/icons/ca-16.png",
    "48": "images/icons/ca-48.png",
    "128": "images/icons/ca-128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Chromazing Web Tools",
    "default_icon": "images/icons/ca-19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "images/smartlink/secure.png",
    "images/smartlink/www.png",
    "images/smartlink/bookmark.png",
    "images/smartlink/hash.png",
    "images/smartlink/internal.png",
    "images/smartlink/external.png",
    "images/smartlink/code.png",
    "images/smartlink/danger.png",
    "pages/previewload.html",
    "pages/failedload.html"
  ]
}
