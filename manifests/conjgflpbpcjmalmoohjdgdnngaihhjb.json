{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "My Dog speaks fluent insults",
  "description": "Insult dog",
  "version": "1",
  "author": "Anupam Mahapatra",
  "browser_action": {
    "default_icon": "ico.png",
    "default_title": "Dogra",
    "default_popup": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "ico.png",
    "48": "ico.png",
    "128": "ico.png"
  }
}
