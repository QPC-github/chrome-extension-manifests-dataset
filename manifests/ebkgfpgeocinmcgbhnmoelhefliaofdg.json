{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "DawgTransit",
  "description": "Launch DawgTransit",
  "version": "0.2",
  "manifest_version": 2,
  "app": {
    "urls": [
      "http://*.dawgtransit.com/"
    ],
    "launch": {
      "web_url": "http://dawgtransit.com/"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
