{
  "chrome_url_overrides": {
    "newtab": "sos.html"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "Install our SOS add-on and never miss any important update from your favorite soap opera.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Soap Opera Spy",
  "permissions": [
    "http://soapoperaspy.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
