{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Keyword Retrieve/Delete Tabs",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Retrieve or delete tab's that is related to a entered keyword",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Keyword Retrieve/Delete Tabs",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
