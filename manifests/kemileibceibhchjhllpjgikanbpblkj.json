{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Buy It Yourself",
  "short_name": "BIY",
  "description": "Why more expensive if you can buy it yourself with lower price. No more middleman online",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  }
}
