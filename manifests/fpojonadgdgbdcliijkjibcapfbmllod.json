{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Download Tumblr images",
  "version": "1.1.0",
  "description": "One click Tumblr large resolution image download from the context menu.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "downloads",
    "contextMenus"
  ]
}
