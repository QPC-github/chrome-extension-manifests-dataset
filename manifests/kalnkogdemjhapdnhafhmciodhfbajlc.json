{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pussify",
  "short_name": "Pussify the crap out of any webpage!",
  "description": "Why waste time looking at pictures of your friends when you can be looking at pictures of cats?!",
  "version": "2.1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Bring on the cats",
    "default_icon": "cat_icon.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "tat.png",
    "cat1.png",
    "cat2.png",
    "cat3.png",
    "cat4.png",
    "cat5.png",
    "cat6.png",
    "cat7.png",
    "cat8.png",
    "cat9.png",
    "cat10.png"
  ]
}
