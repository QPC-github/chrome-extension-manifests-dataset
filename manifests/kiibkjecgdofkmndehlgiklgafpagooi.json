{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Google+ Stamina",
  "version": "0.7.3",
  "description": "__MSG_description__",
  "background": {
    "scripts": [
      "gplus_stamina.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "*://plusone.google.com/*"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "default_locale": "en"
}
