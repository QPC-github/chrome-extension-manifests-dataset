{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "chrome-glo",
  "description": "A web extension for Gitkraken glo",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "chrome-glo",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ]
}
