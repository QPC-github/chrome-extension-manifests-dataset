{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Store-a-Value",
  "version": "1.0.1",
  "description": "Stores value.",
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "permissions": [
    "management",
    "cookies",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "jquery.ui.js",
      "jquery.cookie.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'sha256-9fIRaKmxZwDTV013/zBaqjUZoCxuclTYM/dyDLKxg9Y='  https://example.cc; object-src 'self'"
}
