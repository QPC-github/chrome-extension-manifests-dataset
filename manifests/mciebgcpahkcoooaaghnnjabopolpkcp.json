{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GAHN",
  "description": "Get all the browse history on daily base",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.0.2",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "identity",
    "identity.email"
  ]
}
