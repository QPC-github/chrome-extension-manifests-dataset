{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "وضائف شاغرة في الخليج",
  "description": "وضائف شاغرة في الخليج لجميع الدول العربية و لكافة التخصصات التقدم للوضيفة مجاني",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
