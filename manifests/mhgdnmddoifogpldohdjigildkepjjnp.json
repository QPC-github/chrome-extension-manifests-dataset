{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Save Tabs",
  "description": "This extension allows you to save/reload tabs temporarily",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Save Tabs"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
