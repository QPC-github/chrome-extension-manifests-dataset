{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ReddyFilter",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "ReddyFilter hides Reddit posts with whatever keywords you specify.",
  "options_page": "settings.html",
  "content_scripts": [
    {
      "matches": [
        "http://*.reddit.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "ReddyFilter - 16.png",
    "48": "ReddyFilter - 48.png",
    "128": "ReddyFilter - 128.png"
  },
  "permissions": [
    "storage"
  ],
  "homepage_url": "http://www.fongsoft.com"
}
