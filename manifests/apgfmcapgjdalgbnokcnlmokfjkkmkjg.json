{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quickli",
  "version": "0.709",
  "description": "Quicky find similar profiles. Peek into company information.",
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/recruiter/*",
        "https://www.linkedin.com/in/*",
        "https://www.linkedin.com/search/results/people/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "https://github.com/*"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
