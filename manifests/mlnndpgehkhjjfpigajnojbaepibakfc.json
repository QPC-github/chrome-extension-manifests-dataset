{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "menu.html",
    "default_title": "ATSU-Speed Dial"
  },
  "homepage_url": "http://gmr.pvt.ge",
  "background": {
    "page": "background.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "ATSU-Speed Dial is ultimate replacement of newtab page with quick access to your most visited pages, bookmarks and browsing history.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "options.html",
  "permissions": [
    "bookmarks",
    "topSites",
    "tabs",
    "history",
    "management",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "<all_urls>",
    "contextMenus",
    "notifications",
    "alarms"
  ],
  "version": "1.0.0",
  "web_accessible_resources": [
    "newtab.html"
  ]
}
