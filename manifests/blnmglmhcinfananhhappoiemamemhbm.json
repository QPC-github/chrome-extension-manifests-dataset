{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extract Mapped Resources",
  "version": "5.28.19",
  "description": "Extract all resources mapped within js files with retaining folder structure.",
  "icons": {
    "128": "icon.gif"
  },
  "browser_action": {
    "default_icon": "icon.gif",
    "default_title": "Extracting...",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "*://*/*",
    "downloads",
    "downloads.shelf"
  ],
  "devtools_page": "devtool.html",
  "manifest_version": 2
}
