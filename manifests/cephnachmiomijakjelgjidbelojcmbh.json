{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Picinit",
  "description": "Picinit service in chrome extension",
  "version": "0.8",
  "icons": {
    "16": "favicon2_16.ico",
    "48": "fb2_48.jpg",
    "128": "fb2_128.jpg"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "https://*.picinit.com/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "sample.js"
    ]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
