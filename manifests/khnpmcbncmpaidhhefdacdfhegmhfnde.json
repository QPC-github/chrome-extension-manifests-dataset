    "https://*.biccamera.com/*",
    "https://*.kojima.net/*",
    "https://*.nojima.co.jp/*",
    "https://*.edion.com/*",
    "https://*.ebest.co.jp/*",
    "https://*.onoden.co.jp/*",
    "https://*.beisiadenki.com/*",
    "https://*.bookoffonline.co.jp/*",
    "https://*.hikaritv.net/*",
    "https://*.wowma.jp/*",
    "https://*.yahoo.co.jp/*",
    "https://*.ponparemall.com/*",
    "https://*.omni7.jp/*",
