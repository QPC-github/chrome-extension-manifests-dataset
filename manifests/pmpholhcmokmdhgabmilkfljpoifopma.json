{
  "manifest_version": 2,
  "name": "Amazon Prime Launcher",
  "permissions": [
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2.2",
  "background": {
    "scripts": [
      "jquery-2.1.3.min.js",
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "a-19.png",
      "38": "a-38.png"
    },
    "default_title": "Amazon - One Click"
  },
  "description": "Turn on 1-click settings to access Amazon Prime",
  "icons": {
    "128": "a-128.png",
    "16": "a-16.png",
    "48": "a-48.png"
  },
  "key": ""
}
