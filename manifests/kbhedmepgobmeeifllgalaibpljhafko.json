{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "moodrain-bookmark-ext",
  "description": "",
  "version": "1.0.5",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "moodrain-boomark-ext",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "storage",
    "http://localhost/",
    "https://moodrain.cn/",
    "https://v2.moodrain.cn/",
    "contextMenus"
  ]
}
