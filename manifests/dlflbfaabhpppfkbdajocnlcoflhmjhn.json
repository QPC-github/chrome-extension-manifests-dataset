{
  "name": "Reportr",
  "version": "1.0.0",
  "description": "Statistics for Aller websites provided by Reportr.",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "background"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Reportr"
  }
}
