{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_32x32.png",
    "default_title": ""
  },
  "description": "A Moo.do panel on your desktop",
  "icons": {
    "128": "icon_128x128.png",
    "16": "icon_16x16.png",
    "32": "icon_32x32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://moo.do/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Moo.do Panel",
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.1"
}
