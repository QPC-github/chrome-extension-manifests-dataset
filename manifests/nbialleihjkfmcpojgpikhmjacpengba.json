{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Useless Websites",
  "description": "If you are bored this extension shows you useless websites.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Useless websites",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
