{
  "author": "Dee Anna",
  "name": "VeriWinkle",
  "short_name": "VeriWinkle",
  "homepage_url": "https://VeriWinkle.com",
  "description": "The new VeriWinkle extension creates search keyword bookmarks and makes it easy to just use the VeriWinkle.com Search Engine.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "VeriWinkle"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://VeriWinkle.com/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "VeriWinkle",
      "name": "VeriWinkle",
      "search_url": "https://VeriWinkle.com/?a=web&q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "1.js",
      "2.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "1.html"
  },
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ]
}
