{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Подбор адреса",
  "version": "1.1.0",
  "description": "Сервис находит однозначное соответствие вводимого с клавиатуры почтового адреса со стандартной записью в эталонном справочнике.",
  "icons": {
    "16": "logo_sh16.png",
    "48": "logo_sh.png",
    "128": "logo128.png"
  },
  "permissions": [
    "bookmarks",
    "http://www.iqdq.ru/"
  ],
  "browser_action": {
    "dafault_title": "IQDQ",
    "default_icon": "logo_sh.png",
    "default_popup": "popup.html"
  }
}
