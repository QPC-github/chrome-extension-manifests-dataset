{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Laser Cat",
  "version": "1.11",
  "manifest_version": 2,
  "description": "Shoot laser at things you want to remove from the internet.",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "key": "",
  "browser_action": {
    "name": "Laser Cat",
    "default_title": "Shoot laser at things you want to remove from the internet.",
    "default_icon": "icon128.png",
    "default_popup": "/popup/popup.html",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "background.js",
    "config.js",
    "cat.svg",
    "cat.png",
    "sounds/meow.mp3",
    "sounds/laser.mp3",
    "sounds/rainbow.mp3",
    "sounds/frog.mp3",
    "sounds/slime.mp3",
    "sounds/woop.mp3",
    "sounds/space.mp3",
    "js/util.js",
    "js/soundbox.js",
    "js/app.js",
    "js/rainbow.js",
    "js/eye.js",
    "js/characters/character.js",
    "js/characters/laser-cat.js",
    "js/characters/hungry-frog.js",
    "js/characters/angry-alien.js"
  ],
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
