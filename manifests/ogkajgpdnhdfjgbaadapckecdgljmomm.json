{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icons/19.png",
    "default_popup": "index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "is_default": true,
      "name": "Video Search",
      "keyword": "Video",
      "encoding": "UTF-8",
      "search_url": "http://realgreatsearch.com/srch/?keyword={searchTerms}",
      "favicon_url": "http://realgreatsearch.com/favicon.ico"
    }
  },
  "description": "Extension to search any media files",
  "icons": {
    "128": "icons/128.png",
    "19": "icons/19.png"
  },
  "omnibox": {
    "keyword": "p"
  },
  "manifest_version": 2,
  "name": "Personal Finder",
  "permissions": [
    "cookies",
    "http://*/*",
    "storage",
    "tabs",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "*://search.feedvertizus/*"
  ],
  "short_name": "Personal Finder",
  "version": "2.8.7"
}
