  },
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "GenPass - Умный генератор паролей",
    "default_popup": "genPass.html"
  }
}
