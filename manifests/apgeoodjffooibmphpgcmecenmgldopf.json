{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Skirtly",
  "description": "We're turning your favourite online fashion retailers into an Infinite Closet, so you never have to wear the same thing twice!",
  "version": "0.20",
  "permissions": [
    "http://skirtly.com/"
  ],
  "homepage_url": "http://skirtly.com/",
  "content_scripts": [
    {
      "matches": [
        "*://www.asos.com/*",
        "*://us.asos.com/*"
      ],
      "js": [
        "js/jquery-1.8.2.min.js",
        "js/growl.js",
        "js/handlebars-1.0.rc.1.js",
        "js/jquery.infinitecarousel3.min.js",
        "js/jquery.easing.1.3.js",
        "js/conf.js",
        "js/main.js",
        "js/content-script.js"
      ],
      "css": [
        "css/growl.css",
        "css/main.css"
      ]
    }
  ],
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo.png"
  },
  "web_accessible_resources": [
    "images/*",
    "js/*",
    "*.ttf"
  ],
  "manifest_version": 2
}
