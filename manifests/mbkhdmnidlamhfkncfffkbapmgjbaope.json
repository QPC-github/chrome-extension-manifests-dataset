{
  "background": {
    "scripts": [
      "firstrun.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "crollbar.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Pure CSS3 Wide Dark Grey/Red Scrollbar. (very low resource use!) Kavorka Designs - Crollbar http://crollbar.kavorka-designs.me",
  "homepage_url": "http://crollbar.kavorka-designs.me",
  "icons": {
    "128": "img/Cb128.png",
    "16": "img/Cb128.png",
    "48": "img/Cb128.png"
  },
  "manifest_version": 2,
  "name": "Crollbar (Wide Dark Grey/Red Version)",
  "options_page": "options.html",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.6"
}
