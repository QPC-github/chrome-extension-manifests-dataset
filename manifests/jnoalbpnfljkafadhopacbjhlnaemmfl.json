{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mediaklikkMusor",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Megmutatja a musort a lejátszó alatt",
  "content_scripts": [
    {
      "matches": [
        "http://www.mediaklikk.hu/*-elo*"
      ],
      "js": [
        "js/jquery.js",
        "js/musor.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://www.mediaklikk.hu/*-elo/*",
    "http://www.mediaklikk.hu/*"
  ]
}
