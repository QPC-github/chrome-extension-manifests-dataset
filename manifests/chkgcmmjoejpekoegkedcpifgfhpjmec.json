{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/bookcase.png",
    "default_popup": "config.html",
    "default_title": "Library Extension"
  },
  "content_scripts": [
    {
      "css": [
        "common.css"
      ],
      "js": [
        "common.js"
      ],
      "matches": [
        "https://www.libraryextension.com/*",
        "https://api.libraryextension.com/*"
      ]
    }
  ],
  "content_security_policy": "connect-src 'self' *; script-src 'self'; object-src 'self';",
  "default_locale": "en",
  "description": "See books, music and more at your local library as you browse the internet",
  "icons": {
    "128": "images/bookcase.png"
  },
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "options_ui": {
    "chrome_style": true,
    "page": "config.html"
  },
  "permissions": [
    "tabs",
    "https://www.libraryextension.com/*",
    "https://api.libraryextension.com/*",
    "http://*/",
    "https://*/"
  ],
  "short_name": "__MSG_extName__",
  "version": "2020.922.1",
  "web_accessible_resources": [
    "common.css",
    "css/bootstrap.min.css",
    "css/bootstrap-theme.min.css",
    "images/001_01.png",
    "images/001_02.png",
    "images/001_05.png",
    "images/001_06.png",
    "images/001_30.png",
    "images/001_38.png",
    "images/001_50.png",
    "images/bookcase.png",
    "images/hoopla-49x21.png",
    "images/openlibrary-logo-tighter.svg",
    "images/pause.png",
    "images/play.png",
    "images/toolbar_select.gif"
  ]
}
