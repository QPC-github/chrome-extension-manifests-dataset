{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "not_logged_in.png"
  },
  "default_locale": "en",
  "icons": {
    "128": "icon_128.png"
  },
  "name": "Revisor Chrome Extension",
  "version": "0.0.1",
  "description": "Smart cloud monitoring with revisor.heilihq.com",
  "homepage_url": "http://revisor.heilihq.com",
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.revisor.heilihq.com/"
  ],
  "manifest_version": 2
}
