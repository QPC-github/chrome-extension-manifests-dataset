{
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "Reload"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Reloads the current webpage through a proxy",
  "background": {
    "page": "background.html"
  },
  "name": "Reload with proxy",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "manifest_version": 2,
  "version": "0.2"
}
