{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Musical Timer",
  "description": "An extension to play music at specified times.",
  "version": "2.1",
  "permissions": [
    "alarms",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon19blue.png",
      "38": "icon38blue.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
