{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "description": "This extension displays the MozCast algorithm temperature",
  "icons": {
    "128": "icon128.png",
    "16": "icon.png",
    "48": "icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "MozCast Chrome Extension",
  "permissions": [
    "http://*.mozcast.com/",
    "http://mozcast.com/",
    "http://moz.com/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4.2"
}
