{
  "name": "New Window",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "This extension add a button to the menu bar which allowing you to open a new window.",
  "icons": {
    "16": "icon16.gif"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon16.gif"
    },
    "default_title": "New window"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
