{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "version": "1.1",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "hg_16.png",
    "48": "hg_48.png",
    "128": "hg_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "sample.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ]
}
