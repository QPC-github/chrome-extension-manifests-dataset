{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "instructables",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.instructables.com/editInstructable/*"
      ],
      "js": [
        "jquery-1.10.2.min.js",
        "bootstrap.min.js",
        "instructables.js"
      ],
      "css": []
    }
  ]
}
