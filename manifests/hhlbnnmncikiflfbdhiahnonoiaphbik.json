{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Incentive",
  "version": "0.0.0.9",
  "description": "Incentive is a leading provider of a complete social intranet for mid-market organizations and enterprise teams.",
  "short_name": "Incentive",
  "permissions": [
    "webview",
    "pointerLock",
    "videoCapture",
    "audioCapture",
    "storage",
    "notifications"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  }
}
