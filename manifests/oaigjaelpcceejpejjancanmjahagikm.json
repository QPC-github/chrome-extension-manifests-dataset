{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Url QRcode",
  "manifest_version": 2,
  "version": "1.1",
  "description": "将网页链接生成二维码",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon_19.png",
      "38": "icon_38.png"
    },
    "default_title": "Url Qrcode",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
