{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Pink Rickshaw Design Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "prdlogo3.png",
    "48": "prdlogo2.png",
    "128": "prdlogo1.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://kanavkahol.com/*"
    ]
  }
}
