{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "jQuery Unique Selector",
  "version": "0.1.1",
  "description": "Find the unique selector for any element on page.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "css_selector_icon_128.png",
    "default_title": "Click to start"
  },
  "icons": {
    "128": "css_selector_icon_128.png",
    "48": "css_selector_icon_48.png",
    "16": "css_selector_icon_16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [],
      "js": [
        "js/jquery.js",
        "js/jquery.hotkeys.js",
        "/js/jquery.hoverIntent.js",
        "/src/dom-selector.js",
        "/src/highlighter.js",
        "/src/jquery.ct.js",
        "/js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "**.css",
    "**.html",
    "**.js",
    "**.png",
    "**.gif",
    "**.map"
  ],
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
