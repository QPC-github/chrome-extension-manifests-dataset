{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Right-Click Search",
  "description": "Quickly search for highlighted text on Youtube.com via right-click",
  "version": "1.1",
  "author": "Aso Naderi",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
