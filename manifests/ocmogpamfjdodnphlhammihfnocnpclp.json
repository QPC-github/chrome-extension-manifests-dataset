{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Your own CSS",
  "version": "0.0.0.1",
  "description": "Your own CSS on every web pages !",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "declarativeContent",
    "<all_urls>"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
