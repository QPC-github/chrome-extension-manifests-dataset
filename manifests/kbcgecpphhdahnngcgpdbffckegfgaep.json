{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Photo NewTab",
  "short_name": "Photo NewTab",
  "description": "Shows one Random Image of my Page photo.philippkeschl.at",
  "manifest_version": 2,
  "version": "1.3",
  "permissions": [
    "management",
    "https://photo.philippkeschl.at/"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}
