{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "PonyUpz",
  "description": "Making Ponychan and Lunachan better",
  "version": "1.0.3",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "http://ponychan.net/*",
    "http://lunachan.net/*"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://ponychan.net/*",
        "http://lunachan.net/*",
        "http://www.ponychan.net/*",
        "http://www.lunachan.net/*"
      ],
      "js": [
        "ponyupz.user.js"
      ]
    }
  ],
  "manifest_version": 2
}
