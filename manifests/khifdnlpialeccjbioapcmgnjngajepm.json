{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Favorites Checker"
  },
  "default_locale": "en",
  "description": "Favorites Checker. Scan favorites for broken or dead links. Delete empty, and duplicate folders.",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png"
  },
  "manifest_version": 2,
  "name": "Favorites Checker",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "bookmarks"
  ],
  "short_name": "Bookmark Checker",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.2"
}
