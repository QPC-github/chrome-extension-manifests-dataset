{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ABCnet Translit",
  "description": "This extension translits Kazakh text to ABCnet latin Kazakh alphabet.",
  "version": "4.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
