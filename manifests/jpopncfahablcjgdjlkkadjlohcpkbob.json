{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "NoteIfy",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Store notes to be synced to individual users.",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "NoteIfy",
    "background_page": "background.html",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications"
  ]
}
