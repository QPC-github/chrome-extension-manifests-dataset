{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SensoReport Extension",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Helps to Share Links via SensoReport.",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "sr.html"
  },
  "permissions": [
    "http://*/*",
    "tabs"
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "homepage_url": "http://www.sensoreport.com",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
