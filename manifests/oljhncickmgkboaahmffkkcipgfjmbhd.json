{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Freelancer Popup Remover",
  "version": "1.0",
  "description": "Remove the annoying new job popups from freelancer.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.freelancer.com/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ]
}
