{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hostel 4 Extension",
  "version": "0.0.0.4",
  "description": "All browser extension for Insti ka baps!",
  "homepage_url": "https://www.ybhaw.in",
  "icons": {
    "48": "hostel4.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "hostel4.png",
    "default_title": "Hostel 4",
    "default_popup": "aws.html"
  },
  "web_accessible_resources": [
    "hostel4.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://gpo.iitb.ac.in/src/login.php*",
        "*://moodle.iitb.ac.in/login/index.php*",
        "*://placements.iitb.ac.in/*",
        "*://internet.iitb.ac.in/*"
      ],
      "js": [
        "login.js"
      ]
    }
  ]
}
