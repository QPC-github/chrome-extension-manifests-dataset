{
  "chrome_url_overrides": {
    "newtab": "iframe.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Okay",
      "keyword": "Okay Search Engine",
      "search_url": "https://getokay.com/?a=b&q={searchTerms}",
      "favicon_url": "https://getokay.com/img/sweet_logo2.png",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "browser_action": {
    "default_title": "Okay - One Click Search",
    "default_icon": {
      "16": "./img/16.png",
      "32": "./img/32.png",
      "64": "./img/64.png",
      "128": "./img/128.png"
    }
  },
  "manifest_version": 2,
  "description": "Okay lets you to easily search between Live Gaming, Social, Sports, and  search services all with one click.",
  "icons": {
    "16": "./img/16.png",
    "32": "./img/32.png",
    "64": "./img/64.png",
    "128": "./img/128.png"
  },
  "name": "Okay - Top Streams in One Search",
  "short_name": "Okay",
  "background": {
    "scripts": [
      "ext-background.js"
    ],
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "https://www.getokay.com/*"
    ]
  },
  "permissions": [
    "contextMenus",
    "https://www.getokay.com/*",
    "https://getokay.com/*",
    "webNavigation",
    "activeTab",
    "cookies",
    "unlimitedStorage",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "management"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0.1"
}
