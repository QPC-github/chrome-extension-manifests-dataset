{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "qr2.li URL shortener",
  "version": "1.0.0.1",
  "description": "URL to quick response QR code url shortener",
  "icons": {
    "96": "96.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "96.png",
    "default_title": "Shorten URL",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://qr2.li/*"
  ],
  "default_locale": "en",
  "manifest_version": 2
}
