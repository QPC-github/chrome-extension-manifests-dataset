{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Reddit Signup",
  "version": "1.3",
  "description": "Removes the signup for reddit popup modal on every page when using old.reddit.com",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://old.reddit.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
