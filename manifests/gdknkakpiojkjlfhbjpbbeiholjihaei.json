{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Enjoy Learning English Articles",
  "short_name": "English Articles",
  "version": "1.13",
  "description": "English.Articles is a free service that lets you learn how to use English articles while surfing the Internet.",
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "FUNZIONANTE.jquery.contextmenu.r2.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/img-ok.png",
    "img/img-wrong.png"
  ],
  "browser_action": {
    "default_title": "English Articles",
    "default_popup": "popup.html",
    "default_icon": "img/16x16.png"
  },
  "icons": {
    "128": "img/128x128.png",
    "48": "img/48x48.png",
    "16": "img/16x16.png"
  }
}
