{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This extension attempts to replace all images of Nigel Farage with an image of a frog",
  "manifest_version": 2,
  "name": "Frogage",
  "version": "1.0",
  "web_accessible_resources": [
    "frog.jpg"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
