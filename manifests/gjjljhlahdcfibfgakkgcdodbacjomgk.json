{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Memory Notifier",
  "description": "Show the amount of memory in use.",
  "version": "1.0.1",
  "permissions": [
    "system.memory",
    "storage"
  ],
  "web_accessible_resources": [
    "images/*",
    "script/*",
    "fonts/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_title": "Starting..."
  },
  "manifest_version": 2
}
