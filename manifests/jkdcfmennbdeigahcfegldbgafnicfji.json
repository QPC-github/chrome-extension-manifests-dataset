{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RegistRAWR",
  "description": "Base Level Extension",
  "version": "0.7.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "alarms"
  ],
  "browser_action": {
    "default_popup": "khalas.html"
  },
  "icons": {
    "16": "tigerlogo1.png",
    "48": "tigerlogo2.png",
    "128": "tigerlogo3.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www-banner.aub.edu.lb/*"
      ],
      "js": [
        "undead.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
