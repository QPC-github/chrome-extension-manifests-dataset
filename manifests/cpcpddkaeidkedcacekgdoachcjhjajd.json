{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fast search",
  "version": "1.0",
  "permissions": [
    "*://*/*",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
