{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.html"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.10.1.min.js",
        "coins2openurl.js"
      ],
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "description": "Interrogation du résolveur de liens de l'Université d'Aix-Marseille",
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "img/16.png"
  },
  "manifest_version": 2,
  "name": "COinS 2 SMASH",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "version": "0.1",
  "web_accessible_resources": [
    "img/coins2smash.png"
  ]
}
