{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Video Tilt",
  "description": "Tilt Videos to Portrait or Landscape. Works on: Facebook, Youtube, Vimeo, etc",
  "version": "0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/img/icons/16.png",
    "48": "assets/img/icons/48.png",
    "128": "assets/img/icons/128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
