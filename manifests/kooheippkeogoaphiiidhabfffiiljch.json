{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Website Migration Checklist (WebMaxFormance)",
  "description": "Website migration checklist for helping you undertake complex site migrations.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "checklist.html"
  },
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
