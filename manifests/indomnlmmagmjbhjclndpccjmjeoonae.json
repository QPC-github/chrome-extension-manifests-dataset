{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sedka Alert",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Notifications live pour le Stream de Sedka",
  "permissions": [
    "background",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "notification.js",
      "jquery-3.1.1.min.js"
    ]
  }
}
