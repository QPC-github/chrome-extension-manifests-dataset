{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Australian Dollar to Dollarydoo",
  "description": "900 Dollarydoos! Tobias!",
  "version": "1.0",
  "author": "Burt Stanton",
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
