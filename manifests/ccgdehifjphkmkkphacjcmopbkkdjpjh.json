{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BaiduAD",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "BaiduAD",
  "browser_action": {
    "default_icon": "baiduad.png"
  },
  "permissions": [
    "webNavigation",
    "http://www.baidu.com/*",
    "https://www.baidu.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "matches": []
  },
  "content_scripts": [],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
