{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_popup": "popup.html",
    "icons": [
      ""
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Youtube Mouseover with Link",
  "version": "1.0"
}
