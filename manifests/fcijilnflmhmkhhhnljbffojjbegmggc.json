{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BrandedTab Demo",
  "description": "Simplified example extension to demonstrate the BrandedTab process.",
  "version": "1.2.4",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "*://*.brandedtab.com/*",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "lib/redirect.html"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "web_accessible_resources": [
    "icon16.png"
  ]
}
