{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "JSBin",
  "description": "JS Bin - Collaborative JavaScript Debugging",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "icon.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "JS Bin"
  }
}
