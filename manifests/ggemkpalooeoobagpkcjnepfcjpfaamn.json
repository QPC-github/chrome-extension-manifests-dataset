{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SAFARI Montage Chrome Extension",
  "short_name": "SM Extension",
  "description": "SAFARI Montage users can easily ingest any weblink, YouTube link, Google Doc link or file, or capture a Google Slide as a Slate.",
  "version": "1.0.8",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "images/SM-icon-16px.png",
    "48": "images/SM-icon-48px.png",
    "128": "images/SM-icon-128px.png"
  },
  "browser_action": {
    "default_title": "Add to SAFARI Montage",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/SM-icon-16px.png",
      "32": "images/SM-icon-32px.png",
      "48": "images/SM-icon-48px.png",
      "64": "images/SM-icon-64px.png",
      "128": "images/SM-icon-128px.png"
    }
  },
  "options_page": "options.html",
  "manifest_version": 2
}
