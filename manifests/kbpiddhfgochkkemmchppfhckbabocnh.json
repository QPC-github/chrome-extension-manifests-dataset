{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "GodVille",
  "version": "1.22",
  "manifest_version": 2,
  "description": "Статистика похождений героя в Годвилле.",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "permissions": [
    "http://godville.net/*"
  ]
}
