{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Moacir Cezare Junior",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery.min.js",
      "cacheJS.min.js",
      "background.js"
    ]
  },
  "name": "Cupons de Desconto Rede Natura",
  "description": "Cupom Natura. http://rede.natura.net/espaco/economiza",
  "version": "1.3",
  "icons": {
    "128": "images/icone128.png",
    "22": "images/icone22.png",
    "48": "images/icone48.png",
    "76": "images/icone76.png"
  },
  "browser_action": {
    "default_icon": "images/icone76.png",
    "default_popup": "main.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "jquery.cookie.min.js",
        "cacheJS.min.js",
        "bootstrap.min.js"
      ],
      "css": [
        "css/bootstrap.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "cookies",
    "notifications",
    "contextMenus",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "storage"
  ]
}
