{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "fix twitter !important",
  "description": "reverts the circles back to simple rounded corners",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "css": [
        "revert.css"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
