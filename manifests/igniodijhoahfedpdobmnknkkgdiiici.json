{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Requests Checker",
  "description": "Sending requests until the status code 200",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "offline_enabled": false,
  "permissions": [
    "tabs",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Requests Checker",
    "default_icon": "icon_38.png",
    "default_popup": "popup.html"
  }
}
