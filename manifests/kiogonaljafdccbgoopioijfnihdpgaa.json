{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Replace your new tab with Hacker News",
  "manifest_version": 2,
  "name": "Rocks",
  "author": "Joe Richardson",
  "short_name": "Rocks",
  "version": "0.0.6",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/services/feed/load; object-src 'self'",
  "permissions": [
    "https://ajax.googleapis.com/ajax/services/feed/load",
    "storage"
  ],
  "icons": {
    "16": "images/icons/16.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  }
}
