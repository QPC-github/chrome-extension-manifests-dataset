{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "https://*/unifyle/*"
      ],
      "all_frames": true
    }
  ],
  "description": "Unifyle Extension",
  "name": "Unifyle Extension",
  "permissions": [
    "nativeMessaging"
  ],
  "minimum_chrome_version": "43",
  "version": "1.1.1.424",
  "icons": {
    "16": "pd_appicon16x16.png",
    "48": "pd_appicon48x48.png",
    "128": "pd_appicon128x128.png"
  }
}
