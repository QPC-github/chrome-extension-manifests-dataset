{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "No Vowels Allowed!",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Not a single vowel was allowed that day.",
  "permissions": [
    "http://*.facebook.com/",
    "https://*.facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ]
    }
  ]
}
