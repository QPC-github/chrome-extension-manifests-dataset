{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "sMedia-Visual Scraper",
  "description": "An awesome extension for visual scraper!",
  "permissions": [
    "tabs",
    "contextMenus",
    "declarativeContent",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "version": "1.0",
  "author": "Code Surfer",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
