{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "manifest_version": 2,
  "name": "SnapMega Cashback Notifier",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "cookies"
  ],
  "short_name": "SM Notifier",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1.1",
  "web_accessible_resources": [
    "img/*.png"
  ]
}
