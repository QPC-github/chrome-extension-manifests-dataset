{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kiosk",
  "description": "Return main page after a minute",
  "version": "1.0",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Returnt main page",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "jquery.js",
    "for.js"
  ]
}
