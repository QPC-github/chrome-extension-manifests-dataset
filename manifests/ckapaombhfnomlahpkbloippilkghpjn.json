{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Kickmoji",
  "description": "Utility extension for Aiomoji.",
  "version": "2.0",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "activeTab",
    "proxy",
    "cookies",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Kickmoji",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
