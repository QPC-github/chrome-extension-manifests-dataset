{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SBT Wallet",
  "description": "SBT Wallet",
  "version": "1.0.5",
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.min.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/login_logo.png",
    "default_popup": "KR/dispatcher.html"
  },
  "permissions": [
    "file://*",
    "webRequest",
    "activeTab",
    "declarativeContent",
    "storage",
    "notifications",
    "http://localhost/*",
    "contextMenus"
  ]
}
