{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "2.0.4",
  "name": "Reddit : Jump to subReddit",
  "description": "Find all subReddit listed on a page and open them from the context menu.",
  "icons": {
    "128": "icon-large.png",
    "16": "icon-bitty.png",
    "48": "icon-small.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "content_script.js"
      ],
      "matches": [
        "http://www.reddit.com/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications"
  ],
  "web_accessible_resources": [
    "icon-small.png"
  ]
}
