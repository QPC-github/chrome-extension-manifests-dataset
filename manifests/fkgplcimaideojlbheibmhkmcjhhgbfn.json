{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Get stock data UK",
  "author": "freesharedata.com",
  "description": "Quickly get data on a UK share by selecting its symbol or name on any page. Right-click & open a page from one of several providers.",
  "version": "1.2",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "selectionLookup.js"
    ]
  },
  "manifest_version": 2,
  "homepage_url": "http://freesharedata.com/getstockdata",
  "icons": {
    "64": "Get-stock-data-UK-icon-64.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
