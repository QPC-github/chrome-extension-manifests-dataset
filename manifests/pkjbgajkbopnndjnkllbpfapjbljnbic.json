{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump The Fast",
  "version": "2.0",
  "description": "Replace tempting food with Donald Trump!",
  "background": "background.html",
  "page_action": {
    "default_icon": "icon-16.png",
    "default_title": "Trump your tempatations away!"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "trump.js"
      ]
    }
  ]
}
