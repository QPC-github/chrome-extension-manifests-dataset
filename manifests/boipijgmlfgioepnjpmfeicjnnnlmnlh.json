{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hansir 好房研发短信助手",
  "description": "开发、测试时，从数据库或应用日志中查询短信的操作十分繁琐，Hansir 帮你搞定一切~",
  "version": "1.1.3",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "name": "Click to change the icon's color",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "mobile-sms.png",
    "48": "mobile-sms.png",
    "128": "mobile-sms.png"
  },
  "manifest_version": 2
}
