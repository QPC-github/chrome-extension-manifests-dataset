{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Domainistrator",
  "description": "This extension swithches a webpage host while keeping its pathname.",
  "version": "0.1.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Domainistrator",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
