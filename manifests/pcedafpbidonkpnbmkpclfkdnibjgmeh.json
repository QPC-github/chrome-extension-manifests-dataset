{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SF Click by CCCINFOTECH",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Lets you login Force.com with 1 click.",
  "icons": {
    "16": "icons/16-16.png",
    "48": "icons/48-48.png",
    "128": "icons/48-48.png"
  },
  "omnibox": {
    "keyword": "yeah"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19-19.png",
      "38": "icons/38-38.png"
    },
    "default_popup": "main.html"
  },
  "short_name": "SF Click",
  "author": "Neeraj Kumar",
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
