{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "fbrh.js"
      ],
      "matches": [
        "*://*.facebook.com/*",
        "*://*.twitter.com/*",
        "*://*.plus.google.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Currently blocks Facebook, Twitter, Google+.",
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png",
    "16": "images/16.png"
  },
  "manifest_version": 2,
  "name": "Social Rehab",
  "permissions": [
    "tabs"
  ],
  "version": "1.0"
}
