{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Welcome Home -  custom New Tab replacement",
  "version": "0.9.5",
  "description": "Transform your homepage into a highly customisable page that REALLY feels like home.",
  "manifest_version": 2,
  "default_locale": "en",
  "author": "Joe Moses",
  "chrome_url_overrides": {
    "newtab": "homepage.html"
  },
  "permissions": [
    "tabs",
    "https://api.openweathermap.org/*",
    "geolocation",
    "identity",
    "bookmarks"
  ],
  "icons": {
    "16": "assets/img/16.png",
    "48": "assets/img/48.png",
    "128": "assets/img/128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
