{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "掌上书苑豆瓣插件",
  "version": "1.2.0",
  "description": "可以在豆瓣书籍信息页显示对应书籍在掌上书苑的下载链接",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "douban.user.js"
      ]
    }
  ],
  "icons": {
    "128": "logo_128.png"
  }
}
