{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Horse Says",
  "description": "News straight from the horse's mouth.",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.wsj.com/*"
      ],
      "js": [
        "horse.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "horse16.png",
    "48": "horse48.png",
    "128": "horse128.png"
  }
}
