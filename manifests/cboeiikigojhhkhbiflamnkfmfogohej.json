{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VU Dashboard",
  "version": "1.0.0.0",
  "default_locale": "en",
  "description": "VU dashboard chrome extension",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_title": "VU Dashboard",
    "default_icon": "/dist/images/favicon.ico",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "geolocation",
    "chrome://favicon/",
    "*://*.health-iot.labs.vu.nl/*",
    "notifications"
  ]
}
