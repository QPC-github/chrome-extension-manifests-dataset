{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Motivation Video",
  "description": "When you need a quick pep talk, this extension will play a 30 second motivational clip",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "http://youtube.com/*"
  ],
  "browser_action": {
    "default_title": "Motivation Video",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
