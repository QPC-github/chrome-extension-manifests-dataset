{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trippy Deal",
  "description": "TrippyDeal helps you find all the best discounts on travel sites without ever leaving the page.",
  "version": "1.2.2",
  "icons": {
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-80.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "identity",
    "http://*/*",
    "https://*/*"
  ]
}
