{
  "manifest_version": 2,
  "name": "Unblock Websites - Free VPN Proxy - ProxyFish",
  "version": "0.3.2",
  "short_name": "proxyfishvpn",
  "description": "Unblock websites with our free & fast anonymous proxy VPN extension. Change your IP Address instantly and hide your real IP address.",
  "icons": {
    "16": "assets/icons/on-16.png",
    "32": "assets/icons/on-32.png",
    "48": "assets/icons/on-48.png",
    "64": "assets/icons/on-64.png",
    "128": "assets/icons/on-128.png"
  },
  "browser_action": {
    "default_popup": "popups/check.html",
    "default_icon": {
      "16": "assets/icons/on-16.png",
      "32": "assets/icons/on-32.png",
      "48": "assets/icons/on-48.png",
      "64": "assets/icons/on-64.png",
      "128": "assets/icons/on-128.png"
    }
  },
  "author": "SENTQ",
  "background": {
    "persistent": true,
    "scripts": [
      "./assets/js/jquery.js",
      "./assets/js/scripts.js",
      "background.js"
    ]
  },
  "homepage_url": "http://www.proxyfish.com/extension.php",
  "permissions": [
    "proxy",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "privacy",
    "management",
    "background",
    "tabs",
    "http://*/",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "minimum_chrome_version": "40.0.0"
}
