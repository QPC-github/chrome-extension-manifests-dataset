{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "bandcamp stock",
  "version": "0.71",
  "icons": {
    "512": "bc_stock_512.png",
    "128": "bc_stock_128.png",
    "32": "bc_stock_32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ]
}
