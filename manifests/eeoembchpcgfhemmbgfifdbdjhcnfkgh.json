{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YDetector",
  "version": "1.3.3",
  "manifest_version": 2,
  "description": "Have a good laugh, even when you're not on YDetector.com",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "YDetector",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "notifications",
    "http://www.ydetector.com/",
    "storage"
  ]
}
