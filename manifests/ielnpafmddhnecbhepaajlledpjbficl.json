{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Calificaciones desde Moodle",
  "description": "Obtiene Nombres y Evaluaciones de los Participantes en un aula Moodle.",
  "version": "2.1",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
