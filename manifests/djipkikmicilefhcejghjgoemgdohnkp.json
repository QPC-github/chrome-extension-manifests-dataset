{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "StackExchange Copy to Clipboard",
  "version": "1.4",
  "author": "Martin Schmelzer",
  "description": "Double click inside a code chunk to copy it to the clipboard.",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.stackoverflow.com/*",
        "*://*.stackexchange.com/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "sectoc.js"
      ],
      "css": [
        "sectoc.css"
      ]
    }
  ]
}
