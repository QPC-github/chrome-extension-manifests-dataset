{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Project-CMFA Quick List",
  "description": "Browsing the ZarpGaming.com forums without the option to save the best posts is a pain. Save them with Project-CMFA Quick List.",
  "version": "1.0.2",
  "content_security_policy": "script-src 'self' https://api-ssl.bitly.com; object-src 'self'",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
