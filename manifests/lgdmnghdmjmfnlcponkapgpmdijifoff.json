{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Undead",
  "description": "This extension removes any details of people's deaths from Wikipedia",
  "version": "1.11",
  "icons": {
    "16": "daisy_16.png",
    "48": "daisy_48.png",
    "128": "daisy_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://en.wikipedia.org/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "undead.js"
      ],
      "run_at": "document_end"
    }
  ]
}
