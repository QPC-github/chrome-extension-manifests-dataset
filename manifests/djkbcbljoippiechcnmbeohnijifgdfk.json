{
  "app": {
    "background": {
      "scripts": [
        "scripts/background.js"
      ]
    }
  },
  "description": "External Frameless Window for Netflix",
  "icons": {
    "16": "assets/fl-icon-16.png",
    "32": "assets/fl-icon-32.png",
    "128": "assets/fl-icon-128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "name": "Frameless for Netflix",
  "permissions": [
    "webview",
    "alwaysOnTopWindows",
    "storage",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "<all_urls>",
    "browser",
    "fullscreen"
  ],
  "short_name": "Frameless Netflix",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2.2"
}
