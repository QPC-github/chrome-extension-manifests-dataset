{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Chartbeat traffic stats ticker",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Chartbeat traffic stats ticker. See live visitors count from your page. Always have control of your page traffic.",
  "homepage_url": "https://chartbeat.com/",
  "icons": {
    "16": "icons/chartbeat.png",
    "48": "icons/chartbeat.png",
    "128": "icons/chartbeat.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "https://api.chartbeat.com/*",
    "storage"
  ],
  "browser_action": {
    "default_title": "Chartbeat stats",
    "default_popup": "src/popup/popup.html"
  },
  "options_page": "src/options_custom/options.html"
}
