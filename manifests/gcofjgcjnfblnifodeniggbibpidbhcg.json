{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Chatterati",
  "version": "1.3.4",
  "manifest_version": 2,
  "description": "Discuss the web!",
  "background": {
    "scripts": [
      "js/jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_popup": "chatterati.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://chatterati.jainanuj.com/",
    "https://facebook.com/connect/*"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
