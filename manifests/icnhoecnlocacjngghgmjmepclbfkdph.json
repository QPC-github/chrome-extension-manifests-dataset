{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Música Incidental",
  "description": "Póngale efectos de sonido a su vida",
  "version": "0.8.1",
  "manifest_version": 2,
  "background": {
    "page": "app.html"
  },
  "options_page": "settings.html",
  "icons": {
    "16": "img/icon-16x16.png",
    "24": "img/icon-24x24.png",
    "32": "img/icon-32x32.png",
    "48": "img/icon-48x48.png",
    "96": "img/icon-96x96.png",
    "128": "img/icon-128x128.png"
  },
  "permissions": [
    "unlimited_storage",
    "tabs",
    "contextMenus",
    "http://www.myinstants.com/*",
    "http://www.instants.cl/*",
    "http://www.axonema.net/*",
    "https://*.github.com/*",
    "http://*.github.com/*"
  ],
  "browser_action": {
    "default_icon": "img/icon-48x48.png",
    "default_title": "Música Incidental",
    "default_popup": "sonidos.html"
  },
  "content_security_policy": "script-src 'self' chrome-extension-resource:; object-src 'self' "
}
