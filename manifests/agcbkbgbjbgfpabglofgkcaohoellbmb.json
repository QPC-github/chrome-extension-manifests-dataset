{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FluentCloud WebPhone",
  "description": "FluentCloud WebPhone is a revolution in business phone communications.",
  "version": "4.1.2",
  "manifest_version": 2,
  "offline_enabled": false,
  "short_name": "WebPhone",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "16": "images/icons/icon16.png",
    "36": "images/icons/icon36.png",
    "48": "images/icons/icon48.png",
    "57": "images/icons/icon57.png",
    "72": "images/icons/icon72.png",
    "76": "images/icons/icon76.png",
    "78": "images/icons/icon78.png",
    "96": "images/icons/icon96.png",
    "114": "images/icons/icon114.png",
    "120": "images/icons/icon120.png",
    "128": "images/icons/icon128.png",
    "144": "images/icons/icon144.png",
    "152": "images/icons/icon152.png",
    "180": "images/icons/icon180.png"
  },
  "permissions": [
    "browser",
    "storage",
    "identity",
    "contextMenus",
    "notifications",
    "videoCapture",
    "alwaysOnTopWindows",
    "audioCapture",
    "windows",
    {
      "fileSystem": [
        "write"
      ]
    },
    "*://*.fluentcloud.com/*",
    "https://*.google.com/*",
    "*://*.googleusercontent.com/*"
  ],
  "replacement_web_app": "https://fluent.stream/phone/?utm_source=chrome-app-upgrade",
  "oauth2": {
    "client_id": "1015527542630-sdo0g2efuh7iukol6q30d5psfsrg3qa6.apps.googleusercontent.com",
    "scopes": [
      "https://www.google.com/m8/feeds"
    ]
  }
}
