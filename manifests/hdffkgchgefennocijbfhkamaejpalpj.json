{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twurl",
  "description": "Twitter URL deshortener. No more t.co URLs, no redirections, only direct links. Faster loading. No tracking.",
  "version": "0.1.3",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "twurl.js"
      ]
    }
  ],
  "icons": {
    "16": "tw16.png",
    "48": "tw48.png",
    "128": "tw128.png"
  },
  "permissions": []
}
