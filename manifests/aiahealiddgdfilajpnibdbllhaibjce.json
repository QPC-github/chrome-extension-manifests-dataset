{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yellow Cal",
  "version": "0.9.3",
  "manifest_version": 2,
  "description": "Make plans with Yellow!",
  "icons": {
    "16": "/img/icon16x16.png",
    "48": "/img/icon48x48.png",
    "128": "/img/icon128x128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "/js/modules/jquery-2.1.0.min.js",
        "/js/modules/async-0.7.0.js",
        "/js/modules/underscore-1.6.0.min.js",
        "/js/modules/pusher-2.2.2.min.js",
        "/js/modules/chrono-0.1.9.min.js",
        "/js/modules/moment-with-langs-2.6.0.min.js",
        "/js/libs/client.js",
        "/js/libs/contacts.js",
        "/js/libs/db.js",
        "/js/libs/invites.js",
        "/js/libs/members.js",
        "/js/libs/objects.js",
        "/js/libs/responses.js",
        "/js/libs/states.js",
        "/js/libs/streams.js",
        "/js/libs/sync.js",
        "/js/libs/times.js",
        "/js/libs/view.js",
        "/js/background.js"
      ],
      "persistent": false
    }
  },
  "offline_enabled": true,
  "permissions": [
    "idle",
    "storage",
    "unlimitedStorage",
    "gcm",
    "notifications",
    "alarms",
    "contextMenus",
    "webview"
  ],
  "short_name": "Yellow"
}
