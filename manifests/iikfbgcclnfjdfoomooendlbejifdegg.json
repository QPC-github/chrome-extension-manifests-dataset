{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Snowstorm",
  "description": "A convenient tool for knowing the games created and published by Activision-Blizzard!",
  "version": "0.0.2",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "replace.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "128": "images/logo.png"
    }
  },
  "icons": {
    "128": "images/logo.png"
  },
  "manifest_version": 2
}
