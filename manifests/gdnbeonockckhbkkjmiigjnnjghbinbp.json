{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "퍼가여~ 알파",
  "description": "퍼가여~ 알파버전 입니다.",
  "version": "0.6.5",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.7.1.js"
      ]
    }
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
