{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SVU Plus",
  "description": "Everything you want from Syrian Virtual University",
  "version": "0.95.1",
  "version_name": "0.95.1 beta",
  "manifest_version": 2,
  "icons": {
    "128": "images/128.png",
    "64": "images/64.png",
    "48": "images/48.png",
    "32": "images/32.png",
    "24": "images/24.png"
  },
  "background": {
    "persistent": true,
    "page": "html/background.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+V"
      }
    }
  },
  "browser_action": {
    "default_icon": {
      "16": "images/not_logged_in.png"
    },
    "default_title": "SVU Plus | Powerful Extension Simply Everything At Once!"
  },
  "permissions": [
    "tabs",
    "alarms",
    "storage",
    "notifications",
    "background",
    "*://*.svuonline.org/*"
  ]
}
