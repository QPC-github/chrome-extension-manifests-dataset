{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ST图片扩展",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://76.164.232.70/*"
      ],
      "js": [
        "mypic.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "get this page's photo.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
