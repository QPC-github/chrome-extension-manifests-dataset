{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "P2K",
  "short_name": "P2K",
  "description": "P2K is a simple calculator dedicated to the CS:GO Trading community.",
  "version": "1.1.1",
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "browser_action": {
    "default_title": "P2K",
    "default_icon": "key.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "jquery-1.11.3.min.js"
    ]
  },
  "content_security_policy": ""
}
