{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "mamium.png",
    "default_popup": "popup.html",
    "default_title": "mamium.ru"
  },
  "content_scripts": [
    {
      "js": [
        "popup.js"
      ],
      "matches": [
        "http://mamium.ru/*"
      ]
    }
  ],
  "description": "Open in new tab site Mamium.RU",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "Mamium.ru",
  "version": "1.0"
}
