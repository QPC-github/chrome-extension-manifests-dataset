{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VidLog",
  "short_name": "VidLog",
  "description": "Get statistics and insights on any video's performance on YouTube and Facebook.",
  "version": "2.1.0.2",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "https://www.youtube.com/*",
      "https://www.facebook.com/*"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
        "css/vdobootstrap.min.css",
        "css/animate.css",
        "css/vidoolystyle.css"
      ],
      "run_at": "document_end",
      "matches": [
        "*://www.youtube.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/vdobootstrap.min.js",
        "js/highcharts.js",
        "js/moment.min.js",
        "js/helper.js",
        "js/video.js"
      ]
    },
    {
      "all_frames": false,
      "css": [],
      "run_at": "document_end",
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/vdobootstrap.min.js",
        "js/highcharts.js",
        "js/moment.min.js",
        "js/fbPopup.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/moment.min.js",
    "js/vdobootstrap.min.js",
    "js/jquery.min.js",
    "js/highcharts.js",
    "css/vdobootstrap.min.css",
    "css/animate.css",
    "css/vidoolystyle.css",
    "css/images/vid-mnu-icon.png",
    "css/images/search.png",
    "images/copy.png",
    "images/inst-icon.png",
    "images/bar.png",
    "images/fb-icon.png",
    "images/ico-fb.png",
    "images/ico-stats.png",
    "css/images/social-footer.png",
    "images/ico-yt.png",
    "images/yt-icon.png",
    "images/twt-icon.png",
    "images/down-arrow.png",
    "images/down-icon.png",
    "images/negative-sentiment.png",
    "images/positive-sentiment.png",
    "images/neutral-sentiment.png",
    "images/download.png",
    "images/feedback.png",
    "images/history.png",
    "images/home.png",
    "images/icon16.png",
    "images/icon48.png",
    "images/icon128.png",
    "images/icon-globe.png",
    "images/in-flag.png",
    "images/logout.png",
    "images/up-arrow.png",
    "images/up-icon.png",
    "images/video.png",
    "images/vidlog-logo.png",
    "images/vidlog-logo1.png",
    "images/vidooly-icon.png",
    "images/vidooly-icon1.png",
    "css/images/stats-icon.png",
    "css/images/vid-menu.png",
    "css/images/search.png",
    "images/vidooly-tag-icon.png",
    "images/views.png",
    "images/spiffygif_40x40.gif",
    "images/up-icon1.png",
    "images/down-icon1.png",
    "images/up-icon-2.png",
    "images/down-icon-2.png",
    "images/loader.gif",
    "images/*",
    "images/fb/*",
    "modules/*"
  ],
  "author": "Vidooly Media Pvt. Ltd.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/background.js",
      "js/notification.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon16.png"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "activeTab",
    "webNavigation",
    "storage",
    "proxy",
    "http://*/*",
    "webRequestBlocking",
    "unlimitedStorage",
    "http://app.vidooly.com/*",
    "*://www.youtube.com/*",
    "*://www.google.com/*",
    "*://api.vidooly.com/*",
    "*://creator.vidooly.com/*",
    "<all_urls>",
    "*://trends.google.com/*",
    "notifications"
  ],
  "incognito": "split"
}
