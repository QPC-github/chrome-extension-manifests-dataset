{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Praecantatio Churchill",
  "version": "1.1",
  "description": "Garbage collector for your browser history",
  "permissions": [
    "activeTab",
    "history"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "hosts"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/praecantatio16.png",
      "32": "images/praecantatio32.png",
      "48": "images/praecantatio48.png",
      "128": "images/praecantatio128.png",
      "945": "images/praecantatio945.png"
    }
  },
  "icons": {
    "16": "images/praecantatio16.png",
    "32": "images/praecantatio32.png",
    "48": "images/praecantatio48.png",
    "128": "images/praecantatio128.png",
    "945": "images/praecantatio945.png"
  },
  "manifest_version": 2
}
