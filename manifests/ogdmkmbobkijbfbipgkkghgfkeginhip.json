{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Github repository navigation",
  "author": "Starfolll",
  "short_name": "Repository navigation",
  "version_name": "tour",
  "description": "Extension for quick navigation on github users and their repository",
  "version": "1.0.1",
  "icons": {
    "256": "icon_256.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
