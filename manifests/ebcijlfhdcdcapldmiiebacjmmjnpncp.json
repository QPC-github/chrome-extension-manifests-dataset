{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SaveIt",
  "description": "Save Stuff",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "context.js"
    ]
  }
}
