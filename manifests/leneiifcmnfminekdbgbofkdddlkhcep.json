{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PSI In-application Extension",
  "version": "1.0.0.10",
  "manifest_version": 2,
  "description": "PSI In-application extension for Chrome",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.enlightks.com/*",
      "*://*.enlight.net/*",
      "*://*.psionline.com/*",
      "*://*.psiexams.com/*",
      "*://*.skillsbox.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
