{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Color Saver: Pick and save your colors",
  "version": "1.1",
  "description": "Convenient tool to pick and save your colors",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/picker128.png"
  },
  "icons": {
    "16": "images/picker128.png",
    "128": "images/picker128.png"
  },
  "manifest_version": 2
}
