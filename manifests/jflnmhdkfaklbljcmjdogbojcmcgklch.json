{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.2",
  "name": "Avengers Endgame Wallpapers Theme New Tab",
  "description": "Install Avengers Endgame Wallpapers and get high-quality images for a new tab",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/sc/vendor/JQuery.min.js",
      "/sc/background/polyfills/array.js",
      "/sc/background/configurate/engines.js",
      "/sc/background/configurate/base.js",
      "/sc/background/define.js",
      "/sc/background/tutorial/checkTour.js",
      "/sc/background/extender.js",
      "/sc/background/handlers/common.js",
      "/sc/background/handlers/setup.js",
      "/sc/background/handlers/analytics.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/static/icons/main/128.png"
    }
  },
  "icons": {
    "128": "/static/icons/main/128.png"
  },
  "permissions": [
    "*://mail.google.com/*",
    "activeTab",
    "storage",
    "cookies",
    "management",
    "topSites",
    "webNavigation",
    "webRequestBlocking",
    "webRequest"
  ],
  "web_accessible_resources": [
    "*"
  ],
  "manifest_version": 2
}
