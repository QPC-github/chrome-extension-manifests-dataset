{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IITM Netaccess One click Login",
  "short_name": "IITM Netaccess",
  "author": "Anurag Verma",
  "version": "5.0.2",
  "manifest_version": 2,
  "description": "Helps you to Login and Approve netaccess in one click!",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.js",
      "netaccess.js"
    ]
  },
  "options_page": "login1.html",
  "browser_action": {
    "default_title": "IITM Netaccess Login",
    "default_icon": "icon_16.png"
  },
  "permissions": [
    "https://netaccess.iitm.ac.in/*/*",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
