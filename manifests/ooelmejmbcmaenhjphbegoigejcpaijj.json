{
  "name": "买买买购物助手",
  "description": "购物省钱必备插件！",
  "version": "3.9.8",
  "manifest_version": 2,
  "homepage_url": "http://just998.com/apps",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "css/just998_background.css"
      ],
      "js": [
        "js/jquery.js",
        "js/content_script.js",
        "js/jquery.qrcode.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js",
      "js/notification.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/32.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "tabs"
  ]
}
