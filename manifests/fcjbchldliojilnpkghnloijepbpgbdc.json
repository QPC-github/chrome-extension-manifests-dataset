{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Diplex",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Click helper",
  "icons": {
    "200": "images/dp-mini.png"
  },
  "browser_action": {
    "default_icon": {
      "200": "images/dp-mini.png"
    },
    "default_title": "Diplex",
    "default_popup": "views/popup.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "options_page": "views/options.html",
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  }
}
