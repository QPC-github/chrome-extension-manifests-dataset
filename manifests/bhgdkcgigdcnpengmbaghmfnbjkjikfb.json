{
  "name": "English Chinese Dictionary",
  "version": "1.1",
  "description": "Search an English word on TigerNT.com.",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/ecdict.png",
    "default_title": "EC Dict"
  },
  "icons": {
    "16": "images/ecdict.png",
    "48": "images/ecdict48.png",
    "128": "images/ecdict128.png"
  },
  "permissions": [
    "http://www.tigernt.com/*",
    "tabs",
    "contextMenus",
    "notifications",
    "background"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
