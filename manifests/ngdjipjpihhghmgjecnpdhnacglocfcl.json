{
  "name": "Norfolky Blog",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Online High Tech Magazine",
  "homepage_url": "http://blog.norfolky.com/",
  "icons": {
    "16": "images/16.png",
    "38": "images/38.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "38": "images/38.png"
    },
    "default_title": "Online High Tech Magazine"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "notifications"
  ],
  "author": "http://blog.norfolky.com/",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
