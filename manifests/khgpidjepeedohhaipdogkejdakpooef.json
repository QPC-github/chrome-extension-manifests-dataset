{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/19.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "keyword": "Music",
      "is_default": true,
      "name": "Search Assistant",
      "favicon_url": "http://maxsearching.com/favicon.ico",
      "search_url": "http://maxsearching.com/srch/?keyword={searchTerms}"
    }
  },
  "description": "Your personal search assistant to find media content.",
  "icons": {
    "128": "icons/128.png",
    "19": "icons/19.png"
  },
  "manifest_version": 2,
  "name": "Search Assistant",
  "omnibox": {
    "keyword": "s"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "storage",
    "tabs",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "*://search.feedvertizus/*"
  ],
  "short_name": "Search Assistant",
  "version": "2.9.5"
}
