{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Code Speed Type",
  "description": "This extension converts code pieces into timed typing games",
  "version": "0.2",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/speedtype_bundle.js"
      ],
      "css": [
        "dist/styles.css"
      ]
    }
  ]
}
