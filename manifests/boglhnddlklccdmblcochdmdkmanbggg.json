{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tab numberer",
  "description": "This extension adds numbers to tabs.",
  "version": "1.0",
  "background": {
    "scripts": [
      "numbering.js"
    ]
  },
  "permissions": [
    "alarms",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
