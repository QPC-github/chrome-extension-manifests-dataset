{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Stos.me - Url Shortener",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Shorten an Url using Stos.me services.",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://stos.me/"
  ]
}
