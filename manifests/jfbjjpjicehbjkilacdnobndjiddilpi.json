{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "askSpoke Browser Extension",
  "description": "A new tab extension for askSpoke.",
  "version": "0.0.2",
  "icons": {
    "48": "./icon.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'unsafe-eval' https://*.askspoke.com https://cloud.typography.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "./new_tab.html"
  },
  "permissions": [
    "*://*.askspoke.com/*"
  ]
}
