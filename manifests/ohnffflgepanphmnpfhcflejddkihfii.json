{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FRESH! History",
  "short_name": "freshistory",
  "version": "0.1.4",
  "description": "freshlive.tvにおける履歴リスト管理ツールです。",
  "author": "suzujun",
  "icons": {
    "16": "dist/icons/icon16.png",
    "48": "dist/icons/icon48.png",
    "128": "dist/icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "dist/icons/icon38.png",
    "default_title": "FRESH! History",
    "default_popup": "dist/popup.html"
  },
  "permissions": [
    "history"
  ]
}
