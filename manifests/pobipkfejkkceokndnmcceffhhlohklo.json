{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Osfour Screensharing",
  "short_name": "Osfour screensharing",
  "description": "screensharing for Chrome",
  "version": "0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "icons": {
    "128": "osfour128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://osfour.com/*",
        "https://www.osfour.com/*"
      ]
    }
  ]
}
