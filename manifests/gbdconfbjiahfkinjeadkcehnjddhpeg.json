{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "GOP Codenames",
  "version": "1.0",
  "description": "Replaces the names of GOP Presidential candidates with their preffered codenames!",
  "icons": {
    "16": "codenamesmall.png",
    "48": "codenamemedium.png",
    "128": "codenamelarge.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "poppage.js"
      ],
      "run_at": "document_end"
    }
  ]
}
