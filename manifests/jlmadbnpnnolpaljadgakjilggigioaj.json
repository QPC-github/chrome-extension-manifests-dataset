{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "New tabs open immediately instead of in the background.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Tab Activate",
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.1.0"
}
