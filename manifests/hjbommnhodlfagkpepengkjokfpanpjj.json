{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cigar Price History and Free Fall Tracker",
  "description": "Historical pricing displayed right on the page for the product you are currently viewing - includes the best Free Fall price tracker",
  "version": "0.9.6",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "web_accessible_resources": [
    "gear.png"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "https://herf.io/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.cigarbid.com/*",
        "https://www.cigarbid.com/*",
        "http://www.cigarauctioneer.com/*",
        "https://www.cigarauctioneer.com/*",
        "http://www.cigarmonster.com/*",
        "https://www.cigarmonster.com/*"
      ],
      "js": [
        "cigarPriceHistory.js"
      ],
      "css": [
        "cigarPriceHistory.css"
      ]
    }
  ]
}
