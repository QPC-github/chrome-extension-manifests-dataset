{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tarboard",
  "description": "Tarboard | Organize web links- Share & Collaborate",
  "version": "1.0.2",
  "manifest_version": 2,
  "homepage_url": "http://tarboard.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Tarboard | Organize web links- Share & Collaborate",
    "default_popup": "html/browser_action.html"
  },
  "oauth2": {
    "client_id": "696803949515-q7jtv02qr04733qui5j0v1dfqilobmd9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.login",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "activeTab",
    "contextMenus",
    "cookies",
    "bookmarks",
    "http://tarboard.com/*",
    "https://www.facebook.com/dialog/oauth/*",
    "https://accounts.google.com/o/oauth2/auth/*",
    "https://login.live.com/oauth20_authorize.srf/*"
  ]
}
