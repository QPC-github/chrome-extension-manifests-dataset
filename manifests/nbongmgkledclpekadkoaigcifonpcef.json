{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Reddactit",
  "version": "1.4.2",
  "description": "Username redactor for reddit with color coding for easy reading",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "templogo.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "*://*.reddit.com/*"
  ],
  "manifest_version": 2
}
