{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kids These Days",
  "version": "1.1",
  "description": "The millennial think piece translator.",
  "icons": {
    "16": "images/kidsthesedays16.png",
    "64": "images/kidsthesedays64.png",
    "128": "images/kidsthesedays128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
