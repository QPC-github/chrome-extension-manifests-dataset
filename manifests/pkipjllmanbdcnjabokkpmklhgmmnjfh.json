{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "China Digital Times Chrome Extension",
  "version": "0.1.3",
  "description": "Launching China Digital Times using Google Chrome extension",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "China Digital Times",
    "default_icon": "icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}
