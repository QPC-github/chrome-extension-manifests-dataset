{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "manager.js"
    ]
  },
  "default_locale": "en",
  "description": "Chrome Download Manager",
  "icons": {
    "128": "data/icon128.png",
    "16": "data/icon16.png",
    "32": "data/icon32.png",
    "48": "data/icon48.png"
  },
  "manifest_version": 2,
  "name": "Download Manager",
  "options_page": "options.html",
  "permissions": [
    "downloads"
  ],
  "short_name": "DManager",
  "version": "1.0.0"
}
