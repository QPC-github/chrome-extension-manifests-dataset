{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TankiOnline Info",
  "version": "1.2",
  "description": "TankiOnline",
  "permissions": [
    "http://ratings.tankionline.com/*",
    "activeTab"
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_title": "TankiOnline Info",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
