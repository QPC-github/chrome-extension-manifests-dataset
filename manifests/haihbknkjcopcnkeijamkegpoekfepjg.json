{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Memento Translate",
  "description": "Helps you memorize new words",
  "version": "0.0.0.2",
  "permissions": [
    "management",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "icons": {
    "16": "memento-dict-16.png",
    "48": "memento-dict-48.png",
    "128": "memento-dict-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
