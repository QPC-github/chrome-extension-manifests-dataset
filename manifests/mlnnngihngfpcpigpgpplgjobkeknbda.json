{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "46",
  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "default_locale": "ru",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "__MSG_name__"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "is_default": true,
      "encoding": "UTF-8",
      "name": "__MSG_rambler__",
      "search_url": "https://nova.rambler.ru/search?query={searchTerms}&utm_source=browser&utm_medium=extension&utm_campaign=self_promo&utm_content=search&utm_term=search_chrome_ext",
      "suggest_url": "https://nova.rambler.ru/suggest?v=3&query={searchTerms}&utm_source=browser&utm_medium=extension&utm_campaign=self_promo&utm_content=search&utm_term=search_chrome_ext",
      "keyword": "r",
      "favicon_url": "https://nova.rambler.ru/favicon.ico"
    }
  }
}
