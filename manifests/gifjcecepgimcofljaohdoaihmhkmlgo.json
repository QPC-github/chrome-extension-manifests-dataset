{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Nyx: new at the board",
  "version": "1.0.0",
  "description": "Shows how many new entries are at the current board.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.nyx.cz/index.php?l=topic*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
