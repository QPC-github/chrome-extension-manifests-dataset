{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No More Registry Error Camsys",
  "description": "This extension fixes the registry error on Camsys",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "*://cms.mmu.edu.my/",
    "webRequestBlocking"
  ]
}
