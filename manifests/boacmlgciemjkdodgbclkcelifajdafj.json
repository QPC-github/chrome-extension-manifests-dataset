{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FBCLID Remover",
  "description": "Removes FBCLID parameters at the source in Messenger.",
  "version": "1",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "https://www.messenger.com/*",
        "https://l.facebook.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
