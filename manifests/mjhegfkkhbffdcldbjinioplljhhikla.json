{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "rettiwt",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An alternate interface for Twitter you will never get from Twitter",
  "web_accessible_resources": [
    "theme.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/"
      ],
      "js": [
        "jquery.min.js",
        "headroom.min.js",
        "jQuery.headroom.js",
        "contentscript.js"
      ],
      "css": [
        "main.css",
        "theme.css"
      ]
    }
  ]
}
