{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PriceMole Price Tracker",
  "description": "PriceMole Price Tracker web extension",
  "version": "0.1.1",
  "manifest_version": 2,
  "icons": {
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "PriceMole Price Tracker",
    "default_icon": "icons/icon_128.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "*://*/",
    "notifications",
    "alarms",
    "unlimitedStorage",
    "activeTab"
  ]
}
