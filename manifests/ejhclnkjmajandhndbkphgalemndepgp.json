{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TUBETUBE",
  "short_name": "TUBETUBE",
  "description": "Put Youtube comments on the video. Youtubeでコメントを流す",
  "options_page": "options.html",
  "icons": {
    "16": "images/tubetube.png",
    "48": "images/tubetube.png",
    "128": "images/tubetube.png"
  },
  "version": "0.1.3.8",
  "permissions": [
    "https://www.youtube.com/*",
    "https://dws2.voicetext.jp/*",
    "storage",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "fonts/*.ttf",
    "script/*",
    "fonts/*.otf"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "TUBETUBE"
  },
  "manifest_version": 2
}
