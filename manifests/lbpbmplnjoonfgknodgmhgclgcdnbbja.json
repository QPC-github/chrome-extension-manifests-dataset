{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Keeper",
  "description": "This extension helps to quickly save open tabs and return to them later.",
  "permissions": [
    "tabs"
  ],
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "popup.png"
  }
}
