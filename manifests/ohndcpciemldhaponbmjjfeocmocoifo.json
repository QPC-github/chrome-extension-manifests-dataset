{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hausmittel Hexe",
  "description": "Die neuesten Naturheilmitte, Hausmittel und Gesundheitstipps von HausmittelHexe.com per RSS-Feed",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup-page.html",
    "default_title": "HausmittelHexe Updates"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com;object-src 'self'"
}
