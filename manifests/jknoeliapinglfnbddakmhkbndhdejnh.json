{
  "name": "Peaky Blinders HD Wallpapers New Tab",
  "manifest_version": 2,
  "short_name": "Blinders",
  "description": "Peaky Blinders HD Wallpapers and New Tab Themes for Your Browser!",
  "version": "2.0",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "detail/index.html"
  },
  "browser_action": {
    "default_title": "Peaky Blinders HD Wallpapers New Tab",
    "default_icon": {
      "16": "detail/icon16.png",
      "48": "detail/icon48.png",
      "128": "detail/icon128.png"
    }
  },
  "offline_enabled": true,
  "incognito": "split",
  "permissions": [
    "unlimitedStorage",
    "storage",
    "contextMenus",
    "management"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png"
  }
}
