{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Webgraffiti",
  "version": "1.0.2.1",
  "manifest_version": 2,
  "description": "Webgraffiti - comment on any web page, anywhere, anonymously.",
  "browser_action": {
    "default_icon": "speech_bubble.png",
    "default_popup": "chrome_popup.html"
  },
  "web_accessible_resources": [
    "get_comments.js"
  ],
  "permissions": [
    "http://webyawp.com/*",
    "tabs"
  ]
}
