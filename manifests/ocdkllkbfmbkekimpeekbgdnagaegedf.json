{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_title": "markdown"
  },
  "content_scripts": [
    {
      "js": [
        "js/highlight.pack.js",
        "js/markdown.js"
      ],
      "css": [
        "js/github.css",
        "js/github-markdown.css"
      ],
      "matches": [
        "http://doc.hz.netease.com/*"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "description": "markdown",
  "manifest_version": 2,
  "name": "markdown",
  "short_name": "markdown",
  "permissions": [
    "https://*/*"
  ],
  "version": "1.3"
}
