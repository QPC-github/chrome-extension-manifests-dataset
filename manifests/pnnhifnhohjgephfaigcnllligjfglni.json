{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vbox",
  "description": "Vbox keeps record of ordered lunch box.",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "38": "Taco.png"
    },
    "default_popup": "popup.html",
    "default_title": "Vbox - Lunch Count"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
