{
  "name": "mSpy",
  "description": "mSpy works by installing a small hidden app to the device to be monitored.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "mSpy",
    "default_popup": "mspy.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "0.1"
}
