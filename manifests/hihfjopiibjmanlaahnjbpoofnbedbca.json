{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Private Bookmark",
  "description": "Private Bookmark用 chrome拡張機能.気になったWebページでクリック！",
  "version": "1.0",
  "manifest_version": 2,
  "author": "yumet",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Private Bookmark"
  },
  "options_page": "option.html",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  }
}
