{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Contextless Redditing",
  "version": "1.0.0",
  "description": "Add a bit of surprise to your redditing experience!",
  "manifest_version": 2,
  "icons": {
    "16": "contextlessRedditing-16.png",
    "48": "contextlessRedditing-48.png",
    "128": "contextlessRedditing-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*"
      ],
      "all_frames": true,
      "js": [
        "contextlessRedditing.user.js"
      ]
    }
  ]
}
