{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "IntoDNS Chrome",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Easy way to check the current site in IntoDNS",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}
