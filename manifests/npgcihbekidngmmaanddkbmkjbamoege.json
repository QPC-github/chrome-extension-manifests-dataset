{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Vpnrating Blog",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This extension Provides the best information and tips on Many Types of VPN Provider.",
  "background": {
    "page": "background.html"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "*://*/*"
  ]
}
