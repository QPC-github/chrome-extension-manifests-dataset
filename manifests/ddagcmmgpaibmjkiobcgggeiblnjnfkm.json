{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clear Downloads",
  "short_name": "dicldds",
  "description": "This extension clears and hides the download bar on download completion",
  "version": "0.2.1",
  "author": "dicode VOF",
  "permissions": [
    "downloads",
    "browsingData"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
