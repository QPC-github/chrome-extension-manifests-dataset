{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IP Extender",
  "version": "1.3",
  "description": "Replaces the text 'IP' with 'immense penis' because that's way better.",
  "icons": {
    "16": "iplogo16.png",
    "48": "iplogo48.png",
    "128": "iplogo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
