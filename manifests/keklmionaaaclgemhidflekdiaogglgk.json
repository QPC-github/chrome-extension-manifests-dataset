{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Check this Web page on WebArchive.",
  "manifest_version": 2,
  "name": "WebArchive",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "32": "icons/parthenon.png",
    "64": "icons/parthenon64.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/parthenon64.png"
    }
  }
}
