{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ByteScout Time Tracker",
  "short_name": "ByteScout Time Tracker",
  "description": "Free time tracking extension that helps you to track billable hours, edit records, export to CSV",
  "version": "1.9.0",
  "author": "ByteScout",
  "icons": {
    "128": "time-tracker-icon-128px.png"
  },
  "browser_action": {
    "default_icon": "time-tracker-icon-128px.png",
    "default_title": "ByteScout Time Tracker",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
