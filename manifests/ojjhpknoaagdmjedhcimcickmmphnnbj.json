{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "description": "A new elegant way to list favorite bookmarks in new tab page. Formerly known as \"btab\"",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2,
  "name": "Basit New Tab",
  "permissions": [
    "bookmarks",
    "chrome://favicon/*",
    "storage"
  ],
  "version": "0.4.0"
}
