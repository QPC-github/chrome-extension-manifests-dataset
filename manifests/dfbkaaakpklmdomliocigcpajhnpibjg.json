{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Saman's Launcher",
  "description": "Quick launch Saman's Socialmedia sites",
  "version": "1.0.0",
  "icons": {
    "128": "ghost_128.png"
  },
  "browser_action": {
    "default_icon": "ghost.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
