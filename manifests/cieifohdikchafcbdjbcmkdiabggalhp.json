{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Simple Form Recovery",
  "description": "Never lose anything you type into a web form again!",
  "version": "0.3.1",
  "minimum_chrome_version": "2",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-1.8.2.min.js",
        "garlic-0.0.2.min.js",
        "app.js"
      ]
    }
  ],
  "manifest_version": 2
}
