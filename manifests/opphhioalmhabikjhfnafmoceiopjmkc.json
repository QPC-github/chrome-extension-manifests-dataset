{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quotips",
  "version": "1.0.0",
  "description": "See motivational quotes when you open a new tab. You can edit your own quote collection.",
  "icons": {
    "48": "dist/img/logo48.png"
  },
  "default_locale": "en",
  "homepage_url": "https://quotips.com",
  "chrome_url_overrides": {
    "newtab": "tab/newtab.html"
  },
  "permissions": [
    "storage"
  ]
}
