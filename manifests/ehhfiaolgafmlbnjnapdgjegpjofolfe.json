{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_application_title__",
  "manifest_version": 2,
  "default_locale": "en_US",
  "version": "1.0.0.9",
  "offline_enabled": true,
  "container": [
    "GOOGLE_DRIVE"
  ],
  "description": "__MSG_application_description__",
  "permissions": [
    "storage",
    "clipboardWrite",
    "https://*.emojis-keyboard.com/*"
  ],
  "browser_action": {
    "default_popup": "Default.html"
  },
  "icons": {
    "16": "Content/Images/16.png",
    "24": "Content/Images/24.png",
    "32": "Content/Images/32.png",
    "64": "Content/Images/64.png",
    "128": "Content/Images/128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "homepage_url": "https://emojis-keyboard.com",
  "background": {
    "scripts": [
      "/notify/update-conf.js",
      "/notify/update.min.js"
    ]
  }
}
