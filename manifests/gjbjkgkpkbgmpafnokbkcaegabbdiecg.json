{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vision Bored",
  "description": "Beautifully designed positive affirmations",
  "version": "2.7",
  "author": "Sarah Joy Hood",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Vision Bored"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"
}
