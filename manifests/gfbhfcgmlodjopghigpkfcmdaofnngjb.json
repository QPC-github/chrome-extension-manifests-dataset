{
  "content_scripts": [
    {
      "js": [
        "src/inject/inject.js"
      ],
      "matches": [
        "https://vk.com/*",
        "http://vk.com/*",
        "https://new.vk.com/*",
        "http://new.vk.com/*"
      ]
    }
  ],
  "default_locale": "en",
  "description": "Like VK.com photos with space button",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "VK space liker",
  "permissions": [
    "https://vk.com/*",
    "http://vk.com/*",
    "https://new.vk.com/*",
    "http://new.vk.com/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.9.6"
}
