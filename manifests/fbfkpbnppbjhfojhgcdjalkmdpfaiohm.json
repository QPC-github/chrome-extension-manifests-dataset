{
  "name": "Amazon Search",
  "description": "Search The Worlds Best Shopping Site - Find Every Thing You Want And More At Amazon Search App",
  "browser_action": {
    "default_icon": {
      "16": "amazon16.png"
    },
    "icons": {
      "16": "amazon16.png",
      "32": "amazon32.png",
      "48": "amazon48.png",
      "128": "amazon128.png"
