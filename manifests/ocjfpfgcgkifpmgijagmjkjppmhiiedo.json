{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Borimed",
  "description": "Borimed",
  "version": "1.1",
  "icons": {
    "732": "image/mob-logo.png"
  },
  "background": {
    "scripts": [
      "js/main.js"
    ]
  },
  "browser_action": {
    "default_icon": "image/mob-logo.png",
    "default_popup": "popUp.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
