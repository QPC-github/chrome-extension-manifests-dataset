{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ProBug",
  "description": "Quickly report bugs for ProTrainings",
  "version": "1.1",
  "browser_action": {
    "default_icon": "bug.png",
    "default_popup": "popup.html",
    "default_title": "ProBug"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
