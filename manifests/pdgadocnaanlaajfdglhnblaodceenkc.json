{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Secure My Cookies",
  "version": "1.0",
  "description": "Secure your cookies!",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
