{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Listen Trackers",
  "version": "2.3.2",
  "manifest_version": 2,
  "description": "Кнопка быстрой подписки. Сервис Listen Trackers (http://litr.cc) следит за обновлениями на большинстве RU и UA торрент трекерах.",
  "icons": {
    "16": "resousce/img/16.png",
    "48": "resousce/img/64.png",
    "128": "resousce/img/128.png"
  },
  "permissions": [
    "http://litr.cc/*",
    "https://litr.cc/*",
    "tabs",
    "contextMenus"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Подписаться на раздачу",
    "default_icon": "resousce/img/64.png"
  },
  "options_page": "options.html"
}
