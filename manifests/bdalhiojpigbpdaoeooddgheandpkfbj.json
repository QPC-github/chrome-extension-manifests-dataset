{
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.htm"
  },
  "description": "Motore di ricerca Amazon sempre a portata di mano, con suggerimento di keywords durante la digitazione ed altro ancora.",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  },
  "manifest_version": 2,
  "name": "Amazon Search Suggestions",
  "permissions": [
    "http://*.amazon.ca/*",
    "http://*.amazon.cn/*",
    "http://*.amazon.co.uk/*",
    "http://*.amazon.com/*",
    "http://*.amazon.com.br/*",
    "http://*.amazon.de/*",
    "http://*.amazon.es/*",
    "http://*.amazon.fr/*",
    "http://*.amazon.in/*",
    "http://*.amazon.it/*",
    "http://*.amazon.co.jp/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.25"
}
