{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "STRAVA AUTO KUDOS",
  "description": "Give KUDOS on each visit of Strava / Dev : Mathieu Benhalima",
  "version": "1.1",
  "browser_action": {
    "default_icon": "strava.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-2.2.3.min.js",
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
