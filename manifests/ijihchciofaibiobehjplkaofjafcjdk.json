{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "description": "This is a slightly modified version of the Meetecho extension utility for screensharing support in the Janus WebRTC gateway",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Janus WebRTC Screensharing",
  "permissions": [
    "desktopCapture"
  ],
  "short_name": "Janus screensharing",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.5"
}
