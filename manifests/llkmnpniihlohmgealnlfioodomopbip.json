{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "externally_connectable": {
    "matches": [
      "*://*.fenjianli.com/*",
      "*://*.liepin.com/*",
      "*://*.liepin.cn/*",
      "*://*.linkedin.com/*",
      "*://*.jianlika.com/*",
      "*://*.zhaopin.com/*",
      "*://weibo.com/*",
      "*://*.zhihu.com/*",
      "*://*.highpin.cn/*",
      "*://stackoverflow.com/*",
      "*://github.com/*",
      "*://*.jianshu.com/*",
      "*://maimai.cn/*",
      "*://*.lagou.com/*",
      "*://*.zhipin.com/*",
      "*://*.01hr.com/*",
      "*://*.72zhao.com/*",
      "*://*.lieluobo.com/*"
    ],
    "accepts_tls_channel_id": false
  },
  "version": "4.4.10",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "content_security_policy": "script-src 'self'  https://*.bomayu.com  ; object-src 'self'  https://*.bomayu.com; img-src: 'self' https://*.bomayu.com",
  "icons": {
    "16": "images/icon-bmy-16.png",
    "128": "images/icon-bmy-128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-bmy-16.png",
      "24": "images/icon-bmy-24.png",
      "32": "images/icon-bmy-32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://h.liepin.com/*",
    "https://h.liepin.cn/*",
    "http://*/*",
    "https://*/*",
    "background",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "cookies",
    "*://*.google.com/",
    "debugger",
    "history",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "topSites",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "activeTab"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "*://*.bomayu.com/"
      ],
      "js": [
        "scripts/lib/jquery-1.11.1.min.js",
        "scripts/lib/md5.js",
        "scripts/ct/sider.js",
        "scripts/ct/init.js"
      ],
      "css": [],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "omnibox": {
    "keyword": "OMNIBOX-KEYWORD"
  },
  "web_accessible_resources": [
    "images/icon-48.png",
    "views/*",
    "scripts/*"
  ]
}
