{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Legal Disclosure Blocker",
  "version": "1.1",
  "description": "Block all the legal disclosures!",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/imprint*",
        "https://*/*/imprint*",
        "https://*/impressum*",
        "https://*/*/impressum*"
      ]
    }
  ],
  "manifest_version": 2
}
