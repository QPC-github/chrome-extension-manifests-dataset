{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quotes Book",
  "manifest_version": 2,
  "description": "Collect and share your favourite quotes and quotations.",
  "version": "1.4",
  "app": {
    "urls": [
      "http://quotbook.com/"
    ],
    "browse_urls": [
      "http://quotbook.com/"
    ],
    "launch": {
      "web_url": "http://quotbook.com/"
    }
  },
  "icons": {
    "16": "16.png",
    "24": "24.png",
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "notifications"
  ]
}
