{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Capnis Live Chat",
  "description": "Monitor chat requests from Capnis Live Chat without opening a chat",
  "version": "1.2",
  "icons": {
    "48": "img/favicon.png",
    "16": "img/favicon.png",
    "128": "img/favicon.png"
  },
  "permissions": [
    "idle"
  ],
  "short_name": "A product of capnis infotech private limited.",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  }
}
