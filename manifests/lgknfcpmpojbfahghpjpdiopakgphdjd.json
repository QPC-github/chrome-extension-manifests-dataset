{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "0.94",
  "description": "__MSG_description__",
  "permissions": [
    "<all_urls>",
    "tts",
    "tabs",
    "contextMenus"
