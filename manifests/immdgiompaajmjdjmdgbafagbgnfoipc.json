{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "version": "3.4",
  "manifest_version": 2,
  "default_locale": "es",
  "description": "__MSG_description__",
  "icons": {
    "48": "images/amssp.png",
    "64": "images/Warning.png"
  },
  "permissions": [
    "http://*/*",
    "tabs",
    "https://*/*",
    "https://www.amsspecialist.com/*",
    "notifications",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com/; object-src 'self'",
  "background": {
    "scripts": [
      "js/back.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_name__",
    "default_icon": "images/amssp.png"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "images/amssp.png",
    "images/Warning.png"
  ]
}
