{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Gmail Shortcut",
  "description": "Access Gmail on Chrome within a snap",
  "version": "1.0",
  "background": {
    "page": "bg.html"
  },
  "permissions": [
    "https://mail.google.com/"
  ],
  "browser_action": {
    "default_icon": "icon_32x32.png"
  },
  "icons": {
    "16": "icon_16x16.png",
    "32": "icon_32x32.png",
    "128": "icon_128x128.png"
  }
}
