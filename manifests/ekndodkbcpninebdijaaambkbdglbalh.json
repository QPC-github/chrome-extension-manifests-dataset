{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "app": {
    "background": {
      "pages": [
        "html/embed.html"
      ],
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "description": "A native Memrise standalone app for your Chrome Browser.",
  "icons": {
    "128": "img/icon.png"
  },
  "manifest_version": 2,
  "name": "Memrise",
  "permissions": [
    "webview",
    "audioCapture",
    "videoCapture",
    "http://*/",
    "https://*/"
  ],
  "version": "1.0.5"
}
