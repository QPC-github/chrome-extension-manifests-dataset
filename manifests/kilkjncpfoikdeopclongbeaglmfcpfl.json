{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "KTH Canvas Fix",
  "description": "This extension fixes various flaws on KTH's Canvas implementation.",
  "version": "1.0.0",
  "permissions": [
    "https://kth.instructure.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://kth.instructure.com/*"
      ],
      "css": [
        "canvas.css"
      ],
      "js": []
    }
  ]
}
