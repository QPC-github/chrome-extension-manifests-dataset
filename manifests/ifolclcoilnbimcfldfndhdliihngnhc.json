{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/Qiosk-16.png",
    "128": "images/Qiosk-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/Qiosk-16.png",
      "38": "images/Qiosk-38.png"
    },
    "default_title": "Qiosk",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs"
  ],
  "web_accessible_resources": [
    "icons/*.png",
    "src/*.html",
    "index.html"
  ]
}
