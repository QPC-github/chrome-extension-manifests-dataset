{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Cybersafety Help Button (by Google)",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Go to Cybersafety Help and Advice page",
  "icons": {
    "16": "cybersafetybutton-web-16x16.png",
    "32": "cybersafetybutton-web-32x32.png",
    "48": "cybersafetybutton-web-48x48.png",
    "128": "cybersafetybutton-web-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "cybersafetybutton-web-32x32.png",
    "default_title": "Go to Cybersafety Help and Advice page"
  },
  "permissions": [
    "tabs"
  ]
}
