{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DailyFailBlock",
  "version": "1.0",
  "description": "redirect any request to a Daily Fail URL",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.dailymail.co.uk/*",
    "*://*.express.co.uk/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js",
      "parseUri.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
