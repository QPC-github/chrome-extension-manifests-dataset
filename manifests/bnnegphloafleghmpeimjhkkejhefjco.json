{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RedCritter Feed",
  "description": "RedCritter Notifications for Chrome.",
  "manifest_version": 2,
  "version": "2.0",
  "icons": {
    "128": "assets/redcritterlogo128x128.png"
  },
  "background": {
    "scripts": [
      "jquery-1.11.1.min.js",
      "jquery.signalR-2.0.0.min.js",
      "redcritterhub.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/RCTile19px.png",
      "38": "assets/RCTile38px.png"
    },
    "default_title": "RedCritter Feed",
    "default_popup": "settings.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "notifications"
  ]
}
