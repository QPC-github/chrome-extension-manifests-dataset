{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Page Color",
  "version": "1.0",
  "description": "A simple extension to change webpage color.",
  "author": "NiMaker LLC",
  "icons": {
    "128": "px128.png",
    "48": "px48.png",
    "16": "px16.png"
  },
  "browser_action": {
    "default_icon": "px16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
