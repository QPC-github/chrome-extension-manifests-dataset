{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "딥링크 업로더",
  "description": "현재 페이지의 주소와 제목을 이용해 딥링크에 새 링크를 올립니다.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "딥링크에 올리기"
  },
  "manifest_version": 2
}
