{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wget GUI Light",
  "manifest_version": 2,
  "description": "Add to context menu for links, images and media option for sending url to \"Wget GUI Light\". Check \"Settings\" before use!",
  "version": "0.0.3",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "img/icon.png"
  ]
}
