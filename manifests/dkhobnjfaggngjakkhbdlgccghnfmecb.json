{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/script.js",
        "js/popup.js",
        "js/options.js",
        "js/jquery.jqplot.min.js",
        "js/jqplot.highlighter.min.js",
        "js/jqplot.cursor.min.js",
        "js/jqplot.dateAxisRenderer.min.js",
        "fancybox/jquery.fancybox.js",
        "fancybox/helpers/jquery.fancybox-media.js"
      ],
      "matches": [
        "*://*.aliexpress.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "imgs/loader.gif",
    "imgs/vk.png",
    "imgs/ok.png",
    "imgs/fb.png",
    "imgs/tw.png",
    "imgs/mr.png",
    "imgs/youtube16x16.png",
    "imgs/itao16x16.png",
    "imgs/alitrack16x16.png",
    "imgs/options.png",
    "favicon.png",
    "css/font-awesome.css",
    "css/style.css",
    "css/jquery.jqplot.min.css",
    "fonts/fontawesome-webfont.eot",
    "fonts/fontawesome-webfont.svg",
    "fonts/FontAwesome.otf",
    "fonts/fontawesome-webfont.woff",
    "fonts/fontawesome-webfont.woff2",
    "fancybox/jquery.fancybox.css",
    "fancybox/fancybox_sprite.png",
    "fancybox/fancybox_sprite@2x.png",
    "fancybox/fancybox_overlay.png",
    "fancybox/blank.gif",
    "fancybox/fancybox_loading.gif",
    "fancybox/fancybox_loading@2x.gif",
    "fancybox/helper/fancybox_buttons.png"
  ],
  "permissions": [
    "contextMenus",
    "cookies",
    "storage",
    "unlimitedStorage",
    "tabs",
    "*://*.aliexpress.com/*",
    "*://*.alitrack.ru/*"
  ],
  "icons": {
    "16": "favicon.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "name": "AliTrack.ru",
  "description": "Безопасные покупки на AliExpress.com, отслеживание посылок и многое другое!",
  "options_page": "options.html",
  "version": "3.9.4"
}
