{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SinBlock Content Filter",
  "version": "0.1.0",
  "description": "This extension will help you by screening and blocking sinful content",
  "icons": {
    "48": "icon48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "webRequestBlocking",
    "*://*/*"
  ]
}
