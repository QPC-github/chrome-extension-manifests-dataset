{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Valentine's Search Utility",
  "description": "Search utility for job boards and Linkedin profiles",
  "omnibox": {
    "keyword": "li"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.1.0",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus"
  ]
}
