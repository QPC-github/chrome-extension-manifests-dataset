{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "zale - udsalg på tøj",
  "description": "Få de bedste tilbud på tøj fra 10 webshops hver gang du åbner en ny fane.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "https://zale.dk/"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.zale.dk/*"
    ]
  },
  "browser_action": {
    "onClick": ""
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "zale.html"
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}
