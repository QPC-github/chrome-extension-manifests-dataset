{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No Youtube Ads",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Open current YouTube link in YouTubeSkip for no ads",
  "background": {
    "scripts": [
      "scripts.js"
    ]
  },
  "icons": {
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "http://youtube.com/watch*"
  ]
}
