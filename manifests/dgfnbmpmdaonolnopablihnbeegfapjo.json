{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Bronydell",
  "background": {
    "scripts": [
      "require.js",
      "jquery.js",
      "vib.js",
      "fetcher.js",
      "stats.js",
      "main.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "plist.html",
    "default_title": "Pnoty"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Оповещает пользователя о новых постах паблика игры для слабых ПК",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Игры для слабых ПК",
  "options_page": "plist.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "version": "2.0"
}
