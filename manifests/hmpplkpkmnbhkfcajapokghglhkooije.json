{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Monolegal Actuaciones judiciales al instante",
  "short_name": "Monolegal",
  "version": "1.12",
  "description": "Registra desde la web de la Rama Judicial de Colombia los procesos que quieras que revisemos DIA a DIA y evita que venzan t√©rminos!",
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "*://*.monolegal.co/*",
    "*://procesos.ramajudicial.gov.co/procesoscs/*",
    "http://cdn.mxpnl.com/",
    "http://www.mixpanel.com/",
    "http://api.mixpanel.com/",
    "*://*.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.monolegal.co/*",
        "*://procesos.ramajudicial.gov.co/procesoscs/*"
      ],
      "js": [
        "page.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Registra el proceso en Monolegal, tu dependiente judicial virtual",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
