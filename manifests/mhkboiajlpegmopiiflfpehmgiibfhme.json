{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NAM.SH",
  "description": "Create your shortened video playlist easily.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "NAM.SH"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
