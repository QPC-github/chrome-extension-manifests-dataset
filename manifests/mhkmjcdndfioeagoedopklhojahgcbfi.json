{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_chrome_extension_name__",
  "description": "__MSG_chrome_extension_description__",
  "offline_enabled": true,
  "version": "1.1.0",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "default_locale": "en"
}
