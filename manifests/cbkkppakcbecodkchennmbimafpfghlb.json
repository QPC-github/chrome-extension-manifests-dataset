{
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "iconx48.png"
  },
  "options_page": "options.html",
  "description": "Online Stream Notification",
  "manifest_version": 2,
  "name": "Stream Notification",
  "permissions": [
    "tabs",
    "notifications",
    "storage",
    "http://streams.sdesigner.de/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.4"
}
