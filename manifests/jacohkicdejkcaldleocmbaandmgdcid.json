{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Blinds",
  "version": "0.1.0",
  "description": "Hide the video recommendations on home and video playing pages of YouTube.",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
