{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yay.com Click to Dial",
  "version": "1.1.4",
  "description": "The official Yay.com Click To Dial Extension",
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Yay.com",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
