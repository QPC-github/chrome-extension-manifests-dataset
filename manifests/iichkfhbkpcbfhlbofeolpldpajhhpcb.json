{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_AppName__",
  "version": "1.0.1",
  "description": "__MSG_AppDesc__",
  "default_locale": "en",
  "icons": {
    "128": "logo-128.png",
    "48": "logo-48.png",
    "16": "logo-16.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "logo-16.png",
      "32": "logo-32.png"
    }
  },
  "background": {
    "scripts": [
      "/js/common-push-init.js",
      "/js/common-push.min.js",
      "/js/background.js"
    ]
  },
  "permissions": [
    "https://*.freegamesky.com/*",
    "notifications"
  ],
  "offline_enabled": true
}
