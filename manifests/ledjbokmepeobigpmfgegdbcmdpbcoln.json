{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mirror",
  "description": "Your makeup Mirror is here!",
  "version": "0.1.0",
  "version_name": "initial",
  "offline_enabled": true,
  "icons": {
    "16": "assets/badges/icon-16x16.png",
    "48": "assets/badges/icon-48x48.png",
    "128": "assets/badges/icon-128x128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/badges/icon-16x16.png",
      "48": "assets/badges/icon-48x48.png",
      "128": "assets/badges/icon-128x128.png"
    },
    "default_title": "Mirror",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+A",
        "mac": "Alt+Shift+A",
        "chromeos": "Alt+Shift+A",
        "linux": "Alt+Shift+A"
      }
    }
  },
  "web_accessible_resources": [
    "assets/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://www.googletagmanager.com https://www.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "videoCapture"
  ],
  "key": ""
}
