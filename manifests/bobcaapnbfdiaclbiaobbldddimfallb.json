{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Breathe",
  "description": "Take a little time for a deep breathing exercise, helping you calm down, realax, and focus.",
  "version": "1.1",
  "author": "Lucas Haas",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Breathe"
  },
  "chrome_url_overrides": {
    "newtab": "breathe.html"
  },
  "permissions": [
    "activeTab"
  ]
}
