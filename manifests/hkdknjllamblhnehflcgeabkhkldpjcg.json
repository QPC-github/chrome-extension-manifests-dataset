{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Codeforces_Green_Dot",
  "short_name": "CFGreenDot",
  "description": "Shows recent codeforces platform actions and check your friends list status and submissions.",
  "version": "2.0.0.2",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "80": "icons/icon80.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "content_scripts": [
      "background.js"
    ]
  }
}
