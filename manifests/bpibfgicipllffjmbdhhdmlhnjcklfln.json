{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Cat Convert",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
