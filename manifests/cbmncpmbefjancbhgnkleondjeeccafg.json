{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dashub",
  "description": "Dashub is an online archive tool allowing to save the information when you see and to retrieve it when your need it.",
  "version": "2.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "logo_128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.dashub.info/",
    "storage"
  ]
}
