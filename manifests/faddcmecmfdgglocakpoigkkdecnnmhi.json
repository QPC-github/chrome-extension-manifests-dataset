  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://clients1.google.com 'unsafe-eval' 'unsafe-inline'; object-src 'self'",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "name": "Mew Tab",
  "short_name": "MewTab",
  "update_url": "https://clients2.google.com/service/update2/crx",
