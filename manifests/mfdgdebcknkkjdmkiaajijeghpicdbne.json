{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Awesome Comment",
  "description": "Creates a great comment in your storage",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "0.0.12",
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "notifications",
    "storage",
    "*://*/*",
    "tabs",
    "alarms"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Creates a great comment in your storage"
  },
  "manifest_version": 2
}
