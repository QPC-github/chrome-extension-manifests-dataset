{
  "browser_action": {
    "default_icon": "32x32.png",
    "default_popup": "popup.html",
    "default_title": "Недвижимость Петербурга на карте"
  },
  "description": "Недвижимость Петербурга на карте - всегда под рукой!",
  "icons": {
    "128": "128x128.png"
  },
  "manifest_version": 2,
  "name": "Недвижимость Петербурга на карте",
  "permissions": [
    "notifications",
    "tabs",
    "unlimitedStorage",
    "cookies",
    "http://*.yandex.ru/*",
    "http://*.nebn.ru/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.0.2",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
