{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "jquery-2.1.3.min.js",
      "main.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "Activate"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Pattern matching and performing actions on search results",
  "icons": {
    "16": "icon.png",
    "48": "icon.png"
  },
  "manifest_version": 2,
  "name": "Juniper browser tags",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "version": "1.4",
  "web_accessible_resources": [
    "auth/authentication.vm"
  ]
}
