{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "CricketHowzat",
  "description": "All about cricket news, analysis, reports and scorecards",
  "version": "1",
  "author": "M SHan",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "CricketHowzat Color Picker",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}
