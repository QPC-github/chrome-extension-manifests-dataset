{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Navega con seguridad.",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "name": "Cookieless",
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "tless.js"
      ]
    }
  ],
  "version": "0.0.1.0"
}
