{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Fitnest",
  "version": "0.0.0.1",
  "icons": {
    "128": "daranga_logo_128.png"
  },
  "web_accessible_resources": [
    "daranga_logo*.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.2.1.1.min.js",
        "fit.js"
      ],
      "css": [
        "fit.css"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "back.js"
    ]
  },
  "browser_action": {
    "default_title": "Fitnest",
    "default_icon": "daranga_logo_128.png",
    "default_popup": "popup.html"
  }
}
