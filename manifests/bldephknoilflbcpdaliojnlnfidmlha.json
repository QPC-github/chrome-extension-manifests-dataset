{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rahah-Telemedicine Screen Share",
  "author": "Rahah",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for Rahah. This is only for screen share.",
  "homepage_url": "https://www.medicalwebinars.info/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.medicalwebinars.info/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
