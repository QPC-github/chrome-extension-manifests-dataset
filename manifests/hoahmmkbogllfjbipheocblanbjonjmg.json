{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yack.net - Screen Sharing",
  "description": "An extension to allow you to share screen with Yack.net.",
  "version": "1.5.4",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "minimum_chrome_version": "67.0",
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.yack.net/*",
      "*://.yack.net/*"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon16.png",
    "icon48.png",
    "icon128.png"
  ]
}
