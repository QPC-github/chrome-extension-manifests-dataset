{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "WikImage",
  "description": "Affiche l'image en rapport au lien wikipedia",
  "version": "1.0.1",
  "icons": {
    "128": "img/128.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/main.js"
      ]
    }
  ]
}
