{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "1Mobile QR",
  "version": "1.3",
  "description": "1Mobile QR",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "1Mobile QR",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "declarativeContent"
  ],
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2
}
