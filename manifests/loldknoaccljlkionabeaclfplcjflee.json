{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cute Animals New Tab Theme",
  "version": "0.3.1000.101",
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "topSites",
    "alarms",
    "https://*.joona.io/*",
    "http://*.joona.io/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Enjoy Cute Animal HD Wallpapers, Web Search & More!",
  "chrome_url_overrides": {
    "newtab": "html/load.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/cs/data-frame.js"
      ],
      "matches": [
        "https://*.joona.io/*",
        "http://*.joona.io/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "page": "html/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "web_accessible_resources": [
    "js/bg/focus.js"
  ]
}
