{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "38": "ChromeIcon.png"
    },
    "default_title": "Chatfeed.me"
  },
  "description": "Chatfeed.me Chrome Extension",
  "icons": {
    "128": "ChromeIcon.png",
    "48": "ChromeIcon.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "26.0.0.0",
  "name": "Chatfeed.me",
  "permissions": [
    "cookies",
    "background",
    "idle",
    "notifications",
    "tabs",
    "*://*.chatfeed.me/*",
    "*://*.thelandsoft.com/*"
  ],
  "version": "2015.1.1.9",
  "web_accessible_resources": [
    "*"
  ]
}
