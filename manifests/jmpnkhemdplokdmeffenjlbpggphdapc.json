{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Louder",
  "description": "Allows to boost sound beyond maximum level.",
  "version": "0.3.1",
  "author": "no9org@gmail.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*/*"
      ],
      "js": [
        "louder.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "file:///*/*"
  ],
  "browser_action": {
    "default_icon": "icon19.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "applications": {
    "gecko": {
      "id": "{8ceee5ac-ebc4-4254-9789-e8d8694becf5}",
      "strict_min_version": "42.0"
    }
  }
}
