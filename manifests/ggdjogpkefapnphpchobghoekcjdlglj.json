{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "browser_action": {
    "default_icon": "tower.png",
    "default_title": "Colbert Weather",
    "default_popup": "popup.html"
  },
  "description": "Local weather information for Colbert, Ga.",
  "manifest_version": 2,
  "name": "Colbert Weather",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "webNavigation",
    "notifications"
  ]
}
