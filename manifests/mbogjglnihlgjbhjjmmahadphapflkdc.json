{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No More #NORMCORE",
  "description": "This extension Redacts all Normcore",
  "version": "2.2",
  "icons": {
    "16": "no16.png",
    "48": "no48.png",
    "128": "no128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "jquery-1.11.0.min.js",
        "killnorm.js"
      ]
    }
  ]
}
