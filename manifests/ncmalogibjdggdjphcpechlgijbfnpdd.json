{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cash to BTC",
  "version": "0.0.1",
  "browser_action": {
    "default_title": "Change to Bitcoin!",
    "default_icon": "bitcoin.png"
  },
  "background": {
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
