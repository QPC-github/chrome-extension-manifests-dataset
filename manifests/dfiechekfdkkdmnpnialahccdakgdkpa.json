{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Usabilime ScreenSharing",
  "version": "1.0.8",
  "author": "Agility Feat, Inc",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "homepage_url": "http://agilityfeat.com/",
  "description": "Usabilime screensharing is an extension to enable screensharing using WebRTC. Powered by Agility Feat.",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "permissions": [
    "desktopCapture"
  ]
}
