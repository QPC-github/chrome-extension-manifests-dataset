{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy Clear Cache",
  "description": "This extension creates an easy button to clear browser cache",
  "version": "1.0.0",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_title": "Clear Cache"
  },
  "permissions": [
    "browsingData",
    "tabs"
  ]
}
