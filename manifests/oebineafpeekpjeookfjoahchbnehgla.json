{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "MSE-Multiple search engine",
  "version": "1.1",
  "description": "MSE is Stands for Multiple Web Search Engine, Private Web Search Engine, Meta Search Engine , Wikipedia Search in a Single Click",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
