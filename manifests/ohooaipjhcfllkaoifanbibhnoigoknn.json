{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "colashme",
  "version": "1.30",
  "manifest_version": 2,
  "description": "Envia imagenes a colashme.com",
  "browser_action": {
    "default_icon": "icono.png",
    "default_title": "colashme.com"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  }
}
