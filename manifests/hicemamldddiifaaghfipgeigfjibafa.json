{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "CoinPin",
  "version": "1.0",
  "description": "CoinPin",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CoinPin"
  }
}
