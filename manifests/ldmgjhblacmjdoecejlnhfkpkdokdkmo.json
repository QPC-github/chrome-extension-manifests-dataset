{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Maroc Radar d'emploi",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Maroc Radar d'emploi",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "alarms",
    "notifications",
    "*://*/*"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  }
}
