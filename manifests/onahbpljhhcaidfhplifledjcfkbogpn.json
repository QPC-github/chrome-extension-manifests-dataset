{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Mobile Insights Monitor",
  "description": "Continiously track your sites and pages to ensure that the are always mobile friendly",
  "version": "2013.8.11.2",
  "minimum_chrome_version": "23",
  "icons": {
    "128": "images/128.png",
    "256": "images/256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "alarms"
  ]
}
