{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "easyLink",
  "version": "0.1",
  "background_page": "background.html",
  "description": "Use the goo.gl URL shortening service to make URLs easier to work with.",
  "browser_action": {
    "default_title": "Shorten URL",
    "default_icon": "easyLinkIcon.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "*://*/*"
  ],
  "manifest_version": 2
}
