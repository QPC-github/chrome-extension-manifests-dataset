{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "JumpRope Converter",
  "version": "0.9.2",
  "content_scripts": [
    {
      "matches": [
        "https://a.jumpro.pe/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
