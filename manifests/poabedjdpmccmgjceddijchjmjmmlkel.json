{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Remove Centrelink Cookies",
  "description": "This extension removes cookies from centrelink.gov.au",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png"
  },
  "browser_action": {
    "default_icon": "icon96.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "https://*/",
    "http://*/",
    "https://*.centrelink.gov.au/*"
  ]
}
