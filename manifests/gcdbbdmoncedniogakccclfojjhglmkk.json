{
  "background": {
    "scripts": [
      "js/searchUtil.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "html/NewTab.html"
  },
  "description": "Show your favorite inboxs in one place from your Chromeâ„¢ New Tab in an instant!.",
  "icons": {
    "128": "images/icons/inbox-128x128.png",
    "16": "images/icons/inbox-16x16.png",
    "48": "images/icons/inbox-48x48.png"
  },
  "manifest_version": 2,
  "name": "Ultimate Inbox",
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "short_name": "Ultimate Inbox",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0.9",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "js/generalSearchPage.js",
    "html/NewTab.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*index.html?extid*"
      ],
      "js": [
        "js/injectExtID.js"
      ]
    }
  ]
}
