{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "One Nation Conservative",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Keep up to date with the latest posts from One Nation Conservative",
  "icons": {
    "128": "news_icon.png"
  },
  "browser_action": {
    "default_title": "Google News",
    "default_icon": "news_action.png",
    "default_popup": "feed.html"
  },
  "permissions": [
    "tabs",
    "http://onenationconservative.t15.org/*"
  ]
}
