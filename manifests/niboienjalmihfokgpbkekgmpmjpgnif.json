{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "seo density tracker",
  "version": "0.1",
  "description": "get top 3 word occurences & enter a comma sepearated list to find occurences",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon32.png",
    "128": "icon32.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "author": "devon dunn <github> "
}
