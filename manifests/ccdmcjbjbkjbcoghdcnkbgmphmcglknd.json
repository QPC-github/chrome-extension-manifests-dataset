{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Displays SSRN PDFs in the browser instead of downloading them.",
  "incognito": "spanning",
  "name": "SSRN PDF",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "background"
  ],
  "version": "1.2",
  "manifest_version": 2
}
