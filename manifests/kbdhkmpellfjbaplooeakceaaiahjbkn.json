{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "HODL Maxcoin Ticker",
  "icons": {
    "128": "Images/icon.png",
    "16": "Images/icon.png",
    "48": "Images/icon.png"
  },
  "description": "HODL Maxcoin is an easy to use MAX price ticker. Hourly, Daily & Weekly Price Movements in Fiat & real currencies such as Bitcoin.",
  "version": "2.1",
  "browser_action": {
    "default_icon": "Images/icon.png",
    "default_popup": "max.html",
    "default_title": "Check me out!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
