{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Delivery Tracker",
  "description": "Tracks UPS, FedEx and USPS packages",
  "version": "0.6",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon32.png"
  },
  "background": {
    "scripts": [
      "deliverytracker.js"
    ]
  },
  "manifest_version": 2
}
