{
  "name": "Highlight H1 H2, H3 & highlight nofollow",
  "version": "2.0",
  "description": "Resalta etiquetas Hx con un borde de puntos rojos para el H1, verde H2 y azul H3. Tacha los enlazes con nofollow by FunnelPunk.com",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "all_frames": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2
}
