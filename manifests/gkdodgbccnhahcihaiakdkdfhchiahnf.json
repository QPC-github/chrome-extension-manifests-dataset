{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TouchEn 확장프로그램",
  "description": "브라우저에서 개인정보 입력을 보호하기 위한 라온시큐어의 PC보안 확장프로그램입니다.",
  "version": "1.0.1.15",
  "author": "라온시큐어㈜",
  "homepage_url": "https://www.raonsecure.com",
  "icons": {
    "16": "default_16.png",
    "48": "default_48.png",
    "128": "default_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*/*",
    "nativeMessaging",
    "tabs"
  ]
}
