{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TextEncryptor",
  "description": "Simple tool to encrypt/decrypt text with AES algorithm.",
  "version": "0.0.4",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "dialog.js",
    "styles/*",
    "html/*"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_title": "TextEncryptor",
    "default_popup": "popup.html"
  }
}
