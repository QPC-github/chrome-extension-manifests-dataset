{
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.3.2.min.js",
        "jquery.exif.js",
        "jquery.gmap.min.js",
        "myscript.js"
      ],
      "matches": [
        "http://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "View photo EXIF and GPS Data in ur Chrome.",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "name": "EXIF Reader",
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.7.5.24"
}
