{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ClickIn",
  "version": "1.0",
  "description": "Сервис укорачивания ссылок",
  "manifest_version": 2,
  "web_accessible_resources": [
    "popup.js"
  ],
  "icons": {
    "48": "img/ico_48.png",
    "128": "img/ico_128.png"
  },
  "permissions": [
    "tabs",
    "http://click.in.ua/*"
  ],
  "browser_action": {
    "default_title": "ClickIn",
    "default_icon": "img/ico_128.png",
    "default_popup": "popup.html"
  }
}
