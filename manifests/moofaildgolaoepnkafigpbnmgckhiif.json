{
  "name": "折满满购物管家",
  "version": "9.0.8",
  "manifest_version": 2,
  "description": "天猫/淘宝商品优惠券一键领取,下单直接抵扣；在淘宝，天猫，京东，苏宁易购展示历史价格信息及多平台的比价信息。买到爽！省到嗨！",
  "content_scripts": [
    {
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/const.js",
        "scripts/vue.min.js",
        "scripts/jquery.js",
        "scripts/gm.js",
        "scripts/echarts.min.js",
        "scripts/jquery.cookie.js",
        "scripts/content.zmm.js",
        "scripts/coupon.zmm.list.js"
      ],
      "matches": [
        "*://*.taobao.com/*",
        "*://*.chi.taobao.com/*",
        "*://*.ju.taobao.com/*",
        "*://*.alitrip.com/*",
        "*://*.tmall.com/*",
        "*://*.tmall.hk/*",
        "*://*.detail.tmall.com/*",
        "*://*.jd.com/*",
        "*://*.jd.hk/*",
        "*://*.baidu.com/*",
        "*://*.sogou.com/*",
        "*://*.so.com/*",
        "*://*.hao123.com/*",
        "*://*.amazon.cn/*",
        "*://*.dangdang.com/*",
        "*://*.vip.com/*",
        "*://*.yhd.com/*",
        "*://*.jumei.com/*",
        "*://*.suning.com/*",
        "*://*.gome.com.cn/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon/icon_19.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon/icon_16.png",
    "19": "icon/icon_19.png",
    "32": "icon/icon_32.png",
    "48": "icon/icon_48.png",
    "96": "icon/icon_96.png",
    "128": "icon/icon_128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "unlimitedStorage",
    "notifications",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
