{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "E Okul",
  "version": "1.0",
  "manifest_version": 2,
  "description": "E Okul giriş ekranı (öğretmen & öğrenci) tarayıcınızdan erişin. https://eokul.school/ Ayrıca Mebbis uygulaması mevcut.",
  "browser_action": {
    "default_icon": "icon.png",
