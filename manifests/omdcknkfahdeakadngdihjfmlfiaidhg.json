{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Super Mario Kart",
  "description": "Mario game from Arcade Games years ago.",
  "version": "1.6",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Mario"
  }
}
