{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Yahoo!リアルタイム検索で評判チェック",
  "short_name": "RtCheck",
  "version": "0.4",
  "manifest_version": 2,
  "description": "今見ているページのURLをYahoo!リアルタイム検索に投げるChrome拡張です。",
  "permissions": [
    "tabs",
    "http://*/*",
    "contextMenus"
  ],
  "icons": {
    "16": "escs.png",
    "48": "escs48.png",
    "128": "escs128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
