{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Spotifork",
  "description": "Adds little links to the Pitchfork website that allow you to immediately jump to a Spotify search.",
  "version": "1.3",
  "icons": {
    "16": "sfork16.png",
    "48": "sfork48.png",
    "128": "sfork128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://pitchfork.com/*",
        "https://pitchfork.com/*"
      ],
      "js": [
        "jquery321min.js",
        "pforkify.js"
      ],
      "css": [
        "pforkify.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "spotify-icon.png"
  ],
  "manifest_version": 2
}
