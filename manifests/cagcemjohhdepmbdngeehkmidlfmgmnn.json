{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Git Search",
  "description": "GitHub CommandBar for Chrome. Type 'gh' and a search term to jump to a repository, user or a search. Enjoy the quick access!",
  "version": "1.0",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "omnibox": {
    "keyword": "gh"
  },
  "permissions": [
    "tabs",
    "https://github.com/"
  ],
  "manifest_version": 2
}
