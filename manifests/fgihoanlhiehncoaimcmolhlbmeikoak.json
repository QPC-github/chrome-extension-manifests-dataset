{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Youtube Spacer",
  "description": "Lets you use space to play/pause youtube videos without having it in focus!",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/watch?v=*",
        "https://www.youtube.com/watch?v=*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
