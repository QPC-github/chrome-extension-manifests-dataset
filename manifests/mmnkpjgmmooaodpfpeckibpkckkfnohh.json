{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "gyazo Extension",
  "description": "This extension will automatically take you to the .jpg extension of your gyazo screenshot.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/background.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}
