{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Site Home",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Brings you to the home page of the current site with the click of a button.",
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
