{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SpringRole + BeeBeeRecruiter",
  "version": "0.3",
  "description": "Latest job list in India & USA.",
  "icons": {
    "16": "images/icon.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "homepage_url": "http://beebeerecruiter.weebly.com/",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://rss.springrole.com/rss/"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "SpringRole + BeeBeeRecruiter",
    "default_popup": "popup.html"
  }
}
