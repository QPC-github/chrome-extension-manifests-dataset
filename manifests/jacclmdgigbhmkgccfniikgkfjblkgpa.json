{
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.min.js",
      "startup.js"
    ]
  },
  "browser_action": {
    "default_icon": "res/icon.png",
    "icons": [
      "res/icon.png"
    ],
    "name": "__MSG_appName__"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.min.js",
        "js/background.js",
        "js/avgrund/jquery.avgrund.min.js",
        "js/jquery-ui.js"
      ],
      "matches": [
        "*://*.roblox.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "homepage_url": "https://www.roblox.com/my/account?tab=rbxutility",
  "icons": {
    "128": "res/icon_128.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "tts",
    "storage",
    "downloads",
    "unlimitedStorage",
    "contextMenus",
    "notifications",
    "*://*.roblox.com/*",
    "*://*.rbxcdn.com/*"
  ],
  "short_name": "__MSG_appShortname__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.0.0.24",
  "web_accessible_resources": [
    "/res/*",
    "/css/*",
    "/js/*",
    "changelog/*"
  ]
}
