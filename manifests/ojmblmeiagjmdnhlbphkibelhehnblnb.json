{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Split and Merge by Kami",
  "short_name": "Split and Merge",
  "description": "Split and Merge your PDFs online",
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [],
  "app": {
    "launch": {
      "web_url": "https://tools.kamihq.com/merge"
    }
  },
  "offline_enabled": false,
  "container": [
    "GOOGLE_DRIVE",
    "DOMAIN_INSTALLABLE"
  ],
  "api_console_project_id": "1017501917745",
  "gdrive_mime_types": {
    "http://drive.google.com/intents/opendrivedoc": [
      {
        "type": [
          "application/pdf",
          "application/vnd.google.drive.ext-type.pdf"
        ],
        "href": "https://tools.kamihq.com/merge",
        "title": "Open",
        "disposition": "window"
      }
    ]
  }
}
