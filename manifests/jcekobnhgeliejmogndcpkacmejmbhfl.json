{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Appypie Extension",
  "manifest_version": 2,
  "version": "3.0",
  "description": "App Maker Appy Pie rated best FREE Mobile App Builder",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "background": "background.html"
}
