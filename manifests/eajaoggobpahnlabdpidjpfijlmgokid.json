{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Forex Robot Reviews",
  "description": "Forex robot reviews with trading ideas, currency pairs and timeframe, backtests, live performance results, prices & websites rating.",
  "version": "1.2.3",
  "short_name": "CashEncode",
  "browser_action": {
    "default_icon": {
      "16": "images/16_off.png",
      "32": "images/32_off.png",
      "38": "images/38_off.png",
      "48": "images/48.png",
      "96": "images/96.png",
      "128": "images/128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Forex Robot Reviews"
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "38": "images/38.png",
    "48": "images/48.png",
    "96": "images/96.png",
    "128": "images/128.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/jquery.browser.min.js",
      "js/jquery.jfeed.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://www.reviewforexrobots.com/",
    "storage",
    "unlimitedStorage",
    "notifications",
    "contextMenus"
  ]
}
