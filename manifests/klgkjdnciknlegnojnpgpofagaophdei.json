{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Aspect Ratio calculator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A simple extension to calculate aspect ratio",
  "homepage_url": "http://brightfox.com.au",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Aspect Ratio Calculator",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "notifications"
  ],
  "omnibox": {
    "keyword": "ARcalc"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' "
}
