{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Never apply too late for an interesting job again!!",
  "manifest_version": 2,
  "name": "TellMeUpwork (by tvsurftv team)",
  "short_name": "TellMeUpwork",
  "version": "1.0.12",
  "homepage_url": "https://tvsurftv.com",
  "icons": {
    "32": "common/icons/upwork-48x48.png",
    "64": "common/icons/upwork-48x48.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "common/vendor/js/jquery.min.js",
      "common/vendor/js/underscore-min.js",
      "common/vendor/js/throttled-queue.js",
      "common/vendor/js/google-analytics-bundle.js",
      "common/js/util.js",
      "common/js/utils.js",
      "common/js/api.js",
      "common/js/options.js",
      "common/js/emitter.js",
      "common/js/messenger.js",
      "common/js/iconeApplication.js",
      "common/js/notification.js",
      "common/js/cookie.js",
      "main.js"
    ]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "cookies",
    "storage",
    "contextMenus",
    "identity",
    "identity.email",
    "downloads",
    "management",
    "notifications",
    "*://*/*"
  ],
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": "common/icons/upwork-48x48.png",
    "default_title": "Tell me upwork"
  },
  "web_accessible_resources": [
    "pages/*",
    "resources/templates/*",
    "css/*",
    "common/icons/*"
  ]
}
