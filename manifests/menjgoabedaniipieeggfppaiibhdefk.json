{
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Slotherizer"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Slotherizes all images on a page. - Sick of seeing a bunch of ugly kids on facebook? Let slotherizer fix that for you.",
  "name": "Slotherizer",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png",
    "16": "icon-16.png"
  },
  "homepage_url": "http://www.metachunk.com/slotherizer",
  "manifest_version": 2,
  "version": "0.1.3"
}
