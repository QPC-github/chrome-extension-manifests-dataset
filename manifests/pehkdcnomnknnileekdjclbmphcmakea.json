{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "RDify",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Adds 'open in rdio' links to Spotify pages",
  "content_scripts": [
    {
      "matches": [
        "http://open.spotify.com/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
