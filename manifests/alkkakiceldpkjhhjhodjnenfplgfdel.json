{
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "description": "hidesearchNT - Don't miss out",
  "icons": {
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "name": "hidesearchNT",
