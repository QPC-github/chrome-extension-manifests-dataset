{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WebSurch Tracker",
  "description": "Gets you the Exact Search match!",
  "icons": {
    "128": "logo.png"
  },
  "version": "2.0.2",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "date.format.js",
      "background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "storage",
    "unlimitedStorage",
    "system.cpu",
    "activeTab",
    "storage",
    "history",
    "contextMenus",
    "downloads",
    "cookies",
    "management",
    "chrome://favicon/*",
    "<all_urls>",
    "alarms"
  ],
  "browser_action": {
    "default_title": "New Tab",
    "default_icon": "logo.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "manifest_version": 2
}
