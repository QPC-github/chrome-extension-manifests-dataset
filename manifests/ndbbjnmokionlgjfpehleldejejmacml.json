{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Taripebi.Ge - Online Currency",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "description": "All currency rates in Georgia, gold prices, online loan !",
  "version": "1.0",
  "short_name": "Taripebi",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "body.html"
  },
  "chrome_url_overrides": {
    "newtab": "background.html"
  },
  "permissions": [
    "activeTab",
    "http://taripebi.ge/*"
  ]
}
