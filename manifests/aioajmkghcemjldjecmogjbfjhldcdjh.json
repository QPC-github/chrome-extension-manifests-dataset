{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Vidtu Screen Share",
  "version": "0.15",
  "background": {
    "scripts": [
      "extension.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://*.vidtu.com/*"
    ]
  },
  "permissions": [
    "desktopCapture"
  ]
}
