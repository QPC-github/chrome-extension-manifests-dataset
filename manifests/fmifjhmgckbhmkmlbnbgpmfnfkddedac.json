{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Image Doctor",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "version": "2.1",
  "description": "This extension allows users to improve the quality of downloaded pictures on the web",
  "icons": {
    "16": "ImageDoctorSm.png",
    "48": "ImageDoctorMed.png",
    "128": "ImageDoctorLrg.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://webservice.e-stellar.com:4545/*",
    "http://webservice.e-stellar.com:8080/*"
  ],
  "background": {
    "scripts": [
      "ImageDoctor.js"
    ]
  }
}
