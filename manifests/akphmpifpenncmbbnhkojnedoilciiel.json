{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ding",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "Ding.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "assets/icons/bell-lit.png"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "notifications"
  ],
  "icons": {
    "16": "assets/icons/bell-16.png",
    "32": "assets/icons/bell-32.png",
    "64": "assets/icons/bell-64.png",
    "128": "assets/icons/bell-128.png"
  }
}
