{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Zoop Web SDK Extension",
  "version": "1.4",
  "manifest_version": 2,
  "description": "Extensao para integracao com o Zoop Payments, para realizar transacoes de cartao presente na rede Zoop",
  "icons": {
    "128": "icon-128.png"
  },
  "externally_connectable": {
    "matches": [
      "*://64.62.167.81/*"
    ]
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": true
  },
  "permissions": [
    "nativeMessaging"
  ],
  "options_page": "options.html"
}
