{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump Fart",
  "version": "1.1",
  "description": "Replaces the text 'Trump' with a synonym of fart.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "replace.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
