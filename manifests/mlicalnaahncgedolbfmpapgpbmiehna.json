{
  "manifest_version": 2,
  "name": "Pixfans.com",
  "version": "0.1",
  "description": "Pixfans es un blog de videojuegos, gadgets, series... Con esta extensión podrás estar al tanto de sus últimas actualizaciones.",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Pixfans.com",
    "default_popup": "pixfans.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
