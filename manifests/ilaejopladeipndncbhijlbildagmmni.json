{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Non-Official Retweeter for Twitter",
  "version": "0.3.0",
  "manifest_version": 2,
  "description": "You will be able to send a message as unofficial (quote) retweet on the Twitter.",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jQuery1.8.1.js",
        "contentscript.js"
      ],
      "matches": [
        "https://twitter.com/*/*",
        "https://twitter.com/*",
        "http://twitter.com/*/*",
        "http://twitter.com/*"
      ]
    }
  ]
}
