{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Block facebook trends",
  "version": "1.1",
  "permissions": [
    "*://facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
