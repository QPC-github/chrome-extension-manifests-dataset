{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Card Cutter",
  "description": "A card cutting chrome extension for Speech & Debate and research projects.",
  "version": "0.0.0.6",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "contextMenus"
  ],
  "browser_action": {
    "default_title": "Cut a card from this page",
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "scripts/backgroundScripts.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon.png"
  },
  "content_security_policy": "script-src 'self' https://www.googletagmanager.com https://www.google-analytics.com; object-src 'self'"
}
