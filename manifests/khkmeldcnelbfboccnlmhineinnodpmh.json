{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "iEditor",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A javascript interactive editor, it evaluates the javascript side by side and traces the running process of the program step by step",
  "browser_action": {
    "default_icon": "icon-ieditor-chrome.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon-ieditor-chrome.png",
    "48": "icon-ieditor-chrome-48.png",
    "128": "icon-ieditor-chrome-128.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
