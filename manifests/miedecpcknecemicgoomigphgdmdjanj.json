{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hey Pirate",
  "description": "Get the list of ThePirateBay mirrors instantly!",
  "version": "1.0.1",
  "icons": {
    "128": "assets/img/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html",
    "default_title": "Get the list of ThePirateBay mirrors!"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://www.heypirateproxy.com/*"
    ]
  }
}
