{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Baylorify",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Make every image the glorious John Baylor. Guaranteed to boost your ACT score 9001%.",
  "browser_action": {
    "default_icon": {
      "19": "john19x.png",
      "38": "john38x.png"
    },
    "default_title": "Release the Baylor! v1.5"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "http://www.johnbaylorprep.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "baylorify.js"
    ]
  }
}
