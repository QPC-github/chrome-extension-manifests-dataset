{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ankama Direct Link",
  "description": "Bypass the Ankama external link warning",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://go.ankama.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "packages/ankama.png",
    "48": "packages/ankama.png",
    "128": "packages/ankama.png"
  }
}
