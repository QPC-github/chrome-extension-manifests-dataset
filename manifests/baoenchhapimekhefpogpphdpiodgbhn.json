{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Notedown.io BETA",
  "description": "This extension allows you to capture your notes and tasks.",
  "version": "1.2.5",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides": {
    "newtab": "window.html"
  }
}
