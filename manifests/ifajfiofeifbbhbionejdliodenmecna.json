    "default_icon": {
      "19": "Graphics/CoWriterIcon-Loading_19.png",
      "38": "Graphics/CoWriterIcon-Loading_38.png"
    },
    "default_title": "Co:Writer",
    "default_popup": "Background/popup.html"
  },
  "icons": {
    "16": "Graphics/CoWriterIcon-16x16.png",
