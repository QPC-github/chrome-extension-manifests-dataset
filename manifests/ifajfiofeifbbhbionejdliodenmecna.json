        "https://login.stagedji.com/*",
        "https://pdf.stagedji.com/onedrive/*",
        "https://pdf.stagedji.com/gdrive/*",
        "https://login.qadji.com/*",
        "https://pdf.qadji.com/onedrive/*",
        "https://pdf.qadji.com/gdrive/*"
      ],
      "run_at": "document_start"
