        "ContentScripts/ui/cweFonts.css"
      ],
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "https://accounts.google.com/signin/*",
        "https://login.microsoftonline.com/common/oauth2/*",
        "https://login.microsoftonline.com/*",
        "https://login.live.com/*",
        "https://login.donjohnston.net/*",
        "https://pdf.snapandread.com/onedrive/*",
        "https://pdf.snapandread.com/gdrive/*",
        "https://login.stagedji.com/*",
        "https://pdf.stagedji.com/onedrive/*",
        "https://pdf.stagedji.com/gdrive/*",
        "https://login.qadji.com/*",
        "https://pdf.qadji.com/onedrive/*",
        "https://pdf.qadji.com/gdrive/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "Common/color.js",
        "WebExtensions/utils/timer.js",
        "WebExtensions/utils/logger.js",
        "Common/cweLogger.js",
        "ContentScripts/cweTopicHelper.js",
        "ContentScripts/ui/djiUiTemplatesRegistry.js",
        "WebExtensions/ui/djiDOMExtensions.js",
