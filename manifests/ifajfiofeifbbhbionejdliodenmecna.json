    "Graphics/CoWriterIcon-256x256.png",
    "Graphics/ic_mic_black_24px.svg",
    "Graphics/ic_mic_off_black_24px.svg",
    "Fonts/*.woff",
    "Fonts/*.woff2",
    "Fonts/*.ttf",
    "Fonts/OpenDyslexic-BoldItalic.woff"
  ],
  "permissions": [
    "<all_urls>",
    "tabs",
    "notifications",
    "unlimitedStorage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "webNavigation"
