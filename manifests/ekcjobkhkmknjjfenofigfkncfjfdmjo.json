{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Cache Cleaner",
  "short_name": "CC",
  "version": "1.0",
  "author": "Maxym Gorbenko",
  "description": "Clear browser cache for the last week.",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "browsingData",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Clear cache!",
    "default_icon": "icons/128x128.png"
  }
}
