{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Audio Pan",
  "version": "1.2",
  "short_name": "AudioPan",
  "description": "Allows stereo audio panning of a tab's audio. Works for as many tabs as you have memory for.",
  "permissions": [
    "tabCapture",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Testing",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
