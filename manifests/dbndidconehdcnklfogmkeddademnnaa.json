{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trump To He Who Must Not Be Named",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "description": "Replaces various version of Donald Trump's name with 'He Who Must Not Be Named'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "background.js"
      ],
      "run_at": "document_end"
    }
  ]
}
