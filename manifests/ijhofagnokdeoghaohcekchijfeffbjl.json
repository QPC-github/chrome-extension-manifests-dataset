{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Desktop Selector",
  "description": "Use to desktop selector for sharing the screens",
  "version": "0.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {},
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}
