{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Newerth Servers",
  "description": "Keep an eye on the Newerth world.",
  "version": "0.0.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "jquery-2.0.3.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://www.newerth.com/*",
    "http://www.heuuuth.com/*"
  ]
}
