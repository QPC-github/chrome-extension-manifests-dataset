{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Heniek Quick Search",
  "description": "Ten dodatek pozwala na szybkie wyszukiwanie zaznaczonego tekstu w wyszukiwarce Google",
  "version": "1.0",
  "icons": {
    "64": "ikona.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
