{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tabr",
  "version": "0.1.5",
  "description": "Quickly find your tabs",
  "icons": {
    "16": "med/t16.png",
    "48": "med/t48.png",
    "128": "med/t128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "med/t19.png",
      "38": "med/t38.png"
    },
    "default_title": "Tabr",
    "default_popup": "tabr-popup.html"
  },
  "author": "Barak Gall",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Q"
      }
    }
  },
  "content_security_policy": "default-src 'self'; img-src *",
  "current_locale": "en",
  "externally_connectable": {
    "ids": []
  },
  "incognito": "spanning",
  "minimum_chrome_version": "24",
  "offline_enabled": true,
  "permissions": [
    "tabs"
  ]
}
