{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "A theme solely devoted to the incredibily dark and still the best Stealth and Action game, Dishonored. Enjoy :-)",
  "name": "Dishonored Theme",
  "browser_action": {
    "default_icon": "finalIcon.png",
    "default_popup": "popupDishonored.html",
    "default_title": "Revenge Solves Everything"
  },
  "theme": {
    "images": {
      "theme_ntp_background": "Dishonored.png"
    },
    "properties": {
      "ntp_background_alignment": "center"
    }
  },
  "icons": {
    "64": "finalIconSmall.png",
    "128": "finalIcon.png"
  },
  "incognito": "split"
}
