{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "VSD Exerciser",
  "description": "This is a Nuage VSD API exerciser",
  "version": "1.1.6",
  "permissions": [
    "webRequest",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "scripts": [
      "content.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "launcher.html"
  },
  "offline_enabled": true
}
