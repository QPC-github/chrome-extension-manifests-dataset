{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Route 50 Notifier",
  "manifest_version": 2,
  "description": "Counts Notifications and new Mail",
  "version": "2.0.0",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png"
  },
  "permissions": [
    "notifications",
    "http://*.route50.net/"
  ],
  "browser_action": {
    "default_icon": "img/icon_16.png",
    "default_title": "Unread",
    "default_popup": "popup/page.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "js/bg.js"
    ]
  }
}
