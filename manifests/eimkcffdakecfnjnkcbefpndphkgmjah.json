{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Graphulator With Numerical Calculus",
  "short_name": "Graphulator",
  "description": "Graphulator with Calculus",
  "version": "5.3.0",
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "72": "images/72.png",
      "128": "images/128.png",
      "144": "images/144.png",
      "192": "images/192.png"
    }
  },
  "permissions": [
    "http://cloud3.graphulator.com/direct/Desktop/indexChrome.html",
    "http://cloud3.graphulator.com/direct/Desktop/noflash.html",
    "http://cloud3.graphulator.com/direct/Desktop/indexaf.html",
    "http://cloud3.graphulator.com/direct/Desktop/JS.html",
    "http://cloud3.graphulator.com/direct/Desktop/grid/grid12.html",
    "http://cloud3.graphulator.com/direct/Desktop/Document/translate.html",
    "http://cloud3.graphulator.com/direct/video.html",
    "http://cloud3.graphulator.com/PolicyFile.html",
    "http://cloud3.graphulator.com/PolicyUse.html"
  ],
  "requirements": {
    "3D": {
      "features": [
        "webgl"
      ]
    }
  }
}
