{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "CleanClear",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  },
  "description": "make your social network more concision",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "cleanclear.png"
  }
}
