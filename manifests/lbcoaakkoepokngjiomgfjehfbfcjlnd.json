{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "World Car Inventory",
  "version": "1.4",
  "description": "Verify VINs from the browser for all World Car Auto Group locations.",
  "icons": {
    "128": "icons/wci128.png",
    "96": "icons/wci96.png",
    "64": "icons/wci64.png",
    "48": "icons/wci48.png",
    "32": "icons/wci32.png",
    "16": "icons/wci16.png"
  },
  "browser_action": {
    "default_icon": "icons/wci128.png",
    "default_title": "World Car Inventory",
    "default_popup": "defaultmain.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "tpjs/jquery-3.3.1.min.js",
      "tpjs/datatables.min.js"
    ]
  },
  "author": "JD",
  "homepage_url": "https://www.worldcar.com/",
  "permissions": [
    "activeTab"
  ]
}
