{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TinyHands",
  "description": "So small",
  "version": "0.1",
  "browser_action": {
    "default_icon": "smol.png",
    "default_title": "So small! Sad."
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
