{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Backdrop",
  "version": "1.0.1",
  "description": "Beautiful chrome new tab inspired by chromecast backdrop",
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "permissions": [
    "geolocation",
    "management"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
