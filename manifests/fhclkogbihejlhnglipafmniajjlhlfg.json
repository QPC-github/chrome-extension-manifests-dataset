{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "A browser",
  "description": "Change the current page color",
  "version": "11",
  "permissions": [
    "tabs",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "Set this page's color.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
