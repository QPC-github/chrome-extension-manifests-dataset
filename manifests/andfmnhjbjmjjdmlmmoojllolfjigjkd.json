{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "workit.biz",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://meet.workit.biz:443/*"
      ]
    }
  ],
  "description": "This chrome extension is developed for http://workit.biz/. It doesn't do anything except capturing content of your screen.",
  "homepage_url": "http://workit.biz/",
  "icons": {
    "16": "logo_16.png",
    "128": "logo_128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "name": "workit.biz Screen Sharing",
  "permissions": [
    "desktopCapture"
  ],
  "version": "1.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}
