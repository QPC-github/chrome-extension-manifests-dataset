{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Find Concerts and Artists tour dates",
  "description": "Concerts, tour dates, and festivals for your favorite artists",
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "version": "1.1",
  "short_name": "Gigs finder",
  "permissions": [
    "http://api.songkick.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "19x19.png",
      "38": "38x38.png"
    },
    "default_popup": "popup.html"
  }
}
