{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "short_name": "YTRadio",
  "name": "YTRadio",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "content_security_policy": "script-src 'self' https://*.firebaseio.com; object-src 'self'",
  "version": "0.1"
}
