{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "My New Tab",
  "short_name": "My New Tab",
  "description": "Improve your new tab experience!",
  "version": "0.3",
  "incognito": "split",
  "externally_connectable": {
    "matches": [
      "*://*.newtab.cc/*",
      "*://newtab.cc/*"
    ]
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*.newtab.cc/*",
    "*://newtab.cc/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png"
    },
    "default_title": "Enhanced Search"
  },
  "manifest_version": 2
}
