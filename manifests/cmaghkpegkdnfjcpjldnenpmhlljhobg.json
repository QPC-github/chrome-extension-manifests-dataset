{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qlook",
  "version": "1.0.4",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts.js"
      ]
    }
  ],
  "icons": {
    "16": "Icon_16x16.png",
    "48": "Icon_16x16@3x.png",
    "128": "Icon_128x128.png"
  },
  "browser_action": {
    "default_icon": "Icon_16x16.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
