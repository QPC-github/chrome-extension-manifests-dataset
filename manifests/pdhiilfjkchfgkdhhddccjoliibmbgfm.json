{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tiinkk it!",
  "short_name": "Tiinkk it!",
  "version": "0.6",
  "author": {
    "name": "The Frontier Group",
    "url": "http://thefrontiergroup.com.au"
  },
  "description": "Tiinkk the decisions you make on the web",
  "homepage_url": "http://tiinkk.com",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "https://tiinkk.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Tiinkk it!",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "offline_enabled": false,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}
