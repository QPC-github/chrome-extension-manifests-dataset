{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Premise Time Tracker",
  "description": "The Premise Time Tracker Chrome Extension. Track time at premisetimetracker.com directly from chrome.",
  "version": "3.0.1",
  "browser_action": {
    "default_icon": "favicon.png",
    "icons": [
      "favicon.png"
    ],
    "default_popup": "index.html",
    "default_title": "Start tracking time!"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*.premisetimetracker.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": []
}
