{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Bookmark.fm",
  "manifest_version": 2,
  "default_locale": "en_US",
  "page_action": {
    "default_icon": "images/bfm-128.png",
    "default_title": "__MSG_actionTitle__"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_page": "options.html",
  "description": "__MSG_payoff__",
  "homepage_url": "http://bookmark.fm",
  "icons": {
    "128": "images/bfm-128.png",
    "48": "images/bfm-48.png",
    "16": "images/bfm-16.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.1.19",
  "offline_enabled": false
}
