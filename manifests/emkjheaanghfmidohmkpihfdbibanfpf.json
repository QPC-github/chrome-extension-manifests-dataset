{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "User Page•Pins",
  "short_name": "UP•P",
  "version": "1.0.3",
  "page_action": {
    "default_icon": "Images/icon_pin_20.png",
    "default_popup": "User_Interface.html"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Alt+P"
      }
    }
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "icons": {
    "16": "Images/icon_pin_20.png",
    "128": "Images/icon_pin_128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "Images/*.jpg",
    "Images/*.gif",
    "Images/*.png"
  ]
}
