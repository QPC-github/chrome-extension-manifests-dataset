{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "നിഘണ്ടു",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "options.html"
  ],
  "short_name": "Dictioanry",
  "description": "നിഘണ്ടു  @ ശ്യാമു വെള്ളനാട്.  Searches for the selected text in the Malayalam Dictioanry",
  "version": "0.0.0.4",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "assets/search.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "നിഘണ്ടു  @ ശ്യാമു വെള്ളനാട്"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  }
}
