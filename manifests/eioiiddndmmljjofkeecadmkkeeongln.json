{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Inline",
  "description": "Quickly insert text using :inline: syntax.",
  "version": "1.1.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
