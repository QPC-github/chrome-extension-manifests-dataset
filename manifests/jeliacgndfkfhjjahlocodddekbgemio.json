{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Preview viewer",
  "description": "This extension opens the previews on reddit",
  "version": "1.0",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "View all previews"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*.reddit.com/*",
    "tabs"
  ]
}
