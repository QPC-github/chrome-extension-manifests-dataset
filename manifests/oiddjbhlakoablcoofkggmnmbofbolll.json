{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.7.2.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Used to upload images from any site for use with the trends tool at SHOP.COM website.",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Shop.com Trends Image Uploader",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "short_name": "Trends Image Grabber",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.6"
}
