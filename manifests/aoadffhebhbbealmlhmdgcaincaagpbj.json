{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Clarin - Login OUT",
  "version": "0.5.0",
  "description": "Sacar cartelito de login clarin",
  "content_scripts": [
    {
      "run_at": "document_end",
      "all_frames": true,
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*.clarin.com/*"
      ]
    },
    {
      "run_at": "document_end",
      "all_frames": true,
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*.lanacion.com.ar/*"
      ]
    },
    {
      "all_frames": true,
      "run_at": "document_start",
      "css": [
        "styles.css"
      ],
      "js": [
        "content2.js"
      ],
      "matches": [
        "http://*.corriere.it/*"
      ],
      "permissions": [
        "cookies",
        "*://*.corriere.it/*"
      ]
    }
  ]
}
