{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SVM Click-to-Call",
  "version": "1.8",
  "description": "",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "c2c.css"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "storage"
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "pencil-15.png"
  ]
}
