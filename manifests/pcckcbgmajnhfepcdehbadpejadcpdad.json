{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0",
  "name": "Ice Cream Wallpapers New Tab Theme",
  "description": "Ð¡ool Theme with Ice Cream wallpaper for your new tab",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/scripts/vendor/JQuery.min.js",
      "/scripts/background/polyfills/array.js",
      "/scripts/background/configuration/core.js",
      "/scripts/background/configuration/base.js",
      "/scripts/background/helpers/prefs-sys.js",
      "/scripts/background/tutorial/checkTour.js",
      "/scripts/background/helpers/utils.js",
      "/scripts/background/handlers/common.js",
      "/scripts/background/handlers/setup.js",
      "/scripts/background/handlers/opener.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/static/icons/main/128.png"
    }
  },
  "icons": {
    "128": "/static/icons/main/128.png"
  },
  "permissions": [
    "*://mail.google.com/*",
    "tabs",
    "<all_urls>",
    "activeTab",
    "storage",
    "cookies",
    "management",
    "topSites",
    "webNavigation",
    "webRequestBlocking",
    "webRequest"
  ],
  "web_accessible_resources": [
    "*"
  ],
  "manifest_version": 2
}
