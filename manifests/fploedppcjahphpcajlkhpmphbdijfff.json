{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DatClock",
  "version": "1.1.4",
  "description": "Replaces your New Tab-Page with a nice looking clock",
  "icons": {
    "16": "icons/datclock16.png",
    "48": "icons/datclock48.png",
    "96": "icons/datclock96.png",
    "128": "icons/datclock128.png"
  },
  "chrome_url_overrides": {
    "newtab": "app/index.html"
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/datclock128.png",
    "default_popup": "app/popup.html"
  }
}
