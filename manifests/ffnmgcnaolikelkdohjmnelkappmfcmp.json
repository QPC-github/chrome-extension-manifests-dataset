  "name": "Отправка СМС по Молдове и Приднестровью",
  "version": "1.0",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "permissions": [
    "*://*.sms.idknet.com/*",
    "*://moldcell.md/*",
    "https://www.orangetext.md/*",
    "*://it-gid.com/*"
