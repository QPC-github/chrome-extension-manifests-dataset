{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PayK12 CC Reader and Printer Extension",
  "description": "PayK12 Credit Card Reader and Printer Extension",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "alarms",
    "tabs",
    "bookmarks",
    "declarativeWebRequest",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "PayK12"
  },
  "commands": {
    "open-google": {
      "description": "Open a tab to google.com",
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  }
}
