{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Android Calculator",
  "description": "Simple and easy to use calculator",
  "version": "1.6",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles/content.css"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
