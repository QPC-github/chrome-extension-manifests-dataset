  "default_locale": "zh_CN",
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png"
    },
    "default_title": "__MSG_title__"
  }
}
