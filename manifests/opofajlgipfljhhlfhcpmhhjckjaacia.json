{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_name__",
  "short_name": "JOF",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "__MSG_detail__",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
