{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Burstcoin Price Tracker",
  "author": "Dev Singh",
  "description": "Displays the current Burstcoin (BURST) price in USD/EUR/Bitcoin.",
  "version": "1.5.8.3",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "currencySelector.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
