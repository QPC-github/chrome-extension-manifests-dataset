{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube Suggestion Blocker",
  "description": "Untrap you from YouTube",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}
