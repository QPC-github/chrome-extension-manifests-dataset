{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kuvvium",
  "description": "使用Chrome浏览kuvva website的同时，可以将图片以合适的尺寸保存到本地。",
  "version": "0.1.2",
  "browser_action": {
    "default_icon": "icon-gray.png"
  },
  "background": {
    "scripts": [
      "packed.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://www.kuvva.com/*"
  ]
}
