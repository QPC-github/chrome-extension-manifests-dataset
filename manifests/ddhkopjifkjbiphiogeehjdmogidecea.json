{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Equinox Meet",
  "description": "Equinox Meet extension that allows Equinox Family Members (employees, customers, partners, etc.) to screenshare.",
  "version": "0.1.7",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "e5-icon.png",
    "48": "e5-icon.png",
    "128": "e5-icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.equinoxfive.com/*"
    ]
  }
}
