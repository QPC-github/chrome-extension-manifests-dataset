{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TrafficAdPays",
  "description": "Extension for trafficadpays.com",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
