{
  "author": "Jeremy",
  "name": "5Finder",
  "short_name": "5Finder",
  "homepage_url": "https://5Finder.com",
  "description": "5 Finder delivers secure search results.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "5Finder"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "https://5Finder.com/favicon.ico",
      "image_url": "",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "5Finder",
      "name": "5Finder",
      "search_url": "https://5Finder.com/index.php?a=web&q={searchTerms}",
      "search_url_post_params": "",
      "suggest_url_post_params": ""
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "1.js",
      "2.js"
    ]
  },
  "icons": {
    "128": "128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "*"
  ]
}
