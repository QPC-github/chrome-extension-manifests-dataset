{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Contributor",
  "description": "Log history for Thesis project",
  "version": "0.1",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "pop_up.html",
    "default_title": "Open contributor"
  },
  "permissions": [
    "storage",
    "history",
    "alarms"
  ],
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "128": "images/logo_128.png"
  },
  "default_locale": "fr",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com https://browser.sentry-cdn.com; object-src 'self'",
  "manifest_version": 2
}
