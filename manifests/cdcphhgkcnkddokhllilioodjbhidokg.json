{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "UniPrint Mint Beta for Chromebooks",
  "short_name": "UP Mint Beta for Chromebooks",
  "description": "Print to your personal printers without going through the Google cloud",
  "version": "0.1.13",
  "minimum_chrome_version": "44",
  "background": {
    "scripts": [
      "jquery-2.2.0.min.js",
      "rc4.js",
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "assets/MintBeta16.png",
    "32": "assets/MintBeta32.png",
    "64": "assets/MintBeta64.png",
    "128": "assets/MintBeta128.png"
  },
  "permissions": [
    "printerProvider",
    "identity",
    "identity.email",
    "storage",
    "background",
    "http://*/*",
    "https://*/*"
  ]
}
