{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Adds Do Not Track Header to all requests.",
  "manifest_version": 2,
  "icons": {
    "128": "128.png",
    "48": "48.png"
  },
  "name": "Do Not Track Header",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "version": "0.0.1"
}
