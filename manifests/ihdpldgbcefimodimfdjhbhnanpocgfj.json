{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Copy URL text",
  "description": "A tool to decode URL when copying links from a web page.",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "oncopy.js"
      ]
    }
  ]
}
