{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.3.0",
  "name": "Wapsi - Facilitador de Aportaciones",
  "manifest_version": 2,
  "description": "No pierdas ninguna aportaci√≥n gratuita con el Facilitador de Aportaciones",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "author": "Equilibri/Pedro Agullo Soliveres, http://wapsi.org y http://pedroagullo.com",
  "short_name": "Facilitador de Aportaciones",
  "permissions": [
    "https://www.wapsi.org/*",
    "webRequestBlocking",
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "wapsi_content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "wapsi_background.js"
    ]
  },
  "browser_action": {
    "default_icon": "32-grey.png"
  }
}
