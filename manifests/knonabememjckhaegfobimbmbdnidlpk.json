{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_chrome_app_name__",
  "version": "1.1.25",
  "description": "__MSG_chrome_app_description__",
  "permissions": [
    "history",
    "management",
    "bookmarks",
    "*://*/*",
    "tabs",
    "cookies"
  ],
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "default_locale": "zh_CN",
  "background": {
    "scripts": [
      "eventPage.js"
    ]
  },
  "icons": {
    "128": "qileke_logo.png",
    "16": "16.png"
  },
  "manifest_version": 2
}
