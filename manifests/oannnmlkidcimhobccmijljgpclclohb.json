{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Magic Mirror List for Trello",
  "description": "Mirror the list of one board on another",
  "version": "1.13.0",
  "icons": {
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/b/*"
      ],
      "js": [
        "js/jquery-1.9.1.min.js",
        "js/content.js"
      ]
    }
  ]
}
