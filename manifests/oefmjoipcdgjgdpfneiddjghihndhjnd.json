{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Newspaper Viet Times",
  "description": "Newspaper Viet Times. Reading news on chrom extension",
  "version": "4.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "activeTab",
    "alarms",
    "cookies",
    "history",
    "notifications",
    "browsingData",
    "https://ajax.googleapis.com/"
  ]
}
