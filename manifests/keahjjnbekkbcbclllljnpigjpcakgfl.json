{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "commnpo BigBlueButton Screenshare Extension",
  "author": "IVR BigBlueButton",
  "version": "0.0.772",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Chrome Extension granting WebRTC screen capturing capabilities to BigBlueButton meetings.",
  "homepage_url": "https://bbb.commnpo.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "*://*.commborg.com/*",
      "*://*.commnpo.com/*",
      "*://*.comminno.com/*",
      "*://*.cloudwiz.net/*"
    ]
  },
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
