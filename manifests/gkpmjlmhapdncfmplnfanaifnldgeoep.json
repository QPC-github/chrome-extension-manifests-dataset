{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "https://*.directorsdesk.com/*"
      ]
    }
  ],
  "description": "Directors Desk SecureView extension for Google Chrome",
  "icons": {
    "128": "ndaq_logo_128.png",
    "48": "ndaq_logo_48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Directors Desk SecureView for Google Chrome",
  "permissions": [
    "nativeMessaging",
    "tabs",
    "activeTab",
    "background"
  ],
  "short_name": "Directors Desk SecureView",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.41.0",
  "web_accessible_resources": [
    "detect.js"
  ]
}
