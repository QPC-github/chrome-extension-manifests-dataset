{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DabbaDirect",
  "description": "This extension allows you to make an order from any UK online store and have it delivered to Gibraltar.",
  "version": "1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "tabs"
  ]
}
