{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ФОТОЛЮБИТЕЛИ",
  "version": "1.0",
  "description": "Parser sites",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png"
  },
  "browser_action": {
    "default_title": "ФОТОЛЮБИТЕЛИ",
    "default_icon": "img/16.png",
    "default_popup": "main.html"
  }
}
