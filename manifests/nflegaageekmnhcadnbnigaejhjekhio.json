{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remind Me Later",
  "description": "Now never forget what you have planned.",
  "version": "1.0.1.5",
  "short_name": "RML",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "main.html",
      "container": "tab"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "128": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "background",
    "notifications"
  ]
}
