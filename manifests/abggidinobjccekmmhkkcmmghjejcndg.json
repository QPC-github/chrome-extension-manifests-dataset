{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "ZodiaLc",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "app.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "web_accessible_resources": [
    "jquery.js",
    "popup.js"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Гороскоп",
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "notifications"
  ]
}
