{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kestrel Notifier",
  "description": "Notifies you of interviews, trainings, meetings, etc.",
  "version": "0.7",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Look for events"
  },
  "permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
