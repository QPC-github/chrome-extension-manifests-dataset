{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Week Number",
  "version": "2.000",
  "description": "Displays week numbers",
  "icons": {
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Week Number",
    "default_icon": "128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "alarms",
    "storage"
  ],
  "manifest_version": 2
}
