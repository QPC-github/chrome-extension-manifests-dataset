{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "d20 SRD Sanitizer",
  "description": "This stops all the annoying notices on the d20 pathfinder srd site",
  "version": "1.0",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.d20pfsrd.com/*",
        "https://*.d20pfsrd.com/*"
      ],
      "js": [
        "jquery.js",
        "content.js"
      ]
    }
  ]
}
