{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TranZact - Tally Integration",
  "short_name": "TranZact",
  "manifest_version": 2,
  "description": "Tool for syncing TranZact data into your Tally Software",
  "version": "3.0.0.94",
  "browser_action": {
    "default_title": "Tranzact!",
    "default_icon": "images/tranzact.png"
  },
  "icons": {
    "16": "images/tranzact_16.png",
    "48": "images/tranzact_48.png",
    "128": "images/tranzact_128.png"
  },
  "background": {
    "scripts": [
      "system_files/background.js"
    ]
  },
  "permissions": [
    "http://localhost/*",
    "https://training.letstranzact.com/*",
    "https://training.letstranzact.net/*",
    "https://fcbtechnologies.com/*",
    "https://staging1.letstranzact.net/*",
    "https://staging2.letstranzact.net/*",
    "https://staging3.letstranzact.net/",
    "http://staging4.letstranzact.net/",
    "https://staging5.letstranzact.net/",
    "https://staging6.letstranzact.net/",
    "https://staging7.letstranzact.net/",
    "https://staging8.letstranzact.net/",
    "https://demo.letstranzact.com/",
    "https://app.letstranzact.com/",
    "http://127.0.0.1:8000/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
