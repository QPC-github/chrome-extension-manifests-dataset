{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Guild 1Stop",
  "description": "Grab this browser extension for quick access to Guild and University links from within Google Chrome.",
  "version": "1.2",
  "homepage_url": "http://www.uwastudentguild.com/",
  "options_page": "options.html",
  "icons": {
    "128": "blue_128.png"
  },
  "browser_action": {
    "default_icon": "icon_sm.png",
    "default_popup": "popup.html",
    "default_title": "Guild 1Stop"
  },
  "permissions": [
    "storage",
    "http://www.uwastudentguild.com/*"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
