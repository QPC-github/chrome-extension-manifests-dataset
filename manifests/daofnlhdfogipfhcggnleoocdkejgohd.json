{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Encrypted Chat (beta)",
  "description": "Encrypted Chat (beta)",
  "version": "0.0.8",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "storage",
    "notifications",
    "https://www.gravatar.com/avatar/*"
  ],
  "icons": {
    "16": "images/chat-16a.png",
    "128": "images/chat-128a.png"
  }
}
