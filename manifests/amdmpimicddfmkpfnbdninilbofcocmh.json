{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide That",
  "version": "0.2.1",
  "description": "Easily hide unwanted elements on the current page.",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "enable-hide": {
      "suggested_key": {
        "default": "Alt+Z"
      },
      "description": "Toggle rightclick-to-hide"
    }
  }
}
