{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Thyself",
  "version": "1.3.1",
  "description": "Master your mental health. Mood tracking, journal prompts, self-care, intention setting all in one place.",
  "permissions": [
    "tabs",
    "storage",
    "*://*.thyself.me/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "pages/options.html",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png"
    },
    "default_title": "Thyself (click to see your moods)"
  },
  "manifest_version": 2
}
