{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "煎蛋妹子图小助手",
  "description": "帮你快速查看妹子图",
  "version": "1.2.0",
  "permissions": [
    "http://jandan.net/",
    "http://jandan.net/ooxx/",
    "http://jandan.net/pic",
    "storage",
    "tabs",
    "alarms"
  ],
  "background": {
    "scripts": [
      "js/lib/jquery.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo/icon128.png"
  },
  "options_page": "options.html",
  "icons": {
    "16": "logo/icon16.png",
    "48": "logo/icon64.png",
    "128": "logo/icon128.png"
  }
}
