{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ruralbit IFAP form filler",
  "version": "1.8",
  "description": "Extensão para preenchimento do formulário de nascimentos do IFAP com dados das aplicações Ruralbit.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "webNavigation",
    "tabs"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://www.ruralbit.com https://ruralbit.com; object-src 'self'",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "imagens/logo-16.png",
      "32": "imagens/logo-32.png",
      "48": "imagens/logo-48.png",
      "128": "imagens/logo-128.png"
    }
  },
  "icons": {
    "16": "imagens/logo-16.png",
    "32": "imagens/logo-32.png",
    "48": "imagens/logo-48.png",
    "128": "imagens/logo-128.png"
  },
  "manifest_version": 2
}
