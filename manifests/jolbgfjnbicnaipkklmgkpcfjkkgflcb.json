{
  "background": {
    "scripts": [
      "lib/jquery-3.1.0.min.js",
      "const.js",
      "auth.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Group Extractor"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "lib/sweetalert2.css",
        "main.css"
      ],
      "js": [
        "lib/jquery-3.1.0.min.js",
        "lib/sweetalert.min.js",
        "text.js",
        "main.js"
      ],
      "matches": [
        "https://www.facebook.com///requests/",
        "https://www.facebook.com///requests",
        "https://www.facebook.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Extract facebook group members information when they join your group",
  "icons": {
    "128": "icon.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Group Extractor",
  "oauth2": {
    "client_id": "424989431661-avddjrtuhfmv3s3bcsvqq0pmglmc0j6n.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1.19",
  "web_accessible_resources": [
    "*.png"
  ]
}
