{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "School Bell Reminder",
  "short_name": "SCB",
  "description": "The School Bell Reminder is a chrome extension that is able to tell and warn users of the users next period.",
  "background": {
    "scripts": [
      "/Main_System/Backgroundscripts/systemchangelistener.js",
      "/Main_System/Backgroundscripts/notification.js",
      "/Main_System/Backgroundscripts/timescriptupdater.js"
    ]
  },
  "version": "13.0.2",
  "minimum_chrome_vesion": "38",
  "icons": {
    "128": "/Resources/Images/School_Bell_Reminder_Logo.png"
  },
  "permissions": [
    "storage",
    "identity",
    "alarms",
    "notifications",
    "background",
    "https://docs.google.com/"
  ],
  "offline_enabled": true,
  "omnibox": {
    "keyword": "sbr"
  },
  "browser_action": {
    "default_icon": "/Resources/Images/School_Bell_Reminder_Logo.png",
    "default_title": "School Bell Reminder",
    "default_popup": "/Startup_Setup/Basic_Info/basic_info.html"
  }
}
