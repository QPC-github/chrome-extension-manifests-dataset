{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Aviso de Cumpleaños",
  "version": "1.0.1.3",
  "description": "Comprueba si alguien cumple años hoy. Configura tu propia lista de cumpleaños.",
  "icons": {
    "16": "icono.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "jquery/jquery-1.11.1.min.js",
      "background.min.js",
      "languages.min.js"
    ]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icono.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "48.png"
  ]
}
