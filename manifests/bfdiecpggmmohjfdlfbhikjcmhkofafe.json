{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Milkman Inspector",
  "short_name": "Milkman",
  "description": "Debugging and profiling tools for Milkman",
  "version": "1.0.0",
  "devtools_page": "lib/devtools.html",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "lib/background.js"
    ]
  },
  "icons": {
    "16": "milkman.png",
    "48": "milkman.png",
    "128": "milkman.png"
  }
}
