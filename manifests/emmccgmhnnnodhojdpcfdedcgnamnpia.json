{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yandex Wordstat парсер сезонности по областям",
  "description": "Сбор частотностей по списку запросов",
  "version": "3.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "r16.png",
    "48": "r48.png",
    "128": "r128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "downloads",
    "cookies",
    "<all_urls>",
    "activeTab"
  ]
}
