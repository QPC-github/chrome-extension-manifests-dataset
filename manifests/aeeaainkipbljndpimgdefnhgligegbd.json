{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Bookmarks",
  "version": "0.0.3",
  "description": "Extension that launches sites you wanted to view later",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "L2.js"
    ]
  },
  "icons": {
    "128": "betterBookmarksLogo.png"
  },
  "browser_action": {
    "default_icon": "betterBookmarksLogoSingle.png",
    "default_title": "BetterBookmarks"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "background",
    "bookmarks",
    "alarms",
    "notifications",
    "contextMenus"
  ]
}
