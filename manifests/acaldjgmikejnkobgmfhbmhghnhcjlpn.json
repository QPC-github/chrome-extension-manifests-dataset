{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Order Items Chrome Extension",
  "version": "0.1",
  "description": "Makes the last purchase info column more visible.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.netsuite.com/app/accounting/transactions/orderitems.nl?whence="
      ],
      "js": [
        "jquery.js",
        "main.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
