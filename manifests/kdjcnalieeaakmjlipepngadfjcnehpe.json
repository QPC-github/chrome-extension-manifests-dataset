{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab Limit",
  "description": "Limit the number of open browser tabs",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/19s.png",
      "38": "icons/38s.png"
    },
    "default_title": "Tab Limit"
  },
  "icons": {
    "16": "icons/16s.png",
    "48": "icons/48s.png",
    "128": "icons/128s.png"
  },
  "manifest_version": 2
}
