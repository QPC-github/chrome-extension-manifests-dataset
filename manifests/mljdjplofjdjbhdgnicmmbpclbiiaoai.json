{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "No More Trending",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.PNG",
    "48": "icon.PNG",
    "16": "icon.PNG"
  },
  "description": "Short extension to block trendings tab from Youtube (cause it's cancer)",
  "browser_action": {
    "default_icon": "icon.PNG",
    "default_popup": "popup.html",
    "default_title": "Hide Logan Paul Vlogs"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ]
}
