{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Start Tab",
  "description": "Start Tab - is a convenient tree-like tabs with visual display.",
  "version": "2.4",
  "permissions": [
    "storage",
    "bookmarks",
    "identity",
    "idle",
    "webRequest",
    "webRequestBlocking",
    "pageCapture",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_title": "Start Tab",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
