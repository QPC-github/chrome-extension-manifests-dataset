{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clickable Links",
  "version": "2.4",
  "manifest_version": 2,
  "description": "Turns unclickable urls & email addresses into clickable ones.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_globs": [
        "*://*.google.*/*",
        "*://*.bing.*/*",
        "http://*.acidtests.org/*",
        "*://*twitter.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "jquery.ba-replacetext.js",
        "clickable_links.js"
      ]
    }
  ]
}
