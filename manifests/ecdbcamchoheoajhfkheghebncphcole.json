{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spiderman Wallpapers HD New Tab Theme",
  "description": "New Tab themes with Spiderman HD wallpapers made by fans, for fans of Spiderman",
  "version": "1.0",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "scripts/purple.js",
      "scripts/green.js",
      "scripts/red.js",
      "scripts/yellow.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "icons/i-128.png"
    }
  },
  "icons": {
    "128": "icons/i-128.png"
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "topSites",
    "chrome://favicon/*",
    "http://*/*",
    "https://*/*"
  ]
}
