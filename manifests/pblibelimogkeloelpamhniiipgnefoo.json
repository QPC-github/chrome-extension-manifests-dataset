{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Saltar Modal de Clarín",
  "short_name": "saltearmodal2",
  "version": "0.1.7",
  "description": "Elimina el temerario paso de registración exigido por Clarín para acceder a sus notas.",
  "author": "Marcelo Zanni",
  "icons": {
    "16": "saltearmodal216.png",
    "48": "saltearmodal248.png",
    "128": "saltearmodal2128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.clarin.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "manifest_version": 2
}
