{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GroupUp Browser Extension",
  "short_name": "GroupUp.me",
  "description": "Save and Share with the GroupUp Browser Extension",
  "version": "1.83",
  "permissions": [
    "contextMenus",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "groupup.js"
    ]
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.groupup.me/*",
        "https://groupup.me/*"
      ],
      "js": [
        "hasextension.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "dist/*",
    "pub/*"
  ],
  "content_security_policy": "script-src 'self' https://*.groupup.me 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
