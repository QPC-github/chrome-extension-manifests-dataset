{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SSO Passport",
  "version": "1.0.10",
  "manifest_version": 2,
  "description": "This extension gives you seamless and secure access to all of your web-based teaching and learning resources",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "SSO Passport"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ]
}
