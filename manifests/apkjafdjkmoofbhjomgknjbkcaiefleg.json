{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FutaKuro",
  "version": "3.55.285",
  "description": "ふたばちゃんねる閲覧ビューア",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "https://futakuro.com/",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/jquery.easing.js",
        "js/jquery.plugin.js",
        "js/viewer_new.js",
        "js/tiny.js",
        "js/encoding.js",
        "js/jquery-ui-selectable.min.js",
        "js/regex.js",
        "js/blowfish.js"
      ],
      "css": [
        "css/style.css"
      ],
      "matches": [
        "http://*.2chan.net/*",
        "https://*.2chan.net/*",
        "http://futaba.qs.cjb.net/*",
        "http://futapo.futakuro.com/*",
        "https://futapo.futakuro.com/*",
        "http://*.futakuro.com/*",
        "https://*.futakuro.com/*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "icon/icon128.png",
    "48": "icon/icon48.png",
    "32": "icon/icon32.png",
    "16": "icon/icon16.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon19.png",
      "38": "icon/icon38.png"
    },
    "default_title": "FutaKuro",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "notification.html",
    "notification2.html",
    "options.html",
    "nglist.html",
    "css/style.css",
    "icon/icon128.png",
    "icon/icon16.png",
    "img/close.png",
    "img/del.png",
    "img/futakuro.png",
    "img/icon1.png",
    "img/icon2.png",
    "img/icon3.png",
    "img/loading.gif",
    "img/maxi.png",
    "img/mini.png",
    "img/quote.png",
    "img/search.png",
    "img/thumb_404.png",
    "img/title.png",
    "img/write.png",
    "img/alert-hr.png",
    "clipboard.swf",
    "img/tp.png",
    "js/jszip.js",
    "img/ng.png",
    "js/oejs.js"
  ],
  "content_security_policy": "default-src 'self'; img-src 'self' http://* https://* data:; style-src 'self' http://* https://* 'unsafe-inline' ; connect-src 'self' http://* https://* ws://*; frame-src 'self' http://* https://* http://localhost:*/; media-src 'self' http://* https://*;script-src 'self' 'unsafe-eval';",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "notifications",
    "downloads"
  ]
}
