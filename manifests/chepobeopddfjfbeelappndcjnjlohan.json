  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Trumped",
  "description": "Replace references to Donald Trump with other accurate descriptors",
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
