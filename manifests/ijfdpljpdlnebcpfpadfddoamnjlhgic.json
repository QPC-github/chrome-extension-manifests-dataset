{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TheAmplituhedron",
  "description": "Visit TheAmplituhedron social media accounts and view the newest posts in chronological order.",
  "version": "2.0",
  "manifest_version": 2,
  "permissions": [
    "http://www.theamplituhedron.com/",
    "https://www.theamplituhedron.com/",
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "default_title": "TheAmplituhedron",
    "default_popup": "extensionMain.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+T",
        "mac": "MacCtrl+Shift+T"
      },
      "description": "Shortcut to the last action."
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.theamplituhedron.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
