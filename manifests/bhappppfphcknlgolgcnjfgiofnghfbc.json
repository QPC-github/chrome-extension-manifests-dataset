{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "The Current State of Knowledge",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Part of an ongoing project to catalogue what it is that we don't know",
  "background": {
    "scripts": [
      "background.html"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js.js"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ]
}
