{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Project Refit",
  "discription": "Connects Verteran to Veteran in an online chat",
  "version": "2.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Project Refit"
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "tabs",
    "windows"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}
