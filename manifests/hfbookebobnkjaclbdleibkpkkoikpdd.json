{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Daily url",
  "description": "Open specified urls once a day",
  "version": "1.4",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "Daily url"
  },
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
