{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Jump2Connect",
  "short_name": "gotoConnect",
  "author": "Renars Rozitis",
  "version": "1.4",
  "description": "Look up a 24-digit service request in Ariba Connect",
  "icons": {
    "128": "networking.png",
    "64": "networking.png",
    "48": "networking.png",
    "32": "networking.png",
    "24": "networking.png",
    "16": "networking.png"
  },
  "background": {
    "persisten": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "32": "networking.png"
    },
    "default_title": "Look up SR in Connect"
  },
  "omnibox": {
    "keyword": "c"
  }
}
