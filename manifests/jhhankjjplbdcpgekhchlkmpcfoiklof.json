{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Image to Markdown",
  "version": "1.8",
  "short_name": "img2markdown",
  "description": "Convert image to base64 for markdown or other purpose\n https://github.com/Arryboom/img2base64",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "6.0.0.0",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 2
}
