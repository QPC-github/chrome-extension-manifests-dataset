{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Techlearning Classroom",
  "author": "Sandeep Kumar Tiwary",
  "version": "2.1",
  "manifest_version": 2,
  "description": "Techlearning Classroom for attend class online",
  "homepage_url": "https://techlearning.in/app/home",
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://techlearning.in/*",
      "https://standupmeet.herokuapp.com/*",
      "*://localhost/*"
    ]
  },
  "icons": {
    "48": "logo.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "logo.png"
  ]
}
