{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube PopOut",
  "manifest_version": 2,
  "description": "Pop out a youtube video into panel window from a YouTube video page",
  "version": "0.0.0.4",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "http://youtube.com/*"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  }
}
