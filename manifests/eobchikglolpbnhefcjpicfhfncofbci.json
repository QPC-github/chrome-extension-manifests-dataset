{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ingress Intel Improvement [BETA]",
  "description": "Map and Comm improvement for the Ingress Intel Map",
  "version": "1.3",
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.ingress.com/intel*"
      ],
      "css": [
        "style.css",
        "bootstrap.css"
      ],
      "js": [
        "jquery.js",
        "cookie.js",
        "optimizer.js"
      ]
    }
  ],
  "icons": {
    "16": "img/icons/icon_xsm.png",
    "48": "img/icons/icon_sm.png",
    "128": "img/icons/icon_md.png"
  },
  "web_accessible_resources": [
    "img/icons/*"
  ]
}
