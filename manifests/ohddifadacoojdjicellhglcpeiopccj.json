{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Virtualbania",
  "version": "1.0.0",
  "description": "Extension for virtualbania.",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon-16x16.png",
    "default_title": "Virtualbania",
    "default_popup": "popup_browser.html"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
