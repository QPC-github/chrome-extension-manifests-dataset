{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Price Hunter - Wish4This",
  "description": "Track Amazon, eBay, Bestbuy and websites that YOU request every month. Track any product you love on any site!",
  "version": "2.1.0.4",
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "activeTab",
    "cookies",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Wish4This Product Tracking"
  }
}
