{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "No LDS Footnote Letters",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Removes the footnote superscript letters at lds.org",
  "icons": {
    "128": "script.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.lds.org/scriptures/*"
      ],
      "js": [
        "no_lds_footnote_letters.js"
      ]
    }
  ]
}
