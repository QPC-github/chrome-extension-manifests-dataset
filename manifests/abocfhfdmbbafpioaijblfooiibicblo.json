{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "http://mail.google.com/*"
      ],
      "run_at": "document_end",
      "css": [
        "css/main.css",
        "css/gdropdown.css",
        "css/toastr.css",
        "lib/vendors/chosen/chosen.css"
      ],
      "js": [
        "insights-min.js"
      ]
    }
  ],
  "name": "Collabspot: SugarCRM 6 for Gmail",
  "default_locale": "en",
  "web_accessible_resources": [
    "manifest.json",
    "templates_sugar.html",
    "lib/vendors/jquery.1.4.2.js",
    "lib/vendors/underscore.js",
    "lib/vendors/gmail.js",
    "lib/observer.js",
    "img/handle.png",
    "img/global_nav_sprite.png",
    "img/company_avat.png",
    "img/closelabel.png",
    "img/black-001-loading.gif",
    "img/glyphicons_192_circle_remove.png",
    "img/glyphicons_216_circle_arrow_left.png",
    "img/glyphicons_003_user.png",
    "img/glyphicons_194_circle_question_mark.png",
    "img/glyphicons_003_user_white.png",
    "img/glyphicons_027_search.png",
    "img/glyphicons_010_envelope.png",
    "img/glyphicons_045_calendar.png",
    "img/glyphicons_152_check.png",
    "img/glyphicons_190_circle_plus.png",
    "img/glyphicons_190_circle_plus.png",
    "img/contact_tab.gif",
    "img/icons-color.png",
    "img/icons-gray.png",
    "img/toggle.png",
    "img/icons_ns10.png",
    "img/highrise_sprites.png",
    "img/icon48.png",
    "img/icon40.png",
    "img/next.gif",
    "img/prev.gif",
    "img/sprite.png",
    "img/person.gif",
    "css/main.css",
    "img/email.png",
    "img/gmail.png",
    "img/highrise.png",
    "img/rapportive.png",
    "img/edit.png",
    "img/scroll.png",
    "css/jquery.ui.bootstrap.css",
    "img/arrow_down.png",
    "img/email_tpl.jpg",
    "img/glyphicons_150_edit.png",
    "img/glyphicons_193_circle_ok.png",
    "img/sugar_sprite.png",
    "img/bg_glass_75_ffffff_1x400.png",
    "img/account_select.png",
    "img/welcome_alert.png",
    "img/loading_small.gif",
    "img/twitter_icon.png",
    "img/glyphicons_043_group.png",
    "img/glyphicons_043_group_white.png",
    "img/facebook_icon.png",
    "img/linkedin_icon.png",
    "img/glyphicons_040_stats.png",
    "img/avatar.gif",
    "img/icon-hand-up.png",
    "img/icon-eye-open.png",
    "img/icon-mail-reply.png",
    "img/glyphicons_123_message_out.png",
    "img/glyphicons_136_cogwheel.png",
    "img/opp_avat.png",
    "img/case_avat.png",
    "img/glyphicons_081_refresh.png",
    "img/glyphicons_223_chevron-right.png",
    "img/glyphicons_224_chevron-left.png"
  ],
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png",
    "48": "img/icon48_real.png"
  },
  "version": "2019.12.19.1022",
  "background": {
    "scripts": [
      "back-insights-min.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "alarms",
    "management",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "https://collabspot-sugar.appspot.com/",
    "http://*/*",
    "https://*/*"
  ],
  "description": "Collabspot is a time-saving tool for SugarCRM users in Gmail."
}
