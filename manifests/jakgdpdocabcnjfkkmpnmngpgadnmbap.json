{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ynet",
  "description": "Gets rid of Ynet's nagging AdBlock message",
  "icons": {
    "128": "logo.png"
  },
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.ynet.co.il/*"
      ],
      "js": [
        "ynetscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.ynet.co.il/*"
  ]
}
