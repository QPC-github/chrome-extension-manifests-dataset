{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Email this page (ver I. DUMAIRE)",
  "description": "This extension adds an email button to the toolbar.You can set a default email address.The selected text is added inside the email.",
  "version": "1.2.6.3",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "mail_128x128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Email this page !",
    "default_icon": "email_16x16.png"
  },
  "manifest_version": 2
}
