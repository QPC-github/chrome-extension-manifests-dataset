{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Black Market Time Tracking",
  "version": "1.0",
  "description": "Time tracking extension for Blkmktx.com",
  "icons": {
    "48": "icons/clock.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "alarms"
  ],
  "background": {
    "page": "background/timer.html"
  },
  "browser_action": {
    "default_icon": "icons/clock.png",
    "default_title": "Black Market Time Tracking",
    "default_popup": "popup/index.html"
  }
}
