{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Duplication Killer",
  "version": "0.0.1",
  "description": "If the url of tab you opened is the same as one you've already opened, close the new tab and activate the old one.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "manifest_version": 2,
  "author": "@p_craft"
}
