{
  "background": {
    "persistent": false,
    "scripts": [
      "lib/jquery.js",
      "lib/q.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_scripts.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "images/icon19.png"
  },
  "options_page": "options.html",
  "description": "Mass Liker Authorize Profile",
  "manifest_version": 2,
  "name": "Mass Liker Authorize Profile",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
