{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "unforget.io",
  "short_name": "unforget.io",
  "description": "Unforgettable bookmarks and notes",
  "version": "4.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "unforget"
  },
  "permissions": [
    "activeTab"
  ]
}
