{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BookmarkVault",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "Safe and easy bookmark storage",
  "icons": {
    "128": "img/logo128.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "img/logo128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "identity",
    "activeTab"
  ],
  "applications": {
    "gecko": {
      "id": "bookmarkvault@zookatron.com"
    }
  }
}
