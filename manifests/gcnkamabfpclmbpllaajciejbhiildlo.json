{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Degreener",
  "version": "1.0.2",
  "description": "Many foods contain junk you don't need - kind of like the internet. Cleanse your feed of overused references to some old movie.",
  "icons": {
    "48": "Degreener-favIcon.jpg",
    "128": "Degreener-Icon.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "degreen.js"
      ],
      "run_at": "document_end"
    }
  ]
}
