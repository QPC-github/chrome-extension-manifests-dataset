{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SEMSTORM: keywords",
  "description": "SEMSTORM: keywords is a free keyword tool which allows to get more keyword suggestions from Google.",
  "version": "1.0.2",
  "homepage_url": "https://www.semstorm.com",
  "icons": {
    "128": "img/semstorm-128.png",
    "48": "img/semstorm-48.png",
    "24": "img/semstorm-24.png",
    "16": "img/semstorm-16.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "32": "img/semstorm-32.png"
    },
    "default_title": "SEMSTORM keywords",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "cache.js",
        "util.js",
        "common.js",
        "api.js",
        "content/keywords_panel.class.js",
        "content/plugins/serp_keywords.js",
        "content/plugins/gsc_data.js",
        "content/plugins/keywords_panel.js"
      ],
      "css": [
        "icons.css",
        "common.css",
        "content/keywords_panel.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "util.js",
      "cache.js",
      "background/context_actions.js",
      "background/events.js",
      "background/api_requests.js"
    ]
  },
  "web_accessible_resources": [
    "img/*.png",
    "img/*.jpg",
    "data/*.json",
    "pages/*"
  ],
  "permissions": [
    "storage",
    "contextMenus",
