{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Springfield Search FTK",
  "description": "Help raise money FTK through GoodSearch",
  "author": "DJHerls",
  "version": "1.4.4",
  "browser_action": {
    "default_icon": "icons/springfield_32.png"
  },
  "icons": {
    "16": "icons/springfield_16.png",
    "32": "icons/springfield_32.png",
    "48": "icons/springfield_48.png",
    "128": "icons/springfield_128.png",
    "1700": "icons/springfield_1700.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "*://*.goodsearch.com/*",
    "*://*.goodshop.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
