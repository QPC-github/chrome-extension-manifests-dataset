{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Email Access",
  "short_name": "Quick Email Access",
  "version": "1.0.3",
  "description": "Quick Access to different Email clients and services..",
  "homepage_url": "http://wawsearch.com/",
  "manifest_version": 2,
  "permissions": [
    "topSites",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [],
      "encoding": "UTF-8",
      "favicon_url": "http://www.wawsearch.com/images/web-logo.png",
      "instant_url": "http://www.wawsearch.com/search.php?search={searchTerms}&type=web",
      "instant_url_post_params": "",
      "is_default": true,
      "keyword": "wawsearch.com",
      "name": "",
      "search_url": "http://www.wawsearch.com/search.php?search={searchTerms}&type=web",
      "search_url_post_params": ""
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "minimum_chrome_version": "22.0.0.0",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
