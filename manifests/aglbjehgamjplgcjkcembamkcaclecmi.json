{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Show Password",
  "author": "smeraner",
  "version": "1.0.1",
  "description": "Forgotten your password? Show password of password field. Source Code can be found in Github.",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "icons": {
    "16": "images/img16.png",
    "32": "images/img32.png",
    "48": "images/img48.png",
    "128": "images/img128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
