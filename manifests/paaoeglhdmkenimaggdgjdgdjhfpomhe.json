{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "COZA iRadio",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "page": "player.html"
  },
  "description": "COZA iRadio - Streaming the gospel of Jesus Christ to the ends of the earth!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "radio.html"
  },
  "permissions": [
    "notifications",
    "contextMenus"
  ]
}
