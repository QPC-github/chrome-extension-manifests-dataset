{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ninja Run",
  "short_name": "Play Ninja Run game",
  "description": "With a ninja very skillful, run by the scenario, jump across platforms and survive the deadly obstacles that arise along the way.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "ninjarun128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "ninjarun128.png",
    "48": "ninjarun128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
