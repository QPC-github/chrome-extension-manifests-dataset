{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extensi√≥n Fiscal Neotec",
  "version": "1.5.1",
  "description": "Extension para impresion fiscal atravez de odoo y la interfaz",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/axios.js",
      "js/protocol.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assents/jquery.js",
        "js/content.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assents/images/neotec1.png",
      "32": "assents/images/neotec1.png",
      "48": "assents/images/neotec1.png",
      "128": "assents/images/neotec1.png"
    }
  },
  "icons": {
    "16": "assents/images/neotec1.png",
    "32": "assents/images/neotec1.png",
    "48": "assents/images/neotec1.png",
    "128": "assents/images/neotec1.png"
  },
  "manifest_version": 2
}
