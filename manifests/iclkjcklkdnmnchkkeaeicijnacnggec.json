{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "js/jquery-2.2.1.min.js",
      "js/bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon19_no.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ad-core.ru/*",
        "https://www.instagram.com/*",
        "https://youtu.be/*",
        "https://www.youtube.com/*",
        "https://m.youtube.com/*"
      ],
      "js": [
        "js/jquery-2.2.1.min.js",
        "js/addad.js"
      ],
      "all_frames": true,
      "css": [],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ad-core.ru/; object-src 'self'",
  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "version": "0.98",
  "name": "AdCore",
  "short_name": "AdCore",
  "description": "Расширяет возможности заработка и взаимодействия с проектом.",
  "homepage_url": "https://ad-core.ru/",
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "permissions": [
    "activeTab",
    "https://ad-core.ru/ajax/call.php"
  ],
  "web_accessible_resources": [
    "css/*",
    "js/addad.js"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.ad-core.ru/*"
    ]
  }
}
