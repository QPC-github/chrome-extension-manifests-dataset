{
  "browser_action": {
    "default_icon": "icons/dark/next.png",
    "default_title": "Play next track"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Tracksflow like track button",
  "background": {
    "scripts": [
      "extension.js",
      "background.js"
    ]
  },
  "default_locale": "en",
  "name": "Tracksflow Audio Player Next",
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png",
    "32": "icons/32.png",
    "24": "icons/24.png",
    "16": "icons/16.png"
  },
  "manifest_version": 2,
  "version": "1.2.0"
}
