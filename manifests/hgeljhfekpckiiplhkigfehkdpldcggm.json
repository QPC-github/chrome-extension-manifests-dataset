{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "Icon/icon-32.png",
    "default_popup": "popup.html",
    "default_title": "Auto Refresh Plus"
  },
  "description": "Refresh page automatically, plus page monitor feature.",
  "icons": {
    "128": "Icon/icon-64.png",
    "64": "Icon/icon-64.png",
    "48": "Icon/icon-48.png",
    "32": "Icon/icon-32.png",
    "16": "Icon/icon-16.png"
  },
  "manifest_version": 2,
  "name": "Auto Refresh Plus - ARP",
  "short_name": "Auto Refresh Plus",
  "options_page": "options.html",
  "permissions": [
    "notifications"
  ],
  "optional_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "version": "7.1.9",
  "web_accessible_resources": [
    "blank.html"
  ]
}
