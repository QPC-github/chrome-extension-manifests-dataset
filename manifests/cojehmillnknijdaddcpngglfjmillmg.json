{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Simple History Manager",
  "version": "1.2.2",
  "author": "Guokai Han",
  "manifest_version": 2,
  "description": "A simple Chrome history manager.",
  "offline_enabled": true,
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "browser_action": {},
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "history",
    "storage",
    "sessions",
    "tabs",
    "chrome://favicon/"
  ]
}
