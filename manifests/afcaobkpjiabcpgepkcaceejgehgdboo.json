{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "jMilk",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Changes all the images on webpages to photos of Jo√£o Leite",
  "homepage_url": "http://cisneiros.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
