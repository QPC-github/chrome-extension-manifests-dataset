{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "css/mycss.css"
      ],
      "js": [
        "js/jquery-min.js",
        "js/myscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "128": "images/fastdomain128.png",
    "16": "images/fastdomain16.png",
    "48": "images/fastdomain48.png"
  },
  "name": "__MSG_extName__",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "1.3",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
