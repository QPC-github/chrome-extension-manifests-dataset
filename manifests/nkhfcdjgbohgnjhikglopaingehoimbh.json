{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Plugin Vulnerabilities",
  "description": "Adds warning message to WordPress Plugin Directory pages when plugins have one of a couple types of security issues.",
  "version": "1.0.24",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.wordpress.org/plugins/*",
        "https://*.wordpress.org/support/plugin/*",
        "https://*.wordpress.org/support/view/plugin-reviews/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://www.pluginvulnerabilities.com/?pk_campaign=Chrome"
}
