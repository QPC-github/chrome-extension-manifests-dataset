{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "/assets/javascripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "/assets/images/f.png",
    "default_popup": "popup.html",
    "default_title": "fireflyy"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "MacCtrl+Shift+D"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*,",
        "http://www.google.com/robots.txt*"
      ],
      "js": [
        "/vendor/javascripts/jquery-2.0.0.min.js",
        "/assets/javascripts/tab.js"
      ],
      "css": [
        "/assets/stylesheets/tab.css"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Save content to fireflyy.",
  "incognito": "spanning",
  "icons": {
    "16": "/assets/images/f.png",
    "128": "/assets/images/f128.png"
  },
  "manifest_version": 2,
  "name": "fireflyy",
  "omnibox": {
    "keyword": "@del"
  },
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "http://www.fireflyy.co/*",
    "http://fireflyy.co/*",
    "https://ajax.googleapis.com/*",
    "tabs"
  ],
  "version": "1.0.7",
  "web_accessible_resources": [
    "/popup.html"
  ]
}
