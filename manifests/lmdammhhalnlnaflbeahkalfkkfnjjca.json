{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Restores all unused reactions back to Facebook.",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png"
  },
  "manifest_version": 2,
  "name": "Unused Reactions for Facebook",
  "permissions": [
    "*://*.facebook.com/*"
  ],
  "version": "1.0",
  "web_accessible_resources": [
    "hook.js"
  ]
}
