{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Random Gradient",
  "description": "Sit back. Relax. Crack a cold one. Take in a gradient.",
  "version": "0.0.1",
  "manifest_version": 2,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "management"
  ],
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
