{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "openlab 백신 B",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "openlab에서 만든 IT백신 B! GS SHOP in NAVER 가격비교",
  "icons": {
    "16": "images/oici-B-16.png",
    "48": "images/oici-B-48.png",
    "128": "images/oici-B-128.png"
  },
  "browser_action": {
    "default_icon": "images/oici-B-48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "quip-extension.js",
    "sandbox.html",
    "quip-extension.css",
    "jquery.min.js",
    "jquery.ajax-cross-origin.min.js",
    "jquery.ui.css",
    "jquery.ui.js",
    "images/plus.png",
    "images/minus.png"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
