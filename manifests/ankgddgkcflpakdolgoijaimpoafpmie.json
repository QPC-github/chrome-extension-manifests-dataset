{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Imgur Numbering",
  "version": "0.0.1",
  "description": "Number imgur images.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://imgur.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "content-injected.js"
  ]
}
