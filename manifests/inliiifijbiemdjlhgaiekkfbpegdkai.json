{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "fnContact.com Form Notifier",
  "manifest_version": 2,
  "version": "1.0.2",
  "description": "Get notified whenever your fncontact forms get submissions.",
  "icons": {
    "16": "128.png",
    "48": "128.png",
    "128": "128.png"
  },
  "homepage_url": "https://fncontact.com",
  "permissions": [
    "https://fncontact.com/*",
    "notifications",
    "tabs"
  ],
  "web_accessible_resources": [
    "128.png"
  ],
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "fnContact.com Form Notifier"
  }
}
