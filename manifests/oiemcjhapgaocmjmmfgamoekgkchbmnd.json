{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RecLabs chrome extension",
  "version": "0.0.0.1",
  "description": "Extracts candidate information from linked in profile page",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "RecLabs"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}
