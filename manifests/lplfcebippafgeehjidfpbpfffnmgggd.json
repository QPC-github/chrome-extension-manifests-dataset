{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "RA Chocolate Cookie",
  "version": "1.2",
  "description": "Chrome extension to force Facebook logout when user reach https://www.respondeai.com.br/materias",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "icons": {
    "16": "images/icons/favicon-16x16.png",
    "48": "images/icons/favicon-32x32.png",
    "128": "images/icons/favicon.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "*://*.facebook.com/*",
    "*://*.google.com/*",
    "storage"
  ]
}
