{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "FB - Sidebar Badge Hover",
  "description": "Show the indicators on the left sidebar of your Facebook homepage only on hover.",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "css": [
        "pretty.css"
      ],
      "js": [
        "jquery.js",
        "hover.js"
      ]
    }
  ]
}
