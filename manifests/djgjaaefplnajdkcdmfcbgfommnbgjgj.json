{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Tahrir Squared Bookmarker",
  "version": "1.1.7",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "description": "Chrome extension to add content to Tahrir Squared",
  "page_action": {
    "default_icon": "icons/page_action_inactive.png",
    "default_title": "Add to Tahrir Squared",
    "default_popup": "bookmark.html"
  },
  "permissions": [
    "http://localhost/",
    "*://tahrirsquared.com/",
    "http://api.embed.ly/",
    "https://api.embed.ly/",
    "*://*.tahrirsquared.com/",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
