{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Spammiej's Extension",
  "description": "For remain informed about everything Spam's live | Features : Badge ON/OFF, Notification, PopUpSound, Setting's page.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "64": "./img/icon_64(1).png"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ]
  },
  "options_page": "options/options.html",
  "permissions": [
    "notifications",
    "storage"
  ]
}
