{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Ultracarweb Signer",
  "version": "1.6",
  "manifest_version": 2,
  "description": "Assinar e transmitir notas fiscais eletr√¥nicas.",
  "page_action": {
    "default_icon": "icon-128.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon-128.png"
  },
  "externally_connectable": {
    "matches": [
      "https://www.ultracarweb.com/*",
      "https://ultracarweb.com/*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ultracarweb.com/*",
        "https://ultracarweb.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "https://www.ultracarweb.com/",
    "https://ultracarweb.com/"
  ]
}
