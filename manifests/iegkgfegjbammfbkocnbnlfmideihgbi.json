{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Urban Tecno",
  "manifest_version": 2,
  "version": "0.3",
  "description": "Consulta los últimos artículos de Urban Tecno.",
  "icons": {
    "128": "images/f128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "bg.js"
    ]
  },
  "permissions": [
    "https://*.urbantecno.com/"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "images/f48.png",
    "default_popup": "popup.html"
  }
}
