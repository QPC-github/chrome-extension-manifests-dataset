{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Browserling - Cross-browser testing",
  "short_name": "Browserling",
  "version": "1.0.2",
  "icons": {
    "128": "icons/icon-128x128.png",
    "48": "icons/icon-48x48.png",
    "16": "icons/icon-16x16.png"
  },
  "description": "Cross-browser test websites in the most popular browsers such as Internet Explorer, Chrome, Firefox, Safari and Opera. Try for free!",
  "homepage_url": "https://www.browserling.com/",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icons/icon-19x19.png",
      "38": "icons/icon-76x76.png"
    },
    "default_title": "Browserling - Cross-browser Testing"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
