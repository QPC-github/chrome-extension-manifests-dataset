{
  "browser_action": {
    "default_icon": "pluslogo.png"
  },
  "content_scripts": [
    {
      "js": [
        "background.js"
      ],
      "matches": [
        "https://services.gst.gov.in/services/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Check GSTIN extension by www.aakashinfo.com",
  "icons": {
    "128": "pluslogo.png"
  },
  "manifest_version": 2,
  "name": "Plus GSTIN Checker",
  "permissions": [
    "activeTab",
    "clipboardRead"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
