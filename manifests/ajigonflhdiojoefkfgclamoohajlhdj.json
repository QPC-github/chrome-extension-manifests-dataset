{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Falcorp screenshare Chrome extension",
  "description": "Falcorp screen sharing Chrome extension",
  "version": "1.1.32",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo-16x16.png",
    "48": "logo-48x48.png",
    "128": "logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture"
  ],
  "externally_connectable": {
    "matches": [
      "*://utahhcc.newvirtualsolution.com/*",
      "*://viplearning.miscursos.net/*",
      "*://conferencias.falcorp.net/*",
      "*://masternutricion.falcorp.net/*",
      "*://micoope.miscursos.net/*",
      "*://miaulavirtual.micoope.com.gt/*",
      "*://viplearning.uni.edu.gt/*",
      "*://vc.falcorp.net/*"
    ]
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}
