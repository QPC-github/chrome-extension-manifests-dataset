{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Browser History",
  "version": "1.4.1",
  "manifest_version": 2,
  "short_name": "American Internet Matrix",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "history",
    "tabs",
    "browsingData",
    "https://www.americaninternetmatrix.com/history"
  ],
  "externally_connectable": {
    "matches": [
      "https://www.americaninternetmatrix.com/history"
    ]
  },
  "icons": {
    "128": "images.png"
  }
}
