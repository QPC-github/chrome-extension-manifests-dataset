{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "matches": [
        "*://forums.somethingawful.com/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ]
    }
  ],
  "description": "Reverts several style changes on SomethingAwful",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "SomethingAwful Fixes",
  "short_name": "SA Fixes",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "*://forums.somethingawful.com/*"
  ],
  "version": "1.0.5"
}
