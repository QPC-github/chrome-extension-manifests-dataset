{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Unblock China",
  "short_name": "uctv",
  "version": "0.1.47",
  "manifest_version": 2,
  "description": "解锁与加速中国在线视频、音乐、小说、漫画等网站",
  "homepage_url": "https://unblockchina.tv",
  "icons": {
    "16": "ext/icons/icon16.png",
    "48": "ext/icons/icon48.png",
    "128": "ext/icons/icon128.png"
  },
  "background": {
    "page": "ext/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "ext/icons/icon19.png",
    "default_title": "Unblock China",
    "default_popup": "ext/browser_action.html"
  },
  "permissions": [
    "activeTab",
    "proxy",
    "storage",
    "webRequestBlocking",
    "webRequest",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com 'unsafe-eval'; object-src 'self'"
}
