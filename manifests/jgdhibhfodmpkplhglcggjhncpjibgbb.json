{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rundavoo",
  "version": "0.11",
  "manifest_version": 2,
  "description": "Rendezvous with Friends",
  "permissions": [
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.js",
        "script.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "icons": {
    "16": "icons/rundavoo-16.png",
    "128": "icons/rundavoo.png"
  }
}
