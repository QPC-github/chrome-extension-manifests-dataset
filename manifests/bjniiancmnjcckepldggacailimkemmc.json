{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Restraint",
  "description": "This extension will make you more productive",
  "version": "1.1",
  "permissions": [
    "tabs",
    "storage",
    "https://play.spotify.com/*",
    "notifications"
  ],
  "options_page": "options.html",
  "browser_action": {},
  "icons": {
    "48": "logo.png"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "eventpage.js",
      "bg.js",
      "popup.js"
    ],
    "persistent": false
  }
}
