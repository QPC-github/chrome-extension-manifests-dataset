{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Articl",
  "description": "A simple tool to share an article.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "images/icon_chromeWebStore-128x128.png",
    "default_popup": "index.html",
    "default_title": "Articl"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+A"
      }
    }
  },
  "permissions": [
    "activeTab"
  ]
}
