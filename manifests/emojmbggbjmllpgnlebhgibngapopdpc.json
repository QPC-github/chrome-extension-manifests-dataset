{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FGL Pool Pick Populator",
  "short_name": "FGL",
  "description": "Select all picked golfers onFGL site and favorite PGA Leaderboard page",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.fglweb.com/fglrptdraftedgolferowner.php*"
      ],
      "js": [
        "Vendors/jquery-2.1.4.min.js",
        "FGLScript.js"
      ],
      "css": [
        "style.css"
      ]
    },
    {
      "matches": [
        "http://www.pgatour.com/leaderboard.html?FGL=True*"
      ],
      "js": [
        "Vendors/jquery-2.1.4.min.js",
        "PGAScript.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "http://www.fglweb.com/fglrptdraftedgolferowner.php*",
    "http://www.pgatour.com/leaderboard.html"
  ]
}
