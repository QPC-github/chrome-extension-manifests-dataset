{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js",
      "options.js"
    ]
  },
  "browser_action": {
    "default_title": ""
  },
  "default_locale": "ru",
  "description": "__MSG_extensionDescription__",
  "engines": {
    "firefox": ">=38.0a1"
  },
  "icons": {
    "128": "NEUTRAL/128.png"
  },
  "externally_connectable": {
    "matches": [
      "https://infinity-discounts.ru/*"
    ]
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "https://infinity-discounts.ru/*"
  ],
  "version": "1.0.0.9"
}
