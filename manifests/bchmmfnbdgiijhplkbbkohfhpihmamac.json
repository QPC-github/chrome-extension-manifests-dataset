{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Twitch Emotes for Rabb.it ",
  "short_name": "Rabbemotes",
  "version": "1.1.4",
  "content_scripts": [
    {
      "matches": [
        "https://www.rabb.it/*"
      ],
      "js": [
        "rabbemotes.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "128.png"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
