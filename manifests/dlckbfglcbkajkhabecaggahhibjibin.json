{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "URL Kısaltıcı ve QR Kod Oluşturucu",
  "version": "0.9",
  "icons": {
    "16": "i/16x16.png",
    "32": "i/32x32.png",
    "48": "i/48x48.png",
    "128": "i/128x128.png"
  },
  "permissions": [
    "tabs"
  ],
  "short_name": "Linklerinizi kısaltın ve QR Kod oluşturun.",
  "signature": "Bikalab",
  "browser_action": {
    "default_title": "Kısa URL oluştur",
    "default_icon": "i/buton.png",
    "default_popup": "popup.html"
  }
}
