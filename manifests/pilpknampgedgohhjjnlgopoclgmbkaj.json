{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Hyperlogs",
  "description": "Track time effortlessly from your Google Chrome Browser.",
  "version": "1.0",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Hyperlogs",
    "default_popup": "popup.html"
  }
}
