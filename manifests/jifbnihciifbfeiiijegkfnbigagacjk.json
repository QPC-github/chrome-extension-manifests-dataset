{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "IA4Chrome",
  "description": "Extension Interact For Chrome Browser",
  "version": "4.1.2.0",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "webNavigation",
    "nativeMessaging",
    "management"
  ],
  "background": {
    "scripts": [
      "ChromeIAV40.js",
      "ChromeInteract.js"
    ],
    "persistent": true
  }
}
