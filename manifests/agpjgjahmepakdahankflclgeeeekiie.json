{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "SaltyWhiskey's Notifications",
  "version": "1.0.8",
  "manifest_version": 2,
  "description": "Use this extension to stay updated about new videos at http://www.youtube.com/saltywhiskey",
  "browser_action": {
    "default_title": "SaltyWhiskey",
    "default_icon": "img/20.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "page": "html/background.html"
  },
  "options_page": "html/options.html",
  "icons": {
    "19": "img/19.png",
    "20": "img/20.png",
    "30": "img/30.png",
    "48": "img/48.png",
    "128": "img/128.png",
    "256": "img/256.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "https://gdata.youtube.com/*",
    "https://api.twitch.tv/*"
  ]
}
