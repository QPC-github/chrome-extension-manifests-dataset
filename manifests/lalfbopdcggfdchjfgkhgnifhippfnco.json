{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1",
  "manifest_version": 2,
  "name": "Flipper",
  "description": "Make you page loads more interesting.",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "ext/flipper.js",
        "ext/helper.js"
      ]
    }
  ],
  "icons": {
    "16": "ext/small16.png",
    "48": "ext/medium48.png",
    "128": "ext/large128.png"
  }
}
