{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Starmarks Bookmark Manager",
  "short_name": "Starmarks",
  "version": "0.1.3",
  "description": "Powerfully filter & sort by tags, ratings, visit count, date added, and last visit. And save smart collections for later!",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "app/assets/img/icon.png",
    "48": "app/assets/img/star-icon48.png",
    "128": "app/assets/img/star-icon128.png"
  },
  "browser_action": {
    "default_title": "starmarks",
    "default_icon": "app/assets/img/icon.png",
    "default_popup": "app/popup/popup.html"
  },
  "manifest_version": 2
}
