{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hereisthelist",
  "version": "1.0.4",
  "content_scripts": [
    {
      "matches": [
        "http://www.hereisthelist.com/*",
        "https://www.hereisthelist.com/*",
        "http://localhost/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Hereisthelist"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
