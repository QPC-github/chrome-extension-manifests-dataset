{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "FB Birth Year Reader",
  "description": "Get Your Birth Year",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "FB Birth Year Reader",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
