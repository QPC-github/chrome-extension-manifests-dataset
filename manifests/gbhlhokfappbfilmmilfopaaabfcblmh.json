{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Peeka Quick Add",
  "description": "Easily and quickly add items to your Peeka orders.",
  "version": "1.0.50",
  "icons": {
    "16": "peeka16.png",
    "48": "peeka48.png",
    "128": "peeka128.png"
  },
  "background": {
    "scripts": [
      "peeka.js"
    ]
  },
  "page_action": {
    "default_icon": "peeka48.png",
    "default_title": "Add this item to your Peeka orders"
  },
  "permissions": [
    "tabs",
    "http://www.peeka.com.sg/*"
  ]
}
