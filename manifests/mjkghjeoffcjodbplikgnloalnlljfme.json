{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "YouTube Controller",
  "description": "This extension helps you to control youtube feature from any page.",
  "version": "1.2.1",
  "browser_action": {
    "default_popup": "youtubeController.html",
    "default_title": "YouTube Controller"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
