{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Indicadores Economicos Chile",
  "version": "0.2.1",
  "description": "Una extension de chrome para ver los indicadores diarios de Chile.",
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png",
    "128": "/img/icon128.png"
  },
  "browser_action": {
    "default_icon": "/img/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://indicadoresdeldia.cl/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
