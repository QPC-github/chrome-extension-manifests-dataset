{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "InstaPDFPro",
  "description": "Easily convert documents to PDF.",
  "version": "1.5.9",
  "manifest_version": 2,
  "icons": {
    "16": "instaicon16.png",
    "48": "instaicon48.png",
    "128": "instaicon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "bg.html"
  },
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://*.instapdfpro.com/*"
  ]
}
