{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Today!",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Makes the \"Today\" button of Google Calendar more patriotic.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/"
  ],
  "web_accessible_resources": [
    "today.wav",
    "neildiamond.png"
  ]
}
