{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Death Clock Calendar",
  "short_name": "Death Calendar",
  "version": "1.2.1",
  "description": "Visualises personalized time until expected death every time you open a new tab. ",
  "manifest_version": 2,
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "author": "Poyan Nabati",
  "homepage_url": "https://deathclock.io/extension",
  "incognito": "not_allowed",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com/ga.js; object-src 'self'",
  "permissions": [
    "storage",
    "https://fonts.googleapis.com/"
  ]
}
