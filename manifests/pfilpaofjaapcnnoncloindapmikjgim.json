{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "うぇぶりんぐーぐる♪(検索結果をボタンで巡回)",
  "version": "3.0.0",
  "description": "Googleやヤフー系の検索結果にヤフー、Amazon、楽天、Youtube、ニコニコ等へのリンクボタンを追加する拡張機能です。キーワードで対象サイトをボタンで回れます。",
  "content_scripts": [
    {
      "matches": [
        "http://www.google.co.jp/*",
        "https://www.google.co.jp/*",
        "http://www.google.com/*",
        "https://www.google.com/*",
        "http://*.yahoo.co.jp/*",
        "http://*.bing.com/*",
        "http://*.youtube.com/*",
        "https://*.youtube.com/*",
        "http://*.baidu.com/*",
        "https://*.baidu.com/*",
        "https://*.baidu.com/*",
        "http://*.nicovideo.jp/*",
        "http://*.wikipedia.org/*",
        "http://*.uncyclopedia.info/*"
      ],
      "js": [
        "ecl.js",
        "core.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    "images/icon16.png",
    "images/icon32.png",
    "images/icon64.png",
    "images/icon128.png",
    "images/yahoo.png",
    "images/yahooshop.png",
    "images/amazon.png",
    "images/rakuten.png",
    "images/youtube.png",
    "images/niconico.png",
    "images/bing.png",
    "images/dmm.png",
    "images/naver.png",
    "images/wikipedia.png",
    "images/yahooauction.png",
    "images/uncyclo.png",
    "images/chie.png",
    "images/vector.png",
    "images/google.png",
    "images/baidu.png",
    "images/option.png",
    "options.html"
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
