{
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Imgchili.com Upload",
  "version": "2.0",
  "description": "Host images with right click to imgChili.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "chili16.png",
    "32": "chili32.png",
    "48": "chili48.png",
    "128": "chili128.png"
  }
}
