{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello Stretch",
  "version": "0.0.0.1",
  "description": "Stretches trello boards to fit available space.",
  "icons": {
    "128": "TrelloStretch-128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://trello.com/b/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "manifest_version": 2
}
