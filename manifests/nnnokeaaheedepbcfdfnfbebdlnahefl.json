{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Librascan",
  "version": "1.01",
  "description": "Librascan is the plugin for scanning Libra blockchain.",
  "permissions": [
    "https://libraview.org/*"
  ],
  "browser_action": {
    "default_title": "Librascan",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
