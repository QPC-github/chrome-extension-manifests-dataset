{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "conipacs Screen Sharing",
  "author": "GlobalMed, Inc.",
  "version": "3.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed primary for http://conipacs.com/.",
  "homepage_url": "https://conipacs.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.conipacs.com/*"
    ]
  },
  "icons": {
    "16": "GlobalMed_Logo16.png",
    "48": "GlobalMed_Logo48.png",
    "128": "GlobalMed_Logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
