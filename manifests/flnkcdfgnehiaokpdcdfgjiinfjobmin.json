{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Remove Twitter AD",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "ツイッターのプロモーションを非表示にします。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "embeded-script.js"
  ],
  "permissions": [
    "<all_urls>"
  ]
}
