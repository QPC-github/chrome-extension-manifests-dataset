{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Inboxize Gmail",
  "short_name": "Inboxize",
  "version": "2019.04.04",
  "description": "Make new Gmail look like good old loved Google Inbox.",
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "72": "icon72.png",
    "96": "icon96.png",
    "128": "icon128.png",
    "256": "icon256.png",
    "512": "icon512.png"
  },
  "browser_action": {
    "default_title": "Inboxize Gmail",
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png",
      "72": "icon72.png",
      "96": "icon96.png",
      "128": "icon128.png",
      "256": "icon256.png",
      "512": "icon512.png"
    }
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "matches": [
        "https://mail.google.com/*"
      ]
    }
  ]
}
