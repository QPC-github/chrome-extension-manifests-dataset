{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Agenda Assessoria - WebAPI2",
  "version": "5.0",
  "description": "Excelência em Previdência",
  "homepage_url": "http://www.agendaassessoria.com.br",
  "icons": {
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_title": "Agenda Assessoria - WebAPI2",
    "default_icon": "icons/19x19.png"
  },
  "permissions": [
    "nativeMessaging"
  ],
  "web_accessible_resources": [
    "icons/19x19.png"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
