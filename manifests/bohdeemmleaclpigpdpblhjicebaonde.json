{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ikura",
  "version": "2.1.4",
  "description": "商品価格の変化をグラフで見られます。",
  "author": "Ikura Tokyo",
  "content_scripts": [
    {
      "matches": [
        "http://www.dmm.co.jp/digital/*/detail/*/cid=*",
        "https://www.dmm.co.jp/digital/*/detail/*/cid=*",
        "https://www.yodobashi.com/product/*",
        "https://store.playstation.com/*",
        "https://ec.nintendo.com/JP/ja/titles/*",
        "https://ec.nintendo.com/JP/ja/bundles/*",
        "https://ec.nintendo.com/JP/ja/aocs/*",
        "https://store.steampowered.com/app/*",
        "https://store.steampowered.com/bundle/*",
        "https://store.steampowered.com/sub/*"
      ],
      "js": [
        "moment.min.js",
        "Chart.min.js",
        "jquery-3.2.1.min.js",
        "contentScripts.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "38": "icon38.png",
    "19": "icon19.png",
    "16": "icon16.png"
  }
}
