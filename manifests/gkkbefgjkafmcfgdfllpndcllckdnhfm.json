{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Blacktalon",
  "description": "A space shootem up built in html 5",
  "version": "1.0",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "144": "icon-114.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "permissions": [
    "unlimitedStorage"
  ],
  "offline_enabled": true
}
