{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Compartilhamento de Tela",
  "author": "Neomundi",
  "version": "0.2.3",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Esta extensão é utilizada para capturar a tela e compartilhá-la no bate papo por vídeo ao vivo.",
  "homepage_url": "https://startup.neomundi.com.br",
  "key": "",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ],
  "externally_connectable": {
    "matches": [
      "*://comvox.com.br/*",
      "*://comvox.com.br/demo/*",
      "*://comvox.com.br/test/*",
      "*://*.comvox.com.br/*",
      "*://startup.neomundi.com.br/*",
      "*://comvox.neomundi.com.br/*",
      "*://localhost/*"
    ]
  }
}
