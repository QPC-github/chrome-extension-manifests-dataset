{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Coup d'Bungie",
  "version": "6.1.18",
  "manifest_version": 2,
  "description": "Personalize your bungie.net experience.",
  "permissions": [
    "http://*.bungie.net/*",
    "https://*.bungie.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.bungie.net/*",
        "https://*.bungie.net/*"
      ],
      "js": [
        "jquery-2.0.2.min.js",
        "xt.js",
        "main.js"
      ]
    }
  ]
}
