{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Product Hunt is Bot Hunt",
  "description": "An extension that replaces all instances of the word 'Product Hunt' with the word 'Bot Hunt', @rahulr047",
  "version": "1.0",
  "icons": {
    "8": "bothunt8.png",
    "16": "bothunt16.png",
    "32": "bothunt32.png",
    "48": "bothunt48.png",
    "64": "bothunt64.png",
    "128": "bothunt128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
