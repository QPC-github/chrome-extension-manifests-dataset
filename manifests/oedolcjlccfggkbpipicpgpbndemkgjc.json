{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "네이버 뮤직 for Mac - Google Chrome Plugin",
  "version": "1.1.4",
  "description": "맥용 네이버 뮤직을 위한 크롬 플러그인",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "http://music.naver.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://music.naver.com/*"
      ],
      "js": [
        "navermusic.js"
      ],
      "run_at": "document_end"
    }
  ]
}
