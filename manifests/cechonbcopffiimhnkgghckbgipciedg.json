{
  "background": {
    "persistent": false,
    "scripts": [
      "event_page.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-2.1.0.js",
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "This plugin enhances the defect submission process to JIRA.",
  "icons": {
    "128": "icon16.png",
    "16": "icon16.png",
    "48": "icon16.png"
  },
  "manifest_version": 2,
  "name": "Katalon Integration",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
