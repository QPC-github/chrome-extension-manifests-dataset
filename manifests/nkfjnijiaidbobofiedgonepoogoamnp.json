{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "AWS Title Changer",
  "version": "1.2",
  "description": "Change page title on AWS Management Console",
  "icons": {
    "48": "icon_48.png",
    "19": "icon_19.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "aws_title_changer.js"
    ]
  },
  "permissions": [
    "https://console.aws.amazon.com/*",
    "webNavigation",
    "tabs"
  ]
}
