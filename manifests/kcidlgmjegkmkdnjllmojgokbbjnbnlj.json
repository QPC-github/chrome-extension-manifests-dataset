{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Bookmark Tab",
  "description": "Replace standard New Tab. Each bookmark folder is a button. Click a button to open a window with links.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "version": "2.0",
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "chrome://favicon/"
  ]
}
