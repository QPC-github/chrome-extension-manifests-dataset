{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Clipular! Research, save & share screenshot",
  "description": "Screen capture, reinvented. Organize, search & bookmark the web.",
  "version": "10.8.29.2046",
  "manifest_version": 2,
  "app": {
    "urls": [
      "*://clipular.com/"
    ],
    "launch": {
      "web_url": "http://clipular.com/?Clipboard=a"
    }
  },
  "icons": {
    "128": "clipular_128px.png"
  },
  "permissions": [
    "unlimitedStorage"
  ]
}
