{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "PlayCanvas",
  "short_name": "PlayCanvas",
  "description": "Use PlayCanvas to create stunning 3D games for browsers and mobiles.",
  "version": "1.0.1",
  "app": {
    "urls": [
      "*://playcanvas.com/",
      "*://apps.playcanvas.com/"
    ],
    "launch": {
      "web_url": "https://login.playcanvas.com/signup"
    }
  },
  "icons": {
    "128": "Logo_CHRO.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "minimum_chrome_version": "38"
}
