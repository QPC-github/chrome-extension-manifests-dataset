{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "short_name": "Multitask",
  "default_locale": "en",
  "icons": {
    "128": "favicon/favicon_128.png",
    "16": "favicon/favicon_16.png",
    "48": "favicon/favicon_48.png"
  },
  "version": "1.0.4.2",
  "browser_action": {
    "default_title": "Welcome to MULTITASK",
    "default_icon": "favicon/favicon_128.png"
  },
  "permissions": [
    "activeTab",
    "history",
    "topSites",
    "bookmarks",
    "storage",
    "geolocation",
    "webNavigation",
    "identity",
    "cookies",
    "chrome://favicon/",
    "https://ajax.googleapis.com/",
    "http://multitask/",
    "https://extensions.mjdp.com.ua/multitask/",
    "tabs",
    "https://docs.google.com/feeds/",
    "https://accounts.google.com/",
    "https://apis.google.com/"
  ],
  "web_accessible_resources": [
    "oauth2/oauth2.html"
  ],
  "background": {
    "scripts": [
      "all.min.js",
      "develop.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://suggestqueries.google.com/complete/ https://apis.google.com https://accounts.google.com/ https://ajax.googleapis.com/ajax/services/feed/ https://*.googleapis.com/ https://www.google.com/; object-src 'self'",
  "oauth2": {
    "client_id": "121562475318-aslnabfujtg8do98rrk501leri7jq07h.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "openid"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
