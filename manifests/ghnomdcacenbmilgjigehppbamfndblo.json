{
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "short_name": "Camelizer",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "3.0.7",
  "icons": {
    "256": "icon-256.png",
    "128": "icon-128.png",
    "96": "icon-96.png",
    "64": "icon-64.png",
    "48": "icon-48.png",
    "32": "icon-32.png",
    "16": "icon-16.png"
  },
  "page_action": {
    "default_title": "__MSG_pageActionTitle__",
    "default_icon": "icon-96.png",
    "default_popup": "main.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://camelcamelcamel.com/*",
    "https://camelcamelcamel.com/*",
    "http://*.camelcamelcamel.com/*",
    "https://*.camelcamelcamel.com/*",
    "http://amazon.com/*",
    "https://amazon.com/*",
    "http://*.amazon.com/*",
    "https://*.amazon.com/*",
    "http://amazon.co.uk/*",
    "https://amazon.co.uk/*",
    "http://*.amazon.co.uk/*",
    "https://*.amazon.co.uk/*",
    "http://amazon.fr/*",
    "https://amazon.fr/*",
    "http://*.amazon.fr/*",
    "https://*.amazon.fr/*",
    "http://amazon.de/*",
    "https://amazon.de/*",
    "http://*.amazon.de/*",
    "https://*.amazon.de/*",
    "http://amazon.es/*",
    "https://amazon.es/*",
    "http://*.amazon.es/*",
    "https://*.amazon.es/*",
    "http://amazon.ca/*",
    "https://amazon.ca/*",
    "http://*.amazon.ca/*",
    "https://*.amazon.ca/*",
    "http://amazon.it/*",
    "https://amazon.it/*",
    "http://*.amazon.it/*",
    "https://*.amazon.it/*",
    "http://amazon.com.au/*",
    "https://amazon.com.au/*",
    "http://*.amazon.com.au/*",
    "https://*.amazon.com.au/*",
    "http://camelizer.net/*",
    "https://camelizer.net/*",
    "http://*.camelizer.net/*",
    "https://*.camelizer.net/*",
    "http://camelizer.org/*",
    "https://camelizer.org/*",
    "http://*.camelizer.org/*",
    "https://*.camelizer.org/*"
  ],
  "author": "Cosmic Shovel, Inc.",
  "homepage_url": "https://camelcamelcamel.com/",
  "options_ui": {
    "page": "options.html"
  }
}
