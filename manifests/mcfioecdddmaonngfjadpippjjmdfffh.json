{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "AutoFace",
  "version": "1.1.2",
  "manifest_version": 2,
  "description": "Facebook automation",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "option.html"
  },
  "permissions": [
    "activeTab"
  ]
}
