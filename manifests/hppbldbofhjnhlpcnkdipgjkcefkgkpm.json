{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Save Chrome Tabs For Later",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Set aside your currently open tabs to quickly reopen them later.",
  "permissions": [
    "tabs",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/bitmap16.png",
      "32": "images/bitmap32.png",
      "48": "images/T48.png",
      "128": "images/T.png"
    }
  },
  "icons": {
    "16": "images/bitmap16.png",
    "32": "images/bitmap32.png",
    "48": "images/T48.png",
    "128": "images/T.png"
  }
}
