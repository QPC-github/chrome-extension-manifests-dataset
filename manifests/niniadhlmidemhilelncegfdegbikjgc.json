{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UNO Heroes",
  "manifest_version": 2,
  "description": "UNO Heroes is a web browser game that lets you play the classic UNO game with a twist on your computer.",
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "version": "1",
  "browser_action": {},
  "chrome_accept_languages": {
    "message": "$CHROME$ accepts $languages$ languages",
    "placeholders": {
      "chrome": {
        "content": "Chrome",
        "example": "Chrome"
      },
      "languages": {
        "content": "$1",
        "example": "en,sr,de"
      }
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persistent": false
  }
}
