{
  "name": "WhatMouse.com RSS & Search",
  "description": "Simple Chrome extension for WhatMouse.com which lets you read RSS and search website.",
  "browser_action": {
    "default_icon": {
      "38": "icon.png"
    },
    "default_title": "WhatMouse.com RSS & Search",
    "default_popup": "labnol.html"
  },
  "manifest_version": 2,
  "update_url": "http://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "version": "0.2"
}
