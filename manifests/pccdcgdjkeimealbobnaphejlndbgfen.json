{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "TinyUrl Maker",
  "version": "1.5",
  "description": "Shorten any URL. Made by Nick.",
  "options_page": "options.html",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "permissions": [
    "http://tinyurl.com/*",
    "tabs"
  ]
}
