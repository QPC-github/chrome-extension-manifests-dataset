{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "H5P to edX bridge",
  "description": "Embedding H5P activities inside the edX open platform.",
  "version": "1.0.1",
  "author": "Yotam Salmon",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "incognito": "not_allowed",
  "permissions": [
    "activeTab",
    "webNavigation",
    "*://*.h5p.org/*",
    "tabs"
  ]
}
