{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "GoFetch",
  "version": "1.7",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://platform.twitter.com https://cdn.api.twitter.com https://syndication.twimg.com https://cdn.syndication.twimg.com; object-src 'self'",
  "description": "GoFetch Internet Companion",
  "icons": {
    "16": "GoFetch16.png",
    "48": "GoFetch48.png",
    "128": "GoFetch128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "management",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "home.html",
    "default_icon": "GoFetch128.png",
    "default_title": "GoFetch"
  }
}
