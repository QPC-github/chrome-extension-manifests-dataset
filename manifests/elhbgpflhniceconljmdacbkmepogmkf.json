{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "twitter auto reload",
  "manifest_version": 2,
  "version": "1.1",
  "description": "twitter timeline auto reload",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/"
      ],
      "js": [
        "js/jquery.js",
        "js/auto_reload.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
