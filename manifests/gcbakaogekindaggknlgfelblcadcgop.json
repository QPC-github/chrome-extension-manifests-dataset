{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "RendeljKínait támogatói bővítmény",
  "description": "A bővítmény használatával a RendeljKínait csapatát támogatod, számodra ingyenes módon.",
  "version": "1.7",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background_min.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.gearbest.com/*",
    "*://*.banggood.com/*",
    "*://*.geekbuying.com/item*",
    "*://*.cart.cafago.com/*",
    "*://*.gearvita.com/*",
    "*://*.geekmaxi.com/*/order",
    "*://*.xiaomishop.hu/*"
  ]
}
