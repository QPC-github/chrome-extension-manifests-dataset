{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Socialet",
  "description": "Socialet",
  "version": "2.1",
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icons/ss16.png",
    "48": "icons/ss64.png",
    "128": "icons/ss128.png"
  },
  "background": {
    "scripts": [
      "js/libs/jquery.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "html/*",
    "js/*",
    "css/*",
    "fonts/*"
  ],
  "options_page": "html/config.html",
  "browser_action": {
    "default_icon": "icons/ss64.png",
    "default_title": "Socialet"
  },
  "manifest_version": 2
}
