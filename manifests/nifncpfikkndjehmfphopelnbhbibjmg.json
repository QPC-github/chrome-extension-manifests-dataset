{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "jquery-2.2.0.min.js",
        "main.js"
      ],
      "matches": [
        "http://store.steampowered.com/app/*",
        "http://steamcommunity.com/app/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Adds warning messages to Steam store and community pages which relate to games where developers have done something dodgy.",
  "name": "Steam Dodgy Developer Warner",
  "version": "1.0.1"
}
