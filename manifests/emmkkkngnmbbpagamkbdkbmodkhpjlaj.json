{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Investing.com",
  "short_name": "Investing.com New Tab",
  "description": "Customized New Tab for Investing.com",
  "version": "1.9",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "browsingData",
    "cookies",
    "contextMenus",
    "notifications",
    "chrome://favicon/",
    "activeTab",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "show.html"
  },
  "icons": {
    "16": "Investing_Icon_16.png",
    "48": "Investing_Icon_48.png",
    "128": "Investing_Icon_128.png"
  },
  "browser_action": {
    "default_icon": "Investing_Icon_16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
