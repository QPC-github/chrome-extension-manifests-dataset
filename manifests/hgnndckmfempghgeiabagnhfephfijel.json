{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Cached View",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Fast and simple access to a cached version of currently viewing page. Just in a click.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Cached page viewer",
    "default_icon": "icon16.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "homepage_url": "http://binaryoptionsaz.com"
}
