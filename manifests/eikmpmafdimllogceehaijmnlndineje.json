{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Right-Click Search Wikipedia",
  "description": "Quickly search for highlighted text on Wikipedia via right-click",
  "version": "0.9.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "core.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "wikipedia-icon.png",
    "48": "wikipedia-icon-big.png"
  }
}
