{
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/default.png",
    "default_title": "Last.fm",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "name": "Last.fm",
  "permissions": [
    "http://ws.audioscrobbler.com/",
    "http://www.last.fm/",
    "tabs"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.7.3"
}
