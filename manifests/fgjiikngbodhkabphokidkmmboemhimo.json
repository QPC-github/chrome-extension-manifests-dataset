{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LINKSEO",
  "description": "LINKSEO - Seo backlink management apps",
  "version": "3",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "main.js",
        "services.js",
        "username.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    "http://*/*",
    "https://*/*"
  ]
}
