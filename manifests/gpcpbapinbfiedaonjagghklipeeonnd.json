{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OpenTouch Conversation One",
  "short_name": "OTC One",
  "version": "2.21.6710",
  "version_name": "2.21.6710",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_app_description__",
  "icons": {
    "128": "otmeetingmanager_128.png",
    "48": "otmeetingmanager_48.png"
  },
  "sockets": {
    "tcp": {
      "connect": "*:*"
    }
  },
  "permissions": [
    "storage",
    "https://www.google-analytics.com/",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "unlimitedStorage",
    "contextMenus",
    "alwaysOnTopWindows",
    "notifications",
    "webview",
    "fullscreen",
    "videoCapture",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    }
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
