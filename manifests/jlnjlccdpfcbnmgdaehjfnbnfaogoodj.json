{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tabs Rule Everything Around Me",
  "description": "Record and visualize how you use tabs.",
  "version": "0.6",
  "permissions": [
    "http://*/*",
    "storage",
    "tabs",
    "alarms"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js",
      "d3.v3.min.js"
    ]
  },
  "browser_action": {
    "default_title": "Show Tab Visualization",
    "default_icon": "icon128_2.png"
  },
  "icons": {
    "16": "icon16_2.png",
    "48": "icon48_2.png",
    "128": "icon128_2.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
