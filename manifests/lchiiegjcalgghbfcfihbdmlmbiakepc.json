{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "VK Photo Hotkeys",
  "version": "1.2",
  "description": "Useful hotkeys extending features of vk.com (ВКонтакте) photo viewer",
  "permissions": [
    "*://*.vk.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.vk.com/*"
      ],
      "js": [
        "hotkeys.js"
      ]
    }
  ],
  "manifest_version": 2
}
