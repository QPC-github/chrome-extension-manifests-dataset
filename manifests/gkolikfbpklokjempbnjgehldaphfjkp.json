{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "urlBlocker",
  "version": "1.0",
  "description": "Chrome URLs Blocking Extension",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "Ad-blocker.png"
  },
  "manifest_version": 2
}
