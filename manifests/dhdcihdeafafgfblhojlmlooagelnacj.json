{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "NameBlock",
  "description": "This extension helps trans teens avoid their dead names in their mail and other places they might be at risk. ",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "19": "images/enabled.png"
    },
    "default_title": "NameBlock options",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "images/icon128.png"
  }
}
