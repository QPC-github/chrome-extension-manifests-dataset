{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "PENGUINS SCHEDULE",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "A quick way to view the entire Pittsburgh Penguins schedule.",
  "icons": {
    "16": "src/browser_action/images/icon.png",
    "48": "src/browser_action/images/icon.png",
    "128": "src/browser_action/images/icon.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "src/browser_action/images/icon.png",
    "default_title": "Penguins Schedule",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
