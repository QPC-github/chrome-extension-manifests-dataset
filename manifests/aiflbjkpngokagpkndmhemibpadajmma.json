{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Lightweight Multi Notepad",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Simple and yet multiple notepad.",
  "browser_action": {
    "default_icon": "Notepad.png",
    "default_popup": "notepad.html"
  },
  "permissions": [
    "http://api.flickr.com/"
  ],
  "content_scripts": [
    {
      "js": [
        "function.js",
        "jquery-2.0.3.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
