{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "The Offensifier",
  "description": "Finds words that rhyme with swear words and switches them",
  "version": "2",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
