{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Somebody",
  "description": "Somebody",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "version": "2.0",
  "permissions": [
    "activeTab",
    "notifications",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "*://*/*search*"
      ],
      "js": [
        "extra/jquery-3.3.1.min.js",
        "content-script.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Somebody"
  },
  "manifest_version": 2
}
