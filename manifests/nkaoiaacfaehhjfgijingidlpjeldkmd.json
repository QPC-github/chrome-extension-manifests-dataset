{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "content.js",
      "popup.js",
      "colorTransform.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon3.png",
    "default_popup": "popup.html",
    "default_title": "Change the page background color,redefine the page skin."
  },
  "description": "Change the page background color,redefine friendly page skin.",
  "icons": {
    "128": "icon3.png"
  },
  "manifest_version": 2,
  "name": "BackgroundSkin",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "version": "2.70"
}
