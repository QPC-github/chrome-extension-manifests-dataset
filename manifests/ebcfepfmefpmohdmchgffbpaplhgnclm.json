{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sopra Bookmarks",
  "short_name": "Bookmarks",
  "version": "0.0.2.7",
  "description": "Extension helps to save bookmarks with minimum effort! Save bookmarks in categories, and open them all in one click.",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js",
      "./js/modules/settings.module.js",
      "./js/popup.js",
      "./js/services/utils.sevice.js",
      "./js/services/storage.service.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started.png",
      "32": "images/get_started.png",
      "48": "images/get_started.png",
      "128": "images/get_started.png"
    }
  },
  "icons": {
    "16": "images/get_started.png",
    "32": "images/get_started.png",
    "48": "images/get_started.png",
    "128": "images/get_started.png"
  }
}
