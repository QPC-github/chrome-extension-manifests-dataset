{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Breadcrumb",
  "description": "Start a breadcrumb of the current page.",
  "version": "0.1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Breadcrumb",
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
