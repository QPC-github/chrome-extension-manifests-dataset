{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "img/av-icon.png",
    "default_title": "__MSG_chrome_browser_action__"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "zh_CN",
  "description": "__MSG_meta_extension_description__",
  "icons": {
    "128": "img/av-icon.png",
    "16": "img/av-icon.png",
    "48": "img/av-icon.png"
  },
  "manifest_version": 2,
  "name": "__MSG_meta_extension_name__",
  "permissions": [
    "unlimitedStorage",
    "contextMenus",
    "cookies",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.1.1",
  "web_accessible_resources": [
    "css/av-custom.css",
    "css/bulk.css",
    "css/base--theme-1.css",
    "img/av-icon.png",
    "img/bookmarklet--shadows.png",
    "img/curtain--close.png",
    "img/curtain--shading.png",
    "img/dialog--frame.png",
    "img/dialog--login.png",
    "img/dialog-login--arrow.png",
    "img/dialog-notification--icons--clip.png",
    "img/dialog-notification--icons--failed.png",
    "img/favicon.ico",
    "img/loading--background.png",
    "img/loading--big.gif",
    "img/loading--small.gif",
    "img/options--tabs-sprite.png",
    "img/overlay.png",
    "img/toolbar-back-o.png",
    "img/toolbar-back.png",
    "js/av-custom-2.js",
    "js/av-custom.js",
    "js/bulk.js",
    "js/targetanyview.js",
    "js/__bookmarklet_to_injectanyview.js",
    "js/__bookmarklet_to_injectanyview__andClipOnLaunch.js",
    "libs/bootstrap/bootstrap.css",
    "libs/jQuery.js",
    "libs/jquery.jscrollpane.css",
    "libs/jquery.jscrollpane.min.js",
    "libs/jquery.mousewheel.js",
    "libs/jsonrpc.js",
    "libs/konami.js",
    "libs/miniColors/images/circle.gif",
    "libs/miniColors/images/gradient.png",
    "libs/miniColors/images/line.gif",
    "libs/miniColors/images/rainbow.png",
    "libs/miniColors/images/trigger.png"
  ]
}
