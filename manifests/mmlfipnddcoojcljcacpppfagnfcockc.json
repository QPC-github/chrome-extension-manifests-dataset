{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Swirly Fluids New Tab",
  "description": "Start your new tab page with a fluid simulation - all credit goes to https://paveldogreat.github.io/WebGL-Fluid-Simulation/",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' ; object-src https://paveldogreat.github.io/WebGL-Fluid-Simulation/",
  "chrome_url_overrides": {
    "newtab": "MyNewTab.html"
  }
}
