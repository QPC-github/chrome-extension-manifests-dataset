{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WaniTab for NewTab",
  "short_name": "WaniTab",
  "version": "0.1",
  "description": "Change out that boring NewTab page with something that'll help you learn Kanji from WaniKani!",
  "manifest_version": 2,
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "/img/icon/16.png",
      "32": "/img/icon/32.png",
      "48": "/img/icon/48.png",
      "128": "/img/icon/128.png"
    },
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "/img/icon/16.png",
    "32": "/img/icon/32.png",
    "48": "/img/icon/48.png",
    "128": "/img/icon/128.png"
  }
}
