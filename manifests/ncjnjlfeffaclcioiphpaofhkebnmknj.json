{
  "page_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html",
    "default_title": "Source Quicklinks"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Allows quick jumping between Chrome, Blink, WebKit, V8, etc. source links",
  "background": {
    "scripts": [
      "js/closure-lite.js",
      "js/link.js",
      "js/tab-state.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "name": "Source Quicklinks",
  "omnibox": {
    "keyword": "sql"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "manifest_version": 2,
  "version": "1.5"
}
