{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_appName__",
  "version": "2.2.9.723",
  "description": "__MSG_appDesc__",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "Icon.png",
    "default_title": "CAYOVA Box"
  },
  "default_locale": "en",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "chrome/content/common.js",
        "webkit/common.js",
        "webkit/chrome/content.js",
        "webkit/inpage.js",
        "chrome/content/events.js",
        "chrome/content/socialButtons.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "page": "webkit/chrome/background.html"
  },
  "web_accessible_resources": [
    "webkit/chrome/blank.gif",
    "webkit/chrome/blank.html"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "cookies",
    "<all_urls>"
  ]
}
