{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hide Facebook like",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Hides the Facebook Like button.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "permissions": [
    "https://www.facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "css": [
        "css/inject.css"
      ]
    }
  ]
}
