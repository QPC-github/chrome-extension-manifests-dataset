{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quotable",
  "description": "Displays random user-uploaded quotes on each blank 'new tab' HTML page.",
  "version": "1.3",
  "incognito": "split",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "chrome_url_overrides": {
    "newtab": "quotable.html"
  },
  "manifest_version": 2
}
