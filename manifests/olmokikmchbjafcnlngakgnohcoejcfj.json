{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "StackCite",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Include a citation link for Stack Overflow answers",
  "content_scripts": [
    {
      "matches": [
        "http://stackoverflow.com/*",
        "https://stackoverflow.com/*"
      ],
      "js": [
        "StackCite.js"
      ]
    }
  ]
}
