{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Free Mileage &Timeline Spreadsheets:Taxes/IRS",
  "description": "Save Google timeline location history to a CSV file. Limited Free Version allows only ten days back. ",
  "version": "1.5",
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_popup": "index.html",
    "default_title": "Location History"
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "permissions": [
    "http://www.google.com/*",
    "https://www.google.com/*",
    "downloads",
    "storage",
    "tabs"
  ]
}
