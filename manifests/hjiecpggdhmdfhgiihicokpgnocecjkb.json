{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Don't Forget Me",
  "description": "When opening a new tab, this extension will randomly select one of your bookmarks to display.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "happy128.png"
  },
  "icons": {
    "16": "egg16.png",
    "48": "egg48.png",
    "128": "egg128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "tabs",
    "storage"
  ]
}
