{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "Philanthropist",
  "version": "1.0.7",
  "manifest_version": 2,
  "description": "Associated Amazon shopping made easy.",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "page_action": {
    "default_icon": "assets/icon16.png",
    "default_title": "Philanthropist"
  },
  "background": {
    "scripts": [
      "lib/vendor/underscore/underscore.js",
      "lib/vendor/URI.js/src/URI.js",
      "lib/background/logger.js",
      "lib/background/store.js",
      "lib/background/session_manager.js",
      "lib/background/tab_manager.js",
      "lib/background/main.js"
    ]
  },
  "options_page": "lib/options/options.html",
  "permissions": [
    "tabs",
    "cookies",
    "http://*.amazon.com/*"
  ]
}
