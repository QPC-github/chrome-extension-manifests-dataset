{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "image*",
  "description": "This extension allows you to preview a folder of images on a browser.",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "tabs",
    "chrome://favicon/"
  ],
  "browser_action": {
    "default_icon": "img/icon48.png"
  }
}
