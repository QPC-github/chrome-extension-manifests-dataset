{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "TipidPC.com Easy Search",
  "short_name": "TPC Easy Search",
  "version": "0.0.1",
  "description": "Search the TipidPC.com marketplace easily using this FREE Chrome extension",
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_title": "TipidPC.com Easy Search",
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  },
  "author": "JP Caparas",
  "permissions": [
    "tabs",
    "storage"
  ]
}
