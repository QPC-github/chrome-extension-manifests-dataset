{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Favicon2SleipnirSearch",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "FaviconをSleipnirの検索結果に挿入する拡張。",
  "content_scripts": [
    {
      "matches": [
        "http://search.fenrir-inc.com/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}
