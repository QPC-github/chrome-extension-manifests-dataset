{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Disstream Online",
  "short_name": "disstream_online",
  "version": "1.2.0",
  "description": "Otrzymuj na bieżąco notyfikacje od Twojego ulubionego YouTubera.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Kemsan",
  "background": {
    "scripts": [
      "js/jquery.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Zobacz ostatnie powiadomienia",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "*://api.kemsan.xyz/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "homepage_url": "https://www.youtube.com/user/DisStream"
}
