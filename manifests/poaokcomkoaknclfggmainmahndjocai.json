{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tab By Friday",
  "description": "A weekly challenge tab extension.",
  "author": "Tyler Robertson",
  "manifest_version": 2,
  "version": "0.0.0.4",
  "permissions": [
    "management",
    "tabs",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "script.js"
    ]
  }
}
