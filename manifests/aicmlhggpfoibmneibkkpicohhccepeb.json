{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Retrovision Classic Movies",
  "description": "Watch Hollywood Classics with your browser for free.  ",
  "version": "2.3",
  "manifest_version": 2,
  "app": {
    "urls": [
      "*://retrovision.tv/"
    ],
    "launch": {
      "web_url": "http://retrovision.tv//"
    }
  },
  "icons": {
    "128": "128x128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
