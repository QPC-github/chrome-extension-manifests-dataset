{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "EXIF.js",
        "js.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Display EXIF overlay on images. Again!",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Exponator Live!",
  "options_page": "options.html",
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
