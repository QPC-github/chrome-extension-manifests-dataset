{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Quick Stock Search",
  "description": "A search and visualization tool for reviewing the stock price for of any publicly traded company.",
  "version": "1",
  "author": "Jim Martin",
  "browser_action": {
    "default_title": "Stay informed on the market"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
