{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "SmartAGB.de",
  "description": "Einfach und schnell wichtige Informationen aus den AGB deiner Lieblings-Shops auf einen Blick.",
  "short_name": "einfach & schnell AGBs verstehen",
  "version": "1.3.2",
  "permissions": [
    "http://service.smartagb.de/*",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_default.png",
    "default_popup": "popup.html"
  }
}
