{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SNDL",
  "version": "1.6",
  "manifest_version": 2,
  "description": "اضافة لتسهيل تحميل المقالات العلمية",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
