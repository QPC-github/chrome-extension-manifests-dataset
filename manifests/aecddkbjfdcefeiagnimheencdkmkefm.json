{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Smart card access extension",
  "description": "Smart cards access from Web browsers",
  "version": "0.0.6",
  "minimum_chrome_version": "40.0",
  "key": "",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "sc-access-background.js"
    ]
  },
  "permissions": [
    "nativeMessaging"
  ]
}
