{
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "entry.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://astyle-src.alicdn.com; object-src 'self'",
  "description": "Khi xem sản phẩm trên Taobao và Tmall, tự động đề xuất nguồn cung cấp 1688 tương tự cho bạn. Đồng Quốc Vương said",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "Tìm Nguồn Hàng 1688",
  "permissions": [
    "tabs",
    "https://*.tmall.com/",
    "https://*.taobao.com/",
    "http://*/",
    "https://*/"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.2"
}
