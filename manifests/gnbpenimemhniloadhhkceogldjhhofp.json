{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.2",
  "name": "Limbo Tab",
  "description": "opens a new tab to a free fall",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "downloads",
    "*://limbotab.com/*,http://localhost:8080/*,*://*.media.tumblr.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://*.twitter.com 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
