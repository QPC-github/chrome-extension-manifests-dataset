{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Managify Bookmark All Tabs",
  "description": "Bookmark all tabs in your current window together instantly.",
  "short_name": "bookmark-All-Tabs",
  "version": "1.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/bookmark-all-tabs-app.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "tabs"
  ],
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
