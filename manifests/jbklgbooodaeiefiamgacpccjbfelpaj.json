{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "BanGraphs",
  "description": "Mute Trolls in FanGraphs' comment section.",
  "version": "1.0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.fangraphs.com/*"
      ],
      "js": [
        "jquery-3.0.0.min.js",
        "bangraphs.js"
      ]
    }
  ]
}
