{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "LoL Ruletext Checker",
  "version": "0.5.1",
  "description": "League of Legends Ruletext Checker",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/icon_016.png",
    "48": "icons/icon_048.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_048.png",
    "default_popup": "panel.html"
  }
}
