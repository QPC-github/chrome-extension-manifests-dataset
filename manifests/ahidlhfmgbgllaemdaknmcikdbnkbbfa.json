{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "jquery-2.0.3.min.js",
        "main.js"
      ],
      "matches": [
        "http://www.isnichwahr.de/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "Erweitert Isnchwahr.de mit dingen wie Mindestwertung, Imagefix, Kategorie filter...",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Isnichwahr.de Pro Addon",
  "permissions": [
    "tabs",
    "http://www.isnichwahr.de/*",
    "https://www.isnichwahr.de/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.4"
}
