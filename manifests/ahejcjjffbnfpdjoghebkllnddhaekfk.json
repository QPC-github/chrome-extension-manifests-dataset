{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "No Hurry, Tweetdeck",
  "version": "0.1",
  "description": "Hide the 'new tweets' prompt in Tweetdeck.",
  "author": "Jakub Koziol",
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
