{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "趴趴教育屏幕共享插件",
  "description": "Allows you to capture your desktop for use in video applications",
  "version": "1.0.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "page_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "papa education screensharing file"
  },
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://test.tutor.papaen.com/*",
      "*://tutor.papaen.com/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "declarativeContent"
  ]
}
