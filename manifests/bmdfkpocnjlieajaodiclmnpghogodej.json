{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "OnePgr Meet Screen Sharing",
  "author": "OnePgr, Inc.",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed for http://*.onepgr.com/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://app.onepgr.com",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "https://*.onepgr.com/*",
      "https://meet.onepgr.com/*"
    ]
  },
  "icons": {
    "16": "onepgr_16.png",
    "48": "onepgr_48.png",
    "128": "onepgr_128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "onepgr_16.png"
  ]
}
