{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mine",
  "version": "0.0.7",
  "manifest_version": 2,
  "description": "Annotate any image on the internet.",
  "icons": {
    "16": "static/images/icon-16.png",
    "128": "static/images/icon-128.png"
  },
  "background": {
    "scripts": [
      "chromereload.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "static/images/icon-19.png",
      "38": "static/images/icon-38.png"
    },
    "default_title": "mine chrome extension"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
