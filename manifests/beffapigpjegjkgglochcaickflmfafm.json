{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Công cụ ký điện tử dành cho e-invoice.com.vn",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "40.0",
  "name": "tokenSigner.odis.com.vn",
  "permissions": [
    "activeTab",
    "http://e-invoice.com.vn/*",
    "https://e-invoice.com.vn/*",
    "nativeMessaging"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.2",
  "web_accessible_resources": [
    "page.js",
    "checkPlugin.js"
  ]
}
