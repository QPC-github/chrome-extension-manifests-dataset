{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_scripts": [
    {
      "css": [
        "style.css",
        "transition.min.css"
      ],
      "js": [
        "jquery.min.js",
        "transition.min.js",
        "script.js"
      ],
      "matches": [
        "https://*.instagram.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Allows you to zoom in on the photos",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "19": "icon19.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "Instant Zoom",
  "version": "415"
}
