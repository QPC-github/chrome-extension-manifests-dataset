{
  "browser_action": {
    "default_title": "Muhammed Akşam",
    "default_icon": "images/192.png",
    "default_popup": "index.html"
  },
  "description": "Muhammed Akşam Chrome eklentisi ile teknoloji alanında makalelere kolayca ulaşabilirsiniz.",
  "icons": {
    "128": "images/128.png",
    "16": "images/16.png",
    "24": "images/24.png",
    "32": "images/32.png"
  },
  "manifest_version": 2,
  "name": "Muhammed Akşam",
  "short_name": "Muhammed Akşam",
  "permissions": [
    "background",
    "activeTab",
    "https://muhammedaksam.com.tr/",
    "https://cdn.fontawesome.com/",
    "gcm",
    "storage",
    "notifications",
    "identity"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "background": {
    "persistent": false,
    "scripts": [
      "js/OneSignal.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "images/192.png"
  ]
}
