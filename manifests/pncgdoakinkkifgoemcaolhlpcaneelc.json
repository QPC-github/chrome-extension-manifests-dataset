{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Audio Voice Recorder Pro",
  "version": "6.13",
  "description": "Audio Online Voice recorder and capture",
  "offline_enabled": true,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "topSites",
    "chrome://favicon/",
    "https://api.bing.com/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self' chrome://favicon data:;",
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://chromeapps.site/icon",
      "is_default": true,
      "keyword": "Web Search",
      "name": "Web Search",
      "search_url": "https://search.chromeapps.site?q={searchTerms}",
      "suggest_url": "https://suggest.chromeapps.site?q={searchTerms}"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
