{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "eventPage.js"
    ]
  },
  "description": "Speak the selected text",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "SpeakIt",
  "permissions": [
    "tts",
    "contextMenus"
  ],
  "version": "1.6",
  "chrome_url_overrides": {
    "newtab": "nt.html"
  }
}
