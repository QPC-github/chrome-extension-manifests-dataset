{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Extension Serial Gramera",
  "version": "1.0.3",
  "description": "Obtiene el valor de la gramera, esta extension sirve como intermediaria con la App Serial Gramera",
  "author": "Javier Garcia",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://web.intercom.com/*",
        "http://webintercom/*",
        "https://webintercom/*",
        "https://sslintercom/*",
        "https://web.intercom.com/*",
        "http://localhost/*",
        "https://localhost/*",
        "http://localhost/root/*",
        "https://localhost/root/*",
        "http://192.168.0.20/*",
        "http://192.168.0.145/*",
        "http://192.168.0.200/*",
        "http://192.168.0.71/*",
        "http://192.168.100.4/*",
        "http://192.168.100.100/*",
        "http://192.168.1.250/*",
        "http://192.168.1.16/*",
        "http://192.168.1.250/*",
        "http://192.168.1.62/*",
        "http://192.168.100.6/*",
        "http://192.168.100.6/*"
      ],
      "css": [
        "css/style_serial.css"
      ],
      "js": [
        "js/jquery-1.11.2.min.js",
        "js/controller_serial.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  }
}
