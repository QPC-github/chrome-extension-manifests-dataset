{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Electronic First",
  "version": "1.0",
  "description": "Browse the web for your favorite games and always know if they are available at ElectronicFirst.com !",
  "icons": {
    "128": "EF.ico.png"
  },
  "author": "Mejba Ahmed",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "EF.ico.png",
    "default_title": "Electronic First"
  }
}
