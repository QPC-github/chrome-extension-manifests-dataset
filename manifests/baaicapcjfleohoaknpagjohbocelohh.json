{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "diep.io gamepad support",
  "description": "This extension adds gamepad support for diep.io",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "http://diep.io/*"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "gamepad.js"
  ]
}
