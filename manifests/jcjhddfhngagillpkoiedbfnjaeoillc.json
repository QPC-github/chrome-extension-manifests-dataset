{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "SenderScore Rating",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Checks the selected IP address or domain name's Sender Score via Return Path",
  "icons": {
    "128": "images/senderscore128.png",
    "16": "images/senderscore16.png",
    "48": "images/senderscore48.png"
  },
  "permissions": [
    "contextMenus",
    "background",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "senderscore.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ]
  },
  "minimum_chrome_version": "6.0.0.0"
}
