{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Scott Morrison Nickname Converter",
  "description": "Replaces common Scott Morrison shorthand variants with his full name.",
  "icons": {
    "48": "48xLogo.png",
    "128": "128xLogo.png"
  },
  "version": "1.2",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
