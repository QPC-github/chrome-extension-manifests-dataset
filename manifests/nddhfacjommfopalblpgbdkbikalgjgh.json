{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Shopify管理后台汉化插件",
  "description": "任何问题欢迎发邮件：dianshangchajian@gmail.com （邮箱是电商插件的中文拼音）",
  "version": "1.0.1",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "declarativeContent",
    "webNavigation",
    "*://*.myshopify.com/",
    "*://tran.xyouwork.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "Shopify管理后台汉化插件。任何问题欢迎给我发邮件：dianshangchajian@gmail.com （邮箱是电商插件的中文拼音）",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
