{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "manifest_version": 2,
  "name": "ArchProxy",
  "version": "0.1.15",
  "description": "Archdiocese of New Orleans Proxy helper",
  "icons": {
    "16": "icon@16.png",
    "48": "icon@48.png",
    "128": "icon@128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "*://proxy.lvh.me/*",
      "*://proxy.arch-no.org/*"
    ]
  },
  "permissions": [
    "<all_urls>",
    "proxy",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ]
}
