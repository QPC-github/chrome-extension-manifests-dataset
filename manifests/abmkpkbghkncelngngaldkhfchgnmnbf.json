{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Giving Assistant Button",
  "version": "7.3.18",
  "author": "Giving Assistant",
  "description": "Giving Assistant Button",
  "background": {
    "page": "bg.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/vendor.chunk.js",
        "js/contentScript.bundle.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "icons": {
    "128": "img/givva-128.png",
    "32": "img/givva-32.png",
    "64": "img/givva-64.png",
    "48": "img/givva-48.png",
    "16": "img/givva-16.png"
  },
  "browser_action": {
    "default_title": "Giving Assistant Button",
    "default_icon": {
      "128": "img/givva-bw-128.png",
      "64": "img/givva-bw-64.png",
      "48": "img/givva-bw-48.png",
      "32": "img/givva-bw-32.png",
      "16": "img/givva-bw-16.png"
    }
  },
  "web_accessible_resources": [
    "iframes/*"
  ],
  "permissions": [
    "webRequest",
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "manifest_version": 2
}
