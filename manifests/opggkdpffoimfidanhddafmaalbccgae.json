{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Deed",
  "version": "2.1",
  "description": "Deed lets you keep track of things you plan to do - your deeds.",
  "manifest_version": 2,
  "app": {
    "urls": [
      "http://www.ordinem.com/"
    ],
    "launch": {
      "web_url": "http://www.ordinem.com"
    }
  },
  "icons": {
    "128": "logo-large.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "http://www.ordinem.com/*",
    "https://www.ordinem.com/*"
  ]
}
