{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Extension Tracker",
  "description": "Track user count,reviews and other details of Extensions",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "Click to action",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
