{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Hobbyking Fans",
  "version": "1.3.1",
  "short_name": "HobbykingFans",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "A tool that makes the HobbyKing website a better place than it is without it!",
  "permissions": [
    "tabs",
    "*://www.hobbyking.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.hobbyking.com/*",
        "*://hobbyking.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "img/HobbykingFans30x30.png"
  ]
}
