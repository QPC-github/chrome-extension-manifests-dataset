{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Qu Video Search",
  "short_name": "Qu",
  "description": "Video Search",
  "version": "0.0.8",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js",
      "options.js"
    ]
  },
  "browser_action": {
    "default_icon": "Cu Icon3 16x16.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "16": "Cu Icon3 16x16.png",
    "32": "Cu Icon3 32x32.png",
    "64": "Cu Icon3 64x64.png"
  }
}
