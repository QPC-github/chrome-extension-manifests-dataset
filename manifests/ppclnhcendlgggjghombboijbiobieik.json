{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Kotleba pre koktavých",
  "description": "Kotlebove skutočné meno",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "kokotleba.js"
      ],
      "run_at": "document_end"
    }
  ]
}
