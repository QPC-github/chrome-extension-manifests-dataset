{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Obamanator",
  "description": "Replaces all images on a page with obama pics and gifs",
  "version": "1.0",
  "content_scripts": [
    {
      "js": [
        "obamanator.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "obama-icon.png"
  }
}
