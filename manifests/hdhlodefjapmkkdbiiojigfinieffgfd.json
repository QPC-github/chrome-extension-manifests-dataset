{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "EtonX Screen Capture Extension",
  "version": "0.0.0.9",
  "manifest_version": 2,
  "description": "Screen Capture for twilio project",
  "background": {
    "scripts": [
      "extension.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://staging-1.etonx.com/*",
      "https://staging-2.etonx.com/*",
      "https://learn.etonx.com/*",
      "https://staging-britishcouncil.etonx.com/*",
      "https://englishonlinelearn.britishcouncil.org/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}
