{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BioVinci User guide",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "User guide and tutorial for BioVinci, a platform for data visualization in life science",
  "homepage_url": "https://vinci.bioturing.com",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/UserGuide.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/128x128.png",
    "default_title": "User guide and tutorial for BioVinci",
    "default_popup": "src/bg/UserGuide.html"
  }
}
