{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "IYWTo",
  "description": "A tool to bookmark digital projects to act on climate change and live more sustainably.",
  "version": "0.3.0",
  "icons": {
    "128": "./img/iywto_logo_128.png",
    "48": "./img/iywto_logo_48.png",
    "32": "./img/iywto_logo_32.png"
  },
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "./js/background.js"
    ]
  }
}
