{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Mastermind Search Companion",
  "short_name": "Mastermind",
  "version": "0.10.2",
  "manifest_version": 2,
  "description": "The Mastermind Search Companion augments your gene and variant searches with articles citing your gene or variant from Mastermind.",
  "icons": {
    "16": "mm16.png",
    "48": "mm48.png",
    "128": "mm128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_idle",
      "js": [
        "script_mastermind.js",
        "search.js",
        "script_search.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background_mastermind.js",
      "numeral.min.js",
      "search.js",
      "background_search.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "mm48.png"
  },
  "options_ui": {
    "page": "index.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [
    "mm48.png"
  ],
  "key": ""
}
