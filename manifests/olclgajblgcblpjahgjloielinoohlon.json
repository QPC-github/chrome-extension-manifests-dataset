{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "osu!blood",
  "author": "WoctorDho",
  "version": "0.1",
  "description": "Download beatmaps via bloodcat mirror",
  "icons": {
    "19": "icon19.png",
    "38": "icon38.png",
    "192": "icon192.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png",
      "192": "icon192.png"
    },
    "default_title": "Download beatmap via bloodcat mirror",
    "show_matches": [
      "https://osu.ppy.sh/beatmapsets/*"
    ]
  }
}
