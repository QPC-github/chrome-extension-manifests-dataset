{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quick Emoji",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Short cut typing of emoji. Follows the convesion of http://www.emoji-cheat-sheet.com/ for all input fields.",
  "icons": {
    "48": "thumbsup.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "emoji.js"
      ],
      "all_frames": true
    }
  ]
}
