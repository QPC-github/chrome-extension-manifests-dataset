{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/refresh.png",
    "default_title": "Update rules"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "Ensure your online safety by automatically blocking known malware websites. Updated daily with tons of defintions!",
  "homepage_url": "http://pc-pros.com",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/button.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2,
  "name": "PC-Pros Safety Net",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.0.1",
  "web_accessible_resources": [
    "res/*"
  ]
}
