{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "WIP.tab",
  "version": "1.5.1",
  "description": "A new-tab extension for WIP.chat",
  "author": "Jerre",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "background",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "pages/app.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "panel.html",
    "js/content.js"
  ]
}
