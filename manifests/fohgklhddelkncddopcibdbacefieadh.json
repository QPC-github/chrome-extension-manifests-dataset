{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "theWebShed",
  "description": "theWebShed Onsite Optimiser extension allows you to manage the optimisaiton of your website directly through Google Chrome!",
  "version": "2.3",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "icons": {
    "16": "images/webshed-green-icon@16.png",
    "128": "images/webshed-green-icon@128.png"
  },
  "browser_action": {
    "default_icon": "images/webshed-grey-icon@19.png"
  },
  "web_accessible_resources": [
    "views/ws-sidebar.html",
    "styles/*",
    "images/webshed-logo.png",
    "fonts/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
