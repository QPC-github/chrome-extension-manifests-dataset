{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Better Typography | Smooth Fonts",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "Enable Kerning & Grayscale Anti-Aliasing for chrome on macOS",
  "icons": {
    "48": "icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
