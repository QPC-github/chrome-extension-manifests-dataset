{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-1.7.min.js",
        "expandsvc.js",
        "content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "description": "Shorten and expand URLs with surf2.me",
  "icons": {
    "128": "s2me128.png",
    "16": "s2me16.png",
    "32": "s2me32.png",
    "48": "s2me48.png"
  },
  "manifest_version": 2,
  "name": "Surf2.me Url Shortener with QR code",
  "options_page": "options.html",
  "page_action": {
    "default_icon": "s2me19.png",
    "default_popup": "popup.html",
    "default_title": "Shorten URL"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "2.0.1",
  "web_accessible_resources": [
    "s2me48.png"
  ]
}
