{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Quierotest Compartir Pantalla",
  "author": "TokBox, Inc.",
  "version": "2.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Extension desarrollada por Doover con la tecnolog√≠a TokBox Web RTC para compartir la pantalla",
  "homepage_url": "http://quierotest.com/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.quierotest.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
