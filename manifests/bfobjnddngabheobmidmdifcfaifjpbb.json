{
  "browser_action": {
    "default_icon": "icon016.png",
    "default_title": "Open the current tab as a popup"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Perform various tasks on your current tab.",
  "commands": {
    "embedtube": {
      "description": "Toggle YouTube Embedding",
      "suggested_key": {
        "default": "Alt+Y"
      }
    },
    "incrementurl": {
      "description": "Increment URL",
      "suggested_key": {
        "default": "Alt+K"
      }
    },
    "decrementurl": {
      "description": "Decrement URL",
      "suggested_key": {
        "default": "Alt+J"
      }
    },
    "pintoggle": {
      "description": "Toggle pinned state of current tab",
      "suggested_key": {
        "default": "Alt+P"
      }
    }
  },
  "background": {
    "scripts": [
      "popup.js",
      "commands.js"
    ]
  },
  "name": "metaTaber",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "icon048.png",
    "128": "icon128.png",
    "16": "icon016.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "sizetip.js"
      ],
      "run_at": "document_end",
      "css": [
        "sizetip.css"
      ]
    }
  ],
  "manifest_version": 2,
  "version": "13.418"
}
