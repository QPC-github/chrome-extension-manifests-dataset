{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "UnMax",
  "version": "1.0",
  "description": "Prevents tabs from maximized windows from maximizing when detached",
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
