{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BetterPlaysTV",
  "version": "0.2.0",
  "manifest_version": 2,
  "browser_action": {},
  "content_scripts": [
    {
      "js": [
        "bundle.user.js"
      ],
      "matches": [
        "http://plays.tv/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
