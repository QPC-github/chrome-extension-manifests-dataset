{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "FlorinaOut.gr",
  "version": "1.0",
  "description": "FlorinaOut for chrome",
  "icons": {
    "48": "img/48.png",
    "128": "img/128.png",
    "16": "img/16.png"
  },
  "browser_action": {
    "default_icon": "img/19.png",
    "default_title": "Press the button to see more actions",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "notifications"
  ]
}
