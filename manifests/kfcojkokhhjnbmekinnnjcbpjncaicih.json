{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "MINTZ Download Controller",
  "description": "MINTZ Download File Controller",
  "version": "0.3",
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": false
  },
  "permissions": [
    "downloads"
  ],
  "content_security_policy": "script-src 'self'; default-src 'self'",
  "manifest_version": 2,
  "icons": {
    "48": "mintz.png"
  },
  "web_accessible_resources": [
    "mintz.png"
  ]
}
