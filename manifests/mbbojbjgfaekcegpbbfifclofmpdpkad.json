      "js": [
        "jquery.min.js",
        "ExpertMode.user.js"
      ],
      "matches": [
        "*://united.com/web/*/apps/booking/flight/searchResult*",
        "*://*.united.com/web/*/apps/booking/flight/searchResult*",
        "*://united.com/ual/*/*/flight-search/book-a-flight*",
