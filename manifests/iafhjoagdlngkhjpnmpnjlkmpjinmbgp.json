{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Streamline Health Coder Notification",
  "short_name": "Coder Notification",
  "version": "0.6.0.5",
  "manifest_version": 2,
  "description": "Streamline Health Coder Notification",
  "browser_action": {
    "default_icon": {
      "32": "img/STRM_Logo_Icon_32.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/STRM_Logo_Icon_128.png"
  },
  "permissions": [
    "gcm",
    "tabs",
    "webNavigation",
    "activeTab",
    "storage",
    "notifications",
    "background",
    "https://*.streamlinehealth.net/*",
    "http://localhost/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
