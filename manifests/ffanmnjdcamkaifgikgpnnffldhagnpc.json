{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Daily5ify",
  "version": "1.0",
  "description": "Forces Dailymotion embed videos to give you HTML5 media instead of wrapping it in Flash",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "util.js",
        "embed.js",
        "dispatch.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png"
  }
}
