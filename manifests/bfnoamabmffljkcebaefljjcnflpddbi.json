{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Tickld Bookmarker",
  "description": "Creates a quick way of generating ticlkd bookmarks",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.tickld.com/*"
      ],
      "js": [
        "scripts.js"
      ]
    }
  ],
  "permissions": [
    "http://tickld.com/"
  ]
}
