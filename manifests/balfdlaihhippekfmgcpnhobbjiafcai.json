{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Canvas Class Roster Pics",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "View your class roster. In Canvas go to People/Everyone then click on the extension icon to view their pics.",
  "homepage_url": "https://github.com/TJMurphy",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "canvaspic.png",
    "default_title": "Canvas roster pics!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
