{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Social Ads Tracking",
  "version": "3.0.0.13",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_default.png",
    "default_popup": "login.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://ads.academy/",
    "http://google.co.in/"
  ],
  "description": "Social Ads Tracking by Ads.Academy."
}
