{
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "logo-19.png",
      "38": "logo-38.png"
    },
    "default_title": "Clarify"
  },
  "description": "Clarify is a free, high quality dictionary app for all your devices.",
  "icons": {
    "128": "logo-128.png",
    "16": "logo-16.png",
    "48": "logo-48.png"
  },
  "manifest_version": 2,
  "name": "Clarify",
  "permissions": [
    "activeTab",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1"
}
