{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TopTap",
  "description": "Jump to top of page",
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Jump to top of page",
    "default_icon": {
      "19": "TopTap_19.png",
      "38": "TopTap_38.png"
    }
  },
  "manifest_version": 2
}
