{
  "manifest_version": 2,
  "icons": {
    "128": "/icons/128.png"
  },
  "name": "StartsSearch",
  "description": "StartsSearch as default search engine",
  "version": "0.24",
  "browser_action": {
    "default_icon": "/icons/128.png",
    "default_title": "Use StartsSearch extension to search the web"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "StartsSearch",
      "keyword": "StartsSearch",
      "search_url": "https://defaultsearch.startssearch.com/?q={searchTerms}",
      "favicon_url": "https://defaultsearch.startssearch.com/favicon.ico",
      "is_default": true,
      "encoding": "UTF-8"
    }
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
