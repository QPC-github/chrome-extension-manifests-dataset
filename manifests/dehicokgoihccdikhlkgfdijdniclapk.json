{
  "browser_action": {
    "default_icon": "assets/search.png",
    "default_title": "WeBuyAmazonExtension",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "assets/jquery.js",
        "content_script.js"
      ],
      "matches": [
        "https://*.webuy.com/*"
      ]
    }
  ],
  "description": "WeBuyAmazonExtension extension",
  "icons": {
    "16": "assets/search.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "WeBuyAmazonExtension",
  "permissions": [
    "https://*.webuy.com/*",
    "https://*.amazon.co.uk/*",
    "https://*.ebay.co.uk/*",
    "https://*.cex.uk.webuy.io/*",
    "https://*.cex.es.webuy.io/*",
    "https://*.cex.ie.webuy.io/*",
    "https://*.cex.it.webuy.io/*",
    "https://*.cex.nl.webuy.io/*",
    "https://*.cex.pt.webuy.io/*",
    "https://*.cex.ic.webuy.io/*",
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.2.2",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
