{
  "manifest_version": 2,
  "background": "sys/background.html",
  "browser_action": {
    "default_icon": "sys/scifi.bg.png",
    "default_popup": "sys/popup.html",
    "default_title": "SciFi.bg",
    "name": "SciFi.bg"
  },
  "description": "SciFi.bg",
  "icons": {
    "128": "sys/scifi.bg.png",
    "16": "sys/scifi.bg.png",
    "48": "sys/scifi.bg.png"
  },
  "name": "SciFi.bg",
  "options_page": "sys/options.html",
  "permissions": [
    "http://scifi.bg/*",
    "http://www.scifi.bg/*"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.3.2",
  "content_scripts": [
    {
      "matches": [
        "http://scifi.bg/*",
        "http://www.scifi.bg/*"
      ],
      "css": [
        "sys/chrome-extension-bar-remove.css"
      ]
    }
  ]
}
