{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Material DevTools Theme Collection",
  "version": "2.7",
  "description": "Material DevTools Theme Collection",
  "short_name": "Material DevTools",
  "manifest_version": 2,
  "author": "Elior Boukhobza (mallowigi)",
  "devtools_page": "devtools.html",
  "homepage_url": "https://www.material-theme.com",
  "options_page": "/public/index.html",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Material DevTools Settings",
    "default_popup": "/public/index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
