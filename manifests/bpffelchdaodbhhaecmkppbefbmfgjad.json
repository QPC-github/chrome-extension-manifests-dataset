{
  "manifest_version": 2,
  "app": {
    "launch": {
      "web_url": "https://app.recruiterbox.com/chrome_webstore_landing?next=/dashboard&ref=chrome_webstore"
    },
    "urls": [
      "*://recruiterbox.com/",
      "*://*.recruiterbox.com/"
    ]
  },
  "description": "Online Recruitment Software & Applicant Tracking System for small businesses.",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "name": "Recruiterbox",
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "8"
}
