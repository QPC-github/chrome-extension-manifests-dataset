{
  "manifest_version": 2,
  "name": "Indumentaria Online",
  "short_name": "indumentariaonline",
  "version": "1.1",
  "description": "La aplicacion oficial de Indumentaria Online",
  "background": {
    "scripts": [
      "scripts/options.js",
      "scripts/react.min.js",
      "scripts/background.js",
      "scripts/popup.js"
    ]
  },
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Indumentaria Online"
  },
  "icons": {
    "128": "images/icon_logo128.png",
    "19": "images/icon_active19.png",
    "48": "images/icon_logo48.png"
  },
  "default_icon": {
    "19": "images/icon_active19.png",
    "38": "images/icon_active38.png"
  },
  "permissions": [
    "http://www.indumentariaonline.com/*",
    "notifications",
    "alarms",
    "tabs",
    "storage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "web_accessible_resources": [
    "images/icon_logo48.png"
  ]
}
