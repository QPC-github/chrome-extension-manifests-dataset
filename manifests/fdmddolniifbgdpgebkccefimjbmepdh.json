{
  "chrome_settings_overrides": {
    "search_provider": {
      "alternate_urls": [
        "http://www.googlando.com/busca/?q={searchTerms}",
        "http://www.googlando.com/busca/?q={searchTerms}"
      ],
      "encoding": "UTF-8",
      "favicon_url": "http://www.googlando.com/favicon.png",
      "image_url": "http://www.googlando.com/busca/?q={searchTerms}",
      "image_url_post_params": "image_lang=__MSG_url_domain__",
      "instant_url": "http://www.googlando.com/busca/?q={searchTerms}",
      "instant_url_post_params": "instant_lang=__MSG_url_domain__",
      "is_default": true,
      "keyword": "keyword.__MSG_url_domain__",
      "name": "Googlando",
      "search_url": "http://www.googlando.com/busca/?q={searchTerms}",
      "search_url_post_params": "search_lang=__MSG_url_domain__",
      "suggest_url": "http://www.googlando.com/busca/?q={searchTerms}",
      "suggest_url_post_params": "suggest_lang=__MSG_url_domain__"
    }
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "description": "Plugin do site de bucas Googlando.com",
  "icons": {
    "128": "img/logo-128.png",
    "19": "img/logo-19.png",
    "48": "img/logo-48.png"
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "img/logo-48.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "name": "Googlando",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
