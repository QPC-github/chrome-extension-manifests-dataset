{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "The Decision Wall - Page SnapShot",
  "description": "Need to make a decision? Gather web pages and rank them according to the criteria you specify at The Decision Wall.",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "img/The Decision Wall Logo Icon 16.png",
    "48": "img/The Decision Wall Logo Icon 48.png",
    "128": "img/The Decision Wall Logo Icon 128.png"
  },
  "browser_action": {
    "default_icon": "img/The Decision Wall Logo Icon 48.png",
    "default_popup": "popup.html",
    "name": "Click to take a PageSnapShot"
  },
  "web_accessible_resources": [
    "img/The Decision Wall Logo Icon 48.png"
  ],
  "manifest_version": 2
}
