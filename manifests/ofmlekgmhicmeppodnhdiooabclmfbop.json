{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Leprabro",
  "version": "0.9",
  "icons": {
    "48": "icon_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.leprosorium.ru/*",
        "http://leprastuff.ru/data/img/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "jquery.js",
        "script.js",
        "jquery-ui.js"
      ]
    }
  ],
  "permissions": [
    "http://leprosorium.ru/",
    "http://*.leprosorium.ru/",
    "http://leprastuff.ru/",
    "http://*.leprastuff.ru/"
  ]
}
