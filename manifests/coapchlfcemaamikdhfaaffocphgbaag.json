{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "アイコンをクリックすることで英和和英辞典の検索フォームや、英語翻訳のフォームを表示",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Weblio検索翻訳フォーム",
    "default_popup": "popup.html"
  },
  "name": "Weblio検索翻訳フォーム",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "contextMenus"
  ],
  "manifest_version": 2,
  "version": "1.1"
}
