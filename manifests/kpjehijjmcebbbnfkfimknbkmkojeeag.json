{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "persistent": false,
    "scripts": [
      "dreamafar.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://query.yahooapis.com https://suggestqueries.google.com 'unsafe-eval'; object-src 'self'",
  "description": "Photo NewTab - Новая вкладка с красивыми фотообоями.",
  "icons": {
    "128": "logo-128.png",
    "64": "logo-64.png",
    "32": "logo-32.png"
  },
  "manifest_version": 2,
  "name": "Photo NewTab",
  "offline_enabled": true,
  "permissions": [
    "geolocation",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "topSites",
    "tabs",
    "chrome://favicon/"
  ],
  "version": "0.9"
}
