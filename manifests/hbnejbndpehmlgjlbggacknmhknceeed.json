{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy ChromeBoard",
  "author": "Thomas R.",
  "description": "Restez connecté au quotidien grâce à une page d'ouverture moderne et customisable: Actus, favoris, météo, ...",
  "version": "1.1.7",
  "permissions": [
    "http://suggestqueries.google.com/*",
    "https://query.yahooapis.com/*",
    "http://www.lefigaro.fr/rss/*",
    "http://infinigag.k3min.eu/*"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_popup": "parts/popup.html"
  },
  "icons": {
    "16": "medias/icon/icon-16.png",
    "48": "medias/icon/icon-48.png",
    "128": "medias/icon/icon-128.png"
  }
}
