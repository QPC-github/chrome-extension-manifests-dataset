{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "ScriptRunner",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Runs JavaScript!",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Script Runner",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "incognito": "spanning",
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
