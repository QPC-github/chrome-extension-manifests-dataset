{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Stand Up! Timer",
  "description": "This extension reminds you to take a break from work.",
  "author": "Erfan Norozi",
  "version": "1.0.2",
  "offline_enabled": true,
  "permissions": [
    "notifications",
    "alarms",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "notification.png"
  ]
}
