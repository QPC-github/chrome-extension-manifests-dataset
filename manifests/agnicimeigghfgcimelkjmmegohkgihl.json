{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "College to Strip Club",
  "version": "1.1",
  "description": "Replaces the text 'college' or 'University' with 'strip club'.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "College to Strip Club.js"
      ],
      "run_at": "document_end"
    }
  ]
}
