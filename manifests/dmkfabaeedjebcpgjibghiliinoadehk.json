{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DemoOnFly",
  "short_name": "DemoOnFly",
  "description": "An extension to check demo on fly for addons placed on ODOO Store: https://apps.odoo.com or Webkul Store: https://store.webkul.com",
  "version": "0.1.4",
  "author": "Mohit Chandra(mohit@webkul.com)",
  "homepage_url": "https://webkul.com",
  "icons": {
    "16": "./img/16x16.png",
    "32": "./img/32x32.png",
    "48": "./img/48x48.png",
    "96": "./img/96x96.png",
    "128": "./img/128x128.png"
  },
  "browser_action": {
    "default_icon": "./img/32x32.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
