{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Sweedly Webbyrå",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sweedly Webbyrå - En webbyrå med kvalité. Hos oss kan du skaffa din hemsida i wordpress och få hjälp med sökmotoroptimering (SEO).",
  "homepage_url": "https://www.sweedly.se",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Sweedly Webbyrå",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ]
}
