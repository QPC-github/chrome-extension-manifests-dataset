{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Collapse Reddit Comments",
  "description": "This collapses all comments in a Reddit comment page except for the outer most comments.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "background",
    "http://www.reddit.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
