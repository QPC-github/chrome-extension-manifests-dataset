{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "DurhamWhatPlus",
  "version": "1.5.1",
  "manifest_version": 2,
  "description": "Enhancements to Durham What Fantasy Football pages.",
  "content_scripts": [
    {
      "matches": [
        "http://games.espn.go.com/ffl/*"
      ],
      "js": [
        "common.js",
        "init.js"
      ]
    },
    {
      "matches": [
        "http://games.espn.go.com/ffl/boxscorequick*"
      ],
      "css": [
        "ffplus.css"
      ],
      "js": [
        "ffpdata.js",
        "common.js",
        "boxscorequick.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
