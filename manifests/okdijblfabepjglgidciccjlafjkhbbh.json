{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "品牌查一查",
  "version": "0.2.2",
  "description": "查询品牌基本信息",
  "options_page": "about.html",
  "author": "亚马跌",
  "permissions": [
    "https://www.ymdie.com/*",
    "tabs"
  ],
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "browser_action": {
    "default_title": "品牌查一查",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "popup.html",
    "script": [
      "js/background.js"
    ],
    "persistence": false
  }
}
