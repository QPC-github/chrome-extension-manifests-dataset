{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Tools for Pawshake vetting page",
  "short_name": "PawsTools",
  "version": "0.2",
  "description": "Pawshake-Copy email address and make edit profile hyperlink functional",
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/dog1_icon16.png",
      "32": "images/dog1_icon32.png",
      "48": "images/dog1_icon48.png",
      "128": "images/dog1_icon128.png"
    }
  },
  "icons": {
    "16": "images/dog1_icon16.png",
    "32": "images/dog1_icon32.png",
    "48": "images/dog1_icon48.png",
    "128": "images/dog1_icon128.png"
  },
  "manifest_version": 2
}
