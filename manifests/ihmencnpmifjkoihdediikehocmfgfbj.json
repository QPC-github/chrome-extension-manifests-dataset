{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "YouTube Ad Tracker",
  "version": "0.2",
  "description": "YouTube Ad Tracker",
  "permissions": [
    "webRequest",
    "storage",
    "*://*.youtube.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "YouTube Ads",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
