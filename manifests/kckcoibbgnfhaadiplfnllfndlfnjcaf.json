{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Rainbow Brackets",
  "description": "Colors brackets on webpage",
  "version": "1.6",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentLoad.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Rainbow Brackets"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
