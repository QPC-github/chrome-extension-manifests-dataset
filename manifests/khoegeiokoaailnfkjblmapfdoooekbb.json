{
  "update_url": "http://clients2.google.com/service/update2/crx",
  "name": "EverBookmark",
  "description": "基于印象笔记的云标签，将书签都保存到印象笔记吧",
  "version": "0.1.8",
  "manifest_version": 2,
  "icons": {
    "16": "images/bk16.png",
    "48": "images/bk48.png",
    "128": "images/bk128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_title": "EverBookmark",
    "default_icon": "images/bk128.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "js/lib/jquery-1.8.2.js",
      "js/tools.js",
      "js/initData.js"
    ]
  }
}
