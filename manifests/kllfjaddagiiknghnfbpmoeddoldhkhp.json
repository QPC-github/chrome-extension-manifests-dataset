{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Yorkshire Goggles",
  "description": "See the web through the eyes of Yorkshireman.",
  "icons": {
    "128": "icon_128.png"
  },
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "yorkshire-goggles.css"
      ],
      "js": [
        "jquery-2-1-1.js",
        "jquery.replacetext.js",
        "burn.js",
        "yorkshire-goggles.js"
      ],
      "all_frames": true
    }
  ]
}
