{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Custom Dashboard",
  "description": "Reimagine Chrome Tabs with Custom Dashboard.",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "version": "1.0.3",
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "author": "Isaiah Francois",
  "permissions": []
}
