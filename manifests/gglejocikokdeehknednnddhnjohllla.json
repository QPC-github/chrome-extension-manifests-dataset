{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Ol√© Plugin",
  "version": "1.0.1",
  "description": "Remueve la pantalla de loguin que se requiere para leer notas",
  "icons": {
    "16": "icono.png",
    "48": "icono.png",
    "128": "icono.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.ole.com.ar/*"
      ],
      "js": [
        "jquery-2.1.4.min.js",
        "remove_elements.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://www.ole.com.ar/*"
  ]
}
