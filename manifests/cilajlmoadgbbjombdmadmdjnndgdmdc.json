{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "pink tumblr",
  "description": "This extension turns the background of your tumblr dash pink.",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "http://www.tumblr.com/*"
      ],
      "css": [
        "pink.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.tumblr.com/*"
  ]
}
