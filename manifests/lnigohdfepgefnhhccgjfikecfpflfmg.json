{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "icons": {
    "16": "ch16x16.png",
    "48": "ch48x48.png",
    "128": "ch128x128.png"
  },
  "name": "Chivalo",
  "version": "0.1.7",
  "description": "Add items to your Chivalo wishlist",
  "browser_action": {
    "default_icon": {
      "19": "ch19x19.png",
      "38": "ch38x38.png"
    }
  },
  "background": {
    "scripts": [
      "listeners.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
