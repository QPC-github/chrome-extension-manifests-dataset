{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/logo-eye-16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "scripts/jquery.min.js",
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Get phones from actions",
  "icons": {
    "128": "images/logo-eye-128.png",
    "16": "images/logo-eye-16.png",
    "48": "images/logo-eye-48.png"
  },
  "manifest_version": 2,
  "name": "Aion - Click là ra số - Chỉ việc chốt đơn",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "version": "1.3.1"
}
