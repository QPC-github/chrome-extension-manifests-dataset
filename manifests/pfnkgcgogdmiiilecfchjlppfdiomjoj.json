{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "include_globs": [
        "https://insideflyer.com/forums/*",
        "http://insideflyer.com/forums/*"
      ],
      "js": [
        "if2mp.user.js"
      ],
      "matches": [
        "https://insideflyer.com/forums/*",
        "http://insideflyer.com/forums/*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "description": "Make InsideFlyer look like Milepoint",
  "name": "IF2MP",
  "version": "0.0.4",
  "homepage_url": "http://wandr.me/",
  "short_name": "IF2MP",
  "web_accessible_resources": [
    "if2mp.user.js"
  ],
  "icons": {
    "16": "icon16.jpg",
    "48": "icon48.jpg",
    "128": "icon128.jpg"
  }
}
